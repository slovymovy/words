{
    "word_family": [
        "you",
        "your",
        "yours"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "5313cddb-c0e8-3989-ad41-9c0cb313d720",
                    "sense_definition": "The reflexive form of <w>you</w> (singular), used when the subject and object of the sentence refer to the same person.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful with that fire, or you will burn <w>yourself</w>.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен с огнём, иначе ты <w>обожжёшься</w>."
                            }
                        },
                        {
                            "text": "You can do it <w>by</w> <w>yourself</w> if you really try.",
                            "target_lang_translations": {
                                "ru": "Ты можешь сделать это <w>сам</w>, если действительно постараешься."
                            }
                        }
                    ],
                    "common_phrases": [
                        "by yourself",
                        "for yourself",
                        "be yourself",
                        "look after yourself",
                        "suit yourself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвратная форма местоимения <w>you</w> (ты/вы), используемая, когда подлежащее и дополнение относятся к одному и тому же лицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            },
                            {
                                "target_lang_word": "сам",
                                "target_lang_sense_clarification": "самостоятельно, без помощи (в фразах типа 'by yourself')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7541cc1-572d-37d9-8fe7-2bd70be03373",
                    "sense_definition": "Used to emphasize the person being addressed, indicating that they personally did something or are the specific one involved.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emphatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You <w>yourself</w> admitted that it was a mistake.",
                            "target_lang_translations": {
                                "ru": "Ты <w>сам</w> признал, что это была ошибка."
                            }
                        },
                        {
                            "text": "If you want the job done right, you must do it <w>yourself</w>.",
                            "target_lang_translations": {
                                "ru": "Если хочешь, чтобы работа была сделана хорошо, ты должен сделать её <w>сам</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "personally"
                    ],
                    "common_phrases": [
                        "do it yourself",
                        "see for yourself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления, указывая, что собеседник лично совершил действие или является именно тем лицом, о котором идет речь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сам",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "сама",
                                "target_lang_sense_clarification": "женский род"
                            },
                            {
                                "target_lang_word": "сами",
                                "target_lang_sense_clarification": "вежливое обращение на 'Вы'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7222cb72-edaa-3955-bf3f-31841fcbf863",
                    "sense_definition": "Used as a substitute for <w>you</w> (singular) in subject or object position, often intended to sound more polite or formal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "substitute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please let me know if <w>yourself</w> or your team need any assistance.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, дайте знать, если <w>вам</w> или вашей команде понадобится помощь."
                            }
                        },
                        {
                            "text": "Is that cup of coffee for <w>yourself</w>?",
                            "target_lang_translations": {
                                "ru": "Эта чашка кофе для <w>вас</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "you"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in business contexts or Irish English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется вместо <w>you</w> (ты/вы) в позиции подлежащего или дополнения, часто для придания оттенка вежливости или формальности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы",
                                "target_lang_sense_clarification": "вежливая или формальная замена"
                            },
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "разговорная замена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2535681f-a40b-386b-881e-4989bf7bdd6f",
                    "sense_definition": "An honorific form of <w>yourself</w>, typically capitalized, used when referring to a deity or a person of high rank.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "honorific",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We offer our humble prayers to <w>Yourself</w>.",
                            "target_lang_translations": {
                                "ru": "Мы возносим наши смиренные молитвы к <w>Тебе</w>."
                            }
                        },
                        {
                            "text": "It is <w>Yourself</w> who guides us through the darkness.",
                            "target_lang_translations": {
                                "ru": "Именно <w>Ты</w> ведешь нас сквозь тьму."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "capitalized honorific"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Почтительная форма, обычно пишущаяся с большой буквы, используемая при обращении к божеству или высокопоставленному лицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ты",
                                "target_lang_sense_clarification": "обращение к божеству"
                            },
                            {
                                "target_lang_word": "Вы",
                                "target_lang_sense_clarification": "обращение к высокопоставленному лицу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
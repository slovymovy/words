{
    "word_family": [
        "godłowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "051416b0-1c57-35ff-b21c-aeec4214452a",
                    "sense_definition": "graficzny symbol państwa, narodu lub jednostki terytorialnej, umieszczany na urzędowych dokumentach i budynkach",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "symbol_panstwowy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Godłem Rzeczypospolitej Polskiej jest wizerunek orła białego w koronie w czerwonym polu.",
                            "target_lang_translations": {
                                "ru": "<Гербом> Республики Польша является изображение белого орла в короне на красном поле."
                            }
                        },
                        {
                            "text": "W każdej klasie szkolnej nad tablicą wisi <godło> państwowe.",
                            "target_lang_translations": {
                                "ru": "В каждом школьном классе над доской висит государственный <герб>."
                            }
                        }
                    ],
                    "synonyms": [
                        "herb",
                        "emblemat"
                    ],
                    "common_phrases": [
                        "godło państwowe",
                        "godło Polski",
                        "znieważenie godła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Графический символ государства, народа или территориальной единицы, размещаемый на официальных документах и зданиях"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герб",
                                "target_lang_sense_clarification": "государственный символ"
                            },
                            {
                                "target_lang_word": "эмблема",
                                "target_lang_sense_clarification": "символическое изображение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd0cd2a8-e0e3-3593-addc-cd2b9f5f12fd",
                    "sense_definition": "symboliczny znak rozpoznawczy organizacji, stowarzyszenia, zawodu lub grupy ludzi",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "symbol_organizacji",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy członkowie klubu nosili na marynarkach wyhaftowane <godło>.",
                            "target_lang_translations": {
                                "ru": "Все члены клуба носили на пиджаках вышитую <эмблему>."
                            }
                        },
                        {
                            "text": "Wąż owinięty wokół kielicha to znane <godło> aptekarskie.",
                            "target_lang_translations": {
                                "ru": "Змея, обвивающая чашу, — это известная аптекарская <эмблема>."
                            }
                        }
                    ],
                    "synonyms": [
                        "emblemat"
                    ],
                    "common_phrases": [
                        "godło stowarzyszenia",
                        "godło klubu",
                        "godło aptekarskie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Символический опознавательный знак организации, ассоциации, профессии или группы людей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эмблема",
                                "target_lang_sense_clarification": "знак организации или клуба"
                            },
                            {
                                "target_lang_word": "символ",
                                "target_lang_sense_clarification": "отличительный знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d483416-a6bc-3fdd-8c34-9345fca2fef3",
                    "sense_definition": "znak słowny lub graficzny zastępujący podpis autora, używany dla zachowania anonimowości w konkursach",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anonimowosc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dokumenty przetargowe należy opatrzyć <godłem> w lewym górnym rogu.",
                            "target_lang_translations": {
                                "ru": "Тендерные документы следует пометить <девизом> в левом верхнем углу."
                            }
                        },
                        {
                            "text": "Autorzy prac konkursowych nie podawali nazwisk, lecz podpisywali się wybranym <godłem>.",
                            "target_lang_translations": {
                                "ru": "Авторы конкурсных работ не указывали фамилий, а подписывались выбранным <девизом>."
                            }
                        }
                    ],
                    "synonyms": [
                        "identyfikator"
                    ],
                    "common_phrases": [
                        "opatrzyć godłem",
                        "koperta z godłem",
                        "praca oznaczona godłem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Словесный или графический знак, заменяющий подпись автора для сохранения анонимности в конкурсах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девиз",
                                "target_lang_sense_clarification": "псевдоним в конкурсах"
                            },
                            {
                                "target_lang_word": "шифр",
                                "target_lang_sense_clarification": "условное обозначение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ee68366-a86e-3b2a-b04c-601707499d9e",
                    "sense_definition": "wizerunek umieszczony na tarczy herbowej, stanowiący główny element herbu",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "heraldyka",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz dumnie prezentował rodowe <godło> namalowane na swojej tarczy.",
                            "target_lang_translations": {
                                "ru": "Рыцарь с гордостью демонстрировал родовой <герб> (эмблему), нарисованный на его щите."
                            }
                        },
                        {
                            "text": "Historyk sztuki analizował <godło> widniejące na średniowiecznej pieczęci.",
                            "target_lang_translations": {
                                "ru": "Искусствовед анализировал <гербовую фигуру>, изображенную на средневековой печати."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tarcza z godłem",
                        "pole godła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображение, помещенное на гербовом щите, составляющее главный элемент герба"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гербовая фигура",
                                "target_lang_sense_clarification": "элемент на щите"
                            },
                            {
                                "target_lang_word": "эмблема",
                                "target_lang_sense_clarification": "геральдическое изображение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
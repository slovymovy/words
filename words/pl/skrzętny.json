    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d8b2953b-68b7-36b8-92b3-30021c5a1caa",
                    "sense_definition": "Wykonywany z dużą starannością, dbałością o szczegóły i skrupulatnością.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Urzędnik prowadził <skrzętne> zapiski dotyczące wszystkich wydatków miasta.",
                            "target_lang_translations": {
                                "ru": "Чиновник вел <тщательные> записи, касающиеся всех расходов города."
                            }
                        },
                        {
                            "text": "Jego <skrzętne> przygotowania do egzaminu przyniosły oczekiwane rezultaty.",
                            "target_lang_translations": {
                                "ru": "Его <скрупулёзная> подготовка к экзамену принесла ожидаемые результаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrupulatny",
                        "dokładny",
                        "staranny",
                        "wnikliwy"
                    ],
                    "antonyms": [
                        "niedbały",
                        "powierzchowny",
                        "niestaranny"
                    ],
                    "common_phrases": [
                        "skrzętne notatki",
                        "skrzętne badania",
                        "skrzętne ukrywanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполняемый с большим старанием, вниманием к деталям и скрупулезностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тщательный",
                                "target_lang_sense_clarification": "выполненный с большой точностью и вниманием"
                            },
                            {
                                "target_lang_word": "скрупулёзный",
                                "target_lang_sense_clarification": "чрезвычайно точный, до мелочей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fb13f37-d2b0-3986-a041-4353b1e38255",
                    "sense_definition": "Oszczędny, gospodarny, unikający zbędnych wydatków i marnotrawstwa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako <skrzętny> gospodarz, zawsze odkładał część plonów na czarną godzinę.",
                            "target_lang_translations": {
                                "ru": "Как <бережливый> хозяин, он всегда откладывал часть урожая на черный день."
                            }
                        },
                        {
                            "text": "Dzięki temu, że była <skrzętna>, udało jej się szybko zaoszczędzić na własne mieszkanie.",
                            "target_lang_translations": {
                                "ru": "Благодаря тому, что она была <хозяйственной>, ей удалось быстро накопить на собственную квартиру."
                            }
                        }
                    ],
                    "synonyms": [
                        "oszczędny",
                        "gospodarny",
                        "zapobiegliwy"
                    ],
                    "antonyms": [
                        "rozrzutny",
                        "marnotrawny"
                    ],
                    "common_phrases": [
                        "skrzętny gospodarz",
                        "skrzętna pani domu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Экономный, хозяйственный, избегающий лишних трат и расточительства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бережливый",
                                "target_lang_sense_clarification": "умеющий беречь, экономно расходовать"
                            },
                            {
                                "target_lang_word": "хозяйственный",
                                "target_lang_sense_clarification": "умело ведущий хозяйство, запасливый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f488f7eb-a3bf-3dff-9996-041947f81d29",
                    "sense_definition": "Pracowity, pilny, gorliwie zajmujący się swoimi obowiązkami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <skrzętny> rzemieślnik pracował w swoim warsztacie od świtu do nocy.",
                            "target_lang_translations": {
                                "ru": "Этот <усердный> ремесленник работал в своей мастерской от рассвета до ночи."
                            }
                        },
                        {
                            "text": "Obserwował <skrzętne> mrówki, które bezustannie nosiły igły do mrowiska.",
                            "target_lang_translations": {
                                "ru": "Он наблюдал за <трудолюбивыми> муравьями, которые без устали носили иголки в муравейник."
                            }
                        }
                    ],
                    "synonyms": [
                        "pracowity",
                        "pilny",
                        "gorliwy"
                    ],
                    "antonyms": [
                        "leniwy",
                        "gnuśny"
                    ],
                    "common_phrases": [
                        "skrzętna praca",
                        "skrzętny lud"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудолюбивый, прилежный, ревностно занимающийся своими обязанностями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усердный",
                                "target_lang_sense_clarification": "проявляющий большое старание в деле"
                            },
                            {
                                "target_lang_word": "трудолюбивый",
                                "target_lang_sense_clarification": "любящий трудиться"
                            }
                        ]
                    }
                }
            ]
        }
    ]

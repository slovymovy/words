            "senses": [
                {
                    "sense_id": "801c54c2-e29f-329c-a7bc-2b681fb9c36b",
                    "sense_definition": "Mały, słodki wyrób cukierniczy; zdrobnienie od słowa <w>ciastko</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do popołudniowej kawy najlepiej pasuje małe, kruche <w>ciasteczko</w>.",
                            "target_lang_translations": {
                                "ru": "К послеобеденному кофе лучше всего подходит маленькое рассыпчатое <w>печенье</w>."
                            }
                        },
                        {
                            "text": "Ta firma produkuje wyborne <w>ciasteczka</w> z wiśnią i czekoladą.",
                            "target_lang_translations": {
                                "ru": "Эта фирма производит восхитительное <w>печенье</w> с вишней и шоколадом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kruche ciasteczko",
                        "ciasteczko z wróżbą",
                        "piec ciasteczka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькое сладкое кондитерское изделие; уменьшительная форма от слова ciastko."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печенье",
                                "target_lang_sense_clarification": "кондитерское изделие"
                            },
                            {
                                "target_lang_word": "печенька",
                                "target_lang_sense_clarification": "разг. одно печенье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23999e28-5ced-3499-b88f-380bc67e9d97",
                    "sense_definition": "Mały plik tekstowy zapisywany przez przeglądarkę internetową na urządzeniu użytkownika, służący do zapamiętywania danych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Większość stron internetowych prosi o zgodę na zapisywanie plików <w>ciasteczka</w>.",
                            "target_lang_translations": {
                                "ru": "Большинство веб-сайтов просит разрешения на сохранение файлов <w>cookie</w>."
                            }
                        },
                        {
                            "text": "Gdy tylko uda się skraść <w>ciasteczko</w>, można mieć dostęp do konta ofiary bez hasła.",
                            "target_lang_translations": {
                                "ru": "Как только удастся украсть <w>cookie</w>, можно получить доступ к аккаунту жертвы без пароля."
                            }
                        }
                    ],
                    "synonyms": [
                        "cookie"
                    ],
                    "common_phrases": [
                        "pliki ciasteczka",
                        "akceptować ciasteczka",
                        "usuwać ciasteczka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой текстовый файл, сохраняемый веб-браузером на устройстве пользователя для запоминания данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "cookie",
                                "target_lang_sense_clarification": "компьютерный термин"
                            },
                            {
                                "target_lang_word": "куки",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "237745e1-390d-30d6-9c85-6d2bc9d8c34b",
                    "sense_definition": "Osoba bardzo atrakcyjna fizycznie, zazwyczaj młoda kobieta.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "slang_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Widziałeś tę nową dziewczynę w biurze, to niezłe <w>ciasteczko</w>.",
                            "target_lang_translations": {
                                "ru": "Ты видел эту новую девушку в офисе? Она настоящая <w>конфетка</w>."
                            }
                        },
                        {
                            "text": "Ej, <w>ciasteczko</w>, jakbyś tu jeszcze kiedyś była, to odezwij się do mnie.",
                            "target_lang_translations": {
                                "ru": "Эй, <w>красотка</w>, если будешь здесь еще когда-нибудь, дай знать."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciacho"
                    ],
                    "common_phrases": [

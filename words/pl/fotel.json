{
    "word_family": [
        "fotelik",
        "fotelowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3bb0391e-a670-356e-bebf-cace93f7d5ff",
                    "sense_definition": "Wygodny mebel do siedzenia dla jednej osoby, posiadający oparcie na plecy oraz podłokietniki.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziadek usiadł w swoim ulubionym <w>fotelu</w> i zaczął czytać gazetę.",
                            "target_lang_translations": {
                                "ru": "Дедушка сел в своё любимое <w>кресло</w> и начал читать газету."
                            }
                        },
                        {
                            "text": "Do naszego nowego salonu kupiliśmy duży, skórzany <w>fotel</w>.",
                            "target_lang_translations": {
                                "ru": "В нашу новую гостиную мы купили большое кожаное <w>кресло</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) zesel"
                    ],
                    "common_phrases": [
                        "wygodny fotel",
                        "siedzieć w fotelu",
                        "skórzany fotel",
                        "głęboki fotel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удобный предмет мебели для сидения одного человека, имеющий спинку и подлокотники."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кресло",
                                "target_lang_sense_clarification": "мебель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa61a3fe-3014-3a98-b0bd-f5011d718605",
                    "sense_definition": "Pojedyncze, komfortowe miejsce do siedzenia zamontowane w pojeździe, kinie lub teatrze.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed rozpoczęciem jazdy musisz odpowiednio ustawić <w>fotel</w> kierowcy.",
                            "target_lang_translations": {
                                "ru": "Перед началом поездки нужно правильно отрегулировать водительское <w>кресло</w>."
                            }
                        },
                        {
                            "text": "W kinie zajęliśmy miejsca w ostatnim rzędzie, gdzie są najwygodniejsze <w>fotele</w>.",
                            "target_lang_translations": {
                                "ru": "В кино мы заняли места в последнем ряду, где самые удобные <w>кресла</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fotel kierowcy",
                        "fotel pasażera",
                        "tylny fotel",
                        "fotel lotniczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельное комфортабельное сиденье, установленное в транспортном средстве, кинотеатре или театре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кресло",
                                "target_lang_sense_clarification": "в театре, самолете"
                            },
                            {
                                "target_lang_word": "сиденье",
                                "target_lang_sense_clarification": "в автомобиле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04c650d0-1b1d-3ed5-94f5-f019eefd6491",
                    "sense_definition": "Specjalistyczne urządzenie do siedzenia przeznaczone dla pacjenta lub klienta w gabinetach usługowych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent usiadł na <w>fotelu</w> dentystycznym i szeroko otworzył usta.",
                            "target_lang_translations": {
                                "ru": "Пациент сел в стоматологическое <w>кресло</w> и широко открыл рот."
                            }
                        },
                        {
                            "text": "Fryzjer zaprosił klienta na <w>fotel</w> znajdujący się przed dużym lustrem.",
                            "target_lang_translations": {
                                "ru": "Парикмахер пригласил клиента в <w>кресло</w>, стоящее перед большим зеркалом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fotel dentystyczny",
                        "fotel fryzjerski",
                        "fotel ginekologiczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специализированное устройство для сидения, предназначенное для пациента или клиента в кабинетах услуг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кресло",
                                "target_lang_sense_clarification": "стоматологическое, парикмахерское"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "663d4550-7321-3963-983e-f59f9eeb4656",
                    "sense_definition": "Ważne stanowisko, urząd lub funkcja pełniona w organizacji, parlamencie albo instytucji.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Walka polityczna o <w>fotel</w> prezydenta miasta była w tym roku bardzo zacięta.",
                            "target_lang_translations": {
                                "ru": "Политическая борьба за <w>кресло</w> мэра города была в этом году очень ожесточённой."
                            }
                        },
                        {
                            "text": "Po wielu latach ciężkiej pracy w końcu objął <w>fotel</w> prezesa firmy.",
                            "target_lang_translations": {
                                "ru": "После многих лет тяжёлой работы он наконец занял <w>кресло</w> президента компании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fotel prezydenta",
                        "fotel prezesa",
                        "objąć fotel",
                        "stracić fotel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Важная должность, пост или функция, выполняемая в организации, парламенте или учреждении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кресло",
                                "target_lang_sense_clarification": "перен. должность"
                            },
                            {
                                "target_lang_word": "пост",
                                "target_lang_sense_clarification": "должность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
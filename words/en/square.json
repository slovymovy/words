                            {
                                "target_lang_word": "полноценный",
                                "target_lang_sense_clarification": "достаточный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aa522b5d-6636-3190-9d4c-e81f9332f83a",

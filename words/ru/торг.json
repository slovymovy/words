    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f0d2451b-911b-359d-9923-e76ce2c193b1",
                    "sense_definition": "Спор о цене товара с целью её снижения или обсуждение условий сделки.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом магазине цены фиксированные, поэтому <w>торг</w> здесь неуместен.",
                            "target_lang_translations": {
                                "en": "Prices in this store are fixed, so <w>bargaining</w> is inappropriate here."
                            }
                        },
                        {
                            "text": "После долгого <w>торга</w> продавец согласился уступить нам машину дешевле.",
                            "target_lang_translations": {
                                "en": "After lengthy <w>haggling</w>, the seller agreed to let us have the car cheaper."
                            }
                        }
                    ],
                    "common_phrases": [
                        "торг неуместен",
                        "вести торг",
                        "вступить в торг"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of negotiating the price of goods or terms of a transaction to get a better deal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bargaining",
                                "target_lang_sense_clarification": "negotiating for a better price"
                            },
                            {
                                "target_lang_word": "haggling",
                                "target_lang_sense_clarification": "persistent bargaining over small amounts"
                            },
                            {
                                "target_lang_word": "negotiation",
                                "target_lang_sense_clarification": "formal discussion to reach an agreement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94353081-5d71-356c-bf46-a0f3d35acd34",
                    "sense_definition": "Публичная продажа имущества или конкурс на заключение договора, при котором покупателем становится тот, кто предложит лучшие условия (часто используется во множественном числе).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Строительная компания выиграла <w>торги</w> на ремонт городской дороги.",
                            "target_lang_translations": {
                                "en": "The construction company won the <w>tender</w> for the repair of the city road."
                            }
                        },
                        {
                            "text": "Имущество должника было продано с публичного <w>торга</w>.",
                            "target_lang_translations": {
                                "en": "The debtor's property was sold at public <w>auction</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "базар",
                        "рынок",
                        "торжище",
                        "ярмарка"
                    ],
                    "common_phrases": [
                        "выиграть торги",
                        "выставить на торги",
                        "продать с торга",
                        "публичные торги"
                    ],
                    "target_lang_definitions": {
                        "en": "A public sale or competition where a contract or property goes to the best offer (often used in plural)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tender",
                                "target_lang_sense_clarification": "formal offer to carry out work"
                            },
                            {
                                "target_lang_word": "auction",
                                "target_lang_sense_clarification": "public sale to the highest bidder"
                            },
                            {
                                "target_lang_word": "bidding",
                                "target_lang_sense_clarification": "the process of offering prices"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a4445ef-a53b-3b49-9b94-2cefea4e5efb",
                    "sense_definition": "Коммерческая деятельность, процесс купли-продажи товаров.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Между двумя странами шёл оживлённый <w>торг</w> зерном и лесом.",
                            "target_lang_translations": {
                                "en": "There was a lively <w>trade</w> in grain and timber between the two countries."
                            }
                        },
                        {
                            "text": "Этот <w>торг</w> оказался для купца очень выгодным.",
                            "target_lang_translations": {
                                "en": "This <w>trade</w> turned out to be very profitable for the merchant."
                            }
                        }
                    ],
                    "synonyms": [
                        "торговля"
                    ],
                    "common_phrases": [
                        "бойкий торг",
                        "выгодный торг"
                    ],
                    "target_lang_definitions": {
                        "en": "Commercial activity involving the buying and selling of goods."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trade",
                                "target_lang_sense_clarification": "exchange of goods"
                            },
                            {
                                "target_lang_word": "commerce",
                                "target_lang_sense_clarification": "large-scale buying and selling"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a04347d-7f81-3bc0-a771-711e130a8c96",
                    "sense_definition": "Место, где происходит торговля; рынок или базар.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Крестьяне с раннего утра везли свои товары на городской <w>торг</w>.",
                            "target_lang_translations": {
                                "en": "From early morning, the peasants brought their goods to the city <w>market</w>."
                            }
                        },
                        {
                            "text": "Шумный <w>торг</w> на площади затих только к вечеру.",
                            "target_lang_translations": {
                                "en": "The noisy <w>marketplace</w> on the square quieted down only towards evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "рынок",
                        "базар",
                        "ярмарка",
                        "торжище"
                    ],
                    "common_phrases": [
                        "ехать на торг"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Used in historical contexts or literature"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A place where trading occurs; a market or bazaar (archaic or historical context)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "market",
                                "target_lang_sense_clarification": "place for trading"
                            },
                            {
                                "target_lang_word": "marketplace",
                                "target_lang_sense_clarification": "open space where a market is held"
                            }
                        ]
                    }
                }
            ]
        }

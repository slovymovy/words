                {
                    "sense_id": "978eef1b-e067-3120-8126-32b2ce11e5de",
                    "sense_definition": "Een staanplaats achterin een tram, trein of bus, vaak bij de deuren.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

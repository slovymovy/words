    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "585a3c42-ddad-3b61-831e-3cf755b6a3b9",
                    "sense_definition": "Spullen in een tas, koffer of doos doen om ze mee te nemen of te bewaren",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literal_packing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet vanavond mijn koffer nog <w>inpakken</w> voor de vakantie.",
                            "target_lang_translations": {
                                "en": "I still have to <w>pack</w> my suitcase for the vacation tonight.",
                                "ru": "Мне нужно сегодня вечером еще <w>собрать</w> чемодан для отпуска."
                            }
                        },
                        {
                            "text": "Bij de verhuizing hielpen vrienden met het <w>inpakken</w> van de dozen.",
                            "target_lang_translations": {
                                "en": "During the move, friends helped with <w>packing</w> the boxes.",
                                "ru": "При переезде друзья помогали <w>упаковывать</w> коробки."
                            }
                        }
                    ],
                    "antonyms": [
                        "uitpakken"
                    ],
                    "common_phrases": [
                        "je koffer inpakken",
                        "dozen inpakken",
                        "alles inpakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To put items into a bag, suitcase, or box for transport or storage",
                        "ru": "Класть вещи в сумку, чемодан или коробку, чтобы взять их с собой или сохранить"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pack",
                                "target_lang_sense_clarification": "general term for putting things into containers"
                            },
                            {
                                "target_lang_word": "pack up",
                                "target_lang_sense_clarification": "often implies finishing a task or preparing to move"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "упаковывать",
                                "target_lang_sense_clarification": "складывать вещи в коробки или тару"
                            },
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "подготавливать чемодан или сумку к поездке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8700f435-0781-3672-884a-e42a094544b1",
                    "sense_definition": "Iets in papier of folie wikkelen, meestal om het als cadeau te geven",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literal_packing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil je dit boek alsjeblieft feestelijk voor mij <w>inpakken</w>?",
                            "target_lang_translations": {
                                "en": "Could you please <w>wrap</w> this book festively for me?",
                                "ru": "Ты не мог бы празднично <w>упаковать</w> для меня эту книгу?"
                            }
                        },
                        {
                            "text": "Ze heeft het verjaardagscadeau heel netjes <w>ingepakt</w>.",
                            "target_lang_translations": {
                                "en": "She <w>wrapped</w> the birthday gift very neatly.",
                                "ru": "Она очень аккуратно <w>упаковала</w> подарок на день рождения."
                            }
                        }
                    ],
                    "antonyms": [
                        "uitpakken"
                    ],
                    "common_phrases": [
                        "een cadeautje inpakken",
                        "feestelijk inpakken",
                        "mooi inpakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To cover something in paper or material, usually as a gift",
                        "ru": "Заворачивать что-либо в бумагу или фольгу, обычно чтобы подарить"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wrap",
                                "target_lang_sense_clarification": "to cover in paper, especially a gift"
                            },
                            {
                                "target_lang_word": "wrap up",
                                "target_lang_sense_clarification": "phrasal verb for wrapping something completely"
                            },
                            {
                                "target_lang_word": "gift-wrap",
                                "target_lang_sense_clarification": "specifically for presents"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заворачивать",
                                "target_lang_sense_clarification": "обертывать в бумагу"
                            },
                            {
                                "target_lang_word": "упаковывать",
                                "target_lang_sense_clarification": "оформлять подарок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f70a862-c641-3ce3-8055-170e3137039b",
                    "sense_definition": "Weggaan of ergens mee stoppen omdat het geen zin meer heeft",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_leaving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het begint hard te regenen, we kunnen wel <w>inpakken</w>.",
                            "target_lang_translations": {
                                "en": "It's starting to rain heavily, we might as well <w>pack up</w>.",
                                "ru": "Начинается сильный дождь, можно <w>сворачиваться</w>."
                            }
                        },
                        {
                            "text": "Na die grote fout kon de minister wel <w>inpakken</w>.",
                            "target_lang_translations": {
                                "en": "After that big mistake, the minister could just <w>pack his bags</w>.",
                                "ru": "После этой грубой ошибки министру можно было <w>собирать вещи</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertrekken"
                    ],
                    "common_phrases": [
                        "we kunnen wel inpakken",
                        "je biezen pakken"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in the phrase 'we kunnen wel inpakken'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stop an activity or leave, often because of failure or futility",
                        "ru": "Уходить или прекращать что-либо делать, потому что это больше не имеет смысла"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pack up",
                                "target_lang_sense_clarification": "to stop working or leave a place"
                            },
                            {
                                "target_lang_word": "pack it in",
                                "target_lang_sense_clarification": "informal/colloquial: to stop doing something, to give up"
                            },
                            {
                                "target_lang_word": "call it a day",
                                "target_lang_sense_clarification": "idiom: to stop working"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сворачиваться",
                                "target_lang_sense_clarification": "разг. прекращать деятельность"
                            },
                            {
                                "target_lang_word": "собирать вещи",
                                "target_lang_sense_clarification": "перен. уходить, увольняться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca04e08b-3a04-3123-ba34-8085e65a3ed9",
                    "sense_definition": "Iemand voor zich winnen door charme of overtuigingskracht",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_charm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met zijn mooie praatjes wist hij het hele publiek <w>in</w> te <w>pakken</w>.",
                            "target_lang_translations": {
                                "en": "With his smooth talk, he managed to <w>win</w> the whole audience <w>over</w>.",
                                "ru": "Своими красивыми речами он сумел <w>обаять</w> всю публику."
                            }
                        },
                        {
                            "text": "Zij heeft hem volledig <w>ingepakt</w> met haar glimlach.",
                            "target_lang_translations": {
                                "en": "She completely <w>charmed</w> him with her smile.",
                                "ru": "Она полностью <w>очаровала</w> его своей улыбкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iemand inpakken",
                        "het publiek inpakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To win someone over through charm or persuasion",
                        "ru": "Завоевать чье-то расположение с помощью обаяния или убеждения"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "charm",
                                "target_lang_sense_clarification": "to delight or attract someone"
                            },
                            {
                                "target_lang_word": "win over",
                                "target_lang_sense_clarification": "to gain someone's support or favor"
                            },
                            {
                                "target_lang_word": "wrap around one's finger",
                                "target_lang_sense_clarification": "idiom: to gain complete control over someone through charm"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обаять",
                                "target_lang_sense_clarification": "очаровать кого-либо"
                            },
                            {
                                "target_lang_word": "покорить",
                                "target_lang_sense_clarification": "завоевать симпатию"
                            }
                        ]
                    }
                }
            ]
        }

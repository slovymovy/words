    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f15e9c33-1613-324e-9d97-00b4436031a0",
                    "sense_definition": "Крупное хищное млекопитающее семейства кошачьих с пятнистой шкурой; то же, что леопард.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "big_cats",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В зоопарке мы долго наблюдали, как грациозный <w>барс</w> ходит по клетке.",
                            "target_lang_translations": {
                                "en": "At the zoo, we watched for a long time as the graceful <w>leopard</w> paced in its cage."
                            }
                        },
                        {
                            "text": "Великий комбинатор прошёлся по комнате, как <w>барс</w>, готовясь к решительному разговору.",
                            "target_lang_translations": {
                                "en": "The great schemer paced the room like a <w>panther</w>, preparing for the decisive conversation."
                            }
                        }
                    ],
                    "synonyms": [
                        "леопард",
                        "панте́ра"
                    ],
                    "common_phrases": [
                        "шкура барса",
                        "прыжок барса",
                        "пятнистый барс"
                    ],
                    "target_lang_definitions": {
                        "en": "A large carnivorous mammal of the cat family with a spotted coat; same as a leopard."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leopard",
                                "target_lang_sense_clarification": "the spotted wild cat Panthera pardus"
                            },
                            {
                                "target_lang_word": "panther",
                                "target_lang_sense_clarification": "general term for big cats, often used for leopards"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f252e362-42e0-30c4-abc2-c7ba22408108",
                    "sense_definition": "Хищное млекопитающее семейства кошачьих, обитающее в высокогорьях Центральной Азии; то же, что ирбис.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "big_cats",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Снежный <w>барс</w> обитает высоко в горах и редко показывается людям.",
                            "target_lang_translations": {
                                "en": "The <w>snow leopard</w> lives high in the mountains and rarely shows itself to people."
                            }
                        },
                        {
                            "text": "Этот редкий <w>барс</w> занесён в Красную книгу как исчезающий вид.",
                            "target_lang_translations": {
                                "en": "This rare <w>snow leopard</w> is listed in the Red Book as an endangered species."
                            }
                        }
                    ],
                    "synonyms": [
                        "ирбис",
                        "снежный барс"
                    ],
                    "common_phrases": [
                        "снежный барс",
                        "горный барс"
                    ],
                    "target_lang_definitions": {
                        "en": "A carnivorous mammal of the cat family inhabiting the highlands of Central Asia; same as the irbis."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "snow leopard",
                                "target_lang_sense_clarification": "the species Panthera uncia"
                            },
                            {
                                "target_lang_word": "ounce",
                                "target_lang_sense_clarification": "alternative name for the snow leopard"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f0d0242-8c69-3d96-a635-1f448b32d9d6",
                    "sense_definition": "Тяжёлый молот в виде бревна или куска металла, используемый на судах.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Матросы с трудом подняли тяжёлый <w>барс</w> для выполнения работ на палубе.",
                            "target_lang_translations": {
                                "en": "The sailors struggled to lift the heavy <w>sledgehammer</w> to carry out work on the deck."
                            }
                        },
                        {
                            "text": "Старый ржавый <w>барс</w> лежал в углу трюма, забытый командой.",
                            "target_lang_translations": {
                                "en": "The old rusty <w>sledgehammer</w> lay in the corner of the hold, forgotten by the crew."
                            }
                        }
                    ],
                    "synonyms": [
                        "молот"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "nautical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A heavy hammer in the form of a log or piece of metal, used on ships."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sledgehammer",
                                "target_lang_sense_clarification": "heavy tool used for striking"
                            },
                            {
                                "target_lang_word": "maul",
                                "target_lang_sense_clarification": "heavy hammer"
                            }
                        ]
                    }
                }
            ]
        },

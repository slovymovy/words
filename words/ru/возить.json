    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "abd113e8-ec74-38b8-bbae-83c527aec70f",
                    "sense_definition": "Регулярно перемещать кого-либо или что-либо, используя транспортное средство (туда и обратно или в разных направлениях).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое утро отец <w>возит</w> детей в школу на машине.",
                            "target_lang_translations": {
                                "en": "Every morning the father <w>takes</w> the children to school by car."
                            }
                        },
                        {
                            "text": "Этот грузовик <w>возит</w> строительные материалы на новый объект.",
                            "target_lang_translations": {
                                "en": "This truck <w>carries</w> building materials to the new site."
                            }
                        },
                        {
                            "text": "Я не люблю <w>возить</w> с собой много наличных денег в путешествиях.",
                            "target_lang_translations": {
                                "en": "I don't like to <w>carry</w> a lot of cash with me when travelling."
                            }
                        }
                    ],
                    "synonyms": [
                        "перевозить",
                        "транспортировать",
                        "доставлять"
                    ],
                    "common_phrases": [
                        "возить пассажиров",
                        "возить грузы",
                        "возить с собой",
                        "возить на машине",
                        "возить детей"
                    ],
                    "target_lang_definitions": {
                        "en": "To regularly move someone or something using a vehicle (back and forth or in different directions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "to transport goods or materials"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "to transport people"
                            },
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "to transport by vehicle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b799dc7c-7a8c-34b3-81ff-8ae73431d57d",
                    "sense_definition": "Водить предметом по поверхности, часто с нажимом или размазывая что-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хватит <w>возить</w> грязной тряпкой по столу, лучше прополощи её.",
                            "target_lang_translations": {
                                "en": "Stop <w>smearing</w> the dirty rag across the table, you'd better rinse it out."
                            }
                        },
                        {
                            "text": "Маленький ребёнок начал <w>возить</w> ложкой по тарелке, отказываясь есть.",
                            "target_lang_translations": {
                                "en": "The small child started <w>dragging</w> the spoon across the plate, refusing to eat."
                            }
                        }
                    ],
                    "synonyms": [
                        "возюкать",
                        "елозить"
                    ],
                    "common_phrases": [
                        "возить тряпкой",
                        "возить по столу",
                        "возить по полу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "употребляется в разговорной речи"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To move an object across a surface, often with pressure or smearing something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drag",
                                "target_lang_sense_clarification": "to move something across a surface"
                            },
                            {
                                "target_lang_word": "smear",
                                "target_lang_sense_clarification": "to spread messily across a surface"
                            },
                            {
                                "target_lang_word": "rub",
                                "target_lang_sense_clarification": "to move repeatedly with pressure"
                            }
                        ]
                    }
                }
            ]
        }
    ]

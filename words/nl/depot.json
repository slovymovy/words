            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ed75ead-8cdb-3518-b292-93b4fa60a550",
                    "sense_definition": "Een plaats waar goederen, kunstwerken of voertuigen worden opgeslagen of bewaard.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "storage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De inbeslaggenomen goederen gaan voorlopig in <w>depot</w> totdat de rechter een beslissing neemt.",
                            "target_lang_translations": {
                                "en": "The confiscated goods are temporarily going into <w>storage</w> until the judge makes a decision."
                            }
                        },
                        {
                            "text": "Het museum bewaart de meeste schilderijen in een beveiligd <w>depot</w> buiten de stad.",
                            "target_lang_translations": {
                                "en": "The museum keeps most paintings in a secured <w>depot</w> outside the city."
                            }
                        }
                    ],
                    "synonyms": [
                        "opslagplaats",
                        "bewaarplaats",
                        "magazijn",
                        "pakhuis"
                    ],
                    "common_phrases": [
                        "in depot geven",
                        "in depot nemen",
                        "in depot zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "A place where goods, artworks, or vehicles are stored or kept."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "depot",
                                "target_lang_sense_clarification": "storage facility for goods or vehicles"
                            },
                            {
                                "target_lang_word": "storage",
                                "target_lang_sense_clarification": "place or state of keeping items for future use"
                            },
                            {
                                "target_lang_word": "warehouse",
                                "target_lang_sense_clarification": "large building for commercial storage"
                            },
                            {
                                "target_lang_word": "depository",
                                "target_lang_sense_clarification": "place where things are stored for safekeeping"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9794d783-0b80-34bb-9994-edcae93f41b3",
                    "sense_definition": "Het bezinksel dat zich op de bodem van een fles vloeistof, meestal wijn, vormt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sediment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Schenk de oude rode wijn voorzichtig uit zodat het <w>depot</w> in de fles achterblijft.",
                            "target_lang_translations": {
                                "en": "Pour the old red wine out carefully so that the <w>sediment</w> remains in the bottle."
                            }
                        },
                        {
                            "text": "Bij het decanteren scheid je de heldere wijn van het <w>depot</w> op de bodem.",
                            "target_lang_translations": {
                                "en": "When decanting, you separate the clear wine from the <w>deposit</w> on the bottom."
                            }
                        }
                    ],
                    "synonyms": [
                        "droesem",
                        "draf"
                    ],
                    "target_lang_definitions": {
                        "en": "The sediment that forms at the bottom of a bottle of liquid, usually wine."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sediment",
                                "target_lang_sense_clarification": "solid matter settling at the bottom of a liquid"
                            },
                            {
                                "target_lang_word": "deposit",
                                "target_lang_sense_clarification": "accumulation of solid matter"
                            },
                            {
                                "target_lang_word": "dregs",
                                "target_lang_sense_clarification": "remnants at the bottom of a liquid"
                            },
                            {
                                "target_lang_word": "lees",
                                "target_lang_sense_clarification": "sediment of wine in the barrel or bottle"
                            }
                        ]
                    }
                }

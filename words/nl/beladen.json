    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fa2a31cb-7276-38df-bf64-67f554efb3ea",
                    "sense_definition": "Zwaar gevuld met een lading of vracht; volgeladen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_load",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vrachtwagen was zwaar <w>beladen</w> met bouwmaterialen.",
                            "target_lang_translations": {
                                "ru": "Грузовик был тяжело <w>нагружен</w> строительными материалами."
                            }
                        },
                        {
                            "text": "We zagen een ezel die <w>beladen</w> was met zakken graan.",
                            "target_lang_translations": {
                                "ru": "Мы видели осла, который был <w>нагружен</w> мешками с зерном."
                            }
                        }
                    ],
                    "synonyms": [
                        "belasten",
                        "bevrachten",
                        "laden"
                    ],
                    "antonyms": [
                        "onbeladen"
                    ],
                    "common_phrases": [
                        "zwaar beladen",
                        "beladen met"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжело наполненный грузом или ношей; загруженный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нагруженный",
                                "target_lang_sense_clarification": "несущий на себе груз"
                            },
                            {
                                "target_lang_word": "гружёный",
                                "target_lang_sense_clarification": "содержащий груз (о транспорте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96fe42d6-bdbe-3a01-88c2-5fa8a2d4cba2",
                    "sense_definition": "Waarover veel emotie, spanning of discussie bestaat; wat gevoelig ligt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_load",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politieke discussie over dit onderwerp is zeer <w>beladen</w>.",
                            "target_lang_translations": {
                                "ru": "Политическая дискуссия на эту тему очень <w>напряжённая</w>."
                            }
                        },
                        {
                            "text": "Er viel een <w>beladen</w> stilte toen hij het slechte nieuws vertelde.",
                            "target_lang_translations": {
                                "ru": "Наступила <w>тягостная</w> тишина, когда он сообщил плохие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "controversieel",
                        "heikel",
                        "pregnant"
                    ],
                    "antonyms": [
                        "onbeladen"
                    ],
                    "common_phrases": [
                        "een beladen onderwerp",
                        "emotioneel beladen",
                        "politiek beladen",
                        "een beladen term"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий много эмоций, напряжения или споров; чувствительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напряжённый",
                                "target_lang_sense_clarification": "эмоционально тяжёлый"
                            },
                            {
                                "target_lang_word": "щекотливый",
                                "target_lang_sense_clarification": "деликатный, требующий осторожности"
                            },
                            {
                                "target_lang_word": "тягостный",
                                "target_lang_sense_clarification": "гнетущий (о тишине или атмосфере)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "462c6e01-d448-3c78-901f-08751bd51495",
                    "sense_definition": "Een lading of vracht op of in een voertuig, vaartuig of lastdier plaatsen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_load",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De havenarbeiders <w>belaadden</w> het schip met containers voor de lange reis.",
                            "target_lang_translations": {
                                "ru": "Портовые рабочие <w>загружали</w> судно контейнерами для долгого плавания."
                            }
                        },
                        {
                            "text": "Zij <w>belaadden</w> de wagen snel zodat ze voor het donker konden vertrekken.",
                            "target_lang_translations": {
                                "ru": "Они быстро <w>нагрузили</w> повозку, чтобы успеть уехать до темноты."
                            }
                        }
                    ],
                    "synonyms": [
                        "inladen",
                        "laden",
                        "bevrachten",
                        "belasten"
                    ],
                    "antonyms": [
                        "afladen",
                        "uitladen",
                        "ontladen"
                    ],
                    "common_phrases": [
                        "een schip beladen",
                        "een wagen beladen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать груз или ношу на или в транспортное средство, судно или вьючное животное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нагружать",
                                "target_lang_sense_clarification": "класть груз на что-либо"
                            },
                            {
                                "target_lang_word": "загружать",
                                "target_lang_sense_clarification": "наполнять грузом"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
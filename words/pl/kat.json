    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ce45be48-4a69-30f1-9c30-ffb89e4bd3fc",
                    "sense_definition": "Urzędnik sądowy wykonujący wyroki śmierci, tortur lub kar cielesnych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych wiekach <kat> często nosił kaptur, aby ukryć swoją tożsamość.",
                            "target_lang_translations": {
                                "ru": "В старину <палач> часто носил капюшон, чтобы скрыть свою личность."
                            }
                        },
                        {
                            "text": "Tłum zebrał się na rynku, oczekując na przybycie <kata>.",
                            "target_lang_translations": {
                                "ru": "Толпа собралась на рыночной площади, ожидая прибытия <палача>."
                            }
                        }
                    ],
                    "synonyms": [
                        "małodobry",
                        "ceklarz",
                        "oprawca"
                    ],
                    "antonyms": [
                        "skazaniec",
                        "ofiara"
                    ],
                    "common_phrases": [
                        "topór kata",
                        "miecz kata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Судебный исполнитель, приводящий в исполнение смертные приговоры, пытки или телесные наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "палач",
                                "target_lang_sense_clarification": "исполнитель казни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3d4295e-886d-3253-b6ad-295fc58ac916",
                    "sense_definition": "Osoba, która znęca się nad innymi fizycznie lub psychicznie; okrutnik.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dla swoich podwładnych kierownik był prawdziwym <katem>.",
                            "target_lang_translations": {
                                "ru": "Для своих подчиненных начальник был настоящим <палачом>."
                            }
                        },
                        {
                            "text": "Ofiary wojny nigdy nie zapomniały twarzy swojego <kata>.",
                            "target_lang_translations": {
                                "ru": "Жертвы войны никогда не забыли лица своего <мучителя>."
                            }
                        }
                    ],
                    "synonyms": [
                        "oprawca",
                        "siepacz"
                    ],
                    "antonyms": [
                        "ofiara"
                    ],
                    "common_phrases": [
                        "być katem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который физически или морально издевается над другими; жестокий человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мучитель",
                                "target_lang_sense_clarification": "тот, кто причиняет страдания"
                            },
                            {
                                "target_lang_word": "палач",
                                "target_lang_sense_clarification": "перен. жестокий человек"
                            },
                            {
                                "target_lang_word": "изверг",
                                "target_lang_sense_clarification": "крайне жестокий человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7d0d6d8-f262-3caf-ac71-d00b93fc2a6d",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>kata</w> (ćwiczenie formalne w sztukach walki).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas treningu karate uczniowie ćwiczyli sekwencje różnych <kat>.",
                            "target_lang_translations": {
                                "ru": "Во время тренировки по карате ученики отрабатывали последовательности различных <ката>."
                            }
                        },
                        {
                            "text": "Sędziowie oceniali precyzję wykonania wszystkich <kat>.",
                            "target_lang_translations": {
                                "ru": "Судьи оценивали точность выполнения всех <ката>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "dopełniacz liczby mnogiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа слова «kata» (формальное упражнение в боевых искусствах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ката",
                                "target_lang_sense_clarification": "упражнение в боевых искусствах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb2fa621-911b-3355-8cc6-0a28dcd44a67",
                    "sense_definition": "Regionalna nazwa małej ryby słodkowodnej, znanej jako ciernik.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_fish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wędkarz złowił w stawie małego <kata>.",
                            "target_lang_translations": {
                                "ru": "Рыбак поймал в пруду маленькую <колюшку>."
                            }
                        },
                        {
                            "text": "W tej rzece rzadko można spotkać <katy>.",
                            "target_lang_translations": {
                                "ru": "В этой реке редко можно встретить <колюшек>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciernik",
                        "kolka",
                        "czart"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "nazwa ludowa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное название мелкой пресноводной рыбы, известной как колюшка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колюшка",
                                "target_lang_sense_clarification": "рыба"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "256d6486-d507-3cdd-9cff-b46bfda6f4df",
                    "sense_definition": "Ręczna, krótka broń palna, zasilana z magazynka lub jednostrzałowa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policjant pewnym ruchem schował służbowy <w>pistolet</w> do kabury.",
                            "target_lang_translations": {
                                "ru": "Полицейский уверенным движением спрятал служебный <w>пистолет</w> в кобуру."
                            }
                        },
                        {
                            "text": "W upalne dni dzieci uwielbiają bawić się <w>pistoletem</w> na wodę.",
                            "target_lang_translations": {
                                "ru": "В жаркие дни дети обожают играть с водяным <w>пистолетом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "spluwa",
                        "klamka",
                        "gnat",
                        "pukawka",
                        "kopyto"
                    ],
                    "common_phrases": [
                        "pistolet maszynowy",
                        "pistolet na wodę",
                        "pistolet hukowy",
                        "kabura na pistolet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ручное короткоствольное огнестрельное оружие, магазинное или однозарядное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пистолет",
                                "target_lang_sense_clarification": "оружие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a43c4af-5b5d-3fa7-8767-cd56b139d68f",
                    "sense_definition": "Narzędzie w kształcie pistoletu, służące na przykład do malowania, klejenia lub pompowania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do naprawy tego drewnianego krzesła przyda ci się <w>pistolet</w> do kleju.",
                            "target_lang_translations": {
                                "ru": "Для починки этого деревянного стула тебе пригодится клеевой <w>пистолет</w>."
                            }
                        },
                        {
                            "text": "Lakiernik napełnił <w>pistolet</w> farbą i zaczął malować karoserię samochodu.",
                            "target_lang_translations": {
                                "ru": "Маляр наполнил <w>пистолет</w> краской и начал красить кузов автомобиля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pistolet do kleju",
                        "pistolet lakierniczy",
                        "pistolet natryskowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструмент в форме пистолета, используемый, например, для покраски, склеивания или накачивания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пистолет",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "238b8f8a-dfe3-37e1-897e-1935cebe9841",
                    "sense_definition": "Osoba bardzo energiczna, szybka i rzutka.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy pracownik to prawdziwy <w>pistolet</w>, załatwia sprawy błyskawicznie.",
                            "target_lang_translations": {
                                "ru": "Этот новый сотрудник — настоящий <w>живчик</w>, решает дела молниеносно."
                            }
                        },
                        {
                            "text": "Trudno rywalizować z takimi młodymi <w>pistoletami</w>, którzy mają mnóstwo energii.",
                            "target_lang_translations": {
                                "ru": "Трудно соперничать с такими молодыми <w>шустряками</w>, у которых куча энергии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niezły pistolet",
                        "młody pistolet"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "określenie osoby"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень энергичный, быстрый и бойкий человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живчик",
                                "target_lang_sense_clarification": "энергичный человек"
                            },
                            {
                                "target_lang_word": "шустряк",
                                "target_lang_sense_clarification": "бойкий человек"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
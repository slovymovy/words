                {
                    "sense_id": "bc3a3bbe-a0f4-39de-bc82-168dc411141d",
                    "sense_definition": "imię męskie pochodzenia biblijnego",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mam na imię <w>Ariel</w> i mieszkam wraz z rodziną w Lądku-Zdroju.",
                            "target_lang_translations": {
                                "ru": "Меня зовут <w>Ариэль</w>, и я живу вместе с семьей в Лёндек-Здруе."
                            }
                        },
                        {
                            "text": "Mój kolega <w>Ariel</w> świetnie gra na gitarze elektrycznej.",
                            "target_lang_translations": {
                                "ru": "Мой друг <w>Ариэль</w> отлично играет на электрогитаре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mały Ariel",
                        "wujek Ariel"
                    ],
                    "target_lang_definitions": {
                        "ru": "мужское имя библейского происхождения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ариэль",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8c5f7f9-7b94-3295-a3ea-c07626c1809e",
                    "sense_definition": "imię żeńskie (rzadziej spotykana forma, częściej występuje jako Ariela)",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Bohaterką tej powieści jest dziewczyna o imieniu <w>Ariel</w>.",
                            "target_lang_translations": {
                                "ru": "Героиня этого романа — девушка по имени <w>Ариэль</w>."
                            }
                        },
                        {
                            "text": "Na liście gości znalazła się <w>Ariel</w> z Francji.",
                            "target_lang_translations": {
                                "ru": "В списке гостей оказалась <w>Ариэль</w> из Франции."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "женское имя (встречается реже, чаще используется форма Ариэла)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ариэль",
                                "target_lang_sense_clarification": "женское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c7b17fe-09b6-39d0-baa8-71fb759d01da",
                    "sense_definition": "najjaśniejszy z księżyców planety Uran",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Astronomowie skierowali teleskopy na orbitę, po której krąży <w>Ariel</w>.",
                            "target_lang_translations": {
                                "ru": "Астрономы направили телескопы на орбиту, по которой вращается <w>Ариэль</w>."
                            }
                        },
                        {
                            "text": "Powierzchnia, którą ma <w>Ariel</w>, jest pokryta licznymi kraterami i dolinami.",
                            "target_lang_translations": {
                                "ru": "Поверхность, которую имеет <w>Ариэль</w>, покрыта многочисленными кратерами и долинами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "księżyc Ariel",
                        "powierzchnia Ariela"
                    ],
                    "target_lang_definitions": {
                        "ru": "самый яркий из спутников планеты Уран"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ариэль",
                                "target_lang_sense_clarification": "спутник Урана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04a26ddb-3ecc-3d8b-aa3e-5378c9352a44",
                    "sense_definition": "jeden z aniołów występujących w tradycji biblijnej i judaistycznej",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "W dawnych pismach <w>Ariel</w> bywa przedstawiany jako strażnik natury.",
                            "target_lang_translations": {
                                "ru": "В древних писаниях <w>Ариэль</w> иногда изображается как хранитель природы."
                            }
                        },
                        {
                            "text": "Legenda głosi, że anioł <w>Ariel</w> zstąpił na ziemię.",
                            "target_lang_translations": {
                                "ru": "Легенда гласит, что ангел <w>Ариэль</w> спустился на землю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "anioł Ariel"
                    ],
                    "target_lang_definitions": {

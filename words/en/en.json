    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "e09b1567-5eb8-3e76-acae-5cc3cb6c3bae",
                    "sense_definition": "A preposition used in various fixed phrases borrowed from French, typically meaning 'in', 'on', or 'into'.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We stopped for a quick lunch while <w>en</w> route to the airport.",
                            "target_lang_translations": {
                                "ru": "Мы остановились быстро пообедать, находясь <w>в</w> пути в аэропорт."
                            }
                        },
                        {
                            "text": "The protesters marched <w>en</w> masse towards the city center.",
                            "target_lang_translations": {
                                "ru": "Протестующие <w>в</w> полном составе двинулись к центру города."
                            }
                        }
                    ],
                    "common_phrases": [
                        "en route",
                        "en masse",
                        "en suite",
                        "en vogue",
                        "en garde"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлог, используемый в различных устойчивых фразах, заимствованных из французского языка; обычно означает «в», «на» или «внутрь»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "в составе устойчивых выражений (напр., en route)"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "в составе устойчивых выражений"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e2d21558-eb3d-30da-87ec-785cb22cc5ed",
                    "sense_definition": "The written name of the letter N/n.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "letter_name",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The word 'nine' is spelled with two <w>ens</w>.",
                            "target_lang_translations": {
                                "ru": "Слово «nine» пишется с двумя <w>эн</w>."
                            }
                        },
                        {
                            "text": "Make sure your handwriting is clear, especially the difference between an em and an <w>en</w>.",
                            "target_lang_translations": {
                                "ru": "Следи за разборчивостью почерка, особенно за разницей между эм и <w>эн</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название латинской буквы N/n."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эн",
                                "target_lang_sense_clarification": "название буквы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1df7e1fa-b93a-3d83-93cf-c4a5cabf1a22",
                    "sense_definition": "A unit of measurement in typography equal to half the width of an em, roughly the width of the letter 'n'.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "typography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "An <w>en</w> dash is used to represent a range of numbers, such as 1990–2000.",
                            "target_lang_translations": {
                                "ru": "<w>Среднее</w> тире используется для обозначения диапазона чисел, например, 1990–2000."
                            }
                        },
                        {
                            "text": "The typesetter adjusted the spacing by adding a single <w>en</w>.",
                            "target_lang_translations": {
                                "ru": "Наборщик скорректировал интервал, добавив одну <w>полукруглую</w> шпацию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "en dash",
                        "en space"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения в типографике, равная половине ширины круглой шпации (em), примерно равна ширине буквы «n»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полукруглая шпация",
                                "target_lang_sense_clarification": "типографская единица измерения"
                            },
                            {
                                "target_lang_word": "эн",
                                "target_lang_sense_clarification": "профессиональный жаргон"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "973253d7-628c-3c82-8a8b-126a67c642ec",
                    "sense_definition": "Him or it; an object pronoun used in some dialects to refer to a male person or a masculine object.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you see the old farmer, tell <w>en</w> I said hello.",
                            "target_lang_translations": {
                                "ru": "Если увидишь старого фермера, передай <w>ему</w> привет от меня."
                            }
                        },
                        {
                            "text": "That's a fine tractor; I've seen <w>en</w> working in the fields all day.",
                            "target_lang_translations": {
                                "ru": "Отличный трактор; я видел <w>его</w> в работе на полях весь день."
                            }
                        }
                    ],
                    "synonyms": [
                        "him"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Southwest England"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Он или его; объектное местоимение, используемое в некоторых диалектах для обозначения лица мужского пола или предмета мужского рода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "его",
                                "target_lang_sense_clarification": "диалектное"
                            },
                            {
                                "target_lang_word": "он",
                                "target_lang_sense_clarification": "диалектное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

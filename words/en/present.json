{
    "word_family": [
        "presence",
        "presentation",
        "presenter",
        "presently",
        "presentable"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4b684c44-5d77-3ef5-8b65-61d6dc261b26",
                    "sense_definition": "Relating to the current time or moment; happening now.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>present</w> situation is difficult for everyone involved.",
                            "target_lang_translations": {
                                "ru": "<w>Нынешняя</w> ситуация трудна для всех участников."
                            }
                        },
                        {
                            "text": "We need to focus on the <w>present</w> day rather than the past.",
                            "target_lang_translations": {
                                "ru": "Нам нужно сосредоточиться на <w>сегодняшнем</w> дне, а не на прошлом."
                            }
                        }
                    ],
                    "synonyms": [
                        "current",
                        "present-day",
                        "contemporary",
                        "modern"
                    ],
                    "antonyms": [
                        "past",
                        "future"
                    ],
                    "common_phrases": [
                        "present day",
                        "present time",
                        "present situation",
                        "present moment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к текущему времени или моменту; происходящий сейчас."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нынешний",
                                "target_lang_sense_clarification": "относящийся к данному времени"
                            },
                            {
                                "target_lang_word": "настоящий",
                                "target_lang_sense_clarification": "текущий, происходящий сейчас"
                            },
                            {
                                "target_lang_word": "текущий",
                                "target_lang_sense_clarification": "происходящий в данный момент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da5aab52-d1f4-35d2-aa81-a745f763d33a",
                    "sense_definition": "Located in a specific place; not absent.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "All students must be <w>present</w> for the final exam.",
                            "target_lang_translations": {
                                "ru": "Все студенты должны <w>присутствовать</w> на выпускном экзамене."
                            }
                        },
                        {
                            "text": "Several family members were <w>present</w> at the wedding.",
                            "target_lang_translations": {
                                "ru": "Несколько членов семьи <w>присутствовали</w> на свадьбе."
                            }
                        }
                    ],
                    "synonyms": [
                        "nearby",
                        "near",
                        "close"
                    ],
                    "antonyms": [
                        "absent"
                    ],
                    "common_phrases": [
                        "be present",
                        "present at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в определенном месте; не отсутствующий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присутствующий",
                                "target_lang_sense_clarification": "находящийся в данном месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e003de87-2598-3b59-a1b2-5826b08aad93",
                    "sense_definition": "Mentally attentive, alert, and focused on what is happening.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You seem distracted; please try to be more <w>present</w> during the conversation.",
                            "target_lang_translations": {
                                "ru": "Ты кажешься рассеянным; пожалуйста, постарайся быть более <w>внимательным</w> во время разговора."
                            }
                        },
                        {
                            "text": "Meditation helps you stay <w>present</w> and aware of your surroundings.",
                            "target_lang_translations": {
                                "ru": "Медитация помогает оставаться <w>сосредоточенным</w> и внимательным к окружению."
                            }
                        }
                    ],
                    "synonyms": [
                        "audient",
                        "heedful"
                    ],
                    "antonyms": [
                        "distracted",
                        "inattentive"
                    ],
                    "common_phrases": [
                        "stay present",
                        "be present"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ментально внимательный, вовлеченный и сосредоточенный на происходящем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сосредоточенный",
                                "target_lang_sense_clarification": "внимательный к текущему моменту"
                            },
                            {
                                "target_lang_word": "внимательный",
                                "target_lang_sense_clarification": "не отвлекающийся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39757fdd-296f-36e3-a93f-29b37cafac2e",
                    "sense_definition": "Used to refer to the specific thing (such as a document or study) that is currently being discussed or written.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The results of the <w>present</w> study confirm earlier findings.",
                            "target_lang_translations": {
                                "ru": "Результаты <w>данного</w> исследования подтверждают ранние выводы."
                            }
                        },
                        {
                            "text": "In the <w>present</w> case, the court must decide on the liability.",
                            "target_lang_translations": {
                                "ru": "В <w>настоящем</w> деле суд должен принять решение об ответственности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the present study",
                        "the present case",
                        "the present article"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения конкретного предмета (например, документа или исследования), который обсуждается в данный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "данный",
                                "target_lang_sense_clarification": "этот конкретный"
                            },
                            {
                                "target_lang_word": "настоящий",
                                "target_lang_sense_clarification": "официально: этот"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "24c0acc4-abdd-321e-8453-098da6bc2824",
                    "sense_definition": "A gift given to someone on a special occasion.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "gift_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She received a lovely <w>present</w> for her birthday.",
                            "target_lang_translations": {
                                "ru": "Она получила чудесный <w>подарок</w> на день рождения."
                            }
                        },
                        {
                            "text": "I need to buy a Christmas <w>present</w> for my brother.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить рождественский <w>подарок</w> брату."
                            }
                        }
                    ],
                    "common_phrases": [
                        "birthday present",
                        "Christmas present",
                        "give a present",
                        "buy a present"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещь, которую дарят кому-то по особому случаю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подарок",
                                "target_lang_sense_clarification": "дар"

{
    "word_family": [
        "podlewać",
        "podlewaczka",
        "podlanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "791d2776-d612-3637-9453-283e1e76bf35",
                    "sense_definition": "Dostarczyć wody roślinom, wlewając ją do ziemi, w której rosną.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <w>podlać</w> kwiaty, ponieważ ziemia w doniczkach jest już bardzo sucha.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>полить</w> цветы, так как земля в горшках уже очень сухая."
                            }
                        },
                        {
                            "text": "Przed wyjazdem na wakacje poprosiłem sąsiada, aby <w>podlał</w> mój ogródek.",
                            "target_lang_translations": {
                                "ru": "Перед отъездом в отпуск я попросил соседа, чтобы он <w>полил</w> мой огород."
                            }
                        }
                    ],
                    "synonyms": [
                        "nawodnić"
                    ],
                    "common_phrases": [
                        "podlać kwiaty",
                        "podlać ogródek",
                        "podlać trawnik",
                        "podlać rośliny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечить растения водой, наливая её в землю, в которой они растут."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полить",
                                "target_lang_sense_clarification": "растения, цветы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05d6bc10-dd0e-36db-acc6-084c5b347a59",
                    "sense_definition": "Dodać niewielką ilość płynu (np. wody, wina, sosu) do potrawy w trakcie jej gotowania lub pieczenia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kucharz postanowił <w>podlać</w> duszone mięso czerwonym winem, aby wzbogacić jego smak.",
                            "target_lang_translations": {
                                "ru": "Повар решил <w>полить</w> тушеное мясо красным вином, чтобы обогатить его вкус."
                            }
                        },
                        {
                            "text": "Warto <w>podlać</w> pieczeń wytworzonym sosem, żeby nie wyschła w piekarniku.",
                            "target_lang_translations": {
                                "ru": "Стоит <w>полить</w> жаркое образовавшимся соусом, чтобы оно не высохло в духовке."
                            }
                        }
                    ],
                    "synonyms": [
                        "dolać"
                    ],
                    "common_phrases": [
                        "podlać sosem",
                        "podlać winem",
                        "podlać bulionem",
                        "podlać wodą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавить небольшое количество жидкости (например, воды, вина, соуса) в блюдо во время его приготовления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полить",
                                "target_lang_sense_clarification": "еду соусом или жидкостью"
                            },
                            {
                                "target_lang_word": "подлить",
                                "target_lang_sense_clarification": "добавить немного жидкости"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
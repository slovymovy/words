                            {
                                "target_lang_word": "continue",
                                "target_lang_sense_clarification": "to proceed with"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "продолжаешь",
                                "target_lang_sense_clarification": "действие продолжения"
                            },
                            {
                                "target_lang_word": "преследуешь",
                                "target_lang_sense_clarification": "действие преследования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6172d712-1424-32bd-a5de-d7babb8c3b12",
                    "sense_definition": "Gebiedende wijs van <w>vervolgen</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Vervolg</w> je werk en laat je niet afleiden door het lawaai.",
                            "target_lang_translations": {
                                "en": "<w>Continue</w> your work and don't let the noise distract you.",
                                "ru": "<w>Продолжай</w> работу и не отвлекайся на шум."
                            }
                        },
                        {
                            "text": "<w>Vervolg</w> die auto onmiddellijk, hij mag niet ontsnappen!",
                            "target_lang_translations": {

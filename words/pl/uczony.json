    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5804fe4c-74e4-3108-8a5f-f9b92dcb9ac7",
                    "sense_definition": "Osoba posiadająca rozległą wiedzę i zajmująca się zawodowo prowadzeniem badań naukowych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "science_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany <w>uczony</w> otrzymał wczoraj prestiżową nagrodę za swoje odkrycia w dziedzinie fizyki.",
                            "target_lang_translations": {
                                "ru": "Известный <w>учёный</w> вчера получил престижную награду за свои открытия в области физики."
                            }
                        },
                        {
                            "text": "Wielu młodych studentów marzy o tym, by w przyszłości zostać wybitnym <w>uczonym</w>.",
                            "target_lang_translations": {
                                "ru": "Многие молодые студенты мечтают о том, чтобы в будущем стать выдающимся <w>учёным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "naukowiec",
                        "badacz"
                    ],
                    "common_phrases": [
                        "wybitny uczony",
                        "wielki uczony",
                        "radziecki uczony",
                        "szanowany uczony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обладающий обширными знаниями и профессионально занимающийся научными исследованиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учёный",
                                "target_lang_sense_clarification": "специалист, занимающийся наукой"
                            },
                            {
                                "target_lang_word": "научный работник",
                                "target_lang_sense_clarification": "исследователь"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2815d1ce-afbe-3d27-ad28-ad2bc0ff3d85",
                    "sense_definition": "O człowieku: posiadający gruntowne wykształcenie, dużą wiedzę i mądrość.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intellect_characteristic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek był bardzo <w>uczonym</w> człowiekiem, który znał biegle pięć języków obcych.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был очень <w>образованным</w> человеком, который свободно владел пятью иностранными языками."
                            }
                        },
                        {
                            "text": "Nawet najbardziej <w>uczony</w> profesor może czasem nie znać odpowiedzi na proste pytanie.",
                            "target_lang_translations": {
                                "ru": "Даже самый <w>учёный</w> профессор может иногда не знать ответа на простой вопрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "naukowiec",
                        "badacz"
                    ],
                    "common_phrases": [
                        "uczony mąż",
                        "uczony człowiek",
                        "wielce uczony"
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке: обладающий глубоким образованием, большими знаниями и мудростью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учёный",
                                "target_lang_sense_clarification": "обладающий большими знаниями, начитанный"
                            },
                            {
                                "target_lang_word": "образованный",
                                "target_lang_sense_clarification": "имеющий хорошее образование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "167e7bf1-405e-3c11-a223-6c782c75feb3",
                    "sense_definition": "O rzeczy, wypowiedzi lub zachowaniu: świadczący o dużej wiedzy, poważny lub skomplikowany.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intellect_characteristic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Student zrobił bardzo <w>uczoną</w> minę, chociaż nie miał pojęcia, o czym mówi wykładowca.",
                            "target_lang_translations": {
                                "ru": "Студент сделал очень <w>умный</w> вид, хотя понятия не имел, о чём говорит преподаватель."
                            }
                        },
                        {
                            "text": "Nie lubię czytać zbyt <w>uczonych</w> książek, które są napisane trudnym i niezrozumiałym językiem.",
                            "target_lang_translations": {
                                "ru": "Я не люблю читать слишком <w>заумные</w> книги, которые написаны трудным и непонятным языком."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uczona mina",
                        "uczona dysputa",
                        "uczone słowa",
                        "uczone księgi"
                    ],
                    "target_lang_definitions": {
                        "ru": "О предмете, высказывании или поведении: свидетельствующий о больших знаниях, серьезный или сложный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умный",
                                "target_lang_sense_clarification": "выражающий интеллект (о виде, лице)"
                            },
                            {
                                "target_lang_word": "заумный",
                                "target_lang_sense_clarification": "сложный для понимания, книжный"
                            },
                            {
                                "target_lang_word": "научный",
                                "target_lang_sense_clarification": "относящийся к науке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cf428d82-ad89-3a71-86cc-d18b85407801",
                    "sense_definition": "Imiesłów przymiotnikowy bierny od czasownika <w>uczyć</w>: taki, któremu przekazywana jest wiedza lub umiejętności.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "education_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten pies był <w>uczony</w> posłuszeństwa przez najlepszego trenera w całym mieście.",
                            "target_lang_translations": {
                                "ru": "Эту собаку <w>обучал</w> послушанию лучший тренер в городе."
                            }

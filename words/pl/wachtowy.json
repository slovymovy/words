{
    "word_family": [
        "wachta"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "91cdc7f2-2b2c-39a9-a002-57ccbb15f29c",
                    "sense_definition": "Dotyczący wachty, czyli dyżuru pełnionego na statku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oficer <w>wachtowy</w> odpowiada za bezpieczeństwo statku oraz załogi podczas swojego dyżuru.",
                            "target_lang_translations": {
                                "ru": "<w>Вахтенный</w> офицер отвечает за безопасность судна и экипажа во время своего дежурства."
                            }
                        },
                        {
                            "text": "Każde ważne zdarzenie na morzu musi zostać wpisane w dziennik <w>wachtowy</w>.",
                            "target_lang_translations": {
                                "ru": "Каждое важное событие в море должно быть записано в <w>вахтенный</w> журнал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oficer wachtowy",
                        "dziennik wachtowy",
                        "służba wachtowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к вахте, то есть дежурству на судне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вахтенный",
                                "target_lang_sense_clarification": "связанный с дежурством на корабле"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "db6f9e90-00dc-3b59-a122-42fd438ae488",
                    "sense_definition": "Marynarz pełniący w danym momencie wachtę (dyżur) na statku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_roles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wachtowy</w> zauważył światła innego statku na horyzoncie i zameldował o tym kapitanowi.",
                            "target_lang_translations": {
                                "ru": "<w>Вахтенный</w> заметил огни другого судна на горизонте и доложил об этом капитану."
                            }
                        },
                        {
                            "text": "Po czterech godzinach służby zmęczony <w>wachtowy</w> zszedł pod pokład na odpoczynek.",
                            "target_lang_translations": {
                                "ru": "После четырех часов службы уставший <w>вахтенный</w> спустился под палубу на отдых."
                            }
                        }
                    ],
                    "synonyms": [
                        "wachtarz"
                    ],
                    "common_phrases": [
                        "obowiązki wachtowego",
                        "czujny wachtowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Моряк, несущий вахту (дежурство) на судне в данный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вахтенный",
                                "target_lang_sense_clarification": "человек на вахте"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        "бросать"
                    ],
                    "common_phrases": [
                        "поддерживать под руку",
                        "поддерживать конструкцию",
                        "поддерживать голову"
                    ],
                    "target_lang_definitions": {
                        "en": "To serve as a physical support for someone or something, preventing it from falling."
                    },
                    "translations": {

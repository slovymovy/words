    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "92f28529-97ad-37b2-8031-3c19ae649291",
                    "sense_definition": "A regular sum of money paid by the government or a former employer to a person who has retired from work.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many elderly people depend entirely on their <w>pension</w> to pay the bills."
                        },
                        {
                            "text": "He contributes a portion of his salary every month to a private <w>pension</w>."
                        }
                    ],
                    "synonyms": [
                        "superannuation"
                    ],
                    "common_phrases": [
                        "state pension",
                        "retirement pension",
                        "private pension",
                        "receive a pension"
                    ]
                },
                {
                    "sense_id": "803c07ec-3945-39da-82d9-ae1da2c158a7",
                    "sense_definition": "A small, relatively cheap hotel or boarding house, especially in continental Europe, that usually provides breakfast and sometimes other meals.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We stayed at a charming family-run <w>pension</w> in the south of France."
                        },
                        {
                            "text": "The <w>pension</w> was much quieter and more affordable than the large hotels in the city center."
                        }
                    ],
                    "synonyms": [
                        "bed and breakfast",
                        "lodging place"
                    ],
                    "common_phrases": [
                        "stay at a pension"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used especially in continental Europe"
                        }
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5fa865cd-8133-39a2-81e1-ad4a3376d69e",
                    "sense_definition": "To force someone to retire from their job and pay them a pension, often because they are old or no longer needed.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company decided to <w>pension</w> off several older employees to reduce costs."
                        },
                        {
                            "text": "After thirty years of service, he was <w>pensioned</w> off with a generous package."
                        }
                    ],
                    "synonyms": [
                        "pension off"
                    ],
                    "common_phrases": [
                        "pension off"
                    ]
                },
                {
                    "sense_id": "3615779a-7cab-32c8-a178-167fd7d6a8a2",
                    "sense_definition": "To grant or pay a pension to someone.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government <w>pensions</w> soldiers who have been injured in the line of duty."
                        },
                        {
                            "text": "She was <w>pensioned</w> by the state after her long career in public service."
                        }
                    ]
                }
            ]
        }
    ]

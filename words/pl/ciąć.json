                {
                    "sense_id": "503e8a9e-6d6b-39d3-ae71-82cc607d0cb7",
                    "sense_definition": "Dzielić materiał na części lub robić w nim nacięcia przy użyciu ostrego narzędzia",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_cutting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz ostrożnie <ciąć> ten materiał, aby nie zniszczyć wzoru.",
                            "target_lang_translations": {
                                "ru": "Ты должен осторожно <резать> этот материал, чтобы не испортить узор."
                            }
                        },
                        {
                            "text": "Kucharz zaczął szybko <ciąć> warzywa na drobną kostkę.",
                            "target_lang_translations": {
                                "ru": "Повар начал быстро <резать> овощи на мелкие кубики."
                            }
                        }
                    ],
                    "synonyms": [
                        "kroić",
                        "przecinać",
                        "ciachać"
                    ],
                    "antonyms": [
                        "łączyć",
                        "sklejać",
                        "zszywać"
                    ],
                    "common_phrases": [
                        "ciąć nożem",
                        "ciąć na kawałki",
                        "ciąć papier",
                        "ciąć drewno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разделять материал на части или делать в нем надрезы с помощью острого инструмента"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резать",
                                "target_lang_sense_clarification": "разделять на части ножом или ножницами"
                            },
                            {
                                "target_lang_word": "разрезать",
                                "target_lang_sense_clarification": "делить на куски"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1dad431-8908-3500-b92a-17a1f50957e1",
                    "sense_definition": "Ranić własne ciało ostrym przedmiotem (często przypadkowo)",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_harm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze <tnę się>, gdy używam tej starej maszynki do golenia.",
                            "target_lang_translations": {
                                "ru": "Я всегда <режусь>, когда пользуюсь этой старой бритвой."
                            }
                        },
                        {
                            "text": "Uważaj z tą kartką, bo łatwo się nią <ciąć> w palce.",
                            "target_lang_translations": {
                                "ru": "Осторожно с этим листом, им легко <порезаться>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kaleczyć się",
                        "zacinać"
                    ],
                    "common_phrases": [
                        "ciąć się przy goleniu",
                        "ciąć się szkłem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ранить собственное тело острым предметом (часто случайно)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резаться",
                                "target_lang_sense_clarification": "ранить себя (несов. вид)"

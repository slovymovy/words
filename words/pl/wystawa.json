    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2ee1f037-4456-382a-a741-9d9bf6e64741",
                    "sense_definition": "Publiczna prezentacja przedmiotów, takich jak dzieła sztuki, towary lub eksponaty historyczne, zorganizowana w określonym miejscu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "prezentacja",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj poszliśmy całą klasą na ciekawą <w>wystawę</w> fotografii przyrodniczej.",
                            "target_lang_translations": {
                                "ru": "Вчера мы всем классом ходили на интересную <w>выставку</w> природной фотографии."
                            }
                        },
                        {
                            "text": "Muzeum Narodowe przygotowało nową <w>wystawę</w> poświęconą malarstwu impresjonistów.",
                            "target_lang_translations": {
                                "ru": "Национальный музей подготовил новую <w>выставку</w>, посвященную живописи импрессионистов."
                            }
                        }
                    ],
                    "synonyms": [
                        "ekspozycja",
                        "pokaz"
                    ],
                    "common_phrases": [
                        "wystawa sztuki",
                        "wystawa psów rasowych",
                        "iść na wystawę",
                        "otwarcie wystawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичная демонстрация предметов, таких как произведения искусства, товары или исторические экспонаты, организованная в определенном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выставка",
                                "target_lang_sense_clarification": "мероприятие"
                            },
                            {
                                "target_lang_word": "экспозиция",
                                "target_lang_sense_clarification": "собрание экспонатов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2625eb5e-baa4-356c-a350-1fa28b12a2f6",
                    "sense_definition": "Przeszklone miejsce w oknie sklepowym, w którym ustawione są towary, aby zachęcić klientów do ich zakupu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "prezentacja",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zatrzymała się przed sklepem, ponieważ spodobała jej się sukienka na <w>wystawie</w>.",
                            "target_lang_translations": {
                                "ru": "Она остановилась перед магазином, так как ей понравилось платье на <w>витрине</w>."
                            }
                        },
                        {
                            "text": "Pracownicy sklepu często zmieniają dekorację <w>wystawy</w> przed świętami Bożego Narodzenia.",
                            "target_lang_translations": {
                                "ru": "Работники магазина часто меняют оформление <w>витрины</w> перед Рождеством."
                            }
                        }
                    ],
                    "synonyms": [
                        "witryna"
                    ],
                    "common_phrases": [
                        "oglądać wystawy",
                        "towar z wystawy",
                        "stać na wystawie",
                        "manekin na wystawie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Застекленное пространство в окне магазина, где выставлены товары для привлечения покупателей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "витрина",
                                "target_lang_sense_clarification": "окно магазина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb3cd992-119c-3463-b234-c94d67159bb2",
                    "sense_definition": "Położenie budynku, okna lub terenu względem stron świata, określające stopień nasłonecznienia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "kierunek",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkanie ma południową <w>wystawę</w>, dzięki czemu przez cały dzień jest w nim bardzo jasno.",
                            "target_lang_translations": {
                                "ru": "Квартира имеет южную <w>ориентацию</w>, благодаря чему в ней весь день очень светло."
                            }
                        },
                        {
                            "text": "Te rośliny wymagają słonecznej <w>wystawy</w>, aby mogły prawidłowo rosnąć i kwitnąć.",
                            "target_lang_translations": {
                                "ru": "Этим растениям требуется солнечная <w>экспозиция</w>, чтобы они могли правильно расти и цвести."
                            }
                        }
                    ],
                    "synonyms": [
                        "ekspozycja"
                    ],
                    "common_phrases": [
                        "południowa wystawa",
                        "wystawa okien",
                        "wystawa słoneczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположение здания, окна или участка относительно сторон света, определяющее степень освещенности солнцем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ориентация",
                                "target_lang_sense_clarification": "расположение"
                            },
                            {
                                "target_lang_word": "экспозиция",
                                "target_lang_sense_clarification": "освещенность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab79b26e-275a-33a6-b9e5-f16ae199d926",
                    "sense_definition": "W siatkówce: precyzyjne podanie piłki do zawodnika, który ma wykonać atak na stronę przeciwnika.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rozgrywający wykonał idealną <w>wystawę</w>, co pozwoliło drużynie zdobyć decydujący punkt.",
                            "target_lang_translations": {
                                "ru": "Связующий выполнил идеальную <w>передачу</w>, что позволило команде выиграть решающее очко."
                            }
                        },
                        {
                            "text": "Trener zwrócił uwagę zawodnikom na niedokładną <w>wystawę</w> piłki przy siatce.",
                            "target_lang_translations": {
                                "ru": "Тренер обратил внимание игроков на неточную <w>передачу</w> мяча у сетки."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystawienie"
                    ],
                    "common_phrases": [
                        "wysoka wystawa",
                        "wystawa do ataku",
                        "ćwiczyć wystawę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "sport jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В волейболе: точная передача мяча игроку, который должен выполнить атакующий удар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передача",
                                "target_lang_sense_clarification": "пас"
                            },
                            {
                                "target_lang_word": "пас",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3fc005d-edf8-3c1c-b7ec-8dbb1a81b169",
                    "sense_definition": "Przesadna okazałość, bogactwo i przepych w sposobie życia, mające na celu zaimponowanie innym.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cecha",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawna arystokracja często organizowała przyjęcia z wielką <w>wystawą</w>, aby podkreślić swój status.",
                            "target_lang_translations": {
                                "ru": "Старая аристократия часто устраивала приемы с большой <w>пышностью</w>, чтобы подчеркнуть свой статус."
                            }
                        },
                        {
                            "text": "Jego dom urządzony był z niespotykaną <w>wystawą</w>, która onieśmielała skromniejszych gości.",
                            "target_lang_translations": {
                                "ru": "Его дом был обставлен с невиданной <w>роскошью</w>, которая смущала более скромных гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystawność",
                        "okazałość"
                    ],
                    "common_phrases": [
                        "żyć z wystawą",
                        "wielka wystawa"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезмерная пышность, богатство и роскошь в образе жизни, призванные произвести впечатление на других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пышность",
                                "target_lang_sense_clarification": "богатство"
                            },
                            {
                                "target_lang_word": "показная роскошь",
                                "target_lang_sense_clarification": "шик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29dab3a8-4379-3523-956d-a58f76ed47f4",
                    "sense_definition": "Dobudówka przy domu, ganek lub zadaszenie służące często do przechowywania narzędzi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "budynek",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziadek schował grabie i łopaty pod drewnianą <w>wystawę</w> znajdującą się za stodołą.",
                            "target_lang_translations": {
                                "ru": "Дедушка спрятал грабли и лопаты под деревянный <w>навес</w>, находящийся за сараем."
                            }
                        },
                        {
                            "text": "W upalne letnie popołudnie lubiliśmy odpoczywać w cieniu pod małą <w>wystawą</w>.",
                            "target_lang_translations": {
                                "ru": "В жаркий летний полдень мы любили отдыхать в тени под маленьким <w>навесом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ganek"
                    ],
                    "common_phrases": [
                        "siedzieć pod wystawą",
                        "schować pod wystawę"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "dialectal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пристройка к дому, крыльцо или навес, часто используемые для хранения инструментов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навес",
                                "target_lang_sense_clarification": "крыша на столбах"
                            },
                            {
                                "target_lang_word": "пристройка",
                                "target_lang_sense_clarification": "часть здания"
                            }
                        ]
                    }
                }

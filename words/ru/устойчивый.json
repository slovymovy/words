    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "52462415-80ca-3776-8a57-79c8213c4c47",
                    "sense_definition": "Постоянный, не подверженный частым изменениям, колебаниям или разрушению (о погоде, экономике, связи).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "stability_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем регионе уже неделю держится тёплая и <устойчивая> погода."
                        },
                        {
                            "text": "Компания демонстрирует <устойчивый> рост прибыли на протяжении последних пяти лет."
                        }
                    ],
                    "synonyms": [
                        "постоянный",
                        "стабильный",
                        "неизменный"
                    ],
                    "antonyms": [
                        "изменчивый",
                        "переменчивый",
                        "неустойчивый"
                    ],
                    "common_phrases": [
                        "устойчивая погода",
                        "устойчивый спрос",
                        "устойчивая связь",
                        "устойчивое выражение"
                    ]
                },
                {
                    "sense_id": "82f31882-e25d-3b21-86f3-4253a5b03cf8",
                    "sense_definition": "Способный твёрдо стоять, сохранять равновесие, не падать и не шататься.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "stability_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта <устойчивая> лестница позволяет безопасно добраться до самой крыши дома."
                        },
                        {
                            "text": "Стол оказался очень <устойчивым>, несмотря на свои тонкие и изящные ножки."
                        }
                    ],
                    "synonyms": [
                        "стойкий"
                    ],
                    "antonyms": [
                        "шаткий",
                        "неустойчивый"
                    ],
                    "common_phrases": [
                        "устойчивая лестница",
                        "устойчивая мебель",
                        "устойчивая походка",
                        "устойчивое положение"
                    ]
                },
                {
                    "sense_id": "fd528828-d5ff-3a4f-8e44-e59672976ed0",
                    "sense_definition": "Относящийся к развитию, которое удовлетворяет текущие потребности без ущерба для будущих поколений (обычно в экологии и экономике).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "stability_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Многие страны стремятся обеспечить <устойчивое> развитие своей экономики без вреда для природы."
                        },
                        {
                            "text": "Концепция <устойчивого> развития предполагает баланс между потреблением и сохранением ресурсов."
                        }
                    ],
                    "synonyms": [
                        "стабильный"
                    ],
                    "common_phrases": [
                        "устойчивое развитие",
                        "устойчивая экономика",
                        "устойчивый рост"
                    ]
                },
                {
                    "sense_id": "5a663e9a-6218-3cf1-93f9-50d988464c3f",
                    "sense_definition": "Способный выдерживать неблагоприятное воздействие внешней среды, болезней или нагрузок (о растениях, материалах, организме).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "resistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Агрономы вывели новый сорт пшеницы, <устойчивый> к сильным морозам и засухе."
                        },
                        {
                            "text": "У этого материала очень <устойчивая> структура, которая не разрушается от влаги."
                        }
                    ],
                    "synonyms": [
                        "стойкий"
                    ],
                    "antonyms": [
                        "нестойкий",
                        "неустойчивый"
                    ],
                    "common_phrases": [
                        "устойчивый к болезням",
                        "устойчивый сорт",
                        "устойчивый иммунитет",
                        "устойчивый к коррозии"
                    ]
                },
                {
                    "sense_id": "040ba38e-1f8b-3e8e-8569-05d9a33a563d",
                    "sense_definition": "Твёрдый в своих убеждениях и взглядах, не поддающийся чужому влиянию (о характере или психике).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "resistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У него сформировалось <устойчивое> мнение по этому вопросу, и переубедить его невозможно."
                        },
                        {
                            "text": "Для работы спасателем требуется крепкое здоровье и <устойчивая> психика."
                        }
                    ],
                    "synonyms": [
                        "непоколебимый",
                        "стойкий",
                        "неколебимый"
                    ],
                    "antonyms": [
                        "нетвёрдый",
                        "неустойчивый"
                    ],
                    "common_phrases": [
                        "устойчивая психика",
                        "устойчивое мировоззрение",
                        "устойчивые взгляды",
                        "устойчивое внимание"
                    ]
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cbecb9ba-1ba9-3778-aa96-f50ca07450f4",
                    "sense_definition": "Relating to, situated in, or directed towards the northwest.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Brittany is located in <w>northwestern</w> France.",
                            "target_lang_translations": {
                                "ru": "Бретань расположена в <w>северо-западной</w> Франции."
                            }
                        },
                        {
                            "text": "They decided to explore the <w>northwestern</w> coast during their summer vacation.",
                            "target_lang_translations": {
                                "ru": "Они решили исследовать <w>северо-западное</w> побережье во время летнего отпуска."
                            }
                        }
                    ],
                    "antonyms": [
                        "southeastern"
                    ],
                    "common_phrases": [
                        "northwestern part",
                        "northwestern region",
                        "northwestern coast"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к северо-западу, расположенный на северо-западе или направленный на северо-запад."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "северо-западный",
                                "target_lang_sense_clarification": "о местоположении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bb42039-170c-369d-8f9a-64a5a4d1dc37",
                    "sense_definition": "Of wind, blowing from the northwest.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A cold <w>northwestern</w> wind brought a sudden drop in temperature.",
                            "target_lang_translations": {
                                "ru": "Холодный <w>северо-западный</w> ветер принес резкое понижение температуры."
                            }
                        },
                        {
                            "text": "The sailors had to adjust their course to handle the strong <w>northwestern</w> breeze.",
                            "target_lang_translations": {
                                "ru": "Морякам пришлось скорректировать курс, чтобы справиться с сильным <w>северо-западным</w> ветром."
                            }
                        }
                    ],
                    "antonyms": [
                        "southeastern"
                    ],
                    "common_phrases": [
                        "northwestern wind",
                        "northwestern gale"
                    ],
                    "target_lang_definitions": {
                        "ru": "О ветре: дующий с северо-запада."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "северо-западный",
                                "target_lang_sense_clarification": "о ветре"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c9c108cb-fe2c-36d5-83e5-33d818d96384",
                    "sense_definition": "A film or dramatic work set primarily in the northwest of North America, typically in the late 19th or early 20th century.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The director is known for producing gritty <w>northwesterns</w> that depict life in the Yukon.",
                            "target_lang_translations": {
                                "ru": "Режиссер известен созданием суровых <w>норт-вестернов</w>, описывающих жизнь на Юконе."
                            }
                        },
                        {
                            "text": "Unlike traditional westerns, <w>northwesterns</w> often feature snow, forests, and mounties.",
                            "target_lang_translations": {
                                "ru": "В отличие от традиционных вестернов, в <w>норт-вестернах</w> часто присутствуют снег, леса и конная полиция."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фильм или драматическое произведение, действие которого происходит на северо-западе Северной Америки (обычно в конце XIX — начале XX века)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "норт-вестерн",
                                "target_lang_sense_clarification": "жанр кино"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e06053b9-66e3-3db4-b71e-34ad36f24f0a",
                    "sense_definition": "In a general way, without considering specific details or exceptions.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new policy is <w>broadly</w> similar to the one we had last year.",
                            "target_lang_translations": {
                                "ru": "Новая политика <w>в целом</w> схожа с той, что была у нас в прошлом году."
                            }
                        },
                        {
                            "text": "<w>Broadly</w> speaking, the project was a success despite a few minor issues.",
                            "target_lang_translations": {
                                "ru": "<w>В целом</w>, проект был успешным, несмотря на несколько мелких проблем."
                            }
                        }
                    ],
                    "synonyms": [
                        "generally",
                        "largely",
                        "roughly",
                        "loosely"
                    ],
                    "antonyms": [
                        "specifically",
                        "precisely",
                        "exactly"
                    ],
                    "common_phrases": [
                        "broadly speaking",
                        "broadly defined",
                        "broadly similar",
                        "broadly consistent"
                    ],
                    "target_lang_definitions": {
                        "ru": "В общих чертах, не вдаваясь в конкретные детали или исключения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в целом",
                                "target_lang_sense_clarification": "в общем, по большому счету"
                            },
                            {
                                "target_lang_word": "в общих чертах",
                                "target_lang_sense_clarification": "без подробностей"
                            },
                            {
                                "target_lang_word": "широко",
                                "target_lang_sense_clarification": "в широком смысле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4839cfd-be0a-3401-b0de-494b66f974e7",
                    "sense_definition": "In a wide and open manner, especially referring to a facial expression like a smile.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He smiled <w>broadly</w> when he saw his family waiting at the airport.",
                            "target_lang_translations": {
                                "ru": "Он <w>широко</w> улыбнулся, когда увидел свою семью, ожидающую в аэропорту."
                            }
                        },
                        {
                            "text": "She grinned <w>broadly</w> after winning the competition.",
                            "target_lang_translations": {
                                "ru": "Она <w>широко</w> улыбнулась после победы в соревновании."
                            }
                        }
                    ],
                    "synonyms": [
                        "widely",
                        "openly"
                    ],
                    "antonyms": [
                        "faintly",
                        "slightly"
                    ],
                    "common_phrases": [
                        "smile broadly",
                        "grin broadly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Широко и открыто (обычно используется для описания улыбки)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широко",
                                "target_lang_sense_clarification": "об улыбке"

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5dc1b503-5a60-3550-9dd7-f5c8666564df",
                    "sense_definition": "Een groot gedomesticeerd zoogdier met manen en een staart, dat wordt gebruikt om op te rijden of lasten te trekken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer heeft een bruin <w>paard</w> in de wei staan.",
                            "target_lang_translations": {
                                "ru": "У фермера на лугу стоит коричневая <w>лошадь</w>."
                            }
                        },
                        {
                            "text": "Zij gaat elke zaterdag naar de manege om op een <w>paard</w> te rijden.",
                            "target_lang_translations": {
                                "ru": "Каждую субботу она ходит в манеж, чтобы покататься на <w>лошади</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ros",
                        "knol"
                    ],
                    "common_phrases": [
                        "te paard",
                        "het paard van Troje",
                        "werken als een paard",
                        "op het verkeerde paard wedden",
                        "een gegeven paard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное домашнее млекопитающее с гривой и хвостом, используемое для верховой езды или перевозки грузов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лошадь",
                                "target_lang_sense_clarification": "животное"
                            },
                            {
                                "target_lang_word": "конь",
                                "target_lang_sense_clarification": "самец или верховая лошадь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d173014e-cdfc-3ae7-9bc8-aa808118754b",
                    "sense_definition": "Een schaakstuk dat in een L-vorm beweegt en als enige stuk over andere stukken heen mag springen.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zette zijn <w>paard</w> op een strategische plek op het schaakbord.",
                            "target_lang_translations": {
                                "ru": "Он поставил своего <w>коня</w> на стратегическую позицию на шахматной доске."
                            }
                        },
                        {
                            "text": "In het eindspel kan een <w>paard</w> erg lastig zijn voor de tegenstander.",
                            "target_lang_translations": {
                                "ru": "В эндшпиле <w>конь</w> может быть очень неприятен для противника."
                            }
                        }
                    ],
                    "common_phrases": [
                        "paard slaat toren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шахматная фигура, которая ходит буквой «Г» и единственная может перепрыгивать через другие фигуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конь",
                                "target_lang_sense_clarification": "шахматная фигура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e74cc7e4-72c4-3786-8bd8-5e2359d58586",
                    "sense_definition": "Een turntoestel op vier poten, vaak met handvatten, dat wordt gebruikt voor sprongen en turnoefeningen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De turner maakte een perfecte sprong over het <w>paard</w>.",
                            "target_lang_translations": {
                                "ru": "Гимнаст совершил идеальный прыжок через <w>коня</w>."
                            }
                        },
                        {
                            "text": "Oefeningen op het <w>paard</w> vereisen veel kracht in de armen en schouders.",
                            "target_lang_translations": {
                                "ru": "Упражнения на <w>коне</w> требуют большой силы рук и плеч."
                            }
                        }
                    ],
                    "common_phrases": [
                        "voltige op het paard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гимнастический снаряд на четырех ножках, часто с ручками, используемый для прыжков и упражнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конь",
                                "target_lang_sense_clarification": "гимнастический снаряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b6f0d54-64d5-320f-8810-14064ade771d",
                    "sense_definition": "Het zevende dier in de twaalfjarige cyclus van de Chinese dierenriem.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Mensen die in het jaar van het <w>paard</w> zijn geboren, worden vaak als energiek gezien.",
                            "target_lang_translations": {
                                "ru": "Люди, родившиеся в год <w>Лошади</w>, часто считаются энергичными."
                            }
                        },
                        {
                            "text": "Volgens de Chinese astrologie brengt het jaar van het <w>paard</w> voorspoed.",
                            "target_lang_translations": {
                                "ru": "Согласно китайской астрологии, год <w>Лошади</w> приносит процветание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jaar van het paard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Седьмое животное в двенадцатилетнем цикле китайского зодиака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лошадь",
                                "target_lang_sense_clarification": "знак зодиака"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f179c6a1-9328-397e-8760-54a483bf47e6",
                    "sense_definition": "Een houten standaard op vier poten waarop men planken of balken legt om ze te zagen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De timmerman legde de zware balk op het <w>paard</w> om hem door te zagen.",
                            "target_lang_translations": {
                                "ru": "Плотник положил тяжелую балку на <w>козлы</w>, чтобы распилить ее."
                            }
                        },
                        {
                            "text": "Zorg dat het <w>paard</w> stabiel staat voordat je met de zaag aan de slag gaat.",
                            "target_lang_translations": {
                                "ru": "Убедись, что <w>козлы</w> стоят устойчиво, прежде чем начинать работать пилой."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревянная подставка на четырех ножках, на которую кладут доски или бревна для распиливания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "козлы",
                                "target_lang_sense_clarification": "подставка для пилки дров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5e0e807-09d3-3c6e-9c70-094d63a13523",
                    "sense_definition": "Een touw onder de ra van een zeilschip waar matrozen op staan tijdens het werken aan de zeilen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De matroos balanceerde voorzichtig op het <w>paard</w> om het zeil vast te maken.",
                            "target_lang_translations": {
                                "ru": "Матрос осторожно балансировал на <w>перте</w>, чтобы закрепить парус."
                            }
                        },
                        {
                            "text": "Bij stormachtig weer is het gevaarlijk om hoog in de mast op het <w>paard</w> te staan.",
                            "target_lang_translations": {
                                "ru": "В штормовую погоду опасно стоять высоко на мачте на <w>перте</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "scheepvaartterm"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Трос под реем парусного судна, на котором стоят матросы во время работы с парусами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перт",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff5eb5cb-0c74-3be6-8ef1-74d1da439051",
                    "sense_definition": "Een onaantrekkelijke vrouw.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij maakte een onaardige opmerking en noemde haar een <w>paard</w>.",
                            "target_lang_translations": {
                                "ru": "Он сделал неприятное замечание и назвал ее <w>лошадью</w>."

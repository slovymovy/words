                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Żłób</w> ostrożnie dłutem w drewnie, aby nie zniszczyć delikatnego wzoru.",
                            "target_lang_translations": {

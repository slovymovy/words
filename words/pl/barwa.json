    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c34e88c7-14aa-372c-89db-3bc573e49cb3",
                    "sense_definition": "Wrażenie wzrokowe wywoływane przez światło odbite od przedmiotu; kolor.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jesienią liście drzew zmieniają <barwę> na żółtą i czerwoną.",
                            "target_lang_translations": {
                                "ru": "Осенью листья деревьев меняют <окраску> на желтую и красную."
                            }
                        },
                        {
                            "text": "Ten malarz słynie z używania bardzo jasnych i ciepłych <barw>.",
                            "target_lang_translations": {
                                "ru": "Этот художник славится использованием очень ярких и теплых <красок>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolor",
                        "odcień",
                        "ubarwienie",
                        "zabarwienie"
                    ],
                    "common_phrases": [
                        "barwy podstawowe",
                        "barwy ochronne",
                        "ciepłe barwy",
                        "zimne barwy",
                        "pełna barwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зрительное впечатление, вызываемое светом, отраженным от предмета; цвет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цвет",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "окраска",
                                "target_lang_sense_clarification": "свойство поверхности или предмета"
                            },
                            {
                                "target_lang_word": "краска",
                                "target_lang_sense_clarification": "тон, колорит (часто в искусстве)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57610cfd-5d87-3cbe-b59c-2d149036f429",
                    "sense_definition": "Kolory symbolizujące państwo, organizację lub klub sportowy (zwykle w liczbie mnogiej).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "symbols",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kibice dumnie nosili szaliki w <barwach> swojego ulubionego klubu.",
                            "target_lang_translations": {
                                "ru": "Болельщики с гордостью носили шарфы в <цветах> своего любимого клуба."
                            }
                        },
                        {
                            "text": "Podczas święta państwowego całe miasto udekorowano <barwami> narodowymi.",
                            "target_lang_translations": {
                                "ru": "Во время государственного праздника весь город был украшен национальными <цветами>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "barwy narodowe",
                        "barwy klubowe",
                        "bronić barw",
                        "reprezentować barwy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Цвета, символизирующие государство, организацию или спортивный клуб (обычно во множественном числе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цвета",
                                "target_lang_sense_clarification": "символика клуба или флага"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5a8c715-5a45-3b9b-b019-6927847db357",
                    "sense_definition": "Cecha dźwięku, która pozwala odróżnić głosy ludzi lub instrumenty muzyczne, nawet gdy wydają dźwięk o tej samej wysokości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej głos ma bardzo charakterystyczną, głęboką <barwę>.",
                            "target_lang_translations": {
                                "ru": "У ее голоса очень характерный, глубокий <тембр>."
                            }
                        },
                        {
                            "text": "Po <barwie> dźwięku poznałem, że grają skrzypce, a nie altówka.",
                            "target_lang_translations": {
                                "ru": "По <тембру> звука я понял, что играет скрипка, а не альт."
                            }
                        }
                    ],
                    "synonyms": [
                        "tembr",
                        "brzmienie",
                        "ton"
                    ],
                    "common_phrases": [
                        "barwa głosu",
                        "barwa dźwięku",
                        "ciepła barwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеристика звука, позволяющая различать голоса людей или музыкальные инструменты, даже если они имеют одинаковую высоту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тембр",
                                "target_lang_sense_clarification": "звука или голоса"
                            },
                            {
                                "target_lang_word": "окраска",
                                "target_lang_sense_clarification": "звучания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9469b692-e0fd-32f3-9470-ba324638c476",
                    "sense_definition": "Specyficzny charakter, nastrój lub wydźwięk czegoś (np. wypowiedzi, utworu literackiego).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego przemówienie miało wyraźnie polityczną <barwę>.",
                            "target_lang_translations": {
                                "ru": "Его речь имела явно политическую <окраску>."
                            }
                        },
                        {
                            "text": "To opowiadanie ma smutną <barwę>, mimo że kończy się optymistycznie.",
                            "target_lang_translations": {
                                "ru": "У этого рассказа грустный <оттенок>, несмотря на то, что он заканчивается оптимистично."
                            }
                        }
                    ],
                    "synonyms": [
                        "charakter",
                        "zabarwienie",
                        "ton"
                    ],
                    "common_phrases": [
                        "barwa emocjonalna",
                        "barwa uczuciowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфический характер, настроение или оттенок чего-либо (например, высказывания, литературного произведения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окраска",
                                "target_lang_sense_clarification": "эмоциональная или стилистическая"
                            },
                            {
                                "target_lang_word": "оттенок",
                                "target_lang_sense_clarification": "нюанс смысла или настроения"
                            },
                            {
                                "target_lang_word": "тон",
                                "target_lang_sense_clarification": "характер звучания или текста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da3c6fae-98d4-34de-8ba9-bbf93ee5463d",
                    "sense_definition": "Czerwony kolor na twarzy pojawiający się pod wpływem emocji lub zdrowia; rumieńce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długim spacerze na mrozie jej twarz nabrała zdrowych <barw>.",
                            "target_lang_translations": {
                                "ru": "После долгой прогулки на морозе ее лицо набралось здоровых <красок>."
                            }
                        },
                        {
                            "text": "Na wspomnienie o tamtym wydarzeniu dziewczyna straciła <barwę> i zbladła.",
                            "target_lang_translations": {
                                "ru": "При воспоминании о том событии девушка потеряла <краску> и побледнела."
                            }
                        }
                    ],
                    "synonyms": [
                        "rumieniec",
                        "wypieki"
                    ],
                    "common_phrases": [
                        "nabrać barw",
                        "stracić barwę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Красный цвет лица, появляющийся под влиянием эмоций или состояния здоровья; румянец."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "румянец",
                                "target_lang_sense_clarification": "здоровый цвет лица"
                            },
                            {
                                "target_lang_word": "краска",
                                "target_lang_sense_clarification": "прилив крови к лицу"
                            },
                            {
                                "target_lang_word": "цвет",
                                "target_lang_sense_clarification": "лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "210e83d1-8537-35bb-81c5-2a851386af74",
                    "sense_definition": "Strój o określonym kolorze i kroju, charakterystyczny dla danej służby dworskiej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_historical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed pałacem stali służący ubrani w bogate <barwy> swojego pana.",
                            "target_lang_translations": {
                                "ru": "Перед дворцом стояли слуги, одетые в богатые <ливреи> своего господина."
                            }
                        },
                        {
                            "text": "W dawnych czasach <barwa> sługi świadczyła o zamożności rodu, któremu służył.",
                            "target_lang_translations": {
                                "ru": "В давние времена <ливрея> слуги свидетельствовала о богатстве рода, которому он служил."
                            }
                        }
                    ],
                    "synonyms": [
                        "liberia",
                        "mundur"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane głównie w kontekście historycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одежда определенного цвета и покроя, характерная для придворной службы (историческое)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ливрея",
                                "target_lang_sense_clarification": "форменная одежда слуг"
                            },
                            {
                                "target_lang_word": "цвета",
                                "target_lang_sense_clarification": "геральдические цвета на одежде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43096609-247a-3712-a273-aefbb518bb67",
                    "sense_definition": "Krew zwierzyny łownej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hunting_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myśliwy uważnie śledził ślady <barwy> na śniegu, by odnaleźć rannego dzika.",
                            "target_lang_translations": {
                                "ru": "Охотник внимательно следил за следами <крови> на снегу, чтобы найти раненого кабана."
                            }
                        },
                        {
                            "text": "Pies myśliwski szybko podjął trop po świeżej <barwie>.",
                            "target_lang_translations": {
                                "ru": "Охотничья собака быстро взяла след по свежей <краске>."
                            }
                        }
                    ],
                    "synonyms": [
                        "farba"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кровь промыслового зверя (охотничий термин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кровь",
                                "target_lang_sense_clarification": "животного"
                            },
                            {
                                "target_lang_word": "краска",
                                "target_lang_sense_clarification": "охотничий жаргон"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
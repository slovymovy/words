                    }
                },
                {
                    "sense_id": "f4f18711-5fbd-374f-bbee-077ca0ed0e69",
                    "sense_definition": "Sprawdzać smak potrawy lub napoju, jedząc lub pijąc niewielką ilość",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "taste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kucharz <kosztuje> zupy, aby sprawdzić, czy jest wystarczająco słona.",
                            "target_lang_translations": {
                                "ru": "Повар <пробует> суп, чтобы проверить, достаточно ли он соленый."
                            }
                        },
                        {
                            "text": "Goście chętnie <kosztowali> lokalnych win podczas wycieczki.",
                            "target_lang_translations": {
                                "ru": "Гости охотно <пробовали> местные вина во время экскурсии."
                            }
                        }
                    ],
                    "synonyms": [

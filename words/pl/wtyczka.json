                },
                {
                    "sense_id": "fdf5fd90-5e72-3f60-9a86-7284f34728f2",
                    "sense_definition": "Dodatek do programu komputerowego zwiększający jego funkcjonalność.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "software",
                    "name_type": "no",
                    "examples": [

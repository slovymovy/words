{
    "word_family": [
        "thence",
        "thenceforth"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "470a0594-b323-3661-b203-e7988bccb47e",
                    "sense_definition": "Soon afterward.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He finished his breakfast and <w>then</w> went to work.",
                            "target_lang_translations": {
                                "ru": "Он закончил завтракать, а <w>потом</w> пошел на работу."
                            }
                        },
                        {
                            "text": "Turn left at the corner, <w>then</w> go straight for two blocks.",
                            "target_lang_translations": {
                                "ru": "Поверните за угол налево, <w>затем</w> идите прямо два квартала."
                            }
                        }
                    ],
                    "synonyms": [
                        "subsequently",
                        "and then"
                    ],
                    "antonyms": [
                        "before",
                        "previously"
                    ],
                    "common_phrases": [
                        "and then",
                        "just then",
                        "right then"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вскоре после этого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потом",
                                "target_lang_sense_clarification": "наречие времени, разговорное"
                            },
                            {
                                "target_lang_word": "затем",
                                "target_lang_sense_clarification": "наречие, обозначающее последовательность"
                            },
                            {
                                "target_lang_word": "после",
                                "target_lang_sense_clarification": "спустя некоторое время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c01ce837-27a2-3356-a1e0-75d02aea6565",
                    "sense_definition": "At that time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_past",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was living in a small village <w>then</w>.",
                            "target_lang_translations": {
                                "ru": "Я жил в маленькой деревне <w>тогда</w>."
                            }
                        },
                        {
                            "text": "Life was much simpler back <w>then</w>.",
                            "target_lang_translations": {
                                "ru": "Жизнь была намного проще <w>тогда</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "at the time",
                        "back in the day"
                    ],
                    "common_phrases": [
                        "back then",
                        "even then"
                    ],
                    "target_lang_definitions": {
                        "ru": "В то время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тогда",
                                "target_lang_sense_clarification": "в прошлом"
                            },
                            {
                                "target_lang_word": "в то время",
                                "target_lang_sense_clarification": "фраза, указывающая на определенный период в прошлом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a540377a-f658-34a7-97ce-09062b58b4f0",
                    "sense_definition": "In that case; as a consequence.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "logical_consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you are tired, <w>then</w> you should go to sleep.",
                            "target_lang_translations": {
                                "ru": "Если ты устал, <w>то</w> тебе следует пойти спать."
                            }
                        },
                        {
                            "text": "You do not want to help? <w>Then</w> do not complain about the result.",
                            "target_lang_translations": {
                                "ru": "Ты не хочешь помогать? <w>Тогда</w> не жалуйся на результат."
                            }
                        }
                    ],
                    "synonyms": [
                        "therefore",
                        "accordingly",
                        "so",
                        "as a result"
                    ],
                    "common_phrases": [
                        "if so then",
                        "well then",
                        "okay then"
                    ],
                    "target_lang_definitions": {
                        "ru": "В таком случае; как следствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тогда",
                                "target_lang_sense_clarification": "следовательно"
                            },
                            {
                                "target_lang_word": "то",
                                "target_lang_sense_clarification": "союз, часть конструкции 'если..., то...'"
                            },
                            {
                                "target_lang_word": "в таком случае",
                                "target_lang_sense_clarification": "фраза, выражающая условие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18ba8c35-9656-34fc-875b-ba2ca80763d7",
                    "sense_definition": "Next in order of place or position.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "First there is a bakery, and <w>then</w> a bank.",
                            "target_lang_translations": {
                                "ru": "Сначала идет пекарня, а <w>потом</w> банк."
                            }
                        },
                        {
                            "text": "The list includes apples, oranges, and <w>then</w> bananas.",
                            "target_lang_translations": {
                                "ru": "В список входят яблоки, апельсины, а <w>затем</w> бананы."
                            }
                        }
                    ],
                    "synonyms": [
                        "followed by"
                    ],
                    "common_phrases": [
                        "and then"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий по порядку места или позиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потом",
                                "target_lang_sense_clarification": "при перечислении"
                            },
                            {
                                "target_lang_word": "затем",
                                "target_lang_sense_clarification": "далее по списку или маршруту"
                            },
                            {
                                "target_lang_word": "далее",
                                "target_lang_sense_clarification": "следующим пунктом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eff63956-8b17-3b6f-97fe-d2dc60841db8",
                    "sense_definition": "At the same time; on the other hand (used to introduce a contrasting thought).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "contrast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is an expensive car, but <w>then</w> it is also very reliable.",
                            "target_lang_translations": {
                                "ru": "Это дорогая машина, но <w>зато</w> она очень надежная."
                            }
                        },
                        {
                            "text": "He can be strict, but <w>then</w> again, he wants us to succeed.",
                            "target_lang_translations": {
                                "ru": "Он может быть строгим, но, <w>с другой стороны</w>, он хочет, чтобы мы добились успеха."
                            }
                        }
                    ],
                    "common_phrases": [
                        "but then",
                        "but then again"
                    ],
                    "target_lang_definitions": {
                        "ru": "С другой стороны; в то же время (используется для введения противопоставления)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с другой стороны",
                                "target_lang_sense_clarification": "вводная фраза для контраста"
                            },
                            {
                                "target_lang_word": "зато",
                                "target_lang_sense_clarification": "союз, выражающий возмещение или противопоставление"
                            },
                            {
                                "target_lang_word": "впрочем",
                                "target_lang_sense_clarification": "вводное слово"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c2121d7-5970-3634-b193-5bf904cc8b4a",
                    "sense_definition": "That time (used after a preposition).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will be ready by <w>then</w>.",
                            "target_lang_translations": {
                                "ru": "Я буду готов к <w>тому времени</w>."
                            }
                        },
                        {
                            "text": "We have not seen him since <w>then</w>.",
                            "target_lang_translations": {
                                "ru": "Мы не видели его с <w>тех пор</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "by then",
                        "until then",
                        "since then",
                        "before then"
                    ],
                    "target_lang_definitions": {
                        "ru": "То время (используется после предлога)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "то время",
                                "target_lang_sense_clarification": "существительное с указательным местоимением"
                            },
                            {
                                "target_lang_word": "тех пор",
                                "target_lang_sense_clarification": "в выражении 'с тех пор'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c448fa47-8ae8-3145-82d4-c2f800e23047",
                    "sense_definition": "Being so at that specific time (used before a noun).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>then</w> president signed the new law.",
                            "target_lang_translations": {
                                "ru": "<w>Тогдашний</w> президент подписал новый закон."
                            }
                        },
                        {
                            "text": "He argued with his <w>then</w> girlfriend about the trip.",
                            "target_lang_translations": {
                                "ru": "Он спорил со своей <w>тогдашней</w> девушкой о поездке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the then president",
                        "his then wife",
                        "the then government"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существовавший в то конкретное время (используется перед существительным)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тогдашний",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "бывший",
                                "target_lang_sense_clarification": "занимавший должность в прошлом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "ec51ea40-3ae1-31e9-8a8d-3573e9d16705",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Thens</w> arrived late for the meeting.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Тенс</w> опоздал на встречу."
                            }
                        },
                        {
                            "text": "We looked up the <w>Then</w> family in the registry.",
                            "target_lang_translations": {
                                "ru": "Мы искали семью <w>Тен</w> в реестре."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "rare"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тенс",
                                "target_lang_sense_clarification": "транслитерация"
                            },
                            {
                                "target_lang_word": "Тен",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cb05a0e0-ef86-3108-b755-8b428e116097",
                    "sense_definition": "Выравнивать, очищать или сглаживать поверхность земли, например, под пашню или строительство.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "land_preparation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Трактор выехал в поле, чтобы <w>плинтовать</w> землю перед весенней посадкой культур.",
                            "target_lang_translations": {
                                "en": "The tractor went out into the field to <w>level</w> the earth before the spring planting of crops."
                            }
                        },
                        {
                            "text": "Строители должны тщательно <w>плинтовать</w> этот неровный участок до начала основных работ.",
                            "target_lang_translations": {
                                "en": "The builders must carefully <w>grade</w> this uneven site before beginning the main work."
                            }
                        }
                    ],
                    "synonyms": [
                        "выравнивать",
                        "планировать"
                    ],
                    "common_phrases": [
                        "плинтовать землю",
                        "плинтовать участок",
                        "плинтовать болото"
                    ],
                    "target_lang_definitions": {
                        "en": "To level, clear, or smooth the ground surface, for example, for plowing or construction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to level",
                                "target_lang_sense_clarification": "ground surface"
                            },
                            {
                                "target_lang_word": "to grade",
                                "target_lang_sense_clarification": "land for construction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a85d0ff4-165c-3c6d-a444-b07dde9653a4",
                    "sense_definition": "Раскалывать крупные глыбы камня на части нужного размера для дальнейшей обработки.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "stone_processing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный мастер учил ученика правильно <w>плинтовать</w> гранитные блоки молотом.",
                            "target_lang_translations": {
                                "en": "The experienced master taught the apprentice how to correctly <w>split</w> granite blocks with a hammer."
                            }
                        },
                        {
                            "text": "Нам нужно <w>плинтовать</w> этот огромный камень, чтобы он поместился в грузовик.",
                            "target_lang_translations": {
                                "en": "We need to <w>break up</w> this huge stone so it fits in the truck."
                            }
                        }
                    ],
                    "synonyms": [
                        "раскалывать",
                        "дробить"
                    ],
                    "common_phrases": [
                        "плинтовать камень",
                        "плинтовать глыбы",
                        "плинтовать гранит"
                    ],
                    "target_lang_definitions": {
                        "en": "To split large blocks of stone into parts of the required size for further processing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to split",
                                "target_lang_sense_clarification": "stone blocks"
                            },
                            {
                                "target_lang_word": "to break up",
                                "target_lang_sense_clarification": "large rocks"
                            }
                        ]
                    }
                }
            ]
        }
    ]

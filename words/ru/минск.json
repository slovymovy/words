    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c1f39c3c-a894-36dd-bdca-76815505c3b5",
                    "sense_definition": "Столица Республики Беларусь, крупный административный, культурный и научный центр страны.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Каждое лето мы всей семьёй ездим в <w>Минск</w> к родственникам.",
                            "target_lang_translations": {
                                "en": "Every summer, our whole family goes to <w>Minsk</w> to visit relatives."
                            }
                        },
                        {
                            "text": "<w>Минск</w> известен своими широкими проспектами, парками и чистотой на улицах.",
                            "target_lang_translations": {
                                "en": "<w>Minsk</w> is known for its wide avenues, parks, and clean streets."
                            }
                        }
                    ],
                    "common_phrases": [
                        "город Минск",
                        "центр Минска",
                        "поездка в Минск",
                        "жить в Минске"
                    ],
                    "target_lang_definitions": {
                        "en": "The capital of the Republic of Belarus, a major administrative, cultural, and scientific center of the country."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Minsk",
                                "target_lang_sense_clarification": "capital city"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "61a427a8-f1f1-376c-9629-1fd2a182db7e",
                    "sense_definition": "Правительство Белоруссии (используется в политическом контексте и новостях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Официальный <w>Минск</w> выразил готовность к продолжению мирных переговоров.",
                            "target_lang_translations": {
                                "en": "Official <w>Minsk</w> expressed readiness to continue peace negotiations."
                            }
                        },
                        {
                            "text": "В ответ на санкции <w>Минск</w> принял решение ограничить транзит некоторых товаров.",
                            "target_lang_translations": {
                                "en": "In response to sanctions, <w>Minsk</w> decided to limit the transit of certain goods."
                            }
                        }
                    ],
                    "synonyms": [
                        "правительство"
                    ],
                    "common_phrases": [
                        "официальный Минск",
                        "позиция Минска",
                        "решение Минска"
                    ],
                    "target_lang_definitions": {
                        "en": "The government of Belarus (used in political contexts and news)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Minsk",
                                "target_lang_sense_clarification": "referring to the government"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75439e96-c03c-39d0-bd77-33b7839305cb",
                    "sense_definition": "Минские соглашения (комплекс документов по урегулированию конфликта).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "document_or_program_name",
                    "examples": [
                        {
                            "text": "Многие эксперты утверждают, что альтернативы <w>Минску</w> в данный момент не существует.",
                            "target_lang_translations": {
                                "en": "Many experts argue that there is currently no alternative to <w>Minsk</w>."
                            }
                        },
                        {
                            "text": "Полное выполнение <w>Минска</w> является ключевым условием для снятия ограничений.",
                            "target_lang_translations": {
                                "en": "Full implementation of <w>Minsk</w> is a key condition for lifting restrictions."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выполнение Минска",
                        "пункты Минска",
                        "альтернатива Минску"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "сокращенное название соглашений"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The Minsk Agreements (a set of documents for conflict resolution)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Minsk",
                                "target_lang_sense_clarification": "referring to the Minsk Agreements"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "942c8d6d-32cc-38c6-a933-cd1c301200b5",
                    "sense_definition": "Марка белорусских холодильников, а также сам такой холодильник.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "На даче до сих пор работает старый <w>Минск</w>, который родители купили тридцать лет назад.",
                            "target_lang_translations": {
                                "en": "At the dacha, the old <w>Minsk</w> that my parents bought thirty years ago is still working."
                            }
                        },
                        {
                            "text": "Мы решили не выбрасывать <w>Минск</w>, а отдать его знакомым, которым нужен холодильник.",
                            "target_lang_translations": {
                                "en": "We decided not to throw away the <w>Minsk</w>, but to give it to acquaintances who need a fridge."
                            }
                        }
                    ],
                    "synonyms": [
                        "холодильник"
                    ],
                    "common_phrases": [
                        "холодильник Минск",
                        "старый Минск"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "в значении бытового прибора"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A brand of Belarusian refrigerators, as well as the refrigerator itself."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Minsk",
                                "target_lang_sense_clarification": "refrigerator brand"
                            }
                        ]
                    }
                }
            ]
        }
    ]

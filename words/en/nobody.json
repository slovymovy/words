                    "sense_id": "ea47474b-66c3-3350-8236-754462b4ad08",
                    "sense_definition": "Not any person; no one.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I knocked on the door, but <w>nobody</w> answered.",
                            "target_lang_translations": {
                                "ru": "Я постучал в дверь, но <w>никто</w> не ответил."
                            }
                        },
                        {
                            "text": "<w>Nobody</w> knows the answer to that difficult question.",
                            "target_lang_translations": {
                                "ru": "<w>Никто</w> не знает ответа на этот сложный вопрос."
                            }
                        },
                        {
                            "text": "There was <w>nobody</w> else in the room when I arrived.",
                            "target_lang_translations": {
                                "ru": "Когда я пришёл, в комнате больше <w>никого</w> не было."
                            }
                        }
                    ],
                    "synonyms": [
                        "no one"
                    ],
                    "antonyms": [
                        "somebody",
                        "someone",
                        "anybody",
                        "anyone",
                        "everybody",
                        "everyone"
                    ],
                    "common_phrases": [
                        "nobody else",
                        "nobody knows",
                        "nobody cares"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ни один человек; никто."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никто",
                                "target_lang_sense_clarification": "отрицательное местоимение"

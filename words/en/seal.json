{
    "word_family": [
        "sealant",
        "sealer",
        "unseal",
        "reseal",
        "resealable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "841514c2-32c5-3616-856d-8ec0fac80361",
                    "sense_definition": "A sea mammal with flippers that lives mostly in cold water and eats fish.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We watched a <seal> swimming near the harbor.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали за <тюленем>, плавающим возле гавани."
                            }
                        },
                        {
                            "text": "The baby <seal> has soft white fur.",
                            "target_lang_translations": {
                                "ru": "У детеныша <тюленя> мягкий белый мех."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Морское млекопитающее с ластами, которое обитает преимущественно в холодных водах и питается рыбой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюлень",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c65af5c1-6d05-3998-8d0d-7b5afc90c2bb",
                    "sense_definition": "A piece of rubber or other material used to join two parts tightly to prevent liquid or gas from leaking.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rubber <seal> around the door needs to be replaced.",
                            "target_lang_translations": {
                                "ru": "Резиновый <уплотнитель> вокруг двери нужно заменить."
                            }
                        },
                        {
                            "text": "Make sure the jar has a tight <seal> so the food does not spoil.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что банка имеет плотное <уплотнение>, чтобы еда не испортилась."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок резины или другого материала, используемый для плотного соединения двух частей с целью предотвращения утечки жидкости или газа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уплотнитель",
                                "target_lang_sense_clarification": "материал для герметичности"
                            },
                            {
                                "target_lang_word": "прокладка",
                                "target_lang_sense_clarification": "деталь между частями механизма"
                            },
                            {
                                "target_lang_word": "сальник",
                                "target_lang_sense_clarification": "техническое уплотнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b66c667-9ccd-3467-ba25-78acb3d410da",
                    "sense_definition": "An official mark stamped on a document to show it is authentic, or the object used to make this mark.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "stamp",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The certificate bears the official <seal> of the university.",
                            "target_lang_translations": {
                                "ru": "На сертификате стоит официальная <печать> университета."
                            }
                        },
                        {
                            "text": "He broke the wax <seal> on the envelope to read the letter.",
                            "target_lang_translations": {
                                "ru": "Он сломал сургучную <печать> на конверте, чтобы прочитать письмо."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный знак, проставляемый на документе для подтверждения его подлинности, или предмет, используемый для проставления этого знака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печать",
                                "target_lang_sense_clarification": "оттиск или инструмент"
                            },
                            {
                                "target_lang_word": "клеймо",
                                "target_lang_sense_clarification": "выжженный или выбитый знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b316891-c60a-3873-a714-3456e5d34916",
                    "sense_definition": "A statement or sign that confirms approval or quality.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "approval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The project received the manager's <seal> of approval.",
                            "target_lang_translations": {
                                "ru": "Проект получил <знак> одобрения менеджера."
                            }
                        },
                        {
                            "text": "Her work was given a final <seal> of acceptance by the committee.",
                            "target_lang_translations": {
                                "ru": "Ее работа получила окончательный <знак> приемки комитетом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "seal of approval"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заявление или знак, подтверждающие одобрение или качество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знак",
                                "target_lang_sense_clarification": "символ одобрения"
                            },
                            {
                                "target_lang_word": "печать",
                                "target_lang_sense_clarification": "в переносном смысле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "110369cd-fcdc-3900-b4ba-def0094e624a",
                    "sense_definition": "A member of a special operations force in the US Navy trained for sea, air, and land missions.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He underwent rigorous training to become a Navy <SEAL>.",
                            "target_lang_translations": {
                                "ru": "Он прошел суровую подготовку, чтобы стать <морским котиком>."
                            }
                        },
                        {
                            "text": "The mission was successfully carried out by a team of <SEALs>.",
                            "target_lang_translations": {
                                "ru": "Миссия была успешно выполнена командой <морских котиков>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Navy SEAL"
                    ],
                    "target_lang_definitions": {
                        "ru": "Военнослужащий сил специальных операций ВМС США, подготовленный для действий на море, в воздухе и на суше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морской котик",
                                "target_lang_sense_clarification": "спецназ ВМС США"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "56a799b6-670c-3986-9adb-a081394f7526",
                    "sense_definition": "To close a container, envelope, or opening tightly so that nothing can get in or out.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "close_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <seal> the envelope before you mail it.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <запечатайте> конверт перед отправкой."
                            }
                        },
                        {
                            "text": "She <sealed> the bag to keep the cookies fresh.",
                            "target_lang_translations": {
                                "ru": "Она <запечатала> пакет, чтобы печенье оставалось свежим."
                            }
                        }
                    ],
                    "synonyms": [
                        "close"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плотно закрывать контейнер, конверт или отверстие, чтобы ничего не могло попасть внутрь или наружу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запечатывать",
                                "target_lang_sense_clarification": "плотно закрывать"
                            },
                            {
                                "target_lang_word": "заклеивать",
                                "target_lang_sense_clarification": "конверт или пакет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42c50d5b-4e93-3631-8a6a-7914655526b2",
                    "sense_definition": "To close off an area or border to prevent people or vehicles from entering or leaving.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "block_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police had to <seal> the border to stop the fugitives.",
                            "target_lang_translations": {
                                "ru": "Полиции пришлось <перекрыть> границу, чтобы остановить беглецов."
                            }
                        },
                        {
                            "text": "They decided to <seal> off the street after the accident.",
                            "target_lang_translations": {
                                "ru": "Они решили <оцепить> улицу после аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "block",
                        "close off",
                        "seal off"
                    ],
                    "common_phrases": [
                        "seal off"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перекрывать территорию или границу, чтобы предотвратить вход или выход людей или транспортных средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перекрывать",
                                "target_lang_sense_clarification": "блокировать проход"
                            },
                            {
                                "target_lang_word": "оцеплять",
                                "target_lang_sense_clarification": "окружать охраной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea9be899-b0f2-3662-bdd1-b68c4c5f26cb",
                    "sense_definition": "To apply a coating or substance to a surface to make it watertight or airtight.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "watertight_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <seal> the wood to protect it from the rain.",
                            "target_lang_translations": {
                                "ru": "Вам следует <обработать герметиком> дерево, чтобы защитить его от дождя."
                            }
                        },
                        {
                            "text": "The plumber <sealed> the pipe joints to prevent leaks.",
                            "target_lang_translations": {
                                "ru": "Сантехник <загерметизировал> стыки труб, чтобы предотвратить протечки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить покрытие или вещество на поверхность, чтобы сделать её водонепроницаемой или воздухонепроницаемой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герметизировать",
                                "target_lang_sense_clarification": "делать непроницаемым"
                            },
                            {
                                "target_lang_word": "покрывать защитным слоем",
                                "target_lang_sense_clarification": "обрабатывать поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f23ba0bf-e2e9-3fd4-9b66-6a058f216516",
                    "sense_definition": "To make a decision, agreement, or victory definite and final.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "finalize_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A late goal helped <seal> the victory for the home team.",
                            "target_lang_translations": {
                                "ru": "Поздний гол помог <закрепить> победу хозяев."
                            }
                        },
                        {
                            "text": "They shook hands to <seal> the deal.",
                            "target_lang_translations": {
                                "ru": "Они пожали руки, чтобы <скрепить> сделку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "seal the deal",
                        "seal one's fate",
                        "seal a victory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать решение, соглашение или победу окончательными и определенными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скреплять",
                                "target_lang_sense_clarification": "подтверждать договоренность"
                            },
                            {
                                "target_lang_word": "закреплять",
                                "target_lang_sense_clarification": "фиксировать победу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8055bf3-71a2-3d1e-8890-58dedc03d702",
                    "sense_definition": "To fry meat quickly at a high temperature to keep the juices inside.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cooking_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <seal> the steak in a hot pan before roasting it.",
                            "target_lang_translations": {
                                "ru": "Вам следует <обжарить> стейк на горячей сковороде перед запеканием."
                            }
                        },
                        {
                            "text": "<Seal> the meat on both sides until it is brown.",
                            "target_lang_translations": {
                                "ru": "<Обжарьте> мясо с обеих сторон до коричневого цвета."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро обжаривать мясо при высокой температуре, чтобы сохранить соки внутри."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обжаривать",
                                "target_lang_sense_clarification": "кулинария"
                            },
                            {
                                "target_lang_word": "запечатывать",
                                "target_lang_sense_clarification": "кулинарный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b819e39-49f2-3b89-b6db-8f07e792618f",
                    "sense_definition": "To hunt seals for their fur or meat.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hunt_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crew went out <sealing> in the icy waters.",
                            "target_lang_translations": {
                                "ru": "Команда отправилась на <охоту на тюленей> в ледяные воды."
                            }
                        },
                        {
                            "text": "Many people protest against the practice of <sealing>.",
                            "target_lang_translations": {
                                "ru": "Многие люди протестуют против <охоты на тюленей>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Охотиться на тюленей ради их меха или мяса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охотиться на тюленей",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "8091f658-e9d5-3e04-9d47-e0ff13e3c574",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <Seal> has lived in this town for many years.",
                            "target_lang_translations": {
                                "ru": "Мистер <Сил> живет в этом городе много лет."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <Seal> at the conference.",
                            "target_lang_translations": {
                                "ru": "Я встретил женщину по имени Сара <Сил> на конференции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сил",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d5f956e-bd21-3f08-bc32-13aa94afd895",
                    "sense_definition": "A village in Kent, England.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They visited the small village of <Seal> during their trip to England.",
                            "target_lang_translations": {
                                "ru": "Они посетили небольшую деревню <Сил> во время поездки в Англию."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревня в графстве Кент, Англия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сил",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e981ec63-dc78-38cd-8ea4-a64a8c2e0685",
                    "sense_definition": "Имеющий дефект или изъян, сделанный с нарушением стандартов качества.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "defective_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы вернули в магазин <w>бракованный</w> чайник и получили свои деньги обратно.",
                            "target_lang_translations": {
                                "en": "We returned the <w>defective</w> kettle to the store and got our money back."
                            }
                        },
                        {
                            "text": "Вся партия товара оказалась <w>бракованной</w>, поэтому её пришлось списать.",
                            "target_lang_translations": {
                                "en": "The entire batch of goods turned out to be <w>defective</w>, so it had to be written off."
                            }
                        }
                    ],
                    "synonyms": [
                        "дефектный",
                        "испорченный",
                        "некачественный"
                    ],
                    "antonyms": [
                        "качественный",
                        "годный",
                        "пригодный"
                    ],
                    "common_phrases": [
                        "бракованный товар",
                        "бракованная деталь",
                        "бракованное изделие",
                        "бракованная партия"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a defect or flaw, failing to meet quality standards."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defective",
                                "target_lang_sense_clarification": "having a fault or flaw"
                            },
                            {
                                "target_lang_word": "faulty",
                                "target_lang_sense_clarification": "not working correctly or made incorrectly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f071dd22-a225-3b8f-a603-f589d86b1dcd",
                    "sense_definition": "Негодный, непригодный для использования или выполнения функций (часто о людях или животных).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "defective_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После ранения солдат чувствовал себя <w>бракованным</w> и никому не нужным.",
                            "target_lang_translations": {
                                "en": "After the injury, the soldier felt <w>damaged</w> and useless to anyone."
                            }
                        },
                        {
                            "text": "Эта лошадь красивая, но <w>бракованная</w> для скачек из-за старой травмы.",
                            "target_lang_translations": {
                                "en": "This horse is beautiful, but <w>unfit</w> for racing due to an old injury."
                            }
                        }
                    ],
                    "synonyms": [
                        "невалидный",
                        "некачественный"
                    ],
                    "antonyms": [
                        "пригодный"
                    ],
                    "target_lang_definitions": {
                        "en": "Unfit or unsuitable for use (often referring to people or animals)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unfit",
                                "target_lang_sense_clarification": "not of the necessary quality or standard to perform a function"
                            },
                            {
                                "target_lang_word": "damaged",
                                "target_lang_sense_clarification": "figurative: psychologically or physically impaired"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "90c0980d-13bc-3f7f-93da-97b7d4c2e3db",
                    "sense_definition": "Страдательное причастие прошедшего времени от <w>браковать</w>; признанный негодным кем-либо.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лес, <w>бракованный</w> специалистами, отправляли на переработку в щепу.",
                            "target_lang_translations": {
                                "en": "The timber <w>rejected</w> by specialists was sent to be processed into wood chips."
                            }
                        },
                        {
                            "text": "Этот материал, <w>бракованный</w> комиссией ещё на первом этапе, всё равно попал на склад.",
                            "target_lang_translations": {
                                "en": "This material, <w>rejected</w> by the commission back at the first stage, still ended up in the warehouse."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Passive past participle of 'to reject'; declared unfit or substandard."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rejected",
                                "target_lang_sense_clarification": "dismissed as inadequate or unacceptable"
                            },
                            {
                                "target_lang_word": "discarded",
                                "target_lang_sense_clarification": "gotten rid of as useless or undesirable"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
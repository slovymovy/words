                {
                    "sense_id": "0e2b81e3-83d7-35bd-8ed3-b653532ed701",
                    "sense_definition": "Taki, który stawia wysokie wymagania, oczekuje dużo od innych lub od siebie i nie jest pobłażliwy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "strict_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz nowy nauczyciel matematyki jest bardzo <w>wymagający</w> wobec uczniów.",
                            "target_lang_translations": {
                                "ru": "Наш новый учитель математики очень <w>требователен</w> к ученикам."
                            }
                        },
                        {
                            "text": "To <w>wymagający</w> klient, który zawsze oczekuje najwyższej jakości usług.",
                            "target_lang_translations": {
                                "ru": "Это <w>требовательный</w> клиент, который всегда ожидает услуг высочайшего качества."
                            }
                        }
                    ],
                    "synonyms": [
                        "surowy",
                        "rygorystyczny",
                        "bezkompromisowy"
                    ],
                    "antonyms": [
                        "niewymagający",
                        "niewymagający"
                    ],
                    "common_phrases": [
                        "wymagający szef",
                        "wymagający klient",
                        "wymagający nauczyciel",
                        "wymagający rodzic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто предъявляет высокие требования, ожидает многого от других или от себя и не проявляет снисходительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "требовательный",
                                "target_lang_sense_clarification": "строгий, ожидающий высоких результатов"
                            },
                            {
                                "target_lang_word": "взыскательный",
                                "target_lang_sense_clarification": "строгий в выборе, разборчивый (часто о клиентах или вкусах)"
                            },
                            {
                                "target_lang_word": "строгий",
                                "target_lang_sense_clarification": "не допускающий снисхождения"

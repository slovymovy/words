                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простой",
                                "target_lang_sense_clarification": "скромный, без излишеств"
                            },
                            {
                                "target_lang_word": "незатейливый",
                                "target_lang_sense_clarification": "незамысловатый"
                            }
                        ]

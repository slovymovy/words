{
    "word_family": [
        "уходить",
        "ухаживать"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "80330c4f-e542-3fae-87ee-127fc8d82bef",
                    "sense_definition": "Действие по глаголу «уходить»; покидание какого-либо места, удаление откуда-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его внезапный <w>уход</w> удивил всех.",
                            "target_lang_translations": {
                                "en": "His sudden <w>departure</w> surprised everyone."
                            }
                        },
                        {
                            "text": "Ранний <w>уход</w> гостей расстроил хозяйку.",
                            "target_lang_translations": {
                                "en": "The guests' early <w>departure</w> upset the hostess."
                            }
                        }
                    ],
                    "synonyms": [
                        "отбытие"
                    ],
                    "antonyms": [
                        "приход"
                    ],
                    "common_phrases": [
                        "уход из дома",
                        "быстрый уход"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of leaving a place; departure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "departure",
                                "target_lang_sense_clarification": "act of leaving"
                            },
                            {
                                "target_lang_word": "leaving",
                                "target_lang_sense_clarification": "act of going away"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "348fff43-0414-3915-9c74-14f72e3a7e6b",
                    "sense_definition": "Забота о ком-либо или чём-либо, обслуживание и поддержание в порядке.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правильный <w>уход</w> за кожей очень важен для здоровья.",
                            "target_lang_translations": {
                                "en": "Proper skin <w>care</w> is very important for health."
                            }
                        },
                        {
                            "text": "Больному требуется постоянный медицинский <w>уход</w>.",
                            "target_lang_translations": {
                                "en": "The patient requires constant medical <w>care</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "забота",
                        "ухаживание"
                    ],
                    "antonyms": [
                        "невнимание",
                        "безразличие"
                    ],
                    "common_phrases": [
                        "уход за кожей",
                        "уход за больным",
                        "медицинский уход"
                    ],
                    "target_lang_definitions": {
                        "en": "The provision of what is necessary for the health, welfare, maintenance, and protection of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "care",
                                "target_lang_sense_clarification": "looking after someone or something"
                            },
                            {
                                "target_lang_word": "maintenance",
                                "target_lang_sense_clarification": "keeping something in good condition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94567fee-3ec4-35b4-a8e2-7b756dbf19a0",
                    "sense_definition": "Оставление должности, прекращение служебной или профессиональной деятельности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор объявил про свой <w>уход</w> на собрании.",
                            "target_lang_translations": {
                                "en": "The director announced his <w>resignation</w> at the meeting."
                            }
                        },
                        {
                            "text": "Его <w>уход</w> с поста был добровольным.",
                            "target_lang_translations": {
                                "en": "His <w>departure</w> from the post was voluntary."
                            }
                        }
                    ],
                    "common_phrases": [
                        "уход с работы",
                        "уход в отставку"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of leaving a job, position, or professional activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resignation",
                                "target_lang_sense_clarification": "act of quitting a job"
                            },
                            {
                                "target_lang_word": "retirement",
                                "target_lang_sense_clarification": "leaving one's job permanently, usually due to age"
                            },
                            {
                                "target_lang_word": "departure",
                                "target_lang_sense_clarification": "leaving a specific role"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb8a38eb-1a42-31a1-98dc-97fcbe78bdc7",
                    "sense_definition": "Смерть, кончина (обычно употребляется как смягчающее выражение).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тяжёлая болезнь ускорила его <w>уход</w>.",
                            "target_lang_translations": {
                                "en": "A severe illness hastened his <w>passing</w>."
                            }
                        },
                        {
                            "text": "Её безвременный <w>уход</w> из жизни стал трагедией для семьи.",
                            "target_lang_translations": {
                                "en": "Her untimely <w>passing</w> was a tragedy for the family."
                            }
                        }
                    ],
                    "synonyms": [
                        "смерть",
                        "кончина",
                        "уход из жизни"
                    ],
                    "antonyms": [
                        "воскрешение"
                    ],
                    "common_phrases": [
                        "безвременный уход",
                        "скорый уход"
                    ],
                    "target_lang_definitions": {
                        "en": "Death or passing away (often used euphemistically)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "passing",
                                "target_lang_sense_clarification": "euphemism for death"
                            },
                            {
                                "target_lang_word": "death",
                                "target_lang_sense_clarification": "end of life"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "588c2b34-fbb2-3a6d-a1d1-25e90df520be",
                    "sense_definition": "Het meervoud van <w>wees</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "orphan",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de oorlog moesten veel <w>wezen</w> een nieuw thuis vinden.",
                            "target_lang_translations": {
                                "ru": "После войны многим <w>сиротам</w> пришлось искать новый дом."
                            }
                        },
                        {
                            "text": "Het oude gebouw diende vroeger als opvanghuis voor arme <w>wezen</w>.",
                            "target_lang_translations": {
                                "ru": "Старое здание раньше служило приютом для бедных <w>сирот</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число существительного 'wees' (сирота)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сироты",
                                "target_lang_sense_clarification": "дети без родителей"
                            }
                        ]
                    }
                }
            ]
        }
    ]

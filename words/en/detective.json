{
    "word_family": [
        "detect",
        "detection",
        "detector",
        "detectable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ac0d7032-d2cc-3404-8d9d-6fc8db137537",
                    "sense_definition": "A police officer whose job is to discover information about crimes and catch criminals.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "law_enforcement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>detective</w> interviewed the witness to gather more details about the robbery.",
                            "target_lang_translations": {
                                "ru": "<w>Детектив</w> опросил свидетеля, чтобы собрать больше подробностей об ограблении."
                            }
                        },
                        {
                            "text": "She was promoted to <w>detective</w> after serving five years as a uniformed officer.",
                            "target_lang_translations": {
                                "ru": "Её повысили до <w>детектива</w> после пяти лет службы в патрульной полиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "investigator",
                        "police detective"
                    ],
                    "common_phrases": [
                        "police detective",
                        "homicide detective",
                        "detective inspector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полицейский, в чьи обязанности входит расследование преступлений, сбор улик и поимка преступников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детектив",
                                "target_lang_sense_clarification": "сотрудник полиции"
                            },
                            {
                                "target_lang_word": "следователь",
                                "target_lang_sense_clarification": "должностное лицо, ведущее следствие"
                            },
                            {
                                "target_lang_word": "оперативник",
                                "target_lang_sense_clarification": "сотрудник уголовного розыска"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ed320a4-b114-3fda-8cb2-b0edf5071157",
                    "sense_definition": "A person employed to find information or investigate situations, often privately rather than for the police.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "private_investigation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He hired a private <w>detective</w> to find his missing brother.",
                            "target_lang_translations": {
                                "ru": "Он нанял частного <w>детектива</w>, чтобы найти своего пропавшего брата."
                            }
                        },
                        {
                            "text": "Sherlock Holmes is the most famous fictional <w>detective</w> in the world.",
                            "target_lang_translations": {
                                "ru": "Шерлок Холмс — самый известный вымышленный <w>сыщик</w> в мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "private investigator",
                        "private detective",
                        "sleuth",
                        "investigator"
                    ],
                    "common_phrases": [
                        "private detective",
                        "consulting detective"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, нанятый для поиска информации или проведения расследований, часто в частном порядке, а не от лица полиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детектив",
                                "target_lang_sense_clarification": "частный сыщик"
                            },
                            {
                                "target_lang_word": "сыщик",
                                "target_lang_sense_clarification": "тот, кто ведет расследование"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5c024667-c8d4-323c-b8c2-032947f6a4a3",
                    "sense_definition": "Relating to the work of investigating crimes or uncovering information.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It took a lot of <w>detective</w> work to find the missing documents.",
                            "target_lang_translations": {
                                "ru": "Потребовалось много <w>детективной</w> работы, чтобы найти пропавшие документы."
                            }
                        },
                        {
                            "text": "She enjoys reading a good <w>detective</w> story before going to sleep.",
                            "target_lang_translations": {
                                "ru": "Она любит почитать хороший <w>детектив</w> перед сном."
                            }
                        }
                    ],
                    "common_phrases": [
                        "detective story",
                        "detective novel",
                        "detective work"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к работе по расследованию преступлений или к жанру произведений, описывающих такие расследования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детективный",
                                "target_lang_sense_clarification": "связанный с расследованием"
                            },
                            {
                                "target_lang_word": "детектив",
                                "target_lang_sense_clarification": "жанр литературы или кино"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
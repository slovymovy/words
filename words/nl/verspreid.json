                },
                {
                    "sense_id": "413baef3-a402-38b9-963f-1060d0169a7a",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>verspreiden</w>",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>verspreid</w> de folders huis-aan-huis in de wijk.",
                            "target_lang_translations": {
                                "ru": "Я <w>распространяю</w> листовки по домам в этом районе."
                            }
                        },
                        {
                            "text": "Als ik nies, <w>verspreid</w> ik onbedoeld bacteriën.",
                            "target_lang_translations": {
                                "ru": "Когда я чихаю, я непреднамеренно <w>распространяю</w> бактерии."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "1-е лицо единственного числа настоящего времени от глагола verspreiden"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распространяю",
                                "target_lang_sense_clarification": "действие от первого лица"
                            },
                            {
                                "target_lang_word": "раздаю",
                                "target_lang_sense_clarification": "в контексте материалов, листовок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c4ac271-e155-36d3-9d9c-c08e4a405792",
                    "sense_definition": "Gebiedende wijs van <w>verspreiden</w>",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Verspreid</w> je over de zaal zodat iedereen genoeg ruimte heeft.",
                            "target_lang_translations": {
                                "ru": "<w>Рассредоточьтесь</w> по залу, чтобы у всех было достаточно места."
                            }
                        },
                        {
                            "text": "<w>Verspreid</w> geen geruchten die niet waar zijn.",
                            "target_lang_translations": {
                                "ru": "Не <w>распространяй</w> ложные слухи."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола verspreiden"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распространяй",
                                "target_lang_sense_clarification": "приказ или просьба"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dcafb1ae-daea-3297-a3bc-50db5ae1abc8",
                    "sense_definition": "Отправлять что-либо (письмо, груз, деньги) по почте или через посредника для доставки кому-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "send_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я каждую неделю <посылаю> письма своим родителям в другой город.",
                            "target_lang_translations": {
                                "en": "Every week I <send> letters to my parents in another city."
                            }
                        },
                        {
                            "text": "Бабушка любит <посылать> нам вкусные подарки на праздники.",
                            "target_lang_translations": {
                                "en": "Grandma loves to <send> us tasty gifts for the holidays."
                            }
                        }
                    ],
                    "synonyms": [
                        "отправля́ть",
                        "слать",
                        "отсыла́ть"
                    ],
                    "antonyms": [
                        "получать",
                        "принимать"
                    ],
                    "common_phrases": [
                        "посылать письмо",
                        "посылать деньги",
                        "посылать посылку",
                        "посылать сообщение"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause something (such as a letter, package, or money) to go from one place to another via mail or an intermediary."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "send",
                                "target_lang_sense_clarification": "general term for transferring objects or information"
                            },
                            {
                                "target_lang_word": "mail",
                                "target_lang_sense_clarification": "specifically for letters or packages sent via postal service"
                            },
                            {
                                "target_lang_word": "dispatch",
                                "target_lang_sense_clarification": "formal, implying speed or official business"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d1ac467-547f-391f-8269-4cb5f2b65c4d",
                    "sense_definition": "Направлять кого-либо куда-либо с определенной целью, поручением или заданием.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "send_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама часто <посылает> сына в магазин за свежим хлебом.",
                            "target_lang_translations": {
                                "en": "Mom often <sends> her son to the store for fresh bread."
                            }
                        },
                        {
                            "text": "Директор решил <посылать> лучшего сотрудника на важную конференцию.",
                            "target_lang_translations": {
                                "en": "The director decided to <send> the best employee to an important conference."
                            }
                        }
                    ],
                    "synonyms": [
                        "отправля́ть",
                        "отсыла́ть"
                    ],
                    "antonyms": [
                        "вызывать",
                        "звать"
                    ],
                    "common_phrases": [
                        "посылать в магазин",
                        "посылать за врачом",
                        "посылать на помощь",
                        "посылать в командировку"
                    ],
                    "target_lang_definitions": {
                        "en": "To direct or order someone to go to a specific place for a particular purpose or task."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "send",
                                "target_lang_sense_clarification": "general term for directing a person"
                            },
                            {
                                "target_lang_word": "dispatch",
                                "target_lang_sense_clarification": "formal, often used for employees, agents, or messengers"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d357a9f-202a-3d94-9806-db6ace1f12a4",
                    "sense_definition": "Направлять движение чего-либо (мяча, взгляда, жеста) в определенную сторону.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "direct_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Футболист умеет точно <посылать> мяч в верхний угол ворот.",
                            "target_lang_translations": {
                                "en": "The football player knows how to accurately <send> the ball into the top corner of the goal."
                            }
                        },
                        {
                            "text": "Девушка улыбнулась и <посылала> воздушные поцелуи своим поклонникам.",
                            "target_lang_translations": {
                                "en": "The girl smiled and <blew> air kisses to her admirers."
                            }
                        }
                    ],
                    "synonyms": [
                        "отправля́ть",
                        "слать"
                    ],
                    "common_phrases": [
                        "посылать мяч",
                        "посылать воздушный поцелуй",
                        "посылать улыбку",
                        "посылать проклятия"
                    ],
                    "target_lang_definitions": {
                        "en": "To direct the movement or trajectory of something (like a ball, glance, or gesture) towards a specific target."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "send",
                                "target_lang_sense_clarification": "to propel an object or direct a signal"
                            },
                            {
                                "target_lang_word": "blow",
                                "target_lang_sense_clarification": "specifically used in the phrase 'blow a kiss'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03e4cd78-5cba-38db-8aaa-51406f46b26b",
                    "sense_definition": "Грубо ругать кого-либо, требуя уйти или отстать; использовать нецензурные выражения в адрес кого-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "swear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не стоит грубить и <посылать> людей, даже если ты очень расстроен.",
                            "target_lang_translations": {
                                "en": "You shouldn't be rude and <tell> people <to get lost>, even if you are very upset."
                            }
                        },
                        {
                            "text": "В гневе он начал кричать и <посылать> всех к чёрту.",
                            "target_lang_translations": {
                                "en": "In anger, he started shouting and <sending> everyone to hell."
                            }
                        }
                    ],
                    "common_phrases": [
                        "посылать к чёрту",
                        "посылать подальше",
                        "посылать матом"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To rudely tell someone to go away or leave one alone, often using profanity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tell to get lost",
                                "target_lang_sense_clarification": "idiomatic, rude dismissal"
                            },
                            {
                                "target_lang_word": "send to hell",
                                "target_lang_sense_clarification": "literal translation of common phrases, expressing anger"
                            },
                            {
                                "target_lang_word": "cuss out",
                                "target_lang_sense_clarification": "slang, to swear at someone"
                            }
                        ]
                    }
                }
            ]

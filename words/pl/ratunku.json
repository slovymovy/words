                    "sense_id": "a706ca08-9102-31fe-abde-aaadad898863",
                    "sense_definition": "Okrzyk wzywający pomocy w sytuacji zagrożenia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ludzie, <w>ratunku</w>, ten dom się pali!",
                            "target_lang_translations": {
                                "ru": "Люди, <w>помогите</w>, этот дом горит!"
                            }
                        },
                        {
                            "text": "Zaczął głośno krzyczeć: <w>ratunku</w>, tonę!",
                            "target_lang_translations": {
                                "ru": "Он начал громко кричать: <w>спасите</w>, я тону!"
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "753bf857-658c-38ad-b022-ab78136d3bb9",
                    "sense_definition": "Bier (of andere dranken) bereiden door ingrediënten zoals water, mout en hop te koken en te laten gisten.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De monniken in dit klooster <w>brouwen</w> al eeuwenlang hun eigen trappistenbier.",
                            "target_lang_translations": {
                                "en": "The monks in this monastery have been <w>brewing</w> their own Trappist beer for centuries."
                            }
                        },
                        {
                            "text": "Mijn buurman heeft een nieuwe hobby en leert nu thuis bier te <w>brouwen</w>.",
                            "target_lang_translations": {
                                "en": "My neighbor has a new hobby and is now learning to <w>brew</w> beer at home."
                            }
                        }
                    ],
                    "synonyms": [
                        "bereiden"
                    ],
                    "common_phrases": [
                        "bier brouwen"
                    ],
                    "target_lang_definitions": {
                        "en": "To prepare beer (or other drinks) by boiling ingredients such as water, malt, and hops and allowing them to ferment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brew",
                                "target_lang_sense_clarification": "beer or coffee/tea"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44a6c900-40c2-325e-9a1b-f342c51f239c",
                    "sense_definition": "Iets samenstellen of mengen uit verschillende ingrediënten, vaak op een creatieve of experimentele manier.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De barman kan voor elke gast een unieke cocktail <w>brouwen</w>.",
                            "target_lang_translations": {
                                "en": "The bartender can <w>concoct</w> a unique cocktail for every guest."
                            }
                        },
                        {
                            "text": "De heks stond in haar huisje een magisch drankje te <w>brouwen</w>.",
                            "target_lang_translations": {
                                "en": "The witch stood in her cottage <w>brewing</w> a magic potion."
                            }
                        }
                    ],
                    "synonyms": [
                        "klaarmaken",
                        "bereiden",
                        "bereiden"
                    ],
                    "common_phrases": [
                        "een drankje brouwen"
                    ],
                    "target_lang_definitions": {
                        "en": "To compose or mix something from various ingredients, often in a creative or experimental way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "concoct",
                                "target_lang_sense_clarification": "a mixture or drink"
                            },
                            {
                                "target_lang_word": "brew",
                                "target_lang_sense_clarification": "a potion or magical drink"
                            },
                            {
                                "target_lang_word": "mix",
                                "target_lang_sense_clarification": "cocktails or drinks"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83c51134-799a-31ba-9b76-538e6e3f4e49",
                    "sense_definition": "Iets verzinnen, in elkaar zetten of begrijpen (vaak gebruikt in negatieve of twijfelachtige context).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_making",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leerling probeerde snel een excuus te <w>brouwen</w> voor zijn late binnenkomst.",
                            "target_lang_translations": {
                                "en": "The student tried to quickly <w>cook up</w> an excuse for his late arrival."
                            }
                        },
                        {
                            "text": "De handleiding was zo onduidelijk dat ik er helemaal niets van kon <w>brouwen</w>.",
                            "target_lang_translations": {
                                "en": "The manual was so unclear that I couldn't <w>make</w> heads or tails of it."
                            }
                        }
                    ],
                    "synonyms": [
                        "bereiden",
                        "klaarmaken"
                    ],
                    "common_phrases": [
                        "er niets van brouwen",
                        "een plan brouwen"
                    ],
                    "target_lang_definitions": {
                        "en": "To invent, put together, or understand something (often used in a negative or doubtful context)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "concoct",
                                "target_lang_sense_clarification": "a plan or excuse"
                            },
                            {
                                "target_lang_word": "cook up",
                                "target_lang_sense_clarification": "an excuse or story"
                            },
                            {
                                "target_lang_word": "make sense of",
                                "target_lang_sense_clarification": "understand (usually negative)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d6a1667-f920-3761-bc1f-69f2ece96512",
                    "sense_definition": "De letter 'r' uitspreken met de huig achter in de keel in plaats van met de tongpunt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In bepaalde regio's van Nederland hoor je mensen de r <w>brouwen</w>.",
                            "target_lang_translations": {
                                "en": "In certain regions of the Netherlands, you hear people <w>burr</w> the r."
                            }
                        },
                        {
                            "text": "De logopedist oefende met het kind om de r te rollen in plaats van te <w>brouwen</w>.",
                            "target_lang_translations": {
                                "en": "The speech therapist practiced with the child to roll the r instead of <w>burring</w> it."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de r brouwen"
                    ],
                    "target_lang_definitions": {
                        "en": "To pronounce the letter 'r' with the uvula at the back of the throat instead of the tip of the tongue."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "burr",
                                "target_lang_sense_clarification": "pronounce with a uvular sound"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
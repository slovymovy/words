    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ca900dc0-c7f5-390b-bb5a-d8f374886067",
                    "sense_definition": "A unit of land area equal to 4,840 square yards or approximately 4,047 square metres.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer owns fifty <w>acres</w> of fertile land used for growing corn.",
                            "target_lang_translations": {
                                "ru": "Фермер владеет пятьюдесятью <w>акрами</w> плодородной земли, используемой для выращивания кукурузы."
                            }
                        },
                        {
                            "text": "They decided to buy an <w>acre</w> of property to build their dream home.",
                            "target_lang_translations": {
                                "ru": "Они решили купить <w>акр</w> земли, чтобы построить дом своей мечты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "an acre of land",
                        "price per acre",
                        "acres of woodland"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения площади земли, равная 4840 квадратным ярдам или примерно 4047 квадратным метрам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акр",
                                "target_lang_sense_clarification": "единица площади"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62b44951-6d8b-3a4f-91a5-4a5daf28030e",
                    "sense_definition": "A very large area or amount of something, often used hyperbolically.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new warehouse has <w>acres</w> of space for storing inventory.",
                            "target_lang_translations": {
                                "ru": "На новом складе <w>огромное количество</w> места для хранения инвентаря."
                            }
                        },
                        {
                            "text": "We have <w>acres</w> of time to finish the project before the deadline.",
                            "target_lang_translations": {
                                "ru": "У нас <w>вагон</w> времени, чтобы закончить проект до дедлайна."
                            }
                        }
                    ],
                    "common_phrases": [
                        "acres of space",
                        "acres of room",
                        "acres of time"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to emphasize large size or quantity"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень большая площадь или количество чего-либо (часто используется в переносном смысле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "огромное количество",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "гектары",
                                "target_lang_sense_clarification": "о большом пространстве"
                            },
                            {
                                "target_lang_word": "вагон",
                                "target_lang_sense_clarification": "разг., о времени"
                            }
                        ]
                    }
                }
            ]
        },
        {

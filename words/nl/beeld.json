                            },
                            {
                                "target_lang_word": "idea",
                                "target_lang_sense_clarification": "concept"
                            },
                            {
                                "target_lang_word": "image",
                                "target_lang_sense_clarification": "impression"
                            }
                        ],
                        "ru": [

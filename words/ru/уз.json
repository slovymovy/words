                    "sense_id": "2c67c4ea-dde5-32dc-a10f-8db64da8143b",
                    "sense_definition": "Форма родительного падежа множественного числа существительного <w>узы</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "form_uzy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Они прожили долгую жизнь без брачных <уз>, но в полной гармонии.",
                            "target_lang_translations": {
                                "en": "They lived a long life without marriage <bonds>, but in perfect harmony."
                            }
                        },
                        {
                            "text": "Герой стремился освободиться от <уз> старых традиций и предрассудков.",
                            "target_lang_translations": {
                                "en": "The hero sought to free himself from the <bonds> of old traditions and prejudices."
                            }
                        }
                    ],
                    "synonyms": [
                        "оков",
                        "пут"
                    ],
                    "common_phrases": [
                        "брачных уз",
                        "семейных уз",
                        "священных уз"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive plural form of the noun \"узы\" (bonds, ties)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bonds",
                                "target_lang_sense_clarification": "restraints or uniting forces"
                            },
                            {
                                "target_lang_word": "ties",
                                "target_lang_sense_clarification": "connections or relationships"
                            }
                        ]
                    }

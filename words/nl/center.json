        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "64e7de18-589f-3b27-a175-70568da96cb1",
                    "sense_definition": "Een plaats of gebouw waar specifieke activiteiten of diensten geconcentreerd zijn (vaak gebruikt in samenstellingen of als anglicisme).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gaan zaterdag winkelen in het grote shopping <w>center</w>.",
                            "target_lang_translations": {
                                "en": "We are going shopping at the large shopping <w>center</w> on Saturday.",
                                "ru": "В субботу мы идем за покупками в большой торговый <w>центр</w>."
                            }
                        },
                        {
                            "text": "Het medisch <w>center</w> is dagelijks geopend voor patiënten.",
                            "target_lang_translations": {
                                "en": "The medical <w>center</w> is open daily for patients.",
                                "ru": "Медицинский <w>центр</w> открыт для пациентов ежедневно."
                            }
                        }
                    ],
                    "synonyms": [
                        "centrum"
                    ],
                    "common_phrases": [
                        "shopping center",
                        "medisch center"
                    ],
                    "target_lang_definitions": {
                        "en": "A place or building where specific activities or services are concentrated.",
                        "ru": "Место или здание, где сосредоточены определенные виды деятельности или услуги."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "center",
                                "target_lang_sense_clarification": "building or complex"
                            },
                            {
                                "target_lang_word": "centre",
                                "target_lang_sense_clarification": "building or complex (UK spelling)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "центр",
                                "target_lang_sense_clarification": "здание или комплекс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa49ea1c-da45-3fa0-a60b-226d57d28b82",
                    "sense_definition": "Een speler die een centrale positie inneemt in de opstelling van een sportteam, bijvoorbeeld bij basketbal.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De lange <w>center</w> pakte de bal onder de ring.",
                            "target_lang_translations": {
                                "en": "The tall <w>center</w> grabbed the ball under the hoop.",
                                "ru": "Высокий <w>центровой</w> поймал мяч под кольцом."
                            }
                        },
                        {
                            "text": "Onze <w>center</w> scoorde de meeste punten in de wedstrijd.",
                            "target_lang_translations": {
                                "en": "Our <w>center</w> scored the most points in the game.",
                                "ru": "Наш <w>центровой</w> набрал больше всех очков в игре."
                            }
                        }
                    ],
                    "synonyms": [
                        "middenspeler"
                    ],
                    "target_lang_definitions": {
                        "en": "A player who takes a central position in the lineup of a sports team, for example in basketball.",
                        "ru": "Игрок, занимающий центральную позицию в составе спортивной команды, например, в баскетболе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "center",
                                "target_lang_sense_clarification": "sports position"

            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a7ad0659-429a-3c7c-a1ab-666d28d764b0",
                    "sense_definition": "Słowo służące do wyrażenia dużego natężenia cechy lub intensywności czynności; oznacza, że czegoś jest dużo lub dzieje się to z dużą siłą.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój nowy pomysł na projekt jest <w>bardzo</w> ciekawy.",
                            "target_lang_translations": {
                                "ru": "Твоя новая идея проекта <w>очень</w> интересная."
                            }
                        },
                        {
                            "text": "<w>Bardzo</w> dziękuję za twoją pomoc i wsparcie.",
                            "target_lang_translations": {
                                "ru": "<w>Большое</w> спасибо за твою помощь и поддержку."
                            }
                        },
                        {
                            "text": "Wczoraj w nocy padał <w>bardzo</w> ulewny deszcz.",
                            "target_lang_translations": {
                                "ru": "Вчера ночью шёл <w>очень</w> сильный дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogromnie",
                        "wielce",
                        "niezwykle",
                        "mocno",
                        "szalenie",
                        "niesamowicie",
                        "niezmiernie"
                    ],
                    "antonyms": [
                        "mało",
                        "niezbyt"
                    ],
                    "common_phrases": [
                        "bardzo proszę",
                        "bardzo dziękuję",
                        "bardzo mi miło",
                        "bardzo mi przykro",
                        "bardzo dobrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое для выражения высокой степени качества или интенсивности действия; означает, что чего-то много или что-то происходит с большой силой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очень",
                                "target_lang_sense_clarification": "в высокой степени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "398e40f4-9003-36c5-ae29-7be06d31ac71",
                    "sense_definition": "Słowo pomocnicze występujące w formach „bardziej” i „najbardziej”, służące do tworzenia stopnia wyższego i najwyższego przymiotników oraz przysłówków.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten samochód jest <w>bardziej</w> ekonomiczny niż mój stary.",
                            "target_lang_translations": {
                                "ru": "Этот автомобиль <w>более</w> экономичный, чем мой старый."
                            }
                        },
                        {
                            "text": "To jest <w>najbardziej</w> interesująca książka, jaką czytałem.",
                            "target_lang_translations": {
                                "ru": "Это <w>наиболее</w> интересная книга, которую я читал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "coraz bardziej",
                        "tym bardziej"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "stopniowanie opisowe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вспомогательное слово, выступающее в формах «более» и «наиболее», служащее для образования сравнительной и превосходной степени прилагательных и наречий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "более",
                                "target_lang_sense_clarification": "сравнительная степень"
                            },
                            {
                                "target_lang_word": "наиболее",
                                "target_lang_sense_clarification": "превосходная степень"
                            },
                            {
                                "target_lang_word": "больше",
                                "target_lang_sense_clarification": "сравнительная степень (разг.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "3ce45f4a-e922-3ffa-8906-eae3b6d54e9b",
                    "sense_definition": "Een groep mensen die samenwerkt, vaak met slechte of criminele bedoelingen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_gang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie heeft de hele <w>bende</w> gearresteerd na de overval.",
                            "target_lang_translations": {
                                "en": "The police arrested the entire <w>gang</w> after the robbery.",
                                "ru": "Полиция арестовала всю <w>банду</w> после ограбления."
                            }
                        },
                        {
                            "text": "Hij kwam in de problemen omdat hij lid was van een criminele <w>bende</w>.",
                            "target_lang_translations": {
                                "en": "He got into trouble because he was a member of a criminal <w>gang</w>.",
                                "ru": "У него возникли проблемы, потому что он был членом преступной <w>банды</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gang",
                        "troep"
                    ],
                    "common_phrases": [
                        "een criminele bende",
                        "lid van een bende",
                        "bij de bende"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people working together, often with bad or criminal intentions.",
                        "ru": "Группа людей, действующих сообща, часто с плохими или преступными намерениями."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gang",
                                "target_lang_sense_clarification": "criminal group"
                            },
                            {
                                "target_lang_word": "mob",
                                "target_lang_sense_clarification": "disorderly group"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "банда",
                                "target_lang_sense_clarification": "преступная группа"
                            },
                            {
                                "target_lang_word": "шайка",
                                "target_lang_sense_clarification": "группа (часто уничижительно)"
                            }
                        ]
                    }
                },

        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "11134d06-d3f6-3a85-84bd-a1c72b2b3602",
                    "sense_definition": "To add a person to a list of contacts or friends on a social networking site.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action_digital",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>friend</w> you on the app as soon as I get home.",
                            "target_lang_translations": {
                                "ru": "Я <w>добавлю</w> тебя <w>в друзья</w> в приложении, как только вернусь домой."
                            }
                        },
                        {
                            "text": "She decided to <w>friend</w> all her old classmates to stay in touch.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>зафрендить</w> всех своих бывших одноклассников, чтобы оставаться на связи."
                            }
                        }
                    ],
                    "antonyms": [
                        "unfriend",
                        "defriend"
                    ],
                    "common_phrases": [
                        "friend someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавить кого-либо в список друзей в социальной сети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добавить в друзья",
                                "target_lang_sense_clarification": "стандартный термин"
                            },
                            {
                                "target_lang_word": "зафрендить",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]

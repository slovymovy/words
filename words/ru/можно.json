    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bc4e5e97-4bb2-360e-91da-ec3863bad0e2",
                    "sense_definition": "Разрешается, позволительно; нет запрета на совершение действия.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама сказала, что мне <w>можно</w> пойти гулять.",
                            "target_lang_translations": {
                                "en": "Mom said that I <w>can</w> go for a walk."
                            }
                        },
                        {
                            "text": "Извините, <w>можно</w> мне взять этот стул?",
                            "target_lang_translations": {
                                "en": "Excuse me, <w>may</w> I take this chair?"
                            }
                        },
                        {
                            "text": "Здесь <w>можно</w> парковать машину только по выходным.",
                            "target_lang_translations": {
                                "en": "You <w>can</w> park the car here only on weekends."
                            }
                        }
                    ],
                    "synonyms": [
                        "дозволительно",
                        "позволительно"
                    ],
                    "antonyms": [
                        "нельзя",
                        "непозволительно"
                    ],
                    "common_phrases": [
                        "можно войти",
                        "можно спросить",
                        "если можно"
                    ],
                    "target_lang_definitions": {
                        "en": "It is permitted or allowed; there is no prohibition against an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "can",
                                "target_lang_sense_clarification": "to have permission"
                            },
                            {
                                "target_lang_word": "may",
                                "target_lang_sense_clarification": "to have permission (formal)"
                            },
                            {
                                "target_lang_word": "allowed",
                                "target_lang_sense_clarification": "permitted"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01b9f636-3cb9-3b61-b8e0-6ad0bb431d02",
                    "sense_definition": "Имеется возможность, существуют условия для осуществления чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В библиотеке <w>можно</w> найти много интересных книг.",
                            "target_lang_translations": {
                                "en": "In the library, one <w>can</w> find many interesting books."
                            }
                        },
                        {
                            "text": "Пожалуйста, позвони мне как <w>можно</w> скорее.",
                            "target_lang_translations": {
                                "en": "Please call me as soon as <w>possible</w>."
                            }
                        },
                        {
                            "text": "Эту проблему <w>можно</w> решить разными способами.",
                            "target_lang_translations": {
                                "en": "This problem <w>can</w> be solved in different ways."
                            }
                        }
                    ],
                    "synonyms": [
                        "возможно",
                        "есть возможность"
                    ],
                    "antonyms": [
                        "нельзя",
                        "невозможно"
                    ],
                    "common_phrases": [
                        "как можно скорее",
                        "можно подумать",
                        "можно сказать"
                    ],
                    "target_lang_definitions": {
                        "en": "It is possible; conditions exist to perform an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "can",
                                "target_lang_sense_clarification": "to be possible"
                            },
                            {
                                "target_lang_word": "possible",
                                "target_lang_sense_clarification": "feasible"
                            }
                        ]
                    }
                }
            ]
        }
    ]

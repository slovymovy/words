    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "57483e91-fef9-3a71-8354-016958077df5",
                    "sense_definition": "Zaproponować komuś coś lub kogoś, wyrażając o tym pozytywną opinię; doradzić wybór czegoś wartościowego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Możesz mi <polecić> dobrego dentystę w tej okolicy?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <порекомендовать> мне хорошего стоматолога в этом районе?"
                            }
                        },
                        {
                            "text": "Nauczyciel <polecił> uczniom ciekawą książkę do przeczytania w domu.",
                            "target_lang_translations": {
                                "ru": "Учитель <посоветовал> ученикам интересную книгу для чтения дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "powierzyć"
                    ],
                    "antonyms": [
                        "odradzić"
                    ],
                    "common_phrases": [
                        "polecić dobrą książkę",
                        "gorąco polecić",
                        "polecić lekarza",
                        "polecić usługi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предложить кому-то что-то или кого-то, выражая положительное мнение; посоветовать выбрать что-то стоящее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порекомендовать",
                                "target_lang_sense_clarification": "дать рекомендацию"
                            },
                            {
                                "target_lang_word": "посоветовать",
                                "target_lang_sense_clarification": "дать совет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85172beb-adc7-3255-8e09-0a0116a523d1",
                    "sense_definition": "Wydać komuś polecenie lub nakaz wykonania jakiejś czynności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor <polecił> sekretarce przygotowanie raportu na jutro rano.",
                            "target_lang_translations": {
                                "ru": "Директор <поручил> секретарше подготовить отчет на завтрашнее утро."
                            }
                        },
                        {
                            "text": "Policjant <polecił> kierowcy zjechać na pobocze do kontroli.",
                            "target_lang_translations": {
                                "ru": "Полицейский <велел> водителю съехать на обочину для проверки."
                            }
                        }
                    ],
                    "synonyms": [
                        "powierzyć"
                    ],
                    "antonyms": [
                        "zabronić"
                    ],
                    "common_phrases": [
                        "polecić wykonanie zadania",
                        "polecić ciszę",
                        "polecić opuszczenie sali"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать кому-то указание или приказ выполнить какое-либо действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поручить",
                                "target_lang_sense_clarification": "дать задание"
                            },
                            {
                                "target_lang_word": "велеть",
                                "target_lang_sense_clarification": "приказать сделать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1094ad97-992a-3168-9907-7e5c34633cb0",
                    "sense_definition": "Oddać kogoś lub coś pod czyjąś opiekę lub nadzór.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wyjeżdżając na wakacje, <polecił> swój dom opiece sąsiadów.",
                            "target_lang_translations": {
                                "ru": "Уезжая в отпуск, он <поручил> свой дом заботам соседей."
                            }
                        },
                        {
                            "text": "Matka <poleciła> dziecko nauczycielce przed wyjściem do pracy.",
                            "target_lang_translations": {
                                "ru": "Мать <поручила> ребенка учительнице перед уходом на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "powierzyć"
                    ],
                    "common_phrases": [
                        "polecić opiece",
                        "polecić Bogu"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie rzadziej używane w tym znaczeniu"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать кого-то или что-то под чью-то опеку или надзор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поручить",
                                "target_lang_sense_clarification": "доверить заботу"
                            },
                            {
                                "target_lang_word": "вверить",
                                "target_lang_sense_clarification": "передать под охрану"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "361e54d1-4d1e-3562-9620-179fa8caa2f1",
                    "sense_definition": "W formie zwrotnej: oddać się pod czyjąś opiekę lub prosić o pamięć o sobie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W modlitwie chory <polecił> się miłosierdziu Bożemu.",
                            "target_lang_translations": {
                                "ru": "В молитве больной <вверился> божьему милосердию."
                            }
                        },
                        {
                            "text": "Odchodząc z przyjęcia, gość <polecił> się łaskawej pamięci gospodarzy.",
                            "target_lang_translations": {
                                "ru": "Уходя с приема, гость <поручил> себя доброй памяти хозяев."
                            }
                        }
                    ],
                    "common_phrases": [
                        "polecić się pamięci",
                        "polecić się Bogu"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В возвратной форме: отдать себя под чью-то опеку или просить помнить о себе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ввериться",
                                "target_lang_sense_clarification": "отдать себя под покровительство"
                            },
                            {
                                "target_lang_word": "поручить себя",
                                "target_lang_sense_clarification": "доверить себя"
                            }
                        ]
                    }
                }
            ]
        }
    ]

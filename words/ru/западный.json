    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "827e54b5-e5c0-3fd8-8574-c7a594500edc",
                    "sense_definition": "Относящийся к западу как стороне света; расположенный на западе или дующий с запада (о ветре).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный <w>западный</w> ветер принёс в город прохладу и дождь.",
                            "target_lang_translations": {
                                "en": "A strong <w>western</w> wind brought coolness and rain to the city."
                            }
                        },
                        {
                            "text": "Мы решили поехать на <w>западный</w> берег озера, чтобы увидеть закат.",
                            "target_lang_translations": {
                                "en": "We decided to go to the <w>western</w> shore of the lake to see the sunset."
                            }
                        }
                    ],
                    "antonyms": [
                        "восто́чный"
                    ],
                    "common_phrases": [
                        "западный ветер",
                        "западный берег",
                        "западная граница",
                        "западное направление"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the west as a cardinal direction; located in the west or blowing from the west (of wind)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "western",
                                "target_lang_sense_clarification": "relating to the cardinal direction or location"
                            },
                            {
                                "target_lang_word": "westerly",
                                "target_lang_sense_clarification": "blowing from the west (of wind)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbcf226b-520e-389e-bec8-cb2c165a707f",
                    "sense_definition": "Свойственный странам Запада (обычно Европе и Северной Америке), их культуре, политике или образу жизни.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Многие молодые люди стараются перенять <w>западный</w> образ жизни.",
                            "target_lang_translations": {
                                "en": "Many young people try to adopt a <w>Western</w> lifestyle."
                            }
                        },
                        {
                            "text": "В этом музее широко представлено <w>западное</w> искусство двадцатого века.",
                            "target_lang_translations": {
                                "en": "This museum extensively features <w>Western</w> art of the twentieth century."
                            }
                        }
                    ],
                    "antonyms": [
                        "восто́чный"
                    ],
                    "common_phrases": [
                        "западная культура",
                        "западный мир",
                        "западные страны",
                        "западный стиль"
                    ],
                    "target_lang_definitions": {
                        "en": "Characteristic of Western countries (usually Europe and North America), their culture, politics, or lifestyle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Western",
                                "target_lang_sense_clarification": "relating to the Occident (culture/politics)"
                            }
                        ]
                    }
                }
            ]

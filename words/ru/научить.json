    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c3db6bc9-62b2-3a50-a99c-a321795ef1b8",
                    "sense_definition": "Передать кому-либо знания или навыки, сделать способным выполнять какое-либо действие.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой дедушка <научил> меня играть в шахматы.",
                            "target_lang_translations": {
                                "en": "My grandfather <taught> me to play chess."
                            }
                        },
                        {
                            "text": "В школе нас <научили> читать и писать без ошибок.",
                            "target_lang_translations": {
                                "en": "At school, they <taught> us to read and write without mistakes."
                            }
                        },
                        {
                            "text": "Она <научила> свою дочь печь вкусные пироги.",
                            "target_lang_translations": {
                                "en": "She <taught> her daughter to bake delicious pies."
                            }
                        }
                    ],
                    "synonyms": [
                        "обучить"
                    ],
                    "common_phrases": [
                        "научить плавать",
                        "научить читать",
                        "научить жизни",
                        "научить рисовать"
                    ],
                    "target_lang_definitions": {
                        "en": "To pass knowledge or skills to someone, to make them capable of performing an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "teach",
                                "target_lang_sense_clarification": "transfer knowledge or skills"
                            },
                            {
                                "target_lang_word": "instruct",
                                "target_lang_sense_clarification": "formal training"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "616b8b78-2ba3-3f1c-8bb3-9f99f6972eee",
                    "sense_definition": "Заставить понять что-либо или наказать, применяя строгие меры (часто используется как угроза).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я тебя <научу>, как нужно вести себя в обществе!",
                            "target_lang_translations": {
                                "en": "I will <teach> you how to behave in society!"
                            }
                        },
                        {
                            "text": "Жизнь быстро <научит> его уважать чужой труд.",
                            "target_lang_translations": {
                                "en": "Life will quickly <teach> him to respect other people's work."
                            }
                        }
                    ],
                    "common_phrases": [
                        "я тебя научу",
                        "жизнь научит"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone to understand something or to punish them (often used as a threat)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "teach",
                                "target_lang_sense_clarification": "a lesson or punishment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39d3f398-f0ff-372a-86be-97221d3fdeff",
                    "sense_definition": "Посоветовать или подговорить сделать что-либо (обычно хитрое или предосудительное).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кто <научил> тебя так грубо отвечать старшим?",
                            "target_lang_translations": {
                                "en": "Who <taught> you to answer back to your elders so rudely?"
                            }
                        },
                        {
                            "text": "Приятели <научили> мальчика сбежать с последнего урока.",
                            "target_lang_translations": {
                                "en": "Friends <taught> the boy to skip the last class."
                            }
                        }
                    ],
                    "synonyms": [
                        "подговорить",
                        "подбить"
                    ],
                    "common_phrases": [
                        "кто тебя научил",
                        "черт научил"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To advise or instigate someone to do something (usually cunning or reprehensible)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "teach",
                                "target_lang_sense_clarification": "bad behavior or habits"
                            },
                            {
                                "target_lang_word": "put up to",
                                "target_lang_sense_clarification": "instigate or persuade"
                            }
                        ]
                    }
                }
            ]
        }

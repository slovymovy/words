    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5ae7c283-567f-3b03-bee9-258f98de8861",
                    "sense_definition": "A large, shiny black bird of the crow family, known for its loud croaking call and intelligence.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "bird_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The large black <w>raven</w> perched on the branch and looked down at us.",
                            "target_lang_translations": {
                                "ru": "Большой черный <w>ворон</w> сидел на ветке и смотрел на нас сверху вниз."
                            }
                        },
                        {
                            "text": "In many old legends, the <w>raven</w> is seen as a messenger of the gods.",
                            "target_lang_translations": {
                                "ru": "Во многих старых легендах <w>ворон</w> считается посланником богов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "common raven",
                        "black as a raven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная черная птица семейства врановых, известная своим громким карканьем и интеллектом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ворон",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0643e33f-de09-36e6-8e17-a644ef836119",
                    "sense_definition": "A very dark, glossy black color.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "color_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist used a deep <w>raven</w> to paint the night sky.",
                            "target_lang_translations": {
                                "ru": "Художник использовал глубокий <w>цвет воронова крыла</w>, чтобы нарисовать ночное небо."
                            }
                        },
                        {
                            "text": "She chose a dress in a shade of <w>raven</w> that matched her shoes.",
                            "target_lang_translations": {
                                "ru": "Она выбрала платье оттенка <w>воронова крыла</w>, которое подходило к её туфлям."
                            }
                        }
                    ],
                    "synonyms": [
                        "jet-black",
                        "ebony"
                    ],
                    "antonyms": [
                        "white"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень темный, блестящий черный цвет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цвет воронова крыла",
                                "target_lang_sense_clarification": "оттенок"
                            },
                            {
                                "target_lang_word": "иссиня-чёрный цвет",
                                "target_lang_sense_clarification": "цвет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73cc8fb6-7bfd-3308-a464-319d5abfdd20",
                    "sense_definition": "The act of seizing property by force; plunder or rapine.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "plunder_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The village suffered greatly from the <w>raven</w> of the invading army.",
                            "target_lang_translations": {
                                "ru": "Деревня сильно пострадала от <w>грабежа</w> вторгшейся армии."
                            }
                        },
                        {
                            "text": "Stories of war often focus on glory rather than the <w>raven</w> and destruction left behind.",
                            "target_lang_translations": {
                                "ru": "Истории о войне часто фокусируются на славе, а не на <w>разбое</w> и разрушениях, оставленных позади."
                            }
                        }
                    ],
                    "synonyms": [
                        "plunder",
                        "pillage"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This usage is largely obsolete in modern English."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт захвата имущества силой; грабеж или разбой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грабёж",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "разбой",
                                "target_lang_sense_clarification": "насилие"
                            }
                        ]
                    }
                }
            ]
        },
        {

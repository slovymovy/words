    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "308c9522-4a40-3574-be65-27c3eca2e834",
                    "sense_definition": "Происходящий в настоящее время, существующий сейчас.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "current_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В <w>текущем</w> году компания планирует увеличить прибыль.",
                            "target_lang_translations": {
                                "en": "In the <w>current</w> year, the company plans to increase profits."
                            }
                        },
                        {
                            "text": "<w>Текущая</w> ситуация требует от нас быстрых решений.",
                            "target_lang_translations": {
                                "en": "The <w>current</w> situation requires quick decisions from us."
                            }
                        }
                    ],
                    "synonyms": [
                        "ны́нешний",
                        "сего́дняшний",
                        "актуа́льный"
                    ],
                    "antonyms": [
                        "прошлый",
                        "будущий"
                    ],
                    "common_phrases": [
                        "текущий год",
                        "текущий момент",
                        "текущий счёт",
                        "текущие события"
                    ],
                    "target_lang_definitions": {
                        "en": "Happening at the present time, existing now."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "current",
                                "target_lang_sense_clarification": "existing or occurring at the moment"
                            },
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "existing at this time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bc10c3d-f6c2-32c6-836f-acd6ee09bed4",
                    "sense_definition": "Обычный, повседневный, совершаемый регулярно (о делах или работе).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "routine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор был занят <w>текущими</w> делами и не мог никого принять.",
                            "target_lang_translations": {
                                "en": "The director was busy with <w>routine</w> matters and could not see anyone."
                            }
                        },
                        {
                            "text": "В нашем доме сейчас проводится <w>текущий</w> ремонт.",
                            "target_lang_translations": {
                                "en": "In our building, <w>routine</w> maintenance is currently being carried out."
                            }
                        }
                    ],
                    "synonyms": [
                        "повседне́вный"
                    ],
                    "common_phrases": [
                        "текущий ремонт",
                        "текущая работа",
                        "текущие дела"
                    ],
                    "target_lang_definitions": {
                        "en": "Ordinary, everyday, or performed regularly (regarding business or work)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "routine",
                                "target_lang_sense_clarification": "regular and ordinary"
                            },
                            {
                                "target_lang_word": "everyday",
                                "target_lang_sense_clarification": "commonplace or daily"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c8146fd6-1f71-34db-abc6-8cea9dad84e1",
                    "sense_definition": "Действительное причастие настоящего времени от глагола <w>течь</w>; перемещающийся потоком (о жидкости).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "flowing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы долго смотрели на ручей, быстро <w>текущий</w> по камням.",
                            "target_lang_translations": {
                                "en": "We watched the stream quickly <w>flowing</w> over the stones for a long time."
                            }
                        },
                        {
                            "text": "Из крана капала <w>текущая</w> тонкой струйкой вода.",
                            "target_lang_translations": {
                                "en": "Water <w>flowing</w> in a thin stream was dripping from the tap."
                            }
                        }
                    ],
                    "antonyms": [
                        "застывший"
                    ],
                    "common_phrases": [
                        "текущая вода",
                        "текущая река"
                    ],
                    "target_lang_definitions": {
                        "en": "Moving in a continuous stream (about liquid); active present participle of the verb 'to flow'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flowing",
                                "target_lang_sense_clarification": "moving as a fluid"
                            },
                            {
                                "target_lang_word": "running",
                                "target_lang_sense_clarification": "moving continuously (of water)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
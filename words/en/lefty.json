                },
                {
                    "sense_id": "e52f1cf7-a393-3f17-9d87-0a6b44562d4b",
                    "sense_definition": "Relating to or supporting left-wing political views.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "politics_group",
                    "name_type": "no",
                    "examples": [

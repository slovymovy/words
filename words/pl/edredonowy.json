                    "sense_id": "c0b86090-922e-34fb-bb32-858e7205c5e6",
                    "sense_definition": "Wykonany z puchu edredona lub wypełniony nim.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta luksusowa kołdra <w>edredonowa</w> zapewnia ciepło nawet w mroźne noce.",
                            "target_lang_translations": {
                                "ru": "Это роскошное <w>гагачье</w> одеяло обеспечивает тепло даже в морозные ночи."
                            }
                        },
                        {
                            "text": "W sklepie z pościelą oglądaliśmy drogą poduszkę z wypełnieniem <w>edredonowym</w>.",
                            "target_lang_translations": {
                                "ru": "В магазине постельного белья мы рассматривали дорогую подушку с <w>гагачьим</w> наполнителем."
                            }
                        }
                    ],
                    "synonyms": [
                        "puchowy"
                    ],
                    "antonyms": [
                        "syntetyczny"
                    ],
                    "common_phrases": [
                        "kołdra edredonowa",
                        "puch edredonowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный из гагачьего пуха или наполненный им."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гагачий",
                                "target_lang_sense_clarification": "сделанный из пуха гаги"

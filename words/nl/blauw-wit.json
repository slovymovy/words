                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сине-белый",
                                "target_lang_sense_clarification": "цветовая комбинация"
                            },
                            {
                                "target_lang_word": "бело-синий",
                                "target_lang_sense_clarification": "цветовая комбинация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b5a66ea-70e5-3e9c-a719-51ee8b269b86",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "da8ae4c0-7084-3f1f-ab3c-91358d0388cc",
                    "sense_definition": "Степень достоинства, ценности, пригодности вещи или действия; соответствие стандартам.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "excellence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы всегда гарантируем высокое <w>качество</w> нашей продукции.",
                            "target_lang_translations": {
                                "en": "We always guarantee the high <w>quality</w> of our products."
                            }
                        },
                        {
                            "text": "Цена товара часто зависит от его <w>качества</w> и материалов.",
                            "target_lang_translations": {
                                "en": "The price of a product often depends on its <w>quality</w> and materials."
                            }
                        }
                    ],
                    "synonyms": [
                        "добротность",
                        "качественность",
                        "бездефектность"
                    ],
                    "common_phrases": [
                        "высокое качество",
                        "знак качества",
                        "контроль качества",
                        "гарантия качества"
                    ],
                    "target_lang_definitions": {
                        "en": "The degree of excellence, value, or suitability of a thing or action; compliance with standards."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quality",
                                "target_lang_sense_clarification": "level of excellence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d36da57f-f6f7-32f7-882f-542942297558",
                    "sense_definition": "Существенное свойство, характерная особенность или черта кого-либо или чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "feature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этого человека есть замечательное <w>качество</w> — доброта.",
                            "target_lang_translations": {
                                "en": "This person has a wonderful <w>quality</w> — kindness."
                            }
                        },
                        {
                            "text": "Работодатель высоко оценил мои профессиональные <w>качества</w> на собеседовании.",
                            "target_lang_translations": {
                                "en": "The employer highly appreciated my professional <w>qualities</w> at the interview."
                            }
                        }
                    ],
                    "synonyms": [
                        "свойство",
                        "осо́бенность",
                        "черта"
                    ],
                    "common_phrases": [
                        "личные качества",
                        "душевные качества",
                        "профессиональные качества"
                    ],
                    "target_lang_definitions": {
                        "en": "An essential property, characteristic feature, or trait of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quality",
                                "target_lang_sense_clarification": "characteristic"
                            },
                            {
                                "target_lang_word": "trait",
                                "target_lang_sense_clarification": "distinguishing feature"
                            },
                            {
                                "target_lang_word": "feature",
                                "target_lang_sense_clarification": "aspect or attribute"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44c6e6d0-fcee-3fb1-a954-4978445eceb3",
                    "sense_definition": "Роль или положение, в котором кто-либо или что-либо выступает (употребляется с предлогом «в»).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он был приглашён на свадьбу в <w>качестве</w> почётного гостя.",
                            "target_lang_translations": {
                                "en": "He was invited to the wedding <w>as</w> a guest of honor."
                            }
                        },
                        {
                            "text": "Я использую этот деревянный ящик в <w>качестве</w> стола.",
                            "target_lang_translations": {
                                "en": "I use this wooden box <w>as</w> a table."
                            }
                        }
                    ],
                    "common_phrases": [
                        "в качестве"
                    ],
                    "target_lang_definitions": {
                        "en": "The role or position in which someone or something acts (used with the preposition 'в')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "capacity",
                                "target_lang_sense_clarification": "role or position"
                            },
                            {
                                "target_lang_word": "role",
                                "target_lang_sense_clarification": "function"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7083b7ec-4478-3cd8-ae8a-5e234b6be3c3",
                    "sense_definition": "Философская категория, выражающая существенную определённость объекта, благодаря которой он является именно этим, а не иным.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "philosophy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Закон перехода количества в <w>качество</w> является одним из основных в диалектике.",
                            "target_lang_translations": {
                                "en": "The law of the transition of quantity into <w>quality</w> is one of the fundamental laws in dialectics."
                            }
                        },
                        {
                            "text": "Накопление знаний со временем переходит в новое <w>качество</w> понимания предмета.",
                            "target_lang_translations": {
                                "en": "The accumulation of knowledge over time transforms into a new <w>quality</w> of understanding the subject."
                            }
                        }
                    ],
                    "antonyms": [
                        "количество"
                    ],
                    "common_phrases": [
                        "переход количества в качество"
                    ],
                    "target_lang_definitions": {
                        "en": "A philosophical category expressing the essential certainty of an object, by virtue of which it is exactly this and not something else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quality",
                                "target_lang_sense_clarification": "philosophical category"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f53c4c1e-9648-3bd4-9483-8c92366ae1c3",
                    "sense_definition": "В шахматах: разница в ценности между ладьёй и лёгкой фигурой (слоном или конём).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "chess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гроссмейстер пожертвовал <w>качество</w> ради решительной атаки на короля противника.",
                            "target_lang_translations": {
                                "en": "The grandmaster sacrificed the <w>exchange</w> for the sake of a decisive attack on the opponent's king."
                            }
                        },
                        {
                            "text": "Реализация лишнего <w>качества</w> в эндшпиле потребовала от игрока большой точности.",
                            "target_lang_translations": {
                                "en": "Converting the extra <w>exchange</w> in the endgame required great precision from the player."
                            }
                        }
                    ],
                    "common_phrases": [
                        "жертва качества",
                        "выигрыш качества"
                    ],
                    "target_lang_definitions": {
                        "en": "In chess: the difference in value between a rook and a minor piece (bishop or knight)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exchange",
                                "target_lang_sense_clarification": "chess term"
                            }
                        ]
                    }
                }
            ]
        }

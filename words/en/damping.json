                            }
                        },
                        {
                            "text": "Regular <w>damping</w> of the soil ensures the seeds germinate properly.",
                            "target_lang_translations": {
                                "ru": "Регулярное <w>смачивание</w> почвы обеспечивает правильное прорастание семян."
                            }
                        }
                    ],
                    "synonyms": [
                        "wetting",
                        "moistening"
                    ],
                    "antonyms": [
                        "drying"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс легкого смачивания или увлажнения чего-либо."
                    },
                    "translations": {

                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "used in social media contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Упомянуть или ответить конкретному пользователю в социальных сетях, обычно используя символ @."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тегнуть",
                                "target_lang_sense_clarification": "интернет-сленг"

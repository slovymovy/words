                {
                    "sense_id": "5d100f19-9d02-3ad9-bd90-7ef2c1eb8220",
                    "sense_definition": "Proces powielania tekstu lub obrazu za pomocą maszyn drukarskich.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te książki właśnie wyszły z <w>druku</w>.",
                            "target_lang_translations": {
                                "ru": "Эти книги только что вышли из <w>печати</w>."
                            }
                        },
                        {
                            "text": "Koszt <w>druku</w> w kolorze jest zazwyczaj wyższy.",
                            "target_lang_translations": {
                                "ru": "Стоимость цветной <w>печати</w> обычно выше."
                            }
                        }
                    ],
                    "synonyms": [
                        "drukowanie"
                    ],
                    "common_phrases": [
                        "oddać do druku",
                        "wyjść z druku",
                        "błąd w druku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс тиражирования текста или изображений с помощью печатных машин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печать",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "печатание",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fea4eeae-480f-3220-a401-f4ebf258d7bd",
                    "sense_definition": "Oficjalny formularz z rubrykami do wypełnienia lub dokument urzędowy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz wypełnić ten <w>druk</w>, aby otrzymać paszport.",
                            "target_lang_translations": {
                                "ru": "Вы должны заполнить этот <w>бланк</w>, чтобы получить паспорт."
                            }
                        },
                        {
                            "text": "Sprawdź dokładnie te <w>druki</w> przed podpisaniem.",
                            "target_lang_translations": {
                                "ru": "Внимательно проверьте эти <w>бланки</w> перед подписанием."
                            }
                        }
                    ],
                    "synonyms": [
                        "formularz",
                        "pismo",
                        "druczek",
                        "akt"
                    ],
                    "common_phrases": [

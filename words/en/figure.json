        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "da1631c8-c4fd-3ecc-b126-4c6e24b4b4e1",
                    "sense_definition": "To think, suppose, or expect that something is the case.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>figure</w> we have about an hour before the guests arrive.",
                            "target_lang_translations": {
                                "ru": "Я <w>полагаю</w>, у нас есть около часа до прихода гостей."
                            }
                        },
                        {
                            "text": "He didn't <w>figure</w> on the traffic being this bad in the morning.",
                            "target_lang_translations": {
                                "ru": "Он не <w>рассчитывал</w>, что пробки утром будут такими сильными."
                            }
                        }
                    ],
                    "synonyms": [
                        "suppose",
                        "reckon",
                        "guess",
                        "assume",
                        "expect"
                    ],
                    "common_phrases": [
                        "I figure that",
                        "figure on doing"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Думать, полагать или ожидать, что что-то имеет место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полагать",
                                "target_lang_sense_clarification": "считать, думать"

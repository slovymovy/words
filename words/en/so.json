{
    "word_family": [
        "whatsoever",
        "insofar",
        "whosoever",
        "soever"
    ],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "f2971108-c2c0-316f-9908-c45ae881cafa",
                    "sense_definition": "As a result; for that reason; therefore.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was very hungry, <w>so</w> I made a sandwich.",
                            "target_lang_translations": {
                                "ru": "Я был очень голоден, <w>поэтому</w> сделал бутерброд."
                            }
                        },
                        {
                            "text": "He did not study, <w>so</w> he failed the test.",
                            "target_lang_translations": {
                                "ru": "Он не учился, <w>поэтому</w> провалил тест."
                            }
                        }
                    ],
                    "synonyms": [
                        "thus"
                    ],
                    "common_phrases": [
                        "and so",
                        "so then"
                    ],
                    "target_lang_definitions": {
                        "ru": "В результате; по этой причине; поэтому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поэ́тому",
                                "target_lang_sense_clarification": "следовательно; по этой причине"
                            },
                            {
                                "target_lang_word": "та́к что",
                                "target_lang_sense_clarification": "как результат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bf0ba9f-989c-353b-a957-cc6dcd0bbde4",
                    "sense_definition": "In order that; with the purpose that.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please speak louder <w>so</w> everyone can hear you.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, говорите громче, <w>чтобы</w> все могли вас слышать."
                            }
                        },
                        {
                            "text": "I woke up early <w>so</w> I would not be late.",
                            "target_lang_translations": {
                                "ru": "Я проснулся рано, <w>чтобы</w> не опоздать."
                            }
                        }
                    ],
                    "synonyms": [
                        "so that"
                    ],
                    "common_phrases": [
                        "so that",
                        "so as to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Для того, чтобы; с целью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что́бы",
                                "target_lang_sense_clarification": "союз цели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "611ed8e9-0d2b-3b1f-9507-7cdc73a6d95e",
                    "sense_definition": "Provided that; on the condition that.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You may go where you please, <w>so</w> you return before dark.",
                            "target_lang_translations": {
                                "ru": "Можешь идти куда хочешь, <w>лишь бы</w> вернулся до темноты."
                            }
                        },
                        {
                            "text": "It matters not what we eat, <w>so</w> there is enough for everyone.",
                            "target_lang_translations": {
                                "ru": "Неважно, что мы едим, <w>лишь бы</w> всем хватило."
                            }
                        }
                    ],
                    "common_phrases": [
                        "so long as"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly found in older literature"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "При условии, что."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "fa6c6e69-00e8-3bfb-97bf-6685eedd0296",
                    "sense_definition": "To a great extent or degree; very.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>so</w> happy to see you again.",
                            "target_lang_translations": {
                                "ru": "Я <w>так</w> рад видеть тебя снова."
                            }
                        },
                        {
                            "text": "The movie was <w>so</w> good that I watched it twice.",
                            "target_lang_translations": {
                                "ru": "Фильм был <w>настолько</w> хорош, что я посмотрел его дважды."
                            }
                        }
                    ],
                    "synonyms": [
                        "very",
                        "really",
                        "truly"
                    ],
                    "common_phrases": [
                        "so much",
                        "so many",
                        "so good"
                    ],
                    "target_lang_definitions": {
                        "ru": "В значительной степени; очень."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о́чень",
                                "target_lang_sense_clarification": "в высокой степени"
                            },
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "усиление качества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "295dcbc1-b02e-3042-b38e-386a051617c9",
                    "sense_definition": "To the specific extent or degree mentioned or implied.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fish he caught was about <w>so</w> big.",
                            "target_lang_translations": {
                                "ru": "Рыба, которую он поймал, была примерно <w>вот такой</w>."
                            }
                        },
                        {
                            "text": "I did not know the problem was <w>so</w> serious.",
                            "target_lang_translations": {
                                "ru": "Я не знал, что проблема <w>так</w> серьезна."
                            }
                        }
                    ],
                    "synonyms": [
                        "that",
                        "this"
                    ],
                    "common_phrases": [
                        "not so much",
                        "so far",
                        "so long"
                    ],
                    "target_lang_definitions": {
                        "ru": "До указанной степени или размера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "указание на степень"
                            },
                            {
                                "target_lang_word": "тако́й",
                                "target_lang_sense_clarification": "подобный этому"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efe82a81-c80a-3864-a97a-b128327516d1",
                    "sense_definition": "In the same manner or way; likewise; also.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She likes coffee, and <w>so</w> do I.",
                            "target_lang_translations": {
                                "ru": "Она любит кофе, и я <w>тоже</w>."
                            }
                        },
                        {
                            "text": "Just as you work hard, <w>so</w> does he.",
                            "target_lang_translations": {
                                "ru": "Как ты много работаешь, <w>так</w> и он."
                            }
                        }
                    ],
                    "synonyms": [
                        "thus",
                        "like this"
                    ],
                    "common_phrases": [
                        "so do I",
                        "so am I"
                    ],
                    "target_lang_definitions": {
                        "ru": "Таким же образом; так же."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "таким же образом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7005b729-8b04-3007-865d-80ba62a7e322",
                    "sense_definition": "Indeed; used to confirm that something has been observed to be true.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "confirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You said it would rain, and <w>so</w> it has.",
                            "target_lang_translations": {
                                "ru": "Ты сказал, что пойдет дождь, <w>так</w> и случилось."
                            }
                        },
                        {
                            "text": "Look, the bus is here! <w>So</w> it is.",
                            "target_lang_translations": {
                                "ru": "Смотри, автобус здесь! <w>Так</w> и есть."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действительно; используется для подтверждения того, что что-то является правдой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "подтверждение факта"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "7b17dba9-f289-31a4-bc19-9e3400e2e23e",
                    "sense_definition": "That which was previously mentioned; referring to a previous statement or fact.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is he coming to the party? I think <w>so</w>.",
                            "target_lang_translations": {
                                "ru": "Он придет на вечеринку? Я <w>так</w> думаю."
                            }
                        },
                        {
                            "text": "If you are tired, you should say <w>so</w>.",
                            "target_lang_translations": {
                                "ru": "Если ты устал, тебе следует сказать <w>об этом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "that"
                    ],
                    "common_phrases": [
                        "I think so",
                        "I hope so",
                        "if so",
                        "even so"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, о чем говорилось ранее; ссылка на предыдущее утверждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "об этом; это"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d70d003a-1adb-3613-9666-6a2f23ebf08f",
                    "sense_definition": "Used to introduce a new topic, a question, or to pause for thought.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "discourse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>So</w>, what do you want to do today?",
                            "target_lang_translations": {
                                "ru": "<w>Итак</w>, чем ты хочешь заняться сегодня?"
                            }
                        },
                        {
                            "text": "<w>So</w>, tell me more about your new job.",
                            "target_lang_translations": {
                                "ru": "<w>Ну</w>, расскажи мне больше о своей новой работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "well"
                    ],
                    "common_phrases": [
                        "so what",
                        "so then"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения новой темы, вопроса или паузы для размышления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ита́к",
                                "target_lang_sense_clarification": "вводное слово; подведение итога"
                            },
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "вводное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcca678d-a471-3620-8bf1-8f9db5476fdd",
                    "sense_definition": "Used as a question to ask for further explanation or to show indifference.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "discourse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are late again. <w>So</w>?",
                            "target_lang_translations": {
                                "ru": "Ты снова опоздал. <w>Ну и</w>?"
                            }
                        },
                        {
                            "text": "He does not like me, but <w>So</w>? I do not care.",
                            "target_lang_translations": {
                                "ru": "Я ему не нравлюсь, но <w>что с того</w>? Мне все равно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "so what"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often dismissive or impolite"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как вопрос для получения дальнейших объяснений или чтобы показать безразличие."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "202a37b4-d3a4-36ac-a463-4867eb88acd5",
                    "sense_definition": "Agreeing with actual facts or reality; true.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "truth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am afraid that what you say is <w>so</w>.",
                            "target_lang_translations": {
                                "ru": "Боюсь, что то, что вы говорите, — <w>правда</w>."
                            }
                        },
                        {
                            "text": "You said he was angry, but it was not <w>so</w>.",
                            "target_lang_translations": {
                                "ru": "Вы сказали, что он зол, но это было не <w>так</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "true",
                        "correct",
                        "right"
                    ],
                    "common_phrases": [
                        "is that so",
                        "not so"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствующий фактам или действительности; верный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "верно; правда"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "740574d8-02a0-3f08-82cf-4d4364d48060",
                    "sense_definition": "Initialism of significant other; a partner in a romantic relationship.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "initialism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are welcome to bring your <w>SO</w> to the party.",
                            "target_lang_translations": {
                                "ru": "Вы можете привести свою <w>вторую половинку</w> на вечеринку."
                            }
                        },
                        {
                            "text": "She is looking for a gift for her <w>SO</w>.",
                            "target_lang_translations": {
                                "ru": "Она ищет подарок для своего <w>партнера</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от 'significant other'; партнер в романтических отношениях."
                    },
                    "translations": {
                        "ru": []
                    }
                },
                {
                    "sense_id": "940542ed-69c0-3fd8-b494-7628b1449ebb",
                    "sense_definition": "A syllable used in solfège to represent the fifth note of a major scale.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The choir practiced the scale from do to <w>so</w>.",
                            "target_lang_translations": {
                                "ru": "Хор практиковал гамму от до до <w>соль</w>."
                            }
                        },
                        {
                            "text": "In the song, the note <w>so</w> is held for a long time.",
                            "target_lang_translations": {
                                "ru": "В песне нота <w>соль</w> тянется долго."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слог в сольфеджио, обозначающий пятую ноту мажорной гаммы."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4398dbf8-41c3-3218-a894-eca9192ce4dd",
                    "sense_definition": "A Chinese surname from Cantonese.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>So</w> will be joining the meeting shortly.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Со</w> скоро присоединится к встрече."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <w>So</w> at the conference.",
                            "target_lang_translations": {
                                "ru": "Я встретил женщину по имени Сара <w>Со</w> на конференции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "Cantonese origin"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Китайская фамилия кантонского происхождения."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]
}
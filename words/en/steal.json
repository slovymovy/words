    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "de036b6e-1315-33ac-8f36-d864cd828ba4",
                    "sense_definition": "To take something that belongs to someone else illegally or without permission, without intending to return it.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "theft_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Someone <w>stole</w> my bicycle while I was inside the shop."
                        },
                        {
                            "text": "It is wrong to <w>steal</w> money from your parents."
                        },
                        {
                            "text": "The thieves <w>stole</w> several valuable paintings from the gallery."
                        }
                    ],
                    "synonyms": [
                        "thieve",
                        "swipe",
                        "shoplift",
                        "filch",
                        "pilfer",
                        "purloin"
                    ],
                    "antonyms": [
                        "buy",
                        "purchase",
                        "return",
                        "donate"
                    ],
                    "common_phrases": [
                        "steal money",
                        "steal a car",
                        "steal from someone"
                    ]
                },
                {
                    "sense_id": "c4e8903c-df8d-33aa-a4de-43fc3a2487a6",
                    "sense_definition": "To borrow something for a very short time, usually in a casual or friendly context.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "borrow_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can I <w>steal</w> your pen for a second to write this down?"
                        },
                        {
                            "text": "I need to <w>steal</w> a few minutes of your time to discuss the project."
                        }
                    ],
                    "synonyms": [
                        "borrow"
                    ],
                    "common_phrases": [
                        "steal a minute",
                        "steal a pen",
                        "steal a chair"
                    ],
                    "traits": [
                        {

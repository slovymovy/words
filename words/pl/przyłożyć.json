                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "врезать",
                                "target_lang_sense_clarification": "разг. сильно ударить"
                            },
                            {
                                "target_lang_word": "ударить",
                                "target_lang_sense_clarification": "нанести удар"
                            },
                            {
                                "target_lang_word": "двинуть",
                                "target_lang_sense_clarification": "разг. ударить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffffcc71-9276-3568-8f32-a0fe62bab8a3",
                    "sense_definition": "Odcisnąć znak, stempel lub pieczęć na dokumencie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Urzędnik musi <przyłożyć> pieczęć na twoim wniosku, aby był ważny.",
                            "target_lang_translations": {
                                "ru": "Чиновник должен <поставить> печать на твоем заявлении, чтобы оно было действительным."

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "adeb7655-2a22-36ff-a659-15077a010863",
                    "sense_definition": "To come to an end; to stop happening or existing.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "stop_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy rain finally <w>ceased</w> later in the afternoon.",
                            "target_lang_translations": {
                                "ru": "Сильный дождь, наконец, <w>прекратился</w> ближе к вечеру."
                            }
                        },
                        {
                            "text": "The loud noise from the construction site <w>ceased</w> abruptly.",
                            "target_lang_translations": {
                                "ru": "Громкий шум со стройки внезапно <w>прекратился</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stop",
                        "end",
                        "halt",
                        "terminate",
                        "finish"
                    ],
                    "antonyms": [
                        "start",
                        "begin",
                        "continue",
                        "commence"
                    ],
                    "common_phrases": [
                        "cease to exist",
                        "wonders never cease"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекращаться, подходить к концу; переставать происходить или существовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прекращаться",
                                "target_lang_sense_clarification": "о событиях, явлениях или процессах"
                            },
                            {
                                "target_lang_word": "переставать",
                                "target_lang_sense_clarification": "заканчиваться (о дожде, ветре, шуме)"
                            },
                            {
                                "target_lang_word": "заканчиваться",
                                "target_lang_sense_clarification": "подходить к концу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98563c72-fe38-3b3b-9ddb-2179ad1e3b6f",
                    "sense_definition": "To stop performing an action or activity.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "stop_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory <w>ceased</w> production of the old model last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году завод <w>прекратил</w> производство старой модели."
                            }
                        },
                        {
                            "text": "The soldiers were ordered to <w>cease</w> fire immediately.",
                            "target_lang_translations": {
                                "ru": "Солдатам приказали немедленно <w>прекратить</w> огонь."
                            }
                        }
                    ],
                    "synonyms": [
                        "discontinue",
                        "stop",
                        "desist",
                        "give up",
                        "quit"
                    ],
                    "antonyms": [
                        "continue",
                        "maintain",
                        "persist"
                    ],
                    "common_phrases": [
                        "cease fire",
                        "cease operations",
                        "cease trading"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать выполнять действие или заниматься какой-либо деятельностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прекращать",
                                "target_lang_sense_clarification": "останавливать действие или работу"
                            },
                            {
                                "target_lang_word": "переставать",
                                "target_lang_sense_clarification": "бросать делать что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6b24a26-0dc0-3124-a663-e2abf8a5e8bb",
                    "sense_definition": "To pass away, perish, or fail to exist.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poor shall never <w>cease</w> out of the land.",
                            "target_lang_translations": {
                                "ru": "Бедные никогда не <w>переведутся</w> на этой земле."
                            }
                        },
                        {
                            "text": "He feared that his family line would <w>cease</w> with him.",
                            "target_lang_translations": {
                                "ru": "Он боялся, что его род <w>прервется</w> на нем."
                            }
                        }
                    ],
                    "synonyms": [
                        "perish",
                        "die"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Умирать, погибать или переставать существовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исчезать",
                                "target_lang_sense_clarification": "пропадать, переставать быть"
                            },
                            {
                                "target_lang_word": "прекращаться",
                                "target_lang_sense_clarification": "переставать существовать (о роде, линии)"
                            },
                            {
                                "target_lang_word": "переводиться",
                                "target_lang_sense_clarification": "иссякать, исчезать (устар./разг.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c58c35aa-316c-3abb-adf7-20f1fa69a28a",
                    "sense_definition": "The act of stopping or coming to an end.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team worked without <w>cease</w> to complete the project on time.",
                            "target_lang_translations": {
                                "ru": "Команда работала без <w>остановки</w>, чтобы завершить проект вовремя."
                            }
                        },
                        {
                            "text": "The sudden <w>cease</w> of the wind made the lake perfectly still.",
                            "target_lang_translations": {
                                "ru": "Внезапное <w>прекращение</w> ветра сделало озеро совершенно спокойным."
                            }
                        }
                    ],
                    "synonyms": [
                        "cessation",
                        "end",
                        "stop"
                    ],
                    "antonyms": [

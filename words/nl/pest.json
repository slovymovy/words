{
    "word_family": [
        "pesten",
        "pestkop",
        "pestbui",
        "pestilentie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c53b864c-517e-362f-9991-6faed78d16ee",
                    "sense_definition": "Een zeer besmettelijke en dodelijke infectieziekte veroorzaakt door een bacterie, historisch bekend als de Zwarte Dood.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de Middeleeuwen stierf een groot deel van de Europese bevolking aan de <w>pest</w>.",
                            "target_lang_translations": {
                                "ru": "В Средние века значительная часть населения Европы умерла от <w>чумы</w>."
                            }
                        },
                        {
                            "text": "Ik heb echt de <w>pest</w> aan mensen die altijd te laat komen.",
                            "target_lang_translations": {
                                "ru": "Я <w>терпеть не могу</w> людей, которые всегда опаздывают."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwarte dood",
                        "pestilentie"
                    ],
                    "common_phrases": [
                        "de pest hebben aan",
                        "de builenpest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень заразное и смертельное инфекционное заболевание, вызываемое бактерией, исторически известное как «Черная смерть»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чума",
                                "target_lang_sense_clarification": "болезнь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecead039-afdf-32cf-8cb4-3d0a864788d5",
                    "sense_definition": "Een buitengewoon vervelend persoon die anderen tot last is.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn jongere broertje gedraagt zich vandaag als een echte <w>pest</w>.",
                            "target_lang_translations": {
                                "ru": "Мой младший брат ведет себя сегодня как настоящая <w>зараза</w>."
                            }
                        },
                        {
                            "text": "Luister eens naar me, wees niet zo'n <w>pest</w>.",
                            "target_lang_translations": {
                                "ru": "Послушай меня, не будь такой <w>врединой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "klier",
                        "treiteraar",
                        "pestkop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крайне неприятный человек, который доставляет неудобства другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зараза",
                                "target_lang_sense_clarification": "надоедливый человек"
                            },
                            {
                                "target_lang_word": "язва",
                                "target_lang_sense_clarification": "вредный человек"
                            },
                            {
                                "target_lang_word": "вредина",
                                "target_lang_sense_clarification": "о ребенке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "de2efe17-9429-34ca-b718-40822d9740e6",
                    "sense_definition": "Vervoeging van <w>pesten</w>: eerste, tweede en derde persoon enkelvoud tegenwoordige tijd, en de gebiedende wijs.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>pest</w> zijn zusje altijd als hun ouders niet kijken.",
                            "target_lang_translations": {
                                "ru": "Он всегда <w>дразнит</w> свою сестренку, когда родители не видят."
                            }
                        },
                        {
                            "text": "<w>Pest</w> die hond niet zo, hij doet niets verkeerd.",
                            "target_lang_translations": {
                                "ru": "Не <w>дразни</w> собаку, она ничего плохого не делает."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pest niet"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection of pesten"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спряжение глагола pesten: первое, второе и третье лицо единственного числа настоящего времени, а также повелительное наклонение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дразнить",
                                "target_lang_sense_clarification": "насмехаться"
                            },
                            {
                                "target_lang_word": "издеваться",
                                "target_lang_sense_clarification": "обижать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
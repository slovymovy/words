    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c480222a-027e-37a0-9629-2870cad4e261",
                    "sense_definition": "Containing illustrations.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>illustrated</w> guide makes it easy to identify different types of birds.",
                            "target_lang_translations": {
                                "ru": "<w>Иллюстрированный</w> справочник позволяет легко определять различные виды птиц."
                            }
                        },
                        {
                            "text": "She bought a beautifully <w>illustrated</w> book of fairy tales for her daughter.",
                            "target_lang_translations": {
                                "ru": "Она купила прекрасно <w>иллюстрированную</w> книгу сказок для своей дочери."
                            }
                        }
                    ],
                    "synonyms": [
                        "pictorial"
                    ],
                    "antonyms": [
                        "unillustrated"
                    ],
                    "common_phrases": [
                        "illustrated book",
                        "illustrated guide",
                        "heavily illustrated",
                        "richly illustrated"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий иллюстрации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иллюстрированный",
                                "target_lang_sense_clarification": "снабженный рисунками или картинками"
                            },
                            {
                                "target_lang_word": "с картинками",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "31cfa341-ed35-3eb8-8f6e-f4a27cdcaee5",
                    "sense_definition": "Simple past and past participle of <w>illustrate</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor <w>illustrated</w> the complex theory with a simple diagram.",
                            "target_lang_translations": {
                                "ru": "Профессор <w>проиллюстрировал</w> сложную теорию простой диаграммой."
                            }
                        },
                        {
                            "text": "This point is clearly <w>illustrated</w> by the results of the recent study.",
                            "target_lang_translations": {
                                "ru": "Этот момент наглядно <w>проиллюстрирован</w> результатами недавнего исследования."
                            }
                        }
                    ],
                    "common_phrases": [
                        "illustrated by",
                        "illustrated with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола «иллюстрировать»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проиллюстрировал",
                                "target_lang_sense_clarification": "глагол прошедшего времени (пояснил или снабдил рисунком)"
                            },
                            {
                                "target_lang_word": "проиллюстрированный",
                                "target_lang_sense_clarification": "причастие (поясненный или снабженный рисунком)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "976f34f1-7d8f-3c2d-871f-c4e03bc1cd11",

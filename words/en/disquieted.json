                {
                    "sense_id": "1894993f-c829-322e-96b6-6c849ff8580d",
                    "sense_definition": "Feeling uneasy, worried, or lacking peace of mind.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The strange silence in the usually noisy house left him feeling <w>disquieted</w>.",
                            "target_lang_translations": {
                                "ru": "Странная тишина в обычно шумном доме заставила его почувствовать себя <w>встревоженным</w>."
                            }
                        },
                        {
                            "text": "She was <w>disquieted</w> by the sudden and unexplained change in his behavior.",
                            "target_lang_translations": {
                                "ru": "Она была <w>обеспокоена</w> внезапной и необъяснимой переменой в его поведении."
                            }
                        }
                    ],
                    "synonyms": [
                        "uneasy",
                        "anxious",
                        "unsettled",
                        "disturbed",
                        "worried"
                    ],
                    "antonyms": [
                        "calm",
                        "peaceful",
                        "serene",
                        "composed"
                    ],
                    "common_phrases": [
                        "disquieted by",
                        "deeply disquieted",
                        "disquieted mind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий беспокойство, тревогу или отсутствие душевного равновесия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обеспокоенный",

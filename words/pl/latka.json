                    "semantic_group_id": "surname_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Latka</w> został wybrany na przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Пан <w>Латка</w> был избран председателем совета жилого комплекса."
                            }

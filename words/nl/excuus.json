    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea345be3-9b2a-37fa-b6c7-e557726305eb",
                    "sense_definition": "Een uiting van spijt over iets wat men verkeerd heeft gedaan of heeft nagelaten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "apology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil graag mijn <w>excuses</w> aanbieden voor de late reactie op uw e-mail.",
                            "target_lang_translations": {
                                "en": "I would like to offer my <w>apologies</w> for the late reply to your email.",
                                "ru": "Я хотел бы принести свои <w>извинения</w> за поздний ответ на ваше письмо."
                            }
                        },
                        {
                            "text": "Mijn <w>excuus</w>, ik wist niet dat deze stoel al bezet was.",
                            "target_lang_translations": {
                                "en": "My <w>apologies</w>, I didn't know this seat was already taken.",
                                "ru": "Прошу <w>прощения</w>, я не знал, что этот стул уже занят."
                            }
                        }
                    ],
                    "synonyms": [
                        "verontschuldiging"
                    ],
                    "common_phrases": [
                        "excuses aanbieden",
                        "mijn excuses",
                        "oprechte excuses"
                    ],
                    "target_lang_definitions": {
                        "en": "An expression of regret for something done wrong or neglected.",
                        "ru": "Выражение сожаления по поводу того, что было сделано неправильно или упущено."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "apology",
                                "target_lang_sense_clarification": "expression of regret"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "извинение",
                                "target_lang_sense_clarification": "выражение сожаления"
                            },
                            {
                                "target_lang_word": "прощение",
                                "target_lang_sense_clarification": "просьба не винить за проступок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "595eafa0-084d-3fc9-b643-3f890d54d788",
                    "sense_definition": "Een reden die wordt aangevoerd om een fout goed te praten of om ergens onderuit te komen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pretext",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij verzon een <w>excuus</w> om eerder van het saaie feest te kunnen vertrekken.",
                            "target_lang_translations": {
                                "en": "He made up an <w>excuse</w> to leave the boring party early.",
                                "ru": "Он придумал <w>отговорку</w>, чтобы уйти со скучной вечеринки пораньше."
                            }
                        },
                        {
                            "text": "Dat is slechts een slap <w>excuus</w> om je huiswerk niet te hoeven maken.",
                            "target_lang_translations": {
                                "en": "That is just a lame <w>excuse</w> for not having to do your homework.",
                                "ru": "Это лишь слабое <w>оправдание</w>, чтобы не делать домашнее задание."
                            }
                        }
                    ],
                    "synonyms": [
                        "smoes",
                        "voorwendsel",
                        "alibi"
                    ],
                    "common_phrases": [
                        "een slap excuus",
                        "als excuus gebruiken",
                        "geen geldig excuus"
                    ],
                    "target_lang_definitions": {
                        "en": "A reason given to justify a mistake or avoid an obligation.",
                        "ru": "Причина, приводимая для оправдания ошибки или уклонения от чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "excuse",
                                "target_lang_sense_clarification": "reason or pretext"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оправдание",
                                "target_lang_sense_clarification": "довод в защиту своего поведения"
                            },
                            {
                                "target_lang_word": "отговорка",
                                "target_lang_sense_clarification": "выдуманная причина для уклонения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

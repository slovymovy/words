    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3f275c80-483b-33b5-bdf6-daeb279a39fa",
                    "sense_definition": "Ruchoma część zegara lub innego przyrządu, która pokazuje godzinę lub wynik pomiaru.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_pointer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mała <w>wskazówka</w> zegara pokazuje godziny, a duża minuty.",
                            "target_lang_translations": {
                                "ru": "Маленькая <w>стрелка</w> часов показывает часы, а большая — минуты."
                            }
                        },
                        {
                            "text": "Zegar przestał działać i jego <w>wskazówki</w> stanęły w miejscu.",
                            "target_lang_translations": {
                                "ru": "Часы перестали работать, и их <w>стрелки</w> замерли."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wskazówka zegara",
                        "ruch wskazówek zegara",
                        "wskazówka minutowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижная часть часов или измерительного прибора, указывающая время или результат измерения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стрелка",
                                "target_lang_sense_clarification": "часов или прибора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83e0acdd-a44f-3466-9e5f-fff46cd3b66c",
                    "sense_definition": "Praktyczna rada, pouczenie lub informacja, która wyjaśnia, jak coś zrobić lub jak postąpić.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "advice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel dał mi cenną <w>wskazówkę</w> przed trudnym egzaminem.",
                            "target_lang_translations": {
                                "ru": "Учитель дал мне ценный <w>совет</w> перед трудным экзаменом."
                            }
                        },
                        {
                            "text": "Postępuj zgodnie ze <w>wskazówkami</w> zawartymi w instrukcji obsługi.",
                            "target_lang_translations": {
                                "ru": "Следуйте <w>указаниям</w>, содержащимся в инструкции по эксплуатации."
                            }
                        }
                    ],
                    "synonyms": [
                        "rada",
                        "porada",
                        "instrukcja",
                        "pouczenie",
                        "sugestia"
                    ],
                    "common_phrases": [
                        "cenna wskazówka",
                        "udzielić wskazówki",
                        "praktyczna wskazówka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Практический совет, наставление или информация о том, как действовать или выполнить что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совет",
                                "target_lang_sense_clarification": "рекомендация"
                            },
                            {
                                "target_lang_word": "подсказка",
                                "target_lang_sense_clarification": "полезная информация"
                            },
                            {
                                "target_lang_word": "указание",
                                "target_lang_sense_clarification": "инструкция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbcfa6f7-a117-3f58-aa45-68e5c74877cb",
                    "sense_definition": "Ślad, znak lub informacja, która pomaga w odkryciu prawdy lub rozwiązaniu zagadki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "clue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja znalazła w parku nową <w>wskazówkę</w> w sprawie kradzieży.",
                            "target_lang_translations": {
                                "ru": "Полиция нашла в парке новую <w>зацепку</w> по делу о краже."
                            }
                        },
                        {
                            "text": "To była ważna <w>wskazówka</w>, która pomogła detektywowi rozwiązać zagadkę.",
                            "target_lang_translations": {
                                "ru": "Это была важная <w>зацепка</w>, которая помогла детективу разгадать загадку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jedyna wskazówka",
                        "szukać wskazówek",
                        "ważna wskazówka"
                    ],
                    "target_lang_definitions": {
                        "ru": "След, знак или информация, помогающая раскрыть правду или решить загадку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зацепка",
                                "target_lang_sense_clarification": "в расследовании"
                            },
                            {
                                "target_lang_word": "ключ",
                                "target_lang_sense_clarification": "к разгадке"
                            },
                            {
                                "target_lang_word": "улика",
                                "target_lang_sense_clarification": "доказательство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "opvolger",
        "opvolging",
        "opeenvolging",
        "opeenvolgend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dd6b7f2a-379d-3a07-a511-458a5ef29e48",
                    "sense_definition": "Uitvoeren wat een ander aangeraden of bevolen heeft",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij besloot de goede raad van zijn ouders niet <w>op</w> te <w>volgen</w>.",
                            "target_lang_translations": {
                                "ru": "Он решил не <w>следовать</w> хорошему совету своих родителей.",
                                "en": "He decided not to <w>follow</w> his parents' good advice."
                            }
                        },
                        {
                            "text": "Je moet de instructies van de politie altijd onmiddellijk <w>opvolgen</w>.",
                            "target_lang_translations": {
                                "ru": "Вы должны всегда немедленно <w>выполнять</w> указания полиции.",
                                "en": "You must always immediately <w>follow</w> police instructions."
                            }
                        }
                    ],
                    "synonyms": [
                        "gehoorzamen",
                        "naleven"
                    ],
                    "antonyms": [
                        "negeren",
                        "in de wind slaan"
                    ],
                    "common_phrases": [
                        "advies opvolgen",
                        "bevelen opvolgen",
                        "instructies opvolgen",
                        "raad opvolgen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять то, что посоветовал или приказал другой человек",
                        "en": "To carry out what someone else has advised or ordered"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выполнять",
                                "target_lang_sense_clarification": "приказы, инструкции"
                            },
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "совету"
                            },
                            {
                                "target_lang_word": "соблюдать",
                                "target_lang_sense_clarification": "правила"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "advice, instructions"
                            },
                            {
                                "target_lang_word": "obey",
                                "target_lang_sense_clarification": "orders, rules"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cc275e2-c924-3100-8115-a7009e825533",
                    "sense_definition": "Direct na iets of iemand anders komen in een reeks",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De spannende gebeurtenissen in de film <w>volgden</w> elkaar snel <w>op</w>.",
                            "target_lang_translations": {
                                "ru": "Захватывающие события в фильме быстро <w>сменяли</w> друг друга.",
                                "en": "The exciting events in the movie <w>followed</w> each other quickly."
                            }
                        },
                        {
                            "text": "Na een periode van droogte <w>volgden</w> de regenbuien elkaar in hoog tempo <w>op</w>.",
                            "target_lang_translations": {
                                "ru": "После периода засухи ливни <w>сменяли</w> друг друга в быстром темпе.",
                                "en": "After a period of drought, rain showers <w>followed</w> each other in rapid succession."
                            }
                        }
                    ],
                    "synonyms": [
                        "volgen op"
                    ],
                    "common_phrases": [
                        "elkaar opvolgen",
                        "elkaar snel opvolgen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти непосредственно после чего-то или кого-то другого в ряду",
                        "en": "To come directly after something or someone else in a series"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сменять",
                                "target_lang_sense_clarification": "друг друга"
                            },
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "за чем-то"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "come after"
                            },
                            {
                                "target_lang_word": "succeed",
                                "target_lang_sense_clarification": "come after in time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68af2170-fa61-394c-ab8f-e43beee9ff89",
                    "sense_definition": "Iemands functie overnemen",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kroonprins zal zijn moeder volgend jaar <w>opvolgen</w> als staatshoofd.",
                            "target_lang_translations": {
                                "ru": "Кронпринц <w>сменит</w> свою мать на посту главы государства в следующем году.",
                                "en": "The crown prince will <w>succeed</w> his mother next year as head of state."
                            }
                        },
                        {
                            "text": "Het is nog niet bekend wie de vertrekkende directeur gaat <w>opvolgen</w>.",
                            "target_lang_translations": {
                                "ru": "Еще неизвестно, кто <w>сменит</w> уходящего директора.",
                                "en": "It is not yet known who will <w>succeed</w> the departing director."
                            }
                        }
                    ],
                    "synonyms": [
                        "vervangen"
                    ],
                    "antonyms": [
                        "voorafgaan"
                    ],
                    "common_phrases": [
                        "iemand opvolgen",
                        "de koning opvolgen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занять чью-то должность или место",
                        "en": "To take over someone's function or position"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сменить",
                                "target_lang_sense_clarification": "на посту"
                            },
                            {
                                "target_lang_word": "стать преемником",
                                "target_lang_sense_clarification": "наследовать должность"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "succeed",
                                "target_lang_sense_clarification": "take over a position"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "717e1e06-094b-3e58-a3cb-7311d48a6ae4",
                    "sense_definition": "Zorgen voor de verdere afhandeling van een zaak",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn assistent zal uw verzoek tijdens mijn vakantie verder <w>opvolgen</w>.",
                            "target_lang_translations": {
                                "ru": "Мой ассистент продолжит <w>заниматься</w> вашим запросом во время моего отпуска.",
                                "en": "My assistant will <w>follow</w> <w>up</w> on your request during my vacation."
                            }
                        },
                        {
                            "text": "De klantenservice beloofde de klacht serieus <w>op</w> te <w>volgen</w>.",
                            "target_lang_translations": {
                                "ru": "Служба поддержки пообещала серьезно <w>заняться</w> жалобой.",
                                "en": "Customer service promised to <w>follow</w> <w>up</w> on the complaint seriously."
                            }
                        }
                    ],
                    "synonyms": [
                        "afhandelen"
                    ],
                    "common_phrases": [
                        "een klacht opvolgen",
                        "een dossier opvolgen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заниматься дальнейшей обработкой дела или вопроса",
                        "en": "To ensure the further handling of a matter"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заняться",
                                "target_lang_sense_clarification": "вопросом, делом"
                            },
                            {
                                "target_lang_word": "проследить",
                                "target_lang_sense_clarification": "за выполнением"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "follow up",
                                "target_lang_sense_clarification": "a matter, a request"
                            },
                            {
                                "target_lang_word": "monitor",
                                "target_lang_sense_clarification": "progress"
                            }
                        ]
                    }
                }
            ]
        }
    ]

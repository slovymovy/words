    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9bbd5a7-c2b9-3cf5-ab9a-710189957e75",
                    "sense_definition": "Человек, который приводит в исполнение смертный приговор или осуществляет телесные наказания.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "executioner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На городской площади появился <w>палач</w> в маске, держащий в руках тяжёлый топор.",
                            "target_lang_translations": {
                                "en": "An <w>executioner</w> in a mask appeared in the town square, holding a heavy axe."
                            }
                        },
                        {
                            "text": "В средние века профессия, которую исполнял <w>палач</w>, считалась проклятой и презираемой.",
                            "target_lang_translations": {
                                "en": "In the Middle Ages, the profession practiced by the <w>executioner</w> was considered cursed and despised."
                            }
                        }
                    ],
                    "synonyms": [
                        "кат"
                    ],
                    "antonyms": [
                        "жертва"
                    ],
                    "common_phrases": [
                        "топор палача",
                        "рука палача",
                        "королевский палач"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who carries out death sentences or corporal punishment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "executioner",
                                "target_lang_sense_clarification": "official who carries out a death sentence"
                            },
                            {
                                "target_lang_word": "hangman",
                                "target_lang_sense_clarification": "executioner who hangs criminals"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1b97dce-79d1-3bd6-b5c8-669f1b519dac",
                    "sense_definition": "Жестокий человек или мучитель, который причиняет сильные страдания другим людям.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tormentor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Диктатор вошёл в историю как кровавый <w>палач</w> своего собственного народа.",
                            "target_lang_translations": {
                                "en": "The dictator went down in history as the bloody <w>executioner</w> of his own people."
                            }
                        },
                        {
                            "text": "Она смотрела на него с ужасом, видя в нём не спасителя, а безжалостного <w>палача</w>.",
                            "target_lang_translations": {
                                "en": "She looked at him with horror, seeing in him not a savior, but a ruthless <w>tormentor</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "кат"
                    ],
                    "antonyms": [
                        "жертва"
                    ],
                    "common_phrases": [
                        "быть палачом",
                        "палач свободы"
                    ],
                    "target_lang_definitions": {
                        "en": "A cruel person or tormentor who causes severe suffering to others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tormentor",
                                "target_lang_sense_clarification": "someone who inflicts severe mental or physical suffering"
                            },
                            {
                                "target_lang_word": "butcher",
                                "target_lang_sense_clarification": "figurative; a brutal or indiscriminate killer"
                            },
                            {
                                "target_lang_word": "executioner",
                                "target_lang_sense_clarification": "figurative; a ruthless person"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy uznali, że ten człowiek został opętany przez <w>demony</w>.",
                            "target_lang_translations": {
                                "ru": "Все решили, что этот человек одержим <w>демонами</w>."
                            }
                        },
                        {
                            "text": "W dawnych legendach każdy las miał swojego opiekuńczego lub złośliwego <w>demona</w>.",
                            "target_lang_translations": {
                                "ru": "В древних легендах у каждого леса был свой добрый или злой <w>демон</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zły duch",
                        "bies",
                        "czart",
                        "diabeł"
                    ],
                    "antonyms": [
                        "anioł"
                    ],
                    "common_phrases": [

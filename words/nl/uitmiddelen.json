{
    "word_family": [
        "gemiddelde",
        "middeling"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fc067b10-b211-32cb-81d8-14dca96187dc",
                    "sense_definition": "Door het berekenen van een gemiddelde onregelmatigheden, fouten of schommelingen laten verdwijnen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door meerdere metingen te doen, kun je de toevallige fouten <w>uitmiddelen</w>.",
                            "target_lang_translations": {
                                "en": "By taking multiple measurements, you can <w>average out</w> the random errors.",
                                "ru": "Проводя несколько измерений, можно <w>усреднить</w> случайные ошибки."
                            }
                        },
                        {
                            "text": "De kosten voor het project worden over een periode van vijf jaar <w>uitgemiddeld</w>.",
                            "target_lang_translations": {
                                "en": "The costs for the project are <w>averaged out</w> over a period of five years.",
                                "ru": "Расходы на проект <w>усредняются</w> в течение пяти лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "nivelleren",
                        "vereffenen"
                    ],
                    "common_phrases": [
                        "gegevens uitmiddelen",
                        "fouten uitmiddelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To eliminate irregularities, errors, or fluctuations by calculating an average.",
                        "ru": "Устранять неровности, ошибки или колебания путем вычисления среднего значения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "average out",
                                "target_lang_sense_clarification": "calculate the mean to reduce variance"
                            },
                            {
                                "target_lang_word": "smooth out",
                                "target_lang_sense_clarification": "remove irregularities from data"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "усреднять",
                                "target_lang_sense_clarification": "вычислять среднее значение"
                            },
                            {
                                "target_lang_word": "сглаживать",
                                "target_lang_sense_clarification": "устранять колебания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2eaff14a-aa61-3a01-86f8-db0b4a8a4b56",
                    "sense_definition": "Elkaars werking of effect opheffen zodat het eindresultaat neutraal is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De positieve en negatieve resultaten <w>middelen</w> elkaar uiteindelijk <w>uit</w>.",
                            "target_lang_translations": {
                                "en": "The positive and negative results eventually <w>cancel</w> each other <w>out</w>.",
                                "ru": "Положительные и отрицательные результаты в конечном итоге <w>компенсируют</w> друг друга."
                            }
                        },
                        {
                            "text": "Deze twee tegengestelde krachten <w>middelen</w> elkaar volledig <w>uit</w>.",
                            "target_lang_translations": {
                                "en": "These two opposing forces completely <w>cancel</w> each other <w>out</w>.",
                                "ru": "Эти две противоположные силы полностью <w>уравновешивают</w> друг друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "compenseren",
                        "opheffen",
                        "neutraliseren"
                    ],
                    "antonyms": [
                        "versterken"
                    ],
                    "common_phrases": [
                        "elkaar uitmiddelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To neutralize each other's effect or operation.",
                        "ru": "Нейтрализовать действие друг друга, приводя к нулевому или нейтральному результату."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cancel out",
                                "target_lang_sense_clarification": "neutralize an opposing force or effect"
                            },
                            {
                                "target_lang_word": "offset",
                                "target_lang_sense_clarification": "counteract or compensate for"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "компенсировать",
                                "target_lang_sense_clarification": "возмещать действие"
                            },
                            {
                                "target_lang_word": "уравновешивать",
                                "target_lang_sense_clarification": "приводить в баланс"
                            },
                            {
                                "target_lang_word": "нейтрализовать",
                                "target_lang_sense_clarification": "лишать силы воздействия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "503a31cd-61e8-315b-9de8-1692a19fcf0c",
                    "sense_definition": "In evenwicht komen doordat verschillen of variaties tegen elkaar wegvallen (wederkerend).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de lange termijn zal het geluk zich <w>uitmiddelen</w>.",
                            "target_lang_translations": {
                                "en": "In the long run, luck will <w>even</w> itself <w>out</w>.",
                                "ru": "В долгосрочной перспективе везение <w>усреднится</w>."
                            }
                        },
                        {
                            "text": "De verschillen tussen de seizoenen <w>middelen</w> zich door de jaren heen <w>uit</w>.",
                            "target_lang_translations": {
                                "en": "The differences between the seasons <w>even</w> themselves <w>out</w> over the years.",
                                "ru": "Различия между временами года с годами <w>сглаживаются</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "balanceren"
                    ],
                    "common_phrases": [
                        "zich uitmiddelen",
                        "zich op den duur uitmiddelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To come into balance as differences or variations disappear over time.",
                        "ru": "Приходить в равновесие за счет взаимного погашения различий (возвратный глагол)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "even out",
                                "target_lang_sense_clarification": "become balanced or stable over time"
                            },
                            {
                                "target_lang_word": "balance out",
                                "target_lang_sense_clarification": "reach equilibrium"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выравниваться",
                                "target_lang_sense_clarification": "становиться равномерным"
                            },
                            {
                                "target_lang_word": "усредняться",
                                "target_lang_sense_clarification": "приходить к среднему"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "f1580fbb-15a9-31a6-b010-a53ec6ea59f1",
                    "sense_definition": "Used after mentioning two or more people or groups to show that something applies equally to them.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "adv_equal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new museum exhibit is popular with children and adults <w>alike</w>.",
                            "target_lang_translations": {
                                "ru": "Новая музейная выставка <w>одинаково</w> популярна среди детей и взрослых."
                            }
                        },
                        {
                            "text": "This difficult situation challenges managers and employees <w>alike</w>.",
                            "target_lang_translations": {
                                "ru": "Эта сложная ситуация бросает вызов менеджерам и сотрудникам <w>в равной степени</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "equally"
                    ],
                    "common_phrases": [
                        "young and old alike",
                        "friends and family alike",
                        "rich and poor alike"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется после упоминания двух или более людей или групп, чтобы показать, что нечто относится к ним в равной степени."
                    },
                    "translations": {
                        "ru": [

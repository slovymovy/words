    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bc481594-4a19-3efb-931c-e666c1e412e0",
                    "sense_definition": "Выборный или назначаемый предводитель казачьего войска, а также начальник казачьего поселения или административной единицы.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "leader_historical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Казаки единогласно выбрали своего нового <w>атамана</w> на общем сходе.",
                            "target_lang_translations": {
                                "en": "The Cossacks unanimously elected their new <w>ataman</w> at the general assembly."
                            }
                        },
                        {
                            "text": "Станичный <w>атаман</w> строго следил за порядком и дисциплиной в поселении.",
                            "target_lang_translations": {
                                "en": "The stanitsa <w>ataman</w> strictly monitored order and discipline in the settlement."
                            }
                        }
                    ],
                    "synonyms": [
                        "предводитель",
                        "глава"
                    ],
                    "common_phrases": [
                        "казачий атаман",
                        "станичный атаман",
                        "походный атаман",
                        "войсковой атаман"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An elected or appointed leader of a Cossack host, or the head of a Cossack settlement or administrative unit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ataman",
                                "target_lang_sense_clarification": "historical title for Cossack leaders"
                            },
                            {
                                "target_lang_word": "chieftain",
                                "target_lang_sense_clarification": "general term for a leader of a clan or tribe"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8aec7715-8760-37b2-8d08-53221dcdd3be",
                    "sense_definition": "Главарь банды, разбойничьей шайки или (в переносном смысле) лидер группы, зачинщик.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "leader_criminal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция долго не могла поймать хитрого <w>атамана</w> этой преступной группировки.",
                            "target_lang_translations": {
                                "en": "The police could not catch the cunning <w>ringleader</w> of this criminal group for a long time."
                            }
                        },
                        {
                            "text": "В детских играх он всегда был <w>атаманом</w> и командовал остальными мальчишками.",
                            "target_lang_translations": {
                                "en": "In children's games, he was always the <w>leader</w> and commanded the other boys."
                            }
                        }
                    ],
                    "synonyms": [
                        "главарь",
                        "предводитель"
                    ],
                    "common_phrases": [
                        "атаман разбойников",
                        "лихой атаман",
                        "атаман шайки"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "figurative or jocular usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The head of a gang, a band of robbers, or (figuratively) a group leader or instigator."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ringleader",
                                "target_lang_sense_clarification": "leader of a group involved in illicit activities"
                            },
                            {
                                "target_lang_word": "gang leader",
                                "target_lang_sense_clarification": "head of a criminal group"
                            },
                            {
                                "target_lang_word": "chieftain",
                                "target_lang_sense_clarification": "leader of a band of robbers"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
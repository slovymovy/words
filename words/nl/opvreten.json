    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5aaacce5-5c7d-3687-b107-1f209b6b516c",
                    "sense_definition": "Op een gulzige, wilde manier volledig opeten, meestal gezegd van dieren of onbeleefde personen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "eating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hongerige wolf heeft zijn prooi binnen enkele minuten helemaal <w>opgevreten</w>.",
                            "target_lang_translations": {
                                "en": "The hungry wolf <w>devoured</w> its prey completely within a few minutes.",
                                "ru": "Голодный волк всего за несколько минут полностью <w>сожрал</w> свою добычу."
                            }
                        },
                        {
                            "text": "Je moet niet al het snoep in één keer <w>opvreten</w>, want dan word je misselijk.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>gobble up</w> all the candy at once, because then you'll get nauseous.",
                                "ru": "Не стоит <w>сжирать</w> все конфеты за раз, иначе тебя будет тошнить."
                            }
                        }
                    ],
                    "synonyms": [
                        "verslinden",
                        "opschrokken",
                        "naar binnen werken"
                    ],
                    "common_phrases": [
                        "alles opvreten",
                        "restjes opvreten"
                    ],
                    "target_lang_definitions": {
                        "en": "To eat up completely in a greedy or wild manner, often used for animals or rude behavior.",
                        "ru": "Жадно, по-звериному съесть что-либо целиком; часто используется в отношении животных или грубого поведения людей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "devour",
                                "target_lang_sense_clarification": "to eat hungrily or quickly"
                            },
                            {
                                "target_lang_word": "gobble up",
                                "target_lang_sense_clarification": "to eat hastily and noisily"
                            },
                            {
                                "target_lang_word": "wolf down",
                                "target_lang_sense_clarification": "to eat very quickly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сожрать",
                                "target_lang_sense_clarification": "грубо; съесть жадно или быстро"
                            },
                            {
                                "target_lang_word": "слопать",
                                "target_lang_sense_clarification": "разг.; съесть быстро и с аппетитом"
                            },
                            {
                                "target_lang_word": "уплести",
                                "target_lang_sense_clarification": "разг.; съесть с жадностью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d6db718-cf1f-3ab2-bd43-e733b4193005",
                    "sense_definition": "Langzaam aantasten, vernietigen of verteren door een chemisch proces of door hevige emoties.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "consuming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het strooizout heeft de onderkant van de auto door de jaren heen <w>opgevreten</w>.",
                            "target_lang_translations": {
                                "en": "The road salt has <w>eaten away</w> the underside of the car over the years.",
                                "ru": "Дорожная соль с годами <w>разъела</w> днище автомобиля."
                            }
                        },
                        {
                            "text": "Hij werd vanbinnen <w>opgevreten</w> door schuldgevoel over zijn fouten.",
                            "target_lang_translations": {
                                "en": "He was being <w>eaten up</w> inside by guilt over his mistakes.",
                                "ru": "Его изнутри <w>сжирало</w> чувство вины за свои ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "aantasten",
                        "wegvreten",
                        "verteren"
                    ],
                    "common_phrases": [
                        "zichzelf opvreten",
                        "van binnen opvreten"
                    ],
                    "target_lang_definitions": {
                        "en": "To slowly destroy, corrode, or consume, often by chemical action or intense emotion.",
                        "ru": "Медленно разрушать, разъедать или уничтожать (химически или эмоционально)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eat away (at)",
                                "target_lang_sense_clarification": "to destroy gradually (e.g., by acid or rust)"
                            },
                            {
                                "target_lang_word": "consume",
                                "target_lang_sense_clarification": "to absorb or destroy completely (often by emotions)"
                            },
                            {
                                "target_lang_word": "corrode",
                                "target_lang_sense_clarification": "to destroy by chemical action"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "разъедать",
                                "target_lang_sense_clarification": "разрушать поверхность (напр. ржавчиной или кислотой)"
                            },
                            {
                                "target_lang_word": "сжирать",
                                "target_lang_sense_clarification": "перен.; уничтожать или мучить эмоционально"
                            },
                            {
                                "target_lang_word": "глодать",
                                "target_lang_sense_clarification": "перен.; мучить (о тоске, совести)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
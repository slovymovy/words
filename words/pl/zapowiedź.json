{
    "word_family": [
        "zapowiadać"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6b0ff317-fd5f-3a69-8ad2-04e9d7e307ea",
                    "sense_definition": "Informacja przekazana publicznie o tym, co ma się wydarzyć w najbliższym czasie, lub wstępna prezentacja nadchodzącego dzieła.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication_announcement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W radiu usłyszałem <zapowiedź> gwałtownej zmiany pogody na jutro.",
                            "target_lang_translations": {
                                "ru": "По радио я услышал <прогноз> резкой смены погоды на завтра."
                            }
                        },
                        {
                            "text": "To była oficjalna <zapowiedź> wizyty prezydenta w naszym mieście.",
                            "target_lang_translations": {
                                "ru": "Это было официальное <объявление> о визите президента в наш город."
                            }
                        },
                        {
                            "text": "W kinie przed seansem obejrzeliśmy <zapowiedź> nowego filmu akcji.",
                            "target_lang_translations": {
                                "ru": "В кино перед сеансом мы посмотрели <анонс> нового боевика."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogłoszenie",
                        "obwieszczenie",
                        "komunikat",
                        "anons",
                        "zwiastun"
                    ],
                    "common_phrases": [
                        "zapowiedź pogody",
                        "zapowiedź filmu",
                        "zapowiedź zmian",
                        "oficjalna zapowiedź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичное сообщение о том, что произойдет в ближайшее время, или предварительная презентация будущего произведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анонс",
                                "target_lang_sense_clarification": "предварительное оповещение о событии, фильме или книге"
                            },
                            {
                                "target_lang_word": "объявление",
                                "target_lang_sense_clarification": "официальное сообщение"
                            },
                            {
                                "target_lang_word": "прогноз",
                                "target_lang_sense_clarification": "в контексте погоды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c20feed-a534-30b8-93c3-13dcad88c01b",
                    "sense_definition": "Zdarzenie, zjawisko lub sygnał, który jest traktowany jako znak tego, co nadejdzie w przyszłości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "signs_omens",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pierwsze pąki na drzewach to wyraźna <zapowiedź> nadchodzącej wiosny.",
                            "target_lang_translations": {
                                "ru": "Первые почки на деревьях — это явный <предвестник> наступающей весны."
                            }
                        },
                        {
                            "text": "Ciemne chmury na horyzoncie były <zapowiedzią> zbliżającej się burzy.",
                            "target_lang_translations": {
                                "ru": "Тёмные тучи на горизонте были <предвестником> приближающейся бури."
                            }
                        }
                    ],
                    "synonyms": [
                        "oznaka",
                        "zwiastun",
                        "znak",
                        "symptom",
                        "wróżba",
                        "prognostyk"
                    ],
                    "common_phrases": [
                        "zapowiedź sukcesu",
                        "zapowiedź kłopotów",
                        "dobra zapowiedź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Событие, явление или сигнал, который рассматривается как знак того, что произойдет в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предвестник",
                                "target_lang_sense_clarification": "то, что предвещает наступление чего-либо"
                            },
                            {
                                "target_lang_word": "знак",
                                "target_lang_sense_clarification": "символ будущего события"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87c8c461-0828-3cca-85b3-e13583a02e21",
                    "sense_definition": "Oficjalne ogłoszenie w kościele lub urzędzie o zamiarze zawarcia małżeństwa przez konkretne osoby.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "communication_announcement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ksiądz wyczytał ich <zapowiedzi> podczas niedzielnej mszy świętej.",
                            "target_lang_translations": {
                                "ru": "Священник зачитал их <оглашения> во время воскресной мессы."
                            }
                        },
                        {
                            "text": "Młoda para musi udać się do kancelarii, aby dać na <zapowiedzi>.",
                            "target_lang_translations": {
                                "ru": "Молодая пара должна пойти в канцелярию, чтобы подать на <оглашение>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dać na zapowiedzi",
                        "czytać zapowiedzi",
                        "zapowiedzi przedślubne"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually used in plural: zapowiedzi"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное объявление в церкви или государственном учреждении о намерении конкретных лиц вступить в брак."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оглашение",
                                "target_lang_sense_clarification": "церковное объявление о предстоящем браке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
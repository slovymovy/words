                            {
                                "target_lang_word": "stab",
                                "target_lang_sense_clarification": "sudden sharp pain"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "колика",
                                "target_lang_sense_clarification": "в боку"
                            },
                            {
                                "target_lang_word": "острая боль",
                                "target_lang_sense_clarification": "общая"
                            },
                            {
                                "target_lang_word": "прострел",
                                "target_lang_sense_clarification": "внезапная боль"
                            }
                        ]

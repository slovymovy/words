                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The guests received a <w>royal</w> welcome when they arrived at the hotel.",
                            "target_lang_translations": {
                                "ru": "По прибытии в отель гостям оказали <w>королевский</w> прием."
                            }
                        },
                        {
                            "text": "The lion sat on the rock with a <w>royal</w> demeanor.",
                            "target_lang_translations": {

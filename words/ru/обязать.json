    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0af701f8-f6b5-3d0b-908c-cbac09ae3460",
                    "sense_definition": "Наложить на кого-либо обязанность, заставить выполнить какое-либо действие в силу закона, приказа или договора.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Суд <обязал> компанию выплатить крупный штраф за нарушение экологических норм.",
                            "target_lang_translations": {
                                "en": "The court <ordered> the company to pay a large fine for violating environmental standards."
                            }
                        },
                        {
                            "text": "Новый закон <обяжет> всех водителей проходить медицинский осмотр ежегодно.",
                            "target_lang_translations": {
                                "en": "The new law will <require> all drivers to undergo a medical examination annually."
                            }
                        }
                    ],
                    "synonyms": [
                        "заставить",
                        "принудить",
                        "предписать"
                    ],
                    "antonyms": [
                        "освободить"
                    ],
                    "common_phrases": [
                        "обязать судом",
                        "обязать законом",
                        "обязать выплатить",
                        "обязать выполнить"
                    ],
                    "target_lang_definitions": {
                        "en": "To impose a duty on someone or force them to perform an action by virtue of law, order, or contract."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oblige",
                                "target_lang_sense_clarification": "to force someone to do something legally or morally"
                            },
                            {
                                "target_lang_word": "bind",
                                "target_lang_sense_clarification": "to impose a legal or contractual obligation"
                            },
                            {
                                "target_lang_word": "compel",
                                "target_lang_sense_clarification": "to force or drive someone to do something"
                            },
                            {
                                "target_lang_word": "require",
                                "target_lang_sense_clarification": "to demand as necessary or essential"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "242b7ebe-b0f3-3492-90e7-c56365b5796a",
                    "sense_definition": "Оказать услугу или любезность, вызвав у кого-либо чувство благодарности и желание ответить добром.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вы меня очень <обяжете>, если согласитесь передать эти документы лично в руки.",
                            "target_lang_translations": {
                                "en": "You would greatly <oblige> me if you agreed to hand these documents over personally."
                            }
                        },
                        {
                            "text": "Своим быстрым ответом вы нас крайне <обязали> и помогли решить проблему.",
                            "target_lang_translations": {
                                "en": "You have greatly <obliged> us with your quick response and helped solve the problem."
                            }
                        }
                    ],
                    "common_phrases": [
                        "очень обязать",
                        "крайне обязать"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "преимущественно в вежливых оборотах речи"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To do a favor or kindness, causing a feeling of gratitude and a desire to reciprocate."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oblige",
                                "target_lang_sense_clarification": "to do a favor for someone (often used in polite expressions)"
                            },
                            {
                                "target_lang_word": "indebt",
                                "target_lang_sense_clarification": "to place someone under a debt of gratitude"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "scrollbar",
        "scroller",
        "scrollwork",
        "unscroll",
        "scrollable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "480f3590-f085-39ea-a55f-c5fd8868e01d",
                    "sense_definition": "To move text or images up, down, or across a display screen to see different parts of them.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "digital_nav",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>scroll</w> down to the bottom of the page to find the link.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>прокрутите</w> страницу вниз до конца, чтобы найти ссылку."
                            }
                        },
                        {
                            "text": "She <w>scrolled</w> through the photos on her phone to show us her holiday.",
                            "target_lang_translations": {
                                "ru": "Она <w>листала</w> фотографии на телефоне, чтобы показать нам свой отпуск."
                            }
                        }
                    ],
                    "common_phrases": [
                        "scroll down",
                        "scroll up",
                        "scroll through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать текст или изображения вверх, вниз или поперек экрана дисплея для просмотра их различных частей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокручивать",
                                "target_lang_sense_clarification": "стандартный термин для перемещения содержимого на экране"
                            },
                            {
                                "target_lang_word": "листать",
                                "target_lang_sense_clarification": "просматривать ленту новостей или страницы"
                            },
                            {
                                "target_lang_word": "скроллить",
                                "target_lang_sense_clarification": "компьютерный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a054d476-2913-336f-9c86-02f0f3650d85",
                    "sense_definition": "To move continuously across a screen or display, often horizontally or vertically.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_nav",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The breaking news headlines <w>scrolled</w> across the bottom of the television screen.",
                            "target_lang_translations": {
                                "ru": "Заголовки срочных новостей <w>бежали</w> внизу телевизионного экрана."
                            }
                        },
                        {
                            "text": "We watched as the credits <w>scrolled</w> slowly at the end of the movie.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели, как титры медленно <w>плыли</w> в конце фильма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "scroll across",
                        "scroll by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непрерывно перемещаться по экрану или дисплею, часто горизонтально или вертикально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокручиваться",
                                "target_lang_sense_clarification": "о содержимом экрана"
                            },
                            {
                                "target_lang_word": "бежать",
                                "target_lang_sense_clarification": "о бегущей строке текста"
                            },
                            {
                                "target_lang_word": "плыть",
                                "target_lang_sense_clarification": "медленно перемещаться (например, о титрах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "684b2011-6ef5-3a12-b5c8-d2f229f73a8e",
                    "sense_definition": "To flood a chat interface with many lines of text quickly, pushing other messages out of view.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The moderator banned the user who <w>scrolled</w> the chat room with nonsense.",
                            "target_lang_translations": {
                                "ru": "Модератор забанил пользователя, который <w>засорял</w> чат бессмыслицей."
                            }
                        },
                        {
                            "text": "Please do not <w>scroll</w> or spam the channel with repeated messages.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>флудите</w> и не спамьте в канале повторяющимися сообщениями."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро заполнять интерфейс чата множеством строк текста, вытесняя другие сообщения из поля зрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засорять",
                                "target_lang_sense_clarification": "заполнять ненужной информацией"
                            },
                            {
                                "target_lang_word": "флудить",
                                "target_lang_sense_clarification": "интернет-сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9e4e1728-68e3-3094-9f63-68a8ebd49cad",
                    "sense_definition": "The act of moving information up, down, or across a computer screen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "digital_nav",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I gave the document a quick <w>scroll</w> to check the formatting.",
                            "target_lang_translations": {
                                "ru": "Я быстро <w>прокрутил</w> документ, чтобы проверить форматирование."
                            }
                        },
                        {
                            "text": "With a simple <w>scroll</w> of the mouse wheel, you can zoom in on the map.",
                            "target_lang_translations": {
                                "ru": "Простой <w>прокруткой</w> колесика мыши вы можете увеличить масштаб карты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "quick scroll",
                        "smooth scroll"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по перемещению информации вверх, вниз или поперек экрана компьютера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокрутка",
                                "target_lang_sense_clarification": "действие пользователя"
                            },
                            {
                                "target_lang_word": "скроллинг",
                                "target_lang_sense_clarification": "технический термин или сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7905f182-dee7-3125-8a63-e5a45f1de1ff",
                    "sense_definition": "A roll of paper or parchment with writing on it, especially one used in ancient times.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum displayed an ancient <w>scroll</w> containing lost poetry.",
                            "target_lang_translations": {
                                "ru": "В музее был выставлен древний <w>свиток</w>, содержащий утраченную поэзию."
                            }
                        },
                        {
                            "text": "He carefully unrolled the <w>scroll</w> to read the royal decree.",
                            "target_lang_translations": {
                                "ru": "Он осторожно развернул <w>свиток</w>, чтобы прочитать королевский указ."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ancient scroll",
                        "magic scroll",
                        "unroll a scroll"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рулон бумаги или пергамента с письменами, особенно используемый в древние времена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свиток",
                                "target_lang_sense_clarification": "древняя рукопись, свернутая в трубку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86438db4-d615-3f5e-9c8c-9f19457f4fe0",
                    "sense_definition": "A decorative design featuring spirals or curling lines, often resembling a rolled-up paper or plant stem.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "ornament",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The iron gate was decorated with an intricate <w>scroll</w> pattern.",
                            "target_lang_translations": {
                                "ru": "Железные ворота были украшены сложным узором из <w>завитков</w>."
                            }
                        },
                        {
                            "text": "She admired the gold <w>scroll</w> painted on the edge of the vase.",
                            "target_lang_translations": {
                                "ru": "Она любовалась золотым <w>завитком</w>, нарисованным на краю вазы."
                            }
                        }
                    ],
                    "synonyms": [
                        "fiddlehead"
                    ],
                    "common_phrases": [
                        "scroll pattern",
                        "gold scroll"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративный узор со спиралями или извилистыми линиями, часто напоминающий свернутую бумагу или стебель растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завиток",
                                "target_lang_sense_clarification": "спиралевидный элемент узора"
                            },
                            {
                                "target_lang_word": "волюта",
                                "target_lang_sense_clarification": "архитектурный термин"
                            },
                            {
                                "target_lang_word": "завитушка",
                                "target_lang_sense_clarification": "разговорное название сложного узора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2696c121-bdba-3b1e-a24b-32df35170fc2",
                    "sense_definition": "The curved, spiral-shaped top end of the neck of a stringed instrument like a violin or cello.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "ornament",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The luthier spent days carving the delicate <w>scroll</w> of the violin.",
                            "target_lang_translations": {
                                "ru": "Мастер потратил дни, вырезая изящный <w>завиток</w> скрипки."
                            }
                        },
                        {
                            "text": "This cello features a beautifully crafted <w>scroll</w> at the top of the neck.",
                            "target_lang_translations": {
                                "ru": "У этой виолончели красиво выполненный <w>завиток</w> в верхней части грифа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "violin scroll"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изогнутый, спиралевидный верхний конец грифа струнного инструмента, такого как скрипка или виолончель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завиток",
                                "target_lang_sense_clarification": "часть грифа струнных инструментов"
                            },
                            {
                                "target_lang_word": "головка",
                                "target_lang_sense_clarification": "общая часть инструмента, где находятся колки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55ecdf18-0098-37af-8a80-e2e161ddbba8",
                    "sense_definition": "A sweet bun or cake made of dough rolled into a spiral shape.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I ordered a coffee and a cinnamon <w>scroll</w> for breakfast.",
                            "target_lang_translations": {
                                "ru": "Я заказал кофе и <w>улитку</w> с корицей на завтрак."
                            }
                        },
                        {
                            "text": "The bakery sells a delicious fruit <w>scroll</w> covered in icing.",
                            "target_lang_translations": {
                                "ru": "Пекарня продает вкусную фруктовую <w>завитушку</w>, покрытую глазурью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cinnamon scroll",
                        "coffee scroll"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкая булочка или пирожное из теста, свернутого в спираль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "улитка",
                                "target_lang_sense_clarification": "булочка спиральной формы"
                            },
                            {
                                "target_lang_word": "завитушка",
                                "target_lang_sense_clarification": "выпечка сложной формы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
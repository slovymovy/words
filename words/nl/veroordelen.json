{
    "word_family": [
        "veroordeling",
        "oordeel",
        "oordelen",
        "beoordelen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "da035e66-2075-3342-a8c8-b22bf570c4ee",
                    "sense_definition": "In een rechtszaak officieel uitspreken dat iemand schuldig is en een straf krijgt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter <veroordeelde> de verdachte tot een gevangenisstraf van vijf jaar.",
                            "target_lang_translations": {
                                "en": "The judge <sentenced> the suspect to a prison sentence of five years.",
                                "ru": "Судья <приговорил> обвиняемого к пяти годам тюремного заключения."
                            }
                        },
                        {
                            "text": "Hij werd bij verstek <veroordeeld> omdat hij niet bij de rechtszaak aanwezig was.",
                            "target_lang_translations": {
                                "en": "He was <convicted> in absentia because he was not present at the trial.",
                                "ru": "Он был <осужден> заочно, так как не присутствовал на судебном заседании."
                            }
                        }
                    ],
                    "synonyms": [
                        "vonnissen"
                    ],
                    "antonyms": [
                        "vrijspreken"
                    ],
                    "common_phrases": [
                        "tot gevangenisstraf veroordelen",
                        "ter dood veroordelen",
                        "bij verstek veroordelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To officially declare someone guilty in a court of law and impose a punishment.",
                        "ru": "Официально признать кого-либо виновным в суде и назначить наказание."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sentence",
                                "target_lang_sense_clarification": "to impose a punishment"
                            },
                            {
                                "target_lang_word": "convict",
                                "target_lang_sense_clarification": "to find guilty"
                            },
                            {
                                "target_lang_word": "condemn",
                                "target_lang_sense_clarification": "to sentence to a severe punishment"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приговаривать",
                                "target_lang_sense_clarification": "выносить приговор о наказании"
                            },
                            {
                                "target_lang_word": "осуждать",
                                "target_lang_sense_clarification": "признавать виновным юридически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38be6752-4c9d-31dc-9b54-c736c44b21b6",
                    "sense_definition": "Streng en openlijk zeggen dat men iets of iemand slecht, verkeerd of onaanvaardbaar vindt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De regering <veroordeelt> het geweld tegen de vreedzame demonstranten ten zeerste.",
                            "target_lang_translations": {
                                "en": "The government strongly <condemns> the violence against the peaceful protesters.",
                                "ru": "Правительство решительно <осуждает> насилие в отношении мирных демонстрантов."
                            }
                        },
                        {
                            "text": "Veel wetenschappers <veroordelen> de politieke aanvallen op hun onafhankelijke onderzoek.",
                            "target_lang_translations": {
                                "en": "Many scientists <condemn> the political attacks on their independent research.",
                                "ru": "Многие ученые <осуждают> политические нападки на их независимые исследования."
                            }
                        }
                    ],
                    "synonyms": [
                        "afkeuren"
                    ],
                    "antonyms": [
                        "toejuichen"
                    ],
                    "common_phrases": [
                        "scherp veroordelen",
                        "ten stelligste veroordelen",
                        "geweld veroordelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To strongly and openly state that something or someone is bad, wrong, or unacceptable.",
                        "ru": "Резко и публично выражать неодобрение чьим-либо действиям или поведению."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "condemn",
                                "target_lang_sense_clarification": "to express strong disapproval"
                            },
                            {
                                "target_lang_word": "denounce",
                                "target_lang_sense_clarification": "to publicly declare as wrong"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "осуждать",
                                "target_lang_sense_clarification": "выражать неодобрение"
                            },
                            {
                                "target_lang_word": "порицать",
                                "target_lang_sense_clarification": "высказывать укор"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
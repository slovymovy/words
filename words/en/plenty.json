    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "60dbcc49-fa72-3845-ba42-546b7e45d525",
                    "sense_definition": "A large amount; as much as or more than is needed.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We have <w>plenty</w> of time to catch the train.",
                            "target_lang_translations": {
                                "ru": "У нас <w>много</w> времени, чтобы успеть на поезд."
                            }
                        },
                        {
                            "text": "Don't worry about food because there is <w>plenty</w> for everyone.",
                            "target_lang_translations": {
                                "ru": "Не беспокойся о еде, её <w>предостаточно</w> для всех."
                            }
                        }
                    ],
                    "synonyms": [
                        "abundance",
                        "profusion",
                        "abundance"
                    ],
                    "common_phrases": [
                        "plenty of",
                        "plenty more",
                        "plenty to do"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество; столько, сколько нужно, или больше."
                    },
                    "translations": {
                        "ru": [

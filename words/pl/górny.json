    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "da0c1f34-ffb2-3b67-8049-1f40491264ef",
                    "sense_definition": "Znajdujący się na górze, powyżej czegoś innego lub stanowiący wyższą część jakiegoś obiektu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę położyć tę ciężką książkę na <w>górnej</w> półce regału.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, положите эту тяжелую книгу на <w>верхнюю</w> полку стеллажа."
                            }
                        },
                        {
                            "text": "W lewym <w>górnym</w> rogu ekranu znajduje się mały przycisk menu.",
                            "target_lang_translations": {
                                "ru": "В левом <w>верхнем</w> углу экрана находится маленькая кнопка меню."
                            }
                        }
                    ],
                    "synonyms": [
                        "wierzchni",
                        "szczytowy"
                    ],
                    "antonyms": [
                        "dolny"
                    ],
                    "common_phrases": [
                        "górna półka",
                        "górna warga",
                        "lewy górny róg",
                        "górna część"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся наверху, выше чего-либо другого или составляющий верхнюю часть объекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верхний",
                                "target_lang_sense_clarification": "о физическом положении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15ef85c5-024a-3bed-8d2b-5c2e3163bc60",
                    "sense_definition": "Odnoszący się do obszaru położonego wyżej geograficznie lub znajdującego się bliżej źródła rzeki.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Górny</w> bieg Wisły jest niezwykle malowniczy i pełen naturalnych zakrętów.",
                            "target_lang_translations": {
                                "ru": "<w>Верхнее</w> течение Вислы необычайно живописно и полно естественных изгибов."
                            }
                        },
                        {
                            "text": "Mieszkańcy <w>Górnego</w> Śląska są bardzo przywiązani do swojej regionalnej tradycji.",
                            "target_lang_translations": {
                                "ru": "Жители <w>Верхней</w> Силезии очень привязаны к своим региональным традициям."
                            }
                        }
                    ],
                    "antonyms": [
                        "dolny"
                    ],
                    "common_phrases": [
                        "górny bieg rzeki",
                        "Górny Śląsk",
                        "górny taras"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к местности, расположенной выше географически или ближе к истоку реки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верхний",
                                "target_lang_sense_clarification": "географический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4adb0004-5601-3237-930b-5e6e38440b6d",
                    "sense_definition": "Dotyczący najwyższego stopnia, granicy lub poziomu w określonej skali wartości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scale_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ustalono, że <w>górna</w> granica wieku dla uczestników konkursu wynosi trzydzieści lat.",
                            "target_lang_translations": {
                                "ru": "Было установлено, что <w>верхняя</w> граница возраста для участников конкурса составляет тридцать лет."
                            }
                        },
                        {
                            "text": "Samolot pasażerski osiągnął swój <w>górny</w> pułap i leciał teraz bardzo stabilnie.",
                            "target_lang_translations": {
                                "ru": "Пассажирский самолет достиг своего <w>максимального</w> потолка и летел теперь очень стабильно."
                            }
                        }
                    ],
                    "synonyms": [
                        "maksymalny",
                        "najwyższy"
                    ],
                    "antonyms": [
                        "dolny"
                    ],
                    "common_phrases": [
                        "górna granica",
                        "górny pułap",
                        "górny zakres"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к высшей степени, пределу или уровню в определенной шкале."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верхний",
                                "target_lang_sense_clarification": "о пределе или границе"
                            },
                            {
                                "target_lang_word": "максимальный",
                                "target_lang_sense_clarification": "наивысший"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7834520-e162-31d7-8ba0-1c52fe288174",
                    "sense_definition": "Cechujący się wzniosłością, dumą, a czasem zarozumiałością lub przesadnym patosem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "character_style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>górny</w> i skomplikowany styl pisania był trudny do zrozumienia dla przeciętnego czytelnika.",
                            "target_lang_translations": {
                                "ru": "Его <w>высокопарный</w> и сложный стиль письма был труден для понимания среднему читателю."
                            }
                        },
                        {
                            "text": "Przemawiał w sposób niezwykle <w>górny</w>, starając się zaimponować zgromadzonej publiczności.",
                            "target_lang_translations": {
                                "ru": "Он говорил в чрезвычайно <w>возвышенной</w> манере, стараясь произвести впечатление на собравшуюся публику."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyniosły",
                        "wzniosły",
                        "patetyczny"
                    ],
                    "common_phrases": [
                        "górny styl",
                        "górne loty"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane głównie w literaturze dawnej lub stylizacji"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличающийся возвышенностью, гордостью, иногда высокомерием или излишним пафосом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвышенный",
                                "target_lang_sense_clarification": "о стиле"
                            },
                            {
                                "target_lang_word": "высокопарный",
                                "target_lang_sense_clarification": "патетический"
                            }
                        ]
                    }
                }
            ]
        },
        {

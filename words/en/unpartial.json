{
    "word_family": [
        "partial",
        "impartial",
        "partiality",
        "impartiality"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c3313065-ca04-3b9d-86d9-f1717f694834",
                    "sense_definition": "Free from bias or prejudice; treating all sides equally.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "bias_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee promised to conduct an <w>unpartial</w> review of the evidence.",
                            "target_lang_translations": {
                                "ru": "Комитет пообещал провести <w>беспристрастный</w> анализ доказательств."
                            }
                        },
                        {
                            "text": "A judge must remain <w>unpartial</w> when listening to both sides of the argument.",
                            "target_lang_translations": {
                                "ru": "Судья должен оставаться <w>непредвзятым</w>, выслушивая обе стороны спора."
                            }
                        }
                    ],
                    "synonyms": [
                        "impartial",
                        "unbiased",
                        "neutral",
                        "objective",
                        "fair-minded"
                    ],
                    "antonyms": [
                        "biased",
                        "partial",
                        "prejudiced"
                    ],
                    "common_phrases": [
                        "unpartial judgment",
                        "unpartial advice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободный от предвзятости или предубеждений; относящийся ко всем сторонам одинаково."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беспристрастный",
                                "target_lang_sense_clarification": "справедливый, не склоняющийся ни к одной из сторон"
                            },
                            {
                                "target_lang_word": "непредвзятый",
                                "target_lang_sense_clarification": "лишенный предубеждений"
                            },
                            {
                                "target_lang_word": "объективный",
                                "target_lang_sense_clarification": "основанный на фактах, а не на личном мнении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f74b2e2f-b0b3-323d-9d3a-d30fe2a3b6cc",
                    "sense_definition": "Having a dislike or opposition toward something; not fond of.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "preference_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>unpartial</w> to spicy food and avoids it whenever possible.",
                            "target_lang_translations": {
                                "ru": "Он <w>не питает пристрастия</w> к острой пище и избегает её по возможности."
                            }
                        },
                        {
                            "text": "She remained <w>unpartial</w> to the proposed changes in the office.",
                            "target_lang_translations": {
                                "ru": "Она осталась <w>не расположенной</w> к предложенным изменениям в офисе."
                            }
                        }
                    ],
                    "synonyms": [
                        "opposed",
                        "adverse",
                        "hostile"
                    ],
                    "antonyms": [
                        "partial"
                    ],
                    "common_phrases": [
                        "unpartial to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий неприязнь или несогласие по отношению к чему-либо; не испытывающий симпатии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не питающий пристрастия",
                                "target_lang_sense_clarification": "не испытывающий влечения или любви к чему-либо"
                            },
                            {
                                "target_lang_word": "не расположенный",
                                "target_lang_sense_clarification": "испытывающий неприязнь или отсутствие симпатии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
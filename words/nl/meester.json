                {
                    "sense_id": "b6a8e2bf-5d0c-3d69-bfd3-821149750a62",
                    "sense_definition": "Een onderwijzer, met name in het basisonderwijs.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>meester</w> vertelde een spannend verhaal aan de klas.",
                            "target_lang_translations": {
                                "ru": "<w>Учитель</w> рассказал классу захватывающую историю.",
                                "en": "The <w>teacher</w> told the class an exciting story."
                            }
                        },
                        {
                            "text": "Als de bel gaat, mogen we van de <w>meester</w> naar buiten.",
                            "target_lang_translations": {
                                "ru": "Когда прозвенит звонок, <w>учитель</w> разрешает нам выйти.",
                                "en": "When the bell rings, the <w>teacher</w> allows us to go outside."
                            }
                        }

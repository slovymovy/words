    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bc70ae3f-45bc-312c-9371-2ff8b3feeae4",
                    "sense_definition": "A piece of electronic equipment used to increase the volume of sound, especially for musical instruments; short for <w>amplifier</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "audio_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The guitarist plugged his instrument into the <w>amp</w> before starting the show.",
                            "target_lang_translations": {
                                "ru": "Гитарист подключил свой инструмент к <w>усилителю</w> перед началом шоу."
                            }
                        },
                        {
                            "text": "You need a powerful <w>amp</w> to make the bass audible in such a large hall.",
                            "target_lang_translations": {
                                "ru": "Тебе нужен мощный <w>усилитель</w>, чтобы бас был слышен в таком большом зале."
                            }
                        }
                    ],
                    "synonyms": [
                        "amplifier",
                        "speaker"
                    ],
                    "common_phrases": [
                        "guitar amp",
                        "practice amp"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "clipping"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Электронное устройство для усиления звука, особенно для музыкальных инструментов; сокращение от \"усилитель\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усилитель",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "усилок",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "комбик",
                                "target_lang_sense_clarification": "для гитары"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b39fdfc-d21f-3429-9811-d0015f16ecee",
                    "sense_definition": "A unit for measuring the strength of an electric current; short for <w>ampere</w>.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This fuse is designed to handle up to thirteen <w>amps</w> of current.",
                            "target_lang_translations": {
                                "ru": "Этот предохранитель рассчитан на ток до тринадцати <w>ампер</w>."
                            }
                        },
                        {
                            "text": "The electrician checked how many <w>amps</w> the new appliance would draw.",
                            "target_lang_translations": {
                                "ru": "Электрик проверил, сколько <w>ампер</w> будет потреблять новый прибор."
                            }
                        }
                    ],
                    "common_phrases": [
                        "13-amp fuse",
                        "high amps"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "clipping"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения силы электрического тока; сокращение от \"ампер\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ампер",
                                "target_lang_sense_clarification": "единица измерения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a73c4b9e-e036-3772-bf1f-204b37a74909",
                    "sense_definition": "A small sealed glass capsule containing a liquid, usually for injection; short for <w>ampoule</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical_container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nurse carefully broke the top off the <w>amp</w> to fill the syringe.",
                            "target_lang_translations": {
                                "ru": "Медсестра осторожно отломила верхушку <w>ампулы</w>, чтобы наполнить шприц."
                            }
                        },
                        {
                            "text": "Please bring two <w>amps</w> of the medication to the emergency room immediately.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, немедленно принесите две <w>ампулы</w> лекарства в отделение неотложной помощи."
                            }
                        }
                    ],
                    "synonyms": [
                        "vial"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "clipping"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "medical jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая герметичная стеклянная капсула с жидкостью, обычно для инъекций; сокращение от \"ампула\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ампула",
                                "target_lang_sense_clarification": "медицина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfcdbc89-662d-3c85-9d05-4b8c4cde9660",
                    "sense_definition": "A drug that stimulates the central nervous system; short for <w>amphetamine</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "drug",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The documentary explored the dangers of addiction to <w>amps</w> and other stimulants.",
                            "target_lang_translations": {
                                "ru": "В документальном фильме исследовались опасности зависимости от <w>амфетаминов</w> и других стимуляторов."
                            }
                        },
                        {
                            "text": "He was treated for an overdose after taking several <w>amps</w> at the party.",
                            "target_lang_translations": {
                                "ru": "Его лечили от передозировки после того, как он принял несколько доз <w>амфетамина</w> на вечеринке."
                            }
                        }
                    ],
                    "synonyms": [
                        "speed",
                        "upper"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "clipping"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наркотик, стимулирующий центральную нервную систему; сокращение от \"амфетамин\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "амфетамин",
                                "target_lang_sense_clarification": "наркотик"

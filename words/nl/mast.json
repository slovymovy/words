    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3dbbe87f-355d-3717-b689-34c311f71c39",
                    "sense_definition": "Een lange, rechtopstaande paal in het midden van een schip waaraan de zeilen, ra's en touwen worden bevestigd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "vertical_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De matroos klom behendig in de <w>mast</w> om de vlag te repareren.",
                            "target_lang_translations": {
                                "ru": "Матрос ловко взобрался на <w>мачту</w>, чтобы починить флаг."
                            }
                        },
                        {
                            "text": "Tijdens de zware storm brak de grote <w>mast</w> van het zeilschip af.",
                            "target_lang_translations": {
                                "ru": "Во время сильного шторма сломалась большая <w>мачта</w> парусника."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheepsmast"
                    ],
                    "common_phrases": [
                        "in de mast klimmen",
                        "de mast strijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный вертикальный столб на судне, к которому крепятся паруса, реи и такелаж."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мачта",
                                "target_lang_sense_clarification": "на судне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8e3c61a-85b7-3108-8cb6-be86ea44a5d4",
                    "sense_definition": "Een hoge paal of metalen constructie waaraan kabels voor elektriciteit of telefonie zijn opgehangen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vertical_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Langs de snelweg zie je vaak een hoge <w>mast</w> voor de stroomvoorziening.",
                            "target_lang_translations": {
                                "ru": "Вдоль шоссе часто можно увидеть высокую <w>опору</w> линии электропередачи."
                            }
                        },
                        {
                            "text": "De technici moesten de <w>mast</w> beklimmen om de kapotte kabel te vervangen.",
                            "target_lang_translations": {
                                "ru": "Техникам пришлось лезть на <w>опору</w>, чтобы заменить поврежденный кабель."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoogspanningsmast",
                        "pyloon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокий столб или металлическая конструкция, на которой подвешены электрические или телефонные кабели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опора",
                                "target_lang_sense_clarification": "ЛЭП или связи"
                            },
                            {
                                "target_lang_word": "мачта",
                                "target_lang_sense_clarification": "высоковольтная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "907b4f8f-1035-3083-900f-a433f9cfa4ce",
                    "sense_definition": "Een hoge toren of paal die dient voor het zenden en ontvangen van signalen voor radio, televisie of mobiele telefonie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vertical_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sinds er een nieuwe <w>mast</w> is geplaatst, hebben we veel beter bereik met onze mobiele telefoons.",
                            "target_lang_translations": {
                                "ru": "С тех пор как установили новую <w>вышку</w>, у нас стала намного лучше мобильная связь."
                            }
                        },
                        {
                            "text": "De zender huurt ruimte in de <w>mast</w> om zijn radiosignaal te verspreiden.",
                            "target_lang_translations": {
                                "ru": "Вещатель арендует место на <w>мачте</w> для распространения своего радиосигнала."
                            }
                        }
                    ],
                    "synonyms": [
                        "zendmast",
                        "antennemast"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокая башня или столб, служащий для передачи и приема радио-, теле- или мобильных сигналов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вышка",
                                "target_lang_sense_clarification": "сотовой связи"
                            },
                            {
                                "target_lang_word": "мачта",
                                "target_lang_sense_clarification": "антенная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c6c4a3f-a7df-3bc8-9167-c57fe261aeb9",
                    "sense_definition": "Voedsel voor varkens, voornamelijk bestaande uit eikels en beukennoten die in het bos worden gevonden.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fodder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vroeger dreven boeren hun varkens in de herfst het bos in voor de <w>mast</w>.",
                            "target_lang_translations": {
                                "ru": "Раньше осенью фермеры гнали свиней в лес на <w>желуди</w>."
                            }
                        },
                        {
                            "text": "Dit jaar is er veel <w>mast</w> gevallen, wat goed is voor de wilde zwijnen.",
                            "target_lang_translations": {
                                "ru": "В этом году упало много <w>желудей</w>, что хорошо для диких кабанов."
                            }
                        }
                    ],
                    "synonyms": [
                        "varkensvoer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Корм для свиней, состоящий в основном из желудей и буковых орехов, найденных в лесу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "желуди",
                                "target_lang_sense_clarification": "корм"
                            },
                            {
                                "target_lang_word": "лесной корм",
                                "target_lang_sense_clarification": "для свиней"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ce7add19-9797-3514-bb82-7100df4172d8",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>masten</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>mast</w> de varkens met eikels zodat ze goed groeien.",
                            "target_lang_translations": {
                                "ru": "Я <w>откармливаю</w> свиней желудями, чтобы они хорошо росли."
                            }
                        },
                        {
                            "text": "Als ik het schip <w>mast</w>, zorg ik dat alles stevig vastzit.",
                            "target_lang_translations": {
                                "ru": "Когда я <w>ставлю мачту</w> на корабль, я слежу, чтобы все было надежно закреплено."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола masten."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "откармливаю",
                                "target_lang_sense_clarification": "животных"
                            },
                            {
                                "target_lang_word": "ставлю мачту",
                                "target_lang_sense_clarification": "на судно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

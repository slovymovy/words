            "senses": [
                {
                    "sense_id": "2d0f033c-acba-3071-b5a2-84e4f517b3f1",
                    "sense_definition": "mający wyraźnie widoczne, wypukłe żyły pod skórą (zwykle o ciele człowieka)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Popatrz, jakie Marek ma teraz <w>żylaste</w> ręce!",
                            "target_lang_translations": {
                                "ru": "Посмотри, какие у Марека сейчас <w>жилистые</w> руки!"
                            }
                        },
                        {
                            "text": "Starszy mężczyzna miał chude i <w>żylaste</w> ramiona od ciężkiej pracy fizycznej.",
                            "target_lang_translations": {
                                "ru": "У пожилого мужчины были худые и <w>жилистые</w> плечи от тяжелой физической работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "żyłowaty"
                    ],
                    "common_phrases": [
                        "żylaste ręce",
                        "żylaste dłonie",
                        "żylaste ramiona"
                    ],
                    "target_lang_definitions": {
                        "ru": "имеющий отчетливо видимые, выпуклые вены под кожей (обычно о теле человека)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жилистый",
                                "target_lang_sense_clarification": "о теле, руках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60e4964f-d054-39bd-86b2-70637505e252",
                    "sense_definition": "zawierający dużo twardych włókien i tkanki łącznej, trudny do pogryzienia (zwykle o mięsie)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>żylaste</w> mięso jest niemożliwe do pogryzienia, mimo długiego gotowania.",
                            "target_lang_translations": {
                                "ru": "Это <w>жилистое</w> мясо невозможно разжевать, несмотря на долгую варку."
                            }
                        },
                        {
                            "text": "Kupiłem tani kawałek wołowiny, który okazał się twardy i <w>żylasty</w>.",
                            "target_lang_translations": {
                                "ru": "Я купил дешевый кусок говядины, который оказался жестким и <w>жилистым</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "łykowaty",
                        "twardy",
                        "włóknisty"
                    ],
                    "antonyms": [
                        "kruchy",
                        "miękki"
                    ],
                    "common_phrases": [
                        "żylaste mięso",
                        "żylasty kawałek"
                    ],
                    "target_lang_definitions": {
                        "ru": "содержащий много твердых волокон и соединительной ткани, трудный для пережевывания (обычно о мясе)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жилистый",
                                "target_lang_sense_clarification": "о мясе"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "ab294214-1a51-3eae-b1c6-28c7d1d5abc8",
                    "sense_definition": "Geestelijk niet helder of in de war; niet goed wetend wat men moet doen of denken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>verwarde</w> man wist niet meer waar hij woonde.",
                            "target_lang_translations": {
                                "en": "The <w>confused</w> man no longer knew where he lived."
                            }
                        },
                        {
                            "text": "Ze keek me <w>verward</w> aan toen ik haar een vraag in het Frans stelde.",
                            "target_lang_translations": {
                                "en": "She looked at me <w>confused</w> when I asked her a question in French."
                            }
                        }
                    ],
                    "synonyms": [
                        "in de war",
                        "onthutst",
                        "beduusd"
                    ],
                    "antonyms": [
                        "helder",
                        "nuchter"
                    ],
                    "common_phrases": [
                        "verwarde indruk",
                        "verward overkomen",
                        "verwarde man"
                    ],
                    "target_lang_definitions": {
                        "en": "Mentally not clear or confused; uncertain about what to do or think."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "confused",
                                "target_lang_sense_clarification": "unable to think clearly"
                            },
                            {
                                "target_lang_word": "disoriented",
                                "target_lang_sense_clarification": "having lost one's sense of direction or position"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "338ae2c8-9baf-3eb2-9a40-47bfc4b0cfcc",
                    "sense_definition": "Door elkaar geraakt of zonder duidelijke structuur; niet netjes geordend.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disorder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de storm lagen de kabels <w>verward</w> op de grond.",
                            "target_lang_translations": {
                                "en": "After the storm, the cables lay <w>tangled</w> on the ground."
                            }
                        },
                        {
                            "text": "Hij vertelde een <w>verward</w> verhaal waar niemand iets van begreep.",
                            "target_lang_translations": {
                                "en": "He told a <w>confused</w> story that no one understood."
                            }

{
    "word_family": [
        "bedoelen",
        "bedoeling",
        "doel"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6202f227-a63f-3cf5-b162-ef1331415c68",
                    "sense_definition": "Niet met opzet gedaan of gezegd; zonder dat het de bedoeling was.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "unintentional_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was een <w>onbedoelde</w> fout die gelukkig snel kon worden hersteld.",
                            "target_lang_translations": {
                                "en": "It was an <w>unintentional</w> mistake that fortunately could be fixed quickly.",
                                "ru": "Это была <w>непреднамеренная</w> ошибка, которую, к счастью, удалось быстро исправить."
                            }
                        },
                        {
                            "text": "Zijn grapje had een <w>onbedoeld</w> effect op de sfeer in de kamer.",
                            "target_lang_translations": {
                                "en": "His joke had an <w>unintended</w> effect on the atmosphere in the room.",
                                "ru": "Его шутка произвела <w>непреднамеренный</w> эффект на атмосферу в комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "onopzettelijk",
                        "ongewild"
                    ],
                    "antonyms": [
                        "bedoeld"
                    ],
                    "common_phrases": [
                        "onbedoelde gevolgen",
                        "onbedoelde effecten",
                        "onbedoelde bijwerkingen"
                    ],
                    "target_lang_definitions": {
                        "en": "Not done or said on purpose; without it being the intention.",
                        "ru": "Сделанный или сказанный не специально; без намерения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unintentional",
                                "target_lang_sense_clarification": "general term for something not done on purpose"
                            },
                            {
                                "target_lang_word": "unintended",
                                "target_lang_sense_clarification": "often used regarding consequences or effects"
                            },
                            {
                                "target_lang_word": "inadvertent",
                                "target_lang_sense_clarification": "accidental, often resulting from lack of attention"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "непреднамеренный",
                                "target_lang_sense_clarification": "не запланированный заранее"
                            },
                            {
                                "target_lang_word": "неумышленный",
                                "target_lang_sense_clarification": "совершенный без злого умысла"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8bca2bd1-932a-3ee9-9fd4-ca0203fc81f5",
                    "sense_definition": "Op een manier die niet van tevoren gepland of gewenst was; per ongeluk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "unintentional_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb je gisteren <w>onbedoeld</w> gekwetst met mijn opmerking.",
                            "target_lang_translations": {
                                "en": "I <w>unintentionally</w> hurt your feelings yesterday with my remark.",
                                "ru": "Я вчера <w>невольно</w> обидел тебя своим замечанием."
                            }
                        },
                        {
                            "text": "Hij liet <w>onbedoeld</w> de deur openstaan toen hij wegging.",
                            "target_lang_translations": {
                                "en": "He <w>inadvertently</w> left the door open when he left.",
                                "ru": "Уходя, он <w>случайно</w> оставил дверь открытой."
                            }
                        }
                    ],
                    "synonyms": [
                        "onopzettelijk",
                        "per ongeluk"
                    ],
                    "common_phrases": [
                        "onbedoeld kwetsen",
                        "onbedoeld grappig",
                        "onbedoeld meeluisteren"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that was not planned or desired beforehand; by accident.",
                        "ru": "Совершенный без умысла или плана; случайно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unintentionally",
                                "target_lang_sense_clarification": "without purpose or design"
                            },
                            {
                                "target_lang_word": "inadvertently",
                                "target_lang_sense_clarification": "by oversight or accident"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нечаянно",
                                "target_lang_sense_clarification": "по ошибке, без намерения"
                            },
                            {
                                "target_lang_word": "случайно",
                                "target_lang_sense_clarification": "без предварительного плана"
                            },
                            {
                                "target_lang_word": "невольно",
                                "target_lang_sense_clarification": "помимо воли, машинально"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
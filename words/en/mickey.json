                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The stockman chased the <w>mickey</w> back into the herd.",
                            "target_lang_translations": {
                                "ru": "Скотовод погнал <w>бычка</w> обратно в стадо."
                            }

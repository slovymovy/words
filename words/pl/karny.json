            "senses": [
                {
                    "sense_id": "cb906941-d6cd-36d0-a5e5-b1c333e68f15",
                    "sense_definition": "W sporcie (głównie piłce nożnej): rzut karny; strzał na bramkę dyktowany za przewinienie przeciwnika.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sport_penalty",
                    "name_type": "no",
                    "examples": [

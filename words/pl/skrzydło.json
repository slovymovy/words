        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c5e65168-6df0-3963-93b0-59b3db5e10ae",
                    "sense_definition": "Narząd u ptaków, owadów lub nietoperzy, który umożliwia im latanie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "flight_blade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mały ptak złamał <w>skrzydło</w> i nie mógł latać.",
                            "target_lang_translations": {
                                "ru": "Маленькая птица сломала <w>крыло</w> и не могла летать."
                            }
                        },
                        {
                            "text": "Motyl ma bardzo kolorowe i delikatne <w>skrzydła</w>.",
                            "target_lang_translations": {
                                "ru": "У бабочки очень пестрые и нежные <w>крылья</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rozwinąć skrzydła",
                        "podciąć skrzydła",
                        "wziąć pod skrzydła",
                        "trzepotać skrzydłami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Орган у птиц, насекомых или летучих мышей, который позволяет им летать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крыло",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfc59c41-da82-3fa0-a002-322a5b0f7349",
                    "sense_definition": "Część samolotu wytwarzająca siłę nośną, dzięki której maszyna unosi się w powietrzu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "flight_blade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas lotu lubię patrzeć na silnik zamontowany na <w>skrzydle</w>.",
                            "target_lang_translations": {
                                "ru": "Во время полета я люблю смотреть на двигатель, установленный на <w>крыле</w>."
                            }
                        },
                        {
                            "text": "Ten samolot pasażerski ma bardzo długie <w>skrzydła</w>.",
                            "target_lang_translations": {
                                "ru": "У этого пассажирского самолета очень длинные <w>крылья</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "płat"
                    ],
                    "common_phrases": [
                        "miejsce przy skrzydle",
                        "siedzieć na skrzydle",
                        "rozpiętość skrzydeł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть самолета, создающая подъемную силу, благодаря которой машина держится в воздухе."

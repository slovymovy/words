            "senses": [
                {
                    "sense_id": "a440febc-4174-3a1e-8474-bbaa3705976b",
                    "sense_definition": "An offensive term for a person of Irish descent.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slur_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was deeply offended when someone called him a <w>Mick</w> at the bar.",
                            "target_lang_translations": {
                                "ru": "Он был глубоко оскорблен, когда кто-то назвал его <w>ирлашкой</w> в баре."
                            }
                        },
                        {
                            "text": "The use of the word <w>Mick</w> is considered a derogatory slur.",
                            "target_lang_translations": {
                                "ru": "Использование слова <w>Мик</w> считается унизительным оскорблением."
                            }

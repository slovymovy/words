                            },
                            {
                                "target_lang_word": "подальше",
                                "target_lang_sense_clarification": "убрать с глаз долой, на хранение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6fefa0e-5593-35a3-baa1-2774b9fccc0b",

{
    "word_family": [
        "umiejętność",
        "umiejętny",
        "umiejętnie",
        "nieumiejętność"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2270ba50-5368-31ea-b859-167ec3673953",
                    "sense_definition": "Posiadać zdolność lub wiedzę, aby wykonać jakąś czynność; potrafić coś zrobić.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój mały brat już <umie> czytać i pisać.",
                            "target_lang_translations": {
                                "ru": "Мой младший брат уже <умеет> читать и писать."
                            }
                        },
                        {
                            "text": "Czy <umiesz> naprawić ten zepsuty zegarek?",
                            "target_lang_translations": {
                                "ru": "Ты <умеешь> чинить эти сломанные часы?"
                            }
                        },
                        {
                            "text": "Bardzo chciałbym <umieć> pływać tak dobrze jak ty.",
                            "target_lang_translations": {
                                "ru": "Я бы очень хотел <уметь> плавать так же хорошо, как ты."
                            }
                        }
                    ],
                    "synonyms": [
                        "potrafić",
                        "być w stanie"
                    ],
                    "common_phrases": [
                        "umieć pływać",
                        "umieć czytać",
                        "umieć słuchać",
                        "umieć się zachować",
                        "umieć sobie radzić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладать способностью или навыками для выполнения какого-либо действия; уметь делать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уметь",
                                "target_lang_sense_clarification": "обладать навыком"
                            },
                            {
                                "target_lang_word": "мочь",
                                "target_lang_sense_clarification": "быть в состоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c7d994b-afa7-3442-9796-21a5e5c31561",
                    "sense_definition": "Posiadać wiedzę z jakiejś dziedziny lub znać język obcy w stopniu pozwalającym na posługiwanie się nim.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ona świetnie <umie> język angielski i niemiecki.",
                            "target_lang_translations": {
                                "ru": "Она отлично <знает> английский и немецкий языки."
                            }
                        },
                        {
                            "text": "Nie <umiem> matematyki, więc muszę się dużo uczyć.",
                            "target_lang_translations": {
                                "ru": "Я не <знаю> математику, поэтому мне нужно много учиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "znać"
                    ],
                    "common_phrases": [
                        "umieć angielski",
                        "umieć wszystko",
                        "słabo umieć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Владеть знаниями в какой-либо области или знать иностранный язык."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знать",
                                "target_lang_sense_clarification": "владеть информацией или языком"
                            },
                            {
                                "target_lang_word": "владеть",
                                "target_lang_sense_clarification": "языком или предметом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "987d79f1-c52d-34d3-a43c-aef91e2d8607",
                    "sense_definition": "Być w stanie wyrecytować coś z pamięci lub znać odpowiedź na pytania (często w kontekście szkolnym).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <umieć> ten wiersz na pamięć na jutro.",
                            "target_lang_translations": {
                                "ru": "Я должен <знать> это стихотворение наизусть к завтрашнему дню."
                            }
                        },
                        {
                            "text": "Uczeń <umiał> odpowiedzieć na wszystkie pytania nauczyciela.",
                            "target_lang_translations": {
                                "ru": "Ученик <смог> ответить на все вопросы учителя."
                            }
                        }
                    ],
                    "synonyms": [
                        "znać",
                        "wiedzieć"
                    ],
                    "common_phrases": [
                        "umieć na pamięć",
                        "umieć wiersz",
                        "umieć lekcję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знать наизусть или быть готовым ответить урок (обычно в школьном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знать",
                                "target_lang_sense_clarification": "помнить наизусть, выучить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
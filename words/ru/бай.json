                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Моего хорошего знакомого зовут <Бай>.",
                            "target_lang_translations": {
                                "en": "My good acquaintance is named <Bai>."
                            }
                        },
                        {
                            "text": "<Бай> приехал к нам в гости из другого города.",
                            "target_lang_translations": {
                                "en": "<Bai> came to visit us from another city."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A male personal name."

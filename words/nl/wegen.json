            "senses": [
                {
                    "sense_id": "02cd9765-cb8a-3647-8525-6ef2d429f8f3",
                    "sense_definition": "Een bepaald gewicht als eigenschap hebben.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "weight_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hoeveel <w>weeg</w> jij eigenlijk?",
                            "target_lang_translations": {
                                "en": "How much do you actually <w>weigh</w>?",
                                "ru": "Сколько ты на самом деле <w>весишь</w>?"
                            }
                        },
                        {
                            "text": "Mijn koffer <w>woog</w> precies twintig kilo op de luchthaven.",
                            "target_lang_translations": {
                                "en": "My suitcase <w>weighed</w> exactly twenty kilos at the airport.",
                                "ru": "Мой чемодан <w>весил</w> ровно двадцать килограммов в аэропорту."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zwaar wegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a specific weight.",
                        "ru": "Иметь определенный вес как характеристику."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weigh",
                                "target_lang_sense_clarification": "to have a certain weight"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "весить",
                                "target_lang_sense_clarification": "иметь вес"
                            }
                        ]

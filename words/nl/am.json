                {
                    "sense_id": "95521f3b-8278-3c9e-a4ab-661051c09e8e",
                    "sense_definition": "Een techniek om radiosignalen uit te zenden en te ontvangen via de middengolf; afkorting van amplitudemodulatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel oude radiostations zenden nog steeds uit via de <w>AM</w>.",
                            "target_lang_translations": {
                                "ru": "Многие старые радиостанции все еще вещают в диапазоне <w>АМ</w>.",
                                "en": "Many old radio stations still broadcast via <w>AM</w>."
                            }
                        },
                        {
                            "text": "De geluidskwaliteit op de <w>AM</w> is vaak minder goed dan op de FM.",
                            "target_lang_translations": {
                                "ru": "Качество звука на <w>АМ</w> часто хуже, чем на FM.",
                                "en": "The sound quality on <w>AM</w> is often not as good as on FM."
                            }
                        }
                    ],
                    "synonyms": [
                        "amplitudemodulatie"
                    ],
                    "antonyms": [
                        "FM"
                    ],
                    "common_phrases": [
                        "op de AM",
                        "AM-radio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Технология передачи и приема радиосигналов на средних волнах; сокращение от амплитудной модуляции.",
                        "en": "A technique for broadcasting and receiving radio signals via medium wave; abbreviation of amplitude modulation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "АМ",
                                "target_lang_sense_clarification": "радиовещание"
                            },
                            {
                                "target_lang_word": "амплитудная модуляция",
                                "target_lang_sense_clarification": "полный термин"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "AM",
                                "target_lang_sense_clarification": "radio broadcasting"
                            },
                            {
                                "target_lang_word": "amplitude modulation",
                                "target_lang_sense_clarification": "full term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70240a9a-c6f6-3fbd-9d98-d89bcf61fd59",
                    "sense_definition": "Een vrouw die het kind van een andere vrouw borstvoeding geeft, vaak als beroep.",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4ecbf6b3-71ef-3b66-b722-257b7e3ad419",
                    "sense_definition": "Simple past and past participle of <w>cover</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>covered</w> the sleeping child with a warm wool blanket.",
                            "target_lang_translations": {
                                "ru": "Она <w>укры́ла</w> спящего ребёнка тёплым шерстяным одеялом."
                            }
                        },
                        {
                            "text": "The journalist <w>covered</w> the breaking news story from the scene.",
                            "target_lang_translations": {
                                "ru": "Журналист <w>освеща́л</w> срочные новости с места событий."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола cover."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накры́л",
                                "target_lang_sense_clarification": "положил что-то сверху (прошедшее время)"
                            },
                            {
                                "target_lang_word": "укры́л",
                                "target_lang_sense_clarification": "спрятал или защитил (прошедшее время)"
                            },
                            {
                                "target_lang_word": "освеща́л",
                                "target_lang_sense_clarification": "описывал события в СМИ"
                            },
                            {
                                "target_lang_word": "покры́тый",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "43b3427b-4053-34de-8017-5b2fab70ba5f",
                    "sense_definition": "Overlaid with or enclosed within something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mountain peaks are <w>covered</w> in deep white snow all winter.",
                            "target_lang_translations": {
                                "ru": "Горные вершины всю зиму <w>покры́ты</w> глубоким белым снегом."
                            }
                        },
                        {
                            "text": "She found an old wooden box <w>covered</w> with dust in the attic.",
                            "target_lang_translations": {
                                "ru": "На чердаке она нашла старую деревянную шкатулку, <w>покры́тую</w> пылью."
                            }
                        }
                    ],
                    "synonyms": [
                        "coated",
                        "wrapped",
                        "blanketed"
                    ],
                    "antonyms": [
                        "uncovered"
                    ],
                    "common_phrases": [
                        "covered in snow",
                        "covered with dust",
                        "covered bridge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытый чем-либо или заключенный внутри чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покры́тый",
                                "target_lang_sense_clarification": "имеющий слой чего-либо на поверхности"
                            },
                            {
                                "target_lang_word": "усы́панный",
                                "target_lang_sense_clarification": "покрытый множеством мелких объектов"
                            },
                            {
                                "target_lang_word": "закры́тый",
                                "target_lang_sense_clarification": "спрятанный или загороженный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47b29fd0-b0a0-3b30-b4dd-bf2562932cb5",
                    "sense_definition": "Protected by insurance or having a specific situation dealt with.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My travel insurance means I am <w>covered</w> for medical emergencies abroad.",
                            "target_lang_translations": {
                                "ru": "Моя туристическая страховка означает, что я <w>застрахо́ван</w> на случай неотложной медицинской помощи за границей."
                            }
                        },
                        {
                            "text": "Don't worry about the bill because we have got you <w>covered</w>.",
                            "target_lang_translations": {
                                "ru": "Не беспокойся о счёте, мы всё <w>опла́тим</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "insured",
                        "protected"
                    ],
                    "common_phrases": [
                        "got you covered",
                        "fully covered"
                    ],
                    "target_lang_definitions": {
                        "ru": "Застрахованный или обеспеченный защитой в определенной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застрахо́ванный",
                                "target_lang_sense_clarification": "имеющий страховку"
                            },
                            {
                                "target_lang_word": "защищённый",
                                "target_lang_sense_clarification": "находящийся под защитой"
                            },
                            {
                                "target_lang_word": "опла́ченный",
                                "target_lang_sense_clarification": "о расходах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b41c28e-63e2-3a0d-8cf0-0922ab5116bb",
                    "sense_definition": "Wearing a hat or head covering.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers remained <w>covered</w> while standing outside in the cold rain.",
                            "target_lang_translations": {
                                "ru": "Солдаты оставались <w>в головны́х убо́рах</w>, стоя на улице под холодным дождём."
                            }
                        },
                        {
                            "text": "It is considered polite for gentlemen not to stay <w>covered</w> indoors.",
                            "target_lang_translations": {
                                "ru": "Для джентльменов считается вежливым не оставаться <w>в головно́м убо́ре</w> в помещении."
                            }
                        }
                    ],
                    "synonyms": [
                        "hatted"
                    ],
                    "antonyms": [
                        "uncovered",
                        "uncovered",
                        "uncovered"
                    ],
                    "common_phrases": [
                        "remain covered"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "formal or military context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в головном уборе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в головно́м убо́ре",
                                "target_lang_sense_clarification": "носящий шапку или шляпу"
                            },
                            {
                                "target_lang_word": "с покры́той голово́й",
                                "target_lang_sense_clarification": "описание состояния"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24029895-8359-3058-a1d8-0d5b50de7eb7",
                    "sense_definition": "Having less money available for betting than another player.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professional player had the amateur <w>covered</w> by a large margin.",
                            "target_lang_translations": {
                                "ru": "Стек профессионального игрока значительно <w>перекрыва́л</w> стек любителя."
                            }
                        },
                        {
                            "text": "If your opponent has you <w>covered</w>, you risk losing all your chips.",
                            "target_lang_translations": {
                                "ru": "Если оппонент <w>перекрыва́ет</w> тебя по фишкам, ты рискуешь потерять весь свой стек."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have someone covered"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "poker terminology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий меньше денег для ставок, чем другой игрок (в покере)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перекры́тый",
                                "target_lang_sense_clarification": "о стеке фишек в покере"
                            },
                            {
                                "target_lang_word": "покрыва́емый",
                                "target_lang_sense_clarification": "тот, чей стек меньше стека оппонента"
                            }
                        ]
                    }
                }
            ]
        }
    ]

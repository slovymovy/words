            "senses": [
                {
                    "sense_id": "ca1922dd-4ae1-3e9e-aaf7-26240725f1c7",
                    "sense_definition": "Форма вежливого обращения к кому-либо в официальной речи или письменном тексте.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "address_formal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Уважаемые> пассажиры, наш поезд прибывает на конечную станцию.",
                            "target_lang_translations": {
                                "en": "<Dear> passengers, our train is arriving at the terminal station."
                            }
                        },
                        {
                            "text": "<Уважаемый> Иван Петрович, прошу рассмотреть моё заявление.",
                            "target_lang_translations": {
                                "en": "<Dear> Ivan Petrovich, I ask you to consider my application."
                            }
                        }
                    ],
                    "synonyms": [
                        "достопочтенный",
                        "почтеннейший"
                    ],
                    "common_phrases": [

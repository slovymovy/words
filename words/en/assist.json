{
    "word_family": [
        "assistance",
        "assistant",
        "assistive"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "335f8efd-f28c-3a4e-be35-694bea5ba604",
                    "sense_definition": "To give help or support to someone, often by sharing the work or providing resources.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "help_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new software will <w>assist</w> employees in tracking their hours more accurately.",
                            "target_lang_translations": {
                                "ru": "Новое программное обеспечение <w>поможет</w> сотрудникам точнее отслеживать свое рабочее время."
                            }
                        },
                        {
                            "text": "Can you <w>assist</w> me with moving this heavy table?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>помочь</w> мне передвинуть этот тяжелый стол?"
                            }
                        }
                    ],
                    "synonyms": [
                        "help",
                        "help out"
                    ],
                    "antonyms": [
                        "hinder"
                    ],
                    "common_phrases": [
                        "assist with",
                        "assist in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помогать или оказывать поддержку кому-либо, часто разделяя работу или предоставляя ресурсы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помога́ть",
                                "target_lang_sense_clarification": "наиболее общее и употребительное слово"
                            },
                            {
                                "target_lang_word": "соде́йствовать",
                                "target_lang_sense_clarification": "официальное или книжное; оказывать содействие"
                            },
                            {
                                "target_lang_word": "ассисти́ровать",
                                "target_lang_sense_clarification": "помогать специалисту (врачу, профессору) при выполнении работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d1ec483-04e4-33bb-82da-3f310230e08d",
                    "sense_definition": "To make a pass or play that leads directly to a teammate scoring a goal or point.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>assisted</w> the winning goal in the final minute of the match.",
                            "target_lang_translations": {
                                "ru": "Он <w>ассистировал</w> при победном голе на последней минуте матча."
                            }
                        },
                        {
                            "text": "She prefers to <w>assist</w> her teammates rather than take the shot herself.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает <w>ассистировать</w> партнерам по команде, а не бросать сама."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать передачу или разыграть комбинацию, которая приводит непосредственно к голу или очку партнера по команде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ассисти́ровать",
                                "target_lang_sense_clarification": "спорт.; делать результативную передачу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84836ddb-9fd3-3263-92b2-66b79e5cb2f1",
                    "sense_definition": "To be present at an event or ceremony as a spectator or participant.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "presence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>assisted</w> at the opening ceremony of the new library.",
                            "target_lang_translations": {
                                "ru": "Мы <w>присутствовали</w> на церемонии открытия новой библиотеки."
                            }
                        },
                        {
                            "text": "Many foreign dignitaries <w>assisted</w> at the royal wedding.",
                            "target_lang_translations": {
                                "ru": "Многие иностранные сановники <w>присутствовали</w> на королевской свадьбе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "assist at"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern usage typically prefers 'attend'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Присутствовать на событии или церемонии в качестве зрителя или участника (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прису́тствовать",
                                "target_lang_sense_clarification": "находиться в каком-либо месте в данное время"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8c7387ba-ddef-35b0-9d39-3cefb877b55b",
                    "sense_definition": "An act of contributing to a goal or point scored by a teammate, usually by passing the ball.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sports_stat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The midfielder recorded fifteen <w>assists</w> this season.",
                            "target_lang_translations": {
                                "ru": "В этом сезоне полузащитник записал на свой счет пятнадцать <w>результативных передач</w>."
                            }
                        },
                        {
                            "text": "It was a beautiful <w>assist</w> that allowed the striker to score easily.",
                            "target_lang_translations": {
                                "ru": "Это был красивый <w>голевой пас</w>, который позволил нападающему легко забить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "record an assist",
                        "provide an assist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие (обычно передача мяча), которое помогает товарищу по команде забить гол или набрать очки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "результати́вная переда́ча",
                                "target_lang_sense_clarification": "официальный спортивный термин"
                            },
                            {
                                "target_lang_word": "голево́й пас",
                                "target_lang_sense_clarification": "распространенный термин в футболе и хоккее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36913641-1d43-3cd0-aaaa-62db3bfc3c01",
                    "sense_definition": "An act of helping or giving support.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "help_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thanks for the <w>assist</w> with fixing my flat tire.",
                            "target_lang_translations": {
                                "ru": "Спасибо за <w>помощь</w> с починкой моего спущенного колеса."
                            }
                        },
                        {
                            "text": "The project succeeded thanks to a timely <w>assist</w> from the IT department.",
                            "target_lang_translations": {
                                "ru": "Проект увенчался успехом благодаря своевременной <w>поддержке</w> со стороны IT-отдела."
                            }
                        }
                    ],
                    "synonyms": [
                        "help"
                    ],
                    "common_phrases": [
                        "give an assist",
                        "need an assist"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Often used in informal contexts."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт помощи или предоставления поддержки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по́мощь",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "подде́ржка",
                                "target_lang_sense_clarification": "оказание содействия или опоры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c31dceab-b73c-32fc-9689-575dfce72342",
                    "sense_definition": "A defensive play in baseball where a fielder touches the ball before a putout is recorded by another teammate.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "baseball_stat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shortstop was credited with an <w>assist</w> after fielding the ground ball.",
                            "target_lang_translations": {
                                "ru": "Шорт-стопу записали <w>ассист</w> после того, как он обработал граунд-бол."
                            }
                        },
                        {
                            "text": "He leads the league in outfield <w>assists</w> this year.",
                            "target_lang_translations": {
                                "ru": "В этом году он лидирует в лиге по <w>ассистам</w> среди аутфилдеров."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оборонительное действие в бейсболе, когда полевой игрок касается мяча до того, как другой товарищ по команде выводит соперника в аут."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "асси́ст",
                                "target_lang_sense_clarification": "бейсбольный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
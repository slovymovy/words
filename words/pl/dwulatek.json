                    "frequency": "Low",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przedszkolu utworzono nową grupę dla samych <w>dwulatek</w>.",
                            "target_lang_translations": {
                                "ru": "В детском саду создали новую группу только для <w>двухлеток</w>."
                            }

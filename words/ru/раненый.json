    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9034a8ea-2349-354f-a58d-848f81d0a962",
                    "sense_definition": "Имеющий рану, получивший телесное повреждение от оружия, хищника или в результате несчастного случая.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охотники осторожно приближались к <w>раненому</w> медведю, держа ружья наготове.",
                            "target_lang_translations": {
                                "en": "The hunters cautiously approached the <w>wounded</w> bear, holding their guns ready."
                            }
                        },
                        {
                            "text": "На поле боя остался лежать <w>раненый</w> солдат, ожидающий помощи.",
                            "target_lang_translations": {
                                "en": "A <w>wounded</w> soldier remained lying on the battlefield, waiting for help."
                            }
                        }
                    ],
                    "synonyms": [
                        "пораненный",
                        "травмированный"
                    ],
                    "antonyms": [
                        "здоровый",
                        "нетронутый"
                    ],
                    "common_phrases": [
                        "раненый зверь",
                        "раненый солдат",
                        "раненый боец"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a physical injury, especially one caused by a weapon or accident."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wounded",
                                "target_lang_sense_clarification": "suffering from a wound, especially from battle"
                            },
                            {
                                "target_lang_word": "injured",
                                "target_lang_sense_clarification": "physically hurt or harmed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2c6cdd7-887f-3f18-a367-6ea878546849",
                    "sense_definition": "Повреждённый вследствие ранения (о части тела).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он аккуратно перевязал <w>раненую</w> руку чистым белым платком.",
                            "target_lang_translations": {
                                "en": "He carefully bandaged the <w>wounded</w> hand with a clean white handkerchief."
                            }
                        },
                        {
                            "text": "Боец старался не наступать на <w>раненую</w> ногу, чтобы не усиливать боль.",
                            "target_lang_translations": {
                                "en": "The fighter tried not to step on his <w>injured</w> leg so as not to increase the pain."
                            }
                        }
                    ],
                    "synonyms": [
                        "повреждённый"
                    ],
                    "antonyms": [
                        "здоровый"
                    ],
                    "common_phrases": [
                        "раненая рука",
                        "раненая нога",
                        "раненое плечо"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a body part that has been damaged or hurt."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "injured",
                                "target_lang_sense_clarification": "damaged or hurt"
                            },
                            {
                                "target_lang_word": "wounded",
                                "target_lang_sense_clarification": "having an open wound"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9e0372f-d860-3480-a4e7-98c7c3a05089",
                    "sense_definition": "Человек, который получил ранение; пострадавший.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи всю ночь боролись за жизнь <w>раненого</w>, поступившего в госпиталь.",
                            "target_lang_translations": {
                                "en": "Doctors fought all night for the life of the <w>wounded man</w> admitted to the hospital."
                            }
                        },
                        {
                            "text": "Медсестра принесла воды <w>раненому</w>, который тихо стонал на койке.",
                            "target_lang_translations": {
                                "en": "The nurse brought water to the <w>wounded man</w>, who was moaning quietly on the bunk."
                            }
                        }
                    ],
                    "antonyms": [
                        "здоровый"
                    ],
                    "common_phrases": [
                        "помощь раненым",
                        "перевозка раненых",
                        "тяжело раненый"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who has been injured, especially in battle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wounded",
                                "target_lang_sense_clarification": "a person suffering from a wound (often used collectively)"
                            },
                            {
                                "target_lang_word": "casualty",
                                "target_lang_sense_clarification": "a person killed or injured in a war or accident"
                            }
                        ]
                    }
                }
            ]
        }
    ]

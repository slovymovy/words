{
    "word_family": [
        "deern",
        "deerntje"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e87a80fb-de85-3deb-a195-eb0c06782aa1",
                    "sense_definition": "Een jong meisje of jonge vrouw, vaak met een onschuldige of landelijke uitstraling.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "young_female",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaat werd verliefd op een lokale <w>deerne</w> uit het dorp.",
                            "target_lang_translations": {
                                "ru": "Солдат влюбился в местную <w>девицу</w> из деревни."
                            }
                        },
                        {
                            "text": "Hoewel ze eruitziet als een jonge <w>deerne</w>, is ze al bijna zestig.",
                            "target_lang_translations": {
                                "ru": "Хотя она выглядит как молодая <w>девица</w>, ей уже почти шестьдесят."
                            }
                        }
                    ],
                    "synonyms": [
                        "meisje",
                        "meiske",
                        "deern",
                        "griet"
                    ],
                    "common_phrases": [
                        "een jonge deerne",
                        "een mooie deerne"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "wordt in modern Nederlands als ouderwets of literair ervaren"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "komt voor in oostelijke dialecten"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодая девушка или женщина, часто с невинным или деревенским обликом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девица",
                                "target_lang_sense_clarification": "устаревшее или литературное слово для обозначения девушки"
                            },
                            {
                                "target_lang_word": "молодка",
                                "target_lang_sense_clarification": "разговорное, часто о молодой женщине в сельской местности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42b76817-8dc4-32a5-bead-d9542e7704bc",
                    "sense_definition": "Een vrouw of meisje dat in dienst is voor huishoudelijk werk.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "servant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>deerne</w> poetste het zilverwerk in de grote eetkamer.",
                            "target_lang_translations": {
                                "ru": "<w>Служанка</w> чистила столовое серебро в большой столовой."
                            }
                        },
                        {
                            "text": "Zij werkte jarenlang als <w>deerne</w> op de boerderij van de baron.",
                            "target_lang_translations": {
                                "ru": "Она долгие годы работала <w>служанкой</w> на ферме барона."
                            }
                        }
                    ],
                    "synonyms": [
                        "dienstbode",
                        "dienstmeid",
                        "dienstmaagd",
                        "meid"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "verouderde term voor dienstbode"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина или девушка, нанятая для выполнения домашней работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "служанка",
                                "target_lang_sense_clarification": "женщина, работающая по дому"
                            },
                            {
                                "target_lang_word": "девка",
                                "target_lang_sense_clarification": "историческое название служанки (например, сенная девка)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89517bb0-e127-3f1d-8122-58d4790ca5d7",
                    "sense_definition": "Een vrouw die als losbandig of van lichte zeden wordt beschouwd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "pejorative_female",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij waarschuwde zijn zoon voor de omgang met zo'n losbandige <w>deerne</w>.",
                            "target_lang_translations": {
                                "ru": "Он предостерег сына от общения с такой распутной <w>девкой</w>."
                            }
                        },
                        {
                            "text": "In het verhaal werd ze onterecht uitgemaakt voor een <w>deerne</w> van plezier.",
                            "target_lang_translations": {
                                "ru": "В рассказе её несправедливо обозвали продажной <w>девкой</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, считающаяся распутной или легкого поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девка",
                                "target_lang_sense_clarification": "грубое, уничижительное обозначение распутной женщины"
                            },
                            {
                                "target_lang_word": "распутница",
                                "target_lang_sense_clarification": "женщина легкого поведения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
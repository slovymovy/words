            "pos": "noun",
            "senses": [
                {
                    "sense_id": "abba9283-fdba-3307-bffb-f7ca29a987c5",
                    "sense_definition": "Gruby splot nitek lub włókien służący do wiązania, wieszania lub ciągnięcia przedmiotów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz mocno zawiązać ten <w>sznur</w>, żeby paczka się nie otworzyła w transporcie.",
                            "target_lang_translations": {
                                "ru": "Ты должен крепко завязать эту <w>верёвку</w>, чтобы посылка не открылась при транспортировке."
                            }
                        },
                        {
                            "text": "Na strychu znaleźliśmy stary, gruby <w>sznur</w>, który kiedyś służył do wieszania prania.",
                            "target_lang_translations": {
                                "ru": "На чердаке мы нашли старую толстую <w>верёвку</w>, которая когда-то служила для сушки белья."
                            }
                        }
                    ],
                    "synonyms": [
                        "lina",
                        "powróz",
                        "postronek"
                    ],
                    "common_phrases": [
                        "sznur do bielizny",
                        "gruby sznur",
                        "wiązać sznurem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Толстое переплетение нитей или волокон, служащее для связывания, подвешивания или тяги предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верёвка",
                                "target_lang_sense_clarification": "общее название для изделия из крученых нитей"
                            },
                            {
                                "target_lang_word": "шнур",
                                "target_lang_sense_clarification": "тонкая верёвка, часто плетеная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dff8d8a7-da79-36f2-9b6b-d68fdea504fa",
                    "sense_definition": "Długa linia ludzi, pojazdów lub przedmiotów ustawionych jeden za drugim.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Długi <w>sznur</w> samochodów stał w korku na autostradzie przez ponad godzinę.",
                            "target_lang_translations": {
                                "ru": "Длинная <w>вереница</w> автомобилей стояла в пробке на шоссе больше часа."
                            }
                        },
                        {
                            "text": "Przed wejściem do muzeum ustawił się wielki <w>sznur</w> turystów czekających na bilety.",
                            "target_lang_translations": {
                                "ru": "Перед входом в музей выстроилась огромная <w>вереница</w> туристов, ожидающих билеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "rząd",
                        "ciąg",
                        "kolejka"
                    ],
                    "common_phrases": [
                        "sznur samochodów",
                        "sznur ludzi",
                        "sznur aut"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная линия людей, транспортных средств или предметов, расположенных друг за другом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вереница",
                                "target_lang_sense_clarification": "ряд движущихся или стоящих друг за другом объектов"
                            },
                            {
                                "target_lang_word": "цепочка",
                                "target_lang_sense_clarification": "ряд людей или предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac41085a-b1e5-35c9-bf4e-90ee8fca36dc",
                    "sense_definition": "Przewód w izolacji doprowadzający prąd do urządzenia elektrycznego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "material_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj, żeby nie potknąć się o <w>sznur</w> od odkurzacza leżący na podłodze.",
                            "target_lang_translations": {
                                "ru": "Осторожно, не споткнись о <w>шнур</w> от пылесоса, лежащий на полу."
                            }
                        },
                        {
                            "text": "Muszę kupić nowy <w>sznur</w> do żelazka, ponieważ stary się przetarł i jest niebezpieczny.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый <w>шнур</w> для утюга, так как старый перетёрся и стал небезопасным."
                            }
                        }
                    ],
                    "synonyms": [
                        "kabel",
                        "przewód"
                    ],
                    "common_phrases": [
                        "sznur od żelazka",
                        "sznur zasilający",
                        "sznur elektryczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изолированный провод, подводящий ток к электрическому устройству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шнур",
                                "target_lang_sense_clarification": "электрический провод в изоляции"
                            },
                            {
                                "target_lang_word": "провод",
                                "target_lang_sense_clarification": "кабель питания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e136a8d8-ccef-332a-b9c8-e61e34d31c37",
                    "sense_definition": "Ozdobna plecionka noszona na mundurze, zwykle na ramieniu, będąca elementem stroju galowego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oficer miał na mundurze błyszczący, złoty <w>sznur</w> galowy, który dodawał mu powagi.",
                            "target_lang_translations": {
                                "ru": "На мундире офицера был блестящий золотой парадный <w>шнур</w>, который придавал ему важности."
                            }
                        },
                        {
                            "text": "Do munduru paradnego zawsze przypinany jest ozdobny <w>sznur</w> w odpowiednim kolorze.",
                            "target_lang_translations": {
                                "ru": "К парадному мундиру всегда пристёгивается декоративный <w>шнур</w> соответствующего цвета."
                            }
                        }
                    ],
                    "synonyms": [
                        "akselbant"
                    ],
                    "common_phrases": [
                        "sznur galowy",
                        "złoty sznur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративный плетёный элемент на мундире, обычно на плече, часть парадной формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аксельбант",
                                "target_lang_sense_clarification": "наплечный отличительный шнур"
                            },
                            {
                                "target_lang_word": "шнур",
                                "target_lang_sense_clarification": "элемент отделки формы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b374dc1-8523-35b3-9221-9530ce186bab",
                    "sense_definition": "Wąski i długi pas ziemi uprawnej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik powoli orał swój ostatni <w>sznur</w> pola tuż przed zachodem słońca.",
                            "target_lang_translations": {
                                "ru": "Фермер медленно пахал свою последнюю <w>полосу</w> поля прямо перед закатом."
                            }
                        },
                        {
                            "text": "W oddali widać było złocisty <w>sznur</w> zboża gotowego do letnich zbiorów.",
                            "target_lang_translations": {
                                "ru": "Вдали виднелась золотистая <w>полоса</w> зерна, готового к летней уборке."
                            }
                        }
                    ],
                    "synonyms": [
                        "zagon"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane głównie w literaturze lub kontekście historycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкая и длинная полоса пахотной земли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полоса",
                                "target_lang_sense_clarification": "участок земли"
                            },
                            {
                                "target_lang_word": "надел",
                                "target_lang_sense_clarification": "земельный участок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "938c2ce9-61b3-3d0e-a500-a5d401cc0d5d",
                    "sense_definition": "Dawna jednostka miary długości używana historycznie w Polsce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geometra wymierzył działkę, używając starej miary, którą nazywano <w>sznurem</w>.",
                            "target_lang_translations": {
                                "ru": "Землемер измерил участок, используя старую меру, которую называли <w>шнуром</w>."
                            }
                        },
                        {
                            "text": "W starych dokumentach zapisano, że droga miała szerokość jednego <w>sznura</w>.",
                            "target_lang_translations": {
                                "ru": "В старых документах записано, что дорога имела ширину в один <w>шнур</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "jednostka historyczna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная единица измерения длины, исторически использовавшаяся в Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шнур",
                                "target_lang_sense_clarification": "мера длины"
                            }
                        ]
                    }
                }
            ]
        }

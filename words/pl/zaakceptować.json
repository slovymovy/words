                {
                    "sense_id": "d66fbd91-82d3-321a-bebc-dc68d8297191",
                    "sense_definition": "Wyrazić zgodę na propozycję, ofertę lub warunki; uznać coś za odpowiednie i zgodne z oczekiwaniami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "approval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <zaakceptować> nowy regulamin, aby móc dalej korzystać z serwisu.",
                            "target_lang_translations": {
                                "ru": "Ты должен <принять> новые правила, чтобы продолжать пользоваться сервисом."
                            }
                        },
                        {
                            "text": "Komisja postanowiła <zaakceptować> nasz projekt i przyznać nam dofinansowanie.",
                            "target_lang_translations": {
                                "ru": "Комиссия решила <одобрить> наш проект и предоставить нам финансирование."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyjąć",
                        "zgodzić się",
                        "zaaprobować"
                    ],
                    "antonyms": [
                        "odrzucić",
                        "zakwestionować"
                    ],
                    "common_phrases": [
                        "zaakceptować ofertę",
                        "zaakceptować warunki",
                        "zaakceptować zaproszenie",
                        "zaakceptować wniosek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выразить согласие на предложение, условия или утвердить что-либо официально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принять",
                                "target_lang_sense_clarification": "предложение, условия, приглашение"
                            },
                            {
                                "target_lang_word": "одобрить",
                                "target_lang_sense_clarification": "проект, решение, план"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "975a1176-f9a5-362e-9c90-1708f45a3cae",
                    "sense_definition": "Pogodzić się z jakąś sytuacją, faktem lub czyjąś odmiennością; uznać coś za rzeczywiste i przestać z tym walczyć.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "psychological_acceptance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko nie było w stanie <zaakceptować> rozwodu rodziców i bardzo to przeżywało.",
                            "target_lang_translations": {
                                "ru": "Ребенок не был в состоянии <принять> развод родителей и очень переживал из-за этого."
                            }
                        },
                        {
                            "text": "Musisz <zaakceptować> fakt, że nie masz wpływu na decyzje innych ludzi.",
                            "target_lang_translations": {
                                "ru": "Ты должен <принять> тот факт, что не можешь влиять на решения других людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyjąć"
                    ],
                    "antonyms": [
                        "zaprzeczyć"
                    ],
                    "common_phrases": [
                        "zaakceptować siebie",
                        "zaakceptować sytuację",
                        "zaakceptować fakt",
                        "zaakceptować stratę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психологически примириться с ситуацией, фактом или чьей-то индивидуальностью; перестать отрицать реальность."
                    },
                    "translations": {

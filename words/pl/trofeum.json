                },
                {
                    "sense_id": "e18c60ef-7662-3cf1-ab86-be97c26ddd75",
                    "sense_definition": "Zdobycz wojenna, myśliwska lub inny przedmiot stanowiący dowód sukcesu i powód do dumy dla właściciela.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

{
    "word_family": [
        "hełmik",
        "hełmowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b4e969c2-4405-3a61-8f31-a1dbd603215e",
                    "sense_definition": "Twarde nakrycie głowy chroniące przed urazami, noszone przez żołnierzy, strażaków lub pracowników budowlanych",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <hełm> przeszyła strzała.",
                            "target_lang_translations": {
                                "ru": "Его <шлем> пронзила стрела."
                            }
                        },
                        {
                            "text": "Żołnierz poprawił stalowy <hełm> i ruszył do ataku.",
                            "target_lang_translations": {
                                "ru": "Солдат поправил стальной <шлем> и бросился в атаку."
                            }
                        },
                        {
                            "text": "Na placu budowy każdy musi nosić ochronny <hełm>.",
                            "target_lang_translations": {
                                "ru": "На стройке каждый обязан носить защитную <каску>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kask",
                        "szłom"
                    ],
                    "common_phrases": [
                        "hełm rycerski",
                        "hełm strażacki",
                        "hełm stalowy",
                        "założyć hełm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый головной убор для защиты от травм, который носят солдаты, пожарные или строители"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлем",
                                "target_lang_sense_clarification": "исторический, военный, спортивный"
                            },
                            {
                                "target_lang_word": "каска",
                                "target_lang_sense_clarification": "строительная, военная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "318e1d3d-b8d5-30d9-886f-a82a4472c781",
                    "sense_definition": "Ozdobne, często spiczaste lub kopulaste zwieńczenie wieży kościoła, ratusza lub zamku",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten barokowy <hełm> pochodzi z 1713 roku.",
                            "target_lang_translations": {
                                "ru": "Этот барочный <купол> датируется 1713 годом."
                            }
                        },
                        {
                            "text": "Wichura uszkodziła miedziany <hełm> na wieży kościelnej.",
                            "target_lang_translations": {
                                "ru": "Буря повредила медный <купол> на церковной башне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hełm wieży",
                        "barokowy hełm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративное, часто остроконечное или куполообразное завершение башни церкви, ратуши или замка"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "купол",
                                "target_lang_sense_clarification": "архитектурный элемент"
                            },
                            {
                                "target_lang_word": "шлем",
                                "target_lang_sense_clarification": "форма купола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb0dd4b4-305b-31db-85c1-fa4acb125c11",
                    "sense_definition": "Element herbu szlacheckiego umieszczany nad tarczą herbową",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad tarczą herbową znajduje się srebrny <hełm> w koronie.",
                            "target_lang_translations": {
                                "ru": "Над гербовым щитом находится серебряный <шлем> в короне."
                            }
                        },
                        {
                            "text": "W heraldyce <hełm> symbolizuje przynależność do stanu rycerskiego.",
                            "target_lang_translations": {
                                "ru": "В геральдике <шлем> символизирует принадлежность к рыцарскому сословию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hełm heraldyczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент дворянского герба, помещаемый над гербовым щитом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлем",
                                "target_lang_sense_clarification": "геральдический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c295d1c8-9414-3cd9-b6b5-239967804590",
                    "sense_definition": "Fryzura ułożona w wysoki, sztywny kształt przypominający nakrycie głowy",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej natapirowany <hełm> nie poruszył się nawet na silnym wietrze.",
                            "target_lang_translations": {
                                "ru": "Её начесанная <хала> даже не шелохнулась на сильном ветру."
                            }
                        },
                        {
                            "text": "Ciotka nosiła na głowie wielki, lakierowany <hełm>.",
                            "target_lang_translations": {
                                "ru": "Тетка носила на голове огромный лакированный <шлем>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прическа, уложенная в высокую, жесткую форму, напоминающую головной убор"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хала",
                                "target_lang_sense_clarification": "разговорное, о высокой прическе"
                            },
                            {
                                "target_lang_word": "шлем",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "d04cd781-5148-337a-b655-8635b2ebef28",
                    "sense_definition": "Почти незаметный, который трудно увидеть физически.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На старой стене остался лишь <w>малоприметный</w> след от гвоздя.",
                            "target_lang_translations": {
                                "en": "On the old wall, only a <w>faint</w> mark from a nail remained."
                            }
                        },
                        {
                            "text": "В густой траве петляла узкая и <w>малоприметная</w> тропинка.",
                            "target_lang_translations": {
                                "en": "A narrow and <w>inconspicuous</w> path wound through the thick grass."
                            }
                        }
                    ],
                    "synonyms": [
                        "незаметный",
                        "неприметный",
                        "невидный"
                    ],
                    "antonyms": [
                        "заметный",
                        "видный",
                        "броский"
                    ],
                    "common_phrases": [
                        "малоприметный шрам",
                        "малоприметная деталь",
                        "малоприметный вход"
                    ],
                    "target_lang_definitions": {
                        "en": "Almost invisible, difficult to see physically."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inconspicuous",
                                "target_lang_sense_clarification": "not clearly visible or attracting attention"
                            },
                            {
                                "target_lang_word": "barely visible",
                                "target_lang_sense_clarification": "hard to perceive with the eyes"
                            },
                            {
                                "target_lang_word": "faint",
                                "target_lang_sense_clarification": "indistinct, barely perceptible (often used for marks or traces)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cecf9a6-5423-3158-9b73-8b6cbc7bd309",
                    "sense_definition": "Ничем не выделяющийся среди других, не привлекающий к себе внимания своими качествами или внешностью.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это был тихий и <w>малоприметный</w> человек, который всегда сидел в углу.",
                            "target_lang_translations": {
                                "en": "He was a quiet and <w>nondescript</w> man who always sat in the corner."
                            }
                        },
                        {
                            "text": "Она занимала скромную и <w>малоприметную</w> должность в большой компании.",
                            "target_lang_translations": {
                                "en": "She held a modest and <w>unremarkable</w> position in a large company."
                            }
                        }
                    ],
                    "synonyms": [
                        "невзрачный",
                        "неприметный",
                        "заурядный",
                        "обыкновенный"
                    ],
                    "antonyms": [
                        "выдающийся",
                        "яркий",
                        "примечательный"
                    ],
                    "common_phrases": [
                        "малоприметный сотрудник",
                        "малоприметная внешность",
                        "малоприметная роль"
                    ],
                    "target_lang_definitions": {
                        "en": "Not standing out among others; unremarkable in appearance or qualities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unremarkable",
                                "target_lang_sense_clarification": "ordinary, not worthy of special attention"
                            },
                            {
                                "target_lang_word": "nondescript",
                                "target_lang_sense_clarification": "lacking distinctive or interesting features"
                            },
                            {
                                "target_lang_word": "unassuming",
                                "target_lang_sense_clarification": "modest, not pretentious"
                            }
                        ]
                    }
                }
            ]
        }

                {
                    "sense_id": "563ad5f4-7faa-3bae-9299-a3d45fcd320c",
                    "sense_definition": "Behorend tot, afkomstig van of lijkend op de duivel; slecht of kwaadaardig.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "evil_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koning bedacht een <w>duivels</w> plan om de macht in het land te grijpen.",
                            "target_lang_translations": {
                                "ru": "Король придумал <w>дьявольский</w> план, чтобы захватить власть в стране."
                            }
                        },
                        {
                            "text": "In het sprookje had de oude heks een angstaanjagende en <w>duivelse</w> lach.",
                            "target_lang_translations": {
                                "ru": "В сказке у старой ведьмы был пугающий и <w>дьявольский</w> смех."
                            }
                        }
                    ],
                    "synonyms": [
                        "demonisch",
                        "satanisch",
                        "kwaadaardig",
                        "hels"
                    ],
                    "antonyms": [
                        "hemels",
                        "goddelijk",
                        "engelachtig"
                    ],
                    "common_phrases": [
                        "een duivels plan",
                        "duivelse krachten",
                        "duivelse praktijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к дьяволу, исходящий от него или похожий на него; злой или злобный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дьявольский",
                                "target_lang_sense_clarification": "относящийся к дьяволу или злу"

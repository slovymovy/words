    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8fa41f6f-333d-3346-abba-73755d2742dc",
                    "sense_definition": "Inappropriate, awkward, or not well-timed; not suitable for the occasion.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He instantly regretted his <w>infelicitous</w> remark about her appearance.",
                            "target_lang_translations": {
                                "ru": "Он тут же пожалел о своём <w>неудачном</w> замечании по поводу её внешности."
                            }
                        },
                        {
                            "text": "It was an <w>infelicitous</w> choice of words for such a solemn ceremony.",
                            "target_lang_translations": {
                                "ru": "Это был <w>неуместный</w> выбор слов для столь торжественной церемонии."
                            }
                        }
                    ],
                    "synonyms": [
                        "unlucky",
                        "lamentable"
                    ],
                    "antonyms": [
                        "felicitous",
                        "appropriate"
                    ],
                    "common_phrases": [
                        "infelicitous remark",
                        "infelicitous choice",
                        "infelicitous timing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неуместный, неловкий или неудачно подобранный для данного случая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неуместный",
                                "target_lang_sense_clarification": "не соответствующий обстановке или случаю"
                            },
                            {
                                "target_lang_word": "неудачный",
                                "target_lang_sense_clarification": "плохо подобранный, неловкий (о словах или действиях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9811a698-1627-3c9f-8d5b-c8c2ca8ef7be",
                    "sense_definition": "Unfortunate, unlucky, or producing unhappiness.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The project suffered from an <w>infelicitous</w> start due to the bad weather.",
                            "target_lang_translations": {
                                "ru": "Проект пострадал от <w>неудачного</w> старта из-за плохой погоды."
                            }
                        },
                        {
                            "text": "A series of <w>infelicitous</w> events led to the company's closure.",
                            "target_lang_translations": {
                                "ru": "Череда <w>несчастливых</w> событий привела к закрытию компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "unlucky",
                        "lamentable",
                        "sad"
                    ],
                    "antonyms": [
                        "fortunate",
                        "happy"
                    ],
                    "common_phrases": [
                        "infelicitous outcome",
                        "infelicitous circumstance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несчастливый, неудачный, приносящий огорчение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неудачный",
                                "target_lang_sense_clarification": "не имеющий успеха, сопровождающийся неудачей"
                            },
                            {
                                "target_lang_word": "несчастливый",
                                "target_lang_sense_clarification": "связанный с невезением или бедой"
                            },
                            {
                                "target_lang_word": "злополучный",
                                "target_lang_sense_clarification": "приносящий несчастье, роковой (книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1dabcd3f-c15a-30a7-91da-345dc6706619",
                    "sense_definition": "Describing a sentence or utterance that is grammatically correct but not semantically or pragmatically coherent.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The student's response was grammatically sound but pragmatically <w>infelicitous</w>.",
                            "target_lang_translations": {
                                "ru": "Ответ студента был грамматически правильным, но прагматически <w>неуместным</w>."
                            }
                        },
                        {
                            "text": "Linguists analyze why certain statements are <w>infelicitous</w> in specific contexts.",
                            "target_lang_translations": {
                                "ru": "Лингвисты анализируют, почему определенные утверждения являются <w>неуместными</w> в конкретных контекстах."
                            }
                        }
                    ],
                    "antonyms": [
                        "felicitous"
                    ],
                    "common_phrases": [
                        "infelicitous utterance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывает высказывание, которое грамматически верно, но прагматически некорректно или неуместно в данном контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неуместный",
                                "target_lang_sense_clarification": "нарушающий условия успешности речевого акта"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b0ceb007-58e8-3943-b8a6-8bf59546f8b3",
                    "sense_definition": "A period of time in which students are taught a specific subject.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was late for my history <w>class</w> this morning.",
                            "target_lang_translations": {
                                "ru": "Я опоздал на <w>урок</w> истории сегодня утром."
                            }
                        },
                        {
                            "text": "You should not talk while you are in <w>class</w>.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует разговаривать на <w>уроке</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in class",
                        "after class",
                        "go to class",
                        "take a class"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени, в течение которого студентов обучают определенному предмету; урок или занятие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "урок",
                                "target_lang_sense_clarification": "в школе"
                            },
                            {
                                "target_lang_word": "занятие",
                                "target_lang_sense_clarification": "в университете или на курсах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0716339-063e-329c-a989-8ae2ddeb7073",
                    "sense_definition": "A group of students who are taught together.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The whole <w>class</w> laughed at the teacher's joke.",
                            "target_lang_translations": {
                                "ru": "Весь <w>класс</w> смеялся над шуткой учителя."
                            }
                        },
                        {
                            "text": "He is the smartest student in the <w>class</w>.",
                            "target_lang_translations": {
                                "ru": "Он самый умный ученик в <w>классе</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "whole class",
                        "top of the class"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа учеников или студентов, которые обучаются вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класс",
                                "target_lang_sense_clarification": "группа учеников в школе"
                            },
                            {
                                "target_lang_word": "группа",
                                "target_lang_sense_clarification": "студенты в университете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc5bef90-2967-3f90-8046-77adcbea2651",
                    "sense_definition": "A standard of comfort and service on a plane, train, or ship.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We traveled in economy <w>class</w> to save money.",
                            "target_lang_translations": {
                                "ru": "Мы путешествовали эконом-<w>классом</w>, чтобы сэкономить деньги."
                            }
                        },
                        {
                            "text": "They cannot afford to fly first <w>class</w>.",
                            "target_lang_translations": {
                                "ru": "Они не могут позволить себе летать первым <w>классом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "economy class",
                        "business class",
                        "first class"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уровень комфорта и обслуживания в самолете, поезде или на корабле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класс",
                                "target_lang_sense_clarification": "разряд места в транспорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fcb9e29-ec10-3055-a200-fa49b9f5aeec",
                    "sense_definition": "A group of people in a society who share the same social and economic status.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "grouping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The working <w>class</w> often struggles with rising living costs.",
                            "target_lang_translations": {
                                "ru": "Рабочий <w>класс</w> часто страдает от роста стоимости жизни."
                            }
                        },
                        {
                            "text": "She was born into a wealthy upper-<w>class</w> family.",
                            "target_lang_translations": {
                                "ru": "Она родилась в богатой семье из высшего <w>класса</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "working class",
                        "middle class",
                        "upper class",
                        "social class"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей в обществе, имеющих одинаковый социальный и экономический статус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класс",
                                "target_lang_sense_clarification": "социальная группа"
                            },
                            {
                                "target_lang_word": "слой",
                                "target_lang_sense_clarification": "социальная прослойка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfa24503-022f-3a37-9ac4-0ce3df95ca54",
                    "sense_definition": "A category or group of people or things that share similar characteristics.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "grouping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This luxury hotel is in a <w>class</w> of its own.",
                            "target_lang_translations": {
                                "ru": "Этот роскошный отель — единственный в своём <w>классе</w>."
                            }
                        },
                        {
                            "text": "The new car model is the best in its <w>class</w>.",
                            "target_lang_translations": {
                                "ru": "Новая модель автомобиля — лучшая в своём <w>классе</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "category",
                        "type",
                        "kind",
                        "sort",
                        "set"
                    ],
                    "common_phrases": [
                        "world-class",
                        "top class",
                        "in a class of its own"
                    ],
                    "target_lang_definitions": {
                        "ru": "Категория или группа людей или вещей, обладающих схожими характеристиками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класс",
                                "target_lang_sense_clarification": "категория качества"
                            },
                            {
                                "target_lang_word": "категория",
                                "target_lang_sense_clarification": "разряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0507aa37-87c4-3ab1-a945-4c9691086c6f",
                    "sense_definition": "A high standard of style, skill, or behavior; elegance.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She accepted the award with real <w>class</w> and humility.",
                            "target_lang_translations": {
                                "ru": "Она приняла награду с настоящим <w>благородством</w> и смирением."
                            }
                        },
                        {
                            "text": "The player showed his <w>class</w> by scoring a brilliant goal.",
                            "target_lang_translations": {
                                "ru": "Игрок показал свой <w>класс</w>, забив блестящий гол."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have class",
                        "real class",
                        "touch of class"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокий стандарт стиля, мастерства или поведения; элегантность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класс",
                                "target_lang_sense_clarification": "высокое мастерство (разг.)"
                            },
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "качество"
                            },
                            {
                                "target_lang_word": "шик",
                                "target_lang_sense_clarification": "элегантность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8da84125-2dbb-34d0-8577-77e5ce36d46d",
                    "sense_definition": "A rank in the classification of living things, below phylum and above order.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "grouping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mammalia is a <w>class</w> of warm-blooded vertebrates.",
                            "target_lang_translations": {
                                "ru": "Млекопитающие — это <w>класс</w> теплокровных позвоночных."
                            }
                        },
                        {
                            "text": "Birds belong to the biological <w>class</w> Aves.",
                            "target_lang_translations": {
                                "ru": "Птицы принадлежат к биологическому <w>классу</w> Aves."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ранг в классификации живых организмов, ниже типа и выше отряда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класс",
                                "target_lang_sense_clarification": "в биологии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e4dbce4-c04e-3309-8590-21cbbb8f9028",
                    "sense_definition": "A set of instructions in computer programming that defines a specific object.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "grouping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to define the <w>class</w> before creating an instance of it.",
                            "target_lang_translations": {
                                "ru": "Вам нужно определить <w>класс</w> перед созданием его экземпляра."
                            }
                        },
                        {
                            "text": "This <w>class</w> inherits properties from the parent object.",
                            "target_lang_translations": {
                                "ru": "Этот <w>класс</w> наследует свойства от родительского объекта."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор инструкций в программировании, определяющий конкретный объект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класс",
                                "target_lang_sense_clarification": "в программировании"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "71e07af2-14c3-39f2-b3e8-af39c5f27419",
                    "sense_definition": "To consider someone or something as belonging to a particular group or category.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I would <w>class</w> him as one of the best actors of his generation.",
                            "target_lang_translations": {
                                "ru": "Я бы <w>отнёс</w> его к числу лучших актёров его поколения."
                            }
                        },
                        {
                            "text": "The tomato is botanically <w>classed</w> as a fruit, not a vegetable.",
                            "target_lang_translations": {
                                "ru": "Ботанически помидор <w>классифицируется</w> как фрукт, а не овощ."
                            }
                        }
                    ],
                    "common_phrases": [
                        "class as",
                        "be classed as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать кого-то или что-то принадлежащим к определенной группе или категории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "классифицировать",
                                "target_lang_sense_clarification": "распределять по группам"
                            },
                            {
                                "target_lang_word": "относить",
                                "target_lang_sense_clarification": "причислять к категории"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cbd49c43-5cf6-3df0-b7f3-0a4bc63ea00c",
                    "sense_definition": "Very good, stylish, or excellent.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That was a <w>class</w> goal by the striker.",
                            "target_lang_translations": {
                                "ru": "Это был <w>классный</w> гол нападающего."
                            }
                        },
                        {
                            "text": "He is a <w>class</w> player who always performs well.",
                            "target_lang_translations": {
                                "ru": "Он <w>классный</w> игрок, который всегда выступает хорошо."
                            }
                        }
                    ],
                    "common_phrases": [
                        "class act"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень хороший, стильный или превосходный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "классный",
                                "target_lang_sense_clarification": "отличный (разг.)"
                            },
                            {
                                "target_lang_word": "первоклассный",
                                "target_lang_sense_clarification": "высокого качества"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
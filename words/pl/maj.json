    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1e4edef2-e075-3a35-8ee2-52450de2d29a",
                    "sense_definition": "piąty miesiąc w roku według kalendarza gregoriańskiego, trwający 31 dni, następujący po kwietniu",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trzeci <w>maja</w> to ważne polskie święto narodowe.",
                            "target_lang_translations": {
                                "ru": "Третье <w>мая</w> — это важный польский национальный праздник."
                            }
                        },
                        {
                            "text": "W tym roku <w>maj</w> jest wyjątkowo ciepły i słoneczny.",
                            "target_lang_translations": {
                                "ru": "В этом году <w>май</w> исключительно тёплый и солнечный."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pierwszy maja",
                        "trzeci maja",
                        "w maju",
                        "początek maja"
                    ],
                    "target_lang_definitions": {
                        "ru": "пятый месяц года по григорианскому календарю, длящийся 31 день, следующий за апрелем"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "май",
                                "target_lang_sense_clarification": "месяц"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d39bfd27-8fd6-35be-ab6b-9c40e6d91377",
                    "sense_definition": "przedstawiciel grupy ludów indiańskich zamieszkujących Amerykę Środkową, twórca cywilizacji Majów",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "ethnicity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starożytni <w>Majowie</w> stworzyli bardzo precyzyjny kalendarz.",
                            "target_lang_translations": {
                                "ru": "Древние <w>майя</w> создали очень точный календарь."
                            }
                        },
                        {
                            "text": "Ten współczesny <w>Maj</w> opowiedział nam o tradycjach swoich przodków.",
                            "target_lang_translations": {
                                "ru": "Этот современный <w>майя</w> рассказал нам о традициях своих предков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kalendarz Majów",
                        "cywilizacja Majów",
                        "kultura Majów"
                    ],
                    "target_lang_definitions": {
                        "ru": "представитель группы индейских народов, населяющих Центральную Америку, создатель цивилизации майя"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "майя",
                                "target_lang_sense_clarification": "народ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b59dcc5b-8a55-3201-aeba-8e6899c5c7df",
                    "sense_definition": "ludowa lub regionalna nazwa mniszka lekarskiego",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "plants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała łąka pożółkła, gdy zakwitł na niej <w>maj</w>.",
                            "target_lang_translations": {
                                "ru": "Весь луг пожелтел, когда на нём зацвёл <w>одуванчик</w>."
                            }
                        },
                        {
                            "text": "Dzieci zbierały <w>maje</w>, aby upleść z nich wiosenne wianki.",
                            "target_lang_translations": {
                                "ru": "Дети собирали <w>одуванчики</w>, чтобы сплести из них весенние венки."
                            }
                        }
                    ],
                    "synonyms": [
                        "mlecz",
                        "dmuchawiec"
                    ],
                    "traits": [
                        {

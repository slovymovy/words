                            {
                                "target_lang_word": "occasionally",
                                "target_lang_sense_clarification": "happening at infrequent or irregular intervals"
                            },
                            {
                                "target_lang_word": "from time to time",
                                "target_lang_sense_clarification": "occurring at intervals; not regularly"
                            },
                            {
                                "target_lang_word": "now and then",
                                "target_lang_sense_clarification": "sometimes; intermittently"

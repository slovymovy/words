                    "sense_id": "9c9f1593-be59-38cb-9b78-8465068d8c4d",
                    "sense_definition": "Iets wat je kunt horen; trillingen die zich door de lucht verplaatsen en door het oor worden opgevangen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sound_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik hoorde een vreemd <w>geluid</w> in de keuken.",
                            "target_lang_translations": {
                                "en": "I heard a strange <w>sound</w> in the kitchen.",
                                "ru": "Я услышал странный <w>звук</w> на кухне."
                            }
                        },
                        {
                            "text": "Het <w>geluid</w> van de vogels maakte hem wakker.",
                            "target_lang_translations": {
                                "en": "The <w>sound</w> of the birds woke him up.",
                                "ru": "<w>Звуки</w> птиц разбудили его."
                            }
                        },
                        {
                            "text": "Zet het <w>geluid</w> van de televisie wat zachter.",
                            "target_lang_translations": {
                                "en": "Turn the <w>sound</w> of the television down a bit.",
                                "ru": "Сделай <w>звук</w> телевизора потише."
                            }
                        }
                    ],
                    "synonyms": [
                        "luid",
                        "luid"
                    ],
                    "common_phrases": [
                        "hard geluid",
                        "vreemd geluid",
                        "geluid maken"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that is audible; vibrations traveling through the air and perceived by the ear.",
                        "ru": "То, что можно услышать; колебания, распространяющиеся по воздуху и воспринимаемые ухом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sound",
                                "target_lang_sense_clarification": "physical sensation perceived by the ear"
                            },
                            {
                                "target_lang_word": "noise",
                                "target_lang_sense_clarification": "unspecified, often loud or unpleasant sound"
                            }
                        ],
                        "ru": [
                            {

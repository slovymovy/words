{
    "word_family": [
        "durf",
        "aandurven",
        "gedurfd"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "50eeb443-9a84-302d-ad22-9ddc15c1468b",
                    "sense_definition": "De moed hebben om iets te doen wat eng, gevaarlijk of moeilijk kan zijn.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>durf</w> niet in het donker naar huis te lopen.",
                            "target_lang_translations": {
                                "ru": "Я не <w>осмеливаюсь</w> идти домой в темноте.",
                                "en": "I do not <w>dare</w> to walk home in the dark."
                            }
                        },
                        {
                            "text": "Zij <w>durfde</w> de moeilijke vraag eindelijk te stellen.",
                            "target_lang_translations": {
                                "ru": "Она наконец <w>решилась</w> задать этот трудный вопрос.",
                                "en": "She finally <w>dared</w> to ask the difficult question."
                            }
                        }
                    ],
                    "synonyms": [
                        "wagen"
                    ],
                    "antonyms": [
                        "vrezen",
                        "bang zijn"
                    ],
                    "common_phrases": [
                        "niet durven kijken",
                        "alles durven",
                        "niet durven vragen",
                        "het aandurven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь мужество сделать что-то, что может быть страшным, опасным или трудным.",
                        "en": "To have the courage to do something that might be scary, dangerous, or difficult."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осмеливаться",
                                "target_lang_sense_clarification": "иметь мужество"
                            },
                            {
                                "target_lang_word": "решаться",
                                "target_lang_sense_clarification": "преодолевать страх перед действием"
                            },
                            {
                                "target_lang_word": "сметь",
                                "target_lang_sense_clarification": "иметь смелость (часто в отрицании)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "dare",
                                "target_lang_sense_clarification": "to have the courage"
                            },
                            {
                                "target_lang_word": "have the courage",
                                "target_lang_sense_clarification": "to be brave enough"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07ad96be-a367-3ec5-9b48-63f0beb9cd29",
                    "sense_definition": "Brutaliteit of lef tonen in een sociale context.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je <w>durft</w> wel om zoiets tegen je baas te zeggen!",
                            "target_lang_translations": {
                                "ru": "Ты <w>смеешь</w> говорить такое своему начальнику!",
                                "en": "You certainly <w>have the nerve</w> to say something like that to your boss!"
                            }
                        },
                        {
                            "text": "Hij heeft echt lef dat hij dat zomaar <w>durft</w>.",
                            "target_lang_translations": {
                                "ru": "У него действительно есть наглость, раз он просто так <w>смеет</w> это делать.",
                                "en": "He really has guts that he just <w>dares</w> to do that."
                            }
                        }
                    ],
                    "common_phrases": [
                        "je durft wel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявлять дерзость, наглость или смелость в социальном контексте.",
                        "en": "To show boldness, audacity, or nerve in a social context."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сметь",
                                "target_lang_sense_clarification": "проявлять наглость"
                            },
                            {
                                "target_lang_word": "позволять себе",
                                "target_lang_sense_clarification": "вести себя вызывающе"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "dare",
                                "target_lang_sense_clarification": "to be bold enough"
                            },
                            {
                                "target_lang_word": "have the nerve",
                                "target_lang_sense_clarification": "to show audacity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5815c8d4-ac4d-3083-b7c9-4e431e4a2cd5",
                    "sense_definition": "De neiging hebben om voor te komen of te gebeuren (meestal bij iets negatiefs).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>durft</w> in de herfst weleens flink te stormen.",
                            "target_lang_translations": {
                                "ru": "Осенью <w>случается</w> сильный шторм.",
                                "en": "It <w>tends</w> to storm quite a bit in autumn."
                            }
                        },
                        {
                            "text": "Die oude auto <w>durft</w> nogal eens stil te vallen.",
                            "target_lang_translations": {
                                "ru": "Эта старая машина <w>имеет обыкновение</w> часто глохнуть.",
                                "en": "That old car <w>tends</w> to break down quite often."
                            }
                        }
                    ],
                    "common_phrases": [
                        "durft weleens te gebeuren"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Zuid-Nederlands (België)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь склонность происходить или случаться (обычно о чем-то негативном).",
                        "en": "To have a tendency to occur or happen (usually referring to something negative)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "случаться",
                                "target_lang_sense_clarification": "происходить время от времени"
                            },
                            {
                                "target_lang_word": "иметь обыкновение",
                                "target_lang_sense_clarification": "проявлять склонность"
                            },
                            {
                                "target_lang_word": "бывать",
                                "target_lang_sense_clarification": "происходить иногда"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "tend to",
                                "target_lang_sense_clarification": "to be inclined to happen"
                            },
                            {
                                "target_lang_word": "be liable to",
                                "target_lang_sense_clarification": "to be likely to occur"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
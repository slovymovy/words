                {
                    "sense_id": "7684b7a6-c25e-38da-8983-ced91ea8832a",
                    "sense_definition": "Niewielki ptak wędrowny z rodziny jaskółkowatych, gnieżdżący się w norach wygrzebanych w urwistych brzegach rzek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "birds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad rzeką latała zwinna <w>brzegówka</w>, łapiąc owady w locie.",
                            "target_lang_translations": {
                                "ru": "Над рекой летала юркая <w>береговушка</w>, ловя насекомых на лету."
                            }
                        },
                        {
                            "text": "W piaszczystym urwisku zauważyliśmy liczne nory, które zamieszkuje <w>brzegówka</w>.",
                            "target_lang_translations": {
                                "ru": "В песчаном обрыве мы заметили многочисленные норы, в которых обитает <w>береговая ласточка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "jaskółka brzegówka"
                    ],
                    "common_phrases": [
                        "kolonia brzegówek",
                        "gniazdo brzegówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая перелетная птица семейства ласточковых, гнездящаяся в норах, вырытых в обрывистых берегах рек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "береговушка",
                                "target_lang_sense_clarification": "вид ласточки"
                            },
                            {
                                "target_lang_word": "береговая ласточка",
                                "target_lang_sense_clarification": "полное название птицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdf9bc4e-be5c-3df6-9460-cabbf429cf11",
                    "sense_definition": "Drobny ślimak morski z rodzaju Littorina, żyjący w strefie pływów na wybrzeżach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mollusks",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas odpływu na wilgotnych skałach widoczna była mała <w>brzegówka</w>.",
                            "target_lang_translations": {
                                "ru": "Во время отлива на влажных скалах была видна маленькая <w>литторина</w>."
                            }
                        },
                        {
                            "text": "Każda <w>brzegówka</w> potrafi przetrwać krótki czas bez wody dzięki szczelnej muszli.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>литторина</w> способна пережить короткое время без воды благодаря плотной раковине."
                            }
                        }
                    ],
                    "synonyms": [
                        "pobrzeżka",
                        "litorina"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкая морская улитка рода Littorina, обитающая в приливной зоне побережий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "литторина",
                                "target_lang_sense_clarification": "моллюск"
                            },
                            {
                                "target_lang_word": "береговая улитка",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                }
            ]
        }

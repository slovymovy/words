            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "23bde9f7-245c-3d26-85b7-083939982ae0",
                    "sense_definition": "Taki, który zdaje sobie sprawę z istnienia czegoś, posiada wiedzę na dany temat lub rozumie sytuację.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "awareness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niektórzy ludzie nie są <w>świadomi</w> zagrożeń związanych z paleniem tytoniu.",
                            "target_lang_translations": {
                                "ru": "Некоторые люди не <w>осознают</w> угроз, связанных с курением табака."
                            }
                        },
                        {
                            "text": "Jestem w pełni <w>świadomy</w> konsekwencji, jakie niesie za sobą ta decyzja.",
                            "target_lang_translations": {
                                "ru": "Я полностью <w>осознаю</w> последствия, которые влечет за собой это решение."
                            }
                        }
                    ],
                    "antonyms": [
                        "nieświadomy"
                    ],
                    "common_phrases": [
                        "być świadomym czegoś",
                        "w pełni świadomy",
                        "świadomy zagrożenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осознающий существование чего-либо, обладающий знанием на данную тему или понимающий ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осознающий",
                                "target_lang_sense_clarification": "понимающий, отдающий себе отчет"
                            },
                            {
                                "target_lang_word": "осведомлённый",
                                "target_lang_sense_clarification": "информированный, знающий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59d3936e-a98a-3575-b392-47bba063c9ce",
                    "sense_definition": "Taki, który jest wynikiem celowego, przemyślanego działania; zrobiony specjalnie, a nie przypadkowo.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "intentionality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To był mój <w>świadomy</w> wybór, którego wcale nie żałuję.",
                            "target_lang_translations": {
                                "ru": "Это был мой <w>осознанный</w> выбор, о котором я совсем не жалею."
                            }
                        },
                        {
                            "text": "Jego działanie było <w>świadome</w> i miało na celu wprowadzenie nas w błąd.",
                            "target_lang_translations": {
                                "ru": "Его действие было <w>сознательным</w> и имело целью ввести нас в заблуждение."
                            }
                        }
                    ],
                    "synonyms": [
                        "celowy",
                        "zamierzony",
                        "umyślny"
                    ],
                    "antonyms": [
                        "nieświadomy"
                    ],
                    "common_phrases": [
                        "świadomy wybór",
                        "świadoma decyzja",
                        "świadome działanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Являющийся результатом намеренного, продуманного действия; сделанный специально, а не случайно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осознанный",
                                "target_lang_sense_clarification": "осмысленный, продуманный"
                            },
                            {
                                "target_lang_word": "сознательный",
                                "target_lang_sense_clarification": "намеренный, умышленный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cd411ce-94b6-3833-9a85-3a59d2cdeaa0",
                    "sense_definition": "Taki, który ma zdolność myślenia, odczuwania i postrzegania rzeczywistości; zachowujący przytomność umysłu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consciousness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zwierzęta są jednostkami czującymi, myślącymi i <w>świadomymi</w>.",
                            "target_lang_translations": {
                                "ru": "Животные — это чувствующие, мыслящие и <w>сознательные</w> существа."
                            }
                        },
                        {
                            "text": "Po wypadku kierowca był <w>świadomy</w> i logicznie odpowiadał na pytania ratowników.",
                            "target_lang_translations": {
                                "ru": "После аварии водитель был <w>в сознании</w> и логично отвечал на вопросы спасателей."
                            }
                        }
                    ],
                    "antonyms": [
                        "nieświadomy"
                    ],
                    "common_phrases": [
                        "istota świadoma",
                        "pacjent świadomy",
                        "człowiek świadomy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный мыслить, чувствовать и воспринимать действительность; сохраняющий ясность ума."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сознательный",
                                "target_lang_sense_clarification": "обладающий сознанием, разумом"
                            },
                            {
                                "target_lang_word": "находящийся в сознании",
                                "target_lang_sense_clarification": "медицинское состояние, не в обмороке"
                            }
                        ]
                    }
                }
            ]
        }
    ]

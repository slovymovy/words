                    ],
                    "common_phrases": [
                        "править страной",
                        "править миром",
                        "править бал"
                    ],
                    "target_lang_definitions": {
                        "en": "To manage a state, region, or organization; to possess power."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rule",
                                "target_lang_sense_clarification": "to exercise authority or dominion over"
                            },
                            {
                                "target_lang_word": "govern",
                                "target_lang_sense_clarification": "to administer the public policy and affairs of a state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5671ee3e-11da-3f63-82c7-724fc041038e",
                    "sense_definition": "Исправлять ошибки, вносить изменения в текст или работу для улучшения.",

        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d9847280-2988-3275-acd0-9cc743162dad",
                    "sense_definition": "Potoczne słowo używane do potwierdzenia czegoś, wyrażenia zgody lub akceptacji; odpowiednik słowa \"tak\".",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "confirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy to twoja książka? <w>Ano</w> moja, dziękuję za znalezienie."
                        },
                        {
                            "text": "Pójdziesz z nami do kina? <w>Ano</w> pójdę, chętnie obejrzę ten film."
                        }
                    ],
                    "synonyms": [
                        "tak",
                        "no"
                    ],
                    "antonyms": [
                        "nie"
                    ],
                    "common_phrases": [
                        "ano tak",
                        "ano mam",
                        "ano wiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ]
                },
                {
                    "sense_id": "76e7b645-ff78-361c-a402-e1d022a7bdca",
                    "sense_definition": "Słowo wprowadzające wyjaśnienie, odpowiedź na pytanie (często retoryczne) lub nawiązujące do poprzedniej wypowiedzi.",

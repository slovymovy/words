                {
                    "sense_id": "406974f8-8844-3a4b-bf9e-828c71065561",
                    "sense_definition": "Een inwoner of staatsburger van <w>België</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buurman is een <w>Belg</w> die al jaren in Nederland woont.",
                            "target_lang_translations": {
                                "en": "My neighbor is a <w>Belgian</w> who has lived in the Netherlands for years.",
                                "ru": "Мой сосед — <w>бельгиец</w>, который уже много лет живет в Нидерландах."
                            }
                        },
                        {
                            "text": "Als trotse <w>Belg</w> supportert hij altijd voor de Rode Duivels.",
                            "target_lang_translations": {
                                "en": "As a proud <w>Belgian</w>, he always supports the Red Devils.",
                                "ru": "Как гордый <w>бельгиец</w>, он всегда болеет за «Красных дьяволов»."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de gemiddelde Belg",
                        "een bekende Belg"
                    ],
                    "target_lang_definitions": {
                        "en": "An inhabitant or citizen of Belgium.",
                        "ru": "Житель или гражданин Бельгии."
                    },
                    "translations": {

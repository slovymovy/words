    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ecf08f3-c10f-34f4-979a-feb8280587b0",
                    "sense_definition": "A state free of war, in particular war between different countries.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "absence_of_conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two nations signed a treaty to restore <w>peace</w> after years of fighting.",
                            "target_lang_translations": {
                                "ru": "Две страны подписали договор о восстановлении <w>мира</w> после многих лет войны."
                            }
                        },
                        {
                            "text": "We all hope to live in a world full of <w>peace</w> and safety.",
                            "target_lang_translations": {
                                "ru": "Мы все надеемся жить в мире, полном <w>мира</w> и безопасности."
                            }
                        }
                    ],
                    "antonyms": [
                        "war",
                        "violence"
                    ],
                    "common_phrases": [
                        "world peace",
                        "make peace",
                        "peace treaty",
                        "peace process"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние без войны, в частности войны между разными странами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мир",
                                "target_lang_sense_clarification": "отсутствие войны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c555cc7-693b-37f8-9e90-14d459a301a9",
                    "sense_definition": "A state of tranquility, quiet, and harmony, often free from civil disturbance.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tranquility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I went to the countryside to find some <w>peace</w> and quiet away from the city.",
                            "target_lang_translations": {
                                "ru": "Я поехал в деревню, чтобы найти <w>покой</w> и тишину вдали от города."
                            }
                        },
                        {
                            "text": "The library is a place of <w>peace</w> where students can focus on their work.",
                            "target_lang_translations": {
                                "ru": "Библиотека — это место <w>тишины</w>, где студенты могут сосредоточиться на работе."
                            }
                        }
                    ],
                    "antonyms": [
                        "noise",
                        "disruption"
                    ],
                    "common_phrases": [
                        "peace and quiet",
                        "at peace",
                        "breach of the peace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние спокойствия, тишины и гармонии, часто свободное от гражданских волнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покой",
                                "target_lang_sense_clarification": "состояние тишины и отдыха"
                            },
                            {
                                "target_lang_word": "спокойствие",
                                "target_lang_sense_clarification": "отсутствие волнения или шума"
                            },
                            {
                                "target_lang_word": "тишина",
                                "target_lang_sense_clarification": "отсутствие звуков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb25c660-f562-3074-b4df-299c5f63cf8a",
                    "sense_definition": "A state free of oppressive and unpleasant thoughts and emotions.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Having good insurance gives me <w>peace</w> of mind regarding my health.",
                            "target_lang_translations": {
                                "ru": "Наличие хорошей страховки дает мне <w>душевное спокойствие</w> по поводу моего здоровья."
                            }
                        },
                        {
                            "text": "She finally found <w>peace</w> after forgiving him for his mistakes.",
                            "target_lang_translations": {
                                "ru": "Она наконец обрела <w>покой</w> после того, как простила его за ошибки."
                            }
                        }
                    ],
                    "antonyms": [
                        "irritation"
                    ],
                    "common_phrases": [
                        "peace of mind",
                        "find peace",
                        "inner peace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, свободное от гнетущих и неприятных мыслей и эмоций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "душевный покой",
                                "target_lang_sense_clarification": "внутренняя гармония"
                            },
                            {
                                "target_lang_word": "умиротворение",
                                "target_lang_sense_clarification": "чувство глубокого спокойствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32bf9828-e3ee-36fd-8618-4a74bbbd9d56",
                    "sense_definition": "Harmony in personal relations.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tranquility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family tried to keep the <w>peace</w> during the holiday dinner.",
                            "target_lang_translations": {
                                "ru": "Семья старалась сохранять <w>мир</w> во время праздничного ужина."
                            }
                        },
                        {
                            "text": "It is important to maintain <w>peace</w> with your neighbors to avoid conflicts.",
                            "target_lang_translations": {
                                "ru": "Важно поддерживать <w>мир</w> с соседями, чтобы избежать конфликтов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "keep the peace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гармония в личных отношениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мир",
                                "target_lang_sense_clarification": "согласие в отношениях"
                            },
                            {
                                "target_lang_word": "согласие",
                                "target_lang_sense_clarification": "отсутствие конфликтов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dcd242c1-ec49-38fd-9484-8aa945c268b0",
                    "sense_definition": "Death.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After a long illness, he finally sank to <w>peace</w> surrounded by his family.",
                            "target_lang_translations": {
                                "ru": "После долгой болезни он наконец ушел на <w>покой</w> в окружении своей семьи."
                            }
                        },
                        {
                            "text": "May the souls of the departed rest in <w>peace</w>.",
                            "target_lang_translations": {
                                "ru": "Пусть души усопших покоятся с <w>миром</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rest in peace"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often euphemistic or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Смерть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покой",
                                "target_lang_sense_clarification": "вечный отдых"
                            },
                            {
                                "target_lang_word": "мир",
                                "target_lang_sense_clarification": "в фразе 'покойся с миром'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "aa5abdb6-bc3d-304c-9c0e-62e484c4e741",
                    "sense_definition": "Used to say goodbye.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "farewell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am going home now, so <w>peace</w> out!",
                            "target_lang_translations": {
                                "ru": "Я иду домой, так что <w>пока</w>!"
                            }
                        },
                        {
                            "text": "See you guys later, <w>peace</w>!",
                            "target_lang_translations": {
                                "ru": "Увидимся позже, ребята, <w>бывай</w>!"
                            }
                        }

{
    "word_family": [
        "gulgot",
        "zagulgotać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8254a584-8fb5-30f5-8ec4-237b5a12dc22",
                    "sense_definition": "Wydawać głuchy, bulgoczący dźwięk, powstający zwykle przy przepływie cieczy lub gazu, przypominający pękanie pęcherzyków powietrza.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda w starych rurach zaczęła głośno <gulgotać>.",
                            "target_lang_translations": {
                                "ru": "Вода в старых трубах начала громко <булькать>."
                            }
                        },
                        {
                            "text": "Byłem tak głodny, że w brzuchu zaczęło mi nieprzyjemnie <gulgotać>.",
                            "target_lang_translations": {
                                "ru": "Я был так голоден, что у меня в животе начало неприятно <урчать>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bulgotać"
                    ],
                    "common_phrases": [
                        "woda gulgocze",
                        "gulgotać w brzuchu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать глухой, булькающий звук, возникающий обычно при течении жидкости или газа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "булькать",
                                "target_lang_sense_clarification": "о жидкости"
                            },
                            {
                                "target_lang_word": "урчать",
                                "target_lang_sense_clarification": "о звуках в животе"
                            },
                            {
                                "target_lang_word": "клокотать",
                                "target_lang_sense_clarification": "бурлить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1df6c92e-a6e7-3ded-b914-fbd1b1efb7b3",
                    "sense_definition": "Wydawać charakterystyczny, gardłowy głos, typowy dla indyka.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na środku podwórka dumnie <gulgotał> wielki indyk.",
                            "target_lang_translations": {
                                "ru": "Посреди двора гордо <кулдыкал> большой индюк."
                            }
                        },
                        {
                            "text": "Ptaki zaczęły głośno <gulgotać>, gdy tylko nas zobaczyły.",
                            "target_lang_translations": {
                                "ru": "Птицы начали громко <кулдыкать>, как только увидели нас."
                            }
                        }
                    ],
                    "common_phrases": [
                        "indyk gulgocze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать характерный горловой звук, типичный для индюка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кулдыкать",
                                "target_lang_sense_clarification": "об индюке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8756db90-fb82-3486-9770-e380b8b85bc7",
                    "sense_definition": "Mówić w sposób niewyraźny, niezrozumiały i bełkotliwy, często ze złością lub pod wpływem emocji.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "speech_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obrażony mężczyzna zaczął coś wściekle <gulgotać> pod nosem.",
                            "target_lang_translations": {
                                "ru": "Обиженный мужчина начал что-то яростно <бормотать> себе под нос."
                            }
                        },
                        {
                            "text": "Przestań <gulgotać> i powiedz wreszcie wyraźnie, o co ci chodzi.",
                            "target_lang_translations": {
                                "ru": "Перестань <бубнить> и скажи наконец внятно, в чём дело."
                            }
                        }
                    ],
                    "synonyms": [
                        "bełkotać",
                        "mamrotać",
                        "bulgotać"
                    ],
                    "common_phrases": [
                        "gulgotać pod nosem",
                        "coś gulgotać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить невнятно, непонятно, часто со злостью или под влиянием эмоций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бормотать",
                                "target_lang_sense_clarification": "говорить невнятно"
                            },
                            {
                                "target_lang_word": "бубнить",
                                "target_lang_sense_clarification": "ворчать под нос"
                            },
                            {
                                "target_lang_word": "клокотать",
                                "target_lang_sense_clarification": "говорить захлебываясь (напр. от ярости)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
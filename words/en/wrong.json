                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неправильно",
                                "target_lang_sense_clarification": "с ошибками"
                            },
                            {
                                "target_lang_word": "неверно",
                                "target_lang_sense_clarification": "ложно"
                            },
                            {
                                "target_lang_word": "не так",
                                "target_lang_sense_clarification": "иным образом, чем нужно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0ffc392-f64f-3578-aa6c-a2a657d544b7",
                    "sense_definition": "Behavior or action that is not morally good or correct.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "immoral",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Children must learn the difference between right and <w>wrong</w>.",
                            "target_lang_translations": {
                                "ru": "Дети должны научиться отличать добро от <w>зла</w>."
                            }
                        },
                        {
                            "text": "He felt a lot of guilt for the <w>wrong</w> he had done.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал сильную вину за то <w>зло</w>, что причинил."
                            }
                        }
                    ],
                    "synonyms": [
                        "sin",
                        "iniquity",
                        "badness",
                        "wrongness"
                    ],
                    "common_phrases": [
                        "right and wrong",
                        "know right from wrong"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поведение или действие, которое не является морально хорошим или правильным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зло",
                                "target_lang_sense_clarification": "нечто дурное, вредное"

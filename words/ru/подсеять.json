    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9d5c911f-8383-3f72-86b5-a5118016a182",
                    "sense_definition": "Посеять семена дополнительно к тому, что уже было посеяно ранее, чтобы заполнить пустые места или увеличить густоту посева.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sowing_actions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На участках, где газон поредел после зимы, нужно <w>подсеять</w> свежую траву.",
                            "target_lang_translations": {
                                "en": "On patches where the lawn has thinned out after winter, one needs to <w>overseed</w> with fresh grass."
                            }
                        },
                        {
                            "text": "Если всходы моркови окажутся слишком редкими, мы сможем <w>подсеять</w> ещё немного семян.",
                            "target_lang_translations": {
                                "en": "If the carrot shoots turn out too sparse, we can <w>sow</w> some more seeds."
                            }
                        }
                    ],
                    "synonyms": [
                        "досеять"
                    ],
                    "common_phrases": [
                        "подсеять газон",
                        "подсеять семена",
                        "подсеять траву"
                    ],
                    "target_lang_definitions": {
                        "en": "To sow additional seeds to fill gaps or increase the density of the planting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overseed",
                                "target_lang_sense_clarification": "specifically for lawns or pastures"
                            },
                            {
                                "target_lang_word": "sow additional",
                                "target_lang_sense_clarification": "general meaning"
                            },
                            {
                                "target_lang_word": "reseed",
                                "target_lang_sense_clarification": "to fix sparse areas"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b38d828-8382-3803-9b93-0ba5d8270a89",
                    "sense_definition": "Посеять одну сельскохозяйственную культуру (обычно траву) на поле, где уже растёт другая культура.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sowing_actions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Агроном решил <w>подсеять</w> клевер к озимой пшенице для улучшения структуры почвы.",
                            "target_lang_translations": {
                                "en": "The agronomist decided to <w>undersow</w> clover with the winter wheat to improve soil structure."
                            }
                        },
                        {
                            "text": "В этом хозяйстве принято <w>подсеять</w> многолетние травы под покров зерновых культур.",
                            "target_lang_translations": {
                                "en": "In this farm, it is customary to <w>undersow</w> perennial grasses under the cover of grain crops."
                            }
                        }
                    ],
                    "common_phrases": [
                        "подсеять клевер",
                        "подсеять люцерну"
                    ],
                    "target_lang_definitions": {
                        "en": "To sow a secondary crop (usually grass) into a field where a primary crop is already growing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "undersow",
                                "target_lang_sense_clarification": "agricultural term"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
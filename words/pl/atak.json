    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb49c87f-9912-328d-a503-260b29a8d604",
                    "sense_definition": "Działanie zbrojne lub fizyczne skierowane przeciwko wrogowi w celu zadania strat lub zdobycia terenu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_conflict_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wojska nieprzyjaciela rozpoczęły zmasowany <w>atak</w> na nasze pozycje obronne o świcie.",
                            "target_lang_translations": {
                                "ru": "Войска противника начали массированную <w>атаку</w> на наши оборонительные позиции на рассвете."
                            }
                        },
                        {
                            "text": "Generał rozkazał natychmiastowy <w>atak</w>, aby zaskoczyć przeciwnika i przełamać linię frontu.",
                            "target_lang_translations": {
                                "ru": "Генерал приказал начать немедленную <w>атаку</w>, чтобы застать противника врасплох и прорвать линию фронта."
                            }
                        }
                    ],
                    "synonyms": [
                        "natarcie",
                        "uderzenie",
                        "ofensywa",
                        "szturm",
                        "napaść",
                        "szarża",
                        "działania zaczepne"
                    ],
                    "antonyms": [
                        "obrona",
                        "odwrót",
                        "defensywa",
                        "opór",
                        "wycofanie się"
                    ],
                    "common_phrases": [
                        "atak powietrzny",
                        "przypuścić atak",
                        "odeprzeć atak",
                        "atak zbrojny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Военное или физическое действие, направленное против врага с целью нанесения урона или захвата территории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атака",
                                "target_lang_sense_clarification": "общее военное или физическое действие"
                            },
                            {
                                "target_lang_word": "нападение",
                                "target_lang_sense_clarification": "агрессивное действие, штурм"
                            },
                            {
                                "target_lang_word": "штурм",
                                "target_lang_sense_clarification": "решительная атака укрепленной позиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d418ab05-857f-37fa-b26a-65a3d84b8f65",
                    "sense_definition": "Nagłe i gwałtowne wystąpienie objawów choroby lub dolegliwości zdrowotnej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_sudden_onset",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek trafił do szpitala, ponieważ miał wczoraj poważny <w>atak</w> serca.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка попал в больницу, так как вчера у него был серьезный сердечный <w>приступ</w>."
                            }
                        },
                        {
                            "text": "Podczas zebrania dyrektor dostał silnego <w>ataku</w> duszności i musiał wyjść na świeże powietrze.",
                            "target_lang_translations": {
                                "ru": "Во время собрания у директора случился сильный <w>приступ</w> удушья, и ему пришлось выйти на свежий воздух."
                            }
                        }
                    ],
                    "synonyms": [
                        "napad"
                    ],
                    "common_phrases": [
                        "atak serca",
                        "atak padaczki",
                        "atak kaszlu",
                        "atak paniki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное и резкое проявление симптомов болезни или недомогания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приступ",
                                "target_lang_sense_clarification": "сердца, кашля, боли"
                            },
                            {
                                "target_lang_word": "припадок",
                                "target_lang_sense_clarification": "эпилепсии, истерики"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ba3fb2a-e763-38cc-a053-6a15eb342d9c",
                    "sense_definition": "Agresywne wystąpienie przeciw komuś, często w formie ostrej krytyki słownej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_conflict_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polityk przypuścił ostry <w>atak</w> na swoich oponentów podczas debaty telewizyjnej.",
                            "target_lang_translations": {
                                "ru": "Политик предпринял резкую <w>атаку</w> на своих оппонентов во время теледебатов."
                            }
                        },
                        {
                            "text": "Nie spodziewałem się takiego <w>ataku</w> ze strony przyjaciela z powodu mojej opinii.",
                            "target_lang_translations": {
                                "ru": "Я не ожидал таких <w>нападок</w> со стороны друга из-за моего мнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "napaść"
                    ],
                    "antonyms": [
                        "obrona"
                    ],
                    "common_phrases": [
                        "atak personalny",
                        "ostry atak",
                        "atak medialny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Агрессивное выступление против кого-либо, часто в форме резкой словесной критики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нападки",
                                "target_lang_sense_clarification": "критика, обвинения (обычно мн. ч.)"
                            },
                            {
                                "target_lang_word": "атака",
                                "target_lang_sense_clarification": "метафорическое агрессивное действие"
                            },
                            {
                                "target_lang_word": "выпад",
                                "target_lang_sense_clarification": "резкое замечание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "652be802-ebb5-3800-be4c-aeeea6ff76e6",
                    "sense_definition": "Akcja ofensywna w sporcie mająca na celu zdobycie punktów lub przewagi nad rywalem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_conflict_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza drużyna przeprowadziła szybki <w>atak</w> skrzydłem i strzeliła decydującego gola.",
                            "target_lang_translations": {
                                "ru": "Наша команда провела быструю <w>атаку</w> по флангу и забила решающий гол."
                            }
                        },
                        {
                            "text": "Trener nakazał zawodnikom przejść do <w>ataku</w> w ostatnich minutach meczu.",
                            "target_lang_translations": {
                                "ru": "Тренер приказал игрокам перейти в <w>атаку</w> на последних минутах матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "ofensywa",
                        "natarcie",
                        "działania zaczepne",
                        "napad",
                        "uderzenie"
                    ],
                    "antonyms": [
                        "obrona",
                        "defensywa",
                        "działania obronne"
                    ],
                    "common_phrases": [
                        "przejść do ataku",
                        "atak pozycyjny",
                        "szybki atak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наступательное действие в спорте с целью набора очков или получения преимущества над соперником."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атака",
                                "target_lang_sense_clarification": "процесс наступления"
                            },
                            {
                                "target_lang_word": "нападение",
                                "target_lang_sense_clarification": "офенсива"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51eeebbb-7ae6-37a4-b904-4bca3d524c70",
                    "sense_definition": "Gwałtowne i nagłe pogorszenie się warunków atmosferycznych, na przykład nadejście zimy.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_sudden_onset",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niespodziewany <w>atak</w> zimy sparaliżował ruch na drogach w całym regionie.",
                            "target_lang_translations": {
                                "ru": "Неожиданное <w>наступление</w> зимы парализовало движение на дорогах во всем регионе."
                            }
                        },
                        {
                            "text": "Meteorolodzy ostrzegają przed kolejnym <w>atakiem</w> mrozu w nadchodzący weekend.",
                            "target_lang_translations": {
                                "ru": "Метеорологи предупреждают об очередном <w>ударе</w> морозов в предстоящие выходные."
                            }
                        }
                    ],
                    "common_phrases": [
                        "atak zimy",
                        "atak mrozu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резкое и внезапное ухудшение погодных условий, например, наступление зимы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наступление",
                                "target_lang_sense_clarification": "зимы, холодов"
                            },
                            {
                                "target_lang_word": "удар",
                                "target_lang_sense_clarification": "стихии, мороза"
                            },
                            {
                                "target_lang_word": "вторжение",
                                "target_lang_sense_clarification": "холодного воздуха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d808cb9-2c56-312b-b4b4-0937600a7ed1",
                    "sense_definition": "Część drużyny sportowej lub formacja zawodników odpowiedzialna za działania ofensywne.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_sports_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Najlepszy zawodnik w <w>ataku</w> doznał kontuzji i musiał zejść z boiska.",
                            "target_lang_translations": {
                                "ru": "Лучший игрок в <w>нападении</w> получил травму и был вынужден покинуть поле."
                            }
                        },
                        {
                            "text": "Trener postanowił wystawić w <w>ataku</w> dwóch młodych i szybkich piłkarzy.",
                            "target_lang_translations": {
                                "ru": "Тренер решил выставить в <w>атаке</w> двух молодых и быстрых футболистов."
                            }
                        }
                    ],
                    "synonyms": [
                        "napastnicy",
                        "pierwsza linia"
                    ],
                    "antonyms": [
                        "obrońcy",
                        "defensorzy",
                        "linia obronna"
                    ],
                    "common_phrases": [
                        "grać w ataku",
                        "wzmocnić atak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть спортивной команды или группа игроков, отвечающая за наступательные действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нападение",
                                "target_lang_sense_clarification": "линия нападения, форварды"
                            },
                            {
                                "target_lang_word": "атака",
                                "target_lang_sense_clarification": "группа атакующих игроков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17b0c028-ed6a-30db-84a9-dd144c658add",
                    "sense_definition": "Nagły, niekontrolowany wybuch silnych emocji, takich jak szał, śmiech czy histeria.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_sudden_onset",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy usłyszała ten dowcip, dostała takiego <w>ataku</w> śmiechu, że nie mogła przestać.",
                            "target_lang_translations": {
                                "ru": "Когда она услышала эту шутку, у нее случился такой <w>приступ</w> смеха, что она не могла остановиться."
                            }
                        },
                        {
                            "text": "Dziecko w sklepie dostało <w>ataku</w> histerii, ponieważ rodzice nie kupili mu zabawki.",
                            "target_lang_translations": {
                                "ru": "У ребенка в магазине случился <w>припадок</w> истерики, потому что родители не купили ему игрушку."
                            }
                        }
                    ],
                    "synonyms": [
                        "napad"
                    ],
                    "common_phrases": [
                        "atak szału",
                        "atak śmiechu",
                        "atak histerii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапная, неконтролируемая вспышка сильных эмоций, таких как ярость, смех или истерика."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приступ",
                                "target_lang_sense_clarification": "смеха, ярости"
                            },
                            {
                                "target_lang_word": "припадок",
                                "target_lang_sense_clarification": "истерики, безумия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb8e4fa4-47cd-30fd-b4c4-3c0a76c2d1a5",
                    "sense_definition": "Agresywny sposób rozgrywania partii szachowej lub konkretny wariant debiutowy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_conflict_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mistrz świata zastosował ryzykowny <w>atak</w>, poświęcając piona za inicjatywę.",
                            "target_lang_translations": {
                                "ru": "Чемпион мира применил рискованную <w>атаку</w>, пожертвовав пешкой ради инициативы."
                            }
                        },
                        {
                            "text": "Ten wariant debiutowy znany jest jako <w>atak</w> na skrzydle królewskim.",
                            "target_lang_translations": {
                                "ru": "Этот дебютный вариант известен как <w>атака</w> на королевском фланге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "atak królewski",
                        "atak z odsłony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Агрессивный способ ведения шахматной партии или конкретный дебютный вариант."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атака",
                                "target_lang_sense_clarification": "шахматный термин"

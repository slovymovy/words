                            },
                            {
                                "target_lang_word": "Труман",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52b0041a-2863-30c0-9b3b-f4d71f29a35c",
                    "sense_definition": "A place name used for various locations in the United States, such as a city in Minnesota.",

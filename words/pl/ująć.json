            "senses": [
                {
                    "sense_id": "556bc2ee-8f65-358d-a7d5-18d5f74b4296",
                    "sense_definition": "Chwycić coś lub kogoś dłonią; wziąć do ręki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna pewnie <ujął> ster i skierował łódź w stronę portu.",
                            "target_lang_translations": {
                                "ru": "Мужчина уверенно <взял> штурвал и направил лодку в сторону порта."
                            }
                        },
                        {
                            "text": "Matka delikatnie <ujęła> dłoń dziecka, aby przeprowadzić je przez ulicę.",
                            "target_lang_translations": {
                                "ru": "Мать нежно <взяла> ребенка за руку, чтобы перевести его через улицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgarnąć",
                        "przymknąć",
                        "przyskrzynić",
                        "aresztować"
                    ],
                    "antonyms": [

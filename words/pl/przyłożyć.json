                        {
                            "text": "Uważaj na słowa, bo ktoś może ci w końcu <przyłożyć>.",
                            "target_lang_translations": {
                                "ru": "Следи за словами, а то кто-нибудь может тебе в конце концов <врезать>."
                            }
                        }
                    ],
                    "synonyms": [
                        "uderzyć",
                        "walnąć",
                        "przywalić",
                        "grzmotnąć"
                    ],
                    "common_phrases": [
                        "przyłożyć komuś",
                        "przyłożyć z całej siły",
                        "przyłożyć w twarz"

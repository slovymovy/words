                        "ru": [
                            {
                                "target_lang_word": "передача",
                                "target_lang_sense_clarification": "данных или сигналов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0031aca3-0a26-39d7-b00e-6331d3a1a89e",
                    "sense_definition": "De officiële, juridische overdracht van eigendom, bijvoorbeeld van een huis.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De notaris heeft de akte van <w>transport</w> gisteren getekend.",
                            "target_lang_translations": {
                                "en": "The notary signed the deed of <w>conveyance</w> yesterday.",
                                "ru": "Вчера нотариус подписал акт <w>передачи</w>."
                            }
                        },
                        {
                            "text": "Na het <w>transport</w> ben je officieel de eigenaar van de woning.",
                            "target_lang_translations": {
                                "en": "After the <w>transfer</w>, you are officially the owner of the home.",
                                "ru": "После <w>передачи</w> прав вы официально становитесь владельцем дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "overbrenging"
                    ],
                    "common_phrases": [
                        "akte van transport"
                    ],
                    "target_lang_definitions": {
                        "en": "The official, legal transfer of ownership, for example of a house.",
                        "ru": "Официальная, юридическая передача права собственности, например, на недвижимость."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conveyance",
                                "target_lang_sense_clarification": "legal property transfer"
                            },
                            {
                                "target_lang_word": "transfer",
                                "target_lang_sense_clarification": "change of ownership"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "передача",
                                "target_lang_sense_clarification": "прав собственности"
                            },
                            {
                                "target_lang_word": "отчуждение",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b1ed223-039e-38fb-a821-990635336bf4",
                    "sense_definition": "Het overnemen van een totaalbedrag naar een nieuwe bladzijde in de boekhouding.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "accounting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Controleer het <w>transport</w> van de cijfers naar de volgende pagina goed.",
                            "target_lang_translations": {
                                "en": "Check the <w>carry-forward</w> of the figures to the next page carefully.",
                                "ru": "Внимательно проверьте <w>перенос</w> цифр на следующую страницу."
                            }
                        },
                        {
                            "text": "Er is een fout gemaakt bij het <w>transport</w> van het subtotaal.",
                            "target_lang_translations": {
                                "en": "A mistake was made during the <w>carry-forward</w> of the subtotal.",
                                "ru": "Была допущена ошибка при <w>переносе</w> промежуточного итога."
                            }
                        }
                    ],
                    "synonyms": [

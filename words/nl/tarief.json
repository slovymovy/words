{
    "word_family": [
        "tariferen",
        "tarifering"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "91c9229f-ceba-32b6-89fc-96e5f02b099d",
                    "sense_definition": "Een vastgesteld bedrag dat betaald moet worden voor een dienst of per eenheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cost",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>tarief</w> voor het huren van de auto is dertig euro per uur.",
                            "target_lang_translations": {
                                "en": "The <w>rate</w> for renting the car is thirty euros per hour.",
                                "ru": "<w>Тариф</w> на аренду автомобиля составляет тридцать евро в час."
                            }
                        },
                        {
                            "text": "De elektricien rekent een hoger <w>tarief</w> voor werk in het weekend.",
                            "target_lang_translations": {
                                "en": "The electrician charges a higher <w>rate</w> for work on the weekend.",
                                "ru": "Электрик берет более высокий <w>тариф</w> за работу в выходные."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vast tarief",
                        "laag tarief",
                        "tegen een tarief",
                        "gereduceerd tarief"
                    ],
                    "target_lang_definitions": {
                        "en": "A fixed amount charged for a service or per unit.",
                        "ru": "Установленная сумма, которую необходимо заплатить за услугу или за единицу чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rate",
                                "target_lang_sense_clarification": "cost per unit of time or quantity"
                            },
                            {
                                "target_lang_word": "fee",
                                "target_lang_sense_clarification": "charge for a specific service"
                            },
                            {
                                "target_lang_word": "tariff",
                                "target_lang_sense_clarification": "official schedule of charges, especially for utilities"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тариф",
                                "target_lang_sense_clarification": "ставка оплаты за услуги"
                            },
                            {
                                "target_lang_word": "расценка",
                                "target_lang_sense_clarification": "стоимость единицы работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33fcf2fa-6009-3c5e-8e2f-3e3275cb123b",
                    "sense_definition": "Een belasting die betaald moet worden bij de invoer of uitvoer van goederen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tax",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De overheid heeft een nieuw <w>tarief</w> ingevoerd op buitenlands staal.",
                            "target_lang_translations": {
                                "en": "The government has introduced a new <w>tariff</w> on foreign steel.",
                                "ru": "Правительство ввело новый <w>тариф</w> на иностранную сталь."
                            }
                        },
                        {
                            "text": "Door het hoge <w>tarief</w> is de import van deze producten afgenomen.",
                            "target_lang_translations": {
                                "en": "Due to the high <w>tariff</w>, the import of these products has decreased.",
                                "ru": "Из-за высокого <w>тарифа</w> импорт этих продуктов сократился."
                            }
                        }
                    ],
                    "synonyms": [
                        "importheffing"
                    ],
                    "common_phrases": [
                        "douanetarief"
                    ],
                    "target_lang_definitions": {
                        "en": "A tax levied on the import or export of goods.",
                        "ru": "Налог, который необходимо уплатить при ввозе или вывозе товаров."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tariff",
                                "target_lang_sense_clarification": "tax on imports or exports"
                            },
                            {
                                "target_lang_word": "duty",
                                "target_lang_sense_clarification": "customs tax on goods"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пошлина",
                                "target_lang_sense_clarification": "государственный сбор"
                            },
                            {
                                "target_lang_word": "таможенный тариф",
                                "target_lang_sense_clarification": "система ставок пошлин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e9d1e91-ac3a-33c9-bfc8-3eba1d9c68fb",
                    "sense_definition": "Een lijst waarop vastgestelde prijzen of kosten staan vermeld.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "list",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op het <w>tarief</w> in de wachtkamer kun je de kosten van de behandeling zien.",
                            "target_lang_translations": {
                                "en": "You can see the costs of the treatment on the <w>price list</w> in the waiting room.",
                                "ru": "В <w>прейскуранте</w> в приемной можно увидеть стоимость лечения."
                            }
                        },
                        {
                            "text": "De klant vroeg naar het <w>tarief</w> om de prijzen te vergelijken.",
                            "target_lang_translations": {
                                "en": "The customer asked for the <w>price list</w> to compare prices.",
                                "ru": "Клиент попросил <w>прейскурант</w>, чтобы сравнить цены."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A list showing fixed prices or costs.",
                        "ru": "Список, в котором указаны установленные цены или стоимость услуг."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "price list",
                                "target_lang_sense_clarification": "document listing prices"
                            },
                            {
                                "target_lang_word": "rate sheet",
                                "target_lang_sense_clarification": "list of service charges"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прейскурант",
                                "target_lang_sense_clarification": "справочник цен"
                            },
                            {
                                "target_lang_word": "прайс-лист",
                                "target_lang_sense_clarification": "перечень цен"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e5efc568-3d32-3d84-ae34-53a7f0984e87",
                    "sense_definition": "Een doel of object met een worp, schot of slag bereiken en niet missen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hit_touch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De voetballer <w>raakte</w> de lat met een hard schot.",
                            "target_lang_translations": {
                                "en": "The footballer <w>hit</w> the crossbar with a hard shot.",
                                "ru": "Футболист сильным ударом <w>попал</w> в перекладину."
                            }
                        },
                        {
                            "text": "Hij probeerde de roos te schieten, maar <w>raakte</w> de muur.",
                            "target_lang_translations": {
                                "en": "He tried to shoot the bullseye, but <w>hit</w> the wall.",
                                "ru": "Он пытался попасть в яблочко, но <w>попал</w> в стену."
                            }
                        }
                    ],
                    "synonyms": [
                        "treffen"
                    ],
                    "antonyms": [
                        "missen"
                    ],
                    "common_phrases": [
                        "doel <w>raken</w>",
                        "<w>raak</w> schieten",
                        "de bal <w>raken</w>"
                    ],
                    "target_lang_definitions": {
                        "en": "To reach a target or object with a throw, shot, or blow and not miss.",
                        "ru": "Достичь цели или объекта броском, выстрелом или ударом и не промахнуться."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "to strike a target"
                            },
                            {
                                "target_lang_word": "strike",
                                "target_lang_sense_clarification": "to hit forcefully"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "в цель"
                            },
                            {
                                "target_lang_word": "поразить",
                                "target_lang_sense_clarification": "цель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a908859-d6a9-30ae-8adb-1c2dfdd02d6e",
                    "sense_definition": "Fysiek contact maken met iets of iemand; tegen iets aankomen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hit_touch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De takken van de bomen <w>raken</w> het wateroppervlak.",
                            "target_lang_translations": {
                                "en": "The branches of the trees <w>touch</w> the water surface.",
                                "ru": "Ветви деревьев <w>касаются</w> поверхности воды."
                            }
                        },
                        {
                            "text": "Zorg dat je de hete oven niet <w>raakt</w>.",
                            "target_lang_translations": {
                                "en": "Make sure you do not <w>touch</w> the hot oven.",
                                "ru": "Следи за тем, чтобы не <w>коснуться</w> горячей духовки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de grond <w>raken</w>",
                        "elkaar <w>raken</w>"
                    ],
                    "target_lang_definitions": {
                        "en": "To make physical contact with something or someone; to come up against something.",
                        "ru": "Вступить в физический контакт с чем-то или кем-то; задеть что-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "touch",
                                "target_lang_sense_clarification": "to make physical contact"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "касаться",
                                "target_lang_sense_clarification": "физически"
                            },
                            {
                                "target_lang_word": "задевать",
                                "target_lang_sense_clarification": "случайно коснуться"
                            },
                            {
                                "target_lang_word": "трогать",
                                "target_lang_sense_clarification": "прикасаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fc702bb-0650-3d89-943a-e91549e91aa9",
                    "sense_definition": "In een bepaalde toestand, situatie of op een bepaalde plaats terechtkomen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "become_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de wandeling in het bos <w>raakten</w> we de weg kwijt.",
                            "target_lang_translations": {
                                "en": "During the walk in the forest, we <w>got</w> lost.",
                                "ru": "Во время прогулки в лесу мы <w>сбились</w> с пути."
                            }
                        },
                        {
                            "text": "Hij is zwaargewond <w>geraakt</w> bij het auto-ongeluk.",
                            "target_lang_translations": {
                                "en": "He <w>got</w> seriously injured in the car accident.",
                                "ru": "Он был тяжело <w>ранен</w> в автокатастрофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "worden",
                        "belanden"
                    ],
                    "common_phrases": [
                        "gewond <w>raken</w>",
                        "kwijt <w>raken</w>",
                        "in paniek <w>raken</w>",
                        "betrokken <w>raken</w>"
                    ],
                    "target_lang_definitions": {
                        "en": "To end up in a certain state, situation, or place.",
                        "ru": "Оказаться в определенном состоянии, ситуации или месте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "to become or reach a state (e.g. get lost, get hurt)"
                            },
                            {
                                "target_lang_word": "become",
                                "target_lang_sense_clarification": "to come into existence or a state"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "в ситуацию/беду"
                            },
                            {
                                "target_lang_word": "оказаться",
                                "target_lang_sense_clarification": "в каком-либо положении"
                            },
                            {
                                "target_lang_word": "стать",
                                "target_lang_sense_clarification": "изменение состояния"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ecae76f-785a-35fe-91a9-aa011687cab6",
                    "sense_definition": "Iemand emotioneel beïnvloeden; sterke gevoelens oproepen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het droevige verhaal van de oude man <w>raakte</w> ons allemaal.",
                            "target_lang_translations": {
                                "en": "The sad story of the old man <w>touched</w> us all.",
                                "ru": "Печальная история старика <w>тронула</w> нас всех."
                            }
                        },
                        {
                            "text": "Haar mooie zangstem heeft het publiek diep <w>geraakt</w>.",
                            "target_lang_translations": {
                                "en": "Her beautiful singing voice deeply <w>touched</w> the audience.",
                                "ru": "Её прекрасный голос глубоко <w>тронул</w> публику."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontroeren",
                        "treffen"
                    ],
                    "common_phrases": [
                        "diep <w>raken</w>",
                        "iemand <w>raken</w>"
                    ],
                    "target_lang_definitions": {
                        "en": "To influence someone emotionally; to evoke strong feelings.",
                        "ru": "Эмоционально воздействовать на кого-то; вызывать сильные чувства."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "touch",
                                "target_lang_sense_clarification": "to affect emotionally"
                            },
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "to evoke strong feelings"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тронуть",
                                "target_lang_sense_clarification": "эмоционально"
                            },
                            {
                                "target_lang_word": "задеть",
                                "target_lang_sense_clarification": "за живое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfa0d58e-b17e-34cf-a635-e20ba5501747",
                    "sense_definition": "De grond bewerken of materiaal bijeenbrengen met een hark.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tuinman <w>raakt</w> de gevallen bladeren bij elkaar.",
                            "target_lang_translations": {
                                "en": "The gardener <w>rakes</w> the fallen leaves together.",
                                "ru": "Садовник <w>сгребает</w> опавшие листья в кучу."
                            }
                        },
                        {
                            "text": "In de herfst <w>raken</w> we de tuin om alles netjes te houden.",
                            "target_lang_translations": {
                                "en": "In the autumn, we <w>rake</w> the garden to keep everything tidy.",
                                "ru": "Осенью мы <w>сгребаем</w> в саду, чтобы поддерживать порядок."
                            }
                        }
                    ],
                    "synonyms": [
                        "harken",
                        "rakelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To work the ground or gather material with a rake.",
                        "ru": "Обрабатывать землю или собирать материал граблями."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rake",
                                "target_lang_sense_clarification": "to use a rake"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сгребать",
                                "target_lang_sense_clarification": "граблями"
                            },
                            {
                                "target_lang_word": "рыхлить",
                                "target_lang_sense_clarification": "землю"
                            }
                        ]
                    }
                }
            ]
        },

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "появление",
                                "target_lang_sense_clarification": "процесс выхода в поле зрения"
                            },
                            {
                                "target_lang_word": "возникновение",
                                "target_lang_sense_clarification": "начало существования"
                            }
                        ]
                    }
                }
            ]
        }

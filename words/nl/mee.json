    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d2c5b1dc-318e-3988-845b-46b5974292fa",
                    "sense_definition": "Samen met iemand anders of in gezelschap; tegelijkertijd met een ander.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ga je vanavond met ons <w>mee</w> naar de bioscoop?",
                            "target_lang_translations": {
                                "en": "Are you coming <w>along</w> with us to the cinema tonight?",
                                "ru": "Ты пойдешь сегодня вечером <w>с нами</w> в кино?"
                            }
                        },
                        {
                            "text": "Als jij gaat fietsen, fiets ik gezellig een stukje <w>mee</w>.",
                            "target_lang_translations": {
                                "en": "If you go cycling, I'll cycle <w>along</w> for a bit.",
                                "ru": "Если ты поедешь на велосипеде, я с удовольствием проедусь немного <w>вместе с тобой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ga je mee",
                        "doe je mee",
                        "kom mee"
                    ],
                    "target_lang_definitions": {
                        "en": "Together with someone else or accompanying an action.",
                        "ru": "Вместе с кем-то или в компании; одновременно с другим."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "along",
                                "target_lang_sense_clarification": "accompanying movement or action"
                            },
                            {
                                "target_lang_word": "with",
                                "target_lang_sense_clarification": "in the company of"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вместе",
                                "target_lang_sense_clarification": "совместно"
                            },
                            {
                                "target_lang_word": "с собой",
                                "target_lang_sense_clarification": "взять, иметь при себе"
                            },
                            {
                                "target_lang_word": "за компанию",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ad5b5c2-17fd-32cf-aa5b-5356bede5eea",
                    "sense_definition": "De vorm van het voorzetsel <w>met</w> wanneer dit gecombineerd wordt met een voornaamwoordelijk bijwoord (zoals er, hier, daar, waar).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben het er helemaal <w>mee</w> eens.",
                            "target_lang_translations": {
                                "en": "I completely agree <w>with</w> it.",
                                "ru": "Я полностью <w>с</w> этим согласен."
                            }
                        },
                        {
                            "text": "Hij wist niet wat hij daar <w>mee</w> moest doen.",
                            "target_lang_translations": {
                                "en": "He didn't know what to do <w>with</w> that.",
                                "ru": "Он не знал, что ему <w>с</w> этим делать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "er mee stoppen",
                        "daar mee eens",
                        "waar mee bezig"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "gebruikt in voornaamwoordelijke bijwoorden"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The preposition 'with' used in combination with words like 'it', 'that', or 'what' (pronominal adverbs).",
                        "ru": "Форма предлога 'met' (с) при использовании в местоименных наречиях (например, ermee, hiermee)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "with",
                                "target_lang_sense_clarification": "part of a pronominal adverb like 'therewith' or 'with it'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "предлог, управляющий творительным падежом"
                            },
                            {
                                "target_lang_word": "этим",
                                "target_lang_sense_clarification": "в значении 'с этим' (ermee)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ae9962fd-42da-3eec-9a2f-3bd98358962a",
                    "sense_definition": "In staat om iets te volgen of te begrijpen; op de hoogte zijn.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na die ingewikkelde uitleg was ik even niet meer <w>mee</w>.",
                            "target_lang_translations": {
                                "en": "After that complicated explanation, I wasn't <w>following</w> for a moment.",
                                "ru": "После этого сложного объяснения я на какой-то момент перестал <w>понимать</w>."
                            }
                        },
                        {
                            "text": "Is iedereen <w>mee</w> met de nieuwe regels?",
                            "target_lang_translations": {
                                "en": "Is everyone <w>following</w> the new rules?",
                                "ru": "Все <w>в курсе</w> новых правил?"
                            }

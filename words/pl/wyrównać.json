    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "95a07ce0-d4e7-3fe0-8e86-6755a861d0e1",
                    "sense_definition": "Sprawić, że wartości, poziomy, ilości lub szanse staną się takie same; doprowadzić do stanu równowagi lub remisu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "equalize_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ostatniej minucie meczu naszej drużynie udało się <wyrównać> wynik.",
                            "target_lang_translations": {
                                "ru": "На последней минуте матча нашей команде удалось <сравнять> счёт."
                            }
                        },
                        {
                            "text": "Chcę jak najszybciej <wyrównać> wszystkie moje długi wobec banku.",
                            "target_lang_translations": {
                                "ru": "Я хочу как можно скорее <погасить> все свои долги перед банком."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrównać",
                        "zbilansować"
                    ],
                    "antonyms": [
                        "zróżnicować"
                    ],
                    "common_phrases": [
                        "wyrównać wynik",
                        "wyrównać rachunki",
                        "wyrównać szanse",
                        "wyrównać straty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать равным по величине, уровню или значению; привести к равновесию или ничьей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сравнять",
                                "target_lang_sense_clarification": "счёт в игре"
                            },
                            {
                                "target_lang_word": "уравнять",
                                "target_lang_sense_clarification": "шансы, права"
                            },
                            {
                                "target_lang_word": "погасить",
                                "target_lang_sense_clarification": "долги, счета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2fbc661-a9e8-36eb-9061-9eacc10b190e",
                    "sense_definition": "Uczynić powierzchnię płaską i gładką, likwidując zagłębienia lub wypukłości; także uspokoić rytm czegoś (np. oddechu).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "flatten_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ogrodnik musiał <wyrównać> ziemię grabiami, zanim posiał nową trawę.",
                            "target_lang_translations": {
                                "ru": "Садовник должен был <разровнять> землю граблями, прежде чем сеять новую траву."
                            }
                        },
                        {
                            "text": "Biegacz zatrzymał się na chwilę, aby <wyrównać> przyspieszony oddech.",
                            "target_lang_translations": {
                                "ru": "Бегун остановился на минуту, чтобы <выровнять> сбившееся дыхание."
                            }
                        }
                    ],
                    "synonyms": [
                        "wygładzić",
                        "zrównać",
                        "spłaszczyć"
                    ],
                    "antonyms": [
                        "pofałdować",
                        "skrzywić"
                    ],
                    "common_phrases": [
                        "wyrównać teren",
                        "wyrównać oddech",
                        "wyrównać powierzchnię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать поверхность ровной и гладкой; также восстановить ритм (например, дыхания)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выровнять",
                                "target_lang_sense_clarification": "поверхность, дыхание"
                            },
                            {
                                "target_lang_word": "разровнять",
                                "target_lang_sense_clarification": "землю, песок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28fbca30-a09e-3e78-8be1-62c0d29f99b5",
                    "sense_definition": "Ustawić obiekty w jednej linii lub nadać tekstowi odpowiedni układ graficzny względem marginesów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "align_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dowódca kazał żołnierzom natychmiast <wyrównać> szereg na placu apelowym.",
                            "target_lang_translations": {
                                "ru": "Командир приказал солдатам немедленно <выровнять> строй на плацу."
                            }
                        },
                        {
                            "text": "W edytorze tekstu możesz łatwo <wyrównać> akapity do prawej strony.",
                            "target_lang_translations": {
                                "ru": "В текстовом редакторе можно легко <выровнять> абзацы по правому краю."
                            }
                        }
                    ],
                    "synonyms": [
                        "ustawić",
                        "uporządkować"
                    ],
                    "antonyms": [
                        "rozrzucić"
                    ],
                    "common_phrases": [
                        "wyrównać tekst",
                        "wyrównać szereg",
                        "wyrównać krok",
                        "wyrównać do prawej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположить объекты в одну линию или оформить текст с выравниванием по краям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выровнять",
                                "target_lang_sense_clarification": "текст, строй"
                            },
                            {
                                "target_lang_word": "подравнять",
                                "target_lang_sense_clarification": "строй, края"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b1eab84d-6d97-38b0-8a14-cc4d84114aa6",
                    "sense_definition": "Stopień oficerski w policji, wyższy od podkomisarza i niższy od nadkomisarza; także funkcjonariusz noszący ten stopień.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "police_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prowadzący sprawę <w>komisarz</w> przesłuchał już wszystkich świadków.",
                            "target_lang_translations": {
                                "ru": "Ведущий дело <w>комиссар</w> уже допросил всех свидетелей."
                            }
                        },
                        {
                            "text": "Młody <w>komisarz</w> policji szybko awansował dzięki swojej skuteczności.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>комиссар</w> полиции быстро получил повышение благодаря своей эффективности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "komisarz policji",
                        "panie komisarzu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Офицерское звание в полиции (выше подкомиссара и ниже надкомиссара); также сотрудник, носящий это звание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комиссар",
                                "target_lang_sense_clarification": "звание в полиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0e2519d-5a76-3e77-8da3-55877f8cf66c",
                    "sense_definition": "Urzędnik państwowy lub unijny powołany do wykonywania specjalnych zadań, posiadający szczególne uprawnienia i pełnomocnictwa.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd wyznaczył specjalnego <w>komisarza</w> do walki ze skutkami powodzi.",
                            "target_lang_translations": {
                                "ru": "Правительство назначило специального <w>уполномоченного</w> для борьбы с последствиями наводнения."
                            }
                        },
                        {
                            "text": "Każdy <w>komisarz</w> europejski odpowiada za inny obszar polityki unijnej.",
                            "target_lang_translations": {
                                "ru": "Каждый европейский <w>комиссар</w> отвечает за отдельную область политики ЕС."
                            }
                        }
                    ],
                    "synonyms": [
                        "pełnomocnik"
                    ],
                    "common_phrases": [
                        "komisarz wyborczy",
                        "komisarz europejski",
                        "zarząd komisaryczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государственный чиновник или чиновник ЕС, назначенный для выполнения специальных задач и обладающий особыми полномочиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комиссар",
                                "target_lang_sense_clarification": "должностное лицо"
                            },
                            {
                                "target_lang_word": "уполномоченный",
                                "target_lang_sense_clarification": "лицо с особыми поручениями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee1348c2-cd23-3930-a07f-5beabb4b6203",
                    "sense_definition": "W ZSRR i wczesnym okresie komunizmu: urzędnik kierujący resortem administracji państwowej lub oficer polityczny w wojsku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polityczny <w>komisarz</w> dbał o ideologiczną postawę żołnierzy na froncie.",
                            "target_lang_translations": {
                                "ru": "Политический <w>комиссар</w> следил за идеологическим настроем солдат на фронте."
                            }
                        },
                        {
                            "text": "Władzę w mieście przejął <w>komisarz</w> ludowy.",
                            "target_lang_translations": {
                                "ru": "Власть в городе захватил народный <w>комиссар</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "komisarz ludowy",
                        "komisarz polityczny"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context (Soviet Union)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В СССР и ранний период коммунизма: чиновник, возглавляющий ведомство госуправления, или политрук в армии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комиссар",
                                "target_lang_sense_clarification": "историческое, советское звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebc52bb3-e4f2-3009-9ffe-e5d730b6b5cc",
                    "sense_definition": "Osoba zarządzająca majątkiem ziemskim w imieniu właściciela.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hrabia wysłał swojego <w>komisarza</w>, aby dopilnował żniw w odległym folwarku.",
                            "target_lang_translations": {
                                "ru": "Граф послал своего <w>управляющего</w>, чтобы тот проследил за жатвой на отдаленном хуторе."
                            }
                        },
                        {
                            "text": "Stary <w>komisarz</w> znał każdy zakątek powierzonych mu dóbr.",
                            "target_lang_translations": {
                                "ru": "Старый <w>управляющий</w> знал каждый уголок вверенных ему владений."
                            }
                        }
                    ],
                    "synonyms": [
                        "rządca"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, управляющее земельным имением от имени владельца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управляющий",
                                "target_lang_sense_clarification": "имением"
                            }
                        ]
                    }
                }
            ]
        }

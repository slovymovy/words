    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d9f162af-4eda-3dbf-a833-6e1debff3d95",
                    "sense_definition": "To try to achieve a specific aim, goal, or result over a period of time.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_goal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to move to the city to <w>pursue</w> a career in journalism.",
                            "target_lang_translations": {
                                "ru": "Она решила переехать в город, чтобы <w>построить</w> карьеру в журналистике."
                            }
                        },
                        {
                            "text": "The organization will <w>pursue</w> a goal of reducing plastic waste by half.",
                            "target_lang_translations": {
                                "ru": "Организация будет <w>преследовать</w> цель сокращения пластиковых отходов наполовину."
                            }
                        }
                    ],
                    "synonyms": [
                        "seek",
                        "strive for",
                        "aim for",
                        "go after"
                    ],
                    "antonyms": [
                        "give up",
                        "abandon"
                    ],
                    "common_phrases": [
                        "pursue a career",
                        "pursue a goal",
                        "pursue a dream",
                        "pursue a policy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пытаться достичь определенной цели, результата или мечты в течение длительного времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преследовать",
                                "target_lang_sense_clarification": "цель, интересы"
                            },
                            {
                                "target_lang_word": "стремиться",
                                "target_lang_sense_clarification": "к успеху, к цели"
                            },
                            {
                                "target_lang_word": "добиваться",
                                "target_lang_sense_clarification": "результата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95935e08-6a17-3eb0-8f11-795b3a3dd5ca",
                    "sense_definition": "To follow someone or something quickly in order to catch or attack them.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer had to <w>pursue</w> the suspect on foot through the narrow streets.",
                            "target_lang_translations": {
                                "ru": "Полицейскому пришлось <w>преследовать</w> подозреваемого пешком по узким улицам."
                            }
                        },
                        {
                            "text": "Wolves will often <w>pursue</w> their prey for miles before attempting to attack.",
                            "target_lang_translations": {
                                "ru": "Волки часто <w>преследуют</w> свою добычу на протяжении многих миль, прежде чем попытаться напасть."
                            }
                        }
                    ],
                    "synonyms": [
                        "chase",
                        "hunt",
                        "track",
                        "tail"
                    ],
                    "antonyms": [
                        "flee",
                        "escape"
                    ],
                    "common_phrases": [
                        "pursue a suspect",
                        "pursue the enemy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро следовать за кем-то или чем-то, чтобы поймать, схватить или напасть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преследовать",
                                "target_lang_sense_clarification": "врага, преступника"
                            },
                            {
                                "target_lang_word": "гнаться",
                                "target_lang_sense_clarification": "бежать за кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bef52fc5-7492-3864-8231-5f3d45e5fb5e",
                    "sense_definition": "To continue to discuss, investigate, or be involved in a particular topic, activity, or interest.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_goal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He plans to <w>pursue</w> his interest in photography more seriously after he retires.",
                            "target_lang_translations": {
                                "ru": "Он планирует более серьезно <w>заняться</w> своим увлечением фотографией после выхода на пенсию."
                            }
                        },
                        {
                            "text": "We do not have enough evidence to <w>pursue</w> this matter any further right now.",
                            "target_lang_translations": {
                                "ru": "У нас недостаточно улик, чтобы <w>продолжать</w> это дело прямо сейчас."
                            }
                        }
                    ],
                    "synonyms": [
                        "engage in",
                        "practice",
                        "conduct",
                        "follow"
                    ],
                    "antonyms": [
                        "discontinue",
                        "drop"
                    ],
                    "common_phrases": [
                        "pursue a hobby",
                        "pursue an interest",
                        "pursue a matter",
                        "pursue an inquiry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжать заниматься определенной деятельностью, изучать тему или вести расследование."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заниматься",
                                "target_lang_sense_clarification": "хобби, деятельностью"
                            },
                            {
                                "target_lang_word": "продолжать",
                                "target_lang_sense_clarification": "дело, расследование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efecdc66-e647-32c6-a58b-37a3295fa23d",
                    "sense_definition": "To continue speaking or to carry on with a story or line of thought.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "\"And furthermore,\" he <w>pursued</w>, \"we must consider the implications of this decision.\"",
                            "target_lang_translations": {
                                "ru": "«И более того, — <w>продолжил</w> он, — мы должны учитывать последствия этого решения»."
                            }
                        },
                        {
                            "text": "The narrator <w>pursued</w> his tale despite the frequent interruptions from the audience.",
                            "target_lang_translations": {
                                "ru": "Рассказчик <w>продолжал</w> свою историю, несмотря на частые прерывания со стороны аудитории."
                            }
                        }
                    ],
                    "synonyms": [
                        "continue",
                        "proceed"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often found in literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжать говорить или развивать мысль после паузы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продолжать",
                                "target_lang_sense_clarification": "речь, рассказ"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6f80be10-a9d6-3082-80b1-84e1a57a37e8",
                    "sense_definition": "Относящийся к данной местности, краю или району; здешний.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geo_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы перевели часы на <w>местное</w> время сразу после прилёта.",
                            "target_lang_translations": {
                                "en": "We switched our watches to <w>local</w> time immediately after arrival."
                            }
                        },
                        {
                            "text": "<w>Местные</w> жители всегда рады помочь туристам найти дорогу.",
                            "target_lang_translations": {
                                "en": "<w>Local</w> residents are always happy to help tourists find their way."
                            }
                        }
                    ],
                    "synonyms": [
                        "здешний"
                    ],
                    "antonyms": [
                        "неместный",
                        "нездешний",
                        "повсеместный",
                        "общегосударственный"
                    ],
                    "common_phrases": [
                        "местные жители",
                        "местное время",
                        "местные власти",
                        "местная газета"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a specific area, region, or district; from here."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "local",
                                "target_lang_sense_clarification": "relating to a particular area"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c79137eb-cd3d-3f01-a7b1-3248f0bcd844",
                    "sense_definition": "Изготовленный, добытый или растущий в данной местности; не привозной.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geo_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На рынке продают свежие <w>местные</w> овощи и фрукты.",
                            "target_lang_translations": {
                                "en": "They sell fresh <w>local</w> vegetables and fruits at the market."
                            }
                        },
                        {
                            "text": "Туристам очень понравилась <w>местная</w> кухня и напитки.",
                            "target_lang_translations": {
                                "en": "The tourists really liked the <w>local</w> cuisine and drinks."
                            }
                        }
                    ],
                    "synonyms": [
                        "здешний"
                    ],
                    "antonyms": [
                        "привозной"
                    ],
                    "common_phrases": [
                        "местные продукты",
                        "местный колорит",
                        "местные обычаи"
                    ],
                    "target_lang_definitions": {
                        "en": "Produced, extracted, or grown in a given area; not imported."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "local",
                                "target_lang_sense_clarification": "regarding products or produce"
                            },
                            {
                                "target_lang_word": "home-grown",
                                "target_lang_sense_clarification": "grown in the area"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc2b3746-1bb5-3e7f-b1e5-75390f4b2b90",
                    "sense_definition": "Ограниченный определённым местом, не распространяющийся на всё целое (часто используется в медицине).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "limited_scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач решил делать операцию под <w>местным</w> наркозом.",
                            "target_lang_translations": {
                                "en": "The doctor decided to perform the surgery under <w>local</w> anesthesia."
                            }
                        },
                        {
                            "text": "Это <w>местное</w> воспаление, оно скоро пройдёт само.",
                            "target_lang_translations": {
                                "en": "This is a <w>local</w> inflammation; it will pass on its own soon."
                            }
                        }
                    ],
                    "synonyms": [
                        "локальный"
                    ],
                    "antonyms": [
                        "общий"
                    ],
                    "common_phrases": [
                        "местный наркоз",
                        "местная анестезия",
                        "местное значение"
                    ],
                    "target_lang_definitions": {
                        "en": "Limited to a specific place, not extending to the whole (often used in medicine)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "local",
                                "target_lang_sense_clarification": "medical: affecting only a part"
                            },
                            {
                                "target_lang_word": "localized",
                                "target_lang_sense_clarification": "restricted to a specific place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee8e5fb1-af52-3761-8ef1-6908568b32eb",
                    "sense_definition": "В лингвистике: падеж, указывающий на местонахождение (в русском языке соответствует предложному падежу с предлогами «в» и «на»).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В некоторых языках сохранился отдельный <w>местный</w> падеж.",
                            "target_lang_translations": {
                                "en": "In some languages, a separate <w>locative</w> case has been preserved."
                            }
                        },
                        {
                            "text": "Студенты изучали <w>местный</w> падеж на лекции по истории языка.",
                            "target_lang_translations": {
                                "en": "The students studied the <w>locative</w> case at the lecture on the history of the language."
                            }
                        }
                    ],
                    "common_phrases": [
                        "местный падеж"
                    ],
                    "target_lang_definitions": {
                        "en": "In linguistics: a case indicating location (corresponds to the prepositional case with prepositions 'in' and 'on' in Russian)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "locative",
                                "target_lang_sense_clarification": "linguistic term"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9535a298-e15b-31d4-86a8-5f598a0b7a53",
                    "sense_definition": "Человек, который постоянно живёт в данной местности.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не <w>местный</w>, поэтому не знаю, где находится вокзал.",
                            "target_lang_translations": {
                                "en": "I am not <w>local</w>, so I don't know where the train station is."
                            }

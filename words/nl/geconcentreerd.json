                    "sense_id": "2c46882a-5bbf-34f1-ba07-81fab1523ae2",
                    "sense_definition": "Met volledige aandacht ergens op gericht.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_focus",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>geconcentreerde</w> dammer had niet gezien dat zijn vriendin naast hem stond.",
                            "target_lang_translations": {
                                "en": "The <w>focused</w> checkers player had not seen that his girlfriend was standing next to him.",
                                "ru": "<w>Сосредоточенный</w> шашист не заметил, что его девушка стоит рядом с ним."
                            }
                        },
                        {
                            "text": "De leerlingen zaten <w>geconcentreerd</w> te werken aan hun opdracht.",
                            "target_lang_translations": {
                                "en": "The students were working <w>intently</w> on their assignment.",
                                "ru": "Ученики <w>сосредоточенно</w> работали над своим заданием."
                            }
                        }
                    ],
                    "synonyms": [
                        "aandachtig",
                        "ingespannen",
                        "nauwlettend",
                        "verdiept"
                    ],
                    "antonyms": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a16703c5-52e6-307d-8d43-bbe58553d38e",
                    "sense_definition": "Een officiële lijst waarin gegevens over personen, zaken of gebeurtenissen worden bijgehouden",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "lists",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente houdt een <w>register</w> bij van alle inwoners.",
                            "target_lang_translations": {
                                "ru": "Муниципалитет ведет <w>реестр</w> всех жителей.",
                                "en": "The municipality keeps a <w>register</w> of all residents."
                            }
                        },
                        {
                            "text": "Je moet je naam in het <w>register</w> schrijven als je binnenkomt.",
                            "target_lang_translations": {
                                "ru": "Вы должны записать свое имя в <w>журнал</w> при входе.",
                                "en": "You must write your name in the <w>register</w> when you enter."
                            }
                        }
                    ],
                    "synonyms": [
                        "lijst",
                        "bestand",
                        "archief"
                    ],
                    "common_phrases": [
                        "in het register",
                        "register van de burgerlijke stand",
                        "het register raadplegen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный список, в котором хранятся данные о людях, вещах или событиях",
                        "en": "An official list used to record data about people, items, or events"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реестр",
                                "target_lang_sense_clarification": "официальный список"
                            },
                            {
                                "target_lang_word": "журнал",
                                "target_lang_sense_clarification": "книга для записей"
                            },
                            {
                                "target_lang_word": "регистр",
                                "target_lang_sense_clarification": "база данных"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "official list"
                            },
                            {
                                "target_lang_word": "registry",
                                "target_lang_sense_clarification": "database or office"
                            },
                            {
                                "target_lang_word": "record",
                                "target_lang_sense_clarification": "written account"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6f83b50-b026-303a-9199-22060da8dae6",
                    "sense_definition": "Een alfabetische lijst van namen of onderwerpen, meestal achterin een boek",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "lists",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk in het <w>register</w> om te zien op welke pagina het onderwerp staat.",
                            "target_lang_translations": {
                                "ru": "Посмотрите в <w>указателе</w>, чтобы узнать, на какой странице находится эта тема.",
                                "en": "Look in the <w>index</w> to see on which page the subject is located."
                            }
                        },
                        {
                            "text": "De auteur heeft een uitgebreid <w>register</w> aan het boek toegevoegd.",
                            "target_lang_translations": {
                                "ru": "Автор добавил в книгу обширный <w>указатель</w>.",
                                "en": "The author has added an extensive <w>index</w> to the book."
                            }
                        }
                    ],
                    "synonyms": [
                        "index",
                        "trefwoordenlijst",
                        "klapper"
                    ],
                    "common_phrases": [
                        "achterin het register",
                        "alfabetisch register"
                    ],
                    "target_lang_definitions": {
                        "ru": "Алфавитный список имен или тем, обычно в конце книги",
                        "en": "An alphabetical list of names or subjects, usually found at the back of a book"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "указатель",
                                "target_lang_sense_clarification": "в книге"
                            },
                            {
                                "target_lang_word": "предметный указатель",
                                "target_lang_sense_clarification": "список тем"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "index",
                                "target_lang_sense_clarification": "in a book"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d01e8d46-479d-3c5e-a13f-149f1588b3aa",
                    "sense_definition": "Een bepaalde taalstijl die past bij een specifieke situatie of context (bijvoorbeeld formeel of informeel)",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tegen je baas gebruik je vaak een ander <w>register</w> dan tegen je vrienden.",
                            "target_lang_translations": {
                                "ru": "С начальником вы часто используете другой <w>регистр</w>, чем с друзьями.",
                                "en": "You often use a different <w>register</w> with your boss than with your friends."
                            }
                        },
                        {
                            "text": "Het woord dat hij gebruikte paste niet in het formele <w>register</w> van de vergadering.",
                            "target_lang_translations": {
                                "ru": "Слово, которое он использовал, не вписывалось в формальный <w>регистр</w> собрания.",
                                "en": "The word he used did not fit the formal <w>register</w> of the meeting."
                            }
                        }
                    ],
                    "synonyms": [
                        "taalgebruik",
                        "stijl"
                    ],
                    "common_phrases": [
                        "formeel register",
                        "van register wisselen",
                        "lager register"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный стиль речи, соответствующий конкретной ситуации или контексту",
                        "en": "A specific style of language suitable for a particular situation or context"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регистр",
                                "target_lang_sense_clarification": "стиль речи"
                            },
                            {
                                "target_lang_word": "стиль",
                                "target_lang_sense_clarification": "манера общения"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "linguistic style"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6264fb2-c9e9-3b89-b59d-821c8cea0898",
                    "sense_definition": "Het bereik van tonen dat een stem of muziekinstrument kan voortbrengen",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zangeres zingt moeiteloos de noten in het hoge <w>register</w>.",
                            "target_lang_translations": {
                                "ru": "Певица без усилий берет ноты в высоком <w>регистре</w>.",
                                "en": "The singer effortlessly sings the notes in the high <w>register</w>."
                            }
                        },
                        {
                            "text": "Dit instrument klinkt erg warm in het lage <w>register</w>.",
                            "target_lang_translations": {
                                "ru": "Этот инструмент звучит очень тепло в нижнем <w>регистре</w>.",
                                "en": "This instrument sounds very warm in the low <w>register</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "toonomvang",
                        "bereik"
                    ],
                    "common_phrases": [
                        "hoog register",
                        "laag register"
                    ],
                    "target_lang_definitions": {
                        "ru": "Диапазон звуков, которые может воспроизводить голос или музыкальный инструмент",
                        "en": "The range of tones that a voice or musical instrument can produce"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регистр",
                                "target_lang_sense_clarification": "музыкальный диапазон"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "vocal or instrumental range"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a36182df-9de3-39aa-9bcf-268710ffabdb",
                    "sense_definition": "Een reeks orgelpijpen met dezelfde klankkleur die als groep in- of uitgeschakeld kan worden",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De organist trok een extra <w>register</w> open voor meer volume.",
                            "target_lang_translations": {
                                "ru": "Органист открыл дополнительный <w>регистр</w> для увеличения громкости.",
                                "en": "The organist pulled out an extra <w>stop</w> for more volume."
                            }
                        },
                        {
                            "text": "Om indruk te maken besloot hij alle <w>registers</w> open te trekken.",
                            "target_lang_translations": {
                                "ru": "Чтобы произвести впечатление, он решил пустить в ход все <w>средства</w>.",
                                "en": "To make an impression, he decided to pull out all the <w>stops</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "orgelregister"
                    ],
                    "common_phrases": [
                        "alle registers opentrekken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ряд органных труб с одинаковым тембром, который можно включать или выключать как группу",
                        "en": "A set of organ pipes with the same timbre that can be activated as a group"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регистр",
                                "target_lang_sense_clarification": "часть органа"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "organ mechanism"
                            },
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "set of organ pipes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8727dc52-0626-3eb2-a617-16c70a439a75",
                    "sense_definition": "Een klein, zeer snel geheugen in een computerprocessor voor tijdelijke opslag",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De processor slaat de uitkomst van de berekening op in een <w>register</w>.",
                            "target_lang_translations": {
                                "ru": "Процессор сохраняет результат вычисления в <w>регистре</w>.",
                                "en": "The processor stores the result of the calculation in a <w>register</w>."
                            }
                        },
                        {
                            "text": "Programmeurs moeten soms direct naar het <w>register</w> schrijven voor maximale snelheid.",
                            "target_lang_translations": {
                                "ru": "Программистам иногда приходится писать напрямую в <w>регистр</w> для максимальной скорости.",
                                "en": "Programmers sometimes have to write directly to the <w>register</w> for maximum speed."
                            }
                        }
                    ],
                    "common_phrases": [
                        "schuifregister"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая, очень быстрая память в процессоре компьютера для временного хранения данных",
                        "en": "A small, very fast memory in a computer processor for temporary storage"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регистр",
                                "target_lang_sense_clarification": "память процессора"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "CPU memory"
                            }
                        ]
                    }
                }
            ]
        }

                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A monarch's throne or seat of power."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throne",
                                "target_lang_sense_clarification": "royal seat"
                            }
                        ]
                    }
                }
            ]
        }

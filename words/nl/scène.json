                },
                {
                    "sense_id": "5c2c86b7-bbdc-3ef3-81d7-e9bfca6433b3",
                    "sense_definition": "Een luidruchtige ruzie of opschudding waarbij men de aandacht trekt",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij schopte een <w>scène</w> in de winkel omdat hij niet werd geholpen.",
                            "target_lang_translations": {
                                "en": "He made a <w>scene</w> in the store because he wasn't being helped."
                            }
                        },
                        {
                            "text": "Maak alsjeblieft geen <w>scène</w> waar al die mensen bij zijn.",
                            "target_lang_translations": {
                                "en": "Please don't make a <w>scene</w> with all those people around."
                            }
                        }
                    ],
                    "synonyms": [
                        "stennis",
                        "ophef"
                    ],
                    "common_phrases": [
                        "een scène maken",
                        "een scène schoppen"
                    ],
                    "target_lang_definitions": {
                        "en": "A loud or angry display of emotion in public that attracts attention"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scene",
                                "target_lang_sense_clarification": "public disturbance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af187b76-a709-3e48-ba4e-09c58999c136",
                    "sense_definition": "Het podium of de speelruimte in een theater",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "theater_film",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De acteurs kwamen langzaam de <w>scène</w> op voor het slotapplaus.",
                            "target_lang_translations": {
                                "en": "The actors slowly came onto the <w>stage</w> for the final applause."
                            }
                        },
                        {
                            "text": "Er stond slechts één stoel midden op de lege <w>scène</w>.",

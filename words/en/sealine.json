            "senses": [
                {
                    "sense_id": "12b43f59-570f-3fbb-b887-5ff78dfe8992",
                    "sense_definition": "The land along the edge of the sea.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "boundary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We could see the rocky <w>sealine</w> from the deck of the ship.",
                            "target_lang_translations": {
                                "ru": "С палубы корабля мы могли видеть скалистую <w>береговую линию</w>."
                            }
                        },
                        {
                            "text": "The fog completely obscured the distant <w>sealine</w>.",
                            "target_lang_translations": {
                                "ru": "Туман полностью скрыл далекую <w>береговую линию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "coastline",
                        "shoreline",
                        "seaboard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Земля вдоль края моря"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "береговая линия",
                                "target_lang_sense_clarification": "граница суши и моря"
                            },
                            {
                                "target_lang_word": "побережье",
                                "target_lang_sense_clarification": "полоса земли вдоль моря"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61a37d98-4188-3742-b5c8-1d91c1cafed2",
                    "sense_definition": "The apparent line where the sea meets the sky.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "boundary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sun dipped below the <w>sealine</w>, painting the sky red.",
                            "target_lang_translations": {
                                "ru": "Солнце опустилось за <w>морской горизонт</w>, окрасив небо в красный цвет."
                            }
                        },
                        {
                            "text": "A lone ship appeared on the <w>sealine</w> just before dusk.",
                            "target_lang_translations": {
                                "ru": "Одинокий корабль появился на <w>горизонте</w> прямо перед сумерками."

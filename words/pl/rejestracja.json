            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e45cea7-b9f0-3b45-b75f-f34bbe8bb696",
                    "sense_definition": "Miejsce w instytucji (np. w przychodni, szpitalu, urzędzie lub hotelu), w którym dokonuje się zgłoszenia, zapisu na wizytę lub przyjęcia interesantów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_service",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Główna <w>rejestracja</w> znajduje się na parterze, tuż przy wejściu do budynku.",
                            "target_lang_translations": {
                                "ru": "Главная <w>регистратура</w> находится на первом этаже, прямо у входа в здание."
                            }
                        },
                        {
                            "text": "Proszę podejść do <w>rejestracji</w>, aby odebrać wyniki badań.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подойдите в <w>регистратуру</w>, чтобы забрать результаты анализов."
                            }
                        }
                    ],
                    "synonyms": [
                        "rejestratornia"
                    ],
                    "common_phrases": [
                        "podejść do rejestracji",
                        "rejestracja czynna",
                        "pracownik rejestracji",
                        "rejestracja pacjentów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место в учреждении (например, в поликлинике, больнице или отеле), где производится запись на прием или прием посетителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регистратура",
                                "target_lang_sense_clarification": "в медицинском учреждении"
                            },
                            {
                                "target_lang_word": "стойка регистрации",
                                "target_lang_sense_clarification": "в отеле или аэропорту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc4c3e24-46fa-3c0a-a8a1-d61f82c76a9c",
                    "sense_definition": "Czynność polegająca na wpisaniu kogoś lub czegoś do oficjalnego spisu, bazy danych lub wykazu w celu uzyskania uprawnień lub potwierdzenia obecności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "act_of_registering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Internetowa <w>rejestracja</w> na konferencję kończy się w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Онлайн-<w>регистрация</w> на конференцию заканчивается на следующей неделе."
                            }
                        },
                        {
                            "text": "Do założenia konta w tym serwisie wymagana jest pełna <w>rejestracja</w> użytkownika.",
                            "target_lang_translations": {
                                "ru": "Для создания учетной записи на этом сервисе требуется полная <w>регистрация</w> пользователя."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapisy",
                        "ewidencjonowanie",
                        "rejestrowanie"
                    ],
                    "antonyms": [
                        "wyrejestrowanie"
                    ],
                    "common_phrases": [

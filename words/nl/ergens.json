{
    "word_family": [
        "nergens"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3e3b831f-38d7-3342-8a6a-8b2179aa185b",
                    "sense_definition": "Op een plaats die niet precies genoemd wordt of niet bekend is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb mijn sleutels <w>ergens</w> in de keuken laten liggen.",
                            "target_lang_translations": {
                                "en": "I left my keys <w>somewhere</w> in the kitchen.",
                                "ru": "Я оставил свои ключи <w>где-то</w> на кухне."
                            }
                        },
                        {
                            "text": "Zullen we <w>ergens</w> anders een kopje koffie gaan drinken?",
                            "target_lang_translations": {
                                "en": "Shall we go have a cup of coffee <w>somewhere</w> else?",
                                "ru": "Может, выпьем кофе <w>где-нибудь</w> в другом месте?"
                            }
                        }
                    ],
                    "antonyms": [
                        "nergens"
                    ],
                    "common_phrases": [
                        "ergens anders",
                        "ergens wonen",
                        "ergens vandaan"
                    ],
                    "target_lang_definitions": {
                        "en": "At a place that is not precisely named or is unknown.",
                        "ru": "В каком-то месте, которое точно не названо или неизвестно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "somewhere",
                                "target_lang_sense_clarification": "unspecified location"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "где-то",
                                "target_lang_sense_clarification": "в неопределенном месте"
                            },
                            {
                                "target_lang_word": "где-нибудь",
                                "target_lang_sense_clarification": "в любом месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3a41988-a88b-371e-8e11-7b16e9b30a4d",
                    "sense_definition": "Vorm van het woord 'iets' wanneer het samen met een voorzetsel wordt gebruikt (voornaamwoordelijk bijwoord).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pronominal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kan ik je misschien <w>ergens</w> mee helpen?",
                            "target_lang_translations": {
                                "en": "Can I perhaps help you with <w>something</w>?",
                                "ru": "Могу я тебе <w>чем-нибудь</w> помочь?"
                            }
                        },
                        {
                            "text": "Hij is <w>ergens</w> heel erg bang voor.",
                            "target_lang_translations": {
                                "en": "He is very afraid of <w>something</w>.",
                                "ru": "Он <w>чего-то</w> очень сильно боится."
                            }
                        }
                    ],
                    "antonyms": [
                        "nergens"
                    ],
                    "common_phrases": [
                        "ergens mee",
                        "ergens over",
                        "ergens aan",
                        "ergens voor"
                    ],
                    "target_lang_definitions": {
                        "en": "Form of the word 'something' when used with a preposition.",
                        "ru": "Форма местоимения 'что-то', используемая в сочетании с предлогом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "something",
                                "target_lang_sense_clarification": "used with prepositions (e.g., 'ergens mee' -> 'with something')"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "что-то",
                                "target_lang_sense_clarification": "неопределенный предмет"
                            },
                            {
                                "target_lang_word": "чем-то",
                                "target_lang_sense_clarification": "творительный падеж"
                            },
                            {
                                "target_lang_word": "чего-то",
                                "target_lang_sense_clarification": "родительный падеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acfed0bb-0eb5-377a-a308-d4c74d6c69a2",
                    "sense_definition": "In zeker opzicht of op een onbestemde manier; vaak gebruikt om een gevoel of mening te nuanceren.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "modal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ergens</w> begrijp ik wel waarom hij zo boos reageerde.",
                            "target_lang_translations": {
                                "en": "<w>In a way</w> I do understand why he reacted so angrily.",
                                "ru": "<w>Где-то</w> я понимаю, почему он так сердито отреагировал."
                            }
                        },
                        {
                            "text": "Ik vind het <w>ergens</w> toch wel jammer dat we niet zijn gegaan.",
                            "target_lang_translations": {
                                "en": "I find it <w>somehow</w> a pity that we didn't go.",
                                "ru": "Мне <w>отчасти</w> всё же жаль, что мы не пошли."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ergens wel",
                        "ergens ook"
                    ],
                    "target_lang_definitions": {
                        "en": "In a certain respect or in an indefinite way; often used to nuance a feeling or opinion.",
                        "ru": "В некотором смысле или неопределенным образом; часто используется для смягчения мнения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "somehow",
                                "target_lang_sense_clarification": "in an indefinite way"
                            },
                            {
                                "target_lang_word": "in a way",
                                "target_lang_sense_clarification": "to a certain extent"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "где-то",
                                "target_lang_sense_clarification": "переносное значение: в каком-то смысле"
                            },
                            {
                                "target_lang_word": "в чём-то",
                                "target_lang_sense_clarification": "в некотором отношении"
                            },
                            {
                                "target_lang_word": "отчасти",
                                "target_lang_sense_clarification": "частично"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "2515835c-346d-3978-9706-ebd7a2176224",
                    "sense_definition": "De staat of eigenschap van vorstelijk zijn; een indrukwekkende, koninklijke waardigheid.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dignity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koningin straalde een indrukwekkende <w>vorstelijkheid</w> uit tijdens de ceremonie.",
                            "target_lang_translations": {
                                "en": "The queen radiated an impressive <w>regality</w> during the ceremony.",
                                "ru": "Королева излучала впечатляющую <w>царственность</w> во время церемонии."
                            }
                        },
                        {
                            "text": "Ondanks haar eenvoudige kleding behield ze haar natuurlijke <w>vorstelijkheid</w>.",
                            "target_lang_translations": {
                                "en": "Despite her simple clothing, she retained her natural <w>majesty</w>.",
                                "ru": "Несмотря на простую одежду, она сохраняла свою природную <w>величественность</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "majesteit",
                        "grandeur",
                        "waardigheid"
                    ],
                    "antonyms": [
                        "nederigheid"
                    ],
                    "common_phrases": [
                        "natuurlijke vorstelijkheid",
                        "uitstraling van vorstelijkheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The state or quality of being princely; an impressive, royal dignity.",
                        "ru": "Состояние или качество, присущее монарху; впечатляющее королевское достоинство."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "regality",
                                "target_lang_sense_clarification": "quality of being regal or royal"
                            },
                            {
                                "target_lang_word": "majesty",
                                "target_lang_sense_clarification": "impressive dignity or stateliness"
                            },
                            {
                                "target_lang_word": "princeliness",
                                "target_lang_sense_clarification": "state of being princely"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "царственность",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "03ba2b5e-fe02-31ae-8fb7-0e11bc5834df",
                    "sense_definition": "Zauważyć wystąpienie jakiegoś zjawiska, faktu lub zmiany w wyniku uważnego przyglądania się lub badania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "observation_fact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz <zaobserwował> znaczną poprawę stanu zdrowia pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врач <отметил> значительное улучшение состояния здоровья пациента."
                            }
                        },
                        {
                            "text": "W ostatnich latach <zaobserwowano> wzrost średniej temperatury.",
                            "target_lang_translations": {
                                "ru": "В последние годы <был зафиксирован> рост средней температуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "odnotować",
                        "stwierdzić",
                        "zauważyć"
                    ],
                    "antonyms": [
                        "przeoczyć"
                    ],
                    "common_phrases": [
                        "zaobserwować zmiany",
                        "zaobserwować zjawisko",
                        "zaobserwować spadek",
                        "zaobserwować wzrost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заметить появление какого-либо явления, факта или изменения в результате внимательного наблюдения или исследования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зафиксировать",
                                "target_lang_sense_clarification": "отметить наличие факта или изменения (формально)"
                            },
                            {
                                "target_lang_word": "отметить",
                                "target_lang_sense_clarification": "констатировать факт"
                            },
                            {
                                "target_lang_word": "заметить",
                                "target_lang_sense_clarification": "обратить внимание на что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21ff211f-4a0e-36b9-8f3d-0fd94fc0ec8a",
                    "sense_definition": "Dostrzec kogoś lub coś wzrokiem; zobaczyć coś, co działo się przez pewien czas.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "observation_visual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Udało mi się <zaobserwować> rzadki gatunek ptaka w lesie.",
                            "target_lang_translations": {
                                "ru": "Мне удалось <заметить> редкий вид птицы в лесу."
                            }
                        },
                        {
                            "text": "Policjant <zaobserwował> podejrzane zachowanie mężczyzny.",
                            "target_lang_translations": {
                                "ru": "Полицейский <заметил> подозрительное поведение мужчины."
                            }
                        }
                    ],
                    "synonyms": [
                        "dostrzec",
                        "zobaczyć",
                        "ujrzeć"
                    ],
                    "antonyms": [
                        "przeoczyć"
                    ],
                    "common_phrases": [
                        "zaobserwować ruch",
                        "zaobserwować obiekt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увидеть кого-то или что-то; заметить что-то, происходящее в течение некоторого времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заметить",
                                "target_lang_sense_clarification": "обнаружить взглядом"
                            },
                            {
                                "target_lang_word": "увидеть",
                                "target_lang_sense_clarification": "воспринять зрением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1f9d414-9f17-38ba-a6df-7201c6d583d5",
                    "sense_definition": "Zacząć śledzić aktywność konta w mediach społecznościowych, aby widzieć jego publikacje.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "social_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <zaobserwować> ten profil, jeśli interesujesz się modą.",
                            "target_lang_translations": {
                                "ru": "Ты должен <подписаться> на этот профиль, если интересуешься модой."
                            }
                        },
                        {
                            "text": "Wczoraj <zaobserwowałem> mojego ulubionego aktora na Instagramie.",
                            "target_lang_translations": {
                                "ru": "Вчера я <подписался> на моего любимого актера в Инстаграме."
                            }
                        }
                    ],
                    "synonyms": [
                        "zasubskrybować"
                    ],
                    "antonyms": [
                        "odobserwować"
                    ],
                    "common_phrases": [
                        "zaobserwować profil",
                        "zaobserwować konto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать следить за активностью аккаунта в социальных сетях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписаться",
                                "target_lang_sense_clarification": "на аккаунт или профиль"
                            }
                        ]
                    }
                }
            ]
        }
    ]

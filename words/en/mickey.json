                        {
                            "text": "They spent the afternoon trying to catch a stray <w>mickey</w> in the bush.",
                            "target_lang_translations": {
                                "ru": "Они провели день, пытаясь поймать отбившегося <w>бычка</w> в буше."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Australia"
                        }
                    ],
                    "target_lang_definitions": {

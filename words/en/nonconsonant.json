                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несогласный звук",
                                "target_lang_sense_clarification": "лингвистический термин"
                            },
                            {
                                "target_lang_word": "гласный",
                                "target_lang_sense_clarification": "как противоположность согласному"

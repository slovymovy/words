    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7ccbfbbb-5b1b-34a0-89b2-092f40f9f90d",
                    "sense_definition": "Op die plaats; niet hier.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn ouders wonen <w>daar</w> in dat witte huis.",
                            "target_lang_translations": {
                                "en": "My parents live <w>there</w> in that white house.",
                                "ru": "Мои родители живут <w>там</w>, в том белом доме."
                            }
                        },
                        {
                            "text": "Leg je jas maar <w>daar</w> op de stoel neer.",
                            "target_lang_translations": {
                                "en": "Just put your coat down <w>there</w> on the chair.",
                                "ru": "Положи куртку <w>туда</w> на стул."
                            }
                        },
                        {
                            "text": "Kijk, <w>daar</w> komt de bus al aan.",
                            "target_lang_translations": {
                                "en": "Look, <w>there</w> comes the bus already.",
                                "ru": "Смотри, <w>вон</w> уже едет автобус."
                            }
                        }
                    ],
                    "synonyms": [
                        "ginds",
                        "ginder",
                        "daarginds"
                    ],
                    "antonyms": [
                        "hier"
                    ],
                    "common_phrases": [
                        "hier en daar",
                        "daar zo",
                        "daar ginds"
                    ],
                    "target_lang_definitions": {
                        "en": "At that place; not here.",
                        "ru": "В том месте; не здесь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "there",
                                "target_lang_sense_clarification": "location away from speaker"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "там",
                                "target_lang_sense_clarification": "местоположение"
                            },
                            {
                                "target_lang_word": "вон",
                                "target_lang_sense_clarification": "указание на место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "082c8a95-64e4-375e-9848-433f282f2ddc",
                    "sense_definition": "Verwijst naar iets dat eerder genoemd is of aangewezen wordt, vaak in combinatie met een voorzetsel (vervangt 'dat' + voorzetsel).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pronominal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Daar</w> ben ik het helemaal mee eens.",
                            "target_lang_translations": {
                                "en": "I agree with <w>that</w> completely.",
                                "ru": "Я с <w>этим</w> полностью согласен."
                            }
                        },
                        {
                            "text": "Ik heb <w>daar</w> op dit moment geen tijd voor.",
                            "target_lang_translations": {
                                "en": "I don't have time for <w>that</w> at the moment.",
                                "ru": "У меня сейчас нет на <w>это</w> времени."
                            }
                        },
                        {
                            "text": "We hebben <w>daar</w> gisteren nog over gesproken.",
                            "target_lang_translations": {
                                "en": "We spoke about <w>that</w> just yesterday.",
                                "ru": "Мы об <w>этом</w> только вчера говорили."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to something previously mentioned or pointed out, often used in combination with a preposition (replaces 'that' + preposition).",
                        "ru": "Указывает на что-то, упомянутое ранее или на что указывают; часто используется в сочетании с предлогом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "reference to a specific thing or situation"
                            },
                            {
                                "target_lang_word": "it",
                                "target_lang_sense_clarification": "reference to an object or abstract concept"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "это",
                                "target_lang_sense_clarification": "ссылка на ситуацию или объект"
                            },
                            {
                                "target_lang_word": "то",
                                "target_lang_sense_clarification": "ссылка на удаленный объект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7eb58163-7f36-3325-a9d5-382895ffee05",
                    "sense_definition": "Wordt gebruikt om nadruk te leggen in een zin of als opvulwoord, zonder letterlijke betekenis van plaats.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "modal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat sta je <w>daar</w> nu te dromen?",
                            "target_lang_translations": {
                                "en": "What are you standing <w>there</w> dreaming?",
                                "ru": "Чего ты <w>там</w> стоишь и мечтаешь?"
                            }
                        },
                        {
                            "text": "Dat doet <w>daar</w> verder niets aan af.",
                            "target_lang_translations": {
                                "en": "That doesn't detract from <w>it</w> at all.",
                                "ru": "Это ничего в <w>этом</w> не меняет."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to add emphasis or as a filler word, often without a literal meaning of place.",
                        "ru": "Используется для усиления или как слово-паразит, часто без буквального значения места."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "there",
                                "target_lang_sense_clarification": "emphatic presence"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "там",
                                "target_lang_sense_clarification": "эмоциональное усиление"
                            },
                            {
                                "target_lang_word": "тут",
                                "target_lang_sense_clarification": "разговорное усиление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "3a1281ff-422a-3713-a694-29a3b63cac47",
                    "sense_definition": "Omdat; geeft een reden of oorzaak aan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Daar</w> hij ziek was, kon hij niet naar het feest komen.",
                            "target_lang_translations": {
                                "en": "<w>Since</w> he was sick, he could not come to the party.",
                                "ru": "<w>Так как</w> он был болен, он не смог прийти на вечеринку."
                            }
                        },
                        {
                            "text": "We bleven thuis, <w>daar</w> het buiten hard regende.",
                            "target_lang_translations": {
                                "en": "We stayed home, <w>as</w> it was raining hard outside.",
                                "ru": "Мы остались дома, <w>поскольку</w> на улице шел сильный дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "omdat",
                        "aangezien",
                        "vermits"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formeel"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Because; indicates a reason or cause.",
                        "ru": "Так как; указывает на причину или основание."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "since",
                                "target_lang_sense_clarification": "indicating a reason"
                            },
                            {
                                "target_lang_word": "as",
                                "target_lang_sense_clarification": "indicating a reason"
                            },
                            {
                                "target_lang_word": "because",
                                "target_lang_sense_clarification": "indicating a cause"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "так как",
                                "target_lang_sense_clarification": "союз причины"
                            },
                            {
                                "target_lang_word": "поскольку",
                                "target_lang_sense_clarification": "союз причины"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "79bd2133-b0fc-3390-8df1-8615b3ced952",
                    "sense_definition": "Większa liczba przedmiotów ułożonych jeden na drugim.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W zlewie piętrzył się <w>stos</w> brudnych talerzy.",
                            "target_lang_translations": {
                                "ru": "В раковине громоздилась <w>гора</w> грязных тарелок."
                            }
                        },
                        {
                            "text": "Muszę w końcu uporządkować ten wielki <w>stos</w> dokumentów na biurku.",
                            "target_lang_translations": {
                                "ru": "Мне нужно наконец разобрать эту огромную <w>кипу</w> документов на столе."
                            }
                        }
                    ],
                    "synonyms": [
                        "sterta",
                        "góra",
                        "kupa"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ed6350e0-98d2-30a7-9ace-70acbe13f367",
                    "sense_definition": "Tytuł arystokratyczny lub wojskowy używany dawniej w krajach muzułmańskich, zwłaszcza u Tatarów, Turków i Persów, oznaczający wodza lub księcia; także osoba nosząca ten tytuł.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "titles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Został więc <w>murzą</w> znacznym i wielu wojowników słuchało jego rozkazów.",
                            "target_lang_translations": {
                                "ru": "Так он стал знатным <w>мурзой</w>, и многие воины слушались его приказов."
                            }
                        },
                        {
                            "text": "Turecki <w>murza</w> przybył do obozu, aby negocjować warunki pokoju.",
                            "target_lang_translations": {
                                "ru": "Турецкий <w>мурза</w> прибыл в лагерь, чтобы обсудить условия мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "mirza"
                    ],
                    "common_phrases": [
                        "tatarski murza",
                        "wielki murza"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аристократический или военный титул, использовавшийся ранее в мусульманских странах, особенно у татар, турок и персов; также человек, носящий этот титул."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мурза",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1ef282a-aa4c-348d-9de2-8925d69d5887",
                    "sense_definition": "Osoba małomówna, ponura, stroniąca od ludzi i niechętna do rozmów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Siedział w kącie jak <w>murza</w> i do nikogo się nie odzywał przez cały wieczór.",
                            "target_lang_translations": {
                                "ru": "Он сидел в углу как <w>бирюк</w> и ни с кем не разговаривал весь вечер."
                            }
                        },
                        {
                            "text": "Nie bądź takim <w>murzą</w>, uśmiechnij się i porozmawiaj z gośćmi.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>букой</w>, улыбнись и поговори с гостями."
                            }
                        }
                    ],
                    "synonyms": [
                        "mruk"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неразговорчивый, угрюмый человек, сторонящийся людей и неохотно вступающий в беседу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бирюк",
                                "target_lang_sense_clarification": "нелюдимый человек"
                            },
                            {
                                "target_lang_word": "бука",
                                "target_lang_sense_clarification": "угрюмый человек"
                            },
                            {
                                "target_lang_word": "молчун",
                                "target_lang_sense_clarification": "неразговорчивый человек"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
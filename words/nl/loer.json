                {
                    "sense_id": "20f19631-1a6c-303d-ad12-818f0e045d6f",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>loeren</w>",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>loer</w> voorzichtig door het sleutelgat naar de gang.",

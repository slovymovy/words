            "senses": [
                {
                    "sense_id": "797d108e-0362-30f6-b17f-35d2f0472d8a",
                    "sense_definition": "Fikcyjna istota, będąca ożywionym trupem pozbawionym własnej woli i duszy, często występująca w horrorach i kulturze popularnej jako postać atakująca żywych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "undead_creature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W najnowszym filmie bohaterowie muszą uciekać przed hordą krwiożerczych <w>zombie</w>."
                        },
                        {
                            "text": "Wiele gier komputerowych polega na walce o przetrwanie w świecie opanowanym przez <w>zombie</w>."
                        }
                    ],
                    "synonyms": [
                        "żywy trup",
                        "zombiak",
                        "umarlak"
                    ],
                    "common_phrases": [
                        "apokalipsa zombie",
                        "atak zombie",
                        "film o zombie"
                    ]
                },
                {
                    "sense_id": "6defad6b-b54f-359d-8d02-1a02638849b8",
                    "sense_definition": "Komputer podłączony do internetu, który został przejęty przez hakera lub złośliwe oprogramowanie i jest wykorzystywany do szkodliwych działań bez wiedzy właściciela.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "compromised_computer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hakerzy wykorzystali sieć komputerów <w>zombie</w> do przeprowadzenia ataku na serwery banku."
                        },
                        {
                            "text": "Jeśli twój komputer działa bardzo wolno, może to oznaczać, że stał się on <w>zombie</w> w botnecie."
                        }
                    ],
                    "common_phrases": [
                        "sieć zombie",
                        "komputer zombie"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "informatyczny"
                        }
                    ]
                },
                {
                    "sense_id": "3853d7a1-1464-348f-9fc1-9c5283471817",
                    "sense_definition": "Proces w systemie operacyjnym (głównie Unix/Linux), który zakończył swoje działanie, ale którego status nie został jeszcze odebrany przez proces macierzysty, przez co nadal widnieje w tablicy procesów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "os_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Administrator systemu musiał ręcznie usunąć proces <w>zombie</w>, który blokował zasoby."
                        },
                        {
                            "text": "Zbyt duża liczba procesów typu <w>zombie</w> może świadczyć o błędzie w działaniu aplikacji."
                        }
                    ],
                    "common_phrases": [
                        "proces zombie"
                    ],
                    "traits": [
                        {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f39396c-b9bb-325f-91ad-89bff2af309f",
                    "sense_definition": "Помещать, класть что-либо внутрь чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я аккуратно <вкладываю> письмо в почтовый конверт.",
                            "target_lang_translations": {
                                "en": "I carefully <put> the letter <into> the envelope."
                            }
                        },
                        {
                            "text": "Рыцарь медленно <вкладывал> свой тяжёлый меч в ножны.",
                            "target_lang_translations": {
                                "en": "The knight slowly <sheathed> his heavy sword."
                            }
                        }
                    ],
                    "synonyms": [
                        "помещать",
                        "класть"
                    ],
                    "antonyms": [
                        "вынимать"
                    ],
                    "common_phrases": [
                        "вкладывать в конверт",
                        "вкладывать в коробку",
                        "вкладывать меч-в-ножны"
                    ],
                    "target_lang_definitions": {
                        "en": "To place or put something inside something else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put in",
                                "target_lang_sense_clarification": "general physical action"
                            },
                            {
                                "target_lang_word": "insert",
                                "target_lang_sense_clarification": "more formal or precise placement"
                            },
                            {
                                "target_lang_word": "enclose",
                                "target_lang_sense_clarification": "specifically for letters or documents"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c76d399e-49f2-3a51-821f-ba12f53b3063",
                    "sense_definition": "Помещать деньги или ценности в дело, предприятие или банк с целью получения прибыли.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бизнесмен решил <вкладывать> деньги в развитие новых технологий.",
                            "target_lang_translations": {
                                "en": "The businessman decided to <invest> money in the development of new technologies."
                            }
                        },
                        {
                            "text": "Не стоит <вкладывать> все свои сбережения в один рискованный проект.",
                            "target_lang_translations": {
                                "en": "You shouldn't <invest> all your savings in one risky project."
                            }
                        }
                    ],
                    "synonyms": [
                        "инвестировать"
                    ],
                    "antonyms": [
                        "изымать"
                    ],
                    "common_phrases": [
                        "вкладывать деньги",
                        "вкладывать капитал",
                        "вкладывать средства"
                    ],
                    "target_lang_definitions": {
                        "en": "To place money or assets into a business or bank to make a profit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "invest",
                                "target_lang_sense_clarification": "financial context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33b3dc65-ddff-31ed-b068-ee6331034d8a",
                    "sense_definition": "Отдавать силы, труд, знания или чувства какому-либо делу.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_contribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель старается <вкладывать> душу в каждый свой урок.",
                            "target_lang_translations": {
                                "en": "The teacher tries to <put> his soul <into> every lesson."
                            }
                        },
                        {
                            "text": "Спортсмены должны <вкладывать> много сил в ежедневные тренировки.",
                            "target_lang_translations": {
                                "en": "Athletes must <put> a lot of effort <into> daily training."
                            }
                        }
                    ],
                    "synonyms": [
                        "отдавать",
                        "посвящать"
                    ],
                    "common_phrases": [
                        "вкладывать душу",
                        "вкладывать силы",
                        "вкладывать труд"
                    ],
                    "target_lang_definitions": {
                        "en": "To devote effort, labor, or emotion to a cause or activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put into",
                                "target_lang_sense_clarification": "idiomatic for effort or feelings"
                            },
                            {
                                "target_lang_word": "invest",
                                "target_lang_sense_clarification": "figurative use regarding time or energy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b8f9fb2-f472-3088-80d3-dd4036ae9667",
                    "sense_definition": "Придавать чему-либо определённый смысл или значение.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый читатель может <вкладывать> свой собственный смысл в это стихотворение.",
                            "target_lang_translations": {
                                "en": "Every reader can <read> their own meaning <into> this poem."
                            }
                        },
                        {
                            "text": "Не нужно <вкладывать> в мои слова то значение, которого там нет.",
                            "target_lang_translations": {
                                "en": "You shouldn't <read> a meaning <into> my words that isn't there."
                            }
                        }
                    ],
                    "synonyms": [
                        "придавать"
                    ],
                    "common_phrases": [
                        "вкладывать смысл",
                        "вкладывать значение",
                        "вкладывать понятие"
                    ],
                    "target_lang_definitions": {
                        "en": "To attribute a specific meaning or significance to something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "read into",
                                "target_lang_sense_clarification": "to perceive a meaning that may not be obvious"
                            },
                            {
                                "target_lang_word": "attribute",
                                "target_lang_sense_clarification": "to assign a quality or meaning"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
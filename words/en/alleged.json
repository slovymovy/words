                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prosecution <w>alleged</w> that the defendant was present at the scene.",
                            "target_lang_translations": {

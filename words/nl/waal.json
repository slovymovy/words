                {
                    "sense_id": "cd254b4a-65b9-3f5f-bf04-861ba3947ac6",
                    "sense_definition": "De belangrijkste zuidelijke arm van de rivier de Rijn die door Nederland stroomt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "river_waal",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "De stad Nijmegen ligt aan de zuidoever van de <w>Waal</w>.",
                            "target_lang_translations": {
                                "en": "The city of Nijmegen is located on the south bank of the <w>Waal</w>.",
                                "ru": "Город Неймеген расположен на южном берегу <w>Ваала</w>."
                            }
                        },
                        {
                            "text": "Er varen dagelijks honderden vrachtschepen over de <w>Waal</w>.",
                            "target_lang_translations": {
                                "en": "Hundreds of cargo ships sail on the <w>Waal</w> every day.",
                                "ru": "Ежедневно по <w>Ваалу</w> проходят сотни грузовых судов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "over de Waal",
                        "aan de Waal"
                    ],
                    "target_lang_definitions": {
                        "en": "The main southern branch of the Rhine river flowing through the Netherlands.",
                        "ru": "Главный южный рукав реки Рейн, протекающий через Нидерланды."
                    },
                    "translations": {

                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run through",
                                "target_lang_sense_clarification": "a rehearsal or process"
                            },
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "a computer program or test"
                            }
                        ]
                    }
                }
            ]
        }

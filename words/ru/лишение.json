    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ef59d263-a9ec-3542-8cd2-dfac54827491",
                    "sense_definition": "Действие по глаголу <w>лишать</w>; принудительное изъятие или отнятие чего-либо (прав, свободы, имущества).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За серьёзное нарушение правил дорожного движения водителю грозит <лишение> прав.",
                            "target_lang_translations": {
                                "en": "For a serious traffic violation, the driver faces license <revocation>."
                            }
                        },
                        {
                            "text": "Суд вынес приговор, предусматривающий <лишение> свободы на три года.",
                            "target_lang_translations": {
                                "en": "The court handed down a sentence involving <deprivation> of liberty for three years."
                            }
                        }
                    ],
                    "synonyms": [
                        "отнятие",
                        "изъятие",
                        "отъём"
                    ],
                    "antonyms": [
                        "наделение",
                        "снабжение",
                        "придание"
                    ],
                    "common_phrases": [
                        "лишение свободы",
                        "лишение прав",
                        "лишение родительских прав",
                        "лишение имущества"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of depriving someone of something; the forced removal or taking away of something (rights, freedom, property)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deprivation",
                                "target_lang_sense_clarification": "general act of taking something away (e.g., liberty, sleep)"
                            },
                            {
                                "target_lang_word": "revocation",
                                "target_lang_sense_clarification": "official cancellation of rights or licenses"
                            },
                            {
                                "target_lang_word": "forfeiture",
                                "target_lang_sense_clarification": "loss of property or rights as a penalty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e471434c-735c-3edd-b892-fb282a34a8c0",
                    "sense_definition": "Жизненная трудность, нужда, отсутствие самого необходимого для существования.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Во время долгого путешествия исследователям пришлось переносить голод и <лишения>.",
                            "target_lang_translations": {
                                "en": "During the long journey, the researchers had to endure hunger and <privations>."
                            }
                        },
                        {
                            "text": "Они стойко терпели все <лишения> военного времени ради победы.",
                            "target_lang_translations": {
                                "en": "They steadfastly endured all the <hardships> of wartime for the sake of victory."
                            }
                        }
                    ],
                    "common_phrases": [
                        "терпеть лишения",
                        "переносить лишения",
                        "годы лишений"
                    ],
                    "target_lang_definitions": {
                        "en": "A life difficulty, need, or the absence of essentials required for existence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "privation",
                                "target_lang_sense_clarification": "lack of basic necessities"
                            },
                            {
                                "target_lang_word": "hardship",
                                "target_lang_sense_clarification": "severe suffering or difficulty"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "6fb6e683-bc19-3d46-945a-ae43a5a23996",
                    "sense_definition": "Used in titles to indicate a family relationship that is one generation further removed.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "family_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>great</w> grandfather moved to this country over a hundred years ago.",
                            "target_lang_translations": {
                                "ru": "Мой <w>пра</w>дедушка переехал в эту страну более ста лет назад."
                            }
                        },
                        {
                            "text": "She is knitting a sweater for her new <w>great</w> niece.",
                            "target_lang_translations": {
                                "ru": "Она вяжет свитер для своей новорожденной <w>внучатой</w> племянницы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "great-grandmother",
                        "great-grandfather",
                        "great-grandchild",
                        "great-aunt"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "used as a prefix in family terms"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в названиях родства для обозначения поколения, отстоящего на один шаг дальше."

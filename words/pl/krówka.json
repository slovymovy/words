    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a05acf8b-2df3-3d22-8d23-ca5d54ff3e6e",
                    "sense_definition": "Zdrobnienie od słowa <w>krowa</w>; małe zwierzę hodowlane dające mleko lub pieszczotliwe określenie krowy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zielonej łące pasła się mała, łaciata <w>krówka</w>.",
                            "target_lang_translations": {
                                "ru": "На зеленом лугу паслась маленькая пятнистая <w>коровка</w>."
                            }
                        },
                        {
                            "text": "Dziecko narysowało w swoim zeszycie wesołą <w>krówkę</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок нарисовал в своей тетради веселую <w>коровку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cieliczka",
                        "jałówka",
                        "krasula"
                    ],
                    "common_phrases": [
                        "mała krówka",
                        "łaciata krówka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительно-ласкательная форма слова «krowa» (корова); маленькое животное или ласковое обращение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коровка",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            },
                            {
                                "target_lang_word": "коровушка",
                                "target_lang_sense_clarification": "ласковое обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df9bfd50-50e5-3ab6-bb94-6630d78d63f0",
                    "sense_definition": "Rodzaj popularnego w Polsce cukierka mlecznego, który może być ciągnący w środku lub kruchy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sweets",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem paczkę słodkich <krówek> dla dzieci na deser.",
                            "target_lang_translations": {
                                "ru": "Я купил пачку сладких <w>коровок</w> детям на десерт."
                            }
                        },
                        {
                            "text": "Ta <krówka> ciągutka strasznie klei się do zębów.",
                            "target_lang_translations": {
                                "ru": "Эта <w>коровка</w>-тянучка ужасно липнет к зубам."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciągutka",
                        "mordoklejka",
                        "toffi"
                    ],
                    "common_phrases": [
                        "krówka ciągutka",
                        "krówka mleczna",
                        "pyszna krówka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид популярных молочных конфет (ирис), которые могут быть тягучими или рассыпчатыми внутри."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коровка",
                                "target_lang_sense_clarification": "конфета"
                            },
                            {
                                "target_lang_word": "ирис",
                                "target_lang_sense_clarification": "тип конфет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1dedd38c-0d99-3c58-913b-6f477c96b0b9",
                    "sense_definition": "Rodzaj ciasta przekładanego słodką masą kajmakową o smaku toffi.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sweets",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na niedzielny podwieczorek mama upiekła domową <krówkę>.",
                            "target_lang_translations": {
                                "ru": "К воскресному полднику мама испекла домашнюю <w>коровку</w>."
                            }
                        },
                        {
                            "text": "Do gorzkiej kawy idealnie pasuje słodka <krówka>.",
                            "target_lang_translations": {
                                "ru": "К горькому кофе идеально подходит сладкая <w>коровка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciastko toffi"
                    ],
                    "common_phrases": [
                        "ciasto krówka",
                        "kawałek krówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид торта или пирожного, прослоенного сладкой массой из вареной сгущенки (каймака)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торт «Коровка»",
                                "target_lang_sense_clarification": "название торта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ae1fd4a-2839-3222-bf96-7b1abb95bf55",
                    "sense_definition": "Regionalne lub potoczne określenie małego chrząszcza w kropki, znanego jako biedronka.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na liściu usiadła mała <krówka> i rozłożyła skrzydełka.",
                            "target_lang_translations": {
                                "ru": "На листок села маленькая <w>божья коровка</w> и расправила крылышки."
                            }
                        },
                        {
                            "text": "Dzieci często wołają na tego owada boża <krówka>.",
                            "target_lang_translations": {
                                "ru": "Дети часто называют это насекомое <w>божья коровка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "boża krówka"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное или разговорное название маленького жука в крапинку (божьей коровки)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "божья коровка",
                                "target_lang_sense_clarification": "насекомое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef748e87-cfd7-3c12-aca9-1d7a37dda6c3",
                    "sense_definition": "Potoczne określenie dużej butelki wódki, zazwyczaj o pojemności 0,7 litra lub większej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na stole podczas wesela pojawiła się kolejna <krówka>.",
                            "target_lang_translations": {
                                "ru": "На столе во время свадьбы появился очередной <w>пузырь</w>."
                            }
                        },
                        {
                            "text": "Kupili jedną <krówkę> na całą imprezę urodzinową.",
                            "target_lang_translations": {
                                "ru": "Они купили одну <w>бутылку 0,7</w> на всю вечеринку в честь дня рождения."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сленговое название большой бутылки водки, обычно объемом 0,7 литра или больше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пузырь",
                                "target_lang_sense_clarification": "сленг, бутылка"
                            },
                            {
                                "target_lang_word": "ноль-семь",
                                "target_lang_sense_clarification": "объем бутылки"
                            }
                        ]
                    }
                }
            ]
        }

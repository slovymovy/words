    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e29b45c9-4413-33db-a985-56a96252d495",
                    "sense_definition": "отпущение вины, освобождение от заслуженного наказания или отказ от мести за обиду",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я искренне хочу попросить у тебя <w>прощения</w> за своё грубое поведение.",
                            "target_lang_translations": {
                                "en": "I sincerely want to ask for your <w>forgiveness</w> for my rude behavior."
                            }
                        },
                        {
                            "text": "Она заслужила <w>прощение</w> своим долгим и честным трудом.",
                            "target_lang_translations": {
                                "en": "She earned <w>forgiveness</w> through her long and honest work."
                            }
                        }
                    ],
                    "synonyms": [
                        "извинение",
                        "прощенье"
                    ],
                    "antonyms": [
                        "месть"
                    ],
                    "common_phrases": [
                        "просить прощения",
                        "молить о прощении",
                        "получить прощение",
                        "заслужить прощение"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of forgiving someone for a mistake or offense; the letting go of resentment or anger."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forgiveness",
                                "target_lang_sense_clarification": "general act of forgiving"
                            },
                            {
                                "target_lang_word": "pardon",
                                "target_lang_sense_clarification": "formal forgiveness or excuse"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "915c4035-3f1f-32ab-8e9e-067393013c78",
                    "sense_definition": "официальный акт, отменяющий наказание за проступок или преступление; освобождение от финансовых обязательств",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Президент подписал указ о полном <w>прощении</w> всех политических заключённых.",
                            "target_lang_translations": {
                                "en": "The president signed a decree on the full <w>pardon</w> of all political prisoners."
                            }
                        },
                        {
                            "text": "Банк объявил о частичном <w>прощении</w> долгов для малого бизнеса.",
                            "target_lang_translations": {
                                "en": "The bank announced a partial <w>forgiveness</w> of debts for small businesses."
                            }
                        }
                    ],
                    "synonyms": [
                        "помилование",
                        "помилованье"
                    ],
                    "common_phrases": [
                        "полное прощение",
                        "прощение долга",
                        "прощение грехов"
                    ],
                    "target_lang_definitions": {
                        "en": "An official act that exempts a person from punishment for a crime, or the cancellation of a financial obligation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pardon",
                                "target_lang_sense_clarification": "official release from legal punishment"
                            },
                            {
                                "target_lang_word": "forgiveness",
                                "target_lang_sense_clarification": "cancellation of debts"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "d8252939-d970-32d1-b3ec-4e491f02804a",
                    "sense_definition": "W sporcie: kierować piłkę lub krążek do innego zawodnika ze swojej drużyny.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trener krzyczał, żeby szybciej <podawać> piłkę do napastników.",
                            "target_lang_translations": {
                                "ru": "Тренер кричал, чтобы быстрее <передавали> мяч нападающим."
                            }
                        },
                        {
                            "text": "Ten zawodnik świetnie <podaje> na wolne pole.",
                            "target_lang_translations": {
                                "ru": "Этот игрок отлично <пасует> на свободное поле."
                            }
                        }
                    ],
                    "synonyms": [

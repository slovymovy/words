                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "accompaniment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please bring your sister <w>along</w> to the party.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, возьми сестру <w>с собой</w> на вечеринку."
                            }
                        },
                        {
                            "text": "John played the piano and everyone sang <w>along</w>.",
                            "target_lang_translations": {
                                "ru": "Джон играл на пианино, и все пели <w>вместе</w> с ним."
                            }
                        }
                    ],
                    "synonyms": [
                        "alongst",
                        "endlong"
                    ],
                    "common_phrases": [
                        "come along",
                        "bring along",
                        "sing along",
                        "tag along"
                    ],
                    "target_lang_definitions": {

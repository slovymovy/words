                },
                {
                    "sense_id": "ab4b39e5-cecf-3a17-96eb-dabcf01fdafc",
                    "sense_definition": "Cieszący się szacunkiem i zaufaniem; rzetelny, odpowiedzialny i godny traktowania z uwagą.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "trust",
                    "name_type": "no",
                    "examples": [
                        {

        "gunrunner"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "62062b35-7dcc-3620-b362-c0348480d840",
                    "sense_definition": "To move forward quickly upon two feet by alternately making a short jump off either foot.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_fast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I saw the children <w>run</w> across the field to catch the ball.",
                            "target_lang_translations": {
                                "ru": "Я видел, как дети <w>бежали</w> через поле, чтобы поймать мяч."
                            }
                        },
                        {
                            "text": "You will have to <w>run</w> fast if you want to win the race.",
                            "target_lang_translations": {
                                "ru": "Тебе придется быстро <w>бежать</w>, если хочешь выиграть гонку."
                            }
                        }
                    ],
                    "synonyms": [
                        "gallop"
                    ],
                    "common_phrases": [
                        "run away",
                        "run fast",
                        "run towards"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро передвигаться на ногах, в какой-то момент отрываясь от земли обеими ногами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бежать",
                                "target_lang_sense_clarification": "двигаться быстро в одном направлении или в конкретный момент"
                            },
                            {
                                "target_lang_word": "бегать",
                                "target_lang_sense_clarification": "двигаться быстро (способность или повторяющееся действие)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2d86258-01d6-3e63-a6c0-6057b0c13535",
                    "sense_definition": "To go at a fast pace; to move quickly in order to get somewhere.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_fast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>run</w> to the station or we will miss the train.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>бежать</w> на станцию, иначе мы опоздаем на поезд."
                            }
                        },
                        {
                            "text": "I must <w>run</w> now because I am late for a meeting.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>бежать</w>, потому что опаздываю на встречу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "got to run",
                        "run along"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро двигаться или спешить, чтобы добраться куда-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спешить",
                                "target_lang_sense_clarification": "торопиться"
                            },
                            {
                                "target_lang_word": "бежать",
                                "target_lang_sense_clarification": "быстро перемещаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d9523b7-a642-3b81-aa8d-57cc0903d1fa",
                    "sense_definition": "To control, manage, or be in charge of an organization, business, or system.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "manage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wants to <w>run</w> her own business after she finishes university.",
                            "target_lang_translations": {
                                "ru": "Она хочет <w>вести</w> свой собственный бизнес после окончания университета."
                            }
                        },
                        {
                            "text": "He has the skills to <w>run</w> the department efficiently.",
                            "target_lang_translations": {
                                "ru": "У него есть навыки, чтобы эффективно <w>руководить</w> отделом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "run a business",
                        "run the show",
                        "run a company"
                    ],
                    "target_lang_definitions": {
                        "ru": "Управлять, руководить или отвечать за организацию, бизнес или систему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "контролировать работу"
                            },
                            {
                                "target_lang_word": "руководить",
                                "target_lang_sense_clarification": "быть во главе"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "заниматься делами или бизнесом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2278fead-9d35-3747-9a91-79aad8665202",
                    "sense_definition": "Of a machine, engine, or system, to be operating or working normally.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "operate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My old car does not <w>run</w> very well in the cold winter weather.",
                            "target_lang_translations": {
                                "ru": "Моя старая машина не очень хорошо <w>работает</w> в холодную зимнюю погоду."
                            }
                        },
                        {
                            "text": "Please leave the computer to <w>run</w> overnight to finish the task.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, оставь компьютер <w>работать</w> на ночь, чтобы завершить задачу."
                            }
                        }
                    ],
                    "synonyms": [
                        "start"
                    ],
                    "common_phrases": [
                        "run smoothly",
                        "engine running",
                        "up and running"
                    ],
                    "target_lang_definitions": {
                        "ru": "О машине, двигателе или системе: работать или функционировать нормально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работать",
                                "target_lang_sense_clarification": "функционировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "433492f3-8fff-372e-8548-b179b58e680a",
                    "sense_definition": "Of a liquid, to flow.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tears began to <w>run</w> down her cheeks when she heard the sad news.",
                            "target_lang_translations": {
                                "ru": "Слезы начали <w>течь</w> по ее щекам, когда она услышала печальную новость."
                            }
                        },
                        {
                            "text": "Make sure the water does not <w>run</w> onto the floor.",
                            "target_lang_translations": {
                                "ru": "Убедись, что вода не <w>течет</w> на пол."
                            }
                        }
                    ],
                    "common_phrases": [
                        "run down",
                        "run dry",
                        "run cold"
                    ],
                    "target_lang_definitions": {
                        "ru": "О жидкости: течь, литься."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "литься струей"
                            },
                            {
                                "target_lang_word": "бежать",
                                "target_lang_sense_clarification": "литься (разговорное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70510e52-dee4-3653-b53c-0e130314b5b9",
                    "sense_definition": "Of a means of transportation (like a bus or train), to travel on a specific route or schedule.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The buses <w>run</w> every ten minutes during the rush hour.",
                            "target_lang_translations": {
                                "ru": "Автобусы <w>ходят</w> каждые десять минут в час пик."
                            }
                        },
                        {
                            "text": "Trains to the city do not <w>run</w> late at night.",
                            "target_lang_translations": {
                                "ru": "Поезда в город не <w>ходят</w> поздно ночью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "buses run",
                        "trains run"
                    ],
                    "target_lang_definitions": {
                        "ru": "О транспортном средстве (автобусе, поезде): ходить по определенному маршруту или расписанию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "курсировать по маршруту"
                            },
                            {
                                "target_lang_word": "курсировать",
                                "target_lang_sense_clarification": "двигаться по расписанию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a63da4da-169f-3e5c-8100-5d893a079fa9",
                    "sense_definition": "To execute or carry out a plan, procedure, or computer program.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "execute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>run</w> the software update to fix the security problem.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>запустить</w> обновление программного обеспечения, чтобы исправить проблему безопасности."
                            }
                        },
                        {
                            "text": "The doctor decided to <w>run</w> some tests to check my health.",
                            "target_lang_translations": {
                                "ru": "Врач решил <w>провести</w> несколько анализов, чтобы проверить мое здоровье."
                            }
                        }
                    ],
                    "synonyms": [
                        "execute"
                    ],
                    "common_phrases": [
                        "run a test",
                        "run a program",
                        "run a check"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять или запускать план, процедуру или компьютерную программу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запускать",
                                "target_lang_sense_clarification": "программу или приложение"
                            },
                            {
                                "target_lang_word": "выполнять",
                                "target_lang_sense_clarification": "план или процедуру"
                            },
                            {
                                "target_lang_word": "проводить",
                                "target_lang_sense_clarification": "тест или проверку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "366b8496-7980-36ad-ad5b-a88bcc2f7621",
                    "sense_definition": "To extend or continue through space or time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "extend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This long road will <w>run</w> all the way to the coast.",
                            "target_lang_translations": {
                                "ru": "Эта длинная дорога будет <w>тянуться</w> до самого побережья."
                            }
                        },
                        {
                            "text": "The contract is set to <w>run</w> for another two years.",
                            "target_lang_translations": {
                                "ru": "Контракт будет <w>действовать</w> еще два года."
                            }
                        }
                    ],
                    "common_phrases": [
                        "run for",
                        "run through",
                        "run along"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простираться или продолжаться в пространстве или времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тянуться",
                                "target_lang_sense_clarification": "простираться в пространстве"
                            },
                            {
                                "target_lang_word": "длиться",
                                "target_lang_sense_clarification": "продолжаться во времени"
                            },
                            {
                                "target_lang_word": "действовать",
                                "target_lang_sense_clarification": "о документе или соглашении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71689f9a-ceb5-316f-bb68-98a32065267a",
                    "sense_definition": "To be a candidate in an election.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to <w>run</w> for mayor to improve the local community.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>баллотироваться</w> в мэры, чтобы улучшить местное сообщество."
                            }
                        },
                        {
                            "text": "She is going to <w>run</w> against the current president in the next election.",
                            "target_lang_translations": {
                                "ru": "Она собирается <w>баллотироваться</w> против действующего президента на следующих выборах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "run for office",
                        "run for president"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть кандидатом на выборах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баллотироваться",
                                "target_lang_sense_clarification": "выдвигать кандидатуру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ded814a3-4f80-37c8-b1e2-90f13cf66b68",
                    "sense_definition": "To become different in a specific way, usually referring to a negative change or shortage.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "become",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to buy more milk because our supplies are <w>running</w> low.",
                            "target_lang_translations": {
                                "ru": "Нам нужно купить больше молока, потому что наши запасы <w>заканчиваются</w>."
                            }
                        },
                        {
                            "text": "The project began to <w>run</w> into difficulties regarding the budget.",
                            "target_lang_translations": {
                                "ru": "Проект начал <w>сталкиваться</w> с трудностями в отношении бюджета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "run low",
                        "run short",
                        "run out of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Истощаться или заканчиваться (о запасах); сталкиваться с чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заканчиваться",
                                "target_lang_sense_clarification": "иссякать"
                            },
                            {
                                "target_lang_word": "сталкиваться",
                                "target_lang_sense_clarification": "с трудностями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b686255-976a-325c-8598-2666f7b70a88",
                    "sense_definition": "To cost a certain amount of money.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cost",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A new car like that will <w>run</w> you at least thirty thousand dollars.",
                            "target_lang_translations": {
                                "ru": "Такая новая машина <w>обойдется</w> вам по меньшей мере в тридцать тысяч долларов."
                            }
                        },
                        {
                            "text": "Repairs for this old house could <w>run</w> quite high.",
                            "target_lang_translations": {
                                "ru": "Ремонт этого старого дома может <w>стоить</w> довольно дорого."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in spoken English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стоить определенную сумму денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоить",
                                "target_lang_sense_clarification": "иметь цену"
                            },
                            {
                                "target_lang_word": "обходиться",
                                "target_lang_sense_clarification": "стоить (разговорное)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4c3596a4-aafb-3d56-b4c2-44774127a9cd",
                    "sense_definition": "An act or instance of running, of moving rapidly using the feet for exercise or sport.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I like to go for a morning <w>run</w> to stay healthy.",
                            "target_lang_translations": {
                                "ru": "Я люблю выходить на утреннюю <w>пробежку</w>, чтобы оставаться здоровым."
                            }
                        },
                        {
                            "text": "After his long <w>run</w>, he was very tired and thirsty.",
                            "target_lang_translations": {
                                "ru": "После долгого <w>бега</w> он был очень уставшим и хотел пить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go for a run",
                        "morning run",
                        "long run"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие бега, быстрое перемещение на ногах для упражнения или спорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бег",
                                "target_lang_sense_clarification": "вид спорта или упражнение"
                            },
                            {
                                "target_lang_word": "пробежка",
                                "target_lang_sense_clarification": "короткий бег для разминки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfbfefaf-7926-37b1-8c47-cfbea8f3f921",
                    "sense_definition": "A short trip or journey, often to perform an errand or transport someone.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "trip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to make a quick <w>run</w> to the grocery store for milk.",
                            "target_lang_translations": {
                                "ru": "Нам нужно быстро <w>сгонять</w> в продуктовый магазин за молоком."
                            }
                        },
                        {
                            "text": "It is my turn to do the school <w>run</w> and pick up the kids.",
                            "target_lang_translations": {
                                "ru": "Моя очередь <w>развозить</w> детей в школу и забирать их."
                            }
                        }
                    ],
                    "common_phrases": [
                        "school run",
                        "coffee run",
                        "dry run"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая поездка, часто для выполнения поручения или перевозки кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поездка",
                                "target_lang_sense_clarification": "короткое путешествие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20c17d6c-481d-3de8-bac8-e6ef1aecdc21",
                    "sense_definition": "A continuous period or sequence of similar events or successes.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team enjoyed a great <w>run</w> of victories last season.",
                            "target_lang_translations": {
                                "ru": "Команда наслаждалась отличной <w>серией</w> побед в прошлом сезоне."
                            }
                        },
                        {
                            "text": "He had a <w>run</w> of bad luck with his car breaking down twice.",
                            "target_lang_translations": {
                                "ru": "У него была <w>полоса</w> неудач: его машина ломалась дважды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "run of luck",
                        "run of success",
                        "in the long run"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непрерывный период или последовательность похожих событий или успехов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полоса",
                                "target_lang_sense_clarification": "череда событий (удач или неудач)"
                            },
                            {
                                "target_lang_word": "серия",
                                "target_lang_sense_clarification": "последовательность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "984b474f-1163-3d56-af98-2ccc61b7d4be",
                    "sense_definition": "A route or path, especially one designated for skiing or snowboarding.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "path",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This is the most difficult ski <w>run</w> on the entire mountain.",
                            "target_lang_translations": {
                                "ru": "Это самая сложная лыжная <w>трасса</w> на всей горе."
                            }
                        },
                        {
                            "text": "Beginners should stick to the easy <w>run</w> near the lodge.",
                            "target_lang_translations": {
                                "ru": "Новичкам следует придерживаться легкого <w>спуска</w> возле домика."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ski run"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маршрут или путь, специально предназначенный для катания на лыжах или сноуборде."
                    },
                    "translations": {

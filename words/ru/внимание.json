    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "265e9eaf-b66b-37c3-90ca-def8d5454bdd",
                    "sense_definition": "Сосредоточенность мыслей, зрения или слуха на каком-либо объекте.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "focus",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель попросил всех учеников обратить <w>внимание</w> на новую тему.",
                            "target_lang_translations": {
                                "en": "The teacher asked all the students to pay <w>attention</w> to the new topic."
                            }
                        },
                        {
                            "text": "Громкий звук на улице сразу привлёк моё <w>внимание</w>.",
                            "target_lang_translations": {
                                "en": "A loud sound on the street immediately caught my <w>attention</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "сосредоточенность",
                        "сфокусированность"
                    ],
                    "antonyms": [
                        "рассеянность",
                        "невнимание"
                    ],
                    "common_phrases": [
                        "обратить внимание",
                        "привлечь внимание",
                        "центр внимания",
                        "акцентировать внимание"
                    ],
                    "target_lang_definitions": {
                        "en": "Concentration of thoughts, sight, or hearing on a specific object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attention",
                                "target_lang_sense_clarification": "mental focus"
                            },
                            {
                                "target_lang_word": "notice",
                                "target_lang_sense_clarification": "observation or perception"
                            },
                            {
                                "target_lang_word": "heed",
                                "target_lang_sense_clarification": "careful attention (formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b564db0d-b897-3d40-be28-cf7345f754e5",
                    "sense_definition": "Заботливое и доброе отношение к кому-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она всегда относилась к своим гостям с большим <w>вниманием</w>.",
                            "target_lang_translations": {
                                "en": "She always treated her guests with great <w>care</w>."
                            }
                        },
                        {
                            "text": "Пожилым людям часто не хватает простого человеческого <w>внимания</w>.",
                            "target_lang_translations": {
                                "en": "Elderly people often lack simple human <w>attention</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "сосредоточенность",
                        "сфокусированность"
                    ],
                    "antonyms": [
                        "рассеянность",
                        "невнимание"
                    ],
                    "common_phrases": [
                        "уделять внимание",
                        "окружить вниманием",
                        "знаки внимания"
                    ],
                    "target_lang_definitions": {
                        "en": "A caring and kind attitude towards someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attention",
                                "target_lang_sense_clarification": "care or concern"
                            },
                            {
                                "target_lang_word": "care",
                                "target_lang_sense_clarification": "concern and protection"
                            },
                            {
                                "target_lang_word": "consideration",
                                "target_lang_sense_clarification": "thoughtful regard"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ce3f59e-2eb4-334c-8832-9ae5299e73c1",
                    "sense_definition": "Интерес или заинтересованность в ком-либо или чём-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "interest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот новый фильм определённо заслуживает вашего <w>внимания</w>.",
                            "target_lang_translations": {
                                "en": "This new film definitely deserves your <w>attention</w>."
                            }
                        },
                        {
                            "text": "Мы не должны оставлять такие серьёзные ошибки без <w>внимания</w>.",
                            "target_lang_translations": {
                                "en": "We must not let such serious errors go without <w>notice</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "заслуживать внимания",
                        "оставить без внимания",
                        "приковывать внимание"
                    ],
                    "target_lang_definitions": {
                        "en": "Interest or interestedness in someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attention",
                                "target_lang_sense_clarification": "interest"
                            },
                            {
                                "target_lang_word": "notice",
                                "target_lang_sense_clarification": "recognition"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7b1ee9e0-f0c9-3703-ad4e-6acb36b8684b",
                    "sense_definition": "Призыв быть осторожным, приготовиться или начать слушать.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "call",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Внимание</w>, двери закрываются, следующая станция — центр города.",
                            "target_lang_translations": {
                                "en": "<w>Attention</w>, the doors are closing, the next station is the city center."
                            }
                        },
                        {
                            "text": "<w>Внимание</w>, говорит и показывает Москва!",
                            "target_lang_translations": {
                                "en": "<w>Attention</w>, this is Moscow speaking and broadcasting!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "внимание всем",
                        "внимание на экран",
                        "минуточку внимания"
                    ],
                    "target_lang_definitions": {
                        "en": "A call to be careful, get ready, or start listening."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attention",
                                "target_lang_sense_clarification": "command or announcement"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f2d92d0a-ead7-3686-8c25-9023d851078a",
                    "sense_definition": "A complete path through which an electric current flows, usually composed of wires and electronic components.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "electricity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to overload the electrical <w>circuit</w> by plugging in too many devices.",
                            "target_lang_translations": {
                                "ru": "Будьте осторожны, чтобы не перегрузить электрическую <w>цепь</w>, подключив слишком много приборов."
                            }
                        },
                        {
                            "text": "The lights suddenly went out because of a short <w>circuit</w> in the old wiring.",
                            "target_lang_translations": {
                                "ru": "Свет внезапно погас из-за короткого <w>замыкания</w> в старой проводке."
                            }
                        }
                    ],
                    "synonyms": [
                        "periplus",
                        "route"
                    ],
                    "common_phrases": [
                        "short circuit",
                        "electrical circuit",
                        "circuit board",
                        "break the circuit"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Замкнутый путь, по которому протекает электрический ток, обычно состоящий из проводов и электронных компонентов."
                    },
                    "translations": {

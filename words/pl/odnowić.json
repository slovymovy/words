    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9cdeb614-b276-363d-97f6-f4769df946eb",
                    "sense_definition": "Przywrócić czemuś, np. budynkowi lub przedmiotowi, dobry stan, ładny wygląd lub dawną świetność poprzez naprawę lub remont.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym roku planujemy gruntownie <odnowić> nasz stary dom na wsi.",
                            "target_lang_translations": {
                                "ru": "В следующем году мы планируем основательно <обновить> наш старый дом в деревне."
                            }
                        },
                        {
                            "text": "Malarz użył specjalnych farb, aby <odnowić> zniszczoną fasadę zabytkowego kościoła.",
                            "target_lang_translations": {
                                "ru": "Маляр использовал специальные краски, чтобы <обновить> разрушенный фасад старинной церкви."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyremontować",
                        "odrestaurować",
                        "naprawić",
                        "zregenerować",
                        "odświeżyć"
                    ],
                    "antonyms": [
                        "zniszczyć",
                        "zdewastować",
                        "zaniedbać"
                    ],
                    "common_phrases": [
                        "odnowić mieszkanie",
                        "odnowić meble",
                        "odnowić fasadę",
                        "odnowić wnętrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести что-либо (например, здание или предмет) в хорошее состояние или вернуть прежний вид посредством ремонта или реставрации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обновить",
                                "target_lang_sense_clarification": "придать вид нового, освежить"
                            },
                            {
                                "target_lang_word": "отремонтировать",
                                "target_lang_sense_clarification": "починить, произвести ремонт"
                            },
                            {
                                "target_lang_word": "реставрировать",
                                "target_lang_sense_clarification": "восстановить (о памятниках, искусстве)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a49371d-2f47-343a-ab92-248cadc7861d",
                    "sense_definition": "Sprawić, że coś zacznie istnieć lub obowiązywać na nowo; wznowić lub przedłużyć ważność czegoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu latach milczenia postanowił <odnowić> kontakt ze swoim bratem.",
                            "target_lang_translations": {
                                "ru": "После многих лет молчания он решил <возобновить> контакт со своим братом."
                            }
                        },
                        {
                            "text": "Musisz pamiętać, aby <odnowić> ubezpieczenie samochodu przed końcem miesiąca.",
                            "target_lang_translations": {
                                "ru": "Ты должен помнить, что нужно <продлить> страховку на автомобиль до конца месяца."
                            }
                        }
                    ],
                    "synonyms": [
                        "wznowić",
                        "przedłużyć",
                        "reaktywować"
                    ],
                    "antonyms": [
                        "zerwać",
                        "zakończyć",
                        "anulować"
                    ],
                    "common_phrases": [
                        "odnowić kontakt",
                        "odnowić znajomość",
                        "odnowić umowę",
                        "odnowić subskrypcję",
                        "odnowić przysięgę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы что-то снова начало существовать или действовать; возобновить или продлить срок действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возобновить",
                                "target_lang_sense_clarification": "начать снова (отношения, контакты)"
                            },
                            {
                                "target_lang_word": "продлить",
                                "target_lang_sense_clarification": "увеличить срок действия (договора, подписки)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

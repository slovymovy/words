                            {
                                "target_lang_word": "спрашивать",
                                "target_lang_sense_clarification": "задавать вопрос (несов. вид)"
                            },
                            {
                                "target_lang_word": "спросить",
                                "target_lang_sense_clarification": "задать вопрос (сов. вид)"
                            }
                        ]
                    }
                },
                {

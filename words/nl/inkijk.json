                            {
                                "target_lang_word": "обзор с улицы",
                                "target_lang_sense_clarification": "возможность заглянуть внутрь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01fd71d0-49e8-377f-b19c-707220d7538b",
                    "sense_definition": "Een lage uitsnijding van kleding waardoor een deel van de borsten zichtbaar is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze droeg een elegante jurk met een diepe <w>inkijk</w>.",
                            "target_lang_translations": {
                                "ru": "На ней было элегантное платье с глубоким <w>декольте</w>."
                            }
                        },
                        {
                            "text": "De blouse gaf iets meer <w>inkijk</w> dan ze had bedoeld.",
                            "target_lang_translations": {

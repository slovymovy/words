    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fe0e1a65-42a9-3946-b694-0d650799eb93",
                    "sense_definition": "Брат отца или матери, а также муж тёти.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kinship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой <w>дядя</w> работает врачом в городской больнице.",
                            "target_lang_translations": {
                                "en": "My <w>uncle</w> works as a doctor in the city hospital."
                            }
                        },
                        {
                            "text": "В выходные к нам в гости приедет родной <w>дядя</w>.",
                            "target_lang_translations": {
                                "en": "My <w>uncle</w> is coming to visit us this weekend."
                            }
                        }
                    ],
                    "synonyms": [
                        "дядюшка"
                    ],
                    "antonyms": [
                        "тётя",
                        "племянник",
                        "племянница"
                    ],
                    "common_phrases": [
                        "родной дядя",
                        "двоюродный дядя",
                        "мой дядя"
                    ],
                    "target_lang_definitions": {
                        "en": "The brother of one's father or mother, or the husband of one's aunt."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "uncle",
                                "target_lang_sense_clarification": "family member"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89077a39-49b8-3b54-9620-e31d8f4b2d58",
                    "sense_definition": "Взрослый мужчина (обычно в речи детей или при фамильярном обращении).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Какой-то незнакомый <w>дядя</w> спросил у нас дорогу к метро.",
                            "target_lang_translations": {
                                "en": "Some strange <w>man</w> asked us the way to the metro."
                            }
                        },
                        {
                            "text": "Маленькая девочка испугалась строгого <w>дядю</w> в парке.",
                            "target_lang_translations": {
                                "en": "The little girl was scared of the stern <w>man</w> in the park."
                            }
                        }
                    ],
                    "synonyms": [
                        "мужчина",
                        "дяденька",
                        "дядька"
                    ],
                    "antonyms": [
                        "тётя",
                        "тётенька"
                    ],
                    "common_phrases": [
                        "добрый дядя",
                        "чужой дядя",
                        "взрослый дядя"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An adult man (often used in children's speech or familiarly)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "man",
                                "target_lang_sense_clarification": "adult male"
                            },
                            {
                                "target_lang_word": "mister",
                                "target_lang_sense_clarification": "child's term for a man"
                            },
                            {
                                "target_lang_word": "guy",
                                "target_lang_sense_clarification": "informal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be903b94-ea11-32b2-b010-ff5245cc4763",
                    "sense_definition": "Посторонний человек или наёмный работодатель (обычно в контексте работы не на себя).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "idiomatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я больше не хочу работать на <w>дядю</w>, поэтому открываю свой бизнес.",
                            "target_lang_translations": {
                                "en": "I don't want to work for <w>the man</w> anymore, so I'm opening my own business."
                            }
                        },
                        {
                            "text": "Всю жизнь он трудился на чужого <w>дядю</w> и ничего не накопил.",
                            "target_lang_translations": {
                                "en": "He toiled for <w>someone else</w> all his life and saved nothing."
                            }
                        }
                    ],
                    "common_phrases": [
                        "работать на дядю"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A stranger or employer (usually in the context of working for someone else rather than oneself)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the man",
                                "target_lang_sense_clarification": "employer or authority figure (idiomatic)"
                            },
                            {
                                "target_lang_word": "someone else",
                                "target_lang_sense_clarification": "stranger or outsider"
                            }
                        ]
                    }
                }
            ]
        }
    ]

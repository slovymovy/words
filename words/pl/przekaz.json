    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f167483c-5df3-3957-a653-73eeb908d98e",
                    "sense_definition": "Pieniądze przesyłane za pośrednictwem poczty lub banku; także usługa polegająca na ich przesyłaniu",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "money_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia wysłała mi <w>przekaz</w> pocztowy na urodziny.",
                            "target_lang_translations": {
                                "ru": "Бабушка прислала мне почтовый <w>перевод</w> на день рождения."
                            }
                        },
                        {
                            "text": "Muszę pójść na pocztę, aby odebrać <w>przekaz</w> pieniężny.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти на почту, чтобы получить денежный <w>перевод</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przelew",
                        "transfer"
                    ],
                    "common_phrases": [
                        "przekaz pocztowy",
                        "przekaz pieniężny",
                        "otrzymać przekaz",
                        "nadać przekaz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежные средства, отправляемые по почте или через банк, а также услуга по их пересылке"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевод",
                                "target_lang_sense_clarification": "денежный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c466c4a1-4eb3-3468-abcb-99b9079a4ced",
                    "sense_definition": "Główna myśl, treść lub idea, którą autor chce zakomunikować odbiorcom poprzez swoje dzieło, wypowiedź lub działanie",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "communication_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten film ma bardzo pozytywny <w>przekaz</w> dla młodzieży.",
                            "target_lang_translations": {
                                "ru": "У этого фильма очень позитивный <w>посыл</w> для молодежи."
                            }
                        },
                        {
                            "text": "Polityk starał się, aby jego <w>przekaz</w> był zrozumiały dla wszystkich wyborców.",
                            "target_lang_translations": {
                                "ru": "Политик старался, чтобы его <w>посыл</w> был понятен всем избирателям."
                            }
                        }
                    ],
                    "synonyms": [
                        "przelew"
                    ],
                    "common_phrases": [
                        "przekaz podprogowy",
                        "czytelny przekaz",
                        "przekaz medialny",
                        "przekaz reklamowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главная мысль, содержание или идея, которую автор хочет донести до аудитории через свое произведение или высказывание"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посыл",
                                "target_lang_sense_clarification": "главная мысль"
                            },
                            {
                                "target_lang_word": "сообщение",
                                "target_lang_sense_clarification": "информация, месседж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a954bc0f-127f-3ef9-be6e-f90e76e9c793",
                    "sense_definition": "Proces przesyłania sygnału, informacji lub danych na odległość, często w kontekście mediów",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transmission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oglądaliśmy bezpośredni <w>przekaz</w> z uroczystości otwarcia igrzysk.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели прямую <w>трансляцию</w> с церемонии открытия игр."
                            }
                        },
                        {
                            "text": "<w>Przekaz</w> satelitarny został przerwany przez gwałtowną burzę.",
                            "target_lang_translations": {
                                "ru": "Спутниковая <w>трансляция</w> была прервана сильной грозой."
                            }
                        }
                    ],
                    "synonyms": [
                        "transmisja"
                    ],
                    "common_phrases": [
                        "przekaz na żywo",
                        "przekaz satelitarny",
                        "przekaz danych",
                        "przekaz telewizyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс передачи сигнала, информации или данных на расстояние, часто в контексте СМИ"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трансляция",
                                "target_lang_sense_clarification": "эфир"
                            },
                            {
                                "target_lang_word": "передача",
                                "target_lang_sense_clarification": "сигнала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9496aef-59da-31b3-9738-e2e29b8cd534",
                    "sense_definition": "Papierowy formularz wypełniany przy zlecaniu przesłania pieniędzy",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "money_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę czytelnie wypełnić <w>przekaz</w> drukowanymi literami.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, разборчиво заполните <w>бланк перевода</w> печатными буквами."
                            }
                        },
                        {
                            "text": "Zgubiłem <w>przekaz</w>, więc nie mogłem wpłacić pieniędzy na konto.",
                            "target_lang_translations": {
                                "ru": "Я потерял <w>бланк перевода</w>, поэтому не смог внести деньги на счет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wypełnić przekaz",
                        "blankiet przekazu",
                        "podpisać przekaz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бумажный бланк, заполняемый при отправке денежного перевода"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бланк перевода",
                                "target_lang_sense_clarification": "документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "856f41de-fff9-3c01-a347-85d7f16020db",
                    "sense_definition": "Informacja o przeszłości zachowana w pamięci pokoleń, tradycji ustnej lub dokumentach",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Według starego <w>przekazu</w>, w tym zamku straszy duch księżniczki.",
                            "target_lang_translations": {
                                "ru": "Согласно старинному <w>преданию</w>, в этом замке обитает призрак принцессы."
                            }
                        },
                        {
                            "text": "Historycy badają wiarygodność tego średniowiecznego <w>przekazu</w>.",
                            "target_lang_translations": {
                                "ru": "Историки исследуют достоверность этого средневекового <w>свидетельства</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przekaz ustny",
                        "według przekazu",
                        "przekaz historyczny",
                        "przekaz źródłowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Информация о прошлом, сохранившаяся в памяти поколений, устной традиции или документах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предание",
                                "target_lang_sense_clarification": "легенда"
                            },
                            {
                                "target_lang_word": "свидетельство",
                                "target_lang_sense_clarification": "историческое"
                            }
                        ]
                    }
                }
            ]
        }
    ]

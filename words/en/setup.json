                },
                {
                    "sense_id": "727d668a-3006-3306-9d5e-7a224d4f1061",
                    "sense_definition": "A set of equipment or apparatus designed and arranged for a particular purpose.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "arrangement_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The laboratory included an elaborate <w>setup</w> for measuring the energy.",

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "996ac29f-f05b-326e-a999-b9df7e08f8f3",
                    "sense_definition": "osoba biorąca udział w grze sportowej, towarzyskiej, hazardowej lub giełdowej",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "player_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy <gracz> otrzymał pięć kart na początku rozdania.",
                            "target_lang_translations": {
                                "ru": "Каждый <игрок> получил пять карт в начале раздачи."
                            }
                        },
                        {
                            "text": "Najlepszy <gracz> turnieju odebrał złoty puchar i nagrodę pieniężną.",
                            "target_lang_translations": {
                                "ru": "Лучший <игрок> турнира получил золотой кубок и денежный приз."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawodnik"
                    ],
                    "common_phrases": [
                        "gracz zespołowy",
                        "gracz giełdowy",
                        "doświadczony gracz"
                    ],
                    "target_lang_definitions": {
                        "ru": "человек, принимающий участие в спортивной, общественной, азартной или биржевой игре"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игрок",
                                "target_lang_sense_clarification": "участник игры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62a99477-aca4-30d4-8b79-83e69e7caed0",
                    "sense_definition": "osoba grająca w gry wideo lub komputerowe",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "player_video",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako zapalony <gracz>, spędza dużo czasu na testowaniu nowych tytułów.",
                            "target_lang_translations": {
                                "ru": "Как заядлый <геймер>, он проводит много времени за тестированием новых тайтлов."
                            }
                        },
                        {
                            "text": "Wielu <graczy> narzeka na wysokie wymagania sprzętowe tej produkcji.",
                            "target_lang_translations": {
                                "ru": "Многие <игроки> жалуются на высокие системные требования этой игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "gamer"
                    ],
                    "common_phrases": [
                        "zapalony gracz",
                        "gracz komputerowy",
                        "społeczność graczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "человек, играющий в видеоигры или компьютерные игры"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "геймер",
                                "target_lang_sense_clarification": "любитель видеоигр"
                            },
                            {
                                "target_lang_word": "игрок",
                                "target_lang_sense_clarification": "в компьютерные игры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae81f51d-5a16-38e5-9f8a-7ccd158e9f94",

{
    "word_family": [
        "advance",
        "advancement"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "19c67a56-4deb-3b8c-8f5c-e450c844a873",
                    "sense_definition": "Having the most modern and recently developed ideas, methods, or features.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "progress_level",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The laboratory uses <w>advanced</w> technology to analyze the samples.",
                            "target_lang_translations": {
                                "ru": "Лаборатория использует <w>передовые</w> технологии для анализа образцов."
                            }
                        },
                        {
                            "text": "This car has <w>advanced</w> safety features that protect the driver.",
                            "target_lang_translations": {
                                "ru": "У этого автомобиля есть <w>продвинутые</w> функции безопасности, которые защищают водителя."
                            }
                        }
                    ],
                    "synonyms": [
                        "sophisticated",
                        "progressive"
                    ],
                    "common_phrases": [
                        "advanced technology",
                        "advanced features",
                        "advanced systems"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основанный на новейших и недавно разработанных идеях, методах или функциях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передовой",
                                "target_lang_sense_clarification": "о технологиях или идеях"
                            },
                            {
                                "target_lang_word": "продвинутый",
                                "target_lang_sense_clarification": "современный, технически сложный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ac4e834-9113-3541-856c-2dd46f8f7c66",
                    "sense_definition": "At a high level of difficulty, complexity, or development; not elementary.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "progress_level",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This book is intended for <w>advanced</w> learners of English.",
                            "target_lang_translations": {
                                "ru": "Эта книга предназначена для <w>продвинутых</w> изучающих английский язык."
                            }
                        },
                        {
                            "text": "You can adjust the configuration in the <w>advanced</w> settings menu.",
                            "target_lang_translations": {
                                "ru": "Вы можете изменить конфигурацию в меню <w>расширенных</w> настроек."
                            }
                        }
                    ],
                    "common_phrases": [
                        "advanced level",
                        "advanced course",
                        "advanced studies",
                        "advanced settings"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся на высоком уровне сложности или развития; не начальный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продвинутый",
                                "target_lang_sense_clarification": "об уровне знаний или пользователе"
                            },
                            {
                                "target_lang_word": "расширенный",
                                "target_lang_sense_clarification": "о настройках или опциях"
                            },
                            {
                                "target_lang_word": "углублённый",
                                "target_lang_sense_clarification": "о курсе обучения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06f532bc-810a-3492-8e5c-4278c3f31600",
                    "sense_definition": "Far on in time or course; often used to describe an older age or a late stage of a process.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Despite his <w>advanced</w> age, he continues to travel the world.",
                            "target_lang_translations": {
                                "ru": "Несмотря на свой <w>преклонный</w> возраст, он продолжает путешествовать по миру."
                            }
                        },
                        {
                            "text": "The doctors discovered the disease at an <w>advanced</w> stage.",
                            "target_lang_translations": {
                                "ru": "Врачи обнаружили болезнь на <w>поздней</w> стадии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "advanced age",
                        "advanced years",
                        "advanced stage"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Далеко зашедший по времени или ходу развития; часто используется для описания пожилого возраста или поздней стадии процесса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преклонный",
                                "target_lang_sense_clarification": "о возрасте"
                            },
                            {
                                "target_lang_word": "поздний",
                                "target_lang_sense_clarification": "о стадии развития или болезни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be782f4c-1d84-33f8-9152-a16f12d31a63",
                    "sense_definition": "Indicating a time that is later than the actual correct time.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My watch is five minutes <w>advanced</w>, so I am never late.",
                            "target_lang_translations": {
                                "ru": "Мои часы <w>спешат</w> на пять минут, поэтому я никогда не опаздываю."
                            }
                        },
                        {
                            "text": "The clock was <w>advanced</w> by one hour for daylight saving time.",
                            "target_lang_translations": {
                                "ru": "Часы были <w>переведены вперёд</w> на один час для перехода на летнее время."
                            }
                        }
                    ],
                    "synonyms": [
                        "fast",
                        "ahead"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывающий время, которое позже фактического правильного времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спешащий",
                                "target_lang_sense_clarification": "о часах"
                            },
                            {
                                "target_lang_word": "переведённый вперёд",
                                "target_lang_sense_clarification": "о времени на часах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f71a1e05-9591-3fff-ac03-e98cf9894b56",
                    "sense_definition": "Pronounced with the tongue positioned further forward in the mouth.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this dialect, the vowel sound is more <w>advanced</w> than in standard speech.",
                            "target_lang_translations": {
                                "ru": "В этом диалекте гласный звук более <w>продвинут</w>, чем в стандартной речи."
                            }
                        },
                        {
                            "text": "The linguist noted an <w>advanced</w> articulation of the consonant.",
                            "target_lang_translations": {
                                "ru": "Лингвист отметил <w>продвинутую</w> артикуляцию согласного."
                            }
                        }
                    ],
                    "synonyms": [
                        "fronted"
                    ],
                    "antonyms": [
                        "retracted",
                        "backed"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical (Linguistics)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Произносимый с языком, расположенным ближе к передней части рта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продвинутый",
                                "target_lang_sense_clarification": "лингвистический термин"
                            },
                            {
                                "target_lang_word": "передний",
                                "target_lang_sense_clarification": "об артикуляции"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c46b5a9c-9cae-3f6f-869e-6b0cae64d65d",
                    "sense_definition": "Simple past and past participle of <w>advance</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The troops <w>advanced</w> slowly towards the enemy lines.",
                            "target_lang_translations": {
                                "ru": "Войска медленно <w>продвигались</w> к линиям противника."
                            }
                        },
                        {
                            "text": "She <w>advanced</w> her career by taking on difficult projects.",
                            "target_lang_translations": {
                                "ru": "Она <w>продвинула</w> свою карьеру, берясь за сложные проекты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "advanced forward",
                        "advanced against"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола advance."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продвигался",
                                "target_lang_sense_clarification": "двигался вперёд"
                            },
                            {
                                "target_lang_word": "наступал",
                                "target_lang_sense_clarification": "о войсках"
                            },
                            {
                                "target_lang_word": "развивал",
                                "target_lang_sense_clarification": "улучшал или способствовал прогрессу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
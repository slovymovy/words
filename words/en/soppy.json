{
    "word_family": [
        "sop",
        "soppiness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5105de11-e297-3fef-9340-69fd28411319",
                    "sense_definition": "Overly sentimental or emotional in a silly way.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She loves watching <w>soppy</w> romantic movies on the weekend.",
                            "target_lang_translations": {
                                "ru": "Она любит смотреть <w>слезливые</w> романтические фильмы по выходным."
                            }
                        },
                        {
                            "text": "He wrote a <w>soppy</w> letter to his girlfriend for Valentine's Day.",
                            "target_lang_translations": {
                                "ru": "Он написал <w>слащавое</w> письмо своей девушке на День святого Валентина."
                            }
                        }
                    ],
                    "synonyms": [
                        "sappy"
                    ],
                    "antonyms": [
                        "unemotional",
                        "cynical"
                    ],
                    "common_phrases": [
                        "soppy love story",
                        "soppy movie",
                        "don't be soppy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезмерно сентиментальный или эмоциональный, часто до глупости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сентиментальный",
                                "target_lang_sense_clarification": "проявляющий излишнюю чувствительность"
                            },
                            {
                                "target_lang_word": "слащавый",
                                "target_lang_sense_clarification": "приторно-нежный, неестественный"
                            },
                            {
                                "target_lang_word": "слезливый",
                                "target_lang_sense_clarification": "вызывающий слёзы умиления (о фильмах, историях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47b3d43d-fe1a-3d9b-80d8-e02d18723e3b",
                    "sense_definition": "Very wet; sodden or soaked.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My shoes got completely <w>soppy</w> after walking through the puddles.",
                            "target_lang_translations": {
                                "ru": "Мои ботинки стали совершенно <w>мокрыми</w> после ходьбы по лужам."
                            }
                        },
                        {
                            "text": "We could not play football because of the <w>soppy</w> grass.",
                            "target_lang_translations": {
                                "ru": "Мы не могли играть в футбол из-за <w>размокшей</w> травы."
                            }
                        }
                    ],
                    "antonyms": [
                        "dry"
                    ],
                    "common_phrases": [
                        "soppy ground",
                        "soppy grass"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень мокрый; пропитанный влагой насквозь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "промокший",
                                "target_lang_sense_clarification": "насквозь мокрый"
                            },
                            {
                                "target_lang_word": "размокший",
                                "target_lang_sense_clarification": "пропитавшийся водой (о земле, траве)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
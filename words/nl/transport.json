                {
                    "sense_id": "691d4b82-35bd-319b-be09-532ef5187b17",
                    "sense_definition": "Het overzetten of versturen van digitale gegevens of signalen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor veilig <w>transport</w> van gegevens is een goede verbinding nodig.",
                            "target_lang_translations": {
                                "en": "A good connection is necessary for the secure <w>transport</w> of data.",
                                "ru": "Для безопасной <w>передачи</w> данных необходимо хорошее соединение."
                            }
                        },
                        {
                            "text": "Het <w>transport</w> van de bestanden naar de server ging heel snel.",
                            "target_lang_translations": {
                                "en": "The <w>transfer</w> of the files to the server went very quickly.",
                                "ru": "<w>Передача</w> файлов на сервер прошла очень быстро."
                            }
                        }
                    ],
                    "synonyms": [
                        "overbrenging"
                    ],
                    "common_phrases": [
                        "digitaal transport",
                        "transport van data"
                    ],
                    "target_lang_definitions": {
                        "en": "The transferring or sending of digital data or signals.",
                        "ru": "Передача или отправка цифровых данных или сигналов."
                    },
                    "translations": {

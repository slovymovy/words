    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea2abc14-a96d-33ed-bdb8-cb5acad7e8c9",
                    "sense_definition": "Пища, съестные припасы; продовольственные товары (обычно используется во множественном числе).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы идём в супермаркет, чтобы купить <продукты> на всю неделю.",
                            "target_lang_translations": {
                                "en": "We are going to the supermarket to buy <groceries> for the whole week."
                            }
                        },
                        {
                            "text": "В холодильнике закончились все свежие <продукты>.",
                            "target_lang_translations": {
                                "en": "We have run out of all the fresh <food> in the fridge."
                            }
                        }
                    ],
                    "synonyms": [
                        "продовольствие"
                    ],
                    "common_phrases": [
                        "купить продукты",
                        "свежие продукты",
                        "список продуктов",
                        "магазин продуктов"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in plural to mean groceries"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Food, edible supplies; food goods (usually used in the plural)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "groceries",
                                "target_lang_sense_clarification": "common term for food bought at a store"
                            },
                            {
                                "target_lang_word": "food",
                                "target_lang_sense_clarification": "general term for things to eat"
                            },
                            {
                                "target_lang_word": "foodstuffs",
                                "target_lang_sense_clarification": "formal term for food products"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bddf0554-3cc6-328a-a336-e26958777d27",
                    "sense_definition": "Предмет, созданный в результате человеческого труда или производственной деятельности; товар.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "result_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша компания разработала новый программный <продукт> для бухгалтеров.",
                            "target_lang_translations": {
                                "en": "Our company has developed a new software <product> for accountants."
                            }
                        },
                        {
                            "text": "Завод выпускает качественный <продукт>, который пользуется спросом.",
                            "target_lang_translations": {
                                "en": "The factory manufactures a high-quality <product> that is in demand."
                            }
                        }
                    ],
                    "synonyms": [
                        "изделие"
                    ],
                    "antonyms": [
                        "сырьё"
                    ],
                    "common_phrases": [
                        "программный продукт",
                        "готовый продукт",
                        "качество продукта",
                        "новый продукт"
                    ],
                    "target_lang_definitions": {
                        "en": "An item created as a result of human labor or production activity; a commodity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "product",
                                "target_lang_sense_clarification": "manufactured item or service"
                            },
                            {
                                "target_lang_word": "commodity",
                                "target_lang_sense_clarification": "marketable item"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fc9d632-0602-3ff1-962c-85cf94008827",
                    "sense_definition": "Вещество или явление, полученное как следствие какого-либо процесса (химического, биологического или иного).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "result_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Углекислый газ — это <продукт> дыхания человека и животных.",
                            "target_lang_translations": {
                                "en": "Carbon dioxide is a <product> of human and animal respiration."
                            }
                        },
                        {
                            "text": "Эта книга — <продукт> долгих размышлений и кропотливой работы автора.",
                            "target_lang_translations": {
                                "en": "This book is the <product> of the author's long reflections and painstaking work."
                            }
                        }
                    ],
                    "common_phrases": [
                        "продукт распада",
                        "побочный продукт",
                        "продукт реакции"
                    ],
                    "target_lang_definitions": {
                        "en": "A substance or phenomenon obtained as a consequence of a process (chemical, biological, or other)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "product",
                                "target_lang_sense_clarification": "result of a reaction or process"
                            },
                            {
                                "target_lang_word": "result",
                                "target_lang_sense_clarification": "outcome or consequence"
                            }
                        ]
                    }
                }
            ]
        }
    ]

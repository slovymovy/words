    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6cbbbff7-dff7-3d53-b5bf-ff3f72211874",
                    "sense_definition": "Rzeczownik odczasownikowy od słowa <w>zamknąć</w>; czynność polegająca na uniemożliwieniu dostępu do wnętrza czegoś lub zakończeniu działania czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę o <zamknięcie> okna, ponieważ robi się zimno.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <закройте> окно, так как становится холодно."
                            }
                        },
                        {
                            "text": "Planowane <zamknięcie> sklepu nastąpi o godzinie dwudziestej.",
                            "target_lang_translations": {
                                "ru": "Планируемое <закрытие> магазина произойдёт в двадцать часов."
                            }
                        }
                    ],
                    "antonyms": [
                        "otwarcie",
                        "otworzenie"
                    ],
                    "common_phrases": [
                        "zamknięcie drzwi",
                        "zamknięcie roku",
                        "przed zamknięciem",
                        "po zamknięciu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по глаголу «закрыть»; преграждение доступа внутрь чего-либо или прекращение работы чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрытие",
                                "target_lang_sense_clarification": "действие или событие"
                            },
                            {
                                "target_lang_word": "закрывание",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97edb20e-0a5d-3c68-8c14-6a1186452735",
                    "sense_definition": "Mechanizm, przedmiot lub element służący do zamykania i zabezpieczania czegoś, np. torby lub pudełka.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To pudełko ma magnetyczne <zamknięcie>, które łatwo się otwiera.",
                            "target_lang_translations": {
                                "ru": "У этой коробки есть магнитная <застёжка>, которая легко открывается."
                            }
                        },
                        {
                            "text": "Zepsuło mi się <zamknięcie> w plecaku i nie mogę go zapiąć.",
                            "target_lang_translations": {
                                "ru": "У меня сломалась <застёжка> на рюкзаке, и я не могу его застегнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapięcie",
                        "zamek"
                    ],
                    "common_phrases": [
                        "solidne zamknięcie",
                        "zepsute zamknięcie",
                        "magnetyczne zamknięcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Механизм, предмет или элемент, служащий для закрывания и фиксации чего-либо (например, сумки или коробки)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застёжка",
                                "target_lang_sense_clarification": "механизм крепления"
                            },
                            {
                                "target_lang_word": "замок",
                                "target_lang_sense_clarification": "устройство для запирания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd0a3aa4-e96c-3086-bf24-26d5b55c6f52",
                    "sense_definition": "Stan odosobnienia, przebywanie w miejscu odizolowanym, z którego nie można swobodnie wyjść.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zwierzęta w zoo żyją w <zamknięciu>, co nie zawsze im służy.",
                            "target_lang_translations": {
                                "ru": "Животные в зоопарке живут в <неволе>, что не всегда идёт им на пользу."
                            }
                        },
                        {
                            "text": "Po wielu latach spędzonych w <zamknięciu> więzień wyszedł na wolność.",
                            "target_lang_translations": {
                                "ru": "После многих лет, проведённых в <заключении>, заключённый вышел на свободу."
                            }
                        }
                    ],
                    "synonyms": [
                        "odosobnienie",
                        "izolacja"
                    ],
                    "common_phrases": [
                        "w zamknięciu",
                        "trzymać w zamknięciu",
                        "życie w zamknięciu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние изоляции, пребывание в месте, откуда нельзя свободно выйти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заключение",
                                "target_lang_sense_clarification": "лишение свободы"
                            },
                            {
                                "target_lang_word": "неволя",
                                "target_lang_sense_clarification": "плен, отсутствие свободы"
                            },
                            {
                                "target_lang_word": "изоляция",
                                "target_lang_sense_clarification": "отделение от общества"
                            }
                        ]
                    }
                }
            ]
        }
    ]

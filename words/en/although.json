            "senses": [
                {
                    "sense_id": "9e07aa28-a548-3757-a9d5-0e5a142ca1c5",
                    "sense_definition": "In spite of the fact that; even though.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "concession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Although</w> it was raining heavily, we decided to go for a walk.",
                            "target_lang_translations": {
                                "ru": "<w>Хотя</w> шёл сильный дождь, мы решили пойти на прогулку."
                            }
                        },
                        {
                            "text": "She finished the race <w>although</w> she was very tired.",
                            "target_lang_translations": {
                                "ru": "Она закончила гонку, <w>хотя</w> очень устала."
                            }
                        }
                    ],
                    "synonyms": [
                        "even though",
                        "even if",
                        "notwithstanding"
                    ],
                    "common_phrases": [
                        "although it is",
                        "although I am",
                        "although he was"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несмотря на то, что; даже если."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотя",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "несмотря на то, что",
                                "target_lang_sense_clarification": "более формально"
                            },
                            {
                                "target_lang_word": "хоть и",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ccea8b0-bfbc-3dce-a0bd-8729c20c702e",
                    "sense_definition": "But; however; used to introduce a statement that modifies or reduces the effect of the previous statement.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "modification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The room is quite small, <w>although</w> it is very comfortable.",
                            "target_lang_translations": {
                                "ru": "Комната довольно маленькая, <w>хотя</w> очень уютная."
                            }
                        },
                        {
                            "text": "It was a difficult task, <w>although</w> not impossible to complete.",
                            "target_lang_translations": {
                                "ru": "Это была трудная задача, <w>хотя</w> и не невыполнимая."
                            }
                        }
                    ],
                    "synonyms": [
                        "albeit"
                    ],
                    "common_phrases": [
                        "although not",
                        "although rare"
                    ],
                    "target_lang_definitions": {
                        "ru": "Но; однако; вводит уточнение, ограничивающее смысл предыдущего высказывания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотя",
                                "target_lang_sense_clarification": "в значении «но»"
                            },
                            {
                                "target_lang_word": "однако",
                                "target_lang_sense_clarification": "противопоставление"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "9b9770a5-6e68-373a-b242-0910545c385e",
                    "sense_definition": "Waar men naartoe kan gaan of waarmee men contact kan opnemen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de aanleg van de nieuwe brug is die plaats een stuk <w>bereikbaarder</w> geworden.",
                            "target_lang_translations": {
                                "en": "Due to the construction of the new bridge, that place has become much more <w>accessible</w>.",
                                "ru": "Благодаря строительству нового моста это место стало намного <w>доступнее</w>."
                            }
                        },
                        {
                            "text": "Iedereen moet tegenwoordig dag en nacht telefonisch <w>bereikbaar</w> zijn.",
                            "target_lang_translations": {
                                "en": "Nowadays, everyone must be <w>reachable</w> by phone day and night.",
                                "ru": "В наши дни все должны быть <w>доступны</w> по телефону круглосуточно."
                            }
                        }
                    ],
                    "synonyms": [
                        "toegankelijk"
                    ],
                    "antonyms": [
                        "onbereikbaar"
                    ],
                    "common_phrases": [
                        "telefonisch bereikbaar",
                        "goed bereikbaar",
                        "makkelijk bereikbaar"
                    ],
                    "target_lang_definitions": {
                        "en": "Able to be reached physically or contacted.",
                        "ru": "Место, до которого можно добраться, или человек, с которым можно связаться."
                    },
                    "translations": {
                        "en": [
                            {

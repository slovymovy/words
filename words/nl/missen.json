{
    "word_family": [
        "gemis",
        "misser",
        "vermissen",
        "onmisbaar",
        "mis"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "86bc0a98-1c21-3937-9d72-4b2fea445f58",
                    "sense_definition": "Het gemis voelen van iemand of iets; verlangen naar iemand die er niet is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotional_longing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>mis</w> mijn familie elke dag een beetje meer.",
                            "target_lang_translations": {
                                "ru": "Я с каждым днем все больше <w>скучаю</w> по своей семье."
                            }
                        },
                        {
                            "text": "Tijdens de vakantie <w>miste</w> hij zijn hond enorm.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска он очень <w>скучал</w> по своей собаке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iemand missen",
                        "thuis missen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствовать отсутствие кого-то или чего-то; тосковать по кому-то, кого нет рядом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скучать",
                                "target_lang_sense_clarification": "по кому-то/чему-то"
                            },
                            {
                                "target_lang_word": "тосковать",
                                "target_lang_sense_clarification": "испытывать сильную грусть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd928ea0-c939-3dba-8b1c-a2921a695d8e",
                    "sense_definition": "Te laat komen om mee te gaan met een vervoermiddel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "failure_to_reach",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je niet opschiet, zul je de trein <w>missen</w>.",
                            "target_lang_translations": {
                                "ru": "Если ты не поторопишься, ты <w>опоздаешь</w> на поезд."
                            }
                        },
                        {
                            "text": "Hij kwam te laat en heeft de laatste bus <w>gemist</w>.",
                            "target_lang_translations": {
                                "ru": "Он пришел слишком поздно и <w>опоздал</w> на последний автобус."
                            }
                        }
                    ],
                    "antonyms": [
                        "halen"
                    ],
                    "common_phrases": [
                        "de bus missen",
                        "de trein missen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опоздать на транспортное средство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опоздать",
                                "target_lang_sense_clarification": "на поезд, автобус и т.д."
                            },
                            {
                                "target_lang_word": "пропустить",
                                "target_lang_sense_clarification": "транспорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea1170d7-bce0-3e65-9ba7-8ee5a4d43cc7",
                    "sense_definition": "Niet raken wat men probeerde te raken bij het gooien, schieten of slaan.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "failure_to_reach",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De voetballer schoot hard maar <w>miste</w> het doel.",
                            "target_lang_translations": {
                                "ru": "Футболист сильно ударил, но <w>промахнулся</w> мимо ворот."
                            }
                        },
                        {
                            "text": "Ik heb geprobeerd de vlieg te slaan, maar ik <w>miste</w>.",
                            "target_lang_translations": {
                                "ru": "Я попытался прихлопнуть муху, но <w>промахнулся</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "raken"
                    ],
                    "common_phrases": [
                        "doel missen",
                        "de bal missen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не попасть в цель при броске, ударе или выстреле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "промахнуться",
                                "target_lang_sense_clarification": "мимо цели"
                            },
                            {
                                "target_lang_word": "не попасть",
                                "target_lang_sense_clarification": "в цель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e17c941-20c9-34cf-8d4d-09d2588f79c1",
                    "sense_definition": "Niet aanwezig zijn bij een gebeurtenis; iets aan zich voorbij laten gaan.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "absence_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil dat belangrijke concert voor geen geld <w>missen</w>.",
                            "target_lang_translations": {
                                "ru": "Я ни за что не хочу <w>пропустить</w> этот важный концерт."
                            }
                        },
                        {
                            "text": "Door zijn ziekte moest hij de vergadering <w>missen</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за болезни ему пришлось <w>пропустить</w> собрание."
                            }
                        }
                    ],
                    "antonyms": [
                        "bijwonen"
                    ],
                    "common_phrases": [
                        "niet willen missen",
                        "de start missen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не присутствовать на событии; пропустить что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропустить",
                                "target_lang_sense_clarification": "событие, встречу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a45b23c-9c0f-3734-b7e5-90db10707143",
                    "sense_definition": "Er niet zijn terwijl het er wel zou moeten zijn; ontbreken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "absence_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er <w>mist</w> een knoop aan mijn nieuwe jas.",
                            "target_lang_translations": {
                                "ru": "На моем новом пальто <w>не хватает</w> пуговицы."
                            }
                        },
                        {
                            "text": "Kijk even goed of er hier nog iets <w>mist</w>.",
                            "target_lang_translations": {
                                "ru": "Посмотри внимательно, ничего ли здесь <w>не хватает</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontbreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствовать там, где должно быть; недоставать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не хватать",
                                "target_lang_sense_clarification": "об отсутствии предмета"
                            },
                            {
                                "target_lang_word": "отсутствовать",
                                "target_lang_sense_clarification": "формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0a73a91-3c06-3045-8fcc-2acf1767ba38",
                    "sense_definition": "Niet hebben of niet beschikken over iets dat nodig of gewenst is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "absence_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze soep is lekker, maar hij <w>mist</w> wat zout.",
                            "target_lang_translations": {
                                "ru": "Этот суп вкусный, но в нем <w>не хватает</w> соли."
                            }
                        },
                        {
                            "text": "Het team <w>mist</w> een sterke leider in het veld.",
                            "target_lang_translations": {
                                "ru": "Команде <w>не хватает</w> сильного лидера на поле."
                            }
                        }
                    ],
                    "antonyms": [
                        "bezitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не иметь чего-то необходимого или желаемого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не хватать",
                                "target_lang_sense_clarification": "качества или ингредиента"
                            },
                            {
                                "target_lang_word": "недоставать",
                                "target_lang_sense_clarification": "быть в дефиците"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "961bcd5e-6e11-3399-8af1-b1fdf5b80b6d",
                    "sense_definition": "Iets kunnen afstaan of weggeven zonder dat men er zelf last van heeft.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dispensability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kunt u misschien een euro <w>missen</w> voor het goede doel?",
                            "target_lang_translations": {
                                "ru": "Не могли бы вы <w>выделить</w> евро на благотворительность?"
                            }
                        },
                        {
                            "text": "Ik kan mijn fiets echt niet <w>missen</w> omdat ik hem dagelijks gebruik.",
                            "target_lang_translations": {
                                "ru": "Я действительно не могу <w>обойтись</w> без своего велосипеда, так как пользуюсь им ежедневно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kunnen missen",
                        "geld missen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть в состоянии отдать или обойтись без чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обойтись без",
                                "target_lang_sense_clarification": "жить без чего-то"
                            },
                            {
                                "target_lang_word": "выделить",
                                "target_lang_sense_clarification": "деньги, ресурсы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f7a200f-3958-34bd-8a98-714f0c9807ac",
                    "sense_definition": "Een fout maken; zich vergissen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "error",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Missen</w> is menselijk, dus wees niet te streng voor jezelf.",
                            "target_lang_translations": {
                                "ru": "<w>Ошибаться</w> — это по-человечески, так что не будь к себе слишком строг."
                            }
                        },
                        {
                            "text": "Iedereen kan zich wel eens <w>missen</w> in een berekening.",
                            "target_lang_translations": {
                                "ru": "Каждый может <w>ошибиться</w> в расчетах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "missen is menselijk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершить ошибку; ошибиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ошибаться",
                                "target_lang_sense_clarification": "делать ошибку"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "58761830-262d-388f-bc25-b1244c16db10",
                    "sense_definition": "Meervoud van <w>mis</w> (kerkdienst).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De priester heeft vandaag al twee <w>missen</w> geleid.",
                            "target_lang_translations": {
                                "ru": "Священник провел сегодня уже две <w>мессы</w>."
                            }
                        },
                        {
                            "text": "In deze kathedraal worden dagelijks <w>missen</w> gehouden.",
                            "target_lang_translations": {
                                "ru": "В этом соборе ежедневно проводятся <w>мессы</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова 'mis' (католическая служба)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мессы",
                                "target_lang_sense_clarification": "церковные службы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12edb5e4-ed09-3621-a671-518478414eae",
                    "sense_definition": "Meervoud van <w>miss</w> (schoonheidskoningin).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Alle <w>missen</w> stonden samen op het podium voor de foto.",
                            "target_lang_translations": {
                                "ru": "Все <w>мисс</w> стояли вместе на сцене для фото."
                            }
                        },
                        {
                            "text": "De voormalige <w>missen</w> werden geïnterviewd over hun carrière.",
                            "target_lang_translations": {
                                "ru": "Бывшие <w>мисс</w> давали интервью о своей карьере."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова 'miss' (победительница конкурса красоты)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мисс",
                                "target_lang_sense_clarification": "королевы красоты (несклоняемое)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7176dde2-6dfd-347a-bf46-85f4c751e057",
                    "sense_definition": "Physically strong, fit, and active; having a muscular body structure suitable for sports.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has a very <w>athletic</w> build from years of swimming and running.",
                            "target_lang_translations": {
                                "ru": "У него очень <w>атлетическое</w> телосложение благодаря годам плавания и бега."
                            }
                        },
                        {
                            "text": "You need to be quite <w>athletic</w> to complete this difficult obstacle course.",
                            "target_lang_translations": {
                                "ru": "Нужно быть довольно <w>спортивным</w>, чтобы пройти эту сложную полосу препятствий."
                            }
                        }
                    ],
                    "synonyms": [
                        "fit",
                        "muscular",
                        "sporty",
                        "robust",
                        "brawny"
                    ],
                    "antonyms": [
                        "unfit",
                        "weak",
                        "frail",
                        "puny"
                    ],
                    "common_phrases": [
                        "athletic build",
                        "athletic body",
                        "athletic figure",
                        "athletic ability"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически сильный, подтянутый и активный; обладающий мускулистым телосложением, подходящим для занятий спортом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спорти́вный",
                                "target_lang_sense_clarification": "о человеке: подтянутый, физически развитый"
                            },
                            {
                                "target_lang_word": "атлети́чный",
                                "target_lang_sense_clarification": "обладающий качествами атлета, мускулистый"
                            },
                            {
                                "target_lang_word": "атлети́ческий",
                                "target_lang_sense_clarification": "относящийся к телосложению (напр. атлетическое сложение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7f78c32-0e5a-34e4-a0fd-d807e5b24418",
                    "sense_definition": "Relating to athletes, sports, or physical exercises.",

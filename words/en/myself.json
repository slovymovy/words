                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My partner and <w>myself</w> are happy to accept your invitation.",
                            "target_lang_translations": {
                                "ru": "Мой партнер и <w>я</w> рады принять ваше приглашение."

            "senses": [
                {
                    "sense_id": "3ae38539-49f5-3f68-9ed3-7dd771410f61",
                    "sense_definition": "Iets wat men bezit of waar men recht op heeft aan iemand anders geven of overdragen, vaak omdat het moet of gevraagd wordt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relinquish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij moest zijn rijbewijs <w>afstaan</w> nadat hij met te veel alcohol op had gereden.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось <w>сдать</w> свои водительские права после того, как он вел машину в нетрезвом виде."
                            }
                        },
                        {
                            "text": "Zij heeft haar deel van de erfenis vrijwillig <w>afgestaan</w> aan een goed doel.",
                            "target_lang_translations": {
                                "ru": "Она добровольно <w>отдала</w> свою часть наследства на благотворительность."
                            }
                        }
                    ],
                    "synonyms": [
                        "overdragen",
                        "weggeven",
                        "inleveren",
                        "prijsgeven"
                    ],
                    "antonyms": [
                        "houden",
                        "behouden",
                        "opeisen"
                    ],
                    "common_phrases": [
                        "zijn plaats afstaan",
                        "bloed afstaan",
                        "rechten afstaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать или отдавать что-либо, чем владеешь или на что имеешь право, другому лицу (часто по необходимости или просьбе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уступать",
                                "target_lang_sense_clarification": "место, право"
                            },
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "кровь, документы"
                            },
                            {
                                "target_lang_word": "отдавать",
                                "target_lang_sense_clarification": "имущество, владение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab1b0c82-f0c1-3e5a-b7bf-2917c1f90a5f",
                    "sense_definition": "Zich op een zekere afstand bevinden van iets of iemand, of naar buiten wijzen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is een theorie die ver <w>afstaat</w> van de dagelijkse werkelijkheid.",
                            "target_lang_translations": {
                                "ru": "Это теория, которая далеко <w>отстоит</w> от повседневной реальности."
                            }
                        },
                        {
                            "text": "De jongen werd op school geplaagd omdat hij <w>afstaande</w> oren had.",
                            "target_lang_translations": {
                                "ru": "Мальчика дразнили в школе, потому что у него были <w>оттопыренные</w> уши."
                            }
                        }
                    ],
                    "antonyms": [
                        "aansluiten"
                    ],
                    "common_phrases": [
                        "ver afstaan van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться на определенном расстоянии от чего-либо или быть направленным в стороны."
                    },
                    "translations": {
                        "ru": [
                            {

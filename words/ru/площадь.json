    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0342def-6722-3b64-98ad-a112ebb50727",
                    "sense_definition": "Незастроенное ровное место в городе или другом населённом пункте, обычно окружённое зданиями.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы договорились встретиться с друзьями на центральной <w>площади</w>.",
                            "target_lang_translations": {
                                "en": "We agreed to meet friends at the central <w>square</w>."
                            }
                        },
                        {
                            "text": "Окна нашего номера в отеле выходят на старую городскую <w>площадь</w>.",
                            "target_lang_translations": {
                                "en": "The windows of our hotel room overlook the old town <w>square</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Красная площадь",
                        "центральная площадь",
                        "выйти на площадь",
                        "главная площадь"
                    ],
                    "target_lang_definitions": {
                        "en": "An open, flat public space in a city or town, usually surrounded by buildings."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "square",
                                "target_lang_sense_clarification": "open area in a city"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8862f34f-ac16-369c-ba3e-a47d883f54ce",
                    "sense_definition": "Мера величины геометрической фигуры или участка поверхности.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На уроке геометрии школьники учились вычислять <w>площадь</w> круга.",
                            "target_lang_translations": {
                                "en": "In geometry class, the students learned to calculate the <w>area</w> of a circle."
                            }
                        },
                        {
                            "text": "Общая <w>площадь</w> нашей новой квартиры составляет шестьдесят квадратных метров.",
                            "target_lang_translations": {
                                "en": "The total <w>area</w> of our new apartment is sixty square meters."
                            }
                        }
                    ],
                    "common_phrases": [

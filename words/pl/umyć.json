                    "sense_id": "47d72df8-5b2e-37d2-b940-9dd0bba9b067",
                    "sense_definition": "Wyczyścić coś lub kogoś przy użyciu wody, często z dodatkiem mydła lub innego środka czyszczącego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz dokładnie <umyć> ręce przed każdym posiłkiem.",
                            "target_lang_translations": {
                                "ru": "Ты должен тщательно <помыть> руки перед каждым приемом пищи."
                            }
                        },
                        {
                            "text": "Po obiedzie zawsze pomagam rodzicom <umyć> naczynia.",
                            "target_lang_translations": {
                                "ru": "После обеда я всегда помогаю родителям <помыть> посуду."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyczyścić",
                        "obmyć"
                    ],
                    "antonyms": [
                        "ubrudzić",
                        "zanieczyścić"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c6388ff2-b17d-30e1-87df-6bfbf4cd7cb1",
                    "sense_definition": "Проникать внутрь помещения резкой струёй воздуха через щели или отверстия; дуть (обычно используется в безличной форме).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "airflow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Закрой окно, оттуда сильно <w>сквозит</w>."
                        },
                        {
                            "text": "В старом доме изо всех щелей <w>сквозило</w> холодом."
                        }
                    ],
                    "synonyms": [
                        "дуть"
                    ],
                    "common_phrases": [
                        "сильно сквозит",
                        "из окна сквозит",
                        "по ногам сквозит"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "безличный глагол (impersonal verb)"
                        }
                    ]
                },
                {
                    "sense_id": "1a7e8542-ba3e-3396-86df-a23ff16b3cea",
                    "sense_definition": "Становиться заметным, обнаруживаться или проявляться (о чувствах, мыслях, качествах, которые пытаются скрыть).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В его голосе <w>сквозило</w> явное недоверие к собеседнику."
                        },
                        {
                            "text": "Сквозь вежливую улыбку <w>сквозила</w> холодная ирония."
                        }
                    ],
                    "synonyms": [
                        "проявляться",
                        "проглядывать",
                        "чувствоваться"
                    ],
                    "antonyms": [
                        "скрываться",
                        "таиться"
                    ],
                    "common_phrases": [
                        "сквозит ирония",
                        "сквозит недоверие",
                        "в голосе сквозит"
                    ],
                    "traits": [
                        {
                            "trait_type": "figurative",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "597c98df-19ba-3fe5-82c1-b83c15c38224",
                    "sense_definition": "Быть видным, просвечивать сквозь что-либо (о свете, предметах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "visibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Яркий свет <w>сквозил</w> через плотные занавески."
                        },
                        {
                            "text": "Между деревьями <w>сквозила</w> синева озера."
                        }
                    ],
                    "synonyms": [
                        "просвечивать",
                        "виднеться"
                    ],
                    "antonyms": [
                        "скрываться"
                    ],
                    "common_phrases": [
                        "свет сквозит",
                        "солнце сквозит"
                    ]
                },
                {
                    "sense_id": "63915b41-606e-37c2-b020-f1300c6875c0",
                    "sense_definition": "Быть прозрачным, редким или иметь дыры от изношенности (о ткани, одежде).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "visibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её старое платье совсем износилось и <w>сквозило</w> на локтях."
                        },
                        {
                            "text": "Ткань была такой тонкой, что <w>сквозила</w> на свету."
                        }
                    ],
                    "synonyms": [
                        "просвечивать"
                    ]
                }
            ]
        }
    ]
}
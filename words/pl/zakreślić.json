    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "20273d96-a8f9-3d25-aeba-01605dbff9d4",
                    "sense_definition": "Otoczyć element linią (zwykle w kształcie kółka), aby go wybrać, wyróżnić lub wskazać jako poprawny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "marking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na egzaminie należy <zakreślić> tylko jedną poprawną odpowiedź ołówkiem lub długopisem.",
                            "target_lang_translations": {
                                "ru": "На экзамене следует <обвести> только один правильный ответ карандашом или ручкой."
                            }
                        },
                        {
                            "text": "Nauczyciel poprosił, aby <zakreślić> w tekście wszystkie czasowniki na czerwono.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил <обвести> в тексте все глаголы красным цветом."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaznaczyć",
                        "obwieść"
                    ],
                    "common_phrases": [
                        "zakreślić odpowiedź",
                        "zakreślić na czerwono",
                        "zakreślić kółkiem",
                        "zakreślić właściwy punkt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обвести элемент линией (обычно в кружок), чтобы выбрать, выделить или указать его как правильный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обвести",
                                "target_lang_sense_clarification": "очертить линией вокруг"
                            },
                            {
                                "target_lang_word": "отметить",
                                "target_lang_sense_clarification": "выделить знаком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "327d3dbf-6a46-38b3-85f1-f1ee971ddef7",
                    "sense_definition": "Ustalić lub wyznaczyć granice, zasięg lub ramy czegoś, na przykład planu działania lub kompetencji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "defining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor jasno <zakreślił> zakres moich nowych obowiązków w firmie.",
                            "target_lang_translations": {
                                "ru": "Директор четко <очертил> круг моих новых обязанностей в фирме."
                            }
                        },
                        {
                            "text": "Musimy precyzyjnie <zakreślić> granice naszej współpracy, aby uniknąć nieporozumień.",
                            "target_lang_translations": {
                                "ru": "Мы должны точно <очертить> границы нашего сотрудничества, чтобы избежать недоразумений."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyznaczyć",
                        "określić",
                        "zarysować"
                    ],
                    "common_phrases": [
                        "zakreślić granice",
                        "zakreślić plan",
                        "zakreślić ramy",
                        "zakreślić horyzont"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установить или обозначить границы, пределы или рамки чего-либо (например, плана или полномочий)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очертить",
                                "target_lang_sense_clarification": "установить границы"
                            },
                            {
                                "target_lang_word": "обозначить",
                                "target_lang_sense_clarification": "указать пределы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02cf00df-a148-31c7-a0ab-3fc672ea399b",
                    "sense_definition": "Narysować linię tworzącą zamkniętą figurę geometryczną lub wykonać ruch po łuku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drawing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ptak <zakreślił> szeroki krąg na niebie i odleciał w stronę lasu.",
                            "target_lang_translations": {
                                "ru": "Птица <описала> широкий круг в небе и улетела в сторону леса."
                            }
                        },
                        {
                            "text": "Za pomocą cyrkla możesz idealnie <zakreślić> okrąg na kartce papieru.",
                            "target_lang_translations": {
                                "ru": "С помощью циркуля ты можешь идеально <начертить> окружность на листе бумаги."
                            }
                        }
                    ],
                    "synonyms": [
                        "narysować",
                        "zatoczyć"
                    ],
                    "common_phrases": [
                        "zakreślić okrąg",
                        "zakreślić łuk",
                        "zakreślić koło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарисовать линию, образующую замкнутую фигуру, или совершить движение по дуге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "описать",
                                "target_lang_sense_clarification": "совершить движение по траектории"
                            },
                            {
                                "target_lang_word": "начертить",
                                "target_lang_sense_clarification": "нарисовать геометрическую фигуру"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
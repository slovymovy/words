    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "71dbad6d-6bf9-3c4d-8d49-f6291bfb6527",
                    "sense_definition": "Fakt, że ktoś lub coś staje się inne niż było wcześniej; zastąpienie jednego stanu lub rzeczy innym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każda <zmiana> w planie podróży musi zostać zgłoszona wcześniej.",
                            "target_lang_translations": {
                                "ru": "О каждом <изменении> в плане поездки необходимо сообщать заранее."
                            }
                        },
                        {
                            "text": "Zauważyłem dużą <zmianę> w jego zachowaniu po powrocie z wakacji.",
                            "target_lang_translations": {
                                "ru": "Я заметил большую <перемену> в его поведении после возвращения из отпуска."
                            }
                        }
                    ],
                    "synonyms": [
                        "modyfikacja",
                        "szychta"
                    ],
                    "antonyms": [
                        "stabilizacja",
                        "stagnacja"
                    ],
                    "common_phrases": [
                        "zmiana planów",
                        "zmiana pogody",
                        "gwałtowna zmiana",
                        "zmiana adresu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт того, что кто-то или что-то становится иным, чем раньше; замена одного состояния или вещи другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изменение",
                                "target_lang_sense_clarification": "модификация, поправка"
                            },
                            {
                                "target_lang_word": "перемена",
                                "target_lang_sense_clarification": "в погоде, настроении, планах"
                            },
                            {
                                "target_lang_word": "смена",
                                "target_lang_sense_clarification": "замена одного другим"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "891c25fc-3e5e-3229-81ad-18583b638740",
                    "sense_definition": "Wyznaczony czas w ciągu doby, w którym praca w zakładzie jest wykonywana przez określoną grupę ludzi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "work_shift",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat pracuje w fabryce na nocną <zmianę>.",
                            "target_lang_translations": {
                                "ru": "Мой брат работает на заводе в ночную <смену>."
                            }
                        },
                        {
                            "text": "Druga <zmiana> kończy się o godzinie dwudziestej drugiej.",
                            "target_lang_translations": {
                                "ru": "Вторая <смена> заканчивается в двадцать два часа."
                            }
                        }
                    ],
                    "synonyms": [
                        "szychta"
                    ],
                    "common_phrases": [
                        "nocna zmiana",
                        "pierwsza zmiana",
                        "praca na zmiany",
                        "system trzyzmianowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установленное время суток, в течение которого работа на предприятии выполняется определённой группой людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смена",
                                "target_lang_sense_clarification": "рабочее время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73d85e36-f770-3319-a5c8-40ae4ee2cc98",
                    "sense_definition": "Grupa pracowników wykonująca pracę w tym samym, wyznaczonym czasie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "work_shift",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poprzednia <zmiana> zostawiła po sobie duży bałagan w warsztacie.",
                            "target_lang_translations": {
                                "ru": "Предыдущая <смена> оставила после себя большой беспорядок в мастерской."
                            }
                        },
                        {
                            "text": "Cała <zmiana> strażaków wyjechała do pożaru w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Вся <смена> пожарных выехала на пожар в центре города."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zmiana warty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа работников, выполняющая работу в одно и то же установленное время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смена",
                                "target_lang_sense_clarification": "группа людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93a6e290-fba6-35fe-90ee-3a03273e84fe",
                    "sense_definition": "Komplet czystej bielizny, ubrania lub pościeli przygotowany do wymiany.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_set",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spakowałam do walizki jedną <zmianę> ubrań na wszelki wypadek.",
                            "target_lang_translations": {
                                "ru": "Я упаковала в чемодан одну <смену> одежды на всякий случай."
                            }
                        },
                        {
                            "text": "W szafie leży świeża <zmiana> pościeli dla gości.",
                            "target_lang_translations": {
                                "ru": "В шкафу лежит свежая <смена> постельного белья для гостей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zmiana bielizny",
                        "zmiana pościeli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплект чистого белья, одежды или постельных принадлежностей, подготовленный для замены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смена",
                                "target_lang_sense_clarification": "комплект белья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08e9d5dc-d442-39f3-9441-1cb0936887b6",
                    "sense_definition": "Część sztuki teatralnej wyodrębniona ze względu na zmianę dekoracji.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "theater_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aktorzy mają tylko chwilę odpoczynku przed kolejną <zmianą>.",
                            "target_lang_translations": {
                                "ru": "У актёров есть лишь минута отдыха перед следующей <сменой декораций>."
                            }
                        },
                        {
                            "text": "Ta <zmiana> wymaga szybkiego przestawienia mebli na scenie.",
                            "target_lang_translations": {
                                "ru": "Эта <смена декораций> требует быстрой перестановки мебели на сцене."
                            }
                        }
                    ],
                    "synonyms": [
                        "odsłona"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть театральной пьесы, выделенная в связи со сменой декораций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смена декораций",
                                "target_lang_sense_clarification": "техническое действие"
                            },
                            {
                                "target_lang_word": "картина",
                                "target_lang_sense_clarification": "часть акта"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
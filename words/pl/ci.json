    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "f12fde7d-5a63-3ab5-b5c1-246b91bf355f",
                    "sense_definition": "Krótka, nieakcentowana forma celownika liczby pojedynczej zaimka osobowego <w>ty</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_ty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimno <ci> w ręce?",
                            "target_lang_translations": {
                                "ru": "<Тебе> холодно в руки?"
                            }
                        },
                        {
                            "text": "Chętnie <ci> pomogę w nauce.",
                            "target_lang_translations": {
                                "ru": "Я охотно помогу <тебе> с учебой."
                            }
                        }
                    ],
                    "synonyms": [
                        "tobie (akcentowane)"
                    ],
                    "common_phrases": [
                        "na złość ci",
                        "nic ci do tego",
                        "co ci"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткая, безударная форма дательного падежа единственного числа личного местоимения \"ты\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тебе",
                                "target_lang_sense_clarification": "местоимение \"ты\" в дательном падеже"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eac446be-e67e-3f3a-be38-12c4a9ea963e",
                    "sense_definition": "Forma mianownika liczby mnogiej rodzaju męskoosobowego zaimka wskazującego <w>ten</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_ten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Ci> ludzie czekają na autobus.",
                            "target_lang_translations": {
                                "ru": "<Эти> люди ждут автобуса."
                            }
                        },
                        {
                            "text": "Wszyscy <ci> studenci zdali egzamin.",
                            "target_lang_translations": {
                                "ru": "Все <эти> студенты сдали экзамен."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ci sami",
                        "wszyscy ci",
                        "ci i owi"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural virile"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма именительного падежа множественного числа (мужской личный род) указательного местоимения \"ten\" (этот)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эти",
                                "target_lang_sense_clarification": "указательное местоимение (мн. ч.)"
                            },
                            {
                                "target_lang_word": "те",
                                "target_lang_sense_clarification": "указательное местоимение (мн. ч., дальний план)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "febf687a-1697-3427-a4ab-4c3be655030a",
                    "sense_definition": "Partykuła (funkcja zaimka) wzmacniająca wypowiedź, nadająca jej emocjonalny, często poufały charakter lub wyrażająca zdziwienie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_ty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Masz <ci> los, znowu zapomniałem kluczy!",
                            "target_lang_translations": {
                                "ru": "Вот <тебе> и на, опять забыл ключи!"
                            }
                        },
                        {
                            "text": "A to <ci> niespodzianka, że cię tu widzę.",
                            "target_lang_translations": {
                                "ru": "Вот <так> сюрприз, что я тебя здесь вижу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "masz ci los",
                        "a to ci"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "ethical dative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Частица (функция местоимения), усиливающая высказывание, придающая ему эмоциональный, часто фамильярный характер или выражающая удивление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тебе",
                                "target_lang_sense_clarification": "этический датив (в идиомах)"
                            },
                            {
                                "target_lang_word": "же",
                                "target_lang_sense_clarification": "усилительная частица"
                            },
                            {
                                "target_lang_word": "ну и",
                                "target_lang_sense_clarification": "восклицание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4e7053e0-b1ca-3c96-8418-630d8b94073d",
                    "sense_definition": "Gatunek klasycznej poezji chińskiej, charakteryzujący się nieregularną długością wersów, popularny w czasach dynastii Song.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_chinese_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poeta ten był mistrzem gatunku <ci>.",
                            "target_lang_translations": {
                                "ru": "Этот поэт был мастером жанра <цы>."
                            }
                        },
                        {
                            "text": "Na wykładzie omawialiśmy strukturę utworów <ci>.",
                            "target_lang_translations": {
                                "ru": "На лекции мы обсуждали структуру произведений <цы>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жанр классической китайской поэзии, характеризующийся неравной длиной строк, популярный во времена династии Сун."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цы",
                                "target_lang_sense_clarification": "жанр китайской поэзии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e6d95a9-4dcd-3cdf-afe1-2a57fc24e52c",
                    "sense_definition": "Termin w filozofii chińskiej oznaczający miłość, dobroć lub współczucie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_chinese_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Koncepcja <ci> jest kluczowa dla zrozumienia tej szkoły filozoficznej.",
                            "target_lang_translations": {
                                "ru": "Концепция <цы> является ключевой для понимания этой философской школы."
                            }
                        },
                        {
                            "text": "Mędrzec nauczał o wadze cnoty <ci> w życiu codziennym.",
                            "target_lang_translations": {
                                "ru": "Мудрец учил о важности добродетели <цы> в повседневной жизни."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Термин в китайской философии, означающий любовь, доброту или сострадание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цы",
                                "target_lang_sense_clarification": "философское понятие (милосердие)"
                            }
                        ]
                    }
                }
            ]
        }

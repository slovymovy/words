{
    "word_family": [
        "captaincy",
        "captainship"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea25f27b-e143-3cb1-bf85-3ece1dcc271c",
                    "sense_definition": "The person lawfully in command of a ship, aircraft, or other vessel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "command_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>captain</w> told the passengers to fasten their seatbelts.",
                            "target_lang_translations": {
                                "ru": "<w>Капитан</w> попросил пассажиров пристегнуть ремни."
                            }
                        },
                        {
                            "text": "Our ship's <w>captain</w> has sailed these waters for twenty years.",
                            "target_lang_translations": {
                                "ru": "<w>Капитан</w> нашего корабля ходит по этим водам уже двадцать лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "skipper",
                        "master"
                    ],
                    "common_phrases": [
                        "ship's captain",
                        "captain of the ship",
                        "sea captain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, официально командующий кораблем, самолетом или другим судном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капитан",
                                "target_lang_sense_clarification": "судна"
                            },
                            {
                                "target_lang_word": "командир",
                                "target_lang_sense_clarification": "воздушного или морского судна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d2d2943-3fad-31ce-9cd6-1f5578563556",
                    "sense_definition": "One of the athletes on a sports team who is designated to make decisions and speak for the team.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sports_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was chosen as the <w>captain</w> of the soccer team this season.",
                            "target_lang_translations": {
                                "ru": "В этом сезоне её выбрали <w>капитаном</w> футбольной команды."
                            }
                        },
                        {
                            "text": "The team <w>captain</w> argued with the referee about the penalty.",
                            "target_lang_translations": {
                                "ru": "<w>Капитан</w> команды поспорил с судьей из-за пенальти."
                            }
                        }
                    ],
                    "synonyms": [
                        "skipper"
                    ],
                    "common_phrases": [
                        "team captain",
                        "captain of the team"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортсмен в команде, назначенный принимать решения и представлять команду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капитан",
                                "target_lang_sense_clarification": "спортивной команды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc17653a-e7e8-354b-be10-d15139309727",
                    "sense_definition": "An officer in the army, navy, or air force with a specific rank.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>captain</w> ordered his soldiers to return to the base.",
                            "target_lang_translations": {
                                "ru": "<w>Капитан</w> приказал своим солдатам вернуться на базу."
                            }
                        },
                        {
                            "text": "He was promoted to <w>captain</w> after serving for five years.",
                            "target_lang_translations": {
                                "ru": "Он получил звание <w>капитана</w> после пяти лет службы."
                            }
                        }
                    ],
                    "synonyms": [
                        "Capt.",
                        "CAPT",
                        "CPT"
                    ],
                    "common_phrases": [
                        "army captain",
                        "navy captain",
                        "police captain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Офицерское звание в армии, флоте или авиации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капитан",
                                "target_lang_sense_clarification": "воинское звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9da9bff6-0445-3571-acf8-a1178950d7ef",
                    "sense_definition": "The leader of a group of workers.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "work_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mine <w>captain</w> checked the safety equipment before the shift began.",
                            "target_lang_translations": {
                                "ru": "<w>Бригадир</w> шахты проверил оборудование безопасности перед началом смены."
                            }
                        },
                        {
                            "text": "He worked as a <w>captain</w> overseeing the construction crew.",
                            "target_lang_translations": {
                                "ru": "Он работал <w>бригадиром</w>, руководя строительной бригадой."
                            }
                        }
                    ],
                    "synonyms": [
                        "foreman",
                        "supervisor",
                        "straw boss"
                    ],
                    "common_phrases": [
                        "mine captain",
                        "captain of industry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Руководитель группы рабочих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бригадир",
                                "target_lang_sense_clarification": "руководитель бригады"
                            },
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "на производстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae9f4ce7-2407-39c5-9dfc-f4732a0a51f5",
                    "sense_definition": "A headwaiter or maître d' in a restaurant.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "service_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We asked the <w>captain</w> to recommend a good wine for our dinner.",
                            "target_lang_translations": {
                                "ru": "Мы попросили <w>метрдотеля</w> порекомендовать хорошее вино к ужину."
                            }
                        },
                        {
                            "text": "The <w>captain</w> showed the guests to their table near the window.",
                            "target_lang_translations": {
                                "ru": "<w>Метрдотель</w> проводил гостей к их столику у окна."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старший официант или метрдотель в ресторане."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "метрдотель",
                                "target_lang_sense_clarification": "в ресторане"
                            },
                            {
                                "target_lang_word": "старший официант",
                                "target_lang_sense_clarification": "должность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c20491c6-cba9-346b-9e64-db1e6cb538d1",
                    "sense_definition": "To exercise command of a ship, aircraft, or sports team.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has <w>captained</w> the national team for over five years.",
                            "target_lang_translations": {
                                "ru": "Он <w>был капитаном</w> национальной сборной более пяти лет."
                            }
                        },
                        {
                            "text": "She hopes to <w>captain</w> a large cruise ship one day.",
                            "target_lang_translations": {
                                "ru": "Она надеется однажды <w>командовать</w> большим круизным лайнером."
                            }
                        }
                    ],
                    "common_phrases": [
                        "captain a team",
                        "captain a ship"
                    ],
                    "target_lang_definitions": {
                        "ru": "Командовать кораблем, самолетом или спортивной командой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть капитаном",
                                "target_lang_sense_clarification": "возглавлять команду"
                            },
                            {
                                "target_lang_word": "командовать",
                                "target_lang_sense_clarification": "судном"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d0cb4278-95bc-363d-a745-176890c6c4a1",
                    "sense_definition": "The honorific title used before a name for a person with the rank of captain.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "title",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "We learned about the voyages of <w>Captain</w> Cook in history class.",
                            "target_lang_translations": {
                                "ru": "На уроке истории мы узнали о путешествиях <w>капитана</w> Кука."
                            }
                        },
                        {
                            "text": "Please welcome <w>Captain</w> Smith to the stage.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поприветствуйте <w>капитана</w> Смита на сцене."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Почетный титул, используемый перед именем человека в звании капитана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капитан",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dd7a449-9b3b-33c1-a612-3362c9f59591",
                    "sense_definition": "A title used for a superhero.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fictional_title",
                    "name_type": "fictional_name",
                    "examples": [
                        {
                            "text": "My favorite comic book character is <w>Captain</w> America.",
                            "target_lang_translations": {
                                "ru": "Мой любимый герой комиксов — <w>Капитан</w> Америка."
                            }
                        },
                        {
                            "text": "The children dressed up as <w>Captain</w> Marvel for the party.",
                            "target_lang_translations": {
                                "ru": "Дети нарядились <w>Капитаном</w> Марвел на вечеринку."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул, используемый в именах супергероев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Капитан",
                                "target_lang_sense_clarification": "часть имени"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
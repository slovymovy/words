                {
                    "sense_id": "5f75f405-60c8-3297-a5a7-00216c35edab",
                    "sense_definition": "Relating to government revenue, taxes, and public spending.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "finance_govt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government implemented a new <w>fiscal</w> policy to reduce the national debt.",

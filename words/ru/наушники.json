                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earphones",
                                "target_lang_sense_clarification": "plural form"
                            },
                            {
                                "target_lang_word": "headphones",
                                "target_lang_sense_clarification": "plural form"
                            }
                        ]
                    }
                }
            ]
        }

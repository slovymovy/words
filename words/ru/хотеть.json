    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "37b363e1-9740-386c-9a14-47353739b072",
                    "sense_definition": "Иметь желание, потребность или намерение что-либо сделать или получить.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "desire_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я очень устал и теперь просто <w>хочу</w> спать.",
                            "target_lang_translations": {
                                "en": "I am very tired and now I just <w>want</w> to sleep."
                            }
                        },
                        {
                            "text": "Мы <w>хотим</w> купить большой и красивый дом в деревне.",
                            "target_lang_translations": {
                                "en": "We <w>want</w> to buy a big and beautiful house in the country."
                            }
                        }
                    ],
                    "synonyms": [
                        "желать"
                    ],
                    "common_phrases": [
                        "хотеть есть",
                        "хотеть спать",
                        "хотеть домой",
                        "хотеть знать",
                        "хотеть мира"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a desire, need, or intention to do or get something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "want",
                                "target_lang_sense_clarification": "to wish for or desire something"
                            },
                            {
                                "target_lang_word": "wish",
                                "target_lang_sense_clarification": "to desire or hope for (often more formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0b70c52-c130-3d8e-a2a2-9bc9134cd4ef",
                    "sense_definition": "Испытывать сильное чувственное или сексуальное влечение к кому-либо.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "desire_sexual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он сразу понял, что безумно <w>хочет</w> эту женщину.",
                            "target_lang_translations": {
                                "en": "He immediately realized that he madly <w>wanted</w> this woman."
                            }
                        },
                        {
                            "text": "Герой романа любил её и <w>хотел</w> больше жизни.",
                            "target_lang_translations": {
                                "en": "The hero of the novel loved her and <w>wanted</w> her more than life itself."
                            }
                        }
                    ],
                    "synonyms": [
                        "желать"
                    ],
                    "common_phrases": [
                        "безумно хотеть",
                        "хотеть её",
                        "хотеть его"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "в значении физического влечения"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To experience strong sensual or sexual attraction to someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "want",
                                "target_lang_sense_clarification": "to desire someone sexually or romantically"
                            }
                        ]
                    }
                }
            ]
        }

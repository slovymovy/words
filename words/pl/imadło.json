            "senses": [
                {
                    "sense_id": "2d694f28-d04a-3b4d-b406-9a8cef7a4ba9",
                    "sense_definition": "przyrząd służący do stabilnego mocowania przedmiotów poddawanych obróbce ręcznej lub mechanicznej",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stolarz umieścił deskę w <w>imadle</w>, aby ją bezpiecznie przepiłować.",
                            "target_lang_translations": {
                                "ru": "Столяр поместил доску в <w>тиски</w>, чтобы безопасно ее распилить."
                            }
                        },
                        {
                            "text": "Musisz mocno dokręcić <w>imadło</w>, żeby metalowy element się nie przesuwał.",
                            "target_lang_translations": {
                                "ru": "Ты должен сильно затянуть <w>тиски</w>, чтобы металлическая деталь не сдвигалась."
                            }
                        }
                    ],
                    "synonyms": [
                        "śrubsztak",
                        "szrobsztok"
                    ],
                    "common_phrases": [
                        "szczęki imadła",
                        "wkręcić w imadło",
                        "trzymać jak w imadle"
                    ],
                    "target_lang_definitions": {
                        "ru": "приспособление для фиксации предметов, подвергаемых ручной или механической обработке"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тиски",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc5789a0-4576-3045-9ed5-1ce2098a5c9b",
                    "sense_definition": "narzędzie chirurgiczne używane do chwytania i przytrzymywania igieł lub tkanek",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "medical_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chirurg poprosił asystentkę o <w>imadło</w> do założenia szwów.",
                            "target_lang_translations": {
                                "ru": "Хирург попросил у ассистентки <w>иглодержатель</w> для наложения швов."
                            }
                        },
                        {
                            "text": "To precyzyjne <w>imadło</w> pozwala na pewny chwyt małej igły.",
                            "target_lang_translations": {
                                "ru": "Этот точный <w>иглодержатель</w> позволяет надежно захватить маленькую иглу."
                            }
                        }
                    ],
                    "synonyms": [
                        "kleszcze",
                        "szczypce"
                    ],
                    "common_phrases": [
                        "imadło chirurgiczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "хирургический инструмент, используемый для захвата и удержания игл или тканей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иглодержатель",
                                "target_lang_sense_clarification": "медицинский инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91812643-a534-3490-a8ab-3aa937458379",
                    "sense_definition": "część naczynia lub przedmiotu służąca do chwytania go dłonią",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "handle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zabytkowy dzban miał urwane jedno ozdobne <w>imadło</w>.",
                            "target_lang_translations": {
                                "ru": "У старинного кувшина была оторвана одна декоративная <w>ручка</w>."
                            }
                        },
                        {
                            "text": "Mężczyzna chwycił za <w>imadło</w> skrzyni i podniósł ją z ziemi.",
                            "target_lang_translations": {
                                "ru": "Мужчина схватился за <w>ручку</w> сундука и поднял его с земли."
                            }
                        }
                    ],
                    "synonyms": [
                        "uchwyt",
                        "ucho"
                    ],
                    "target_lang_definitions": {

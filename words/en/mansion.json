                        "shack",
                        "hut",
                        "cottage"
                    ],
                    "common_phrases": [
                        "sprawling mansion",
                        "luxury mansion",
                        "historic mansion",
                        "governor's mansion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой роскошный дом или здание, обычно построенное для богатых людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "особняк",

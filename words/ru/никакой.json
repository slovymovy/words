{
    "word_family": [
        "никак",
        "никакущий"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "0fdb6491-00b9-3f1a-8259-c2129783be71",
                    "sense_definition": "Ни один из имеющихся или возможных; употребляется в отрицательных предложениях для полного отрицания.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня нет <никаких> вопросов к этому ученику.",
                            "target_lang_translations": {
                                "en": "I have <no> questions for this student."
                            }
                        },
                        {
                            "text": "Мы не нашли <никакой> информации о пропавшей собаке.",
                            "target_lang_translations": {
                                "en": "We didn't find <any> information about the missing dog."
                            }
                        },
                        {
                            "text": "Никакие уговоры не заставят его изменить решение.",
                            "target_lang_translations": {
                                "en": "<No> persuasion will make him change his decision."
                            }
                        }
                    ],
                    "synonyms": [
                        "ни один"
                    ],
                    "antonyms": [
                        "любой",
                        "всякий",
                        "каждый"
                    ],
                    "common_phrases": [
                        "никаких проблем",
                        "никаких сомнений",
                        "ни в каком случае",
                        "без никаких"
                    ],
                    "target_lang_definitions": {
                        "en": "None of the available or possible ones; used in negative sentences to express complete absence or denial."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "no",
                                "target_lang_sense_clarification": "used directly before a noun to indicate absence"
                            },
                            {
                                "target_lang_word": "any",
                                "target_lang_sense_clarification": "used with a negative verb (e.g., do not have any)"
                            },
                            {
                                "target_lang_word": "none",
                                "target_lang_sense_clarification": "not any of a group or amount"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "53b4df11-a932-3056-8a3c-d3e5a6b25031",
                    "sense_definition": "Вовсе не, совсем не; используется для эмоционального усиления отрицания какого-либо качества или свойства.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <никакой> не герой, а обычный человек.",
                            "target_lang_translations": {
                                "en": "He is <no> hero, but an ordinary person."
                            }
                        },
                        {
                            "text": "Это <никакая> не шутка, я говорю серьёзно.",
                            "target_lang_translations": {
                                "en": "This is <no> joke, I am speaking seriously."
                            }
                        }
                    ],
                    "synonyms": [
                        "во́все не",
                        "отню́дь"
                    ],
                    "antonyms": [
                        "самый что ни на есть"
                    ],
                    "common_phrases": [
                        "никакой не"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Not at all, in no way; used to emphatically negate a quality or characteristic."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "no",
                                "target_lang_sense_clarification": "emphatic negation before a noun (e.g., he is no hero)"
                            },
                            {
                                "target_lang_word": "not at all",
                                "target_lang_sense_clarification": "intensifier for negation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd1a67f8-0d41-3f97-a108-485696ad1e90",
                    "sense_definition": "Очень плохой, посредственный или находящийся в крайне плохом физическом состоянии (от усталости или болезни).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После бессонной ночи я был совсем <никакой>.",
                            "target_lang_translations": {
                                "en": "After a sleepless night, I was completely <out of it>."
                            }
                        },
                        {
                            "text": "Фильм оказался <никакой>, мне стало скучно через десять минут.",
                            "target_lang_translations": {
                                "en": "The film turned out to be <mediocre>; I got bored after ten minutes."
                            }
                        }
                    ],
                    "synonyms": [
                        "никакущий"
                    ],
                    "common_phrases": [
                        "совсем никакой"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "жаргон"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Very bad, mediocre, or in extremely poor physical condition (due to fatigue or illness)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mediocre",
                                "target_lang_sense_clarification": "lacking quality or interest"
                            },
                            {
                                "target_lang_word": "dull",
                                "target_lang_sense_clarification": "boring or uninteresting"
                            },
                            {
                                "target_lang_word": "exhausted",
                                "target_lang_sense_clarification": "lacking energy or vitality"
                            },
                            {
                                "target_lang_word": "out of it",
                                "target_lang_sense_clarification": "colloquial: tired, unresponsive, or unwell"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
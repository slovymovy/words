    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "418de9b7-9969-3f1d-8df2-8538cdd053a8",
                    "sense_definition": "Потерпеть поражение в соревновании, борьбе или споре; уступить сопернику.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "loss_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша футбольная команда вчера <w>проиграла</w> важный матч.",
                            "target_lang_translations": {
                                "en": "Our football team <w>lost</w> an important match yesterday."
                            }
                        },
                        {
                            "text": "Генерал понимал, что армия может <w>проиграть</w> это сражение.",
                            "target_lang_translations": {
                                "en": "The general understood that the army might <w>lose</w> this battle."
                            }
                        }
                    ],
                    "synonyms": [
                        "уступить"
                    ],
                    "antonyms": [
                        "вы́играть"
                    ],
                    "common_phrases": [
                        "проиграть войну",
                        "проиграть выборы",
                        "проиграть матч",
                        "проиграть спор"
                    ],
                    "target_lang_definitions": {
                        "en": "To suffer defeat in a competition, struggle, or dispute; to yield to an opponent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lose",
                                "target_lang_sense_clarification": "to be defeated"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bd1ddf0-f801-3789-aab8-977556b8ac2f",
                    "sense_definition": "Лишиться денег или имущества в результате неудачной азартной игры.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "loss_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он пошёл в казино и <w>проиграл</w> там всю свою зарплату.",
                            "target_lang_translations": {
                                "en": "He went to the casino and <w>lost</w> his entire salary there."
                            }
                        },
                        {
                            "text": "Не стоит садиться за стол, если боишься <w>проиграть</w> деньги.",
                            "target_lang_translations": {
                                "en": "You shouldn't sit at the table if you are afraid to <w>lose</w> money."
                            }
                        }
                    ],
                    "antonyms": [
                        "вы́играть"
                    ],
                    "common_phrases": [
                        "проиграть деньги",
                        "проиграть всё",
                        "проиграть в карты",
                        "проиграть состояние"
                    ],
                    "target_lang_definitions": {
                        "en": "To be deprived of money or property as a result of unsuccessful gambling."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lose",
                                "target_lang_sense_clarification": "money in gambling"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3f54550-7205-3b81-b113-78a18296a192",
                    "sense_definition": "Исполнить музыкальное произведение, запись или роль (обычно для проверки или репетиции).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пианист решил ещё раз <w>проиграть</w> сложную мелодию перед концертом.",
                            "target_lang_translations": {
                                "en": "The pianist decided to <w>play</w> the complex melody one more time before the concert."
                            }
                        },
                        {
                            "text": "Я попросил диджея <w>проиграть</w> эту песню для моих друзей.",
                            "target_lang_translations": {
                                "en": "I asked the DJ to <w>play</w> this song for my friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "исполнить"
                    ],
                    "common_phrases": [
                        "проиграть мелодию",
                        "проиграть пластинку",
                        "проиграть гамму"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform a musical piece, recording, or role (usually for checking or rehearsal)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "play",
                                "target_lang_sense_clarification": "music or recording"
                            },
                            {
                                "target_lang_word": "play through",
                                "target_lang_sense_clarification": "from beginning to end"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
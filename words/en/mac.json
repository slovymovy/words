    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d6b2eca1-17c0-3342-8cad-f4ff4d998dba",
                    "sense_definition": "A <w>Macintosh</w> computer made by Apple Inc.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do all my graphic design work on a <w>Mac</w> because I prefer the interface.",
                            "target_lang_translations": {
                                "ru": "Я делаю всю свою работу по графическому дизайну на <w>Маке</w>, потому что мне больше нравится интерфейс."
                            }
                        },
                        {
                            "text": "She has been a loyal <w>Mac</w> user for over ten years.",
                            "target_lang_translations": {
                                "ru": "Она преданный пользователь <w>Мака</w> уже более десяти лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Mac user",
                        "on a Mac",
                        "PC and Mac"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "clipping"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютер Macintosh производства Apple."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мак",
                                "target_lang_sense_clarification": "компьютер"
                            },
                            {
                                "target_lang_word": "Макинтош",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8bd589f-9118-35f2-8df6-f47b06ec2245",
                    "sense_definition": "<w>Macaroni</w> pasta, usually eaten with cheese sauce.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The kids want <w>mac</w> and cheese for dinner again tonight.",
                            "target_lang_translations": {
                                "ru": "Дети сегодня снова хотят <w>макароны</w> с сыром на ужин."
                            }
                        },
                        {
                            "text": "I ordered a steak with a side of creamy <w>mac</w>.",
                            "target_lang_translations": {
                                "ru": "Я заказал стейк с гарниром из сливочных <w>макарон</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mac and cheese"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "clipping"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Макароны, обычно подаваемые с сырным соусом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "макароны",
                                "target_lang_sense_clarification": "еда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adf2d300-05b2-3ed6-a5f3-2b696fad011c",
                    "sense_definition": "A waterproof raincoat, specifically a <w>mackintosh</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It looks like it might rain, so you should take your <w>mac</w>.",
                            "target_lang_translations": {
                                "ru": "Похоже, будет дождь, так что тебе стоит взять свой <w>плащ</w>."
                            }
                        },
                        {
                            "text": "He wore a long grey <w>mac</w> to keep his suit dry.",
                            "target_lang_translations": {
                                "ru": "На нем был длинный серый <w>макинтош</w>, чтобы не намочить костюм."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plastic mac"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "clipping"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Водонепроницаемый плащ, в частности макинтош."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плащ",
                                "target_lang_sense_clarification": "одежда"
                            },
                            {
                                "target_lang_word": "макинтош",
                                "target_lang_sense_clarification": "тип плаща"
                            },
                            {
                                "target_lang_word": "дождевик",
                                "target_lang_sense_clarification": "от дождя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cdbb999-acdc-333b-8d74-5b0942b809c6",
                    "sense_definition": "Used to address a man whose name is unknown.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hey <w>Mac</w>, do you have a light for my cigarette?",
                            "target_lang_translations": {
                                "ru": "Эй, <w>приятель</w>, у тебя не найдется прикурить?"
                            }
                        },
                        {
                            "text": "Watch where you are driving, <w>Mac</w>!",
                            "target_lang_translations": {
                                "ru": "Смотри, куда едешь, <w>парень</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "John"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращение к незнакомому мужчине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приятель",
                                "target_lang_sense_clarification": "обращение"
                            },
                            {
                                "target_lang_word": "друг",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "парень",
                                "target_lang_sense_clarification": "фамильярное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de72678e-0b47-39eb-b6af-239d05ad88f3",
                    "sense_definition": "Initialism of media access control; a unique identifier assigned to a network interface controller for use as a network address.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To secure the Wi-Fi network, we filter devices by their <w>MAC</w> address.",
                            "target_lang_translations": {
                                "ru": "Чтобы обезопасить сеть Wi-Fi, мы фильтруем устройства по их <w>MAC</w>-адресу."
                            }
                        },
                        {
                            "text": "You can usually find the <w>MAC</w> printed on a sticker on the back of the router.",
                            "target_lang_translations": {
                                "ru": "Обычно <w>MAC</w> напечатан на наклейке на задней панели роутера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "MAC address"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уникальный идентификатор сетевого устройства (MAC-адрес)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "MAC-адрес",
                                "target_lang_sense_clarification": "сетевой термин"
                            }
                        ]
                    }
                }
            ]
        },
        {

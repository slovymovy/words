    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6daf4994-c028-318a-be99-d027b431a12f",
                    "sense_definition": "Связанный с движением по замкнутому кругу; образующий замкнутую линию (о дороге, маршруте, транспорте).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы выехали на <w>кольцевую</w> дорогу, чтобы быстрее добраться до аэропорта.",
                            "target_lang_translations": {
                                "en": "We drove onto the <w>ring</w> road to get to the airport faster."
                            }
                        },
                        {
                            "text": "Автобус ходит по <w>кольцевому</w> маршруту и возвращается на ту же остановку.",
                            "target_lang_translations": {
                                "en": "The bus follows a <w>circular</w> route and returns to the same stop."
                            }
                        },
                        {
                            "text": "В центре города организовано удобное <w>кольцевое</w> движение для автомобилей.",
                            "target_lang_translations": {
                                "en": "Convenient <w>circular</w> traffic has been organized for cars in the city center."
                            }
                        }
                    ],
                    "synonyms": [
                        "круговой",
                        "замкнутый"
                    ],
                    "antonyms": [
                        "линейный",
                        "прямой"
                    ],
                    "common_phrases": [
                        "кольцевая дорога",
                        "кольцевой маршрут",
                        "кольцевое движение",
                        "кольцевая линия"
                    ],
                    "target_lang_definitions": {
                        "en": "Related to movement in a closed circle; forming a closed loop (of a road, route, or transport)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ring",
                                "target_lang_sense_clarification": "used in 'ring road'"
                            },
                            {
                                "target_lang_word": "circular",
                                "target_lang_sense_clarification": "moving in a circle, e.g., a route"
                            },
                            {
                                "target_lang_word": "orbital",
                                "target_lang_sense_clarification": "referring to a road encircling a city"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ced774f6-4ee1-321a-9bb2-168e04ce9cbc",
                    "sense_definition": "Имеющий форму кольца или относящийся к кольцу как к предмету.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Астрономы предупредили, что завтра произойдёт <w>кольцевое</w> солнечное затмение.",
                            "target_lang_translations": {
                                "en": "Astronomers warned that an <w>annular</w> solar eclipse will occur tomorrow."
                            }
                        },
                        {
                            "text": "Спортсмен долго готовился к сложной <w>кольцевой</w> гонке на новом треке.",
                            "target_lang_translations": {
                                "en": "The athlete prepared for a long time for the difficult <w>circuit</w> race on the new track."
                            }
                        },
                        {
                            "text": "Для ремонта механизма потребовалась специальная <w>кольцевая</w> прокладка.",
                            "target_lang_translations": {
                                "en": "A special <w>ring</w> gasket was required to repair the mechanism."
                            }
                        }
                    ],
                    "synonyms": [
                        "кольцеобразный"
                    ],
                    "common_phrases": [
                        "кольцевая гонка",
                        "кольцевое затмение",
                        "кольцевая структура"
                    ],
                    "target_lang_definitions": {
                        "en": "Having the shape of a ring or relating to a ring as an object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "annular",
                                "target_lang_sense_clarification": "ring-shaped, especially regarding eclipses"
                            },
                            {
                                "target_lang_word": "circuit",
                                "target_lang_sense_clarification": "relating to racing tracks"
                            },
                            {
                                "target_lang_word": "ring-shaped",
                                "target_lang_sense_clarification": "general description of form"
                            }
                        ]
                    }
                }
            ]
        }
    ]

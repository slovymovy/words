            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a90891c7-aa66-3fad-b6ab-9b13da806b07",
                    "sense_definition": "Grof gemalen graan of metaalafval.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brouwer controleerde of het <w>schrot</w> de juiste grofheid had voor het maischen.",
                            "target_lang_translations": {
                                "en": "The brewer checked if the <w>grist</w> had the right coarseness for mashing.",
                                "ru": "Пивовар проверял, имеет ли <w>шрот</w> правильный помол для затирания."
                            }
                        },
                        {
                            "text": "In de oude loods lag nog een hoop roestig <w>schrot</w> opgeslagen.",
                            "target_lang_translations": {
                                "en": "A pile of rusty <w>scrap</w> was still stored in the old shed.",
                                "ru": "В старом сарае лежала куча ржавого <w>металлолома</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "schroot"
                    ],
                    "common_phrases": [
                        "grof schrot",
                        "fijn schrot"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "variant of 'schroot'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Coarsely ground grain or metal waste.",
                        "ru": "Грубо помолотое зерно или металлический лом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grist",
                                "target_lang_sense_clarification": "ground grain for brewing"
                            },
                            {
                                "target_lang_word": "scrap",
                                "target_lang_sense_clarification": "metal waste"
                            },
                            {
                                "target_lang_word": "crushed grain",
                                "target_lang_sense_clarification": "general"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шрот",
                                "target_lang_sense_clarification": "зерно, пивоварение"
                            },
                            {
                                "target_lang_word": "металлолом",
                                "target_lang_sense_clarification": "металл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8aa38cf0-e144-32ad-a171-93faabfe572f",
                    "sense_definition": "Kleine, minderwaardige exemplaren van fruit, met name appels en peren.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fruit_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer scheidde het <w>schrot</w> van de appels die voor de verkoop bestemd waren.",
                            "target_lang_translations": {
                                "en": "The farmer separated the <w>culls</w> from the apples intended for sale.",
                                "ru": "Фермер отделял <w>некондицию</w> от яблок, предназначенных для продажи."
                            }
                        },
                        {
                            "text": "Van het overgebleven <w>schrot</w> werd uiteindelijk appelmoes gemaakt.",
                            "target_lang_translations": {
                                "en": "Applesauce was eventually made from the remaining <w>refuse fruit</w>.",
                                "ru": "Из оставшейся <w>отбраковки</w> в итоге сделали яблочное пюре."
                            }
                        }
                    ],
                    "synonyms": [
                        "kriel",
                        "uitschot",
                        "grut"
                    ],
                    "target_lang_definitions": {
                        "en": "Small, inferior specimens of fruit, especially apples and pears.",
                        "ru": "Мелкие, некачественные плоды, особенно яблоки и груши."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "culls",
                                "target_lang_sense_clarification": "rejected produce"
                            },
                            {
                                "target_lang_word": "refuse fruit",
                                "target_lang_sense_clarification": "inferior quality fruit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "некондиция",
                                "target_lang_sense_clarification": "фрукты низкого качества"
                            },
                            {
                                "target_lang_word": "отбраковка",
                                "target_lang_sense_clarification": "отсортированные плоды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4dce27b5-a2b8-3fdc-bbaf-b12dc9d6810d",
                    "sense_definition": "Een laag van basaltstenen die dient als verharding of bescherming, bijvoorbeeld bij dijken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aannemer bracht een nieuwe laag <w>schrot</w> aan om de dijk te versterken.",
                            "target_lang_translations": {
                                "en": "The contractor applied a new layer of <w>riprap</w> to reinforce the dike.",
                                "ru": "Подрядчик уложил новый слой <w>щебня</w>, чтобы укрепить дамбу."
                            }
                        },
                        {
                            "text": "Het <w>schrot</w> zorgt ervoor dat de onderliggende grond niet wegspoelt door het water.",
                            "target_lang_translations": {
                                "en": "The <w>rubble</w> ensures that the underlying soil does not wash away due to the water.",
                                "ru": "<w>Каменная наброска</w> предотвращает размывание грунта водой."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A layer of basalt stones serving as hardening or protection.",
                        "ru": "Слой базальтовых камней, служащий для укрепления или защиты, например, дамб."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "riprap",
                                "target_lang_sense_clarification": "protective stone layer"
                            },
                            {
                                "target_lang_word": "rubble",
                                "target_lang_sense_clarification": "broken stones"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "каменная наброска",
                                "target_lang_sense_clarification": "гидротехника"
                            },
                            {
                                "target_lang_word": "щебень",
                                "target_lang_sense_clarification": "строительный материал"
                            }
                        ]
                    }
                }
            ]
        }
    ]

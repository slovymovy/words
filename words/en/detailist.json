                    "synonyms": [
                        "intricate",
                        "elaborate",
                        "detailed"
                    ],
                    "antonyms": [
                        "simple",
                        "sketchy"
                    ],
                    "common_phrases": [
                        "detailist style",
                        "detailist work"
                    ],

                            {
                                "target_lang_word": "отель",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "гостиница",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8050895-68b7-358b-b285-0267231754c1",
                    "sense_definition": "Een tijdelijk verblijf voor huisdieren, bijvoorbeeld wanneer de eigenaars op reis zijn",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Onze hond gaat naar een <w>hotel</w> als wij in het buitenland zijn.",

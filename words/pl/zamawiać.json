            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3f421e75-127a-32cf-bf4a-b001732a89fc",
                    "sense_definition": "Składać polecenie dostarczenia towaru, posiłku lub wykonania usługi, za które trzeba zapłacić.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transaction_ordering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdy piątek <zamawiamy> pizzę na kolację do domu.",
                            "target_lang_translations": {
                                "ru": "Каждую пятницу мы <заказываем> пиццу на ужин домой."
                            }
                        },
                        {
                            "text": "Czy już <zamawiałeś> taksówkę na lotnisko, czy mam to zrobić teraz?",
                            "target_lang_translations": {
                                "ru": "Ты уже <заказывал> такси в аэропорт или мне сделать это сейчас?"
                            }
                        }
                    ],
                    "synonyms": [
                        "najmować"
                    ],
                    "common_phrases": [
                        "zamawiać pizzę",
                        "zamawiać taksówkę",
                        "zamawiać kuriera",
                        "zamawiać przez internet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать заказ на доставку товара, еды или выполнение услуги, подлежащей оплате."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заказывать",
                                "target_lang_sense_clarification": "товар, услугу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4c1709e-da44-319a-8d72-9f903da84420",
                    "sense_definition": "Dokonywać rezerwacji miejsca lub usługi do wykorzystania w przyszłości.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transaction_ordering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy wcześniej <zamawiać> pokój w hotelu, bo to popularne miejsce.",
                            "target_lang_translations": {
                                "ru": "Мы должны заранее <бронировать> номер в отеле, потому что это популярное место."
                            }
                        },
                        {
                            "text": "On zawsze <zamawia> ten sam stolik w naszej ulubionej restauracji.",
                            "target_lang_translations": {
                                "ru": "Он всегда <заказывает> один и тот же столик в нашем любимом ресторане."
                            }
                        }
                    ],
                    "synonyms": [
                        "rezerwować"
                    ],
                    "common_phrases": [
                        "zamawiać pokój",
                        "zamawiać bilet",
                        "zamawiać stolik",
                        "zamawiać wizytę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бронировать место или услугу для использования в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бронировать",
                                "target_lang_sense_clarification": "номер, билет"
                            },
                            {
                                "target_lang_word": "заказывать",
                                "target_lang_sense_clarification": "столик, место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14fcaee6-2555-39e8-b15e-a7c486b13cad",
                    "sense_definition": "Odprawiać magiczne rytuały w celu usunięcia choroby lub uroku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "magic_rituals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stara znachorka <zamawiała> ból głowy tajemniczymi słowami.",
                            "target_lang_translations": {
                                "ru": "Старая знахарка <заговаривала> головную боль таинственными словами."
                            }
                        },
                        {
                            "text": "W dawnych wierzeniach ludzie wiedzieli, jak <zamawiać> złe uroki.",
                            "target_lang_translations": {
                                "ru": "В древних поверьях люди знали, как <заговаривать> злые чары."
                            }
                        }
                    ],
                    "synonyms": [
                        "odczyniać"
                    ],
                    "common_phrases": [
                        "zamawiać chorobę",
                        "zamawiać uroki"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "odnosi się do dawnych praktyk ludowych"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершать магические ритуалы для избавления от болезни или порчи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заговаривать",
                                "target_lang_sense_clarification": "болезнь, боль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a4f96b9-a8d5-3748-81a7-23e6d486bb52",
                    "sense_definition": "Zapowiadać swoje przybycie lub wizytę u kogoś.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "social_etiquette",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hrabia listownie <zamawiał się> na wizytę w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Граф письменно <договаривался> о визите на следующей неделе."
                            }
                        },
                        {
                            "text": "Nie wypada przychodzić bez uprzedzenia, trzeba się wcześniej <zamawiać>.",
                            "target_lang_translations": {
                                "ru": "Неприлично приходить без предупреждения, нужно заранее <договариваться о встрече>."
                            }
                        }
                    ],
                    "synonyms": [
                        "anonsować się",
                        "zapowiadać się"
                    ],
                    "common_phrases": [

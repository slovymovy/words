    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a8311f9f-7cf2-3d47-840a-a46379dee3ad",
                    "sense_definition": "Odznaczający się dużą wiedzą, inteligencją i doświadczeniem życiowym; potrafiący podejmować właściwe decyzje.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "wisdom_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek to bardzo <w>mądry</w> człowiek, który wiele w życiu widział.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка — очень <w>мудрый</w> человек, который многое повидал в жизни."
                            }
                        },
                        {
                            "text": "Musisz być <w>mądry</w> i uważać na siebie w nowym mieście.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть <w>умным</w> и беречь себя в новом городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "inteligentny",
                        "rozumny"
                    ],
                    "antonyms": [
                        "głupi",
                        "niemądry"
                    ],
                    "common_phrases": [
                        "mądry człowiek",
                        "mądra głowa",
                        "mądre dziecko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличающийся большими знаниями, интеллектом и жизненным опытом; способный принимать правильные решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мудрый",
                                "target_lang_sense_clarification": "обладающий большим опытом и глубоким умом"
                            },
                            {
                                "target_lang_word": "умный",
                                "target_lang_sense_clarification": "обладающий развитым интеллектом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e122840-f009-30ba-90b3-c6a59547721a",
                    "sense_definition": "Świadczący o rozsądku, przemyśleniu i dobrej ocenie sytuacji (o rzeczach, decyzjach, słowach).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "wisdom_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To była bardzo <w>mądra</w> decyzja, która przyniosła firmie duże zyski.",
                            "target_lang_translations": {
                                "ru": "Это было очень <w>мудрое</w> решение, которое принесло фирме большую прибыль."
                            }
                        },
                        {
                            "text": "Dziękuję ci za twoją <w>mądrą</w> radę, która pomogła mi rozwiązać ten problem.",
                            "target_lang_translations": {
                                "ru": "Спасибо тебе за твой <w>мудрый</w> совет, который помог мне решить эту проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozsądny",
                        "rozważny",
                        "roztropny"
                    ],
                    "antonyms": [
                        "głupi",
                        "niemądry"
                    ],
                    "common_phrases": [
                        "mądra decyzja",
                        "mądry wybór",
                        "mądra rada",
                        "mądre słowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свидетельствующий о здравом смысле, обдуманности и хорошей оценке ситуации (о вещах, решениях, словах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мудрый",
                                "target_lang_sense_clarification": "о решениях, советах или поступках"
                            },
                            {
                                "target_lang_word": "разумный",
                                "target_lang_sense_clarification": "логичный, обоснованный, целесообразный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec793341-e6f2-304c-94fc-53da40a561ff",
                    "sense_definition": "Przemądrzały, uważający się za lepszego od innych (często używane ironicznie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ironic_wisdom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Skoro jesteś taki <w>mądry</w>, to sam napraw ten zepsuty silnik.",
                            "target_lang_translations": {
                                "ru": "Раз ты такой <w>умный</w>, то сам почини этот сломанный двигатель."
                            }
                        },
                        {
                            "text": "Nie bądź taki <w>mądry</w>, bo wcale nie znasz poprawnej odpowiedzi na to pytanie.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>умным</w>, ведь ты вовсе не знаешь правильного ответа на этот вопрос."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nie bądź taki mądry"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często ironicznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Самоуверенный, считающий себя лучше других (часто используется иронично)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умный",
                                "target_lang_sense_clarification": "в ироничном значении: считающий себя умнее всех"
                            }
                        ]
                    }
                }
            ]
        },
        {

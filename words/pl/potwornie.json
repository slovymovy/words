{
    "word_family": [
        "potwór",
        "potworny",
        "potworność"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "60b6851b-9c56-32f4-825d-bf0ffe7e07d4",
                    "sense_definition": "Bardzo, w ogromnym stopniu; słowo używane do wzmocnienia znaczenia przymiotnika, przysłówka lub czasownika (często o zabarwieniu negatywnym, ale nie tylko).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem dzisiaj <w>potwornie</w> zmęczony po całym dniu ciężkiej pracy.",
                            "target_lang_translations": {
                                "ru": "Я сегодня <w>ужасно</w> устал после целого дня тяжелой работы."
                            }
                        },
                        {
                            "text": "Ten nowy samochód jest <w>potwornie</w> drogi, nie stać mnie na niego.",
                            "target_lang_translations": {
                                "ru": "Эта новая машина <w>чудовищно</w> дорогая, она мне не по карману."
                            }
                        }
                    ],
                    "synonyms": [
                        "bardzo",
                        "ogromnie",
                        "strasznie",
                        "okropnie",
                        "nienormalnie",
                        "horrendalnie"
                    ],
                    "common_phrases": [
                        "potwornie zmęczony",
                        "potwornie drogi",
                        "potwornie nudny",
                        "potwornie głodny"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used for emphasis"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень, в огромной степени; используется для усиления значения прилагательного, наречия или глагола (часто с негативным оттенком)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ужасно",
                                "target_lang_sense_clarification": "разговорное усиление (очень сильно)"
                            },
                            {
                                "target_lang_word": "страшно",
                                "target_lang_sense_clarification": "в значении «очень»"
                            },
                            {
                                "target_lang_word": "чудовищно",
                                "target_lang_sense_clarification": "чрезмерно, невероятно"
                            },
                            {
                                "target_lang_word": "жутко",
                                "target_lang_sense_clarification": "разговорное (напр. жутко интересно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fac3035e-5e76-3b13-9be0-b4fe66b5e235",
                    "sense_definition": "W sposób, który budzi strach, grozę lub przerażenie; jak potwór.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bestia zaryczała <w>potwornie</w>, a wszyscy ludzie uciekli w popłochu.",
                            "target_lang_translations": {
                                "ru": "Чудовище зарычало <w>страшно</w>, и все люди в панике разбежались."
                            }
                        },
                        {
                            "text": "Jego twarz w ciemności wyglądała <w>potwornie</w> i nienaturalnie.",
                            "target_lang_translations": {
                                "ru": "Его лицо в темноте выглядело <w>чудовищно</w> и неестественно."
                            }
                        }
                    ],
                    "synonyms": [
                        "przerażająco",
                        "strasznie",
                        "koszmarnie",
                        "okrutnie"
                    ],
                    "common_phrases": [
                        "wyglądać potwornie",
                        "ryczeć potwornie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пугающе, вселяя ужас или страх; подобно чудовищу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чудовищно",
                                "target_lang_sense_clarification": "как монстр, уродливо"
                            },
                            {
                                "target_lang_word": "жутко",
                                "target_lang_sense_clarification": "зловеще, страшно"
                            },
                            {
                                "target_lang_word": "страшно",
                                "target_lang_sense_clarification": "вызывая страх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "216ee298-559e-3e4a-be31-0c20655a686c",
                    "sense_definition": "W sposób wywołujący wstręt, odrazę lub silną niechęć; bardzo brzydko lub nieprzyjemnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disgust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna zachował się <w>potwornie</w> wobec starszej pani w autobusie.",
                            "target_lang_translations": {
                                "ru": "Мужчина повел себя <w>безобразно</w> по отношению к пожилой женщине в автобусе."
                            }
                        },
                        {
                            "text": "W tym starym, zawilgoconym pomieszczeniu śmierdzi <w>potwornie</w>.",
                            "target_lang_translations": {
                                "ru": "В этом старом, сыром помещении <w>ужасно</w> воняет."
                            }
                        }
                    ],
                    "synonyms": [
                        "odrażająco",
                        "wstrętnie",
                        "paskudnie",
                        "okropnie"
                    ],
                    "common_phrases": [
                        "zachować się potwornie",
                        "śmierdzieć potwornie",
                        "traktować potwornie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отвратительно, мерзко, вызывая сильную неприязнь; очень плохо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвратительно",
                                "target_lang_sense_clarification": "мерзко, гадко"
                            },
                            {
                                "target_lang_word": "безобразно",
                                "target_lang_sense_clarification": "о поведении или внешнем виде"
                            },
                            {
                                "target_lang_word": "ужасно",
                                "target_lang_sense_clarification": "очень плохо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "123b4877-d265-30e6-a66e-3e43607fb5b1",
                    "sense_definition": "Het ondernemen van iets of het in beweging zijn; daadkrachtig optreden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brandweer kwam direct in <w>actie</w> na de melding van de brand.",
                            "target_lang_translations": {
                                "ru": "Пожарная команда немедленно приступила к <w>действиям</w> после сообщения о пожаре."
                            }
                        },
                        {
                            "text": "We moeten nu <w>actie</w> ondernemen om het probleem op te lossen.",
                            "target_lang_translations": {
                                "ru": "Мы должны принять <w>меры</w> сейчас, чтобы решить проблему."
                            }

                        "gestrest"
                    ],
                    "common_phrases": [
                        "ontspannen sfeer",
                        "ontspannen houding"
                    ],
                    "target_lang_definitions": {
                        "en": "Free from tension; calm and at ease.",
                        "ru": "Лишенный напряжения; спокойный и непринужденный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "relaxed",
                                "target_lang_sense_clarification": "free from stress or anxiety"
                            },
                            {
                                "target_lang_word": "laid-back",
                                "target_lang_sense_clarification": "easygoing in nature"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "расслабленный",
                                "target_lang_sense_clarification": "свободный от напряжения"
                            },
                            {
                                "target_lang_word": "непринужденный",
                                "target_lang_sense_clarification": "естественный, свободный (о поведении или атмосфере)"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "26331779-b603-3cff-8d0f-7724c2ecb161",
                    "sense_definition": "Een groot bassin gevuld met water om in te zwemmen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze maakten 's winters het <w>zwembad</w> leeg.",
                            "target_lang_translations": {
                                "ru": "Зимой они спускали воду из <w>бассейна</w>."
                            }
                        },
                        {
                            "text": "Het water in het <w>zwembad</w> is lekker warm.",
                            "target_lang_translations": {
                                "ru": "Вода в <w>бассейне</w> приятно теплая."
                            }
                        }
                    ],
                    "synonyms": [
                        "bassin"
                    ],
                    "common_phrases": [
                        "in het zwembad",
                        "diep zwembad",
                        "zwembad in de tuin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой резервуар, наполненный водой, предназначенный для плавания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бассейн",
                                "target_lang_sense_clarification": "резервуар с водой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6aac9e87-56e3-3619-81ff-aee77179ce83",
                    "sense_definition": "Een gebouw of inrichting waar mensen naartoe gaan om te zwemmen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jeugd gaat tegenwoordig graag naar het <w>zwembad</w> op een warme zomerse dag.",
                            "target_lang_translations": {
                                "ru": "Молодежь сегодня любит ходить в <w>бассейн</w> в жаркий летний день."
                            }
                        },
                        {
                            "text": "In onze stad is een nieuw <w>zwembad</w> geopend.",
                            "target_lang_translations": {
                                "ru": "В нашем городе открылся новый <w>бассейн</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naar het zwembad",
                        "openbaar zwembad",
                        "overdekt zwembad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание или комплекс, куда люди приходят, чтобы поплавать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бассейн",
                                "target_lang_sense_clarification": "здание или учреждение"
                            }
                        ]
                    }
                }
            ]

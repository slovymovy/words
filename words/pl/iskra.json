            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6f0dc761-052b-30a4-997a-e06c28114003",
                    "sense_definition": "Mała, rozżarzona i świecąca cząstka, która oddziela się od płonącego materiału lub powstaje podczas tarcia twardych przedmiotów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fire_physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z ogniska wyleciała jasna <w>iskra</w> i spadła na suchą trawę.",
                            "target_lang_translations": {
                                "ru": "Из костра вылетела яркая <w>искра</w> и упала на сухую траву."
                            }
                        },
                        {
                            "text": "Kiedy pociąg gwałtownie hamował, spod kół sypały się <w>iskry</w>.",
                            "target_lang_translations": {
                                "ru": "Когда поезд резко тормозил, из-под колёс сыпались <w>искры</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "skra"
                    ],
                    "common_phrases": [
                        "sypać iskrami",
                        "krzesać iskry",
                        "iskra elektryczna",
                        "iskra nadziei"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мельчайшая частица горящего или раскалённого вещества, образующаяся при горении или трении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искра",
                                "target_lang_sense_clarification": "частица огня"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8491bf08-1da5-3371-97d1-e2b48f04eb5a",
                    "sense_definition": "Osoba bardzo energiczna, żywiołowa, szybka i pełna temperamentu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej najmłodsza córka to prawdziwa <w>iskra</w>, nie potrafi usiedzieć w jednym miejscu.",
                            "target_lang_translations": {
                                "ru": "Её младшая дочь — настоящий <w>огонь</w>, она не может усидеть на одном месте."
                            }
                        },
                        {
                            "text": "Potrzebujemy do tego zadania kogoś aktywnego, a on to <w>iskra</w>, nie człowiek.",
                            "target_lang_translations": {
                                "ru": "Нам нужен для этой задачи кто-то активный, а он — <w>огонь</w>, а не человек."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prawdziwa iskra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень энергичный, живой, быстрый и темпераментный человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "огонь",
                                "target_lang_sense_clarification": "об энергичном человеке"
                            },
                            {
                                "target_lang_word": "живчик",
                                "target_lang_sense_clarification": "о подвижном человеке"
                            }
                        ]
                    }
                }
            ]
        }
    ]

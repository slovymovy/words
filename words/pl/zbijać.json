    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d2b83338-f155-3672-b047-6f475834edc7",
                    "sense_definition": "Niszczyć kruchy przedmiot, taki jak szkło czy porcelana, uderzając w niego lub upuszczając go, co powoduje jego rozpad na kawałki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje dziecko ciągle przypadkowo <w>zbija</w> bombki choinkowe.",
                            "target_lang_translations": {
                                "ru": "Мой ребенок постоянно случайно <w>разбивает</w> елочные игрушки."
                            }
                        },
                        {
                            "text": "Uważaj, żebyś nie <w>zbijał</w> naczyń podczas mycia w zlewie.",
                            "target_lang_translations": {
                                "ru": "Смотри, не <w>бей</w> посуду, когда моешь ее в раковине."
                            }
                        }
                    ],
                    "synonyms": [
                        "bić",
                        "zganiać",
                        "bić"
                    ],
                    "common_phrases": [
                        "zbijać talerze",
                        "zbijać szklanki",
                        "zbijać naczynia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разбивать хрупкий предмет (например, стекло или фарфор) ударом или при падении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбивать",
                                "target_lang_sense_clarification": "разрушать на куски"
                            },
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "ударять с целью разбить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b042dff3-b6e1-374f-acbd-fd69b4a25867",
                    "sense_definition": "Eliminować pionki lub figury przeciwnika z planszy w trakcie gry, zajmując ich miejsce lub wykonując określony ruch.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej grze planszowej <w>zbijasz</w> pionki rywala, rzucając kostką.",
                            "target_lang_translations": {
                                "ru": "В этой настольной игре ты <w>сбиваешь</w> фишки соперника, бросая кубик."
                            }
                        },
                        {
                            "text": "Nie zawsze opłaca się <w>zbijać</w> hetmana, jeśli odsłaniasz króla.",
                            "target_lang_translations": {
                                "ru": "Не всегда выгодно <w>бить</w> ферзя, если открываешь короля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbijać pionki",
                        "zbijać figury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убирать фигуры или фишки противника с доски во время игры, занимая их место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "брать фигуру в игре"
                            },
                            {
                                "target_lang_word": "сбивать",
                                "target_lang_sense_clarification": "удалять фишку с поля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7cd81bb-afbf-3519-abb5-c1434ef310d4",
                    "sense_definition": "Łączyć drewniane elementy w całość, używając do tego gwoździ i młotka.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stolarz sprawnie <w>zbija</w> deski, tworząc solidną skrzynię na owoce.",
                            "target_lang_translations": {
                                "ru": "Столяр ловко <w>сбивает</w> доски, делая прочный ящик для фруктов."
                            }
                        },
                        {
                            "text": "Będziemy <w>zbijali</w> nową budę dla psa w ten weekend.",
                            "target_lang_translations": {
                                "ru": "Мы будем <w>сколачивать</w> новую будку для собаки в эти выходные."
                            }
                        }
                    ],
                    "antonyms": [
                        "rozbijać"
                    ],
                    "common_phrases": [
                        "zbijać deski",
                        "zbijać palety",
                        "zbijać skrzynki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединять деревянные элементы в единое целое с помощью гвоздей и молотка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбивать",
                                "target_lang_sense_clarification": "соединять доски"
                            },
                            {
                                "target_lang_word": "сколачивать",
                                "target_lang_sense_clarification": "мастерить из дерева"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "472b29c6-ff42-3db7-b201-b36a7b825b68",
                    "sense_definition": "Wykazywać fałszywość czyjegoś twierdzenia, argumentu lub dowodu w dyskusji.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "argumentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Adwokat skutecznie <w>zbijał</w> kolejne argumenty oskarżyciela w sądzie.",
                            "target_lang_translations": {
                                "ru": "Адвокат успешно <w>разбивал</w> очередные доводы обвинителя в суде."
                            }
                        },
                        {
                            "text": "Trudno jest <w>zbijać</w> logiczne dowody oparte na twardych faktach.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>опровергать</w> логические доказательства, основанные на твердых фактах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbijać argumenty",
                        "zbijać dowody",
                        "zbijać zarzuty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доказывать ложность чьего-либо утверждения, аргумента или доказательства в споре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбивать",
                                "target_lang_sense_clarification": "опровергать доводы"

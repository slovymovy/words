{
    "word_family": [
        "отрывать",
        "отрыв",
        "отрывок",
        "отрывной"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1353a298-1c23-3bb7-8f00-24a2fd66e808",
                    "sense_definition": "Отделить часть от целого, резко дёрнув или потянув.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я случайно <w>оторвал</w> пуговицу от своего пальто.",
                            "target_lang_translations": {
                                "en": "I accidentally <w>tore off</w> a button from my coat."
                            }
                        },
                        {
                            "text": "Тебе нужно <w>оторвать</w> маленький кусочек бумаги.",
                            "target_lang_translations": {
                                "en": "You need to <w>tear off</w> a small piece of paper."
                            }
                        }
                    ],
                    "synonyms": [
                        "отделить",
                        "оборвать"
                    ],
                    "antonyms": [
                        "пришить",
                        "прикрепить",
                        "приделать"
                    ],
                    "common_phrases": [
                        "оторвать пуговицу",
                        "оторвать листок",
                        "оторвать кусок",
                        "оторвать с корнем"
                    ],
                    "target_lang_definitions": {
                        "en": "To separate a part from the whole by jerking or pulling sharply."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tear off",
                                "target_lang_sense_clarification": "general physical action"
                            },
                            {
                                "target_lang_word": "rip off",
                                "target_lang_sense_clarification": "implies quick or forceful action"
                            },
                            {
                                "target_lang_word": "pull off",
                                "target_lang_sense_clarification": "to detach by pulling"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2abe9b32-8450-3115-a9b0-61fc0997ae10",
                    "sense_definition": "Заставить кого-либо прекратить какое-либо дело, отвлечь внимание.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "distraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Телефонный звонок <w>оторвал</w> меня от важной работы.",
                            "target_lang_translations": {
                                "en": "A phone call <w>tore</w> me <w>away</w> from important work."
                            }
                        },
                        {
                            "text": "Трудно <w>оторвать</w> детей от просмотра телевизора.",
                            "target_lang_translations": {
                                "en": "It is hard to <w>tear</w> children <w>away</w> from watching TV."
                            }
                        }
                    ],
                    "synonyms": [
                        "отвлечь"
                    ],
                    "common_phrases": [
                        "оторвать от дела",
                        "оторвать от работы",
                        "оторвать от учёбы"
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone to stop doing something or distract their attention."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tear away",
                                "target_lang_sense_clarification": "to force someone to leave a task or place"
                            },
                            {
                                "target_lang_word": "pull away",
                                "target_lang_sense_clarification": "to remove someone from a situation"
                            },
                            {
                                "target_lang_word": "drag away",
                                "target_lang_sense_clarification": "implies resistance or difficulty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3102bb67-e38a-35c1-94d7-ce5763bce2e0",
                    "sense_definition": "С усилием перестать смотреть на кого-либо или что-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "gaze",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он не мог <w>оторвать</w> глаз от красивой картины.",
                            "target_lang_translations": {
                                "en": "He could not <w>take</w> his eyes <w>off</w> the beautiful painting."
                            }
                        },
                        {
                            "text": "Она с трудом <w>оторвала</w> взгляд от экрана телефона.",
                            "target_lang_translations": {
                                "en": "She <w>tore</w> her gaze <w>away</w> from the phone screen with difficulty."
                            }
                        }
                    ],
                    "synonyms": [
                        "отвести"
                    ],
                    "common_phrases": [
                        "оторвать взгляд",
                        "оторвать глаза",
                        "не оторвать глаз"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop looking at someone or something with effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take off",
                                "target_lang_sense_clarification": "used specifically with 'eyes'"
                            },
                            {
                                "target_lang_word": "tear away",
                                "target_lang_sense_clarification": "used with 'eyes' or 'gaze', implies effort"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e174d5d7-4a81-39a5-a488-c604ed732ee8",
                    "sense_definition": "Отсечь или отделить часть тела или предмета в результате сильного удара или взрыва.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Взрывом снаряда танку <w>оторвало</w> башню.",
                            "target_lang_translations": {
                                "en": "The shell explosion <w>blew</w> the turret <w>off</w> the tank."
                            }
                        },
                        {
                            "text": "Будьте осторожны, этот станок может <w>оторвать</w> палец.",
                            "target_lang_translations": {
                                "en": "Be careful, this machine can <w>tear off</w> a finger."
                            }
                        }
                    ],
                    "synonyms": [
                        "отсечь",
                        "снести"
                    ],
                    "common_phrases": [
                        "оторвать руку",
                        "оторвать голову"
                    ],
                    "target_lang_definitions": {
                        "en": "To sever or separate a body part or object part due to strong impact or explosion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blow off",
                                "target_lang_sense_clarification": "caused by an explosion"
                            },
                            {
                                "target_lang_word": "tear off",
                                "target_lang_sense_clarification": "caused by mechanical force or violence"
                            },
                            {
                                "target_lang_word": "sever",
                                "target_lang_sense_clarification": "medical or technical term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c762042c-3f01-32cc-a1bf-2477d57edec3",
                    "sense_definition": "Лишить связи с домом, семьёй или привычной средой.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Война надолго <w>оторвала</w> солдата от родного дома.",
                            "target_lang_translations": {
                                "en": "The war <w>tore</w> the soldier <w>away</w> from his home for a long time."
                            }
                        },
                        {
                            "text": "Нельзя <w>оторвать</w> писателя от его культурных корней.",
                            "target_lang_translations": {
                                "en": "One cannot <w>cut</w> a writer <w>off</w> from his cultural roots."
                            }
                        }
                    ],
                    "synonyms": [
                        "разлучить",
                        "изолировать"
                    ],
                    "common_phrases": [
                        "оторвать от дома",
                        "оторвать от семьи",
                        "оторвать от реальности",
                        "оторвать от корней"
                    ],
                    "target_lang_definitions": {
                        "en": "To deprive of connection with home, family, or habitual environment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cut off",
                                "target_lang_sense_clarification": "to isolate from roots or reality"
                            },
                            {
                                "target_lang_word": "tear away",
                                "target_lang_sense_clarification": "emotional separation from home or family"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
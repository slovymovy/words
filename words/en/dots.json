                    "target_lang_definitions": {
                        "ru": "Множественное число от слова \"dot\". Маленькие круглые метки, пятна или элементы узора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точки",
                                "target_lang_sense_clarification": "маленькие круглые метки"
                            },
                            {
                                "target_lang_word": "горошек",
                                "target_lang_sense_clarification": "узор на ткани"
                            },
                            {
                                "target_lang_word": "пятнышки",
                                "target_lang_sense_clarification": "небольшие пятна"
                            }

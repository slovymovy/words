    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e9894640-cc55-38f5-af34-ebcaf4a619d2",
                    "sense_definition": "Taki, który wzbudza sympatię, jest miły, życzliwy i łatwy do polubienia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy sąsiad jest bardzo <w>sympatyczny</w> i zawsze mówi mi dzień dobry.",
                            "target_lang_translations": {
                                "ru": "Мой новый сосед очень <w>симпатичный</w> и всегда здоровается со мной."
                            }
                        },
                        {
                            "text": "Spędziliśmy razem wyjątkowo <w>sympatyczny</w> wieczór w restauracji.",
                            "target_lang_translations": {
                                "ru": "Мы провели вместе исключительно <w>приятный</w> вечер в ресторане."
                            }
                        }
                    ],
                    "synonyms": [
                        "miły"
                    ],
                    "antonyms": [
                        "niesympatyczny",
                        "antypatyczny"
                    ],
                    "common_phrases": [
                        "bardzo sympatyczny",
                        "sympatyczny człowiek",
                        "sympatyczna atmosfera",
                        "sympatyczny uśmiech"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий симпатию, приятный в общении, доброжелательный и располагающий к себе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "симпатичный",
                                "target_lang_sense_clarification": "вызывающий расположение (о человеке)"
                            },
                            {
                                "target_lang_word": "приятный",
                                "target_lang_sense_clarification": "доставляющий удовольствие (о человеке или времени)"
                            },
                            {
                                "target_lang_word": "милый",
                                "target_lang_sense_clarification": "славный, привлекательный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97b8d5bf-8c97-3ab9-8cfd-e99b3d5b1c19",
                    "sense_definition": "O atramencie lub piśmie: niewidoczny gołym okiem w momencie pisania, ujawniający się dopiero pod wpływem czynników chemicznych lub fizycznych (np. ciepła).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chemistry_history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tajny agent napisał wiadomość, używając atramentu <w>sympatycznego</w>.",
                            "target_lang_translations": {
                                "ru": "Тайный агент написал сообщение, используя <w>симпатические</w> чернила."
                            }
                        },
                        {
                            "text": "Tekst stał się czytelny dopiero po podgrzaniu kartki, ponieważ był napisany pismem <w>sympatycznym</w>.",
                            "target_lang_translations": {
                                "ru": "Текст стал читаемым только после нагревания листа, так как был написан <w>симпатическими</w> чернилами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "atrament sympatyczny",
                        "pismo sympatyczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "О чернилах или способе письма: невидимый в момент написания и проявляющийся только под воздействием тепла или химических реактивов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "симпатический",
                                "target_lang_sense_clarification": "о чернилах (химический термин)"
                            },
                            {
                                "target_lang_word": "невидимый",
                                "target_lang_sense_clarification": "скрытый от глаз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bd960ff-6b65-37d6-9d8f-76ac9e90c93b",
                    "sense_definition": "Odnoszący się do układu współczulnego – części autonomicznego układu nerwowego odpowiedzialnej za mobilizację organizmu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Układ <w>sympatyczny</w> przyspiesza akcję serca w sytuacjach stresowych.",
                            "target_lang_translations": {
                                "ru": "<w>Симпатическая</w> система ускоряет сердцебиение в стрессовых ситуациях."
                            }
                        },
                        {
                            "text": "Student medycyny uczył się o funkcjach, jakie pełni nerw <w>sympatyczny</w>.",
                            "target_lang_translations": {
                                "ru": "Студент-медик изучал функции, которые выполняет <w>симпатический</w> нерв."
                            }
                        }
                    ],
                    "synonyms": [
                        "współczulny"
                    ],
                    "common_phrases": [
                        "układ sympatyczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к симпатической нервной системе — части вегетативной нервной системы, отвечающей за мобилизацию организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "симпатический",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
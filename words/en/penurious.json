                },
                {
                    "sense_id": "a7548a54-259e-3e72-b7d1-afb0914795c8",
                    "sense_definition": "Extremely unwilling to spend money; mean or stingy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>penurious</w> landlord refused to pay for any repairs to the building.",
                            "target_lang_translations": {
                                "ru": "<w>Скупой</w> домовладелец отказался платить за какой-либо ремонт здания."
                            }
                        },
                        {
                            "text": "She was known for her <w>penurious</w> habits, saving every penny she earned.",
                            "target_lang_translations": {
                                "ru": "Она была известна своими <w>скаредными</w> привычками, откладывая каждый заработанный пенни."
                            }
                        }
                    ],
                    "synonyms": [
                        "miserly",
                        "stingy",
                        "parsimonious",
                        "penny-pinching",
                        "mean"
                    ],
                    "common_phrases": [
                        "penurious habits",
                        "penurious nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крайне не желающий тратить деньги; скупой или жадный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скупой",
                                "target_lang_sense_clarification": "жадный до денег"
                            },
                            {
                                "target_lang_word": "скаредный",
                                "target_lang_sense_clarification": "чрезмерно экономный"

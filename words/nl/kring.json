{
    "word_family": [
        "kringen",
        "kringloop",
        "omkringen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1e9e17f2-5855-3d94-a691-0a33377ec64c",
                    "sense_definition": "Een figuur in de vorm van een cirkel, of een groep mensen of dingen die in een rondje staan.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen staan hand in hand in een <w>kring</w> op het schoolplein.",
                            "target_lang_translations": {
                                "ru": "Дети стоят, держась за руки, в <w>кругу</w> на школьном дворе.",
                                "en": "The children are standing hand in hand in a <w>circle</w> in the schoolyard."
                            }
                        },
                        {
                            "text": "Teken een <w>kring</w> om het juiste antwoord op het papier.",
                            "target_lang_translations": {
                                "ru": "Нарисуй <w>кружок</w> вокруг правильного ответа на бумаге.",
                                "en": "Draw a <w>circle</w> around the correct answer on the paper."
                            }
                        }
                    ],
                    "synonyms": [
                        "cirkel",
                        "rondje"
                    ],
                    "common_phrases": [
                        "in een kring",
                        "in een kring staan",
                        "in een kring zitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фигура в форме круга или группа людей или предметов, стоящих по кругу.",
                        "en": "A figure in the shape of a circle, or a group of people or things standing in a round formation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "геометрическая фигура или построение людей"
                            },
                            {
                                "target_lang_word": "кружок",
                                "target_lang_sense_clarification": "маленький круг или пометка на бумаге"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "circle",
                                "target_lang_sense_clarification": "geometric shape or arrangement"
                            },
                            {
                                "target_lang_word": "ring",
                                "target_lang_sense_clarification": "circular formation of people"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffd4a7d4-4ce9-3ac9-b1c3-cdf4564cf762",
                    "sense_definition": "Een groep mensen die met elkaar omgaan vanwege gedeelde interesses, belangen of een bepaalde sociale achtergrond.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vierde zijn verjaardag in kleine <w>kring</w> met alleen zijn beste vrienden.",
                            "target_lang_translations": {
                                "ru": "Он отпраздновал свой день рождения в узком <w>кругу</w> только с лучшими друзьями.",
                                "en": "He celebrated his birthday in a small <w>circle</w> with only his best friends."
                            }
                        },
                        {
                            "text": "In politieke <w>kringen</w> wordt er veel gediscussieerd over de nieuwe wet.",
                            "target_lang_translations": {
                                "ru": "В политических <w>кругах</w> много спорят о новом законе.",
                                "en": "In political <w>circles</w>, there is much discussion about the new law."
                            }
                        }
                    ],
                    "synonyms": [
                        "milieu",
                        "omgeving",
                        "groep"
                    ],
                    "common_phrases": [
                        "in besloten kring",
                        "in brede kring",
                        "intieme kring"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, общающихся на основе общих интересов, социального статуса или профессиональной деятельности.",
                        "en": "A group of people who associate with each other due to shared interests, concerns, or a specific social background."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "сфера общения, общество"
                            },
                            {
                                "target_lang_word": "среда",
                                "target_lang_sense_clarification": "социальное окружение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "circle",
                                "target_lang_sense_clarification": "social group or sphere"
                            },
                            {
                                "target_lang_word": "coterie",
                                "target_lang_sense_clarification": "exclusive group of people"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fc6351d-da7f-3ab0-82c7-e70acc26519a",
                    "sense_definition": "Een cirkelvormige vlek die achterblijft op een oppervlak, vaak veroorzaakt door een nat glas of kopje.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "shape_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gebruik alsjeblieft een onderzetter, anders krijg je <w>kringen</w> op de tafel.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, используй подставку, иначе на столе останутся <w>круги</w>.",
                                "en": "Please use a coaster, otherwise you will get <w>rings</w> on the table."
                            }
                        },
                        {
                            "text": "Er zat een lelijke <w>kring</w> van rode wijn op het witte tafelkleed.",
                            "target_lang_translations": {
                                "ru": "На белой скатерти было уродливое <w>пятно</w> от красного вина.",
                                "en": "There was an ugly red wine <w>ring</w> on the white tablecloth."
                            }
                        }
                    ],
                    "synonyms": [
                        "vlek"
                    ],
                    "common_phrases": [
                        "kringen op tafel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Круглый след на поверхности, часто оставляемый мокрым стаканом или чашкой.",
                        "en": "A circular stain left on a surface, often caused by a wet glass or cup."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "след",
                                "target_lang_sense_clarification": "отпечаток на поверхности"
                            },
                            {
                                "target_lang_word": "пятно",
                                "target_lang_sense_clarification": "загрязнение круглой формы"
                            },
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "форма следа"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "ring",
                                "target_lang_sense_clarification": "mark left by a glass"
                            },
                            {
                                "target_lang_word": "stain",
                                "target_lang_sense_clarification": "mark or spot"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cfd3304-d419-3da9-bbac-08d9111843b0",
                    "sense_definition": "Een vereniging van studenten van een bepaalde faculteit of studierichting.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_group",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "De <w>kring</w> organiseert elke week een activiteit voor de eerstejaarsstudenten.",
                            "target_lang_translations": {
                                "ru": "<w>Объединение</w> каждую неделю организует мероприятия для первокурсников.",
                                "en": "The <w>society</w> organizes an activity for the first-year students every week."
                            }
                        },
                        {
                            "text": "Als lid van de <w>kring</w> krijg je korting op je studieboeken.",
                            "target_lang_translations": {
                                "ru": "Как член <w>объединения</w>, ты получаешь скидку на учебники.",
                                "en": "As a member of the <w>association</w>, you get a discount on your textbooks."
                            }
                        }
                    ],
                    "synonyms": [
                        "studentenvereniging",
                        "faculteitskring"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединение студентов определенного факультета или направления (особенно в контексте бельгийских университетов).",
                        "en": "An association of students from a specific faculty or field of study."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "студенческое объединение",
                                "target_lang_sense_clarification": "организация"
                            },
                            {
                                "target_lang_word": "кружок",
                                "target_lang_sense_clarification": "группа по интересам"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "society",
                                "target_lang_sense_clarification": "student organization"
                            },
                            {
                                "target_lang_word": "association",
                                "target_lang_sense_clarification": "formal group"
                            },
                            {
                                "target_lang_word": "club",
                                "target_lang_sense_clarification": "interest group"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "de1889b9-e9dd-305a-a86e-dc022892a113",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>kringen</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kring</w> de rook van mijn sigaar langzaam de lucht in.",
                            "target_lang_translations": {
                                "ru": "Я медленно <w>пускаю кольцами</w> дым от сигары в воздух.",
                                "en": "I slowly <w>curl</w> the smoke from my cigar into the air."
                            }
                        },
                        {
                            "text": "Voorzichtig <w>kring</w> ik om het gevoelige onderwerp heen tijdens het gesprek.",
                            "target_lang_translations": {
                                "ru": "Я осторожно <w>хожу кругами</w> вокруг деликатной темы во время разговора.",
                                "en": "I carefully <w>circle</w> around the sensitive topic during the conversation."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола kringen (виться, образовывать круги).",
                        "en": "First person singular present tense of 'kringen' (to curl, to move in a circle)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пускаю кольца",
                                "target_lang_sense_clarification": "о дыме"
                            },
                            {
                                "target_lang_word": "кружу",
                                "target_lang_sense_clarification": "двигаюсь вокруг чего-либо"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "circle",
                                "target_lang_sense_clarification": "move around something"
                            },
                            {
                                "target_lang_word": "curl",
                                "target_lang_sense_clarification": "move in a spiral or circular shape"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответственный",
                                "target_lang_sense_clarification": "должностное или виновное лицо"
                            },
                            {
                                "target_lang_word": "виновник",
                                "target_lang_sense_clarification": "причина события"
                            }

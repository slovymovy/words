            "pos": "verb",
            "senses": [
                {
                    "sense_id": "69e29bf7-b439-32de-bc61-ac33f9699ba6",
                    "sense_definition": "Met opzet en op gewelddadige wijze iemand van het leven beroven.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "kill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie onderzoekt wie de jonge vrouw heeft <w>vermoord</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция расследует, кто <w>убил</w> молодую женщину.",
                                "en": "The police are investigating who <w>murdered</w> the young woman."
                            }
                        },
                        {
                            "text": "In de spannende film werd de koning in zijn slaap <w>vermoord</w>.",
                            "target_lang_translations": {
                                "ru": "В захватывающем фильме короля <w>убили</w> во сне.",
                                "en": "In the exciting movie, the king was <w>murdered</w> in his sleep."
                            }
                        },
                        {
                            "text": "In de jaren zestig werden bekende politieke leiders <w>vermoord</w>.",
                            "target_lang_translations": {
                                "ru": "В шестидесятые годы были <w>убиты</w> известные политические лидеры.",
                                "en": "In the sixties, famous political leaders were <w>assassinated</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ombrengen",
                        "liquideren"
                    ],
                    "common_phrases": [
                        "iemand vermoorden",
                        "koelbloedig vermoorden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренно и насильственным образом лишить кого-либо жизни.",
                        "en": "To intentionally and violently deprive someone of life."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убить",
                                "target_lang_sense_clarification": "лишить жизни"
                            },
                            {
                                "target_lang_word": "убивать",
                                "target_lang_sense_clarification": "совершать убийство"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "murder",
                                "target_lang_sense_clarification": "to kill unlawfully with intent"
                            },
                            {
                                "target_lang_word": "kill",
                                "target_lang_sense_clarification": "to deprive of life"
                            },
                            {
                                "target_lang_word": "assassinate",
                                "target_lang_sense_clarification": "to murder a prominent person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3eee2534-e464-356b-af0b-c07bb143e1a4",
                    "sense_definition": "Iets abstracts of figuurlijks volledig vernietigen of doen verdwijnen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "kill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "God is dood en de moderniteit heeft Hem <w>vermoord</w>.",
                            "target_lang_translations": {
                                "ru": "Бог мертв, и современность его <w>убила</w>.",
                                "en": "God is dead and modernity has <w>killed</w> Him."
                            }
                        },
                        {
                            "text": "Door zijn constante kritiek heeft hij de goede sfeer <w>vermoord</w>.",
                            "target_lang_translations": {
                                "ru": "Своей постоянной критикой он <w>убил</w> хорошую атмосферу.",
                                "en": "Through his constant criticism, he has <w>killed</w> the good atmosphere."
                            }
                        }
                    ],
                    "synonyms": [
                        "vernietigen",
                        "kapotmaken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью уничтожить или заставить исчезнуть что-либо абстрактное или переносное.",
                        "en": "To completely destroy or make disappear something abstract or figurative."
                    },
                    "translations": {

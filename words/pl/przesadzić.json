    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "95ab1f66-9dff-37b5-af9e-2a3d284e7734",
                    "sense_definition": "Wykopać roślinę z ziemi lub wyjąć z pojemnika i zasadzić ją w innym miejscu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ogrodnictwo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę w końcu <przesadzić> ten duży fikus, bo jego doniczka stała się już za ciasna.",
                            "target_lang_translations": {
                                "ru": "Мне нужно наконец <пересадить> этот большой фикус, потому что его горшок стал уже слишком тесным."
                            }
                        },
                        {
                            "text": "Ogrodnik doradził nam, aby <przesadzić> drzewka owocowe w bardziej nasłonecznione miejsce.",
                            "target_lang_translations": {
                                "ru": "Садовник посоветовал нам <пересадить> фруктовые деревья в более солнечное место."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przesadzić kwiaty",
                        "przesadzić do doniczki",
                        "przesadzić drzewo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выкопать растение из земли или вынуть из горшка и посадить его в другом месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пересадить",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fad7c759-db5b-34c8-8a84-58c936b3079a",
                    "sense_definition": "Zrobić czegoś zbyt wiele, przekroczyć rozsądne granice lub normy w swoim zachowaniu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "nadmiar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chyba wczoraj <przesadziłem> z ćwiczeniami na siłowni, bo dzisiaj bolą mnie wszystkie mięśnie.",
                            "target_lang_translations": {
                                "ru": "Кажется, вчера я <переборщил> с упражнениями в спортзале, потому что сегодня у меня болят все мышцы."
                            }
                        },
                        {
                            "text": "Uważaj, żebyś nie <przesadził> z krytyką, bo możesz sprawić jej dużą przykrość.",
                            "target_lang_translations": {
                                "ru": "Смотри, не <перегни палку> с критикой, а то можешь сильно её обидеть."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeholować",
                        "przebrać miarę",
                        "zagalopować się",
                        "zapędzić się",
                        "przeciągnąć strunę"
                    ],
                    "common_phrases": [
                        "przesadzić z alkoholem",
                        "trochę przesadzić",
                        "chyba przesadziłeś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать чего-то слишком много, выйти за разумные рамки или нормы в своем поведении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переборщить",
                                "target_lang_sense_clarification": "сделать лишнее"
                            },
                            {
                                "target_lang_word": "перегнуть палку",
                                "target_lang_sense_clarification": "идиома"
                            },
                            {
                                "target_lang_word": "перестараться",
                                "target_lang_sense_clarification": "проявить излишнее усердие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a044ff0c-4fa2-3df3-89fc-1d8c24819cad",
                    "sense_definition": "Opisać coś w sposób wyolbrzymiony, przypisując temu większe znaczenie lub rozmiary niż w rzeczywistości.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "nadmiar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek lubi <przesadzić>, opowiadając o swoich rzekomych sukcesach w zagranicznej firmie.",
                            "target_lang_translations": {
                                "ru": "Марек любит <преувеличить>, рассказывая о своих мнимых успехах в иностранной фирме."
                            }
                        },
                        {
                            "text": "Autor artykułu musiał mocno <przesadzić>, nazywając to drobne wydarzenie katastrofą stulecia.",
                            "target_lang_translations": {
                                "ru": "Автор статьи, должно быть, сильно <преувеличил>, назвав это мелкое событие катастрофой века."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeszarżować"
                    ],
                    "common_phrases": [
                        "mocno przesadzić",
                        "przesadzić w opisie",
                        "nie przesadź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описать что-то в преувеличенном виде, приписывая этому большее значение или размеры, чем на самом деле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преувеличить",
                                "target_lang_sense_clarification": "факты, описание"
                            },
                            {
                                "target_lang_word": "сгустить краски",
                                "target_lang_sense_clarification": "идиома"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c2a3352-9c13-3559-a6fb-7affdc949acf",
                    "sense_definition": "Kazać komuś zmienić miejsce siedzenia na inne lub posadzić kogoś gdzie indziej.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "przemieszczanie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczycielka musiała <przesadzić> Jasia do pierwszej ławki, ponieważ ciągle rozmawiał z kolegami.",
                            "target_lang_translations": {
                                "ru": "Учительнице пришлось <пересадить> Яся за первую парту, так как он постоянно болтал с друзьями."
                            }
                        },
                        {
                            "text": "Organizatorzy wesela postanowili <przesadzić> gości, aby uniknąć konfliktów przy stole.",
                            "target_lang_translations": {
                                "ru": "Организаторы свадьбы решили <пересадить> гостей, чтобы избежать конфликтов за столом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przesadzić ucznia",
                        "przesadzić gości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то сменить место сидения на другое или посадить кого-то в другое место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пересадить",
                                "target_lang_sense_clarification": "человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "914c91b9-b18a-3837-9fe9-9c7d464c2aa9",
                    "sense_definition": "Przeskoczyć przez wysoką przeszkodę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ruch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Koń z łatwością zdołał <przesadzić> najwyższą przeszkodę na torze wyścigowym.",
                            "target_lang_translations": {
                                "ru": "Лошадь с легкостью смогла <перемахнуть> через самое высокое препятствие на ипподроме."
                            }
                        },
                        {
                            "text": "Uciekający złodziej próbował <przesadzić> ogrodzenie, ale potknął się i upadł.",
                            "target_lang_translations": {
                                "ru": "Убегающий вор пытался <перемахнуть> через забор, но споткнулся и упал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przesadzić płot",
                        "przesadzić rów",
                        "przesadzić ogrodzenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перепрыгнуть через высокое препятствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перемахнуть",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "перепрыгнуть",
                                "target_lang_sense_clarification": "нейтральное"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "60bcf8a0-f81b-3bbe-9964-4a110645b705",
                    "sense_definition": "Made of clay or similar material that has been permanently hardened by heat.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>ceramic</w> vase stood on the table.",
                            "target_lang_translations": {
                                "ru": "На столе стояла <w>керамическая</w> ваза."
                            }
                        },
                        {
                            "text": "The bathroom floor was covered in white <w>ceramic</w> tiles.",
                            "target_lang_translations": {
                                "ru": "Пол в ванной был выложен белой <w>керамической</w> плиткой."

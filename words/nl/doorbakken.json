            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3daca2bf-2b78-3f8b-9c2f-10ecdb6a9907",
                    "sense_definition": "Door en door gebakken, zodat het geheel gelijkmatig gaar is en niet meer rauw vanbinnen (vooral gebruikt voor vlees of friet).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik eet mijn biefstuk het liefst goed <w>doorbakken</w>.",
                            "target_lang_translations": {
                                "en": "I prefer to eat my steak <w>well-done</w>."
                            }
                        },
                        {
                            "text": "De frietjes waren helaas iets te <w>doorbakken</w> en daardoor hard.",
                            "target_lang_translations": {
                                "en": "Unfortunately, the fries were a bit too <w>well-done</w> and therefore hard."
                            }
                        }
                    ],
                    "synonyms": [
                        "gaar"
                    ],
                    "antonyms": [
                        "rauw"
                    ],
                    "common_phrases": [
                        "goed doorbakken",
                        "medium of doorbakken"
                    ],
                    "target_lang_definitions": {
                        "en": "Baked or fried thoroughly, so that it is evenly cooked and no longer raw inside (especially used for meat or fries)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "well-done",
                                "target_lang_sense_clarification": "cooking level for meat"
                            },
                            {
                                "target_lang_word": "thoroughly cooked",
                                "target_lang_sense_clarification": "general culinary context"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "11c2281e-49df-3ca1-84d5-50af4de997bc",
                    "sense_definition": "Verdergaan met bakken; niet stoppen met het bakproces.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "continue_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als de taart nog niet gaar is, moet je hem nog even laten <w>doorbakken</w>.",
                            "target_lang_translations": {
                                "en": "If the cake is not done yet, you have to let it <w>bake a little longer</w>."
                            }
                        },
                        {
                            "text": "De bakker <w>bakte door</w> om alle bestellingen op tijd af te krijgen.",
                            "target_lang_translations": {
                                "en": "The baker <w>kept baking</w> to finish all orders on time."
                            }
                        }
                    ],
                    "common_phrases": [
                        "even doorbakken",
                        "rustig doorbakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To continue with the baking process; not stopping."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "continue baking",
                                "target_lang_sense_clarification": "prolonging the action"
                            },
                            {
                                "target_lang_word": "keep baking",
                                "target_lang_sense_clarification": "continuous action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6db4c08-47d1-352c-b062-a42932ef5bac",
                    "sense_definition": "Iets volledig en grondig verhitten in een pan of oven totdat het helemaal gaar is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "thorough_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet het brood goed <w>doorbakken</w> voor een stevige korst.",
                            "target_lang_translations": {
                                "en": "You must <w>bake</w> the bread <w>thoroughly</w> for a firm crust."
                            }
                        },
                        {
                            "text": "Hij <w>doorbakte</w> het vlees totdat het helemaal grijs en taai was.",
                            "target_lang_translations": {
                                "en": "He <w>cooked</w> the meat <w>thoroughly</w> until it was completely gray and tough."
                            }
                        }
                    ],
                    "common_phrases": [
                        "goed doorbakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To heat something completely and thoroughly in a pan or oven until it is fully cooked."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cook thoroughly",
                                "target_lang_sense_clarification": "until fully done"
                            },
                            {
                                "target_lang_word": "bake through",
                                "target_lang_sense_clarification": "until the center is cooked"
                            }
                        ]
                    }
                }
            ]
        }

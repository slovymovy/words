    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7f1d59c1-008d-36a7-a7bd-78de5ebb2d6a",
                    "sense_definition": "Taki, który jest spokojny, opanowany i nie wykazuje agresji; o zjawiskach atmosferycznych: niezbyt intensywny, umiarkowany.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "character_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek to bardzo <w>łagodny</w> człowiek, który nigdy nie podnosi głosu.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка — очень <w>мягкий</w> человек, который никогда не повышает голоса."
                            }
                        },
                        {
                            "text": "Tegoroczna zima była wyjątkowo <w>łagodna</w> i prawie nie padał śnieg.",
                            "target_lang_translations": {
                                "ru": "Зима в этом году была исключительно <w>мягкой</w>, и снег почти не шёл."
                            }
                        }
                    ],
                    "synonyms": [
                        "spokojny",
                        "subtelny"
                    ],
                    "antonyms": [
                        "agresywny",
                        "choleryczny",
                        "ostry"
                    ],
                    "common_phrases": [
                        "łagodny człowiek",
                        "łagodny uśmiech",
                        "łagodna zima",
                        "łagodny klimat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который спокоен, сдержан и не проявляет агрессии; о погоде: умеренный, не суровый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мягкий",
                                "target_lang_sense_clarification": "о характере или климате"
                            },
                            {
                                "target_lang_word": "кроткий",
                                "target_lang_sense_clarification": "о нраве: незлобивый, покорный"
                            },
                            {
                                "target_lang_word": "спокойный",
                                "target_lang_sense_clarification": "уравновешенный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e941d172-c58c-3b15-b2f1-384673bddcd5",
                    "sense_definition": "O smaku lub zapachu: delikatny, nieostry, niepowodujący pieczenia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sensory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do tego dania najlepiej pasuje <w>łagodny</w> sos pomidorowy z ziołami.",
                            "target_lang_translations": {
                                "ru": "К этому блюду лучше всего подходит <w>неострый</w> томатный соус с травами."
                            }
                        },
                        {
                            "text": "Kawa, którą zamówiłem, miała bardzo <w>łagodny</w> i przyjemny aromat.",
                            "target_lang_translations": {
                                "ru": "У кофе, который я заказал, был очень <w>мягкий</w> и приятный аромат."
                            }
                        }
                    ],
                    "synonyms": [
                        "subtelny"
                    ],
                    "antonyms": [
                        "pikantny",
                        "ostry",
                        "mocny"
                    ],
                    "common_phrases": [
                        "łagodny smak",
                        "łagodna przyprawa",
                        "łagodny sos"
                    ],
                    "target_lang_definitions": {
                        "ru": "О вкусе или запахе: нежный, не острый, не вызывающий жжения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мягкий",
                                "target_lang_sense_clarification": "о вкусе или аромате"
                            },
                            {
                                "target_lang_word": "неострый",
                                "target_lang_sense_clarification": "не жгучий, не пикантный"
                            },
                            {
                                "target_lang_word": "нежный",
                                "target_lang_sense_clarification": "деликатный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ce9d74f-1d93-3116-ae3c-d67eb7a0e8b8",
                    "sense_definition": "O nurcie rzeki lub ukształtowaniu terenu: płynący powoli, mało stromy, opadający nieznacznie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_topography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzeka w tym miejscu ma <w>łagodny</w> nurt, więc można tu bezpiecznie pływać.",
                            "target_lang_translations": {
                                "ru": "У реки в этом месте <w>спокойное</w> течение, поэтому здесь можно безопасно плавать."
                            }
                        },
                        {
                            "text": "Wybraliśmy <w>łagodne</w> zbocze góry, aby wejście nie było zbyt męczące.",
                            "target_lang_translations": {
                                "ru": "Мы выбрали <w>пологий</w> склон горы, чтобы подъём не был слишком утомительным."
                            }
                        }
                    ],
                    "synonyms": [
                        "spokojny"
                    ],
                    "antonyms": [
                        "rwący",
                        "ostry"
                    ],
                    "common_phrases": [
                        "łagodny nurt",
                        "łagodne zbocze",
                        "łagodny stok"
                    ],
                    "target_lang_definitions": {
                        "ru": "О течении реки или рельефе местности: медленный, некрутой, опускающийся незначительно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пологий",
                                "target_lang_sense_clarification": "о склоне: некрутой"
                            },
                            {
                                "target_lang_word": "спокойный",
                                "target_lang_sense_clarification": "о течении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46ec8c9c-b66f-3369-8ddf-1c9209ce671e",
                    "sense_definition": "O chorobie lub zmianie w organizmie: niegroźny dla życia, przebiegający bez gwałtownych objawów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz uspokoił pacjenta, że wykryty guz jest <w>łagodny</w> i nie daje przerzutów.",
                            "target_lang_translations": {
                                "ru": "Врач успокоил пациента, что обнаруженная опухоль — <w>доброкачественная</w> и не даёт метастазов."
                            }
                        },
                        {
                            "text": "Choroba miała <w>łagodny</w> przebieg i po kilku dniach objawy ustąpiły.",
                            "target_lang_translations": {
                                "ru": "Болезнь имела <w>лёгкое</w> течение, и через несколько дней симптомы исчезли."
                            }
                        }
                    ],
                    "antonyms": [
                        "ostry"
                    ],
                    "common_phrases": [
                        "nowotwór łagodny",
                        "łagodny przebieg",
                        "guz łagodny"
                    ],
                    "target_lang_definitions": {
                        "ru": "О болезни или опухоли: неопасный для жизни, протекающий без тяжелых симптомов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доброкачественный",
                                "target_lang_sense_clarification": "об опухоли"
                            },
                            {
                                "target_lang_word": "лёгкий",
                                "target_lang_sense_clarification": "о течении болезни"
                            }
                        ]
                    }
                }
            ]
        }
    ]

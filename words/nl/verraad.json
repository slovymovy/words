                    ],
                    "synonyms": [
                        "trouwbreuk",
                        "ontrouw"
                    ],
                    "antonyms": [
                        "trouw",
                        "loyaliteit"
                    ],
                    "common_phrases": [
                        "hoog verraad",
                        "verraad plegen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушение верности или преданности стране, группе или человеку.",
                        "en": "The violation of faith or loyalty towards a country, group, or person."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предательство",

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "28bd4ffe-74fa-38a2-a96f-e2353c90288b",
                    "sense_definition": "Sprawiać, że coś lub ktoś staje się cieplejszy; podnosić temperaturę otoczenia, przedmiotu lub ciała.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "temperature_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gorąca herbata z cytryną szybko <w>rozgrzewa</w> w zimowe wieczory.",
                            "target_lang_translations": {
                                "ru": "Горячий чай с лимоном быстро <w>согревает</w> зимними вечерами."
                            }
                        },
                        {
                            "text": "Wiosenne słońce powoli <w>rozgrzewało</w> zmarzniętą ziemię w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Весеннее солнце медленно <w>согревало</w> замерзшую землю в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogrzewać",
                        "grzać",
                        "ocieplać"
                    ],
                    "antonyms": [
                        "chłodzić",
                        "studzić",
                        "oziębiać"
                    ],
                    "common_phrases": [
                        "rozgrzewać dłonie",
                        "rozgrzewać silnik",
                        "słońce rozgrzewa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать кого-либо или что-либо более теплым; повышать температуру окружающей среды, предмета или тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согревать",
                                "target_lang_sense_clarification": "давать тепло, делать теплым"
                            },
                            {
                                "target_lang_word": "разогревать",
                                "target_lang_sense_clarification": "повышать температуру чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9825ade6-1f11-3513-b238-820c7441655b",
                    "sense_definition": "Przygotowywać organizm lub konkretne mięśnie do wysiłku fizycznego poprzez wykonywanie wstępnych ćwiczeń.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_preparation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed każdym meczem zawodnicy muszą dokładnie <w>rozgrzewać</w> mięśnie, aby uniknąć kontuzji.",
                            "target_lang_translations": {
                                "ru": "Перед каждым матчем игроки должны тщательно <w>разогревать</w> мышцы, чтобы избежать травм."
                            }
                        },
                        {
                            "text": "Biegacz <w>rozgrzewał</w> się przez dziesięć minut, zanim ruszył w trasę.",
                            "target_lang_translations": {
                                "ru": "Бегун <w>разогревался</w> десять минут, прежде чем отправиться в путь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rozgrzewać mięśnie",
                        "rozgrzewać stawy",
                        "rozgrzewać się przed biegiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подготавливать организм или определенные мышцы к физической нагрузке с помощью предварительных упражнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разогревать",
                                "target_lang_sense_clarification": "мышцы перед нагрузкой"
                            },
                            {
                                "target_lang_word": "разминать",
                                "target_lang_sense_clarification": "подготавливать тело к спорту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b31eae31-b3e4-36fa-b0af-16fee83eab44",
                    "sense_definition": "Stawać się gorącym lub cieplejszym; ulegać nagrzaniu (zazwyczaj używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "temperature_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Piekarnik <w>rozgrzewa</w> się do dwustu stopni w zaledwie kilka minut.",
                            "target_lang_translations": {
                                "ru": "Духовка <w>разогревается</w> до двухсот градусов всего за несколько минут."
                            }
                        },
                        {
                            "text": "Telefon mocno <w>rozgrzewa</w> się podczas ładowania i grania w gry.",
                            "target_lang_translations": {
                                "ru": "Телефон сильно <w>нагревается</w> во время зарядки и игр."
                            }
                        }
                    ],
                    "synonyms": [
                        "nagrzewać się"
                    ],
                    "antonyms": [
                        "stygnąć",
                        "chłodnąć"
                    ],
                    "common_phrases": [
                        "silnik się rozgrzewa",
                        "powietrze się rozgrzewa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться горячим или более теплым; нагреваться (обычно о предметах или устройствах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нагреваться",
                                "target_lang_sense_clarification": "становиться горячим"
                            },
                            {
                                "target_lang_word": "разогреваться",
                                "target_lang_sense_clarification": "достигать рабочей температуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f596b1b1-557f-3e95-b651-01f5a169c6cc",
                    "sense_definition": "Pobudzać emocjonalnie; wywoływać silne uczucia, zapał, entuzjazm lub podniecenie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emotional_arousal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przemówienie lidera <w>rozgrzewało</w> tłum do walki o swoje prawa.",
                            "target_lang_translations": {
                                "ru": "Речь лидера <w>разогревала</w> толпу к борьбе за свои права."
                            }
                        },
                        {
                            "text": "Ten widok <w>rozgrzewał</w> wyobraźnię artysty i inspirował go do dalszej pracy.",
                            "target_lang_translations": {
                                "ru": "Этот вид <w>разжигал</w> воображение художника и вдохновлял его на дальнейшую работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "ekscytować",
                        "emocjonować",
                        "zapalać",
                        "pobudzać"
                    ],
                    "antonyms": [
                        "studzić",
                        "uspokajać",
                        "gasić"
                    ],
                    "common_phrases": [
                        "rozgrzewać atmosferę",
                        "rozgrzewać wyobraźnię",
                        "rozgrzewać publiczność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Эмоционально возбуждать; вызывать сильные чувства, энтузиазм или волнение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разогревать",
                                "target_lang_sense_clarification": "публику, атмосферу"
                            },
                            {
                                "target_lang_word": "разжигать",
                                "target_lang_sense_clarification": "воображение, страсти"
                            }
                        ]
                    }
                }

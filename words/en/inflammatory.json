    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "39d74ed1-0da8-3b54-b9ce-63e546220479",
                    "sense_definition": "Tending to cause anger, hostility, or strong emotion, especially in a public or political context.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician was criticized for making <w>inflammatory</w> remarks during the debate.",
                            "target_lang_translations": {
                                "ru": "Политика критиковали за <w>подстрекательские</w> высказывания во время дебатов."
                            }
                        },
                        {
                            "text": "Using <w>inflammatory</w> language on social media can quickly lead to arguments.",
                            "target_lang_translations": {
                                "ru": "Использование <w>провокационной</w> лексики в социальных сетях может быстро привести к спорам."
                            }
                        }
                    ],
                    "synonyms": [
                        "provocative",
                        "incendiary"
                    ],
                    "antonyms": [
                        "conciliatory",
                        "calming",
                        "soothing"
                    ],
                    "common_phrases": [
                        "inflammatory remarks",
                        "inflammatory language",
                        "inflammatory speech",
                        "inflammatory rhetoric"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий гнев, враждебность или сильные эмоции; провокационный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подстрекательский",
                                "target_lang_sense_clarification": "побуждающий к конфликту или беспорядкам"
                            },
                            {
                                "target_lang_word": "провокационный",
                                "target_lang_sense_clarification": "вызывающий острую негативную реакцию"
                            },
                            {
                                "target_lang_word": "разжигающий",
                                "target_lang_sense_clarification": "возбуждающий страсти или вражду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23f6f2da-8ca2-3782-b857-a640b4996c5a",
                    "sense_definition": "Relating to or causing inflammation (a condition where a part of the body becomes red, swollen, and painful).",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Arthritis is a chronic <w>inflammatory</w> disease that affects the joints.",
                            "target_lang_translations": {
                                "ru": "Артрит — это хроническое <w>воспалительное</w> заболевание, поражающее суставы."
                            }
                        },
                        {
                            "text": "The body produces an <w>inflammatory</w> response to fight off infection.",
                            "target_lang_translations": {
                                "ru": "Организм вырабатывает <w>воспалительную</w> реакцию для борьбы с инфекцией."
                            }
                        }
                    ],
                    "antonyms": [
                        "anti-inflammatory"
                    ],
                    "common_phrases": [
                        "inflammatory disease",
                        "inflammatory response",
                        "inflammatory condition",
                        "inflammatory bowel disease"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к воспалению или вызывающий его (медицинский термин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспалительный",
                                "target_lang_sense_clarification": "связанный с болезненным процессом в организме"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9cd80837-af7d-34ea-b646-e0f1492edd88",
                    "sense_definition": "Any substance or agent that causes inflammation.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist tested various chemical <w>inflammatories</w> on the tissue samples.",
                            "target_lang_translations": {
                                "ru": "Ученый протестировал различные химические <w>раздражители</w> на образцах тканей."
                            }
                        },
                        {
                            "text": "Certain allergens act as <w>inflammatories</w> when they come into contact with sensitive skin.",
                            "target_lang_translations": {
                                "ru": "Некоторые аллергены действуют как <w>раздражители</w> при контакте с чувствительной кожей."
                            }
                        }
                    ],
                    "synonyms": [
                        "irritant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество или агент, вызывающий воспаление или раздражение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздражитель",
                                "target_lang_sense_clarification": "вещество, вызывающее реакцию ткани"
                            },
                            {
                                "target_lang_word": "воспалительный агент",
                                "target_lang_sense_clarification": "термин в биологии и медицине"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "fatherhood",
        "fatherly",
        "fatherless",
        "grandfather",
        "stepfather",
        "godfather",
        "forefather",
        "fatherland"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d8b4e38-a92f-3f6b-837b-5a38b240536d",
                    "sense_definition": "A male parent of a child.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "biological_father",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>father</w> drives me to school every morning.",
                            "target_lang_translations": {
                                "ru": "Мой <w>отец</w> отвозит меня в школу каждое утро."
                            }
                        },
                        {
                            "text": "He became a <w>father</w> for the first time at the age of thirty.",
                            "target_lang_translations": {
                                "ru": "Он впервые стал <w>отцом</w> в возрасте тридцати лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "dad",
                        "daddy",
                        "papa",
                        "pop",
                        "pa"
                    ],
                    "antonyms": [
                        "mother",
                        "child",
                        "son",
                        "daughter"
                    ],
                    "common_phrases": [
                        "biological father",
                        "adoptive father",
                        "father figure",
                        "like a father"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина по отношению к своим детям"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отец",
                                "target_lang_sense_clarification": "общеупотребительное, нейтральное"
                            },
                            {
                                "target_lang_word": "папа",
                                "target_lang_sense_clarification": "разговорное, в семье"
                            },
                            {
                                "target_lang_word": "батя",
                                "target_lang_sense_clarification": "просторечное, дружеское"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8e7193a-4403-3ce1-b4fc-09fdc6aba197",
                    "sense_definition": "A priest, especially in the Roman Catholic or Orthodox church.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young <w>father</w> gave a moving sermon during the service.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>священник</w> произнес трогательную проповедь во время службы."
                            }
                        },
                        {
                            "text": "She asked the <w>father</w> for advice on spiritual matters.",
                            "target_lang_translations": {
                                "ru": "Она попросила у <w>священника</w> совета по духовным вопросам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "holy father",
                        "confessor father"
                    ],
                    "target_lang_definitions": {
                        "ru": "Священнослужитель, особенно в католической или православной церкви"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "священник",
                                "target_lang_sense_clarification": "название сана"
                            },
                            {
                                "target_lang_word": "батюшка",
                                "target_lang_sense_clarification": "православный священник, разговорное"
                            },
                            {
                                "target_lang_word": "отец",
                                "target_lang_sense_clarification": "при упоминании сана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa86856e-3da9-3818-8d49-a543105adb48",
                    "sense_definition": "A man who originates or founds something; a pioneer in a particular field.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "originator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Albert Einstein is often called the <w>father</w> of modern physics.",
                            "target_lang_translations": {
                                "ru": "Альберта Эйнштейна часто называют <w>отцом</w> современной физики."
                            }
                        },
                        {
                            "text": "As the <w>father</w> of the movement, he was respected by all the members.",
                            "target_lang_translations": {
                                "ru": "Как <w>основатель</w> движения, он пользовался уважением всех участников."
                            }
                        }
                    ],
                    "synonyms": [
                        "patriarch"
                    ],
                    "common_phrases": [
                        "founding father",
                        "father of modern"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который создал или основал что-либо; основоположник"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отец",
                                "target_lang_sense_clarification": "метафорическое"
                            },
                            {
                                "target_lang_word": "основатель",
                                "target_lang_sense_clarification": "создатель"
                            },
                            {
                                "target_lang_word": "родоначальник",
                                "target_lang_sense_clarification": "книжное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6b22ec2-ae27-3319-883f-ad06d99f7b32",
                    "sense_definition": "A male ancestor, especially the founder of a family or line.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biological_father",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He wished to be buried in the land of his <w>fathers</w>.",
                            "target_lang_translations": {
                                "ru": "Он хотел быть похороненным на земле своих <w>отцов</w>."
                            }
                        },
                        {
                            "text": "We must honor the traditions handed down by our <w>fathers</w>.",
                            "target_lang_translations": {
                                "ru": "Мы должны чтить традиции, переданные нам нашими <w>предками</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "patriarch"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предки, люди предшествующих поколений (обычно во множественном числе)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отцы",
                                "target_lang_sense_clarification": "высокий стиль, архаичное"
                            },
                            {
                                "target_lang_word": "предки",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "2d5f5d06-6acf-336a-af51-fcdfc2c2e6a5",
                    "sense_definition": "A respectful title used to address a priest.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "religious_title",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "Good morning, <w>Father</w> Smith, how are you today?",
                            "target_lang_translations": {
                                "ru": "Доброе утро, <w>отец</w> Смит, как вы сегодня?"
                            }
                        },
                        {
                            "text": "Bless me, <w>Father</w>, for I have sinned.",
                            "target_lang_translations": {
                                "ru": "Благословите меня, <w>святой отец</w>, ибо я согрешил."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уважительное обращение к священнику"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отец",
                                "target_lang_sense_clarification": "обычно с именем"
                            },
                            {
                                "target_lang_word": "святой отец",
                                "target_lang_sense_clarification": "обращение (часто в католичестве)"
                            },
                            {
                                "target_lang_word": "батюшка",
                                "target_lang_sense_clarification": "обращение в православии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d51777e3-7e46-3ea6-ae88-b06967194511",
                    "sense_definition": "A respectful term of address for one's own male parent.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_father",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "I will ask <w>Father</w> if we can go to the park.",
                            "target_lang_translations": {
                                "ru": "Я спрошу у <w>отца</w>, можем ли мы пойти в парк."
                            }
                        },
                        {
                            "text": "Please, <w>Father</w>, tell us a story before bed.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>папа</w>, расскажи нам сказку перед сном."
                            }
                        }
                    ],
                    "synonyms": [
                        "dad",
                        "daddy",
                        "papa"
                    ],
                    "antonyms": [
                        "mother"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often considered formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уважительное обращение к своему родителю"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отец",
                                "target_lang_sense_clarification": "строгое или торжественное"
                            },
                            {
                                "target_lang_word": "папа",
                                "target_lang_sense_clarification": "обычное, семейное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83e5118a-44eb-3228-9e04-5d69148eb4ef",
                    "sense_definition": "God, considered as the first person of the Trinity in Christianity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "deity",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "They offered prayers to the <w>Father</w>, the Son, and the Holy Spirit.",
                            "target_lang_translations": {
                                "ru": "Они возносили молитвы <w>Отцу</w>, Сыну и Святому Духу."
                            }
                        },
                        {
                            "text": "May the grace of the <w>Father</w> be with you always.",
                            "target_lang_translations": {
                                "ru": "Да пребудет с вами всегда благодать <w>Отца</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "God the Father",
                        "Heavenly Father"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бог, первая ипостась Святой Троицы в христианстве"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Отец",
                                "target_lang_sense_clarification": "религиозное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98793598-d644-3c6e-8150-09b2dad01378",
                    "sense_definition": "A personification of a force or concept, such as time or Christmas.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "personification",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Old <w>Father</w> Time waits for no man.",
                            "target_lang_translations": {
                                "ru": "<w>Старик</w> Время никого не ждет."
                            }
                        },
                        {
                            "text": "Children leave cookies out for <w>Father</w> Christmas on Christmas Eve.",
                            "target_lang_translations": {
                                "ru": "Дети оставляют печенье для Рождественского <w>Деда</w> в канун Рождества."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Father Time",
                        "Father Christmas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Олицетворение силы или понятия (например, Времени или Рождества)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дед",
                                "target_lang_sense_clarification": "в составе имен собственных (Дед Мороз)"
                            },
                            {
                                "target_lang_word": "Старик",
                                "target_lang_sense_clarification": "в персонификациях (Старик Время)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cc6dda03-20e5-31f1-82d6-574803c56642",
                    "sense_definition": "To be the male parent of a child; to sire.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "to_sire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>fathered</w> three children during his first marriage.",
                            "target_lang_translations": {
                                "ru": "Он <w>стал отцом</w> троих детей в первом браке."
                            }
                        },
                        {
                            "text": "The king <w>fathered</w> no legitimate heirs to the throne.",
                            "target_lang_translations": {
                                "ru": "Король не <w>оставил</w> законных наследников престола."
                            }
                        }
                    ],
                    "synonyms": [
                        "sire"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать отцом ребенка"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стать отцом",
                                "target_lang_sense_clarification": "основной перевод"
                            },
                            {
                                "target_lang_word": "зачать",
                                "target_lang_sense_clarification": "биологический аспект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e11031e7-91ee-3f9f-a261-72f1ded4cb6c",
                    "sense_definition": "To create, invent, or originate something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "to_create",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new political movement was <w>fathered</w> by a group of students.",
                            "target_lang_translations": {
                                "ru": "Новое политическое движение было <w>создано</w> группой студентов."
                            }
                        },
                        {
                            "text": "He <w>fathered</w> a plan that would change the company forever.",
                            "target_lang_translations": {
                                "ru": "Он <w>разработал</w> план, который навсегда изменил компанию."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Создать, изобрести или положить начало чему-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "создать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "породить",
                                "target_lang_sense_clarification": "книжное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
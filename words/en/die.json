{
    "word_family": [
        "death",
        "dead",
        "deadly",
        "dying",
        "undying",
        "deaden"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a908abc1-689d-3069-af36-0dffe1f0e7db",
                    "sense_definition": "To stop living; to become dead; to undergo death.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cease_living",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her grandfather <w>died</w> peacefully in his sleep at the age of ninety.",
                            "target_lang_translations": {
                                "ru": "Ее дедушка мирно <w>умер</w> во сне в возрасте девяноста лет."
                            }
                        },
                        {
                            "text": "If you do not water these plants regularly, they will <w>die</w>.",
                            "target_lang_translations": {
                                "ru": "Если не поливать эти растения регулярно, они <w>погибнут</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pass away",
                        "perish",
                        "expire",
                        "decease",
                        "depart"
                    ],
                    "antonyms": [
                        "live",
                        "survive",
                        "be born"
                    ],
                    "common_phrases": [
                        "die of",
                        "die from",
                        "die young",
                        "die in peace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать жить; стать мертвым; подвергнуться смерти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умирать",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "скончаться",
                                "target_lang_sense_clarification": "официально или вежливо"
                            },
                            {
                                "target_lang_word": "погибать",
                                "target_lang_sense_clarification": "в результате несчастного случая или насилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b01fcdf-8a2c-306b-bb90-f4d4fb34728e",
                    "sense_definition": "To stop working; to break down or lose power (of a machine, battery, or device).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cease_functioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to find a charger immediately because my phone battery just <w>died</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно срочно найти зарядку, потому что батарея моего телефона только что <w>села</w>."
                            }
                        },
                        {
                            "text": "The car engine <w>died</w> suddenly while we were waiting at the traffic light.",
                            "target_lang_translations": {
                                "ru": "Двигатель машины внезапно <w>заглох</w>, пока мы ждали на светофоре."
                            }
                        }
                    ],
                    "synonyms": [
                        "conk out",
                        "fail",
                        "stop"
                    ],
                    "common_phrases": [
                        "battery died",
                        "engine died"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать работать; сломаться или потерять заряд (о механизме, батарее или устройстве)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сесть",
                                "target_lang_sense_clarification": "о батарее или аккумуляторе"
                            },
                            {
                                "target_lang_word": "заглохнуть",
                                "target_lang_sense_clarification": "о двигателе"
                            },
                            {
                                "target_lang_word": "сдохнуть",
                                "target_lang_sense_clarification": "разг., о технике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00717716-bbfb-365d-88bf-f6c8b2764667",
                    "sense_definition": "To yearn intensely; to want something very much.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "desire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After walking in the hot sun for hours, we were <w>dying</w> for a cold drink.",
                            "target_lang_translations": {
                                "ru": "После нескольких часов ходьбы под жарким солнцем мы <w>умирали</w> от желания выпить чего-нибудь холодного."
                            }
                        },
                        {
                            "text": "She is <w>dying</w> to know the results of her final exam.",
                            "target_lang_translations": {
                                "ru": "Она <w>умирает</w> от нетерпения узнать результаты своего выпускного экзамена."
                            }
                        }
                    ],
                    "synonyms": [
                        "yearn",
                        "long",
                        "crave"
                    ],
                    "common_phrases": [
                        "dying to",
                        "dying for"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in continuous forms like 'dying for' or 'dying to'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно желать чего-либо; испытывать острую потребность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умирать",
                                "target_lang_sense_clarification": "перен., от желания или скуки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "506b9f4e-2738-3f40-8414-1531e4a92ed4",
                    "sense_definition": "To perish; to cease to exist; to become lost, extinct, or fade away.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cease_existing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many ancient traditions <w>die</w> out as modern technology changes the way people live.",
                            "target_lang_translations": {
                                "ru": "Многие древние традиции <w>отмирают</w>, поскольку современные технологии меняют образ жизни людей."
                            }
                        },
                        {
                            "text": "The sound of the thunder slowly <w>died</w> away in the distance.",
                            "target_lang_translations": {
                                "ru": "Звук грома медленно <w>затих</w> вдали."
                            }
                        }
                    ],
                    "synonyms": [
                        "vanish",
                        "fade",
                        "disappear"
                    ],
                    "common_phrases": [
                        "die out",
                        "die away",
                        "die hard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исчезать; прекращать существование; угасать или вымирать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отмирать",
                                "target_lang_sense_clarification": "о традициях или обычаях"
                            },
                            {
                                "target_lang_word": "затихать",
                                "target_lang_sense_clarification": "о звуке"
                            },
                            {
                                "target_lang_word": "исчезать",
                                "target_lang_sense_clarification": "прекращать существование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ceab4707-100e-3eef-b3d3-d9616b7e2c8c",
                    "sense_definition": "To lose a life or be eliminated from a game.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "game_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If your character <w>dies</w> in this level, you have to start from the beginning.",
                            "target_lang_translations": {
                                "ru": "Если твой персонаж <w>умрет</w> на этом уровне, тебе придется начать с начала."
                            }
                        },
                        {
                            "text": "I keep <w>dying</w> at the same spot because the jump is too difficult.",
                            "target_lang_translations": {
                                "ru": "Я постоянно <w>умираю</w> на одном и том же месте, потому что прыжок слишком сложный."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерять жизнь или выбыть из игры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умирать",
                                "target_lang_sense_clarification": "в компьютерных играх"
                            },
                            {
                                "target_lang_word": "погибать",
                                "target_lang_sense_clarification": "о персонаже"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff174784-2e87-33b7-9791-e8767eb4084b",
                    "sense_definition": "To be so overcome with emotion, embarrassment, or laughter as to be incapacitated.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The comedian was so funny that I almost <w>died</w> laughing.",
                            "target_lang_translations": {
                                "ru": "Комик был таким смешным, что я чуть не <w>умер</w> со смеху."
                            }
                        },
                        {
                            "text": "I thought I would <w>die</w> of embarrassment when I realized my mistake.",
                            "target_lang_translations": {
                                "ru": "Я думал, что <w>умру</w> от стыда, когда осознал свою ошибку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "die laughing"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "hyperbolic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть переполненным эмоциями, смехом или смущением до потери самообладания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умирать",
                                "target_lang_sense_clarification": "перен., от смеха или стыда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a2247ec-224c-3df0-8ee5-0d867990a7f3",
                    "sense_definition": "To expire at the end of a legislative session without being brought to a vote.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legislative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The proposed bill <w>died</w> in committee because it lacked sufficient support.",
                            "target_lang_translations": {
                                "ru": "Предложенный законопроект <w>был отклонен</w> в комитете из-за недостаточной поддержки."
                            }
                        },
                        {
                            "text": "Many controversial laws <w>die</w> before they ever reach the senate floor.",
                            "target_lang_translations": {
                                "ru": "Многие спорные законы <w>отсеиваются</w> еще до того, как попадают на рассмотрение сената."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекратить действие или быть отклоненным в конце законодательной сессии без вынесения на голосование."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть отклоненным",
                                "target_lang_sense_clarification": "о законопроекте"
                            },
                            {
                                "target_lang_word": "провалиться",
                                "target_lang_sense_clarification": "разг., потерпеть неудачу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dbf42d6a-ea21-3e2c-8295-a4d8643eb676",
                    "sense_definition": "A small cube with numbers or symbols on each side, used in games of chance.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "game_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must roll the <w>die</w> to determine how many spaces you can move.",
                            "target_lang_translations": {
                                "ru": "Ты должен бросить <w>кубик</w>, чтобы определить, на сколько клеток можно продвинуться."
                            }
                        },
                        {
                            "text": "One <w>die</w> fell off the table and rolled under the sofa.",
                            "target_lang_translations": {
                                "ru": "Один <w>кубик</w> упал со стола и закатился под диван."
                            }
                        }
                    ],
                    "common_phrases": [
                        "roll the die",
                        "cast the die"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой кубик с числами или символами на гранях, используемый в азартных или настольных играх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игральная кость",
                                "target_lang_sense_clarification": "термин"
                            },
                            {
                                "target_lang_word": "кубик",
                                "target_lang_sense_clarification": "разг., предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2333cfdb-4fe9-3d41-a1b7-6dbf26109041",
                    "sense_definition": "A specialized tool or device used for cutting, shaping, or stamping material like metal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory uses a heavy steel <w>die</w> to stamp the coins.",
                            "target_lang_translations": {
                                "ru": "Завод использует тяжелый стальной <w>штамп</w> для чеканки монет."
                            }
                        },
                        {
                            "text": "A custom <w>die</w> was created to cut the paper into intricate shapes.",
                            "target_lang_translations": {
                                "ru": "Был создан специальный <w>штамп</w> для нарезки бумаги сложными фигурами."
                            }
                        }
                    ],
                    "synonyms": [
                        "mold",
                        "stamp"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специализированный инструмент для резки, формовки или штамповки материалов, таких как металл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штамп",
                                "target_lang_sense_clarification": "для формовки или чеканки"
                            },
                            {
                                "target_lang_word": "плашка",
                                "target_lang_sense_clarification": "для нарезания резьбы"
                            },
                            {
                                "target_lang_word": "матрица",
                                "target_lang_sense_clarification": "техническая форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b71a11a-8558-3743-a89f-687a0767e510",
                    "sense_definition": "A small block of semiconducting material on which a given functional circuit is fabricated.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Engineers inspected the silicon <w>die</w> for any microscopic defects.",
                            "target_lang_translations": {
                                "ru": "Инженеры осмотрели кремниевый <w>кристалл</w> на наличие микроскопических дефектов."
                            }
                        },
                        {
                            "text": "The processor is built on a single <w>die</w> to improve performance.",
                            "target_lang_translations": {
                                "ru": "Процессор построен на одном <w>кристалле</w> для повышения производительности."
                            }
                        }
                    ],
                    "synonyms": [
                        "chip"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая пластина полупроводникового материала, на которой изготовлена микросхема."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кристалл",
                                "target_lang_sense_clarification": "полупроводниковый"
                            },
                            {
                                "target_lang_word": "чип",
                                "target_lang_sense_clarification": "микросхема"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "eb70e3ad-7aa2-3ca5-bb7d-53fc2d0f9cea",
                    "sense_definition": "Per day; used in medical prescriptions to indicate daily frequency.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "medical_latin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prescription instructions stated the dosage as 100 mg <w>die</w>.",
                            "target_lang_translations": {
                                "ru": "В инструкции к рецепту дозировка была указана как 100 мг <w>в день</w>."
                            }
                        },
                        {
                            "text": "Patients were monitored <w>die</w> to ensure the treatment was effective.",
                            "target_lang_translations": {
                                "ru": "Пациентов наблюдали <w>ежедневно</w>, чтобы убедиться в эффективности лечения."
                            }
                        }
                    ],
                    "synonyms": [
                        "daily"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "Latin"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В день; используется в медицинских рецептах для указания суточной частоты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в день",
                                "target_lang_sense_clarification": "ежедневно"
                            },
                            {
                                "target_lang_word": "сутки",
                                "target_lang_sense_clarification": "в рецептах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
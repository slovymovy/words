                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "equestrianism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeździec nałożył na konia bogato zdobiony <w>podkład</w>.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9ca34a90-ca49-34b1-a9a2-9ff0178688f7",
                    "sense_definition": "Mający właściwości trujące, zdolny do wywołania zatrucia, choroby lub śmierci organizmu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "chem_bio",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Broń chemiczna wykorzystuje związki chemiczne o <w>toksycznych</w> właściwościach.",
                            "target_lang_translations": {
                                "ru": "Химическое оружие использует химические соединения с <w>токсичными</w> свойствами."
                            }
                        },
                        {
                            "text": "Niektóre rośliny doniczkowe mogą być silnie <w>toksyczne</w> dla zwierząt domowych.",
                            "target_lang_translations": {
                                "ru": "Некоторые комнатные растения могут быть сильно <w>токсичны</w> для домашних животных."
                            }
                        }
                    ],
                    "synonyms": [
                        "trujący"
                    ],
                    "antonyms": [
                        "nietoksyczny",
                        "atoksyczny"
                    ],
                    "common_phrases": [
                        "substancja toksyczna",
                        "odpady toksyczne",
                        "gaz toksyczny",
                        "związek toksyczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий ядовитыми свойствами, способный вызвать отравление, болезнь или смерть организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "токсичный",
                                "target_lang_sense_clarification": "о веществах, отходах"
                            },
                            {
                                "target_lang_word": "ядовитый",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b78e560d-03c5-3737-bb55-e18c4fe2b500",
                    "sense_definition": "Wywierający na kogoś destrukcyjny wpływ, wpływający negatywnie na czyjąś psychikę lub relacje międzyludzkie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dopiero przyjaciele uświadomili jej, jak <w>toksyczny</w> był jej związek z Pawłem.",
                            "target_lang_translations": {
                                "ru": "Только друзья помогли ей осознать, насколько <w>токсичными</w> были её отношения с Павлом."
                            }
                        },
                        {
                            "text": "Unikam ludzi, którzy wprowadzają <w>toksyczną</w> atmosferę w miejscu pracy.",
                            "target_lang_translations": {
                                "ru": "Я избегаю людей, которые создают <w>токсичную</w> атмосферу на рабочем месте."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezdrowy"
                    ],
                    "common_phrases": [
                        "toksyczny związek",
                        "toksyczna relacja",
                        "toksyczni ludzie",
                        "toksyczna atmosfera",
                        "toksyczna męskość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывающий разрушительное влияние на кого-либо, негативно влияющий на психику или межличностные отношения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "токсичный",
                                "target_lang_sense_clarification": "перен. о людях, отношениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de5d87f5-ffbd-322e-8b9a-cf2803fe1a7d",
                    "sense_definition": "Spowodowany zatruciem lub działaniem szkodliwych toksyn na organizm.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chem_bio",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent został przyjęty na oddział z objawami wstrząsu <w>toksycznego</w>.",
                            "target_lang_translations": {
                                "ru": "Пациент поступил в отделение с симптомами <w>токсического</w> шока."
                            }
                        },
                        {
                            "text": "Długotrwałe przyjmowanie leku wywołało u niego reakcję <w>toksyczną</w>.",
                            "target_lang_translations": {
                                "ru": "Длительный прием лекарства вызвал у него <w>токсическую</w> реакцию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wstrząs toksyczny",
                        "obrzęk toksyczny",
                        "zespół toksyczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызванный отравлением или воздействием вредных токсинов на организм (медицинский термин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "токсический",
                                "target_lang_sense_clarification": "мед. - о шоке, реакции"
                            }
                        ]
                    }
                }
            ]
        }
    ]

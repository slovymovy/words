    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c19d6cec-337a-356a-9802-ddc098fc95f2",
                    "sense_definition": "Gwałtowny skok do wody (zwykle głową w dół) lub szybkie schowanie się, zanurzenie w czymś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_dive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopiec wziął krótki rozbieg i dał <nura> w chłodną toń jeziora.",
                            "target_lang_translations": {
                                "ru": "Мальчик немного разбежался и <нырнул> в прохладную гладь озера."
                            }
                        },
                        {
                            "text": "Przestraszona żaba dała <nura> pod liść lilii wodnej.",
                            "target_lang_translations": {
                                "ru": "Испуганная лягушка <нырнула> под лист кувшинки."
                            }
                        }
                    ],
                    "synonyms": [
                        "skok",
                        "sus",
                        "zanurzenie"
                    ],
                    "antonyms": [
                        "wynurzenie"
                    ],
                    "common_phrases": [
                        "dać nura"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резкий прыжок в воду (обычно головой вниз) или быстрое укрытие, погружение во что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нырок",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ff95c15-8958-3b68-aef7-cc8e0af54773",
                    "sense_definition": "Duży ptak wodny z rzędu nurokształtnych, świetnie pływający i nurkujący, zamieszkujący północną półkulę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na środku jeziora pływał samotny <nur>, co chwilę znikając pod powierzchnią wody.",
                            "target_lang_translations": {
                                "ru": "Посреди озера плавала одинокая <гагара>, то и дело исчезая под водой."
                            }
                        },
                        {
                            "text": "Ornitolodzy przyjechali nad zalew, aby obserwować rzadkiego <nura> czarnoszyjego.",
                            "target_lang_translations": {
                                "ru": "Орнитологи приехали на залив, чтобы понаблюдать за редкой черношейной <гагарой>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная водоплавающая птица северного полушария, отлично плавающая и ныряющая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гагара",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "71449de9-4c6f-3327-860e-ffd28f74feb1",
                    "sense_definition": "Wieś w Polsce położona w województwie mazowieckim, nad rzeką Bug.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "toponym_poland",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mój dziadek pochodzi z małej miejscowości o nazwie <Nur>.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка родом из маленькой местности под названием <Нур>."
                            }
                        },
                        {
                            "text": "Przejeżdżaliśmy przez <Nur> w drodze na wschodnie Podlasie.",
                            "target_lang_translations": {
                                "ru": "Мы проезжали через <Нур> по дороге на восточное Подляшье."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревня в Польше в Мазовецком воеводстве, на реке Буг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Нур",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "642765aa-0412-319f-9790-fab2c165660d",
                    "sense_definition": "Imię męskie pochodzenia arabskiego, oznaczające światło.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_male",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Historyczny władca <Nur> ed-Din odegrał ważną rolę w czasie wypraw krzyżowych.",
                            "target_lang_translations": {
                                "ru": "Исторический правитель <Нур> ад-Дин сыграл важную роль во времена крестовых походов."
                            }
                        },
                        {
                            "text": "Mój znajomy z wymiany studenckiej ma na imię <Nur>.",
                            "target_lang_translations": {
                                "ru": "Моего знакомого по студенческому обмену зовут <Нур>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя арабского происхождения, означающее «свет»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Нур",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c4ed553-4c8e-34ba-824d-91a07ef5c8f1",
                    "sense_definition": "Imię żeńskie pochodzenia arabskiego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_female",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Turecka zawodniczka <Nur> zdobyła złoty medal w taekwondo.",
                            "target_lang_translations": {
                                "ru": "Турецкая спортсменка <Нур> завоевала золотую медаль в тхэквондо."
                            }
                        },
                        {
                            "text": "Imię <Nur> jest bardzo popularne w krajach Bliskiego Wschodu.",
                            "target_lang_translations": {
                                "ru": "Имя <Нур> очень популярно в странах Ближнего Востока."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя арабского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Нур",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]

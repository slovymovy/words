                        "porzucony"
                    ],
                    "common_phrases": [
                        "został przejęty",
                        "towar przejęty",
                        "budynek przejęty"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imiesłów przymiotnikowy bierny (passive adjectival participle)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Страдательное причастие от глагола przejąć; взятый во владение, перехваченный или взятый под контроль"
                    },
                    "translations": {
                        "ru": [
                            {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "03d4e45d-99e0-3c6c-b821-143bbb5d2135",
                    "sense_definition": "Крупное хищное млекопитающее семейства кошачьих с короткой жёлтой шерстью и пышной гривой у самцов.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В зоопарке мы долго стояли у клетки, где спал огромный <w>лев</w>.",
                            "target_lang_translations": {
                                "en": "At the zoo, we stood for a long time by the cage where a huge <w>lion</w> was sleeping."
                            }
                        },
                        {
                            "text": "Грозный <w>лев</w> медленно вышел на арену цирка под аплодисменты зрителей.",
                            "target_lang_translations": {
                                "en": "The formidable <w>lion</w> slowly walked out into the circus arena to the applause of the audience."
                            }
                        }
                    ],
                    "synonyms": [
                        "царь зверей"
                    ],
                    "common_phrases": [
                        "африканский лев",
                        "грива льва",
                        "рычание льва",
                        "морской лев"
                    ],
                    "target_lang_definitions": {
                        "en": "A large carnivorous mammal of the cat family with short yellow fur and a lush mane in males."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lion",
                                "target_lang_sense_clarification": "animal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8217ff15-f1ec-32ca-8772-566206cb81d3",
                    "sense_definition": "Скульптурное или живописное изображение этого животного.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вход в старинный дворец охранял мраморный <w>лев</w> с отбитой лапой.",
                            "target_lang_translations": {
                                "en": "The entrance to the ancient palace was guarded by a marble <w>lion</w> with a broken paw."
                            }
                        },
                        {
                            "text": "Туристы любят фотографироваться, сидя верхом на бронзовом <w>льве</w>.",
                            "target_lang_translations": {
                                "en": "Tourists love taking photos sitting astride the bronze <w>lion</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "каменный лев",
                        "мраморные львы"
                    ],
                    "target_lang_definitions": {
                        "en": "A sculptural or pictorial representation of this animal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lion",
                                "target_lang_sense_clarification": "statue or image"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb335e6e-92e3-33d5-8c64-74c5dd5c3c06",
                    "sense_definition": "Денежная единица Болгарии.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За вкусный обед в ресторане Софии мы заплатили пятьдесят <w>левов</w>.",
                            "target_lang_translations": {
                                "en": "We paid fifty <w>levs</w> for a delicious lunch in a Sofia restaurant."
                            }
                        },
                        {
                            "text": "Обменный курс <w>лева</w> к евро остаётся стабильным уже долгое время.",
                            "target_lang_translations": {
                                "en": "The exchange rate of the <w>lev</w> to the euro has remained stable for a long time."
                            }
                        }
                    ],
                    "common_phrases": [
                        "болгарский лев",
                        "курс лева"
                    ],
                    "target_lang_definitions": {
                        "en": "The monetary unit of Bulgaria."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lev",
                                "target_lang_sense_clarification": "currency"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75520ac2-f0c0-3fae-8f8d-9f96138ea050",
                    "sense_definition": "Человек, обладающий большой силой, храбростью и благородством.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он защищал свою семью как настоящий <w>лев</w>, не зная страха и сомнений.",
                            "target_lang_translations": {
                                "en": "He defended his family like a real <w>lion</w>, knowing no fear or doubt."
                            }
                        },
                        {
                            "text": "В молодости этот старик был настоящим <w>львом</w>, которого боялась вся округа.",
                            "target_lang_translations": {
                                "en": "In his youth, this old man was a real <w>lion</w> whom the whole neighborhood feared."
                            }
                        }
                    ],
                    "synonyms": [
                        "орёл"
                    ],
                    "antonyms": [
                        "трус",
                        "заяц",
                        "тряпка"
                    ],
                    "common_phrases": [
                        "сражаться как лев"
                    ],
                    "target_lang_definitions": {
                        "en": "A person possessing great strength, courage, and nobility."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lion",
                                "target_lang_sense_clarification": "brave person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dff8b675-eff0-3eb7-8f4c-2c237dd13d6c",
                    "sense_definition": "Человек, пользующийся большим успехом и вниманием в высшем обществе.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На балу появился известный <w>лев</w>, и все дамы тут же обратили на него внимание.",
                            "target_lang_translations": {
                                "en": "A famous <w>social lion</w> appeared at the ball, and all the ladies immediately paid attention to him."
                            }
                        },
                        {
                            "text": "Он считал себя светским <w>львом</w>, хотя многие находили его манеры просто смешными.",
                            "target_lang_translations": {
                                "en": "He considered himself a <w>social lion</w>, although many found his manners simply ridiculous."
                            }
                        }
                    ],
                    "synonyms": [
                        "светский лев"
                    ],
                    "common_phrases": [
                        "светский лев",
                        "местный лев"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often used in 19th-century literature context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person enjoying great success and attention in high society."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "social lion",
                                "target_lang_sense_clarification": "celebrity/socialite"
                            },
                            {
                                "target_lang_word": "lion",
                                "target_lang_sense_clarification": "literary/dated"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a0b4f7c7-d1c3-30a8-b93c-020502d2d27f",
                    "sense_definition": "Мужское личное имя.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Моего дедушку звали <w>Лев</w>, и он был очень добрым и мудрым человеком.",
                            "target_lang_translations": {
                                "en": "My grandfather's name was <w>Lev</w>, and he was a very kind and wise man."
                            }
                        },
                        {
                            "text": "Великий русский писатель <w>Лев</w> Николаевич Толстой известен во всём мире.",
                            "target_lang_translations": {
                                "en": "The great Russian writer <w>Lev</w> Nikolayevich Tolstoy is known all over the world."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Лев Толстой",
                        "дядя Лев"
                    ],
                    "target_lang_definitions": {
                        "en": "A male personal name."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Lev",
                                "target_lang_sense_clarification": "Russian name"
                            },
                            {
                                "target_lang_word": "Leo",
                                "target_lang_sense_clarification": "English equivalent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbc96220-85db-374e-9a56-827c6644b6e4",
                    "sense_definition": "Зодиакальное созвездие и соответствующий знак зодиака (с 23 июля по 23 августа).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "name_group",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Мой брат родился в начале августа, поэтому по гороскопу он <w>Лев</w>.",
                            "target_lang_translations": {
                                "en": "My brother was born in early August, so according to the horoscope he is a <w>Leo</w>."
                            }
                        },
                        {
                            "text": "На ночном небе хорошо видно созвездие <w>Лев</w>, если погода достаточно ясная.",
                            "target_lang_translations": {
                                "en": "The constellation <w>Leo</w> is clearly visible in the night sky if the weather is clear enough."
                            }
                        }
                    ],
                    "synonyms": [
                        "Leo"
                    ],
                    "antonyms": [
                        "Водоле́й"
                    ],
                    "common_phrases": [
                        "знак Льва",
                        "созвездие Льва"
                    ],
                    "target_lang_definitions": {
                        "en": "Zodiac constellation and corresponding zodiac sign (from July 23 to August 23)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Leo",
                                "target_lang_sense_clarification": "zodiac sign"
                            }
                        ]
                    }
                }
            ]
        },
        {

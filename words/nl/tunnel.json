{
    "word_family": [
        "tunnelen",
        "tunnelvisie",
        "tunnelbouw",
        "tunneltje"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2f6de98c-15b9-365d-893d-c05fe0402d8c",
                    "sense_definition": "Een kunstmatige ondergrondse doorgang voor verkeer, treinen of voetgangers.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De trein rijdt met hoge snelheid de donkere <w>tunnel</w> in.",
                            "target_lang_translations": {
                                "ru": "Поезд на высокой скорости въезжает в темный <w>тоннель</w>."
                            }
                        },
                        {
                            "text": "We moesten lang wachten voordat we door de <w>tunnel</w> konden rijden.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось долго ждать, прежде чем мы смогли проехать через <w>тоннель</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderdoorgang"
                    ],
                    "common_phrases": [
                        "door de tunnel",
                        "lange tunnel",
                        "einde van de tunnel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственный подземный проход для транспорта, поездов или пешеходов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тоннель",
                                "target_lang_sense_clarification": "транспортное сооружение"
                            },
                            {
                                "target_lang_word": "туннель",
                                "target_lang_sense_clarification": "вариант написания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5021658-32d9-390d-8db2-8430247c9b39",
                    "sense_definition": "Een langwerpig hol of gangenstelsel onder de grond, vaak gegraven door dieren.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De mol heeft een lange <w>tunnel</w> gegraven dwars door onze tuin.",
                            "target_lang_translations": {
                                "ru": "Крот прорыл длинный <w>ход</w> прямо через наш сад."
                            }
                        },
                        {
                            "text": "Konijnen verdwijnen snel in hun <w>tunnel</w> als er gevaar dreigt.",
                            "target_lang_translations": {
                                "ru": "Кролики быстро исчезают в своей <w>норе</w>, когда грозит опасность."
                            }
                        }
                    ],
                    "synonyms": [
                        "gang",
                        "hol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная нора или система ходов под землей, часто вырытая животными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ход",
                                "target_lang_sense_clarification": "подземный коридор"
                            },
                            {
                                "target_lang_word": "нора",
                                "target_lang_sense_clarification": "жилище животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "722fd505-437e-39bf-b431-d31e5fae87ff",
                    "sense_definition": "Een lange, overdekte ruimte of constructie die doet denken aan een doorgang.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude bomen langs de weg vormden een groene <w>tunnel</w> van bladeren.",
                            "target_lang_translations": {
                                "ru": "Старые деревья вдоль дороги образовали зеленый <w>туннель</w> из листвы."
                            }
                        },
                        {
                            "text": "Op het schoolplein kruipen de kinderen lachend door de plastic <w>tunnel</w>.",
                            "target_lang_translations": {
                                "ru": "На школьном дворе дети смеясь ползают через пластиковый <w>туннель</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинное крытое пространство или конструкция, напоминающая проход."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туннель",
                                "target_lang_sense_clarification": "форма или конструкция"
                            },
                            {
                                "target_lang_word": "коридор",
                                "target_lang_sense_clarification": "метафорически"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "611ca513-ff3a-33e2-b753-ab01af6c1f7a",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>tunnelen</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>tunnel</w> de data via een beveiligde verbinding naar de server.",
                            "target_lang_translations": {
                                "ru": "Я <w>туннелирую</w> данные через защищенное соединение на сервер."
                            }
                        },
                        {
                            "text": "Als ik een weg aanleg, <w>tunnel</w> ik liever onder de berg door dan eroverheen.",
                            "target_lang_translations": {
                                "ru": "Если я прокладываю дорогу, я предпочитаю <w>прокладывать туннель</w> под горой, чем идти поверху."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "1-е лицо ед. ч. настоящего времени от глагола tunnelen."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туннелирую",
                                "target_lang_sense_clarification": "компьютерный термин"
                            },
                            {
                                "target_lang_word": "прокладываю туннель",
                                "target_lang_sense_clarification": "строительство/рытье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07419962-4d45-31d2-b677-39555581390d",

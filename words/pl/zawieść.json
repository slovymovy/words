    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bcd92872-a9da-3866-984e-2763dffc85aa",
                    "sense_definition": "Sprawić komuś przykrość, nie spełniając pokładanych w kimś nadziei lub oczekiwań.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "disappoint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obiecuję, że zrobię wszystko, aby cię nie <w>zawieść</w>.",
                            "target_lang_translations": {
                                "ru": "Обещаю, что сделаю всё, чтобы тебя не <w>подвести</w>."
                            }
                        },
                        {
                            "text": "Bardzo mnie <w>zawiodłeś</w>, bo liczyłem na twoją pomoc.",
                            "target_lang_translations": {
                                "ru": "Ты меня очень <w>подвёл</w>, так как я рассчитывал на твою помощь."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozczarować",
                        "sprawić zawód"
                    ],
                    "antonyms": [
                        "zadowolić"
                    ],
                    "common_phrases": [
                        "zawieść czyjeś zaufanie",
                        "zawieść nadzieje",
                        "zawieść oczekiwania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинить кому-то огорчение, не оправдав возложенных надежд или ожиданий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвести",
                                "target_lang_sense_clarification": "не оправдать доверия или ожиданий"
                            },
                            {
                                "target_lang_word": "разочаровать",
                                "target_lang_sense_clarification": "вызвать чувство неудовлетворенности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1154652d-572d-3084-986d-c721892dcea3",
                    "sense_definition": "Nie przynieść oczekiwanego rezultatu lub zakończyć się niepowodzeniem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fail_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz plan <w>zawiódł</w>, więc musimy zacząć wszystko od początku.",
                            "target_lang_translations": {
                                "ru": "Наш план <w>провалился</w>, поэтому нам придётся начать всё сначала."
                            }
                        },
                        {
                            "text": "Wszystkie tradycyjne metody leczenia niestety <w>zawiodły</w>.",
                            "target_lang_translations": {
                                "ru": "Все традиционные методы лечения, к сожалению, <w>не сработали</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozczarować",
                        "sprawić zawód"
                    ],
                    "antonyms": [
                        "powieść się",
                        "udać się"
                    ],
                    "common_phrases": [
                        "plan zawiódł",
                        "wszystko zawiodło",
                        "metoda zawiodła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не принести ожидаемого результата или закончиться неудачей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провалиться",
                                "target_lang_sense_clarification": "о планах или затеях"
                            },
                            {
                                "target_lang_word": "не сработать",
                                "target_lang_sense_clarification": "о методах или способах"
                            },
                            {
                                "target_lang_word": "потерпеть неудачу",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d887b23b-324b-3451-967e-df3867ad3162",
                    "sense_definition": "Przestać działać prawidłowo lub odmówić posłuszeństwa (o urządzeniach, systemach lub organizmie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fail_mechanical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zamieszanie na lotnisku wynika z tego, że <w>zawiódł</w> system informatyczny.",
                            "target_lang_translations": {
                                "ru": "Хаос в аэропорту возник из-за того, что <w>отказала</w> информационная система."
                            }
                        },
                        {
                            "text": "W najgorszym możliwym momencie <w>zawiodły</w> hamulce w samochodzie.",
                            "target_lang_translations": {
                                "ru": "В самый неподходящий момент <w>отказали</w> тормоза в машине."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozczarować",
                        "sprawić zawód"
                    ],
                    "antonyms": [
                        "zadziałać"
                    ],
                    "common_phrases": [
                        "hamulce zawiodły",
                        "pamięć zawiodła",
                        "zdrowie zawiodło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать работать должным образом или сломаться (об устройствах, системах или здоровье)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказать",
                                "target_lang_sense_clarification": "перестать функционировать"
                            },
                            {
                                "target_lang_word": "выйти из строя",
                                "target_lang_sense_clarification": "сломаться"
                            },
                            {
                                "target_lang_word": "подвести",
                                "target_lang_sense_clarification": "перестать работать в нужный момент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b19782e-7b64-3e48-8eb7-80545e67a665",
                    "sense_definition": "Wskazać drogę i doprowadzić kogoś lub coś do określonego miejsca.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "lead",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myśleliśmy, że zgubiliśmy się na dobre, ale w końcu leśna ścieżka <w>zawiodła</w> nas do głównego szlaku.",
                            "target_lang_translations": {
                                "ru": "Мы думали, что окончательно заблудились, но в конце концов лесная тропинка <w>привела</w> нас к главной тропе."
                            }
                        },
                        {
                            "text": "Przewodnik <w>zawiódł</w> grupę turystów w bezpieczne miejsce przed burzą.",
                            "target_lang_translations": {
                                "ru": "Гид <w>привёл</w> группу туристов в безопасное место до начала грозы."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozczarować",
                        "sprawić zawód"
                    ],
                    "common_phrases": [
                        "zawieść do celu",
                        "droga zawiodła"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "somewhat literary compared to 'zaprowadzić'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Указать путь и привести кого-то или что-то в определенное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привести",
                                "target_lang_sense_clarification": "доставить в место назначения"
                            },
                            {
                                "target_lang_word": "завести",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc094e04-5059-34a7-90d8-f7258a170a02",
                    "sense_definition": "Предмет одежды в виде куска ткани, обычно квадратной формы, который повязывают на голову или накидывают на плечи.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка накинула на плечи тёплый пуховый <w>платок</w>.",
                            "target_lang_translations": {
                                "en": "Grandma threw a warm downy <w>shawl</w> over her shoulders."
                            }
                        },
                        {
                            "text": "В церкви женщины обычно надевают на голову <w>платок</w>.",
                            "target_lang_translations": {
                                "en": "In church, women usually put a <w>headscarf</w> on their head."
                            }
                        }
                    ],
                    "synonyms": [
                        "плат"
                    ],
                    "common_phrases": [
                        "пуховый платок",
                        "повязать платок",
                        "накинуть платок",
                        "головной платок"
                    ],
                    "target_lang_definitions": {
                        "en": "A piece of clothing, usually a square of fabric, worn over the head or shoulders."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shawl",
                                "target_lang_sense_clarification": "worn over the shoulders"
                            },
                            {
                                "target_lang_word": "headscarf",
                                "target_lang_sense_clarification": "worn on the head"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "283ee5ee-babe-39dc-bba7-907bcc3c4dfb",
                    "sense_definition": "Небольшой кусок ткани или бумаги для вытирания носа, лица или глаз.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У тебя есть чистый носовой <w>платок</w>?",
                            "target_lang_translations": {
                                "en": "Do you have a clean <w>handkerchief</w>?"
                            }
                        },
                        {
                            "text": "Он достал из кармана <w>платок</w> и вытер лоб.",
                            "target_lang_translations": {
                                "en": "He took a <w>handkerchief</w> out of his pocket and wiped his forehead."
                            }
                        }
                    ],
                    "synonyms": [
                        "носовой платок"
                    ],
                    "common_phrases": [
                        "носовой платок",
                        "достать платок",
                        "бумажный платок"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "when used without the adjective 'носовой'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A small piece of cloth or paper used for wiping the nose or face."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "handkerchief",
                                "target_lang_sense_clarification": "made of cloth"
                            },
                            {
                                "target_lang_word": "tissue",
                                "target_lang_sense_clarification": "made of paper"
                            }
                        ]
                    }
                }
            ]
        }
    ]

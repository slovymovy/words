                {
                    "sense_id": "be61ef50-2816-3c9d-a4d4-cebcc336f93d",
                    "sense_definition": "Toets op een computertoetsenbord die wordt gebruikt om een actie te annuleren, een menu te sluiten of een lopende opdracht af te breken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computer_key",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Druk op <w>escape</w> om het programma direct te sluiten.",
                            "target_lang_translations": {
                                "en": "Press <w>escape</w> to close the program immediately.",
                                "ru": "Нажмите <w>Escape</w>, чтобы немедленно закрыть программу."
                            }
                        },
                        {
                            "text": "De <w>escape</w> zit meestal linksboven op je toetsenbord.",
                            "target_lang_translations": {
                                "en": "The <w>escape key</w> is usually located at the top left of your keyboard.",
                                "ru": "Клавиша <w>Escape</w> обычно находится в левом верхнем углу клавиатуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "esc-toets",
                        "escapetoets"
                    ],
                    "common_phrases": [
                        "druk op escape",
                        "de toets escape"
                    ],
                    "target_lang_definitions": {
                        "en": "Key on a computer keyboard used to cancel an action, close a menu, or abort a running command.",
                        "ru": "Клавиша на компьютерной клавиатуре, используемая для отмены действия, закрытия меню или прерывания выполняемой команды."
                    },
                    "translations": {

{
    "word_family": [
        "appealing",
        "appellant",
        "appellate",
        "appealable",
        "unappealing"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7c94a594-b3a1-31c0-b065-321566b742ff",
                    "sense_definition": "To be attractive, interesting, or pleasing to someone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The idea of working from home really <w>appeals</w> to me.",
                            "target_lang_translations": {
                                "ru": "Идея работать из дома мне очень <w>нравится</w>."
                            }
                        },
                        {
                            "text": "Bright colors usually <w>appeal</w> to small children more than pastels do.",
                            "target_lang_translations": {
                                "ru": "Яркие цвета обычно <w>привлекают</w> маленьких детей больше, чем пастельные."
                            }
                        }
                    ],
                    "synonyms": [
                        "attract",
                        "interest",
                        "please",
                        "allure"
                    ],
                    "antonyms": [
                        "bore",
                        "repel",
                        "disgust"
                    ],
                    "common_phrases": [
                        "appeal to someone",
                        "appeal to the eye"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть привлекательным, интересным или приятным для кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нравиться",
                                "target_lang_sense_clarification": "быть приятным, вызывать симпатию"
                            },
                            {
                                "target_lang_word": "привлекать",
                                "target_lang_sense_clarification": "вызывать интерес"
                            },
                            {
                                "target_lang_word": "импонировать",
                                "target_lang_sense_clarification": "вызывать уважение или симпатию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28312cbf-1eba-33de-b802-d1acb16dcdc0",
                    "sense_definition": "To make a serious, urgent, or formal request to the public or an authority for help, money, or information.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police <w>appeal</w> to the public for any information regarding the accident.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>обращается</w> к общественности с просьбой сообщить любую информацию об аварии."
                            }
                        },
                        {
                            "text": "I <w>appeal</w> to your sense of justice to make the right decision in this matter.",
                            "target_lang_translations": {
                                "ru": "Я <w>взываю</w> к вашему чувству справедливости, чтобы вы приняли правильное решение в этом вопросе."
                            }
                        }
                    ],
                    "synonyms": [
                        "plead",
                        "beg",
                        "implore",
                        "entreat",
                        "request"
                    ],
                    "common_phrases": [
                        "appeal for help",
                        "appeal to reason",
                        "appeal for calm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться с серьезной, срочной или официальной просьбой к общественности или властям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "с просьбой или призывом"
                            },
                            {
                                "target_lang_word": "взывать",
                                "target_lang_sense_clarification": "эмоционально просить"
                            },
                            {
                                "target_lang_word": "призывать",
                                "target_lang_sense_clarification": "побуждать к действию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20fe17ad-719c-3bd3-9373-d5480b11c050",
                    "sense_definition": "To formally request that a legal decision made by a lower court be reviewed and changed by a higher court.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The defendant decided to <w>appeal</w> against the harsh sentence imposed by the judge.",
                            "target_lang_translations": {
                                "ru": "Подсудимый решил <w>обжаловать</w> суровый приговор, вынесенный судьей."
                            }
                        },
                        {
                            "text": "If they lose this case, they plan to <w>appeal</w> to the Supreme Court.",
                            "target_lang_translations": {
                                "ru": "Если они проиграют это дело, они планируют <w>подать апелляцию</w> в Верховный суд."
                            }
                        }
                    ],
                    "synonyms": [
                        "petition",
                        "contest"
                    ],
                    "common_phrases": [
                        "appeal against a verdict",
                        "appeal a decision",
                        "right to appeal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально просить о пересмотре судебного решения вышестоящим судом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обжаловать",
                                "target_lang_sense_clarification": "подавать жалобу на решение суда"
                            },
                            {
                                "target_lang_word": "подавать апелляцию",
                                "target_lang_sense_clarification": "юридическая процедура"
                            },
                            {
                                "target_lang_word": "апеллировать",
                                "target_lang_sense_clarification": "обращаться в высшую инстанцию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49cf6abf-a914-3201-9559-feadefe186e1",
                    "sense_definition": "In cricket, to ask the umpire for a decision on whether a batsman is out or not.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bowler turned around to <w>appeal</w> loudly after the ball hit the pads.",
                            "target_lang_translations": {
                                "ru": "Боулер обернулся, чтобы громко <w>апеллировать</w> к судье после того, как мяч ударил по щиткам."
                            }
                        },
                        {
                            "text": "Players often <w>appeal</w> in unison to put pressure on the umpire during a close game.",
                            "target_lang_translations": {
                                "ru": "Игроки часто <w>апеллируют</w> хором, чтобы оказать давление на судью во время напряженной игры."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В крикете: спрашивать судью, выведен ли бэтсмен из игры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "апеллировать",
                                "target_lang_sense_clarification": "к судье в спорте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2e751031-3b16-35dc-8e7e-6455aeb9b9ce",
                    "sense_definition": "The quality of being attractive, interesting, or charming.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new video game has a lot of <w>appeal</w> for teenagers because of its graphics.",
                            "target_lang_translations": {
                                "ru": "Новая видеоигра обладает большой <w>привлекательностью</w> для подростков из-за своей графики."
                            }
                        },
                        {
                            "text": "I never really understood the <w>appeal</w> of collecting stamps as a hobby.",
                            "target_lang_translations": {
                                "ru": "Я никогда по-настоящему не понимал <w>привлекательности</w> коллекционирования марок как хобби."
                            }
                        }
                    ],
                    "synonyms": [
                        "attraction",
                        "charm",
                        "allure",
                        "charisma"
                    ],
                    "antonyms": [
                        "repulsion"
                    ],
                    "common_phrases": [
                        "mass appeal",
                        "sex appeal",
                        "broad appeal",
                        "hold appeal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество быть привлекательным, интересным или очаровательным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привлекательность",
                                "target_lang_sense_clarification": "свойство притягивать"
                            },
                            {
                                "target_lang_word": "обаяние",
                                "target_lang_sense_clarification": "шарм"
                            },
                            {
                                "target_lang_word": "притягательность",
                                "target_lang_sense_clarification": "сила притяжения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e1e9581-93e8-3bd4-a54b-699189da02f6",
                    "sense_definition": "A serious, urgent, or formal request for help, support, or a decision.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The charity launched an <w>appeal</w> for donations to help the earthquake victims.",
                            "target_lang_translations": {
                                "ru": "Благотворительная организация выступила с <w>призывом</w> о пожертвованиях для помощи жертвам землетрясения."
                            }
                        },
                        {
                            "text": "His emotional <w>appeal</w> for forgiveness was broadcast on national television.",
                            "target_lang_translations": {
                                "ru": "Его эмоциональный <w>призыв</w> о прощении транслировался по национальному телевидению."
                            }
                        }
                    ],
                    "synonyms": [
                        "plea",
                        "request",
                        "entreaty",
                        "petition",
                        "call"
                    ],
                    "common_phrases": [
                        "make an appeal",
                        "appeal for witnesses",
                        "fundraising appeal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серьезная, срочная или официальная просьба о помощи, поддержке или решении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "призыв",
                                "target_lang_sense_clarification": "просьба о помощи или действии"
                            },
                            {
                                "target_lang_word": "обращение",
                                "target_lang_sense_clarification": "официальное заявление"
                            },
                            {
                                "target_lang_word": "воззвание",
                                "target_lang_sense_clarification": "торжественное обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a835b2fc-42ae-37da-943f-615cb60f4aa1",
                    "sense_definition": "A formal application to a higher court to review and reverse the decision of a lower court.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her lawyer is preparing an <w>appeal</w> against the conviction based on new evidence.",
                            "target_lang_translations": {
                                "ru": "Ее адвокат готовит <w>апелляцию</w> против обвинительного приговора на основании новых улик."
                            }
                        },
                        {
                            "text": "The judge rejected the <w>appeal</w>, so the original sentence stands.",
                            "target_lang_translations": {
                                "ru": "Судья отклонил <w>апелляцию</w>, поэтому первоначальный приговор остается в силе."
                            }
                        }
                    ],
                    "synonyms": [
                        "petition",
                        "retrial request"
                    ],
                    "common_phrases": [
                        "court of appeal",
                        "file an appeal",
                        "win an appeal",
                        "lose an appeal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное заявление в вышестоящий суд о пересмотре решения нижестоящего суда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "апелляция",
                                "target_lang_sense_clarification": "жалоба в суд"
                            },
                            {
                                "target_lang_word": "обжалование",
                                "target_lang_sense_clarification": "процесс оспаривания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
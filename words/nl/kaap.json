{
    "word_family": [
        "kapen",
        "kaper",
        "kaping"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "30c4d335-5d07-314e-8d6d-7a6b20b02aaf",
                    "sense_definition": "Een stuk land dat in zee uitsteekt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het schip voer voorzichtig om de gevaarlijke <w>kaap</w> heen.",
                            "target_lang_translations": {
                                "ru": "Корабль осторожно обогнул опасный <w>мыс</w>."
                            }
                        },
                        {
                            "text": "Vanaf de hoge <w>kaap</w> hadden we een prachtig uitzicht over de oceaan.",
                            "target_lang_translations": {
                                "ru": "С высокого <w>мыса</w> нам открывался прекрасный вид на океан."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Kaap de Goede Hoop",
                        "de kaap ronden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок суши, выдающийся в море."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мыс",
                                "target_lang_sense_clarification": "география"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0319669a-f612-31a8-8f72-f6fcc6716b56",
                    "sense_definition": "Een belangrijk moment, grens of mijlpaal die bereikt wordt, vaak met betrekking tot leeftijd of een bedrag.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft onlangs de <w>kaap</w> van vijftig jaar bereikt.",
                            "target_lang_translations": {
                                "ru": "Недавно он достиг пятидесятилетнего <w>рубежа</w>."
                            }
                        },
                        {
                            "text": "De inzamelingsactie ging vlot over de <w>kaap</w> van duizend euro.",
                            "target_lang_translations": {
                                "ru": "Сбор средств быстро преодолел <w>отметку</w> в тысячу евро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de kaap van",
                        "een magische kaap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Важный момент, граница или веха, часто относящаяся к возрасту или сумме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рубеж",
                                "target_lang_sense_clarification": "граница/веха"
                            },
                            {
                                "target_lang_word": "отметка",
                                "target_lang_sense_clarification": "уровень/сумма"
                            },
                            {
                                "target_lang_word": "порог",
                                "target_lang_sense_clarification": "возраст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5372d0a4-42aa-3e8e-8029-8e6f64c7cf7d",
                    "sense_definition": "Het met overheidstoestemming aanvallen en plunderen van vijandelijke schepen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zeventiende eeuw was de <w>kaap</w> een lucratieve bezigheid voor zeelieden.",
                            "target_lang_translations": {
                                "ru": "В семнадцатом веке <w>каперство</w> было прибыльным занятием для моряков."
                            }
                        },
                        {
                            "text": "De regering gaf toestemming voor de <w>kaap</w> om de vijandelijke handel te verstoren.",
                            "target_lang_translations": {
                                "ru": "Правительство дало разрешение на <w>каперство</w>, чтобы нарушить вражескую торговлю."
                            }
                        }
                    ],
                    "synonyms": [
                        "kaapvaart"
                    ],
                    "common_phrases": [
                        "op de kaap varen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нападение на вражеские суда и их разграбление с разрешения правительства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каперство",
                                "target_lang_sense_clarification": "историческое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "345c0940-f5b2-32c8-ab8d-23a43a12ef29",
                    "sense_definition": "Een baken voor de scheepvaart met een kenmerkende vorm, meestal gemaakt van hout of metaal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude houten <w>kaap</w> op het duin dient als herkenningspunt voor vissers.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный <w>навигационный знак</w> на дюне служит ориентиром для рыбаков."
                            }
                        },
                        {
                            "text": "Zonder de <w>kaap</w> zouden schepen hier gemakkelijk vastlopen op de zandbanken.",
                            "target_lang_translations": {
                                "ru": "Без этого <w>знака</w> корабли могли бы легко сесть здесь на мель."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Навигационный знак для судоходства характерной формы, обычно деревянный или металлический."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навигационный знак",
                                "target_lang_sense_clarification": "морской термин"
                            },
                            {
                                "target_lang_word": "створный знак",
                                "target_lang_sense_clarification": "ориентир"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8eae0418-90cd-3576-88d5-64dcc3b809dd",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>kapen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kaap</w> de laatste stoel voordat iemand anders gaat zitten.",
                            "target_lang_translations": {
                                "ru": "Я <w>перехвачу</w> последний стул, пока никто другой не сел."
                            }
                        },
                        {
                            "text": "Soms <w>kaap</w> ik een goed idee weg uit een vergadering.",
                            "target_lang_translations": {
                                "ru": "Иногда я <w>перехватываю</w> хорошую идею на собрании."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола \"kapen\" (угонять, перехватывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перехватываю",
                                "target_lang_sense_clarification": "быстро забираю"
                            },
                            {
                                "target_lang_word": "угоняю",
                                "target_lang_sense_clarification": "транспорт"
                            },
                            {
                                "target_lang_word": "захватываю",
                                "target_lang_sense_clarification": "силой/хитростью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ef6af43-97b5-3f54-9cea-25fea89a4f06",
                    "sense_definition": "Gebiedende wijs van <w>kapen</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Kaap</w> die bal weg bij de tegenstander!",
                            "target_lang_translations": {
                                "ru": "<w>Перехвати</w> этот мяч у противника!"
                            }
                        },
                        {
                            "text": "<w>Kaap</w> de hoofdprijs weg voor de neus van de concurrentie.",
                            "target_lang_translations": {
                                "ru": "<w>Уведи</w> главный приз из-под носа у конкурентов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение от глагола \"kapen\" (угонять, перехватывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перехвати",
                                "target_lang_sense_clarification": "мяч/идею"
                            },
                            {
                                "target_lang_word": "уведи",
                                "target_lang_sense_clarification": "приз"
                            },
                            {
                                "target_lang_word": "угони",
                                "target_lang_sense_clarification": "транспорт"
                            }
                        ]
                    }
                }
            ]
        }
    ]

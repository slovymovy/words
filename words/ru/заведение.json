            "senses": [
                {
                    "sense_id": "ab96c3f2-2d4e-3a86-b56d-9fe7cc01227c",
                    "sense_definition": "Предприятие или учреждение, часто учебное, лечебное или развлекательное",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем городе открылось новое учебное <заведение> для одарённых детей.",
                            "target_lang_translations": {
                                "en": "A new educational <institution> for gifted children has opened in our city."
                            }
                        },
                        {
                            "text": "Мы решили поужинать в уютном <заведении> недалеко от центрального парка.",
                            "target_lang_translations": {
                                "en": "We decided to have dinner at a cozy <establishment> near the central park."
                            }
                        }
                    ],
                    "synonyms": [
                        "учреждение",
                        "предприятие"
                    ],
                    "common_phrases": [
                        "учебное заведение",
                        "высшее заведение",
                        "питейное заведение",
                        "игорное заведение"
                    ],
                    "target_lang_definitions": {
                        "en": "An organization, business, or public place, such as a school, hospital, restaurant, or club"
                    },
                    "translations": {
                        "en": [
                            {

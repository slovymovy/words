    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "5d899a3f-a562-3c40-9d2a-41f83b24ff94",
                    "sense_definition": "Forma żeńska zaimka <w>sam</w>, używana dla podkreślenia, że czynność wykonuje ta konkretna osoba bez niczyjej pomocy lub że mowa o tej właśnie osobie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sam_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia <w>sama</w> upiekła ten pyszny chleb.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка <w>сама</w> испекла этот вкусный хлеб."
                            }
                        },
                        {
                            "text": "Nie jestem pewna, czy <w>sama</w> poradzę sobie z tym zadaniem.",
                            "target_lang_translations": {
                                "ru": "Я не уверена, справлюсь ли я <w>сама</w> с этим заданием."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ona sama",
                        "sama nie wiem",
                        "zrobić coś sama"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rodzaj żeński"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская форма местоимения \"sam\", используемая для подчеркивания того, что действие выполняется конкретным лицом без посторонней помощи или что речь идет именно об этом лице."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сама",
                                "target_lang_sense_clarification": "самостоятельно, лично"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b88ffe4c-6486-3a1d-87e4-5719b89e5575",
                    "sense_definition": "Forma żeńska w mianowniku i wołaczu liczby pojedynczej od przymiotnika <w>sam</w>, oznaczająca stan bycia bez towarzystwa.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sam_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Anna nie lubi chodzić do kina <w>sama</w>.",
                            "target_lang_translations": {
                                "ru": "Анна не любит ходить в кино <w>одна</w>."
                            }
                        },
                        {
                            "text": "Po wyjeździe dzieci matka została w domu całkiem <w>sama</w>.",
                            "target_lang_translations": {
                                "ru": "После отъезда детей мать осталась в доме совсем <w>одна</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "razem",
                        "wspólnie"
                    ],
                    "common_phrases": [
                        "zupełnie sama",
                        "zostać sama",
                        "mieszkać sama"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "mianownik i wołacz liczby pojedynczej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская форма прилагательного \"sam\", означающая состояние без компании или присутствия других людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одна",
                                "target_lang_sense_clarification": "без компании, в одиночестве"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "be2bed41-8b2c-3ba5-bfb4-a7522c2567be",
                    "sense_definition": "Действительное причастие прошедшего времени от глагола <w>погибнуть</w>; обозначает того, кто умер насильственной или преждевременной смертью (в бою, катастрофе и т. п.).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "literal_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдат, <w>погибший</w> при исполнении служебного долга, был награждён посмертно.",
                            "target_lang_translations": {
                                "en": "The soldier <w>killed</w> in the line of duty was awarded posthumously."
                            }
                        },
                        {
                            "text": "В лесу был найден турист, <w>погибший</w> от сильного переохлаждения.",
                            "target_lang_translations": {
                                "en": "A tourist who <w>died</w> from severe hypothermia was found in the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "павший"
                    ],
                    "antonyms": [
                        "выживший"
                    ],
                    "common_phrases": [
                        "погибший в бою",
                        "погибший герой",
                        "погибший от ран",
                        "трагически погибший"
                    ],
                    "target_lang_definitions": {
                        "en": "Past active participle of the verb <w>погибнуть</w>; denotes someone who died a violent or premature death (in battle, a catastrophe, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "killed",
                                "target_lang_sense_clarification": "died as a result of violence or accident"
                            },
                            {
                                "target_lang_word": "perished",
                                "target_lang_sense_clarification": "died in a tragic or violent way (formal/literary)"
                            },
                            {
                                "target_lang_word": "died",
                                "target_lang_sense_clarification": "general term for ceasing to live"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dbdc04a3-5b42-3821-8e92-77cd4b926320",
                    "sense_definition": "Человек, который лишился жизни при трагических, неблагоприятных обстоятельствах (субстантивированное прилагательное).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "literal_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спасатели продолжают разбирать завалы и искать тела <w>погибших</w>.",
                            "target_lang_translations": {
                                "en": "Rescuers continue to clear the rubble and search for the bodies of the <w>dead</w>."
                            }
                        },
                        {
                            "text": "Родственникам <w>погибших</w> в авиакатастрофе была выплачена компенсация.",
                            "target_lang_translations": {
                                "en": "Compensation was paid to the relatives of the <w>victims</w> of the plane crash."
                            }
                        }
                    ],
                    "synonyms": [
                        "павший"
                    ],
                    "antonyms": [
                        "выживший"
                    ],
                    "common_phrases": [
                        "список погибших",
                        "число погибших",
                        "память погибших",
                        "семьи погибших"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who lost their life under tragic, unfavorable circumstances (substantivized adjective)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "victim",

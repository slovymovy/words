    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7524283a-0f5d-325c-b9d3-a7eb20ffe14c",
                    "sense_definition": "Een godslasterende uiting of ruw woord dat men roept bij schrik, pijn of grote ontevredenheid",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "profanity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik slaakte een knetterende <w>vloek</w> toen ik met de hamer op mijn duim sloeg.",
                            "target_lang_translations": {
                                "en": "I let out a loud <w>curse</w> when I hit my thumb with the hammer.",
                                "ru": "Я выкрикнул отборное <w>ругательство</w>, когда ударил молотком по большому пальцу."
                            }
                        },
                        {
                            "text": "In de andere kamer hoorde hij een hoop gehijg, gestommel en af een toe een <w>vloek</w>.",
                            "target_lang_translations": {
                                "en": "In the other room he heard a lot of panting, stumbling, and occasionally a <w>curse</w>.",
                                "ru": "Из другой комнаты он слышал много пыхтения, грохота и время от времени <w>ругательство</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "krachtterm"
                    ],
                    "common_phrases": [
                        "een vloek slaken",
                        "een vloek onderdrukken",
                        "een hardgrondige vloek"
                    ],
                    "target_lang_definitions": {
                        "en": "A profane word or expression shouted in anger, pain, or shock",
                        "ru": "Богохульство или грубое слово, выкрикиваемое от испуга, боли или сильного недовольства"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "curse",
                                "target_lang_sense_clarification": "profane word"
                            },
                            {
                                "target_lang_word": "swear word",
                                "target_lang_sense_clarification": "offensive language"
                            },
                            {
                                "target_lang_word": "profanity",
                                "target_lang_sense_clarification": "blasphemous language"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ругательство",
                                "target_lang_sense_clarification": "бранное слово"
                            },
                            {
                                "target_lang_word": "брань",
                                "target_lang_sense_clarification": "сквернословие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c692d63-8fb0-329b-bf1c-8b50b8ddefed",
                    "sense_definition": "Een bewust uitgesproken wens of magische spreuk bedoeld om iemand kwaad of leed aan te doen",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "magic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er lijkt een mysterieuze <w>vloek</w> te rusten op dat oude, verlaten huis.",
                            "target_lang_translations": {
                                "en": "A mysterious <w>curse</w> seems to rest on that old, abandoned house.",
                                "ru": "Кажется, на этом старом заброшенном доме лежит таинственное <w>проклятие</w>."
                            }
                        },
                        {
                            "text": "In het sprookje sprak de boze heks een vreselijke <w>vloek</w> uit over de prinses.",
                            "target_lang_translations": {
                                "en": "In the fairy tale, the wicked witch cast a terrible <w>curse</w> on the princess.",
                                "ru": "В сказке злая ведьма наложила страшное <w>проклятие</w> на принцессу."
                            }
                        }
                    ],
                    "antonyms": [
                        "zegen"
                    ],
                    "common_phrases": [
                        "een vloek uitspreken",
                        "de vloek verbreken",
                        "rust een vloek op"
                    ],
                    "target_lang_definitions": {
                        "en": "A magic spell intended to bring bad luck or harm to someone",
                        "ru": "Осознанно высказанное пожелание или магическое заклинание, призванное причинить кому-либо зло или страдания"
                    },
                    "translations": {

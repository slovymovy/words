        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "be9d828b-be77-3516-aafa-3d56b445034f",
                    "sense_definition": "Długa, ocieplana kurtka z kapturem, często obszytym futrem, chroniąca przed wiatrem i zimnem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "jacket",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zimowe spacery najlepiej sprawdza się ciepła <w>parka</w>.",
                            "target_lang_translations": {
                                "ru": "Для зимних прогулок лучше всего подходит теплая <w>парка</w>."
                            }
                        },
                        {
                            "text": "Kupiłam nową, zieloną <w>parkę</w>, która świetnie chroni przed deszczem.",
                            "target_lang_translations": {
                                "ru": "Я купила новую зеленую <w>парку</w>, которая отлично защищает от дождя."
                            }
                        }
                    ],
                    "synonyms": [
                        "krew"
                    ],
                    "common_phrases": [
                        "ciepła parka",
                        "zielona parka",
                        "parka z kapturem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная утепленная куртка с капюшоном, часто отороченным мехом, защищающая от ветра и холода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парка",
                                "target_lang_sense_clarification": "куртка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d89ccc5-1e2b-3cc3-a9c2-fb15a3a171d0",
                    "sense_definition": "Zdrobnienie od słowa <w>para</w> (dwoje ludzi lub zwierząt); często używane pieszczotliwie w odniesieniu do zakochanych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "diminutive_pair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W parku na ławce siedziała zakochana <w>parka</w>.",
                            "target_lang_translations": {
                                "ru": "В парке на скамейке сидела влюбленная <w>парочка</w>."
                            }
                        },
                        {
                            "text": "To była bardzo ładna <w>parka</w>, która zawsze trzymała się za ręce.",
                            "target_lang_translations": {
                                "ru": "Это была очень красивая <w>парочка</w>, которая всегда держалась за руки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zakochana parka",
                        "ładna parka",
                        "młoda parka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова «пара» (двое людей или животных); часто используется ласкательно по отношению к влюбленным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парочка",
                                "target_lang_sense_clarification": "влюбленные"
                            }
                        ]

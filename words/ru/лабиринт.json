    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9059ad34-b15c-31ab-b7ce-6d6761cf2f88",
                    "sense_definition": "Разновидность игры-головоломки, в которой нужно найти правильный путь от входа до выхода.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "games_and_puzzles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок с интересом решал сложный <w>лабиринт</w> в детском журнале.",
                            "target_lang_translations": {
                                "en": "The child was solving a complex <w>maze</w> in a children's magazine with interest."
                            }
                        },
                        {
                            "text": "На уроке мы рисовали <w>лабиринт</w> для своих друзей.",
                            "target_lang_translations": {
                                "en": "In class, we drew a <w>maze</w> for our friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "головоломка"
                    ],
                    "common_phrases": [
                        "пройти лабиринт",
                        "нарисованный лабиринт",
                        "детский лабиринт"
                    ],
                    "target_lang_definitions": {
                        "en": "A type of puzzle game where one must find the correct path from the entrance to the exit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "maze",
                                "target_lang_sense_clarification": "puzzle on paper or physical game"
                            },
                            {
                                "target_lang_word": "labyrinth",
                                "target_lang_sense_clarification": "puzzle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f0fe555-2ba9-3a77-9887-b1c543e96d9b",
                    "sense_definition": "Сооружение, здание или часть парка с очень запутанным планом и множеством ходов, где легко потеряться.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы долго не могли найти выход из зелёного <w>лабиринта</w> в парке.",
                            "target_lang_translations": {
                                "en": "The tourists couldn't find the exit from the green <w>maze</w> in the park for a long time."
                            }
                        },
                        {
                            "text": "Древние мастера построили этот <w>лабиринт</w> много веков назад.",
                            "target_lang_translations": {
                                "en": "Ancient masters built this <w>labyrinth</w> many centuries ago."
                            }
                        }
                    ],
                    "common_phrases": [
                        "запутанный лабиринт",
                        "выход из лабиринта",
                        "зеркальный лабиринт",
                        "подземный лабиринт"
                    ],
                    "target_lang_definitions": {
                        "en": "A structure, building, or part of a park with a very confusing layout and many passages where it is easy to get lost."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "maze",
                                "target_lang_sense_clarification": "confusing structure, often made of hedges"
                            },
                            {
                                "target_lang_word": "labyrinth",
                                "target_lang_sense_clarification": "complex structure, often historical or unicursal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc2c4a1d-e7c3-3f5a-9b87-03817b018f53",
                    "sense_definition": "Сложное, беспорядочное переплетение чего-либо (улиц, коридоров, ходов), напоминающее запутанное сооружение.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы заблудились в <w>лабиринте</w> узких городских улочек.",
                            "target_lang_translations": {
                                "en": "We got lost in the <w>labyrinth</w> of narrow city streets."
                            }
                        },
                        {
                            "text": "Пещера представляла собой настоящий <w>лабиринт</w> тёмных подземных ходов.",
                            "target_lang_translations": {
                                "en": "The cave was a real <w>labyrinth</w> of dark underground passages."
                            }
                        }
                    ],
                    "synonyms": [
                        "сеть",
                        "хитросплетение"
                    ],
                    "common_phrases": [
                        "лабиринт улиц",
                        "лабиринт коридоров",
                        "блуждать по лабиринту"
                    ],
                    "target_lang_definitions": {
                        "en": "A complex, disorderly network of something (streets, corridors, passages) resembling a confusing structure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "labyrinth",
                                "target_lang_sense_clarification": "complex network of paths"
                            },
                            {
                                "target_lang_word": "maze",
                                "target_lang_sense_clarification": "confusing network"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6317d1b2-25dc-3c1c-ada0-f00a6de003b4",
                    "sense_definition": "Сложная, запутанная ситуация, отношения или ход мыслей, в которых трудно разобраться.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_complexity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он не мог найти выход из <w>лабиринта</w> собственных сомнений.",
                            "target_lang_translations": {
                                "en": "He could not find a way out of the <w>labyrinth</w> of his own doubts."
                            }
                        },
                        {
                            "text": "Мысли её бродили в каком-то безвыходном <w>лабиринте</w>.",
                            "target_lang_translations": {
                                "en": "Her thoughts wandered in some inescapable <w>labyrinth</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "путаница",
                        "дебри",
                        "неразбериха"
                    ],
                    "antonyms": [
                        "ясность"
                    ],
                    "common_phrases": [
                        "лабиринт мыслей",
                        "лабиринт лжи",
                        "нравственный лабиринт"
                    ],
                    "target_lang_definitions": {
                        "en": "A complex, confusing situation, relationship, or train of thought that is difficult to sort out."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "labyrinth",
                                "target_lang_sense_clarification": "abstract complexity"
                            },
                            {
                                "target_lang_word": "maze",
                                "target_lang_sense_clarification": "state of confusion or complexity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69cf6afa-50f0-3e25-82af-b0269560e16a",
                    "sense_definition": "Внутренняя часть уха у позвоночных животных и человека, состоящая из улитки и каналов сложной формы.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач внимательно изучил строение <w>лабиринта</w> внутреннего уха пациента.",
                            "target_lang_translations": {
                                "en": "The doctor carefully studied the structure of the <w>labyrinth</w> of the patient's inner ear."
                            }
                        },
                        {
                            "text": "Воспаление <w>лабиринта</w> может привести к серьёзным проблемам со слухом.",
                            "target_lang_translations": {
                                "en": "Inflammation of the <w>labyrinth</w> can lead to serious hearing problems."
                            }
                        }
                    ],
                    "common_phrases": [
                        "костный лабиринт",
                        "перепончатый лабиринт"
                    ],
                    "target_lang_definitions": {
                        "en": "The inner part of the ear in vertebrates and humans, consisting of the cochlea and complex canals."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "labyrinth",
                                "target_lang_sense_clarification": "anatomy"
                            }
                        ]
                    }
                }
            ]
        }

                    "sense_id": "39f885d0-1be7-364b-ab92-c277f2f8b3f4",
                    "sense_definition": "Lasting longer than usual or expected; protracted.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to go on an <w>extended</w> holiday this summer.",
                            "target_lang_translations": {
                                "ru": "Мы решили устроить себе <w>длительный</w> отпуск этим летом."
                            }
                        },
                        {
                            "text": "The shop offers an <w>extended</w> warranty on all electronics.",
                            "target_lang_translations": {
                                "ru": "Магазин предлагает <w>расширенную</w> гарантию на всю электронику."
                            }
                        }
                    ],
                    "synonyms": [
                        "prolonged",
                        "lengthened",
                        "long"
                    ],
                    "antonyms": [
                        "brief",
                        "short",
                        "shortened"
                    ],
                    "common_phrases": [
                        "extended warranty",
                        "extended period",
                        "extended stay",
                        "extended deadline"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длившийся дольше обычного или ожидаемого; затяжной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продолжительный",
                                "target_lang_sense_clarification": "длящийся долгое время"
                            },
                            {
                                "target_lang_word": "продлённый",

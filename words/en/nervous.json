            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8a23da8b-6e7d-3cda-adc3-ab4483713087",
                    "sense_definition": "Feeling worry, fear, or anxiety about something that might happen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I always get <w>nervous</w> before speaking in front of a large crowd.",
                            "target_lang_translations": {
                                "ru": "Я всегда <w>нервничаю</w> перед выступлением перед большой толпой."
                            }
                        },
                        {
                            "text": "She felt <w>nervous</w> about the results of her medical test.",
                            "target_lang_translations": {
                                "ru": "Она <w>нервничала</w> по поводу результатов своего медицинского обследования."
                            }
                        }
                    ],
                    "synonyms": [
                        "anxious",
                        "worried",
                        "apprehensive",
                        "uneasy",
                        "concerned"
                    ],
                    "antonyms": [
                        "calm",
                        "relaxed",
                        "unworried",
                        "unperturbed"
                    ],
                    "common_phrases": [
                        "feel nervous",
                        "nervous about",
                        "get nervous",
                        "nervous wreck"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий беспокойство, страх или тревогу по поводу того, что может произойти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взволнованный",
                                "target_lang_sense_clarification": "испытывающий волнение или тревогу"
                            },
                            {
                                "target_lang_word": "нервный",
                                "target_lang_sense_clarification": "находящийся в состоянии напряжения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "893785d9-f1e6-3965-bee4-8b85936341f9",
                    "sense_definition": "Easily excited, alarmed, or upset; having a tendency to be anxious or timid.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He gave a <w>nervous</w> laugh when he realized he had made a mistake.",
                            "target_lang_translations": {
                                "ru": "Он издал <w>нервный</w> смешок, когда понял, что совершил ошибку."
                            }
                        },
                        {
                            "text": "The <w>nervous</w> horse jumped when the car honked its horn.",
                            "target_lang_translations": {
                                "ru": "<w>Пугливая</w> лошадь подпрыгнула, когда машина посигналила."
                            }
                        }
                    ],
                    "synonyms": [
                        "jumpy",
                        "edgy",
                        "high-strung",
                        "skittish",
                        "jittery",
                        "excitable"
                    ],
                    "antonyms": [
                        "calm",
                        "relaxed"
                    ],
                    "common_phrases": [
                        "nervous energy",
                        "nervous smile",
                        "nervous laugh",
                        "nervous disposition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легко возбудимый, встревоженный или расстроенный; склонный к беспокойству или робости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нервный",
                                "target_lang_sense_clarification": "легко возбудимый, раздражительный"
                            },
                            {
                                "target_lang_word": "беспокойный",
                                "target_lang_sense_clarification": "суетливый, не находящий покоя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6db1d31f-7009-3ef0-96a3-fad7eb400da3",
                    "sense_definition": "Relating to the system of nerves in the body that carries messages to and from the brain.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The brain and spinal cord make up the central <w>nervous</w> system.",
                            "target_lang_translations": {
                                "ru": "Головной и спинной мозг составляют центральную <w>нервную</w> систему."
                            }
                        },
                        {
                            "text": "Too much stress can sometimes lead to a <w>nervous</w> breakdown.",
                            "target_lang_translations": {
                                "ru": "Слишком сильный стресс иногда может привести к <w>нервному</w> срыву."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nervous system",
                        "central nervous system",
                        "nervous breakdown"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к системе нервов в организме, которая передает сигналы к мозгу и от него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нервный",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53b9bd47-45f7-36d6-83c3-bcdcbd4ca0ac",
                    "sense_definition": "Of a literary style or writing: forceful, powerful, and concise.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author was known for his <w>nervous</w> style that conveyed deep emotion with few words.",
                            "target_lang_translations": {
                                "ru": "Автор был известен своим <w>энергичным</w> стилем, который передавал глубокие эмоции немногими словами."
                            }
                        },
                        {
                            "text": "Her speech was <w>nervous</w> and striking, capturing the attention of the entire audience.",
                            "target_lang_translations": {
                                "ru": "Ее речь была <w>экспрессивной</w> и поразительной, захватив внимание всей аудитории."
                            }
                        }
                    ],
                    "traits": [
                        {

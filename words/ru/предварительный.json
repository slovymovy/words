            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "34953510-a53b-3e9b-a6ff-83297956b4b7",
                    "sense_definition": "Предшествующий чему-либо основному, главному; совершаемый в качестве подготовки.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спортсмены успешно прошли <w>предварительный</w> этап соревнований.",
                            "target_lang_translations": {
                                "en": "The athletes successfully passed the <w>preliminary</w> stage of the competition."
                            }
                        },
                        {
                            "text": "Без <w>предварительной</w> подготовки трудно сдать этот сложный экзамен.",
                            "target_lang_translations": {
                                "en": "It is difficult to pass this complex exam without <w>preliminary</w> preparation."
                            }
                        }
                    ],
                    "synonyms": [
                        "подготовительный",
                        "вступительный"
                    ],
                    "antonyms": [
                        "последующий"
                    ],
                    "common_phrases": [
                        "предварительный этап",
                        "предварительное следствие",
                        "предварительный просмотр",
                        "предварительная подготовка"
                    ],
                    "target_lang_definitions": {
                        "en": "Preceding the main event or action; done as preparation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "preliminary",
                                "target_lang_sense_clarification": "happening before a more important action or event"
                            },
                            {
                                "target_lang_word": "preparatory",
                                "target_lang_sense_clarification": "done in order to get ready for something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc3050e3-5f8e-3e9c-85e9-e18217f0c75d",
                    "sense_definition": "Неокончательный; такой, который может быть изменён, уточнён или пересмотрен.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По <w>предварительным</w> данным, прибыль компании выросла в два раза.",
                            "target_lang_translations": {
                                "en": "According to <w>preliminary</w> data, the company's profit has doubled."
                            }
                        },
                        {
                            "text": "Это лишь <w>предварительное</w> решение, окончательный ответ будет завтра.",
                            "target_lang_translations": {
                                "en": "This is only a <w>preliminary</w> decision; the final answer will be tomorrow."
                            }
                        }
                    ],
                    "synonyms": [
                        "неокончательный"
                    ],
                    "antonyms": [
                        "окончательный"
                    ],
                    "common_phrases": [
                        "предварительные данные",
                        "предварительные результаты",
                        "предварительное соглашение",
                        "предварительная оценка"
                    ],
                    "target_lang_definitions": {
                        "en": "Not final; subject to change, clarification, or revision."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "preliminary",
                                "target_lang_sense_clarification": "denoting an initial version before the final one"
                            },
                            {
                                "target_lang_word": "tentative",
                                "target_lang_sense_clarification": "not certain or fixed; provisional"
                            },
                            {
                                "target_lang_word": "provisional",
                                "target_lang_sense_clarification": "arranged for the present time only"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a309c22-3d0a-3a64-9869-e5c967bb819b",
                    "sense_definition": "Осуществляемый, сделанный или подготовленный заранее, раньше установленного срока.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом популярном ресторане требуется <w>предварительный</w> заказ столика.",
                            "target_lang_translations": {
                                "en": "<w>Advance</w> table reservation is required at this popular restaurant."

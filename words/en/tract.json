    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e142ba4-35ad-3e64-92a4-2feee770870c",
                    "sense_definition": "A system of connected organs in the body that perform a specific function.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor checked the patient's digestive <w>tract</w> for any signs of illness.",
                            "target_lang_translations": {
                                "ru": "Врач проверил пищеварительный <w>тракт</w> пациента на наличие признаков заболевания."
                            }
                        },
                        {
                            "text": "An infection in the urinary <w>tract</w> can be very uncomfortable and requires treatment.",
                            "target_lang_translations": {
                                "ru": "Инфекция в мочевыводящем <w>тракте</w> может доставлять сильный дискомфорт и требует лечения."
                            }
                        }
                    ],
                    "synonyms": [
                        "system"
                    ],
                    "common_phrases": [
                        "digestive tract",
                        "respiratory tract",
                        "urinary tract"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система связанных органов в организме, выполняющих определенную функцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тракт",
                                "target_lang_sense_clarification": "анатомическая система"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "395fceee-de80-3c94-87fe-f9b7304f8673",
                    "sense_definition": "A large area or expanse of land or water.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wealthy investor bought a vast <w>tract</w> of land to build a new resort.",
                            "target_lang_translations": {
                                "ru": "Богатый инвестор купил обширный <w>участок</w> земли для строительства нового курорта."
                            }
                        },
                        {
                            "text": "We hiked through a dense <w>tract</w> of forest that had never been explored.",
                            "target_lang_translations": {
                                "ru": "Мы шли через густой <w>участок</w> леса, который никогда не был исследован."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tract of land",
                        "vast tract",
                        "tract of forest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая область или пространство земли или воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "часть территории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fec04be-0131-3979-889c-09ed178d6737",
                    "sense_definition": "A short booklet or essay, usually on a religious or political subject.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "publishing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He stood on the corner handing out a religious <w>tract</w> to passersby.",
                            "target_lang_translations": {
                                "ru": "Он стоял на углу, раздавая прохожим религиозную <w>брошюру</w>."
                            }
                        },
                        {
                            "text": "The author published a controversial political <w>tract</w> criticizing the government.",
                            "target_lang_translations": {
                                "ru": "Автор опубликовал скандальный политический <w>трактат</w> с критикой правительства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "religious tract",
                        "political tract"
                    ],
                    "target_lang_definitions": {

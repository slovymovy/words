    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "533fc51f-e801-38a4-8cf9-876e76856aff",
                    "sense_definition": "An attitude of consideration, admiration, or high regard for someone or something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_admiration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She earned the <w>respect</w> of her colleagues through her hard work and dedication.",
                            "target_lang_translations": {
                                "ru": "Она заслужила <w>уважение</w> коллег своим упорным трудом и преданностью делу."
                            }
                        },
                        {
                            "text": "We should treat our elders with <w>respect</w> and listen to their advice.",
                            "target_lang_translations": {
                                "ru": "Мы должны относиться к старшим с <w>уважением</w> и прислушиваться к их советам."
                            }
                        }
                    ],
                    "synonyms": [
                        "admiration",
                        "esteem",
                        "regard",
                        "reverence",
                        "honor",
                        "deference"
                    ],
                    "antonyms": [
                        "contempt",
                        "disrespect",
                        "scorn",
                        "disdain"
                    ],
                    "common_phrases": [
                        "show respect",
                        "have respect for",
                        "with all due respect",
                        "mutual respect",
                        "deep respect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство глубокого признания достоинств, заслуг или качеств кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уважение",
                                "target_lang_sense_clarification": "признание достоинств"
                            },
                            {
                                "target_lang_word": "почтение",
                                "target_lang_sense_clarification": "глубокое уважение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79f2c6d4-78c1-3a82-924f-eab830c0ccfe",
                    "sense_definition": "A particular aspect, feature, or detail of something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_aspect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new plan is better than the old one in one important <w>respect</w>.",
                            "target_lang_translations": {
                                "ru": "Новый план лучше старого в одном важном <w>отношении</w>."
                            }
                        },
                        {
                            "text": "In many <w>respects</w>, the two sisters are very similar in personality.",
                            "target_lang_translations": {
                                "ru": "Во многих <w>отношениях</w> две сестры очень похожи по характеру."
                            }
                        }
                    ],
                    "synonyms": [
                        "aspect",
                        "facet",
                        "side",
                        "dimension",
                        "reverence",
                        "regard"
                    ],
                    "common_phrases": [
                        "in this respect",
                        "in every respect",
                        "in some respects",
                        "in many respects"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенная сторона, деталь или особенность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отношение",
                                "target_lang_sense_clarification": "в контексте «в этом отношении»"
                            },
                            {
                                "target_lang_word": "аспект",
                                "target_lang_sense_clarification": "сторона дела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0971a473-8b4f-37cc-9b58-5f03ea4fd048",
                    "sense_definition": "Polite greetings, often offered as condolences after a death.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_greetings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went to the funeral to pay our last <w>respects</w> to the deceased poet.",
                            "target_lang_translations": {
                                "ru": "Мы пошли на похороны, чтобы отдать дань <w>уважения</w> покойному поэту."
                            }
                        },
                        {
                            "text": "Please give my <w>respects</w> to your parents when you see them.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, передай мое <w>почтение</w> своим родителям, когда увидишь их."
                            }
                        }
                    ],
                    "synonyms": [
                        "regard"
                    ],
                    "common_phrases": [
                        "pay one's respects",
                        "last respects",
                        "give one's respects"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливые приветствия, часто выражаемые как соболезнования или знак внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почтение",
                                "target_lang_sense_clarification": "формальное приветствие"
                            },
                            {
                                "target_lang_word": "уважение",
                                "target_lang_sense_clarification": "в выражении «дань уважения»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "54fc8901-5356-34e5-abe4-428ecbeb471a",
                    "sense_definition": "To feel or show admiration and high regard for someone or something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_admire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>respect</w> him greatly for his honesty and courage.",
                            "target_lang_translations": {
                                "ru": "Я глубоко <w>уважаю</w> его за честность и смелость."
                            }
                        },
                        {
                            "text": "Students must learn to <w>respect</w> their teachers and classmates.",
                            "target_lang_translations": {
                                "ru": "Ученики должны научиться <w>уважать</w> своих учителей и одноклассников."
                            }
                        }
                    ],
                    "synonyms": [
                        "admiration",
                        "esteem",
                        "honor",
                        "revere",
                        "value",
                        "venerate"
                    ],
                    "antonyms": [
                        "despise",
                        "disrespect",
                        "scorn",
                        "dis",
                        "diss"
                    ],
                    "common_phrases": [
                        "respect deeply",
                        "respect highly",
                        "respect oneself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать или проявлять чувство уважения и восхищения кем-либо или чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уважать",
                                "target_lang_sense_clarification": "ценить за качества"
                            },
                            {
                                "target_lang_word": "почитать",
                                "target_lang_sense_clarification": "относиться с благоговением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "848126c2-199d-38ad-8dab-98c7aabd8b64",
                    "sense_definition": "To agree to recognize and abide by a rule, law, right, or custom.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_obey",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Everyone needs to <w>respect</w> the rules of the game for it to be fair.",
                            "target_lang_translations": {
                                "ru": "Все должны <w>соблюдать</w> правила игры, чтобы она была честной."
                            }
                        },
                        {
                            "text": "Please <w>respect</w> my privacy and do not ask personal questions right now.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>уважайте</w> мою личную жизнь и не задавайте сейчас личных вопросов."
                            }
                        }
                    ],
                    "synonyms": [
                        "honor",
                        "regard",
                        "value"
                    ],
                    "antonyms": [
                        "ignore",
                        "neglect",
                        "slight",
                        "disrespect",
                        "dis"
                    ],
                    "common_phrases": [
                        "respect the law",
                        "respect boundaries",
                        "respect a decision",
                        "respect privacy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Признавать и соблюдать правила, законы, права или обычаи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соблюдать",
                                "target_lang_sense_clarification": "следовать правилам"
                            },
                            {
                                "target_lang_word": "уважать",
                                "target_lang_sense_clarification": "не нарушать права"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7feb0ce9-d6bf-3dc5-b9ee-52aee597ba51",
                    "sense_definition": "Used as a greeting or to express approval.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "intj_greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Respect</w>, man, that was an amazing performance!",
                            "target_lang_translations": {
                                "ru": "<w>Респект</w>, чувак, это было потрясающее выступление!"
                            }
                        },
                        {
                            "text": "I saw him on the street and just said <w>respect</w>.",
                            "target_lang_translations": {
                                "ru": "Я увидел его на улице и просто сказал: «<w>Уважуха</w>»."
                            }

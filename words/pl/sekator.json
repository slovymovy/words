    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6fe557f9-0a59-3ed9-94db-004f680b7993",
                    "sense_definition": "Narzędzie ogrodnicze przypominające nożyce, służące do przycinania gałęzi drzew, krzewów i innych roślin.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Weź <w>sekator</w> i podetnij te gałęzie, które wchodzą na posesję sąsiadów.",
                            "target_lang_translations": {
                                "ru": "Возьми <w>секатор</w> и подрежь те ветки, которые свисают на участок соседей."
                            }
                        },
                        {
                            "text": "Ogrodnik naostrzył swój stary <w>sekator</w>, aby łatwiej przycinać róże.",
                            "target_lang_translations": {
                                "ru": "Садовник наточил свой старый <w>секатор</w>, чтобы было легче обрезать розы."
                            }
                        }
                    ],
                    "synonyms": [
                        "nożyce ogrodowe"
                    ],
                    "common_phrases": [
                        "ostry sekator",
                        "ciąć sekatorem",
                        "sekator ręczny",
                        "sekator do gałęzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Садовый инструмент, напоминающий ножницы, предназначенный для обрезки веток деревьев, кустарников и других растений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секатор",
                                "target_lang_sense_clarification": "садовый инструмент"
                            },
                            {
                                "target_lang_word": "садовые ножницы",
                                "target_lang_sense_clarification": "инструмент для обрезки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d427b39-6794-3d80-af24-4252b937bfdf",
                    "sense_definition": "Potoczne określenie roweru marki Sector, używane w środowisku kolarskim.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "bike_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na dzisiejszy trening zabrałem mój nowy <w>sekator</w>, który świetnie radzi sobie w terenie.",
                            "target_lang_translations": {
                                "ru": "На сегодняшнюю тренировку я взял свой новый <w>секатор</w>, который отлично справляется с бездорожьем."
                            }
                        },
                        {
                            "text": "Wielu zawodników chwali ten model, twierdząc, że to niezawodny <w>sekator</w>.",
                            "target_lang_translations": {
                                "ru": "Многие гонщики хвалят эту модель, утверждая, что это надежный <w>секатор</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "środowiskowe (kolarstwo)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название велосипеда марки Sector, используемое в среде велосипедистов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секатор",
                                "target_lang_sense_clarification": "велосипед марки Sector (сленг)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

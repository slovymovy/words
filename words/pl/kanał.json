    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "572c55af-5afb-3221-9309-989b17896ab7",
                    "sense_definition": "Jeden z programów telewizyjnych lub stacja nadająca audycje, wybierana w odbiorniku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "media_channel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój ulubiony <kanał> nadaje filmy przyrodnicze przez całą dobę.",
                            "target_lang_translations": {
                                "ru": "Мой любимый <канал> показывает фильмы о природе круглосуточно."
                            }
                        },
                        {
                            "text": "Zmień <kanał>, bo ten serial jest bardzo nudny.",
                            "target_lang_translations": {
                                "ru": "Переключи <канал>, этот сериал очень скучный."
                            }
                        }
                    ],
                    "synonyms": [
                        "podscenie"
                    ],
                    "common_phrases": [
                        "zmienić kanał",
                        "kanał sportowy",
                        "kanał informacyjny",
                        "kanał telewizyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Телевизионная программа или станция вещания, выбираемая на приемнике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канал",
                                "target_lang_sense_clarification": "телевизионный"
                            },
                            {
                                "target_lang_word": "телеканал",
                                "target_lang_sense_clarification": "станция вещания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a97648a1-a9a7-3da7-95f5-2cfd8425ee73",
                    "sense_definition": "Sztuczny ciek wodny wykonany przez człowieka, łączący zbiorniki wodne, służący do żeglugi lub nawadniania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "water_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Statki towarowe płyną przez <kanał>, aby skrócić drogę do portu.",
                            "target_lang_translations": {
                                "ru": "Грузовые суда плывут через <канал>, чтобы сократить путь в порт."
                            }
                        },
                        {
                            "text": "Wzdłuż <kanału> biegnie malownicza ścieżka rowerowa.",
                            "target_lang_translations": {
                                "ru": "Вдоль <канала> проходит живописная велосипедная дорожка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Kanał Sueski",
                        "kanał żeglugowy",
                        "budowa kanału",
                        "kanał nawadniający"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственное русло, наполненное водой, соединяющее водоемы и служащее для судоходства или орошения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канал",
                                "target_lang_sense_clarification": "водный путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d916985-2cc9-3110-b3b9-f116ccea5d63",
                    "sense_definition": "Podziemna instalacja lub rura odprowadzająca ścieki i wodę deszczową.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "infrastructure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielkiej ulewie woda spływała prosto w <kanał> burzowy.",
                            "target_lang_translations": {
                                "ru": "После сильного ливня вода стекала прямо в ливневый <канал>."
                            }
                        },
                        {
                            "text": "Szczury w wielkich miastach często zamieszkują podziemne <kanały>.",
                            "target_lang_translations": {
                                "ru": "Крысы в больших городах часто обитают в подземных <каналах>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wpaść do kanału",
                        "kanał ściekowy",
                        "kanał burzowy",
                        "studzienka kanału"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подземная система или труба для отвода сточных и ливневых вод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канализация",
                                "target_lang_sense_clarification": "система"
                            },
                            {
                                "target_lang_word": "коллектор",
                                "target_lang_sense_clarification": "большая труба"
                            },
                            {
                                "target_lang_word": "канал",
                                "target_lang_sense_clarification": "сток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "252736e7-6daf-368a-ac0d-84bede377f77",
                    "sense_definition": "Droga lub sposób przekazywania informacji między nadawcą a odbiorcą.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy znaleźć bezpieczny <kanał> do przesyłania tajnych danych.",
                            "target_lang_translations": {
                                "ru": "Мы должны найти безопасный <канал> для передачи секретных данных."
                            }
                        },
                        {
                            "text": "Rząd wykorzystał oficjalne <kanały> dyplomatyczne, by załagodzić konflikt.",
                            "target_lang_translations": {
                                "ru": "Правительство использовало официальные дипломатические <каналы>, чтобы уладить конфликт."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kanał komunikacji",
                        "kanał dyplomatyczny",
                        "oficjalny kanał",
                        "kanał przerzutowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ или путь передачи информации между отправителем и получателем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канал",
                                "target_lang_sense_clarification": "связи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53982a91-2a5d-385b-b25d-57f4f8199914",
                    "sense_definition": "Bardzo trudna, nieprzyjemna sytuacja lub całkowite niepowodzenie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "slang_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zapomniałem paszportu na lotnisko, ale <kanał>!",
                            "target_lang_translations": {
                                "ru": "Я забыл паспорт в аэропорт, вот это <облом>!"
                            }
                        },
                        {
                            "text": "To był straszny <kanał>, kiedy zepsuł nam się samochód na środku pustkowia.",
                            "target_lang_translations": {
                                "ru": "Это была жуткая <засада>, когда у нас сломалась машина посреди глуши."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ale kanał",
                        "totalny kanał",
                        "niezły kanał"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to express frustration about a bad situation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень трудная, неприятная ситуация или полная неудача."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облом",
                                "target_lang_sense_clarification": "неудача (разг.)"
                            },
                            {
                                "target_lang_word": "засада",
                                "target_lang_sense_clarification": "неприятная ситуация (разг.)"
                            },
                            {
                                "target_lang_word": "труба",
                                "target_lang_sense_clarification": "плохое дело (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d0d8a94-a350-39f4-8a78-a436c424a8e8",
                    "sense_definition": "Przewód w budynku służący do wymiany powietrza lub odprowadzania dymu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "infrastructure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Główny <kanał> wentylacyjny w kuchni jest chyba zatkany.",
                            "target_lang_translations": {
                                "ru": "Главный вентиляционный <канал> на кухне, кажется, засорился."
                            }
                        },
                        {
                            "text": "Dym z kominka wydostaje się na zewnątrz przez <kanał> kominowy.",
                            "target_lang_translations": {
                                "ru": "Дым из камина выходит наружу через дымовой <канал>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kanał wentylacyjny",
                        "kanał dymowy",
                        "drożność kanału"
                    ],
                    "target_lang_definitions": {
                        "ru": "Труба или шахта в здании для вентиляции или отвода дыма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канал",
                                "target_lang_sense_clarification": "вентиляционный/дымовой"
                            },
                            {
                                "target_lang_word": "воздуховод",
                                "target_lang_sense_clarification": "труба для воздуха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b197a3ef-42de-3ffe-9b64-a7d9a8bc4f8f",
                    "sense_definition": "Podłużne wgłębienie w podłodze garażu lub warsztatu, umożliwiające naprawę samochodu od spodu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "automotive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mechanik wjechał autem na <kanał>, żeby sprawdzić stan zawieszenia.",
                            "target_lang_translations": {
                                "ru": "Механик заехал на машине на <яму>, чтобы проверить состояние подвески."
                            }
                        },
                        {
                            "text": "Mój garaż jest wyposażony w <kanał>, więc sam wymieniam olej.",
                            "target_lang_translations": {
                                "ru": "В моем гараже есть <смотровая яма>, так что я сам меняю масло."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wjechać na kanał",
                        "naprawa na kanale",
                        "garaż z kanałem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Углубление в полу гаража или мастерской для ремонта автомобиля снизу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотровая яма",
                                "target_lang_sense_clarification": "в гараже"
                            },
                            {
                                "target_lang_word": "яма",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f46ab7ea-0b48-3b63-8166-0e797e729b0a",
                    "sense_definition": "Obniżone miejsce w teatrze lub operze, znajdujące się przed sceną, przeznaczone dla orkiestry.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "theater",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muzycy stroili instrumenty w <kanale> przed rozpoczęciem spektaklu.",
                            "target_lang_translations": {
                                "ru": "Музыканты настраивали инструменты в <оркестровой яме> перед началом спектакля."
                            }
                        },
                        {
                            "text": "Z widowni nie widać muzyków ukrytych w głębokim <kanale>.",
                            "target_lang_translations": {
                                "ru": "Из зрительного зала не видно музыкантов, скрытых в глубокой <оркестровой яме>."
                            }
                        }
                    ],
                    "synonyms": [
                        "fosa orkiestrowa",
                        "orkiestron"
                    ],
                    "common_phrases": [
                        "kanał orkiestrowy",
                        "siedzieć w kanale"
                    ],
                    "target_lang_definitions": {
                        "ru": "Углубленное место перед сценой в театре или опере, предназначенное для оркестра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оркестровая яма",
                                "target_lang_sense_clarification": "в театре"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
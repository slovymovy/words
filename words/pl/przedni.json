            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "383d6cd4-beb2-3905-8860-51296a37ee2e",
                    "sense_definition": "Znajdujący się z przodu, w części frontowej czegoś; taki, który jest pierwszy od strony patrzącego lub kierunku ruchu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca i pasażer zajęli <w>przednie</w> siedzenia w samochodzie.",
                            "target_lang_translations": {
                                "ru": "Водитель и пассажир заняли <w>передние</w> сиденья в автомобиле."
                            }
                        },
                        {
                            "text": "Podczas wypadku stłukła się <w>przednia</w> szyba pojazdu.",
                            "target_lang_translations": {
                                "ru": "Во время аварии разбилось <w>лобовое</w> стекло автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "frontowy"
                    ],
                    "antonyms": [
                        "tylny",
                        "zadni"
                    ],
                    "common_phrases": [
                        "przednia szyba",
                        "przednie siedzenie",
                        "przedni napęd",
                        "przednie łapy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся спереди, в лицевой части чего-либо; такой, который является первым со стороны наблюдателя или по направлению движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передний",
                                "target_lang_sense_clarification": "расположенный впереди"
                            },
                            {
                                "target_lang_word": "лобовой",
                                "target_lang_sense_clarification": "относящийся к передней части (обычно о стекле или столкновении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9549398-9719-3a2d-99b6-ccf051718b42",
                    "sense_definition": "Wyróżniający się doskonałą jakością, smakiem lub humorem; bardzo dobry.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Goście bawili się doskonale, bo muzyka i towarzystwo były <w>przednie</w>.",
                            "target_lang_translations": {
                                "ru": "Гости отлично веселились, потому что музыка и компания были <w>превосходные</w>."
                            }
                        },
                        {
                            "text": "Mój dziadek opowiedział nam <w>przedni</w> żart przy obiedzie.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка рассказал нам за обедом <w>отличный</w> анекдот."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyborny",
                        "świetny",
                        "wyjątkowy"
                    ],
                    "antonyms": [
                        "lichy",
                        "marny",
                        "kiepski",
                        "pospolity"
                    ],
                    "common_phrases": [
                        "przednia zabawa",
                        "przedni trunek",
                        "przedni żart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличающийся превосходным качеством, вкусом или юмором; очень хороший."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "превосходный",
                                "target_lang_sense_clarification": "очень высокого качества"
                            },
                            {
                                "target_lang_word": "отличный",
                                "target_lang_sense_clarification": "очень хороший"
                            },
                            {
                                "target_lang_word": "знатный",
                                "target_lang_sense_clarification": "разг. замечательный, большой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a31e21f5-33c1-30ac-927f-bb893fb42980",
                    "sense_definition": "Idący lub stojący na czele grupy, stanowiący jej początek (często w kontekście wojskowym).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Przednia</w> straż doniosła dowódcy o zbliżającym się niebezpieczeństwie.",
                            "target_lang_translations": {
                                "ru": "<w>Передовая</w> стража доложила командиру о приближающейся опасности."
                            }
                        },
                        {
                            "text": "Najsilniejsi żołnierze zostali ustawieni w <w>przedniej</w> formacji.",
                            "target_lang_translations": {
                                "ru": "Самые сильные солдаты были расставлены в <w>передовом</w> строю."
                            }
                        }
                    ],
                    "antonyms": [
                        "tylny"
                    ],
                    "common_phrases": [
                        "straż przednia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идущий или стоящий во главе группы, составляющий её начало (часто в военном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передовой",
                                "target_lang_sense_clarification": "находящийся в авангарде"
                            },
                            {
                                "target_lang_word": "авангардный",
                                "target_lang_sense_clarification": "относящийся к передовой части войск"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "427e56cf-f37f-3536-8895-c749f3231655",
                    "sense_definition": "Taki, który był wcześniej; dawny lub poprzedni.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wspominaliśmy <w>przednie</w> czasy z wielkim sentymentem.",
                            "target_lang_translations": {
                                "ru": "Мы с большой сентиментальностью вспоминали <w>прежние</w> времена."
                            }
                        },
                        {
                            "text": "To obyczaj znany z <w>przednich</w> lat, dziś już zapomniany.",
                            "target_lang_translations": {
                                "ru": "Это обычай, известный с <w>давних</w> лет, сегодня уже забытый."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie używa się słowa 'poprzedni' lub 'dawny'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который был раньше; давний или предыдущий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прежний",
                                "target_lang_sense_clarification": "бывший ранее"
                            },
                            {
                                "target_lang_word": "былой",
                                "target_lang_sense_clarification": "минувший, прошлый"
                            }
                        ]
                    }
                }
            ]
        }

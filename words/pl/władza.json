            "senses": [
                {
                    "sense_id": "f25f3838-51ae-3ddb-8dd8-a28e22f02e85",
                    "sense_definition": "Prawo lub możliwość rządzenia państwem, kierowania ludźmi i podejmowania decyzji; także siła oddziaływania na kogoś lub na coś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "power_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy prezydent obiecał sprawować <w>władzę</w> sprawiedliwie i uczciwie.",
                            "target_lang_translations": {
                                "ru": "Новый президент пообещал осуществлять <w>власть</w> справедливо и честно."
                            }
                        },
                        {
                            "text": "Po wielu latach opozycja w końcu doszła do <w>władzy</w> w kraju.",
                            "target_lang_translations": {
                                "ru": "После многих лет оппозиция наконец пришла к <w>власти</w> в стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "panowanie",
                        "rządy",
                        "zwierzchnictwo",
                        "wpływ",
                        "moc",
                        "siła"
                    ],
                    "antonyms": [
                        "podległość",
                        "bezsilność"
                    ],
                    "common_phrases": [
                        "dojść do władzy",
                        "sprawować władzę",
                        "nadużycie władzy",
                        "walka o władzę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Право или возможность управлять государством, руководить людьми и принимать решения; также сила влияния на кого-либо или что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "власть",
                                "target_lang_sense_clarification": "право управления, влияние"
                            },
                            {
                                "target_lang_word": "правление",
                                "target_lang_sense_clarification": "процесс управления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0cf6e89-a0c3-3a37-8f27-e385933a7e0d",
                    "sense_definition": "Osoby lub instytucje, które rządzą państwem, miastem lub organizacją (często stosowane w liczbie mnogiej).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "power_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lokalne <w>władze</w> ogłosiły stan klęski żywiołowej po wielkiej powodzi.",
                            "target_lang_translations": {
                                "ru": "Местные <w>власти</w> объявили чрезвычайное положение после сильного наводнения."
                            }
                        },
                        {
                            "text": "Musimy złożyć oficjalne pismo do <w>władz</w> uczelni w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "Мы должны подать официальное письмо <w>руководству</w> университета по этому вопросу."
                            }
                        }
                    ],
                    "synonyms": [
                        "rządy",
                        "góra"
                    ],
                    "antonyms": [
                        "społeczeństwo",
                        "obywatele"
                    ],
                    "common_phrases": [

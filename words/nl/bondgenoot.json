{
    "word_family": [
        "bondgenootschap",
        "bond",
        "genoot"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "21b5bec1-f496-305b-83c0-c3912c8cc6dd",
                    "sense_definition": "Een land of partij waarmee een officieel verdrag is gesloten om elkaar te steunen, vooral in tijden van oorlog.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nederland en België waren <w>bondgenoten</w> tijdens de oorlog.",
                            "target_lang_translations": {
                                "ru": "Нидерланды и Бельгия были <w>союзниками</w> во время войны."
                            }
                        },
                        {
                            "text": "Als trouwe <w>bondgenoot</w> stuurde het land direct hulpgoederen naar het rampgebied.",
                            "target_lang_translations": {
                                "ru": "Как верный <w>союзник</w>, страна немедленно отправила гуманитарную помощь в зону бедствия."
                            }
                        }
                    ],
                    "synonyms": [
                        "alliantiepartner",
                        "medestander"
                    ],
                    "antonyms": [
                        "vijand",
                        "tegenstander"
                    ],
                    "common_phrases": [
                        "trouwe bondgenoot",
                        "belangrijke bondgenoot",
                        "NAVO-bondgenoot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страна или сторона, с которой заключен официальный договор о взаимной поддержке, особенно во время войны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "союзник",
                                "target_lang_sense_clarification": "по договору или в войне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75058822-10b9-37f3-926f-d41ed06b93f7",
                    "sense_definition": "Iemand die met je samenwerkt en je steunt om een gemeenschappelijk doel te bereiken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de vergadering bleek mijn collega een onverwachte <w>bondgenoot</w> te zijn.",
                            "target_lang_translations": {
                                "ru": "На собрании мой коллега оказался неожиданным <w>союзником</w>."
                            }
                        },
                        {
                            "text": "Zij zochten een <w>bondgenoot</w> om hun plannen voor de nieuwe buurt te realiseren.",
                            "target_lang_translations": {
                                "ru": "Они искали <w>союзника</w>, чтобы реализовать свои планы относительно нового района."
                            }
                        }
                    ],
                    "synonyms": [
                        "partner",
                        "medestander",
                        "helper"
                    ],
                    "antonyms": [
                        "tegenstander",
                        "rivaal"
                    ],
                    "common_phrases": [
                        "onverwachte bondgenoot",
                        "natuurlijke bondgenoot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который сотрудничает с вами и поддерживает вас для достижения общей цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "союзник",
                                "target_lang_sense_clarification": "партнер в общем деле"
                            },
                            {
                                "target_lang_word": "соратник",
                                "target_lang_sense_clarification": "товарищ по борьбе или деятельности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
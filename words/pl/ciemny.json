                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тёмный",
                                "target_lang_sense_clarification": "о цвете волос, предметов"
                            },
                            {
                                "target_lang_word": "чёрный",
                                "target_lang_sense_clarification": "в некоторых контекстах (напр. хлеб)"

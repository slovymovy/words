{
    "word_family": [
        "ostateczny",
        "ostateczność",
        "ostatki",
        "ostatek"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ebb9cd66-be91-3226-9b95-d401142d878f",
                    "sense_definition": "Znajdujący się na samym końcu szeregu, listy, kolejności lub przestrzeni; taki, po którym nie ma już innego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest <w>ostatni</w> dom po prawej stronie ulicy.",
                            "target_lang_translations": {
                                "ru": "Это <w>последний</w> дом на правой стороне улицы."
                            }
                        },
                        {
                            "text": "Mój zawodnik przybiegł na metę jako <w>ostatni</w>.",
                            "target_lang_translations": {
                                "ru": "Мой игрок прибежал к финишу <w>последним</w>."
                            }
                        },
                        {
                            "text": "To już <w>ostatni</w> raz, kiedy ci pożyczam pieniądze.",
                            "target_lang_translations": {
                                "ru": "Это уже <w>последний</w> раз, когда я одалживаю тебе деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "końcowy"
                    ],
                    "antonyms": [
                        "pierwszy",
                        "początkowy"
                    ],
                    "common_phrases": [
                        "ostatni raz",
                        "ostatnie miejsce",
                        "ostatni dzień",
                        "ostatni moment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в самом конце ряда, списка, очереди или пространства; такой, после которого больше нет другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последний",
                                "target_lang_sense_clarification": "по порядку или расположению"
                            },
                            {
                                "target_lang_word": "конечный",
                                "target_lang_sense_clarification": "завершающий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb6018a7-52d4-3f25-a62e-14d03048c316",
                    "sense_definition": "Taki, który wydarzył się niedawno, bezpośrednio przed obecną chwilą; najnowszy z minionych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <w>ostatnim</w> tygodniu pogoda była bardzo deszczowa.",
                            "target_lang_translations": {
                                "ru": "На <w>прошлой</w> неделе погода была очень дождливой."
                            }
                        },
                        {
                            "text": "Czy czytałeś <w>ostatni</w> artykuł w dzisiejszej gazecie?",
                            "target_lang_translations": {
                                "ru": "Ты читал <w>последнюю</w> статью в сегодняшней газете?"
                            }
                        },
                        {
                            "text": "W <w>ostatnich</w> latach technologia bardzo się rozwinęła.",
                            "target_lang_translations": {
                                "ru": "В <w>последние</w> годы технологии очень развились."
                            }
                        }
                    ],
                    "synonyms": [
                        "szczytowy",
                        "gw. (Górny Śląsk) pośledni"
                    ],
                    "antonyms": [
                        "pierwszy"
                    ],
                    "common_phrases": [
                        "w ostatnim czasie",
                        "ostatniej nocy",
                        "w ostatnich dniach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произошедший недавно, непосредственно перед настоящим моментом; новейший из прошедших."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последний",
                                "target_lang_sense_clarification": "недавний, самый новый"
                            },
                            {
                                "target_lang_word": "прошлый",
                                "target_lang_sense_clarification": "минувший (о времени)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a088734-1ad1-3ada-b6e0-5edd53eb8a2c",
                    "sense_definition": "Jedyny, który pozostał; taki, że nie ma już więcej dostępnych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "finality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To była moja <w>ostatnia</w> szansa na zdanie tego egzaminu.",
                            "target_lang_translations": {
                                "ru": "Это был мой <w>последний</w> шанс сдать этот экзамен."
                            }
                        },
                        {
                            "text": "Wydałem na ten prezent swoje <w>ostatnie</w> pieniądze.",
                            "target_lang_translations": {
                                "ru": "Я потратил на этот подарок свои <w>последние</w> деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "ostateczny",
                        "krańcowy"
                    ],
                    "common_phrases": [
                        "ostatnia deska ratunku",
                        "ostatnia wola",
                        "ostatni grosz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единственный оставшийся; такой, что больше нет доступных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последний",
                                "target_lang_sense_clarification": "оставшийся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c112603-380d-38c1-b435-eac1d2b9844f",
                    "sense_definition": "Najgorszy pod względem jakości, wartości lub moralności; najmniej pożądany.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest <w>ostatni</w> łobuz w całej naszej okolicy.",
                            "target_lang_translations": {
                                "ru": "Это <w>последний</w> негодяй во всей нашей округе."
                            }
                        },
                        {
                            "text": "Jedzenie w tej restauracji jest <w>ostatniego</w> gatunku.",
                            "target_lang_translations": {
                                "ru": "Еда в этом ресторане <w>последнего</w> сорта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ostatni drań",
                        "ostatni sort"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to emphasize negative judgment"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Худший по качеству, ценности или морали; наименее желательный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последний",
                                "target_lang_sense_clarification": "худший, никудышный"
                            },
                            {
                                "target_lang_word": "отпетый",
                                "target_lang_sense_clarification": "безнадежный (о человеке)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7147cd85-2345-3d13-99a5-e1a32adbb20d",
                    "sense_definition": "Osoba, która znajduje się na końcu grupy, listy lub kolejki.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pierwszy otrzyma nagrodę, a <w>ostatni</w> będzie musiał posprzątać salę.",
                            "target_lang_translations": {
                                "ru": "Первый получит награду, а <w>последний</w> должен будет убрать зал."
                            }
                        },
                        {
                            "text": "Jako <w>ostatni</w> na listę wpisał się Marek.",
                            "target_lang_translations": {
                                "ru": "<w>Последним</w> в список записался Марек."
                            }
                        }
                    ],
                    "antonyms": [
                        "pierwszy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который находится в конце группы, списка или очереди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последний",
                                "target_lang_sense_clarification": "человек в конце очереди"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
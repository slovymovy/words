{
    "word_family": [
        "suddenly",
        "suddenness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c12cb270-09c2-327c-ad83-d31f2042a0af",
                    "sense_definition": "Occurring quickly with little or no warning or expectation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "unexpected_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>sudden</w> noise made everyone in the room jump.",
                            "target_lang_translations": {
                                "ru": "<w>Внезапный</w> шум заставил всех в комнате вздрогнуть."
                            }
                        },
                        {
                            "text": "There was a <w>sudden</w> change in the weather this afternoon.",
                            "target_lang_translations": {
                                "ru": "Сегодня днём произошла <w>резкая</w> перемена погоды."
                            }
                        }
                    ],
                    "synonyms": [
                        "abrupt"
                    ],
                    "antonyms": [
                        "gradual"
                    ],
                    "common_phrases": [
                        "sudden change",
                        "sudden death",
                        "sudden movement",
                        "sudden stop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий быстро, без предупреждения или неожиданно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внезапный",
                                "target_lang_sense_clarification": "происходящий вдруг, неожиданно"
                            },
                            {
                                "target_lang_word": "неожиданный",
                                "target_lang_sense_clarification": "тот, которого не ждали"
                            },
                            {
                                "target_lang_word": "резкий",
                                "target_lang_sense_clarification": "быстрый и сильный (об изменениях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b634405d-a1c1-3eb5-b165-192f0881a0e1",
                    "sense_definition": "Hasty, violent, rash, or precipitate in behavior or action.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rash_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He regretted his <w>sudden</w> decision to quit his job without a plan.",
                            "target_lang_translations": {
                                "ru": "Он пожалел о своём <w>поспешном</w> решении уволиться без плана."
                            }
                        },
                        {
                            "text": "Her <w>sudden</w> outburst surprised her colleagues.",
                            "target_lang_translations": {
                                "ru": "Её <w>внезапная</w> вспышка гнева удивила коллег."
                            }
                        }
                    ],
                    "synonyms": [
                        "impetuous",
                        "impulsive",
                        "reckless",
                        "hotheaded"
                    ],
                    "common_phrases": [
                        "sudden impulse",
                        "sudden decision"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поспешный, необдуманный или резкий в поведении или действиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поспешный",
                                "target_lang_sense_clarification": "сделанный в спешке"
                            },
                            {
                                "target_lang_word": "скоропалительный",
                                "target_lang_sense_clarification": "принятый слишком быстро, необдуманно"
                            },
                            {
                                "target_lang_word": "импульсивный",
                                "target_lang_sense_clarification": "совершенный под влиянием порыва"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "73236a3b-e036-36c4-9e5f-3ca20c22ff17",
                    "sense_definition": "An unexpected occurrence or surprise.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "unexpected_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "All of a <w>sudden</w>, the lights went out in the entire building.",
                            "target_lang_translations": {
                                "ru": "<w>Внезапно</w> во всём здании погас свет."
                            }
                        },
                        {
                            "text": "The car stopped all of a <w>sudden</w> in the middle of the road.",
                            "target_lang_translations": {
                                "ru": "Машина <w>вдруг</w> остановилась посреди дороги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "all of a sudden"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "chiefly in the phrase 'all of a sudden'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неожиданное событие или сюрприз (преимущественно в идиоме 'all of a sudden')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неожиданность",
                                "target_lang_sense_clarification": "внезапное происшествие"
                            },
                            {
                                "target_lang_word": "вдруг",
                                "target_lang_sense_clarification": "эквивалент фразы 'all of a sudden'"
                            }
                        ]
                    }
                }
            ]
        },
        {

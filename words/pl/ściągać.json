                {
                    "sense_id": "cacd4728-0d80-338d-9e0d-9c60ec5c0826",
                    "sense_definition": "Zdejmować z siebie ubranie, buty lub dodatki.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "removal_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <w>ściągać</w> buty przed wejściem do mieszkania.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>снимайте</w> обувь перед входом в квартиру."
                            }
                        },
                        {
                            "text": "Było bardzo gorąco, więc musiał <w>ściągnąć</w> kurtkę.",
                            "target_lang_translations": {
                                "ru": "Было очень жарко, поэтому ему пришлось <w>снять</w> куртку."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdejmować"
                    ],
                    "common_phrases": [
                        "ściągać buty",
                        "ściągać ubranie",
                        "ściągać czapkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снимать с себя одежду, обувь или аксессуары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снимать",
                                "target_lang_sense_clarification": "одежду, обувь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f88fe663-0a31-3ed6-b25d-d276a3feb170",
                    "sense_definition": "Pobierać pliki, programy lub dane z internetu na urządzenie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "digital_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właśnie zacząłem <w>ściągać</w> nowy film na wieczór.",
                            "target_lang_translations": {
                                "ru": "Я только что начал <w>скачивать</w> новый фильм на вечер."
                            }
                        },
                        {
                            "text": "Nie powinieneś <w>ściągać</w> załączników od nieznanych nadawców.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <w>скачивать</w> вложения от неизвестных отправителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "pobierać"
                    ],
                    "common_phrases": [
                        "ściągać pliki",
                        "ściągać muzykę",
                        "ściągać aplikację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скачивать файлы, программы или данные из интернета на устройство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скачивать",
                                "target_lang_sense_clarification": "файлы, данные"
                            },
                            {
                                "target_lang_word": "загружать",
                                "target_lang_sense_clarification": "из интернета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "150c6e8b-037e-39dd-9afd-a81cfb42921e",
                    "sense_definition": "Nieuczciwie odpisywać od kogoś lub korzystać z niedozwolonych pomocy podczas egzaminu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cheating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel zauważył, że uczeń próbuje <w>ściągać</w> od kolegi.",
                            "target_lang_translations": {
                                "ru": "Учитель заметил, что ученик пытается <w>списывать</w> у одноклассника."
                            }
                        },
                        {
                            "text": "Na studiach nigdy nie odważyłem się <w>ściągać</w> na egzaminie.",
                            "target_lang_translations": {
                                "ru": "В университете я никогда не осмеливался <w>списывать</w> на экзамене."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ściągać na klasówce",
                        "ściągać od kolegi",
                        "ściągać na teście"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used primarily in school/university context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечестно списывать у кого-то или пользоваться шпаргалками во время экзамена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "списывать",
                                "target_lang_sense_clarification": "на экзамене"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e363e1c5-b07b-3a45-866e-b90573b3ed7b",
                    "sense_definition": "Brać coś, co znajduje się wysoko, i kłaść to niżej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "removal_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy możesz <w>ściągnąć</w> tę ciężką walizkę z szafy?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>снять</w> этот тяжелый чемодан со шкафа?"
                            }
                        },
                        {
                            "text": "Musimy szybko <w>ściągać</w> pranie ze sznura, bo idzie burza.",
                            "target_lang_translations": {
                                "ru": "Мы должны быстро <w>снимать</w> белье с веревки, потому что идет гроза."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdejmować"
                    ],
                    "common_phrases": [
                        "ściągać z półki",
                        "ściągać zasłony",
                        "ściągać kota z drzewa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать что-то, находящееся высоко, и класть вниз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снимать",
                                "target_lang_sense_clarification": "сверху, с полки"
                            },
                            {
                                "target_lang_word": "доставать",
                                "target_lang_sense_clarification": "с высоты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b583fdcd-8744-3b61-81c2-46c9bd57c056",
                    "sense_definition": "Sprowadzać kogoś lub coś w jedno miejsce; przyciągać czyjąś uwagę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego głośne zachowanie zaczęło <w>ściągać</w> uwagę przechodniów.",
                            "target_lang_translations": {
                                "ru": "Его громкое поведение начало <w>привлекать</w> внимание прохожих."
                            }
                        },
                        {
                            "text": "Generał postanowił <w>ściągnąć</w> dodatkowe posiłki do obrony miasta.",
                            "target_lang_translations": {
                                "ru": "Генерал решил <w>стянуть</w> дополнительные подкрепления для обороны города."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyciągać",
                        "znosić"
                    ],
                    "common_phrases": [
                        "ściągać uwagę",
                        "ściągać kłopoty",
                        "ściągać posiłki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собирать кого-то или что-то в одном месте; привлекать чье-то внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привлекать",
                                "target_lang_sense_clarification": "внимание"
                            },
                            {
                                "target_lang_word": "стягивать",
                                "target_lang_sense_clarification": "войска, силы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35c39e04-03e0-3699-957f-e63aadfe27aa",
                    "sense_definition": "Egzekwować od kogoś zapłatę należności, na przykład podatków lub długów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Urząd skarbowy będzie skuteczniej <w>ściągać</w> zaległe podatki.",
                            "target_lang_translations": {
                                "ru": "Налоговая служба будет эффективнее <w>взыскивать</w> задолженности по налогам."
                            }
                        },
                        {
                            "text": "Trudno jest <w>ściągać</w> alimenty od osoby, która oficjalnie nie pracuje.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>взыскивать</w> алименты с человека, который официально не работает."
                            }
                        }
                    ],
                    "synonyms": [
                        "pobierać"
                    ],
                    "common_phrases": [
                        "ściągać długi",
                        "ściągać podatki",
                        "ściągać należności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требовать уплаты долга, налогов или других обязательств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взыскивать",
                                "target_lang_sense_clarification": "долги, налоги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dcd886ed-587c-39d6-906f-81da487f7011",
                    "sense_definition": "Pociągać końce czegoś (np. sznurka, paska) lub napinać mięśnie twarzy, powodując ich zwężenie lub zmarszczenie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tightening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z bólu zaczął mocno <w>ściągać</w> brwi.",
                            "target_lang_translations": {
                                "ru": "От боли он начал сильно <w>хмурить</w> брови."
                            }
                        },
                        {
                            "text": "Musisz mocniej <w>ściągnąć</w> ten węzeł, żeby się nie rozwiązał.",
                            "target_lang_translations": {
                                "ru": "Ты должен сильнее <w>затянуть</w> этот узел, чтобы он не развязался."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ściągać brwi",
                        "ściągać usta",
                        "ściągać wodze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стягивать концы чего-либо или напрягать мышцы лица (хмуриться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хмурить",
                                "target_lang_sense_clarification": "брови"
                            },
                            {
                                "target_lang_word": "затягивать",
                                "target_lang_sense_clarification": "узел"
                            }
                        ]
                    }
                }
            ]
        }
    ]

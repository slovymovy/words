                    "sense_id": "908a8f73-53ac-3823-812d-f3cea30c089a",
                    "sense_definition": "Dialectal form of <w>thou</w>; a singular informal second-person pronoun.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "What is <thoo> doing with that old hammer?",
                            "target_lang_translations": {
                                "ru": "Что <ты> делаешь с этим старым молотком?"
                            }
                        },
                        {
                            "text": "I told <thoo> not to go down that road.",
                            "target_lang_translations": {
                                "ru": "Я говорил <тебе> не ходить по той дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "thou",
                        "you"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Northern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Диалектная форма местоимения «thou» (ты); неофициальное местоимение второго лица единственного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "личное местоимение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "d82fe4ee-c49a-34aa-b9a9-7dc5dd7795ea",

{
    "word_family": [
        "plotter",
        "subplot",
        "counterplot"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bfc9c13d-ab9c-3f82-9626-6790937ba70d",
                    "sense_definition": "The main events of a play, novel, movie, or similar work, devised and presented by the writer as an interrelated sequence.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "narrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The book has a very complicated <w>plot</w> involving time travel.",
                            "target_lang_translations": {
                                "ru": "В книге очень запутанный <w>сюжет</w>, включающий путешествия во времени."
                            }
                        },
                        {
                            "text": "I enjoyed the characters, but the <w>plot</w> was a bit slow.",
                            "target_lang_translations": {
                                "ru": "Мне понравились персонажи, но <w>сюжет</w> развивался немного медленно."
                            }
                        }
                    ],
                    "synonyms": [
                        "storyline"
                    ],
                    "common_phrases": [
                        "plot twist",
                        "lose the plot",
                        "movie plot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основные события пьесы, романа, фильма или подобного произведения, представленные автором как взаимосвязанная последовательность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сюжет",
                                "target_lang_sense_clarification": "основное содержание произведения"
                            },
                            {
                                "target_lang_word": "фабула",
                                "target_lang_sense_clarification": "литературоведческий термин; хронологическая последовательность событий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e99a1e5-7c72-34cd-8145-98dd617ca55a",
                    "sense_definition": "A secret plan made by a group of people to do something illegal or harmful.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conspiracy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police uncovered a <w>plot</w> to rob the national bank.",
                            "target_lang_translations": {
                                "ru": "Полиция раскрыла <w>заговор</w> с целью ограбления национального банка."
                            }
                        },
                        {
                            "text": "He was arrested for his role in the assassination <w>plot</w>.",
                            "target_lang_translations": {
                                "ru": "Он был арестован за участие в <w>заговоре</w> с целью убийства."
                            }
                        }
                    ],
                    "synonyms": [
                        "conspiracy",
                        "scheme",
                        "intrigue",
                        "machination"
                    ],
                    "common_phrases": [
                        "hatch a plot",
                        "uncover a plot",
                        "assassination plot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тайный план группы людей совершить что-либо незаконное или вредное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заговор",
                                "target_lang_sense_clarification": "тайное соглашение против кого-либо"
                            },
                            {
                                "target_lang_word": "интрига",
                                "target_lang_sense_clarification": "скрытые действия для достижения цели"
                            },
                            {
                                "target_lang_word": "сговор",
                                "target_lang_sense_clarification": "тайная договоренность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cadd631-5d81-3ad4-ad86-54394a9a916a",
                    "sense_definition": "A small piece of ground marked out for a purpose such as building or gardening.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "land",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They bought a small <w>plot</w> of land to build their dream home.",
                            "target_lang_translations": {
                                "ru": "Они купили небольшой <w>участок</w> земли, чтобы построить дом своей мечты."
                            }
                        },
                        {
                            "text": "She spends her weekends working on her vegetable <w>plot</w>.",
                            "target_lang_translations": {
                                "ru": "Она проводит выходные, работая на своем овощном <w>участке</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "parcel",
                        "lot"
                    ],
                    "common_phrases": [
                        "vegetable plot",
                        "garden plot",
                        "building plot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой кусок земли, выделенный для определенной цели, например, для строительства или садоводства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "часть территории"
                            },
                            {
                                "target_lang_word": "надел",
                                "target_lang_sense_clarification": "выделенная земля (часто историческое или с/х)"
                            },
                            {
                                "target_lang_word": "делянка",
                                "target_lang_sense_clarification": "участок для обработки или вырубки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63a5991a-f01c-3760-9138-325518163881",
                    "sense_definition": "A graphical representation of data, such as a chart or diagram.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "data_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>plot</w> shows the relationship between temperature and pressure.",
                            "target_lang_translations": {
                                "ru": "Этот <w>график</w> показывает зависимость между температурой и давлением."
                            }
                        },
                        {
                            "text": "Please analyze the scatter <w>plot</w> to find any outliers.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проанализируйте точечную <w>диаграмму</w>, чтобы найти выбросы."
                            }
                        }
                    ],
                    "synonyms": [
                        "graph",
                        "chart",
                        "diagram"
                    ],
                    "common_phrases": [
                        "scatter plot",
                        "radar plot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Графическое представление данных, например, схема или диаграмма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "график",
                                "target_lang_sense_clarification": "линии на координатной плоскости"
                            },
                            {
                                "target_lang_word": "диаграмма",
                                "target_lang_sense_clarification": "схематическое изображение данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "382e9984-2b13-38ac-995c-39474988dedf",
                    "sense_definition": "A small piece of ground in a cemetery used for a grave.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "land",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was buried in the family <w>plot</w> alongside his ancestors.",
                            "target_lang_translations": {
                                "ru": "Он был похоронен на семейном <w>участке</w> рядом со своими предками."
                            }
                        },
                        {
                            "text": "They visited the burial <w>plot</w> to pay their respects.",
                            "target_lang_translations": {
                                "ru": "Они посетили место захоронения (<w>участок</w>), чтобы почтить память."
                            }
                        }
                    ],
                    "common_phrases": [
                        "family plot",
                        "burial plot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой участок земли на кладбище, предназначенный для могилы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "место на кладбище"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b5106376-c662-3747-b421-da5772f2c925",
                    "sense_definition": "To make a secret plan with others to do something illegal or harmful.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conspiracy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rebels were <w>plotting</w> to overthrow the government.",
                            "target_lang_translations": {
                                "ru": "Повстанцы <w>замышляли</w> свергнуть правительство."
                            }
                        },
                        {
                            "text": "He spent years <w>plotting</w> revenge against his enemies.",
                            "target_lang_translations": {
                                "ru": "Он провел годы, <w>замышляя</w> месть своим врагам."
                            }
                        }
                    ],
                    "synonyms": [
                        "connive"
                    ],
                    "common_phrases": [
                        "plot against",
                        "plot revenge",
                        "plot a coup"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строить тайные планы с другими людьми для совершения чего-либо незаконного или вредного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замышлять",
                                "target_lang_sense_clarification": "задумывать недоброе"
                            },
                            {
                                "target_lang_word": "устраивать заговор",
                                "target_lang_sense_clarification": "организовывать тайный план"
                            },
                            {
                                "target_lang_word": "плести интриги",
                                "target_lang_sense_clarification": "строить козни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88db75e4-971a-3632-bc25-e677870287c1",
                    "sense_definition": "To mark points on a graph or map to show the position or path of something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "data_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain <w>plotted</w> a new course to avoid the storm.",
                            "target_lang_translations": {
                                "ru": "Капитан <w>проложил</w> новый курс, чтобы избежать шторма."
                            }
                        },
                        {
                            "text": "Students learned how to <w>plot</w> the data points on a graph.",
                            "target_lang_translations": {
                                "ru": "Студенты учились <w>отмечать</w> точки данных на графике."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plot a course",
                        "plot a graph",
                        "plot points"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отмечать точки на графике или карте, чтобы показать положение или путь чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отмечать",
                                "target_lang_sense_clarification": "наносить метки"
                            },
                            {
                                "target_lang_word": "начертить",
                                "target_lang_sense_clarification": "сделать чертеж или схему"
                            },
                            {
                                "target_lang_word": "прокладывать",
                                "target_lang_sense_clarification": "курс или маршрут"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "veruiterlijken",
        "uiterlijkheid"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "87a70cc7-452c-3dd6-a56c-2abba6953322",
                    "sense_definition": "De manier waarop iemand of iets er aan de buitenkant uitziet.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wie alsmaar bezig is met zijn <w>uiterlijk</w>, kan daar flink veel stress van ondervinden.",
                            "target_lang_translations": {
                                "en": "Whoever is constantly concerned with their <w>appearance</w> can experience a lot of stress from it.",
                                "ru": "Тот, кто постоянно озабочен своей <w>внешностью</w>, может испытывать из-за этого сильный стресс."
                            }
                        },
                        {
                            "text": "Zij besteedt veel aandacht aan haar <w>uiterlijk</w> voordat ze naar haar werk gaat.",
                            "target_lang_translations": {
                                "en": "She pays a lot of attention to her <w>appearance</w> before she goes to work.",
                                "ru": "Она уделяет много внимания своей <w>внешности</w> перед тем, как пойти на работу."
                            }
                        }
                    ],
                    "antonyms": [
                        "innerlijk"
                    ],
                    "common_phrases": [
                        "op het uiterlijk",
                        "verzorgd uiterlijk",
                        "knap uiterlijk"
                    ],
                    "target_lang_definitions": {
                        "en": "The way someone or something looks on the outside.",
                        "ru": "То, как кто-то или что-то выглядит снаружи; внешний облик."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appearance",
                                "target_lang_sense_clarification": "physical look"
                            },
                            {
                                "target_lang_word": "looks",
                                "target_lang_sense_clarification": "physical appearance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "внешность",
                                "target_lang_sense_clarification": "облик человека"
                            },
                            {
                                "target_lang_word": "внешний вид",
                                "target_lang_sense_clarification": "общий вид"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1c77e0e5-7952-3118-afc1-908fafffbcdf",
                    "sense_definition": "Niet later dan een bepaald genoemd tijdstip.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit formulier moet <w>uiterlijk</w> op 1 juni ingezonden zijn.",
                            "target_lang_translations": {
                                "en": "This form must be submitted <w>no later than</w> June 1st.",
                                "ru": "Эту форму необходимо отправить <w>не позднее</w> 1 июня."
                            }
                        },
                        {
                            "text": "De belastingaangifte moet <w>uiterlijk</w> volgende week bij de dienst binnen zijn.",
                            "target_lang_translations": {
                                "en": "The tax return must be in <w>no later than</w> next week.",
                                "ru": "Налоговая декларация должна поступить в службу <w>самое позднее</w> на следующей неделе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uiterlijk morgen",
                        "uiterlijk om",
                        "uiterlijk op"
                    ],
                    "target_lang_definitions": {
                        "en": "Not later than a specific mentioned time.",
                        "ru": "Не позднее определенного указанного времени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at the latest",
                                "target_lang_sense_clarification": "deadline limit"
                            },
                            {
                                "target_lang_word": "no later than",
                                "target_lang_sense_clarification": "deadline limit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "не позднее",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "самое позднее",
                                "target_lang_sense_clarification": "в крайнем случае"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "102944f2-9ebd-318e-a30e-3476ea18de27",
                    "sense_definition": "Wat betrekking heeft op de buitenkant of zichtbaar is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een <w>uiterlijk</w> kenmerk van deze vogelsoort is de rode kleur op de rug.",
                            "target_lang_translations": {
                                "en": "An <w>external</w> characteristic of this bird species is the red color on its back.",
                                "ru": "<w>Внешним</w> признаком этого вида птиц является красный цвет на спине."
                            }
                        },
                        {
                            "text": "Er was geen <w>uiterlijke</w> schade aan de auto te zien na het ongeluk.",
                            "target_lang_translations": {
                                "en": "There was no <w>external</w> damage visible on the car after the accident.",
                                "ru": "После аварии на машине не было видно никаких <w>внешних</w> повреждений."
                            }
                        }
                    ],
                    "synonyms": [
                        "extern",
                        "uitwendig"
                    ],
                    "antonyms": [
                        "intern",
                        "innerlijk",
                        "inwendig"
                    ],
                    "common_phrases": [
                        "uiterlijke kenmerken",
                        "uiterlijke verzorging",
                        "uiterlijke schijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the outside or visible.",
                        "ru": "Относящийся к внешней стороне или видимый."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "external",
                                "target_lang_sense_clarification": "on the outside"
                            },
                            {
                                "target_lang_word": "outward",
                                "target_lang_sense_clarification": "visible on the outside"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "внешний",
                                "target_lang_sense_clarification": "находящийся снаружи"
                            },
                            {
                                "target_lang_word": "наружный",
                                "target_lang_sense_clarification": "противоположный внутреннему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb279212-99b8-319d-8b10-c38484ea09c4",
                    "sense_definition": "De laatst mogelijke of toegestane.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is de <w>uiterlijke</w> datum van inzending voor het project.",
                            "target_lang_translations": {
                                "en": "This is the <w>final</w> submission date for the project.",
                                "ru": "Это <w>крайняя</w> дата подачи проекта."
                            }
                        },
                        {
                            "text": "We moeten rekening houden met de <w>uiterlijke</w> termijn die in het contract staat.",
                            "target_lang_translations": {
                                "en": "We must take into account the <w>final</w> deadline stated in the contract.",
                                "ru": "Мы должны учитывать <w>предельный</w> срок, указанный в контракте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uiterlijke datum",
                        "uiterlijke inleverdatum"
                    ],
                    "target_lang_definitions": {
                        "en": "The last possible or permitted.",
                        "ru": "Последний возможный или допустимый."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "final",
                                "target_lang_sense_clarification": "last possible"
                            },
                            {
                                "target_lang_word": "latest",
                                "target_lang_sense_clarification": "most recent or last possible"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "крайний",
                                "target_lang_sense_clarification": "последний допустимый"
                            },
                            {
                                "target_lang_word": "предельный",
                                "target_lang_sense_clarification": "максимальный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
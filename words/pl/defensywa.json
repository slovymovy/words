    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "de93f49e-8d9a-3a9e-a68e-0522c57a2f1f",
                    "sense_definition": "Stan, taktyka lub działania mające na celu obronę przed atakiem przeciwnika (w kontekście wojskowym, politycznym lub w dyskusji).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "def_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po nieudanym ataku wojska nieprzyjaciela zostały zepchnięte do głębokiej <w>defensywy</w>.",
                            "target_lang_translations": {
                                "ru": "После неудачной атаки войска противника были отброшены в глубокую <w>оборону</w>."
                            }
                        },
                        {
                            "text": "W trakcie debaty polityk musiał przejść do <w>defensywy</w>, aby odeprzeć zarzuty oponentów.",
                            "target_lang_translations": {
                                "ru": "В ходе дебатов политику пришлось перейти к <w>обороне</w>, чтобы отбить обвинения оппонентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "obrona"
                    ],
                    "antonyms": [
                        "ofensywa",
                        "atak"
                    ],
                    "common_phrases": [
                        "być w defensywie",
                        "przejść do defensywy",
                        "zepchnąć do defensywy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, тактика или действия, направленные на защиту от нападения противника (в военном, политическом контексте или в дискуссии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оборона",
                                "target_lang_sense_clarification": "военная или тактическая"
                            },
                            {
                                "target_lang_word": "защита",
                                "target_lang_sense_clarification": "общее понятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "930045f9-ded5-381a-b1ba-fd3f33f3f24f",
                    "sense_definition": "Część drużyny sportowej (np. w piłce nożnej) odpowiedzialna za zadania obronne i ochronę własnej bramki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "def_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trener postanowił wzmocnić <w>defensywę</w>, wprowadzając na boisko nowego środkowego obrońcę.",
                            "target_lang_translations": {
                                "ru": "Тренер решил укрепить <w>оборону</w>, выпустив на поле нового центрального защитника."
                            }
                        },
                        {
                            "text": "Mimo słabego ataku, ich solidna <w>defensywa</w> pozwoliła utrzymać bezbramkowy remis do końca meczu.",
                            "target_lang_translations": {
                                "ru": "Несмотря на слабую атаку, их надежная <w>оборона</w> позволила удержать безголевую ничью до конца матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "obrona"
                    ],
                    "antonyms": [
                        "ofensywa",
                        "atak"
                    ],
                    "common_phrases": [
                        "grać w defensywie",
                        "dziurawa defensywa",
                        "szczelna defensywa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть спортивной команды (например, в футболе), отвечающая за оборонительные действия и защиту собственных ворот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оборона",
                                "target_lang_sense_clarification": "линия обороны"
                            },
                            {
                                "target_lang_word": "защита",
                                "target_lang_sense_clarification": "действия защитников"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "903ebc0e-8750-391a-8495-996b3ae06970",
                    "sense_definition": "Potoczne określenie polskiego kontrwywiadu wojskowego i służb bezpieczeństwa w okresie międzywojennym.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W okresie międzywojennym <w>defensywa</w> zajmowała się zwalczaniem obcego wywiadu na terenie kraju.",
                            "target_lang_translations": {
                                "ru": "В межвоенный период <w>дефензива</w> занималась борьбой с иностранной разведкой на территории страны."
                            }
                        },
                        {
                            "text": "Historycy badają archiwa, aby poznać metody działania ówczesnej <w>defensywy</w>.",
                            "target_lang_translations": {
                                "ru": "Историки изучают архивы, чтобы узнать методы работы тогдашней <w>дефензивы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kontrwywiad"
                    ],
                    "traits": [
                        {

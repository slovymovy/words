    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c6aab561-5707-3502-8c56-f7732d6b3b4e",
                    "sense_definition": "Выражение сожаления, грусти или досады по поводу чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_pity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Как <жаль>, что лето так быстро закончилось.",
                            "target_lang_translations": {
                                "en": "It is such a <pity> that summer ended so quickly."
                            }
                        },
                        {
                            "text": "Мне очень <жаль>, что я не смог прийти вовремя.",
                            "target_lang_translations": {
                                "en": "I am very <sorry> that I couldn't come on time."
                            }
                        }
                    ],
                    "synonyms": [
                        "печально"
                    ],
                    "common_phrases": [
                        "как жаль",
                        "очень жаль",
                        "искренне жаль"
                    ],
                    "target_lang_definitions": {
                        "en": "An expression of regret, sadness, or annoyance about a situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pity",
                                "target_lang_sense_clarification": "used in phrases like 'it is a pity'"
                            },
                            {
                                "target_lang_word": "sorry",
                                "target_lang_sense_clarification": "used to express apology or regret"
                            },
                            {
                                "target_lang_word": "shame",
                                "target_lang_sense_clarification": "used in phrases like 'it is a shame'"
                            },
                            {
                                "target_lang_word": "too bad",
                                "target_lang_sense_clarification": "informal expression of regret"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6783e0a5-967f-338e-b05f-5b7f22d71b60",
                    "sense_definition": "Чувство сострадания, жалости по отношению к кому-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_pity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему стало <жаль> маленького бездомного котёнка.",
                            "target_lang_translations": {
                                "en": "He felt <sorry for> the little homeless kitten."
                            }
                        },
                        {
                            "text": "Мне <жаль> её, потому что она попала в трудную ситуацию.",
                            "target_lang_translations": {
                                "en": "I <feel bad for> her because she got into a difficult situation."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мне жаль тебя",
                        "ему жаль",
                        "ей жаль"
                    ],
                    "target_lang_definitions": {
                        "en": "A feeling of compassion or sympathy towards someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "feel sorry for",
                                "target_lang_sense_clarification": "to sympathize with someone"
                            },
                            {
                                "target_lang_word": "feel bad for",
                                "target_lang_sense_clarification": "to feel empathy for someone's misfortune"
                            },
                            {
                                "target_lang_word": "pity",
                                "target_lang_sense_clarification": "to feel compassion for suffering"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ba4f31b-be8f-35e1-ae60-9f4f40abb6e6",
                    "sense_definition": "Чувство нежелания тратить, отдавать что-либо или лишаться чего-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_pity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отцу было <жаль> денег на дорогие игрушки.",
                            "target_lang_translations": {
                                "en": "Father <grudged> the money for expensive toys."
                            }
                        },
                        {
                            "text": "Нам не <жаль> времени, чтобы помочь хорошему другу.",
                            "target_lang_translations": {
                                "en": "We do not <grudge> the time to help a good friend."
                            }
                        }
                    ],
                    "common_phrases": [
                        "жаль денег",
                        "жаль времени",
                        "не жаль сил"
                    ],
                    "target_lang_definitions": {
                        "en": "A feeling of unwillingness to spend, give away, or lose something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grudge",
                                "target_lang_sense_clarification": "to give or allow unwillingly"
                            },
                            {
                                "target_lang_word": "mind",
                                "target_lang_sense_clarification": "usually used in negative constructions (not mind)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c5483417-d7a9-311c-a581-e8c10f7645c4",
                    "sense_definition": "Форма повелительного наклонения глагола <жалить>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эй, пчела, <жаль> врага, если он нападёт!",
                            "target_lang_translations": {
                                "en": "Hey, bee, <sting> the enemy if he attacks!"
                            }
                        },
                        {
                            "text": "Не <жаль> меня, крапива, я просто прохожу мимо.",
                            "target_lang_translations": {
                                "en": "Do not <sting> me, nettle, I am just passing by."
                            }
                        }
                    ],
                    "traits": [
                        {

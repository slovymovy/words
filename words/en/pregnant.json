                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беременная",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "в положении",
                                "target_lang_sense_clarification": "вежливое, идиоматическое выражение"

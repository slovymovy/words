                {
                    "sense_id": "cf833c73-cfb3-3267-a1ba-59fac54f26d8",
                    "sense_definition": "Capable of being seen, heard, or felt easily.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

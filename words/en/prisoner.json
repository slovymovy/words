{
    "word_family": [
        "prison",
        "imprison",
        "imprisonment"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7341d926-2e89-3a36-acb9-7316b94f38cb",
                    "sense_definition": "A person kept in prison as a punishment for a crime or while waiting for trial.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "incarceration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>prisoner</w> was released early for good behavior.",
                            "target_lang_translations": {
                                "ru": "<w>Заключённого</w> освободили досрочно за хорошее поведение."
                            }
                        },
                        {
                            "text": "Guards check the cells to ensure every <w>prisoner</w> is accounted for.",
                            "target_lang_translations": {
                                "ru": "Охранники проверяют камеры, чтобы убедиться, что каждый <w>заключённый</w> на месте."
                            }
                        }
                    ],
                    "synonyms": [
                        "inmate",
                        "jailbird",
                        "guest of Her Majesty"
                    ],
                    "common_phrases": [
                        "political prisoner",
                        "prisoner swap",
                        "model prisoner",
                        "remand prisoner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, находящийся в тюрьме в качестве наказания за преступление или в ожидании суда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заключённый",
                                "target_lang_sense_clarification": "человек, лишенный свободы по приговору суда"
                            },
                            {
                                "target_lang_word": "арестант",
                                "target_lang_sense_clarification": "разг., человек, содержащийся под арестом"
                            },
                            {
                                "target_lang_word": "узник",
                                "target_lang_sense_clarification": "книжн., человек, находящийся в заточении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45feebf1-469c-36bb-bb3f-e35dd0cde2dc",
                    "sense_definition": "A person captured and kept confined against their will, especially in a war.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "capture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers took the enemy captain <w>prisoner</w> after the battle.",
                            "target_lang_translations": {
                                "ru": "Солдаты взяли вражеского капитана в <w>плен</w> после битвы."
                            }
                        },
                        {
                            "text": "He was held <w>prisoner</w> in a basement for three weeks.",
                            "target_lang_translations": {
                                "ru": "Его держали <w>пленником</w> в подвале три недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "guest of Her Majesty"
                    ],
                    "common_phrases": [
                        "prisoner of war",
                        "take prisoner",
                        "hold prisoner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, захваченный и удерживаемый против воли, особенно во время войны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пленный",
                                "target_lang_sense_clarification": "военнопленный, захваченный врагом"
                            },
                            {
                                "target_lang_word": "пленник",
                                "target_lang_sense_clarification": "человек, насильственно удерживаемый в неволе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "757e2260-9716-315b-a96a-763935d6170d",
                    "sense_definition": "A person who feels trapped by a difficult situation, emotion, or set of circumstances.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_trap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He became a <w>prisoner</w> of his own success and could not live a normal life.",
                            "target_lang_translations": {
                                "ru": "Он стал <w>пленником</w> собственного успеха и не мог жить нормальной жизнью."
                            }
                        },
                        {
                            "text": "She refused to be a <w>prisoner</w> to fear and decided to travel alone.",
                            "target_lang_translations": {
                                "ru": "Она отказалась быть <w>пленницей</w> страха и решила путешествовать в одиночку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prisoner of conscience",
                        "prisoner of hope",
                        "prisoner of the past"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, чувствующий себя в ловушке сложной ситуации, эмоций или обстоятельств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пленник",
                                "target_lang_sense_clarification": "перен., человек, зависимый от чего-либо"
                            },
                            {
                                "target_lang_word": "узник",
                                "target_lang_sense_clarification": "перен., жертва обстоятельств"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "tikken",
        "tik",
        "aantikken",
        "aftikken",
        "intikken"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "85012dd0-f347-3fee-86ed-e811c1d48c82",
                    "sense_definition": "Verdergaan met tikken, vaak gebruikt om het onverbiddelijk verstrijken van de tijd aan te duiden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "continuing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude klok in de gang <w>tikt</w> rustig <w>door</w>.",
                            "target_lang_translations": {
                                "en": "The old clock in the hallway quietly <w>ticks on</w>.",
                                "ru": "Старые часы в коридоре спокойно <w>продолжают тикать</w>."
                            }
                        },
                        {
                            "text": "Terwijl wij wachten op het besluit, <w>tikt</w> de tijd genadeloos <w>door</w>.",
                            "target_lang_translations": {
                                "en": "While we wait for the decision, time mercilessly <w>ticks away</w>.",
                                "ru": "Пока мы ждем решения, время безжалостно <w>тикает</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorgaan"
                    ],
                    "antonyms": [
                        "stoppen",
                        "stilvallen"
                    ],
                    "common_phrases": [
                        "de tijd tikt door",
                        "de klok tikt door"
                    ],
                    "target_lang_definitions": {
                        "en": "To continue ticking, often used to indicate the relentless passing of time.",
                        "ru": "Продолжать тикать; часто используется для обозначения неумолимого течения времени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tick on",
                                "target_lang_sense_clarification": "continue making a ticking sound"
                            },
                            {
                                "target_lang_word": "tick away",
                                "target_lang_sense_clarification": "pass (of time)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "продолжать тикать",
                                "target_lang_sense_clarification": "о часах или времени"
                            },
                            {
                                "target_lang_word": "тикать",
                                "target_lang_sense_clarification": "издавать ритмичный звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c9b8cda-da1b-3b0a-9940-93a9048479ff",
                    "sense_definition": "Een bal of voorwerp met een korte, lichte aanraking direct doorspelen naar een ander.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De spits besloot de bal direct <w>door</w> te <w>tikken</w> naar zijn teamgenoot.",
                            "target_lang_translations": {
                                "en": "The striker decided to immediately <w>tap</w> the ball <w>on</w> to his teammate.",
                                "ru": "Нападающий решил сразу <w>переправить</w> мяч своему товарищу по команде."
                            }
                        },
                        {
                            "text": "Hij zag de opening en <w>tikte</w> de bal slim <w>door</w> de verdediging heen.",
                            "target_lang_translations": {
                                "en": "He saw the opening and cleverly <w>tapped</w> the ball <w>through</w> the defense.",
                                "ru": "Он увидел брешь и хитро <w>прокинул</w> мяч сквозь защиту."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorspelen",
                        "doorgeven",
                        "kaatsen"
                    ],
                    "antonyms": [
                        "aannemen",
                        "vasthouden"
                    ],
                    "common_phrases": [
                        "de bal doortikken"
                    ],
                    "target_lang_definitions": {
                        "en": "To pass a ball or object directly to another with a short, light touch.",
                        "ru": "Переправить мяч или предмет другому коротким легким касанием."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tap on",
                                "target_lang_sense_clarification": "pass lightly"
                            },
                            {
                                "target_lang_word": "flick on",
                                "target_lang_sense_clarification": "redirect quickly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "переправить",
                                "target_lang_sense_clarification": "мяч касанием"
                            },
                            {
                                "target_lang_word": "прокинуть",
                                "target_lang_sense_clarification": "мяч вперед или сквозь защиту"
                            },
                            {
                                "target_lang_word": "отпасовать",
                                "target_lang_sense_clarification": "передать пас"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6048aa0d-02ba-30c9-b984-97d4d9e662f8",
                    "sense_definition": "Iets breken of beschadigen door er (te hard) tegenaan te tikken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pas op dat je het glas niet <w>doortikt</w> als je proost.",
                            "target_lang_translations": {
                                "en": "Be careful not to <w>crack</w> the glass when you toast.",
                                "ru": "Осторожно, не <w>разбей</w> бокал, когда чокаешься."
                            }
                        },
                        {
                            "text": "Hij wilde het eitje openen, maar heeft de schaal per ongeluk helemaal <w>doorgetikt</w>.",
                            "target_lang_translations": {
                                "en": "He wanted to open the egg, but accidentally <w>cracked through</w> the shell completely.",
                                "ru": "Он хотел открыть яйцо, но случайно полностью <w>расколол</w> скорлупу."
                            }
                        }
                    ],
                    "synonyms": [
                        "stuktikken"
                    ],
                    "target_lang_definitions": {
                        "en": "To break or damage something by tapping against it too hard.",
                        "ru": "Сломать или повредить что-либо, ударив (слишком сильно) по этому предмету."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crack",
                                "target_lang_sense_clarification": "break without separating parts"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "damage by impact"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "разбить",
                                "target_lang_sense_clarification": "ударом, например, стекло"
                            },
                            {
                                "target_lang_word": "расколоть",
                                "target_lang_sense_clarification": "повредить оболочку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e64440a-de51-3d94-a386-f6f0ed1d1dcd",
                    "sense_definition": "Een bekentenis afleggen of informatie verraden over een misdaad.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na uren verhoor begon de verdachte eindelijk <w>door</w> te <w>tikken</w>.",
                            "target_lang_translations": {
                                "en": "After hours of interrogation, the suspect finally started to <w>crack</w>.",
                                "ru": "После многочасового допроса подозреваемый наконец начал <w>колоться</w>."
                            }
                        },
                        {
                            "text": "Hij weigerde <w>door</w> te <w>tikken</w> over zijn medeplichtigen bij de overval.",
                            "target_lang_translations": {
                                "en": "He refused to <w>snitch</w> on his accomplices in the robbery.",
                                "ru": "Он отказался <w>стучать</w> на своих сообщников по ограблению."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorslaan",
                        "bekennen",
                        "klikken"
                    ],
                    "antonyms": [
                        "zwijgen"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Bargoens"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To make a confession or betray information about a crime.",
                        "ru": "Сделать признание или выдать информацию о преступлении (сленг)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crack",
                                "target_lang_sense_clarification": "confess under pressure"
                            },
                            {
                                "target_lang_word": "sing",
                                "target_lang_sense_clarification": "slang for confessing"
                            },
                            {
                                "target_lang_word": "snitch",
                                "target_lang_sense_clarification": "inform on others"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "расколоться",
                                "target_lang_sense_clarification": "признаться под давлением"
                            },
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "доносить на кого-то"
                            },
                            {
                                "target_lang_word": "сдать",
                                "target_lang_sense_clarification": "выдать сообщников"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
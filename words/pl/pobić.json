                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избиений",
                                "target_lang_sense_clarification": "актов насилия"
                            },
                            {
                                "target_lang_word": "побоев",
                                "target_lang_sense_clarification": "телесных повреждений"
                            }
                        ]
                    }
                }
            ]
        }

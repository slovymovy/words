{
    "word_family": [
        "напор",
        "упор"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f1658d6c-f82c-3904-abe4-c9bb8cb1b131",
                    "sense_definition": "Отражение нападения, активное силовое сопротивление противнику.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "resistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наши войска дали решительный <w>отпор</w> наступающему врагу.",
                            "target_lang_translations": {
                                "en": "Our troops put up a decisive <w>resistance</w> against the advancing enemy."
                            }
                        },
                        {
                            "text": "Грабители встретили неожиданный физический <w>отпор</w> со стороны хозяина дома.",
                            "target_lang_translations": {
                                "en": "The robbers met with unexpected physical <w>resistance</w> from the homeowner."
                            }
                        }
                    ],
                    "synonyms": [
                        "сопротивление",
                        "отражение"
                    ],
                    "antonyms": [
                        "капитуляция",
                        "сдача"
                    ],
                    "common_phrases": [
                        "дать отпор",
                        "получить отпор",
                        "вооружённый отпор"
                    ],
                    "target_lang_definitions": {
                        "en": "Repelling an attack; active, forceful resistance to an enemy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resistance",
                                "target_lang_sense_clarification": "active opposition to an attack or force"
                            },
                            {
                                "target_lang_word": "repulse",
                                "target_lang_sense_clarification": "the act of driving back an enemy force"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2f53b59-24e9-3d67-ac65-a738ba424aa4",
                    "sense_definition": "Решительное возражение, резкий отказ или противодействие чьим-либо словам, мнениям или действиям.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "resistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его грубые шутки сразу получили достойный <w>отпор</w> от присутствующих дам.",
                            "target_lang_translations": {
                                "en": "His rude jokes immediately received a fitting <w>rebuff</w> from the ladies present."
                            }
                        },
                        {
                            "text": "Любая попытка изменить правила встречала яростный <w>отпор</w> со стороны сотрудников.",
                            "target_lang_translations": {
                                "en": "Any attempt to change the rules met with furious <w>pushback</w> from the employees."
                            }
                        }
                    ],
                    "synonyms": [
                        "возражение",
                        "противодействие",
                        "отказ"
                    ],
                    "antonyms": [
                        "согласие",
                        "поддержка",
                        "уступка"
                    ],
                    "common_phrases": [
                        "встретить отпор",
                        "резкий отпор",
                        "достойный отпор"
                    ],
                    "target_lang_definitions": {
                        "en": "A decisive objection, sharp refusal, or opposition to someone's words, opinions, or actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rebuff",
                                "target_lang_sense_clarification": "a blunt or abrupt rejection of an offer or advance"
                            },
                            {
                                "target_lang_word": "pushback",
                                "target_lang_sense_clarification": "negative reaction or resistance to a plan or idea"
                            },
                            {
                                "target_lang_word": "rejection",
                                "target_lang_sense_clarification": "refusal to accept or agree to something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
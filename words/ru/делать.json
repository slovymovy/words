    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "963f6c92-16bb-3486-9c8e-cb329d0f394a",
                    "sense_definition": "Заниматься чем-либо, действовать, проводить время за каким-то занятием.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Что ты <делаешь> сегодня вечером после работы?",
                            "target_lang_translations": {
                                "en": "What are you <doing> tonight after work?"
                            }
                        },
                        {
                            "text": "Я ничего не <делаю>, просто отдыхаю дома.",
                            "target_lang_translations": {
                                "en": "I am <doing> nothing, just relaxing at home."
                            }
                        }
                    ],
                    "synonyms": [
                        "действовать"
                    ],
                    "antonyms": [
                        "бездействовать"
                    ],
                    "common_phrases": [
                        "что ты делаешь",
                        "делать нечего",
                        "делать всё возможное"
                    ],
                    "target_lang_definitions": {
                        "en": "To occupy oneself with something, to act, or to spend time on an activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "do",
                                "target_lang_sense_clarification": "general action or activity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "451a4bf4-4125-3b1f-9023-7a94c639f7d8",
                    "sense_definition": "Создавать, изготавливать или производить что-либо (предметы, еду, произведения).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя бабушка <делает> самые вкусные пироги в мире.",
                            "target_lang_translations": {
                                "en": "My grandmother <makes> the most delicious pies in the world."
                            }
                        },
                        {
                            "text": "На этом заводе <делают> современные автомобили.",
                            "target_lang_translations": {
                                "en": "They <make> modern cars at this factory."
                            }
                        }
                    ],
                    "synonyms": [
                        "создавать",
                        "изготовлять",
                        "производить"
                    ],
                    "common_phrases": [
                        "делать деньги",
                        "делать уроки",
                        "делать ужин"
                    ],
                    "target_lang_definitions": {
                        "en": "To create, manufacture, or produce something (objects, food, works)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "to create or produce"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "527d4ef7-493f-3284-8bc5-d80daba75048",
                    "sense_definition": "Совершать действие, названное существительным (используется в устойчивых сочетаниях).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "light_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент часто <делает> ошибки в диктанте по русскому языку.",
                            "target_lang_translations": {
                                "en": "The student often <makes> mistakes in the Russian dictation."
                            }
                        },
                        {
                            "text": "Нам нужно <делать> выбор прямо сейчас.",
                            "target_lang_translations": {
                                "en": "We need to <make> a choice right now."
                            }
                        }
                    ],
                    "synonyms": [
                        "совершать",
                        "выполнять",
                        "осуществлять",
                        "исполнять"
                    ],
                    "common_phrases": [
                        "делать ошибку",
                        "делать выбор",
                        "делать вид",
                        "делать предложение",
                        "делать шаг"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform an action specified by a noun (used in set phrases)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "used with nouns like 'mistake', 'choice', 'decision'"
                            },
                            {
                                "target_lang_word": "do",
                                "target_lang_sense_clarification": "used with nouns like 'homework', 'business', 'exercises'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "812af16d-2043-3d42-b9b5-020f6c55040c",
                    "sense_definition": "Быть причиной изменения состояния или качества; заставлять кого-либо или что-либо стать каким-то.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "causative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Твоя поддержка <делает> меня намного сильнее и увереннее.",
                            "target_lang_translations": {
                                "en": "Your support <makes> me much stronger and more confident."
                            }
                        },
                        {
                            "text": "Эта грустная музыка <делает> фильм очень трогательным.",
                            "target_lang_translations": {
                                "en": "This sad music <makes> the film very touching."
                            }
                        }
                    ],
                    "common_phrases": [
                        "делать счастливым",
                        "делать больно",
                        "делать известным"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a change in state or quality; to make someone or something become a certain way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "causative; to cause to be"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a96d597-5273-30d6-88e7-a20f443d6069",
                    "sense_definition": "Чинить, приводить в рабочее состояние сломанную вещь.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "repair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец весь день <делал> машину в гараже.",
                            "target_lang_translations": {
                                "en": "Father was <fixing> the car in the garage all day."
                            }
                        },
                        {
                            "text": "Мастер обещал <делать> часы быстро и качественно.",
                            "target_lang_translations": {
                                "en": "The master promised to <fix> the watch quickly and well."
                            }
                        }
                    ],
                    "synonyms": [
                        "чинить",
                        "ремонтировать"
                    ],
                    "common_phrases": [
                        "делать ремонт",
                        "делать машину"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in spoken language instead of 'ремонтировать'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To repair or bring a broken item back into working condition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fix",
                                "target_lang_sense_clarification": "to repair"
                            },
                            {
                                "target_lang_word": "repair",
                                "target_lang_sense_clarification": "to restore to good condition"
                            }
                        ]
                    }
                }
            ]
        }
    ]

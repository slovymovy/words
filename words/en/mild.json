                {
                    "sense_id": "d0c29d6f-7f7c-33e6-bdbe-44c49a7692d3",
                    "sense_definition": "Not sharp, spicy, or bitter; having a gentle flavor.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "taste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I prefer a <w>mild</w> curry because I cannot eat spicy food.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю <w>неострый</w> карри, потому что не могу есть острую пищу."
                            }
                        },
                        {
                            "text": "This cheese has a very <w>mild</w> flavor that children usually like.",
                            "target_lang_translations": {
                                "ru": "У этого сыра очень <w>мягкий</w> вкус, который обычно нравится детям."
                            }
                        }
                    ],
                    "synonyms": [
                        "bland"
                    ],
                    "antonyms": [
                        "strong",
                        "harsh",
                        "severe"
                    ],
                    "common_phrases": [
                        "mild flavor",
                        "mild taste",
                        "mild cheese",
                        "mild sauce",
                        "mild curry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не острый, не пряный и не горький; обладающий мягким, нежным вкусом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неострый",
                                "target_lang_sense_clarification": "о еде, соусах, приправах"
                            },
                            {
                                "target_lang_word": "мягкий",
                                "target_lang_sense_clarification": "о вкусе сыра, вина, табака"
                            }
                        ]
                    }
                },
                {

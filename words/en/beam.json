{
    "word_family": [
        "sunbeam",
        "moonbeam",
        "beamer",
        "high-beam",
        "low-beam"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b065ca00-57c6-348f-a6aa-9c4f32cd36ad",
                    "sense_definition": "A ray or shaft of light or radiation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A bright <w>beam</w> of sunlight shone through the window.",
                            "target_lang_translations": {
                                "ru": "Яркий <w>луч</w> солнечного света светил в окно."
                            }
                        },
                        {
                            "text": "The lighthouse cast a powerful <w>beam</w> across the dark ocean.",
                            "target_lang_translations": {
                                "ru": "Маяк отбрасывал мощный <w>луч</w> через тёмный океан."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beam of light",
                        "laser beam",
                        "high beam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Луч или поток света или излучения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "луч",
                                "target_lang_sense_clarification": "света"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21ab69e4-dab1-3316-b1a3-d6894b8ed610",
                    "sense_definition": "A long, sturdy piece of squared timber, metal, or concrete used to support the roof or floor of a building.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old barn has a massive oak <w>beam</w> running across the ceiling.",
                            "target_lang_translations": {
                                "ru": "В старом амбаре есть массивная дубовая <w>балка</w>, проходящая через весь потолок."
                            }
                        },
                        {
                            "text": "Construction workers lifted the steel <w>beam</w> into place with a crane.",
                            "target_lang_translations": {
                                "ru": "Строители подняли стальную <w>балку</w> на место с помощью крана."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wooden beam",
                        "steel beam",
                        "support beam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный, прочный кусок бруса, металла или бетона, используемый для поддержки крыши или пола здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балка",
                                "target_lang_sense_clarification": "строительная"
                            },
                            {
                                "target_lang_word": "брус",
                                "target_lang_sense_clarification": "деревянный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc93f408-7ed4-34d9-a954-e69a64310db6",
                    "sense_definition": "A narrow, raised horizontal bar used in gymnastics for balancing exercises; short for balance beam.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gymnast performed a perfect backflip on the <w>beam</w>.",
                            "target_lang_translations": {
                                "ru": "Гимнастка выполнила идеальное сальто назад на <w>бревне</w>."
                            }
                        },
                        {
                            "text": "She practiced her routine on the <w>beam</w> for hours.",
                            "target_lang_translations": {
                                "ru": "Она часами отрабатывала свою программу на <w>бревне</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "balance beam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкий, приподнятый горизонтальный брус, используемый в гимнастике для упражнений на равновесие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бревно",
                                "target_lang_sense_clarification": "гимнастический снаряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3177bac9-b40f-3de3-be7e-d1e084639c60",
                    "sense_definition": "A broad, happy smile.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She had a wide <w>beam</w> on her face after winning the prize.",
                            "target_lang_translations": {
                                "ru": "На её лице была широкая <w>улыбка</w> после получения приза."
                            }
                        },
                        {
                            "text": "His face lit up with a <w>beam</w> of pure delight.",
                            "target_lang_translations": {
                                "ru": "Его лицо озарилось <w>улыбкой</w> чистого восторга."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Широкая, счастливая улыбка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сияющая улыбка",
                                "target_lang_sense_clarification": "выражение лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e44ef31-e3fd-3f8b-acfe-ba2816b0b6b2",
                    "sense_definition": "The maximum width of a ship or boat.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The yacht has a wide <w>beam</w>, making it very stable in rough water.",
                            "target_lang_translations": {
                                "ru": "У яхты большая <w>ширина</w>, что делает её очень устойчивой в бурной воде."
                            }
                        },
                        {
                            "text": "This ship has more <w>beam</w> than the previous model.",
                            "target_lang_translations": {
                                "ru": "У этого корабля больше <w>ширины</w>, чем у предыдущей модели."
                            }
                        }
                    ],
                    "synonyms": [
                        "breadth"
                    ],
                    "common_phrases": [
                        "broad in the beam"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "nautical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Максимальная ширина корабля или лодки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ширина",
                                "target_lang_sense_clarification": "судна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7325ec53-39b4-36e5-89d2-c275926049fc",
                    "sense_definition": "A transverse structural member of a ship's frame that supports the deck.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The storm threw the vessel on her <w>beam</w> ends, causing panic among the crew.",
                            "target_lang_translations": {
                                "ru": "Шторм положил судно на <w>бимсы</w>, вызвав панику среди экипажа."
                            }
                        },
                        {
                            "text": "Carpenters repaired the damaged <w>beam</w> below the main deck.",
                            "target_lang_translations": {
                                "ru": "Плотники ремонтировали повреждённый <w>бимс</w> под главной палубой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on her beam ends"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "nautical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поперечный конструктивный элемент каркаса судна, поддерживающий палубу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бимс",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58692eaa-385b-3a12-80ba-b34a52715054",
                    "sense_definition": "A heavy iron lever in early steam engines that transfers motion from the piston to the crank.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "machinery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The massive <w>beam</w> rocked back and forth as the engine pumped water.",
                            "target_lang_translations": {
                                "ru": "Массивный <w>балансир</w> качался взад-вперёд, пока двигатель качал воду."
                            }
                        },
                        {
                            "text": "Engineers inspected the pivot point of the working <w>beam</w>.",
                            "target_lang_translations": {
                                "ru": "Инженеры осмотрели точку опоры рабочего <w>балансира</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "working beam",
                        "walking beam"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical technology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжёлый железный рычаг в ранних паровых двигателях, передающий движение от поршня к кривошипу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балансир",
                                "target_lang_sense_clarification": "деталь двигателя"
                            },
                            {
                                "target_lang_word": "коромысло",
                                "target_lang_sense_clarification": "рычаг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c17986f4-1ee5-32fb-a115-550e454972d4",
                    "sense_definition": "One of the long feathers in the wing of a hawk.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "falconry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The falconer examined the damaged <w>beam</w> on the bird's wing.",
                            "target_lang_translations": {
                                "ru": "Сокольник осмотрел повреждённый <w>ствол</w> пера на крыле птицы."
                            }
                        },
                        {
                            "text": "A healthy <w>beam</w> is essential for the hawk's flight performance.",
                            "target_lang_translations": {
                                "ru": "Здоровое <w>перо</w> необходимо для лётных качеств ястреба."
                            }
                        }
                    ],
                    "synonyms": [
                        "beam feather"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "falconry"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одно из длинных перьев в крыле ястреба."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маховое перо",
                                "target_lang_sense_clarification": "соколиная охота"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4cdfd5bf-9411-3cfe-9cec-40885e15b6c5",
                    "sense_definition": "To smile very happily and broadly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The parents <w>beamed</w> with pride as their son graduated.",
                            "target_lang_translations": {
                                "ru": "Родители <w>сияли</w> от гордости, когда их сын получал диплом."
                            }
                        },
                        {
                            "text": "She <w>beamed</w> at her friend when she heard the good news.",
                            "target_lang_translations": {
                                "ru": "Она <w>улыбнулась</w> подруге, когда услышала хорошие новости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beam with pride",
                        "beam with joy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Улыбаться очень счастливо и широко."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сиять",
                                "target_lang_sense_clarification": "об улыбке"
                            },
                            {
                                "target_lang_word": "лучезарно улыбаться",
                                "target_lang_sense_clarification": "выражение лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51bf73bb-ea39-3889-9799-0eead1f3a24d",
                    "sense_definition": "To emit rays of light; to shine brightly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sun <w>beamed</w> down on the beach all afternoon.",
                            "target_lang_translations": {
                                "ru": "Солнце <w>светило</w> на пляж весь день."
                            }
                        },
                        {
                            "text": "A searchlight <w>beamed</w> into the night sky to attract attention.",
                            "target_lang_translations": {
                                "ru": "Прожектор <w>светил</w> в ночное небо, чтобы привлечь внимание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beam down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испускать лучи света; ярко светить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светить",
                                "target_lang_sense_clarification": "излучать свет"
                            },
                            {
                                "target_lang_word": "бить лучом",
                                "target_lang_sense_clarification": "о направленном свете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "448f80bd-b892-31cb-a671-c4610518da11",
                    "sense_definition": "To transmit a signal, data, or matter (in science fiction) wirelessly.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "transmission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The satellite <w>beams</w> television signals across the world.",
                            "target_lang_translations": {
                                "ru": "Спутник <w>транслирует</w> телевизионные сигналы по всему миру."
                            }
                        },
                        {
                            "text": "In the movie, the aliens <w>beamed</w> the crew onto their ship.",
                            "target_lang_translations": {
                                "ru": "В фильме пришельцы <w>телепортировали</w> экипаж на свой корабль."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beam up",
                        "beam a signal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать сигнал, данные или материю (в научной фантастике) беспроводным способом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "транслировать",
                                "target_lang_sense_clarification": "сигнал"
                            },
                            {
                                "target_lang_word": "телепортировать",
                                "target_lang_sense_clarification": "фантастика"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "14fd4e99-aa9c-3d8e-a5ae-0838d1fecf0a",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Beam</w> signed the contract yesterday.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Бим</w> подписал контракт вчера."
                            }
                        },
                        {
                            "text": "We invited the <w>Beam</w> family to the dinner party.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили семью <w>Бим</w> на званый ужин."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бим",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33d1a93e-9153-3838-80c8-bbd90f2f8abf",
                    "sense_definition": "Acronym of Bogdan's Erlang Abstract Machine.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tech",
                    "name_type": "technology_or_software_name",
                    "examples": [
                        {
                            "text": "Elixir is a functional, dynamic language built on top of Erlang and the Erlang VM (<w>BEAM</w>).",
                            "target_lang_translations": {
                                "ru": "Elixir — это функциональный, динамический язык, созданный на основе Erlang и виртуальной машины Erlang (<w>BEAM</w>)."
                            }
                        },
                        {
                            "text": "Developers optimized the code to run efficiently on the <w>BEAM</w>.",
                            "target_lang_translations": {
                                "ru": "Разработчики оптимизировали код, чтобы он эффективно работал на <w>BEAM</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акроним от Bogdan's Erlang Abstract Machine."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "BEAM",
                                "target_lang_sense_clarification": "технология"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "593f6f84-0f35-333d-92c0-e8ee64a7a171",
                    "sense_definition": "Zgodny z obowiązującym prawem, dozwolony przez przepisy państwowe.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "law_compliance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zarobki tego człowieka są w pełni <w>legalne</w> i opodatkowane.",
                            "target_lang_translations": {
                                "ru": "Доходы этого человека полностью <w>легальные</w> и облагаются налогом."
                            }
                        },
                        {
                            "text": "Jej pobyt w tym kraju jest <w>legalny</w> dzięki ważnej wizie.",
                            "target_lang_translations": {
                                "ru": "Её пребывание в этой стране <w>легальное</w> благодаря действующей визе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgodny z przepisami",
                        "dozwolony prawem",
                        "prawny",
                        "dopuszczalny"
                    ],
                    "antonyms": [
                        "nielegalny",
                        "bezprawny",
                        "nieprawny"
                    ],
                    "common_phrases": [
                        "legalna praca",
                        "legalny pobyt",
                        "legalne źródło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствующий действующему законодательству, разрешённый государственными нормами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "законный",
                                "target_lang_sense_clarification": "общий смысл; соответствующий праву"
                            },
                            {
                                "target_lang_word": "легальный",
                                "target_lang_sense_clarification": "официально разрешённый (о статусе, работе, доходах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abfc07d0-7691-3c1f-9172-1974b0575742",
                    "sense_definition": "Wytworzony, rozpowszechniany lub używany zgodnie z prawem, posiadający odpowiednie licencje (często o oprogramowaniu lub towarach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "law_compliance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sklepach można kupić <w>legalne</w> programy komputerowe z licencją.",
                            "target_lang_translations": {
                                "ru": "В магазинах можно купить <w>легальные</w> компьютерные программы с лицензией."
                            }
                        },
                        {
                            "text": "Tylko <w>legalne</w> kopie filmu gwarantują wysoką jakość obrazu i dźwięku.",
                            "target_lang_translations": {
                                "ru": "Только <w>легальные</w> копии фильма гарантируют высокое качество изображения и звука."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgodny z przepisami",
                        "prawowity"
                    ],
                    "antonyms": [
                        "piracki",
                        "nielegalny"
                    ],
                    "common_phrases": [
                        "legalne oprogramowanie",
                        "legalny system"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произведённый или распространяемый с соблюдением авторских прав, имеющий лицензию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лицензионный",
                                "target_lang_sense_clarification": "о программном обеспечении, фильмах, музыке"
                            },
                            {
                                "target_lang_word": "легальный",
                                "target_lang_sense_clarification": "не пиратский"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88d115ab-7293-3511-b148-6ac95891f6f8",
                    "sense_definition": "Taki, który został ściśle zdefiniowany lub zapisany w tekście aktu prawnego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "law_technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Artykuł ustawy zawiera <w>legalną</w> definicję przedsiębiorcy.",
                            "target_lang_translations": {
                                "ru": "Статья закона содержит <w>легальное</w> определение предпринимателя."
                            }
                        },
                        {
                            "text": "Sąd oparł swój wyrok na <w>legalnej</w> wykładni tego przepisu.",
                            "target_lang_translations": {
                                "ru": "Суд основал своё решение на <w>легальном</w> толковании этого положения."
                            }
                        }
                    ],
                    "synonyms": [
                        "prawny"
                    ],
                    "common_phrases": [
                        "definicja legalna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строго определённый или закреплённый непосредственно в тексте закона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "легальный",
                                "target_lang_sense_clarification": "в юридических терминах (напр. легальное определение)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

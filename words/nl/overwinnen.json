                {
                    "sense_id": "4f213aa1-a6a1-3f69-8d95-809739541418",
                    "sense_definition": "Een moeilijkheid, zwakte of emotie de baas worden of te boven komen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "overcome_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>overwon</w> zijn angst voor het water en nam zwemles.",
                            "target_lang_translations": {
                                "en": "He <w>overcame</w> his fear of water and took swimming lessons.",
                                "ru": "Он <w>поборол</w> свой страх воды и взял уроки плавания."
                            }
                        },
                        {
                            "text": "We moeten samen deze moeilijke periode <w>overwinnen</w>.",
                            "target_lang_translations": {
                                "en": "We must <w>overcome</w> this difficult period together.",
                                "ru": "Мы должны вместе <w>преодолеть</w> этот трудный период."
                            }
                        }
                    ],
                    "synonyms": [
                        "te boven komen",
                        "beheersen",
                        "meester worden"
                    ],
                    "antonyms": [
                        "bezwijken",
                        "opgeven"
                    ],
                    "common_phrases": [
                        "angst overwinnen",
                        "moeilijkheden overwinnen",
                        "zichzelf overwinnen"
                    ],
                    "target_lang_definitions": {
                        "en": "To master or overcome a difficulty, weakness, or emotion.",
                        "ru": "Справиться с трудностью, слабостью или эмоцией; пережить что-то тяжелое."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8e0d5082-020a-32e4-9b3e-fdf896c2a1c3",
                    "sense_definition": "Een dagelijks televisie- of radioprogramma waarin het nieuws wordt gepresenteerd.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "news_program",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We kijken elke avond om acht uur naar het <w>journaal</w>.",
                            "target_lang_translations": {
                                "ru": "Мы каждый вечер в восемь часов смотрим <w>новости</w>.",
                                "en": "We watch <w>the news</w> every evening at eight o'clock."
                            }
                        },
                        {
                            "text": "Heb je het <w>journaal</w> van gisteren al gezien?",
                            "target_lang_translations": {
                                "ru": "Ты уже видел вчерашний <w>выпуск новостей</w>?",
                                "en": "Have you seen yesterday's <w>news broadcast</w> yet?"
                            }
                        }
                    ],
                    "synonyms": [
                        "nieuwsuitzending"
                    ],
                    "common_phrases": [
                        "het acht uur journaal",
                        "op het journaal",
                        "het journaal kijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ежедневная теле- или радиопрограмма, в которой представляются новости.",
                        "en": "A daily television or radio program presenting the news."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "новости",
                                "target_lang_sense_clarification": "телевидение/радио"
                            },
                            {
                                "target_lang_word": "выпуск новостей",
                                "target_lang_sense_clarification": "конкретная передача"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "the news",
                                "target_lang_sense_clarification": "TV/radio program"
                            },
                            {
                                "target_lang_word": "newscast",
                                "target_lang_sense_clarification": "broadcast"
                            },
                            {
                                "target_lang_word": "news broadcast",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eac4e433-a1ae-3def-b451-065da10b1c97",
                    "sense_definition": "Een boek waarin dagelijks gebeurtenissen worden bijgehouden, bijvoorbeeld aan boord van een schip.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "logbook",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kapitein noteerde de positie van het schip in het <w>journaal</w>.",
                            "target_lang_translations": {
                                "ru": "Капитан записал координаты корабля в <w>судовой журнал</w>.",
                                "en": "The captain noted the ship's position in the <w>logbook</w>."
                            }
                        },
                        {
                            "text": "Het is belangrijk om het <w>journaal</w> nauwkeurig bij te houden tijdens de reis.",
                            "target_lang_translations": {
                                "ru": "Важно аккуратно вести <w>бортовой журнал</w> во время путешествия.",
                                "en": "It is important to keep the <w>log</w> accurately during the voyage."
                            }
                        }
                    ],
                    "synonyms": [
                        "logboek",
                        "scheepsjournaal",
                        "scheepslog",
                        "scheepslogboek",
                        "dagboek"
                    ],
                    "common_phrases": [
                        "het journaal bijhouden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Книга, в которую ежедневно записываются события, например, на борту корабля.",
                        "en": "A book in which events are recorded daily, for example on board a ship."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бортовой журнал",
                                "target_lang_sense_clarification": "на судне или самолете"
                            },
                            {
                                "target_lang_word": "судовой журнал",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "logbook",
                                "target_lang_sense_clarification": "nautical/aviation"
                            },
                            {
                                "target_lang_word": "log",
                                "target_lang_sense_clarification": "record of events"
                            },
                            {
                                "target_lang_word": "journal",
                                "target_lang_sense_clarification": "daily record"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d8fc0c6-30ab-300f-8c21-861a7ac99669",
                    "sense_definition": "Een boekhoudkundig register waarin financiële transacties op chronologische volgorde worden vastgelegd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "accounting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De accountant controleerde of alle posten correct in het <w>journaal</w> waren ingevoerd.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер проверил, правильно ли все записи были внесены в <w>журнал</w>.",
                                "en": "The accountant checked if all entries were correctly entered into the <w>journal</w>."
                            }
                        },
                        {
                            "text": "Voor de balansopmaak moeten alle gegevens uit het <w>journaal</w> worden verwerkt.",
                            "target_lang_translations": {
                                "ru": "Для составления баланса необходимо обработать все данные из <w>журнала</w>.",
                                "en": "For the balance sheet preparation, all data from the <w>journal</w> must be processed."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бухгалтерский регистр, в котором финансовые операции записываются в хронологическом порядке.",
                        "en": "An accounting register in which financial transactions are recorded in chronological order."
                    },
                    "translations": {

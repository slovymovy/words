                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кафе",
                                "target_lang_sense_clarification": "общее название заведения общественного питания"
                            },
                            {
                                "target_lang_word": "кофейня",
                                "target_lang_sense_clarification": "заведение, специализирующееся на кофе и десертах"
                            }

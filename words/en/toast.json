    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "abcafa06-1e59-395d-9c8b-dc13be853a9d",
                    "sense_definition": "Bread that has been cooked lightly and browned.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I ate a piece of <w>toast</w> for breakfast.",
                            "target_lang_translations": {
                                "ru": "Я съел <w>тост</w> на завтрак."
                            }
                        },
                        {
                            "text": "This <w>toast</w> and marmalade tastes heavenly.",
                            "target_lang_translations": {
                                "ru": "Этот <w>тост</w> с мармеладом просто восхитителен."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piece of toast",
                        "burnt toast",
                        "toast and jam",
                        "French toast"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поджаренный до румяной корочки ломтик хлеба."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тост",
                                "target_lang_sense_clarification": "поджаренный хлеб"
                            },
                            {
                                "target_lang_word": "гренка",
                                "target_lang_sense_clarification": "поджаренный ломтик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68e8b331-cf1f-3bad-b019-c1696c27043f",
                    "sense_definition": "A proposed salutation given while drinking alcohol to honor someone or something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_salutation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "At the reception, there were many <w>toasts</w> from the well-wishers.",
                            "target_lang_translations": {
                                "ru": "На приеме прозвучало много <w>тостов</w> от доброжелателей."
                            }
                        },
                        {
                            "text": "He raised his glass to propose a <w>toast</w> to the bride and groom.",
                            "target_lang_translations": {
                                "ru": "Он поднял бокал, чтобы произнести <w>тост</w> за жениха и невесту."
                            }
                        }
                    ],
                    "synonyms": [
                        "pledge"
                    ],
                    "common_phrases": [
                        "propose a toast",
                        "make a toast",
                        "drink a toast"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая поздравительная речь перед распитием спиртного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тост",
                                "target_lang_sense_clarification": "застольная речь"
                            },
                            {
                                "target_lang_word": "здравица",
                                "target_lang_sense_clarification": "торжественное пожелание здоровья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78886a9a-38b0-3eac-b029-55144c929b7d",
                    "sense_definition": "Someone or something destined for trouble, destruction, or failure.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "slang_doomed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If I ever get my hands on the guy that stole my wallet, he’s <w>toast</w>!",
                            "target_lang_translations": {
                                "ru": "Если я доберусь до парня, укравшего мой кошелек, ему <w>конец</w>!"
                            }
                        },
                        {
                            "text": "You are going to be <w>toast</w> if you don't submit your school project on time.",
                            "target_lang_translations": {
                                "ru": "Тебе <w>крышка</w>, если ты не сдашь школьный проект вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "dead duck",
                        "dead meat"
                    ],
                    "common_phrases": [
                        "be toast"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или предмет, обреченный на провал, гибель или неприятности (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конец",
                                "target_lang_sense_clarification": "о безвыходном положении"
                            },
                            {
                                "target_lang_word": "крышка",
                                "target_lang_sense_clarification": "разг. гибель, неудача"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c6fccb6-7374-32c5-bf38-5d2b67f8d402",
                    "sense_definition": "A person or group highly admired or celebrated by a particular community.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_salutation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was the <w>toast</w> of high society after his successful novel.",
                            "target_lang_translations": {
                                "ru": "Он был <w>кумиром</w> высшего общества после выхода его успешного романа."
                            }
                        },
                        {
                            "text": "The young actress quickly became the <w>toast</w> of Broadway.",
                            "target_lang_translations": {
                                "ru": "Молодая актриса быстро стала <w>звездой</w> Бродвея."
                            }
                        }
                    ],
                    "common_phrases": [
                        "toast of the town"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, пользующийся широкой популярностью и восхищением в определенном кругу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кумир",
                                "target_lang_sense_clarification": "предмет обожания"
                            },
                            {
                                "target_lang_word": "любимец",
                                "target_lang_sense_clarification": "тот, кого все любят"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "043f8933-9347-353b-89e7-50fa86908889",
                    "sense_definition": "A transient, informational pop-up notification in a user interface.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The application displays a <w>toast</w> when the file download is complete.",
                            "target_lang_translations": {
                                "ru": "Приложение показывает <w>всплывающее уведомление</w>, когда загрузка файла завершена."
                            }
                        },
                        {
                            "text": "Users found the frequent <w>toast</w> messages distracting while working.",
                            "target_lang_translations": {
                                "ru": "Пользователи считали частые <w>всплывающие сообщения</w> отвлекающими во время работы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "toast notification",
                        "toast message"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Всплывающее информационное сообщение в интерфейсе программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всплывающее уведомление",
                                "target_lang_sense_clarification": "элемент интерфейса"
                            },
                            {
                                "target_lang_word": "тост",
                                "target_lang_sense_clarification": "комп. сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2ca34b14-584d-3b61-af25-8f0fb242ef5c",
                    "sense_definition": "To cook food lightly by browning it via direct exposure to heat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We used to enjoy <w>toasting</w> marshmallows around the campfire.",
                            "target_lang_translations": {
                                "ru": "Нам нравилось <w>жарить</w> маршмэллоу на костре."
                            }
                        },
                        {
                            "text": "Top the dish with cheese and <w>toast</w> it under the grill for a few minutes.",
                            "target_lang_translations": {
                                "ru": "Посыпьте блюдо сыром и <w>подрумяньте</w> его под грилем несколько минут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lightly toast",
                        "toast bread"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слегка обжаривать пищу на огне до появления корочки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поджаривать",
                                "target_lang_sense_clarification": "делать румяным"
                            },
                            {
                                "target_lang_word": "сушить",
                                "target_lang_sense_clarification": "делать сухари или тосты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e23a565-1f4a-339e-ae32-421f89245415",
                    "sense_definition": "To drink a beverage in honor of someone or something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_salutation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>toasted</w> the happy couple many times over the course of the evening.",
                            "target_lang_translations": {
                                "ru": "Мы много раз <w>поднимали тосты</w> за счастливую пару в течение вечера."
                            }
                        },
                        {
                            "text": "Let us <w>toast</w> to your future success and happiness.",
                            "target_lang_translations": {
                                "ru": "Давайте <w>выпьем</w> за ваш будущий успех и счастье."
                            }
                        }
                    ],
                    "common_phrases": [
                        "toast the bride",
                        "toast to success"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпивать в честь кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провозглашать тост",
                                "target_lang_sense_clarification": "говорить речь с бокалом"
                            },
                            {
                                "target_lang_word": "пить за",
                                "target_lang_sense_clarification": "салютовать вином"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7711600d-8a33-318c-8e34-3d26e245f504",
                    "sense_definition": "To warm one's body or body parts thoroughly.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>toasted</w> my feet by the fire after walking in the snow.",
                            "target_lang_translations": {
                                "ru": "Я <w>грел</w> ноги у огня после ходьбы по снегу."
                            }
                        },
                        {
                            "text": "Come inside and <w>toast</w> yourself near the heater.",
                            "target_lang_translations": {
                                "ru": "Заходи внутрь и <w>погрейся</w> у обогревателя."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно нагревать или отогревать тело (обычно у источника тепла)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "греть",
                                "target_lang_sense_clarification": "согревать теплом"
                            },
                            {
                                "target_lang_word": "отогревать",
                                "target_lang_sense_clarification": "избавлять от холода"
                            }
                        ]
                    }
                }
            ]
        },

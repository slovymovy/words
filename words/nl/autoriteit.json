{
    "word_family": [
        "autoritair",
        "autoriseren",
        "autorisatie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9e9e25ef-2a78-3591-ae30-5e244d15a12a",
                    "sense_definition": "De overheid of officiële instanties die de macht hebben om regels te maken en te handhaven.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "government",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De lokale <w>autoriteiten</w> hebben besloten om het evenement af te gelasten vanwege het slechte weer.",
                            "target_lang_translations": {
                                "ru": "Местные <w>власти</w> решили отменить мероприятие из-за плохой погоды."
                            }
                        },
                        {
                            "text": "Van de <w>autoriteiten</w> mag er niet meer in de cafés gerookt worden.",
                            "target_lang_translations": {
                                "ru": "По решению <w>властей</w> в кафе больше нельзя курить."
                            }
                        }
                    ],
                    "synonyms": [
                        "overheid",
                        "gezag"
                    ],
                    "common_phrases": [
                        "lokale autoriteiten",
                        "bevoegde autoriteit",
                        "nationale autoriteiten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правительство или официальные органы, обладающие властью устанавливать правила и обеспечивать их соблюдение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "власти",
                                "target_lang_sense_clarification": "правительство, администрация"
                            },
                            {
                                "target_lang_word": "органы власти",
                                "target_lang_sense_clarification": "официальные учреждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ce7559b-1609-3a3e-8725-f73560b56738",
                    "sense_definition": "Het recht of de macht om beslissingen te nemen en gehoorzaamd te worden; persoonlijk aanzien en overwicht.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar had moeite om zijn <w>autoriteit</w> in de drukke klas te handhaven.",
                            "target_lang_translations": {
                                "ru": "Учителю было трудно поддерживать свой <w>авторитет</w> в шумном классе."
                            }
                        },
                        {
                            "text": "Hij straalde macht en <w>autoriteit</w> uit toen hij de vergaderzaal binnenkwam.",
                            "target_lang_translations": {
                                "ru": "Он излучал силу и <w>авторитет</w>, когда вошел в переговорную."
                            }
                        }
                    ],
                    "synonyms": [
                        "gezag",
                        "macht",
                        "overwicht",
                        "invloed"
                    ],
                    "common_phrases": [
                        "autoriteit uitstralen",
                        "gezag en autoriteit",
                        "natuurlijke autoriteit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Право или сила принимать решения и требовать подчинения; личный престиж и влияние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "авторитет",
                                "target_lang_sense_clarification": "влияние, уважение"
                            },
                            {
                                "target_lang_word": "власть",
                                "target_lang_sense_clarification": "право управлять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e88e1e3e-c496-3a35-a0ed-1ee5fe931d8d",
                    "sense_definition": "Iemand die heel veel weet van een bepaald onderwerp en daardoor gerespecteerd wordt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "expert",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij wordt wereldwijd gezien als een toonaangevende <w>autoriteit</w> op het gebied van klimaatverandering.",
                            "target_lang_translations": {
                                "ru": "Во всем мире ее считают ведущим <w>авторитетом</w> в области изменения климата."
                            }
                        },
                        {
                            "text": "Hij is een <w>autoriteit</w> op het gebied van wiskunde en heeft vele boeken geschreven.",
                            "target_lang_translations": {
                                "ru": "Он — <w>авторитет</w> в области математики и написал много книг."
                            }
                        }
                    ],
                    "synonyms": [
                        "expert",
                        "deskundige",
                        "specialist",
                        "kenner"
                    ],
                    "antonyms": [
                        "leek"
                    ],
                    "common_phrases": [
                        "een autoriteit zijn",
                        "erkende autoriteit",
                        "toonaangevende autoriteit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обладающий глубокими знаниями в определенной области и пользующийся уважением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "авторитет",
                                "target_lang_sense_clarification": "признанный эксперт"
                            },
                            {
                                "target_lang_word": "эксперт",
                                "target_lang_sense_clarification": "специалист"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
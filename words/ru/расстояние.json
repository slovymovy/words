    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "490052bc-cc07-3ec7-9e37-e9a920cd679f",
                    "sense_definition": "Пространство, разделяющее два объекта, предмета или пункта.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_gap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы находимся на большом <w>расстоянии</w> от города.",
                            "target_lang_translations": {
                                "en": "We are at a great <w>distance</w> from the city."
                            }
                        },
                        {
                            "text": "Водитель должен соблюдать безопасное <w>расстояние</w> между машинами.",
                            "target_lang_translations": {
                                "en": "The driver must keep a safe <w>distance</w> between cars."
                            }
                        }
                    ],
                    "synonyms": [
                        "диста́нция",
                        "промежуток",
                        "удалённость"
                    ],
                    "common_phrases": [
                        "на большом расстоянии",
                        "безопасное расстояние",
                        "измерить расстояние",
                        "держаться на расстоянии"
                    ],
                    "target_lang_definitions": {
                        "en": "The space separating two objects, items, or points."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "distance",
                                "target_lang_sense_clarification": "amount of space between points"
                            },
                            {
                                "target_lang_word": "space",
                                "target_lang_sense_clarification": "physical extent between objects"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed231b8b-4455-350b-b95d-886a85b57810",
                    "sense_definition": "Отрезок пути, который необходимо преодолеть.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "path_length",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Поезд преодолел это <w>расстояние</w> всего за два часа.",
                            "target_lang_translations": {
                                "en": "The train covered this <w>distance</w> in just two hours."
                            }
                        },
                        {
                            "text": "Спортсмен пробежал короткое <w>расстояние</w> очень быстро.",
                            "target_lang_translations": {
                                "en": "The athlete ran a short <w>distance</w> very quickly."
                            }
                        }
                    ],
                    "synonyms": [
                        "дальность"
                    ],
                    "common_phrases": [
                        "пройти расстояние",
                        "расстояние полёта",
                        "короткое расстояние",
                        "преодолеть расстояние"
                    ],
                    "target_lang_definitions": {
                        "en": "A segment of a journey that needs to be covered."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "distance",
                                "target_lang_sense_clarification": "length of a path or journey"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16c1ddf8-6acb-3b20-bbc9-830b55a130b7",
                    "sense_definition": "Существенное различие или несоответствие в уровне развития, социальном положении или состоянии.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_gap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Между богатыми и бедными существует огромное социальное <w>расстояние</w>.",
                            "target_lang_translations": {
                                "en": "There is a huge social <w>distance</w> between the rich and the poor."
                            }
                        },
                        {
                            "text": "Они быстро сократили <w>расстояние</w> в общении и стали друзьями.",
                            "target_lang_translations": {
                                "en": "They quickly closed the <w>distance</w> in their interaction and became friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "диста́нция"
                    ],
                    "common_phrases": [
                        "социальное расстояние",
                        "сократить расстояние",
                        "огромное расстояние"
                    ],
                    "target_lang_definitions": {
                        "en": "A significant difference or discrepancy in development level, social status, or state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "distance",
                                "target_lang_sense_clarification": "social or emotional separation"
                            },
                            {
                                "target_lang_word": "gap",
                                "target_lang_sense_clarification": "difference or disparity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc26c1dc-7228-3be7-b814-d60c83726479",
                    "sense_definition": "Промежуток времени, отделяющий одно событие от другого.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "time_gap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На <w>расстоянии</w> многих лет это событие кажется совсем неважным.",
                            "target_lang_translations": {
                                "en": "At a <w>distance</w> of many years, this event seems completely unimportant."
                            }
                        },
                        {
                            "text": "Между нашими встречами было <w>расстояние</w> в целый месяц.",
                            "target_lang_translations": {
                                "en": "There was an <w>interval</w> of a whole month between our meetings."
                            }
                        }
                    ],
                    "synonyms": [
                        "промежуток"
                    ],
                    "common_phrases": [
                        "расстояние в год",
                        "через некоторое расстояние"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An interval of time separating one event from another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "interval",
                                "target_lang_sense_clarification": "period of time"
                            },
                            {
                                "target_lang_word": "distance",
                                "target_lang_sense_clarification": "remoteness in time"
                            }
                        ]
                    }
                }
            ]

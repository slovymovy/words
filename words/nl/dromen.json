    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1c2519f6-806a-3f22-803d-6e4976ce052c",
                    "sense_definition": "Beelden, geluiden of gevoelens ervaren terwijl je slaapt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sleeping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>droom</w> bijna elke nacht over vliegen.",
                            "target_lang_translations": {
                                "en": "I <w>dream</w> about flying almost every night.",
                                "ru": "Мне почти каждую ночь <w>снится</w>, что я летаю."
                            }
                        },
                        {
                            "text": "De kinderen <w>dromen</w> over hun verjaardagscadeaus.",
                            "target_lang_translations": {
                                "en": "The children <w>dream</w> about their birthday presents.",
                                "ru": "Детям <w>снятся</w> подарки на день рождения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dromen over",
                        "vies dromen",
                        "hardop dromen"
                    ],
                    "target_lang_definitions": {
                        "en": "To experience images, sounds, or feelings while sleeping.",
                        "ru": "Видеть образы, слышать звуки или испытывать ощущения во время сна."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dream",
                                "target_lang_sense_clarification": "while sleeping"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "видеть сны",
                                "target_lang_sense_clarification": "смотреть сновидения"
                            },
                            {
                                "target_lang_word": "сниться",
                                "target_lang_sense_clarification": "являться во сне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf186324-9984-388f-8096-562972262cd9",
                    "sense_definition": "Fantaseren over iets dat je graag wilt, of met je gedachten ergens anders zijn.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "imagining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>dromen</w> van een lange vakantie in de zon.",
                            "target_lang_translations": {
                                "en": "We <w>dream</w> of a long holiday in the sun.",
                                "ru": "Мы <w>мечтаем</w> о долгом отпуске на солнце."
                            }
                        },
                        {
                            "text": "Zit niet te <w>dromen</w> en let goed op.",
                            "target_lang_translations": {
                                "en": "Don't sit there <w>daydreaming</w> and pay close attention.",
                                "ru": "Хватит <w>мечтать</w>, будь внимательнее."
                            }
                        }
                    ],
                    "synonyms": [
                        "fantaseren",
                        "dagdromen",
                        "mijmeren"
                    ],
                    "antonyms": [
                        "opletten"
                    ],
                    "common_phrases": [
                        "dromen van",
                        "durven dromen",
                        "zitten te dromen"
                    ],
                    "target_lang_definitions": {
                        "en": "To fantasize about something you want, or to have your thoughts elsewhere.",
                        "ru": "Фантазировать о чем-то желанном или быть погруженным в свои мысли."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dream",
                                "target_lang_sense_clarification": "to hope or wish"
                            },
                            {
                                "target_lang_word": "daydream",
                                "target_lang_sense_clarification": "to be lost in thought"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мечтать",
                                "target_lang_sense_clarification": "сильно желать чего-либо"
                            },
                            {
                                "target_lang_word": "грезить",
                                "target_lang_sense_clarification": "предаваться фантазиям"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "94a812e3-24cf-3135-8eea-d1aee670c771",
                    "sense_definition": "Meervoud van <w>droom</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik hoop dat al je <w>dromen</w> uitkomen.",
                            "target_lang_translations": {
                                "en": "I hope that all your <w>dreams</w> come true.",
                                "ru": "Я надеюсь, что все твои <w>мечты</w> сбудутся."
                            }
                        },
                        {
                            "text": "In mijn <w>dromen</w> woon ik in een groot kasteel.",
                            "target_lang_translations": {
                                "en": "In my <w>dreams</w> I live in a big castle.",
                                "ru": "В своих <w>мечтах</w> я живу в большом замке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mooie dromen",
                        "dromen najagen",
                        "dromen waarmaken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of <w>dream</w>.",
                        "ru": "Множественное число от слова <w>droom</w> (мечта или сон)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dreams",
                                "target_lang_sense_clarification": "plural noun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мечты",
                                "target_lang_sense_clarification": "желания"
                            },
                            {
                                "target_lang_word": "сны",
                                "target_lang_sense_clarification": "сновидения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
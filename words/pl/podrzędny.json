                        "lokal podrzędny",
                        "gatunek podrzędny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличающийся низким качеством или худший по сравнению с другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второсортный",

                {
                    "sense_id": "fe778cb5-2b14-3709-b091-e79b445d336f",
                    "sense_definition": "Narząd wzroku u ludzi i zwierząt, służący do odbierania bodźców świetlnych",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje lewe <w>oko</w> trochę mnie boli.",
                            "target_lang_translations": {
                                "ru": "У меня немного болит левый <w>глаз</w>."
                            }
                        },
                        {
                            "text": "Ona ma piękne, niebieskie <w>oczy</w>.",
                            "target_lang_translations": {
                                "ru": "У неё красивые голубые <w>глаза</w>."
                            }
                        },
                        {
                            "text": "Widać to gołym <w>okiem</w>, nie trzeba lupy.",
                            "target_lang_translations": {
                                "ru": "Это видно невооружённым <w>глазом</w>, лупа не нужна."
                            }
                        }
                    ],
                    "synonyms": [
                        "ślepie",
                        "gała",
                        "oczęta"
                    ],
                    "common_phrases": [
                        "gołym okiem",
                        "w mgnieniu oka",
                        "rzucić okiem",
                        "mieć na oku",
                        "przymknąć oko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Орган зрения у людей и животных, служащий для восприятия световых раздражителей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глаз",
                                "target_lang_sense_clarification": "орган зрения"
                            },
                            {
                                "target_lang_word": "око",
                                "target_lang_sense_clarification": "поэтическое или устаревшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2433a66f-6b95-3e28-95e7-802815baef58",
                    "sense_definition": "Tłusta, okrągła plama pływająca na powierzchni płynu, zazwyczaj zupy",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W rosole pływały wielkie, tłuste <w>oka</w>.",

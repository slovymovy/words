                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "борода́",
                                "target_lang_sense_clarification": "сленг, прикрытие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd282b16-22be-3b8c-8775-a6c72b9f6aed",
                    "sense_definition": "The long, stiff hairs or bristles growing from the ear of certain grains like wheat or barley.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sharp <w>beard</w> of the barley can be irritating if it gets stuck in your clothing.",
                            "target_lang_translations": {
                                "ru": "Острая <w>ость</w> ячменя может колоться, если застрянет в одежде."
                            }
                        },
                        {
                            "text": "Farmers inspect the <w>beard</w> of the wheat to help determine if the crop is ready for harvest.",
                            "target_lang_translations": {
                                "ru": "Фермеры осматривают <w>ость</w> пшеницы, чтобы определить, готов ли урожай к уборке."
                            }
                        }
                    ],
                    "synonyms": [
                        "awn",
                        "bristle"
                    ],
                    "common_phrases": [
                        "beard of grain",
                        "wheat beard"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "botany"
                        }

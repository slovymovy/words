        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3f0a29c4-eba7-396e-99e3-21b84e0278eb",
                    "sense_definition": "Made of the hard building material known as concrete.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children scraped their knees while playing on the rough <w>concrete</w> playground.",
                            "target_lang_translations": {
                                "ru": "Дети ободрали колени, играя на неровной <w>бетонной</w> площадке."
                            }
                        },
                        {
                            "text": "They built a high <w>concrete</w> wall around the perimeter of the prison.",
                            "target_lang_translations": {
                                "ru": "Они построили высокую <w>бетонную</w> стену по периметру тюрьмы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "concrete floor",
                        "concrete wall",
                        "concrete block",
                        "concrete jungle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный из твердого строительного материала, известного как бетон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бето́нный",
                                "target_lang_sense_clarification": "сделанный из бетона"

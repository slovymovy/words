{
    "word_family": [
        "design",
        "designer",
        "redesign"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6fda9813-1e5b-339c-9e1c-4c930c0f973c",
                    "sense_definition": "Simple past and past participle of <w>design</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The architect <w>designed</w> a modern building that uses very little energy.",
                            "target_lang_translations": {
                                "ru": "Архитектор <w>спроектировал</w> современное здание, которое потребляет очень мало энергии."
                            }
                        },
                        {
                            "text": "This software was <w>designed</w> to help students learn mathematics more effectively.",
                            "target_lang_translations": {
                                "ru": "Это программное обеспечение было <w>разработано</w>, чтобы помочь студентам изучать математику более эффективно."
                            }
                        }
                    ],
                    "synonyms": [
                        "planned",
                        "created",
                        "devised"
                    ],
                    "common_phrases": [
                        "designed by",
                        "designed for",
                        "specifically designed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола design (проектировать, разрабатывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спроектировал",
                                "target_lang_sense_clarification": "глагол, прошедшее время; создал проект чего-либо"
                            },
                            {
                                "target_lang_word": "разработал",
                                "target_lang_sense_clarification": "глагол, прошедшее время; создал детальный план или конструкцию"
                            },
                            {
                                "target_lang_word": "предназначен",
                                "target_lang_sense_clarification": "краткое причастие; сделан для определенной цели (обычно с 'для')"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e0c6b87a-1b86-35cf-a657-49a0bef99e70",
                    "sense_definition": "Created or fashioned according to a specific plan or pattern.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>designed</w> layout of the garden ensures that flowers bloom throughout the year.",
                            "target_lang_translations": {
                                "ru": "<w>Спроектированная</w> планировка сада гарантирует, что цветы цветут круглый год."
                            }
                        },
                        {
                            "text": "She wore a custom <w>designed</w> dress that fit her perfectly.",
                            "target_lang_translations": {
                                "ru": "На ней было платье, <w>созданное</w> по индивидуальному заказу, которое сидело на ней идеально."
                            }
                        }
                    ],
                    "synonyms": [
                        "planned",
                        "fashioned",
                        "styled"
                    ],
                    "antonyms": [
                        "random",
                        "accidental"
                    ],
                    "common_phrases": [
                        "custom designed",
                        "well designed",
                        "poorly designed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Созданный или выполненный в соответствии с определенным планом или образцом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спроектированный",
                                "target_lang_sense_clarification": "созданный по проекту"
                            },
                            {
                                "target_lang_word": "разработанный",
                                "target_lang_sense_clarification": "тщательно продуманный и созданный"
                            },
                            {
                                "target_lang_word": "дизайнерский",
                                "target_lang_sense_clarification": "относящийся к художественному конструированию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "952c8288-37c5-3da5-8413-640830d5a7da",
                    "sense_definition": "Intended for a specific purpose or destination; designated.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They finally arrived at the <w>designed</w> location after hours of walking.",
                            "target_lang_translations": {
                                "ru": "После нескольких часов ходьбы они наконец прибыли в <w>намеченное</w> место."
                            }
                        },
                        {
                            "text": "The <w>designed</w> outcome of the project was to increase local employment.",
                            "target_lang_translations": {
                                "ru": "<w>Запланированным</w> результатом проекта было увеличение занятости местного населения."
                            }
                        }
                    ],
                    "synonyms": [
                        "intended",
                        "designated"
                    ],
                    "antonyms": [
                        "unintended"
                    ],
                    "common_phrases": [
                        "designed purpose",
                        "designed end"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для конкретной цели или места назначения; назначенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "намеченный",
                                "target_lang_sense_clarification": "запланированный, выбранный заранее (о месте или цели)"
                            },
                            {
                                "target_lang_word": "предназначенный",
                                "target_lang_sense_clarification": "имеющий определенное назначение"
                            },
                            {
                                "target_lang_word": "запланированный",
                                "target_lang_sense_clarification": "входящий в план"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
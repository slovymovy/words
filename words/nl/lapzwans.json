    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3d97f53a-e4b6-3ffb-812e-f4090474713f",
                    "sense_definition": "Een lui persoon die niets uitvoert en nergens goed voor is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broer is een echte <w>lapzwans</w> die de hele dag op de bank ligt te slapen.",
                            "target_lang_translations": {
                                "en": "My brother is a real <w>slacker</w> who lies sleeping on the couch all day.",
                                "ru": "Мой брат — настоящий <w>бездельник</w>, который весь день спит на диване."
                            }
                        },
                        {
                            "text": "Zeg die luie <w>lapzwans</w> dat hij eindelijk eens werk moet gaan zoeken.",
                            "target_lang_translations": {
                                "en": "Tell that lazy <w>good-for-nothing</w> that he finally needs to go look for work.",
                                "ru": "Скажи этому ленивому <w>лоботрясу</w>, чтобы он наконец пошел искать работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "nietsnut"
                    ],
                    "common_phrases": [
                        "luie lapzwans",
                        "die lapzwans"
                    ],
                    "target_lang_definitions": {
                        "en": "A lazy person who does nothing and is good for nothing.",
                        "ru": "Ленивый человек, который ничего не делает и ни на что не годен."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "good-for-nothing",
                                "target_lang_sense_clarification": "a person who is useless or worthless"
                            },
                            {
                                "target_lang_word": "slacker",
                                "target_lang_sense_clarification": "a person who avoids work or effort"
                            },
                            {
                                "target_lang_word": "layabout",
                                "target_lang_sense_clarification": "a person who habitually does little or no work"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бездельник",
                                "target_lang_sense_clarification": "человек, который ничего не делает"
                            },
                            {
                                "target_lang_word": "лоботряс",
                                "target_lang_sense_clarification": "разговорное; ленивый человек"
                            },
                            {
                                "target_lang_word": "лентяй",
                                "target_lang_sense_clarification": "ленивый человек"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "45222d22-4a40-3756-bd49-d70a83d46b17",
                    "sense_definition": "Materialny przedmiot; coś, co istnieje fizycznie i nie jest żywym organizmem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na stole leży jakaś dziwna <w>rzecz</w>.",
                            "target_lang_translations": {
                                "ru": "На столе лежит какая-то странная <w>вещь</w>."
                            }
                        },
                        {
                            "text": "To jest bardzo cenna <w>rzecz</w>, więc uważaj na nią.",
                            "target_lang_translations": {
                                "ru": "Это очень ценная <w>вещь</w>, так что будь с ней осторожен."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedmiot",
                        "obiekt"
                    ],
                    "common_phrases": [
                        "martwa rzecz",
                        "rzecz materialna",
                        "mała rzecz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материальный предмет; что-то, что существует физически и не является живым организмом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вещь",
                                "target_lang_sense_clarification": "физический объект"
                            },
                            {
                                "target_lang_word": "предмет",
                                "target_lang_sense_clarification": "объект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ffdf57b-dfcc-3c9c-b9be-5ee2e64d4b2e",
                    "sense_definition": "Ubrania, bagaż lub przedmioty osobiste będące czyjąś własnością (zwykle w liczbie mnogiej).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "belongings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę spakować swoje <w>rzeczy</w> przed wyjazdem.",
                            "target_lang_translations": {
                                "ru": "Я должен упаковать свои <w>вещи</w> перед отъездом."
                            }
                        },
                        {
                            "text": "Zostawiłem swoje <w>rzeczy</w> w szatni na dole.",
                            "target_lang_translations": {
                                "ru": "Я оставил свои <w>вещи</w> в раздевалке внизу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spakować rzeczy",
                        "rzeczy osobiste",
                        "pilnować rzeczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одежда, багаж или личные предметы, принадлежащие кому-либо (обычно во множественном числе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вещи",
                                "target_lang_sense_clarification": "личные пожитки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f5bbc97-d55f-33a2-b892-0a7674a3a742",
                    "sense_definition": "Sprawa, problem lub zagadnienie, które należy załatwić lub omówić.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_issue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mamy do omówienia jedną ważną <w>rzecz</w>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно обсудить одну важную <w>вещь</w>."
                            }
                        },
                        {
                            "text": "To nie jest twoja <w>rzecz</w>, więc się nie wtrącaj.",
                            "target_lang_translations": {
                                "ru": "Это не твое <w>дело</w>, так что не вмешивайся."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprawa",
                        "kwestia"
                    ],
                    "common_phrases": [
                        "ważna rzecz",
                        "trudna rzecz",
                        "słuszna rzecz",
                        "pewna rzecz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дело, проблема или вопрос, который необходимо решить или обсудить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дело",
                                "target_lang_sense_clarification": "вопрос, касающийся кого-то"
                            },
                            {
                                "target_lang_word": "вопрос",
                                "target_lang_sense_clarification": "тема для обсуждения"
                            },
                            {
                                "target_lang_word": "вещь",
                                "target_lang_sense_clarification": "обстоятельство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b2d635a-169a-35e8-8bcc-b56e3fff15a9",
                    "sense_definition": "Istota problemu, sedno sprawy; to, o co w danej sytuacji chodzi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "essence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie w tym <w>rzecz</w>, żeby się kłócić, ale żeby znaleźć rozwiązanie.",
                            "target_lang_translations": {
                                "ru": "Не в этом <w>дело</w>, чтобы ссориться, а в том, чтобы найти решение."
                            }
                        },
                        {
                            "text": "Cała <w>rzecz</w> polega na tym, że nie mamy czasu.",
                            "target_lang_translations": {
                                "ru": "Всё <w>дело</w> в том, что у нас нет времени."
                            }
                        }
                    ],
                    "common_phrases": [
                        "w tym rzecz",
                        "rzecz w tym",
                        "grunt to rzecz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суть проблемы, главное; то, в чем заключается смысл ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дело",
                                "target_lang_sense_clarification": "суть ситуации"
                            },
                            {
                                "target_lang_word": "суть",
                                "target_lang_sense_clarification": "главное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95d315f7-6818-318d-8a23-564fd8e35307",
                    "sense_definition": "Wydarzenie, fakt, sytuacja lub zjawisko.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj w naszym mieście stała się straszna <w>rzecz</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера в нашем городе случилась страшная <w>вещь</w>."
                            }
                        },
                        {
                            "text": "To niesłychana <w>rzecz</w>, żeby tak traktować ludzi.",
                            "target_lang_translations": {
                                "ru": "Это неслыханная <w>вещь</w> — так обращаться с людьми."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dziwna rzecz",
                        "niesamowita rzecz",
                        "rzecz jasna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Событие, факт, ситуация или явление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вещь",
                                "target_lang_sense_clarification": "событие, явление"
                            },
                            {
                                "target_lang_word": "случай",
                                "target_lang_sense_clarification": "происшествие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc8b4837-8890-31dd-bee7-ec50e84279e9",
                    "sense_definition": "Utwór artystyczny, literacki lub muzyczny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "artistic_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Orkiestra zagrała piękną <w>rzecz</w> Mozarta.",
                            "target_lang_translations": {
                                "ru": "Оркестр сыграл прекрасную <w>вещь</w> Моцарта."
                            }
                        },
                        {
                            "text": "To świetnie napisana <w>rzecz</w> o historii Polski.",
                            "target_lang_translations": {
                                "ru": "Это отлично написанная <w>вещь</w> об истории Польши."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "w środowisku artystycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Художественное, литературное или музыкальное произведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вещь",
                                "target_lang_sense_clarification": "произведение искусства (разг.)"
                            },
                            {
                                "target_lang_word": "произведение",
                                "target_lang_sense_clarification": "сочинение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f53ab047-a2f9-3db0-8d09-675b368a2d7a",
                    "sense_definition": "Mowa, przemówienie lub rozmowa.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>rzecz</w> była długa i nudna.",
                            "target_lang_translations": {
                                "ru": "Его <w>речь</w> была долгой и скучной."
                            }
                        },
                        {
                            "text": "Wstąpili ze sobą w <w>rzecz</w>.",
                            "target_lang_translations": {
                                "ru": "Они вступили друг с другом в <w>разговор</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mowa"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Речь, выступление или беседа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "речь",
                                "target_lang_sense_clarification": "выступление"
                            }
                        ]
                    }
                }
            ]
        },
        {

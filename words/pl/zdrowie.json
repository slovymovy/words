    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "20b305a2-788d-3bae-92ed-1ce3e4e122ff",
                    "sense_definition": "Stan organizmu, w którym wszystkie funkcje życiowe przebiegają prawidłowo i który jest wolny od chorób.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "health_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz bardziej dbać o swoje <zdrowie>, jedząc owoce i warzywa.",
                            "target_lang_translations": {
                                "ru": "Ты должен больше заботиться о своем <здоровье>, употребляя в пищу фрукты и овощи."
                            }
                        },
                        {
                            "text": "Po długiej rehabilitacji dziadek w końcu wraca do <zdrowia>.",
                            "target_lang_translations": {
                                "ru": "После долгой реабилитации дедушка наконец восстанавливает <здоровье>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprawność"
                    ],
                    "antonyms": [
                        "choroba"
                    ],
                    "common_phrases": [
                        "dbać o zdrowie",
                        "wracać do zdrowia",
                        "końskie zdrowie",
                        "okaz zdrowia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние организма, при котором все жизненные функции выполняются правильно и отсутствуют болезни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здоровье",
                                "target_lang_sense_clarification": "состояние организма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93c7c21f-d5db-3538-81bc-839685527c7f",
                    "sense_definition": "Wypicie alkoholu w intencji czyjejś pomyślności.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "toast_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy goście chętnie wypili <zdrowie> młodej pary.",
                            "target_lang_translations": {
                                "ru": "Все гости охотно выпили за <здоровье> молодой пары."
                            }
                        },
                        {
                            "text": "Gospodarz zaproponował, aby wznieść <zdrowie> solenizanta.",
                            "target_lang_translations": {
                                "ru": "Хозяин предложил выпить за <здоровье> именинника."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wypić zdrowie",
                        "wznieść zdrowie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Употребление алкоголя с пожеланием благополучия кому-либо (тост)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здоровье",
                                "target_lang_sense_clarification": "в тостах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f44af56-cf34-3617-8f99-b96692cb92a2",
                    "sense_definition": "Życie, ocalenie lub bezpieczeństwo (często w kontekście wyjścia cało z opresji).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "life_safety",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz cudem uszedł z bitwy z całym <zdrowiem>.",
                            "target_lang_translations": {
                                "ru": "Солдат чудом вышел из битвы живым и <здоровым>."
                            }
                        },
                        {
                            "text": "W tej niebezpiecznej wyprawie stawką było ich <zdrowie> i życie.",
                            "target_lang_translations": {
                                "ru": "В этой опасной экспедиции на кону стояли их <здоровье> и жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "życie",
                        "ocalenie",
                        "ratunek"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "znaczenie historyczne lub literackie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жизнь, спасение или безопасность (часто в контексте выхода невредимым из опасной ситуации)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жизнь",
                                "target_lang_sense_clarification": "спасение"
                            },
                            {
                                "target_lang_word": "целость",
                                "target_lang_sense_clarification": "безопасность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "f4d65a5a-7253-302a-8026-d6e6747528ef",
                    "sense_definition": "Popularny zwrot używany przy wznoszeniu toastu lub jako życzenie pomyślności (np. po kichnięciu).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "toast_interjection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twoje <zdrowie>! – zawołał wesoło Piotr, podnosząc kieliszek.",
                            "target_lang_translations": {
                                "ru": "За твое <здоровье>! – весело крикнул Петр, поднимая бокал."
                            }
                        },
                        {
                            "text": "Kiedy ktoś kichnie, kultura wymaga, by powiedzieć: na <zdrowie>.",
                            "target_lang_translations": {
                                "ru": "Когда кто-то чихает, вежливость требует сказать: будь <здоров>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "na zdrowie",
                        "twoje zdrowie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Популярная фраза, используемая при тосте или как пожелание благополучия (например, когда кто-то чихнул)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на здоровье",
                                "target_lang_sense_clarification": "ответ на благодарность"
                            },
                            {
                                "target_lang_word": "будь здоров",
                                "target_lang_sense_clarification": "при чихании"
                            },
                            {
                                "target_lang_word": "за здоровье",
                                "target_lang_sense_clarification": "тост"
                            }

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "70578545-52ad-35ca-94bf-7707ef0a4a48",
                    "sense_definition": "Подвергать какому-либо взысканию или неприятному воздействию за вину, проступок или преступление.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители не должны физически <наказывать> своих детей за плохие оценки.",
                            "target_lang_translations": {
                                "en": "Parents should not physically <punish> their children for bad grades."
                            }
                        },
                        {
                            "text": "Государство обязано строго <наказывать> преступников для поддержания порядка.",
                            "target_lang_translations": {
                                "en": "The state is obliged to strictly <punish> criminals to maintain order."
                            }
                        }
                    ],
                    "synonyms": [
                        "карать"
                    ],
                    "antonyms": [
                        "поощрять",
                        "награждать"
                    ],
                    "common_phrases": [
                        "строго наказывать",
                        "наказывать за ошибку",
                        "справедливо наказывать",
                        "наказывать преступника"
                    ],
                    "target_lang_definitions": {
                        "en": "To subject someone to a penalty or unpleasant experience for a fault, offense, or crime."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "punish",
                                "target_lang_sense_clarification": "general term for inflicting a penalty"
                            },
                            {
                                "target_lang_word": "discipline",
                                "target_lang_sense_clarification": "to punish in order to correct behavior"
                            },
                            {
                                "target_lang_word": "penalize",
                                "target_lang_sense_clarification": "to impose a penalty, often in rules or law"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cda0254f-2d2d-35ab-a7ca-9eedfb9da084",
                    "sense_definition": "Давать наказ, поручение или распоряжение; приказывать сделать что-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец перед отъездом строго <наказывал> сыну заботиться о матери.",
                            "target_lang_translations": {
                                "en": "Before leaving, the father strictly <ordered> his son to take care of his mother."
                            }
                        },
                        {
                            "text": "Бабушка всегда <наказывала> нам жить честно и по совести.",
                            "target_lang_translations": {
                                "en": "Grandmother always <instructed> us to live honestly and according to conscience."
                            }
                        }
                    ],
                    "synonyms": [
                        "карать",
                        "карать"
                    ],
                    "common_phrases": [
                        "наказывать детям",
                        "строго наказывать"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "устар."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To give an order, instruction, or command; to tell someone to do something (often as advice or parting instruction)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "instruct",

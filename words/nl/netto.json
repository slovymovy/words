{
    "word_family": [
        "net",
        "netteren"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "642c2542-fe8d-3bab-9501-5862fbc82f03",
                    "sense_definition": "Resterend na aftrek van belastingen, kosten of het gewicht van de verpakking.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>netto</w> gewicht van dit product staat duidelijk op de doos vermeld.",
                            "target_lang_translations": {
                                "en": "The <w>net</w> weight of this product is clearly stated on the box.",
                                "ru": "Вес <w>нетто</w> этого продукта четко указан на коробке."
                            }
                        },
                        {
                            "text": "De <w>netto</w> winst viel dit jaar hoger uit dan verwacht.",
                            "target_lang_translations": {
                                "en": "The <w>net</w> profit turned out higher than expected this year.",
                                "ru": "<w>Чистая</w> прибыль в этом году оказалась выше, чем ожидалось."
                            }
                        }
                    ],
                    "antonyms": [
                        "bruto"
                    ],
                    "common_phrases": [
                        "netto gewicht",
                        "netto winst",
                        "netto inkomen",
                        "netto prijs"
                    ],
                    "target_lang_definitions": {
                        "en": "Remaining after deduction of taxes, costs, or the weight of the packaging.",
                        "ru": "Оставшийся после вычета налогов, расходов или веса упаковки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "net",
                                "target_lang_sense_clarification": "remaining after deductions"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чистый",
                                "target_lang_sense_clarification": "о весе, прибыли или доходе"
                            },
                            {
                                "target_lang_word": "нетто",
                                "target_lang_sense_clarification": "неизменяемое прилагательное, обычно ставится после существительного"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8164f89b-0dd7-349e-854b-88ba96f82254",
                    "sense_definition": "Na aftrek van kosten en belastingen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij verdient <w>netto</w> zo'n vijftigduizend euro per jaar.",
                            "target_lang_translations": {
                                "en": "He earns about fifty thousand euros <w>net</w> per year.",
                                "ru": "Он зарабатывает <w>чистыми</w> около пятидесяти тысяч евро в год."
                            }
                        },
                        {
                            "text": "Weet jij hoeveel je <w>netto</w> overhoudt van je vakantiegeld?",
                            "target_lang_translations": {
                                "en": "Do you know how much you have left <w>net</w> from your holiday pay?",
                                "ru": "Ты знаешь, сколько у тебя останется <w>чистыми</w> от отпускных?"
                            }
                        }
                    ],
                    "synonyms": [
                        "schoon"
                    ],
                    "antonyms": [
                        "bruto"
                    ],
                    "common_phrases": [
                        "netto verdienen",
                        "netto overhouden",
                        "netto ontvangen"
                    ],
                    "target_lang_definitions": {
                        "en": "After deduction of costs and taxes.",
                        "ru": "После вычета расходов и налогов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "net",
                                "target_lang_sense_clarification": "after tax"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чистыми",
                                "target_lang_sense_clarification": "разговорное, о деньгах «на руки»"
                            },
                            {
                                "target_lang_word": "нетто",
                                "target_lang_sense_clarification": "финансовый термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2493023d-4adb-3263-948a-ce0d8aac4e36",
                    "sense_definition": "Zonder het gewicht van de verpakking meegerekend.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na uitpakken bleef er een boekje over dat <w>netto</w> nog geen halve ons woog.",
                            "target_lang_translations": {
                                "en": "After unpacking, a booklet remained that weighed less than half an ounce <w>net</w>.",
                                "ru": "После распаковки осталась книжка, которая весила <w>нетто</w> меньше пятидесяти грамм."
                            }
                        },
                        {
                            "text": "Deze zak aardappelen weegt <w>netto</w> precies vijf kilo.",
                            "target_lang_translations": {
                                "en": "This bag of potatoes weighs exactly five kilos <w>net</w>.",
                                "ru": "Этот мешок картофеля весит <w>нетто</w> ровно пять килограммов."
                            }
                        }
                    ],
                    "antonyms": [
                        "bruto"
                    ],
                    "common_phrases": [
                        "netto wegen"
                    ],
                    "target_lang_definitions": {
                        "en": "Without counting the weight of the packaging.",
                        "ru": "Без учета веса упаковки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "net",
                                "target_lang_sense_clarification": "excluding packaging"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нетто",
                                "target_lang_sense_clarification": "о весе товара без тары"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
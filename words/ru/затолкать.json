    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "12f0f6f1-3b8d-3b56-a317-c59910b07891",
                    "sense_definition": "С силой поместить, сунуть внутрь чего-либо; небрежно положить вещи в тесное пространство.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "push_inside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пассажир с трудом <затолкал> свой тяжёлый чемодан на верхнюю полку поезда.",
                            "target_lang_translations": {
                                "en": "The passenger <shoved> his heavy suitcase onto the train's upper rack with difficulty."
                            }
                        },
                        {
                            "text": "Я быстро <затолкал> грязную одежду в стиральную машину и включил её.",
                            "target_lang_translations": {
                                "en": "I quickly <stuffed> the dirty clothes into the washing machine and turned it on."
                            }
                        }
                    ],
                    "synonyms": [
                        "запихнуть",
                        "засунуть",
                        "втиснуть"
                    ],
                    "antonyms": [
                        "вытащить",
                        "достать"
                    ],
                    "common_phrases": [
                        "затолкать вещи в сумку",
                        "затолкать руки в карманы",
                        "кое-как затолкать"
                    ],
                    "target_lang_definitions": {
                        "en": "To put something inside something else with force; to carelessly put things into a tight space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stuff",
                                "target_lang_sense_clarification": "to put things into a space carelessly or quickly"
                            },
                            {
                                "target_lang_word": "shove",
                                "target_lang_sense_clarification": "to push something somewhere roughly"
                            },
                            {
                                "target_lang_word": "cram",
                                "target_lang_sense_clarification": "to force something into a small space"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "febb523a-a123-386e-b9e0-b279c159c209",
                    "sense_definition": "Толкая, заставить кого-либо войти внутрь помещения или транспортного средства (часто против воли).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "push_inside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охрана грубо <затолкала> шумного посетителя обратно в здание клуба.",
                            "target_lang_translations": {
                                "en": "Security roughly <shoved> the noisy visitor back into the club building."
                            }
                        },
                        {
                            "text": "Толпа людей на платформе буквально <затолкала> нас в вагон метро.",
                            "target_lang_translations": {
                                "en": "The crowd of people on the platform literally <pushed> us into the subway car."
                            }
                        }
                    ],
                    "synonyms": [
                        "втолкнуть",
                        "впихнуть"
                    ],
                    "antonyms": [
                        "вытолкать"
                    ],
                    "common_phrases": [
                        "затолкать в машину",
                        "затолкать в автобус",
                        "силой затолкать"
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone to enter a room or vehicle by pushing (often against their will)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shove",
                                "target_lang_sense_clarification": "to push someone roughly into a place"
                            },
                            {
                                "target_lang_word": "push",
                                "target_lang_sense_clarification": "general action of moving someone by force"
                            },
                            {
                                "target_lang_word": "force",
                                "target_lang_sense_clarification": "to compel someone to enter physically"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17459880-1cbc-3205-8f58-9ed847fc1b92",
                    "sense_definition": "Причинить неудобство, боль или утомление постоянными толчками (обычно в тесной толпе).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "harm_by_pushing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В переполненном автобусе меня совсем <затолкали>, и я едва мог дышать.",
                            "target_lang_translations": {
                                "en": "I was completely <jostled> in the overcrowded bus and could barely breathe."
                            }
                        },
                        {
                            "text": "Будь осторожен на выходе со стадиона, там могут <затолкать> в давке.",
                            "target_lang_translations": {
                                "en": "Be careful at the stadium exit, you could get <crushed> in the crowd."
                            }
                        }
                    ],
                    "synonyms": [
                        "затискать"
                    ],
                    "common_phrases": [
                        "совсем затолкали",
                        "затолкать в толпе",
                        "затолкать локтями"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech describing crowds"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cause inconvenience, pain, or fatigue through constant pushing (usually in a tight crowd)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jostle",
                                "target_lang_sense_clarification": "to knock or push roughly against someone in a crowd"
                            },
                            {
                                "target_lang_word": "push around",
                                "target_lang_sense_clarification": "to push someone repeatedly from different sides"
                            },
                            {
                                "target_lang_word": "crush",
                                "target_lang_sense_clarification": "to squeeze or hurt someone by pressure in a crowd"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4254713b-16d0-3821-8063-5c36fa24009f",
                    "sense_definition": "Начать совершать действие «толкать».",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "start_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Машина заглохла посреди дороги, и водители сзади нетерпеливо <затолкали> свои автомобили в объезд.",
                            "target_lang_translations": {
                                "en": "The car stalled in the middle of the road, and the drivers behind impatiently <started pushing> their cars around it."
                            }
                        },
                        {
                            "text": "Услышав сигнал, рабочие дружно <затолкали> тяжёлую тележку в гору.",
                            "target_lang_translations": {
                                "en": "Hearing the signal, the workers <started pushing> the heavy cart up the hill together."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To begin the action of pushing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start pushing",
                                "target_lang_sense_clarification": "to commence the physical act of pushing"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            {
                                "target_lang_word": "blindness",
                                "target_lang_sense_clarification": "medical condition"
                            },
                            {
                                "target_lang_word": "loss of sight",
                                "target_lang_sense_clarification": "physical ability"
                            }
                        ]
                    }
                }
            ]

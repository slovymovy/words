            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d42d84f8-51a6-3953-916e-08524abbe05b",
                    "sense_definition": "Находящийся в самом конце маршрута или пути (обычно об остановках транспорта).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автобус медленно подъехал на <w>конечную</w> остановку.",
                            "target_lang_translations": {
                                "en": "The bus slowly approached the <w>final</w> stop."
                            }
                        },
                        {
                            "text": "Это <w>конечная</w> станция метро, поезд дальше не идёт.",
                            "target_lang_translations": {
                                "en": "This is the <w>final</w> metro station; the train goes no further."
                            }
                        }
                    ],
                    "synonyms": [
                        "последний"
                    ],
                    "antonyms": [
                        "начальный"
                    ],
                    "common_phrases": [
                        "конечная остановка",
                        "конечная станция"
                    ],
                    "target_lang_definitions": {
                        "en": "Situated at the very end of a route or path (usually referring to transport stops)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "final",
                                "target_lang_sense_clarification": "occurring at the end of a route or series"
                            },
                            {
                                "target_lang_word": "last",
                                "target_lang_sense_clarification": "coming after all others in a sequence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "486357c9-3ada-347a-96c7-ae0dfbc41e18",
                    "sense_definition": "Являющийся результатом какого-либо процесса, действий или работы; итоговый.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "result_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нас интересует только <w>конечный</w> результат вашей работы.",
                            "target_lang_translations": {
                                "en": "We are interested only in the <w>final</w> result of your work."
                            }
                        },
                        {
                            "text": "<w>Конечной</w> целью переговоров было подписание договора.",
                            "target_lang_translations": {
                                "en": "The <w>ultimate</w> goal of the negotiations was the signing of the treaty."
                            }
                        }
                    ],
                    "synonyms": [
                        "оконча́тельный",
                        "заключи́тельный",
                        "результирующий"
                    ],
                    "antonyms": [
                        "начальный",
                        "исходный",
                        "промежуточный"
                    ],
                    "common_phrases": [
                        "конечный результат",
                        "конечная цель",
                        "конечный продукт",
                        "конечный потребитель"
                    ],
                    "target_lang_definitions": {
                        "en": "Being the result of a process, action, or work; concluding."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "final",
                                "target_lang_sense_clarification": "conclusive, decisive, or resulting"
                            },
                            {
                                "target_lang_word": "ultimate",
                                "target_lang_sense_clarification": "representing the furthest possible point, goal, or outcome"
                            },
                            {
                                "target_lang_word": "end",
                                "target_lang_sense_clarification": "used attributively to denote a result or product (e.g., end result)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43047b0e-a7d9-356f-b474-2d63ea5643c8",
                    "sense_definition": "Имеющий предел, границы; не вечный и не бесконечный.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "finite_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все природные ресурсы на нашей планете <w>конечны</w>.",
                            "target_lang_translations": {
                                "en": "All natural resources on our planet are <w>finite</w>."
                            }
                        },
                        {
                            "text": "В математике мы часто работаем с <w>конечным</w> множеством чисел.",
                            "target_lang_translations": {
                                "en": "In mathematics, we often work with a <w>finite</w> set of numbers."
                            }
                        }
                    ],
                    "synonyms": [
                        "ограниченный"
                    ],
                    "antonyms": [
                        "бесконечный",
                        "неограниченный"
                    ],
                    "common_phrases": [
                        "конечное множество",
                        "конечное число"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a limit or boundaries; not eternal or infinite."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "finite",
                                "target_lang_sense_clarification": "having limits or bounds"
                            }
                        ]
                    }
                }
            ]

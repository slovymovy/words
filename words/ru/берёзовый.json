    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cf94b973-2bee-3155-92c6-ee25f7d0a0c8",
                    "sense_definition": "Связанный с деревом берёза, принадлежащий ему или являющийся его частью (листья, ветки, кора).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Осенью <w>берёзовый</w> лист становится ярко-жёлтым и опадает."
                        },
                        {
                            "text": "Для поделок дети собрали в парке сухую <w>берёзовую</w> кору."
                        }
                    ],
                    "common_phrases": [
                        "берёзовый лист",
                        "берёзовая кора",
                        "берёзовая ветка",
                        "берёзовые почки"
                    ]
                },
                {
                    "sense_id": "14788fcd-b4ae-3e9e-a1d1-566b476dca9a",
                    "sense_definition": "Добываемый или получаемый из ствола, почек или древесины берёзы.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждую весну мы покупаем в магазине вкусный <w>берёзовый</w> сок."
                        },
                        {
                            "text": "Натуральный <w>берёзовый</w> дёготь часто используют в лечебных целях."
                        }
                    ],
                    "common_phrases": [
                        "берёзовый сок",
                        "берёзовый дёготь"
                    ]
                },
                {
                    "sense_id": "010860fd-9a26-366d-9236-396619eb9b81",
                    "sense_definition": "Состоящий из берёз (о лесе, роще, посадках).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "composition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За нашей деревней шумит густой <w>берёзовый</w> лес."
                        },
                        {
                            "text": "Художник изобразил на картине светлую <w>берёзовую</w> рощу."
                        }
                    ],
                    "common_phrases": [
                        "берёзовая роща",
                        "берёзовый лес",
                        "берёзовая аллея"
                    ]
                },
                {
                    "sense_id": "fff0ecc2-fc7e-31fd-8588-46490f19734e",
                    "sense_definition": "Сделанный, изготовленный из древесины или веток берёзы.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В русской бане принято париться <w>берёзовым</w> веником."
                        },
                        {
                            "text": "Зимой в деревенском доме печь топят сухими <w>берёзовыми</w> дровами."
                        }
                    ],
                    "common_phrases": [
                        "берёзовый веник",
                        "берёзовые дрова",
                        "берёзовая мебель"
                    ]
                }
            ]
        }

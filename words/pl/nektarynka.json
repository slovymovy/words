    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0acf28fb-1d5f-3a74-85e3-bd125155539f",
                    "sense_definition": "Słodki, jadalny owoc odmiany brzoskwini, charakteryzujący się gładką skórką bez meszku.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "plant_and_fruit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na deser zjadłem soczystą <w>nektarynkę</w>, która była bardzo słodka.",
                            "target_lang_translations": {
                                "ru": "На десерт я съел сочный <w>нектарин</w>, который был очень сладким."
                            }
                        },
                        {
                            "text": "Dojrzała <w>nektarynka</w> smakuje najlepiej w środku lata.",
                            "target_lang_translations": {
                                "ru": "Спелый <w>нектарин</w> вкуснее всего в середине лета."
                            }
                        }
                    ],
                    "synonyms": [
                        "nektaryna"
                    ],
                    "common_phrases": [
                        "słodka nektarynka",
                        "dojrzała nektarynka",
                        "soczysta nektarynka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкий съедобный плод разновидности персика, отличающийся гладкой кожицей без пушка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нектарин",
                                "target_lang_sense_clarification": "плод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dff6e59e-93d8-3d8d-9547-238931f30448",
                    "sense_definition": "Drzewo owocowe będące odmianą brzoskwini zwyczajnej, rodzące owoce o gładkiej skórce.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "plant_and_fruit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym ogrodzie rośnie młoda <w>nektarynka</w>, którą posadziliśmy w zeszłym roku.",
                            "target_lang_translations": {
                                "ru": "В нашем саду растет молодой <w>нектарин</w>, который мы посадили в прошлом году."
                            }
                        },
                        {
                            "text": "Ta odmiana <w>nektarynki</w> jest odporna na niskie temperatury.",
                            "target_lang_translations": {
                                "ru": "Этот сорт <w>нектарина</w> устойчив к низким температурам."
                            }
                        }
                    ],
                    "synonyms": [
                        "nektaryna"
                    ],
                    "common_phrases": [
                        "sadzenie nektarynki",
                        "drzewo nektarynki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плодовое дерево, являющееся разновидностью обыкновенного персика и дающее плоды с гладкой кожицей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нектарин",
                                "target_lang_sense_clarification": "дерево"
                            }
                        ]
                    }
                }
            ]
        }
    ]

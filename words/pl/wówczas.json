            "senses": [
                {
                    "sense_id": "c74243dd-93bd-33d0-a173-090acb7ce4a0",
                    "sense_definition": "W tamtym czasie; w okresie, o którym mowa w danej wypowiedzi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miałem <w>wówczas</w> czternaście lat.",
                            "target_lang_translations": {
                                "ru": "Мне было <w>тогда</w> четырнадцать лет."
                            }
                        },
                        {
                            "text": "Byłem <w>wówczas</w> małym chłopcem, gdy ją po raz pierwszy zobaczyłem.",
                            "target_lang_translations": {
                                "ru": "Я был <w>тогда</w> маленьким мальчиком, когда впервые увидел её."
                            }
                        }
                    ],
                    "synonyms": [
                        "wtedy",
                        "wtenczas",
                        "podówczas"
                    ],
                    "antonyms": [
                        "teraz",
                        "obecnie"
                    ],
                    "common_phrases": [
                        "był wówczas",
                        "mieszkał wówczas",
                        "panował wówczas"
                    ],
                    "target_lang_definitions": {
                        "ru": "В то время; в период, о котором идет речь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тогда",
                                "target_lang_sense_clarification": "в то время"
                            },
                            {
                                "target_lang_word": "в то время",
                                "target_lang_sense_clarification": "в тот период"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c6614ef-9965-37f5-97b3-cbda950705e4",
                    "sense_definition": "W takim przypadku; jako skutek spełnienia określonego warunku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conditional_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Porozmawiamy dopiero <w>wówczas</w>, gdy się uspokoisz.",
                            "target_lang_translations": {
                                "ru": "Мы поговорим только <w>тогда</w>, когда ты успокоишься."
                            }
                        },
                        {
                            "text": "Wydam towar dopiero <w>wówczas</w>, gdy dostanę pieniądze.",
                            "target_lang_translations": {
                                "ru": "Я выдам товар только <w>тогда</w>, когда получу деньги."
                            }
                        },
                        {
                            "text": "Jeszcze dobiorę się do was, a <w>wówczas</w> nie chciałbym być w waszej skórze.",
                            "target_lang_translations": {
                                "ru": "Я ещё доберусь до вас, и <w>тогда</w> я не хотел бы оказаться на вашем месте."
                            }
                        }
                    ],
                    "synonyms": [
                        "wtedy"
                    ],
                    "common_phrases": [
                        "dopiero wówczas",
                        "a wówczas"
                    ],
                    "target_lang_definitions": {
                        "ru": "В таком случае; как следствие выполнения условия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тогда",
                                "target_lang_sense_clarification": "в таком случае"
                            }
                        ]
                    }
                }
            ]
        }

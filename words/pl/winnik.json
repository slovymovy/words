    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "05756075-235e-3fd1-93dc-d36246fcfcce",
                    "sense_definition": "Rodzaj pnącza z rodziny winoroślowatych, często uprawianego jako roślina ozdobna ze względu na dekoracyjne owoce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ogrodzie botanicznym podziwialiśmy pięknie przebarwiający się jesienią <w>winnik</w>.",
                            "target_lang_translations": {
                                "ru": "В ботаническом саду мы любовались красиво меняющим цвет осенью <w>виноградовником</w>."
                            }
                        },
                        {
                            "text": "Ten gatunek to <w>winnik</w> zmienny, który jest popularny w nasadzeniach miejskich.",
                            "target_lang_translations": {
                                "ru": "Этот вид — <w>виноградовник</w> разнолистный, который популярен в городских насаждениях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "winnik zmienny",
                        "owoce winnika",
                        "uprawa winnika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Род лиан из семейства Виноградовые, часто выращиваемый как декоративное растение из-за красивых плодов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виноградовник",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f56e114-d274-3a88-a93b-4861d5e22953",
                    "sense_definition": "Osad gromadzący się na dnie beczek lub naczyń podczas fermentacji i przechowywania wina.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po zakończeniu fermentacji wino należy ostrożnie zlać znad <w>winnika</w>.",
                            "target_lang_translations": {
                                "ru": "После завершения брожения вино следует осторожно слить с <w>винного камня</w>."
                            }
                        },
                        {
                            "text": "Gęsty <w>winnik</w> na dnie butli świadczył o naturalnym procesie produkcji trunku.",
                            "target_lang_translations": {
                                "ru": "Густой <w>осадок</w> на дне бутыли свидетельствовал о естественном процессе производства напитка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "winnik w beczce",
                        "zlać znad winnika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осадок, скапливающийся на дне бочек или сосудов во время брожения и хранения вина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "винный камень",
                                "target_lang_sense_clarification": "осадок"
                            },
                            {
                                "target_lang_word": "осадок",
                                "target_lang_sense_clarification": "в вине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a19e7ff3-c59b-3b43-8118-0dbc56f2e66c",
                    "sense_definition": "Dawna, staropolska nazwa miesiąca października.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starych kronikach miesiąc ten zapisywano często jako <w>winnik</w>.",
                            "target_lang_translations": {
                                "ru": "В старых хрониках этот месяц часто записывали как <w>октябрь</w> (винник)."
                            }
                        },
                        {
                            "text": "Nazwa <w>winnik</w> nawiązuje bezpośrednio do czasu jesiennego winobrania.",
                            "target_lang_translations": {
                                "ru": "Название <w>винник</w> (октябрь) напрямую отсылает к поре осеннего сбора винограда."
                            }
                        }
                    ],
                    "synonyms": [
                        "październik"
                    ],
                    "common_phrases": [
                        "miesiąc winnik"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинное польское название октября."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "октябрь",
                                "target_lang_sense_clarification": "старинное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73431f71-5cc7-3b5c-a935-e673bd7db328",
                    "sense_definition": "Przestarzałe określenie osoby winnej popełnienia wykroczenia lub grzechu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Straż miejska szybko schwytała <w>winnika</w> na gorącym uczynku kradzieży.",
                            "target_lang_translations": {
                                "ru": "Городская стража быстро схватила <w>виновника</w> на месте кражи."
                            }
                        },
                        {
                            "text": "Jako główny <w>winnik</w> całego zamieszania musiał przeprosić zgromadzonych gości.",
                            "target_lang_translations": {
                                "ru": "Как главный <w>виновник</w> всего переполоха, он должен был извиниться перед собравшимися гостями."
                            }
                        }
                    ],
                    "synonyms": [
                        "winowajca",
                        "grzesznik"
                    ],
                    "common_phrases": [
                        "ukarać winnika",
                        "główny winnik"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее обозначение человека, виновного в совершении проступка или греха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виновник",
                                "target_lang_sense_clarification": "виновный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56d7c6b3-35f9-3f1e-a173-5a4ebf5c6ec0",
                    "sense_definition": "Pęk witek lub miotełka używana dawniej w łaźni do smagania ciała w celu pobudzenia krążenia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Łaziebny chwycił <w>winnik</w> i zaczął energicznie smagać plecy klienta.",
                            "target_lang_translations": {
                                "ru": "Банщик схватил <w>веник</w> и начал энергично хлестать спину клиента."
                            }
                        },
                        {
                            "text": "Moczenie <w>winnika</w> w ziołowych naparach wzmacniało zdrowotne działanie kąpieli.",
                            "target_lang_translations": {
                                "ru": "Замачивание <w>веника</w> в травяных настоях усиливало целебное действие бани."
                            }
                        }
                    ],
                    "synonyms": [
                        "chwostak"
                    ],
                    "common_phrases": [
                        "smaganie winnikiem",
                        "winnik brzozowy"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пучок веток или метелка, использовавшаяся в старину в бане для хлестания тела с целью улучшения кровообращения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "веник",
                                "target_lang_sense_clarification": "банный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e12c64c-f804-3505-bb37-6f5fd41faa45",
                    "sense_definition": "Dawny posługacz w łaźni, który zajmował się smaganiem kąpiących się witkami.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczony <w>winnik</w> odpoczywał chwilę przed obsłużeniem kolejnego gościa łaźni.",
                            "target_lang_translations": {
                                "ru": "Уставший <w>банщик</w> отдыхал минуту перед обслуживанием следующего гостя бани."
                            }
                        },
                        {
                            "text": "Bogaci kupcy hojnie wynagradzali <w>winnika</w> za jego ciężką pracę.",
                            "target_lang_translations": {
                                "ru": "Богатые купцы щедро вознаграждали <w>банщика</w> за его тяжелый труд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "praca winnika",
                        "zawołać winnika"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical profession"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Служитель в бане, который занимался парением и хлестанием моющихся вениками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "банщик",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                }
            ]
        }

                },
                {
                    "sense_id": "98803782-9b61-3237-8e18-18c3e653e1c5",
                    "sense_definition": "Highly intelligent.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intelligence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a <w>brilliant</w> scientist who has won many awards.",
                            "target_lang_translations": {
                                "ru": "Она <w>блестящий</w> ученый, завоевавший множество наград."
                            }
                        },
                        {
                            "text": "He has a <w>brilliant</w> mind for solving complex puzzles.",
                            "target_lang_translations": {
                                "ru": "У него <w>блестящий</w> ум для решения сложных головоломок."
                            }
                        }
                    ],
                    "synonyms": [
                        "brainy",
                        "ingenious"
                    ],
                    "common_phrases": [
                        "brilliant mind",
                        "brilliant scientist",
                        "brilliant student"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокоинтеллектуальный, одаренный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блестящий",
                                "target_lang_sense_clarification": "об уме или способностях"
                            },
                            {
                                "target_lang_word": "гениальный",
                                "target_lang_sense_clarification": "выдающийся по уму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf853f1b-2286-3311-9b4c-ff44b91aa023",
                    "sense_definition": "Shining brightly.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>brilliant</w> sunshine warmed the air after the storm.",

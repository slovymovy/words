{
    "word_family": [
        "выключать",
        "выключатель",
        "выключение",
        "отключить",
        "переключить"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "de0f9d16-5746-3b64-821b-1ef890a9e243",
                    "sense_definition": "Прекратить работу прибора, механизма, устройства или подачу освещения, газа, воды.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не забудьте <w>выключить</w> свет, когда будете уходить из дома.",
                            "target_lang_translations": {
                                "en": "Don't forget to <w>turn off</w> the light when you leave the house."
                            }
                        },
                        {
                            "text": "Он <w>выключил</w> двигатель машины и вышел на улицу.",
                            "target_lang_translations": {
                                "en": "He <w>turned off</w> the car engine and got out."
                            }
                        },
                        {
                            "text": "Мама попросила детей <w>выключить</w> телевизор и пойти спать.",
                            "target_lang_translations": {
                                "en": "Mom asked the children to <w>turn off</w> the TV and go to sleep."
                            }
                        }
                    ],
                    "synonyms": [
                        "отключить",
                        "отсоединить",
                        "вырубить"
                    ],
                    "antonyms": [
                        "включить"
                    ],
                    "common_phrases": [
                        "выключить свет",
                        "выключить телефон",
                        "выключить компьютер",
                        "выключить газ",
                        "выключить из розетки"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop the operation of an appliance, mechanism, or device, or to stop the flow of light, gas, or water."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn off",
                                "target_lang_sense_clarification": "general term for stopping a device or flow"
                            },
                            {
                                "target_lang_word": "switch off",
                                "target_lang_sense_clarification": "specifically using a switch"
                            },
                            {
                                "target_lang_word": "shut off",
                                "target_lang_sense_clarification": "often used for engines, gas, or water supplies"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cda0a16-b887-3e05-a044-fa6be78d3725",
                    "sense_definition": "Исключить или удалить кого-либо или что-либо из состава, списка или участия в какой-либо деятельности.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Травма колена надолго <w>выключила</w> спортсмена из тренировочного процесса.",
                            "target_lang_translations": {
                                "en": "The knee injury <w>took</w> the athlete <w>out</w> of the training process for a long time."
                            }
                        },
                        {
                            "text": "Этот пункт решили <w>выключить</w> из итогового списка требований.",
                            "target_lang_translations": {
                                "en": "They decided to <w>exclude</w> this point from the final list of requirements."
                            }
                        }
                    ],
                    "synonyms": [
                        "исключить",
                        "изъять",
                        "отключить"
                    ],
                    "antonyms": [
                        "включить"
                    ],
                    "common_phrases": [
                        "выключить из игры",
                        "выключить из списка",
                        "выключить из работы"
                    ],
                    "target_lang_definitions": {
                        "en": "To exclude or remove someone or something from a group, list, or activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exclude",
                                "target_lang_sense_clarification": "formal removal from a list or group"
                            },
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "general term for taking something out"
                            },
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "to remove from participation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e0916d7-0085-35e9-ba08-49a4ad1adf48",
                    "sense_definition": "Привести кого-либо в бессознательное состояние (обычно сильным ударом).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Боксёр точным ударом <w>выключил</w> соперника уже в первом раунде.",
                            "target_lang_translations": {
                                "en": "The boxer <w>knocked out</w> his opponent with a precise hit in the very first round."
                            }
                        },
                        {
                            "text": "Сильный удар головой о землю мгновенно <w>выключил</w> его из реальности.",
                            "target_lang_translations": {
                                "en": "A strong blow of his head against the ground instantly <w>knocked</w> him <w>out</w> of reality."
                            }
                        }
                    ],
                    "synonyms": [
                        "вырубить"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To render someone unconscious, usually by a strong blow."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "knock out",
                                "target_lang_sense_clarification": "to cause loss of consciousness"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
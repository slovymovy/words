    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0ef68c42-3ca8-331e-a848-1cccf4df88f5",
                    "sense_definition": "Pracownik sklepu zajmujący się obsługą klientów i sprzedażą towarów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "seller_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>sprzedawca</w> jest bardzo uprzejmy i zawsze chętnie doradza.",
                            "target_lang_translations": {
                                "ru": "Этот <w>продавец</w> очень вежлив и всегда охотно помогает советом."
                            }
                        },
                        {
                            "text": "Muszę zapytać <w>sprzedawcę</w>, ile kosztuje ten chleb.",
                            "target_lang_translations": {
                                "ru": "Мне нужно спросить у <w>продавца</w>, сколько стоит этот хлеб."
                            }
                        }
                    ],
                    "synonyms": [
                        "ekspedient"
                    ],
                    "common_phrases": [
                        "miły sprzedawca",
                        "zapytać sprzedawcę",
                        "pomocny sprzedawca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Работник магазина, занимающийся обслуживанием покупателей и продажей товаров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продавец",
                                "target_lang_sense_clarification": "работник магазина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51df3a1c-67b0-310d-a5f2-116d5e96ddd4",
                    "sense_definition": "Osoba lub firma oferująca towary albo usługi na sprzedaż; strona transakcji handlowej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "seller_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Internetowy <w>sprzedawca</w> wysłał zamówioną paczkę natychmiast po wpłacie.",
                            "target_lang_translations": {
                                "ru": "Интернет-<w>продавец</w> отправил заказанную посылку сразу после оплаты."
                            }
                        },
                        {
                            "text": "Każdy uczciwy <w>sprzedawca</w> powinien dbać o jakość swoich produktów.",
                            "target_lang_translations": {
                                "ru": "Каждый честный <w>продавец</w> должен заботиться о качестве своих товаров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sprzedawca samochodów",
                        "sprzedawca internetowy",
                        "uczciwy sprzedawca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или компания, предлагающие товары или услуги на продажу; сторона в торговой сделке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продавец",
                                "target_lang_sense_clarification": "сторона сделки"
                            },
                            {
                                "target_lang_word": "торговец",
                                "target_lang_sense_clarification": "коммерсант"
                            }
                        ]
                    }
                }
            ]
        }
    ]

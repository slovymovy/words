    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b80604ef-ba78-3d84-b8a0-deb5658f53a2",
                    "sense_definition": "One of the natural abilities (sight, hearing, smell, taste, and touch) that people and animals use to perceive the physical world.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dogs have a very keen <w>sense</w> of smell.",
                            "target_lang_translations": {
                                "ru": "У собак очень острое <w>обоняние</w>."
                            }
                        },
                        {
                            "text": "The loud music completely overwhelmed my <w>sense</w> of hearing.",
                            "target_lang_translations": {
                                "ru": "Громкая музыка полностью оглушила мой <w>слух</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sense",
                        "sense"
                    ],
                    "common_phrases": [
                        "sense of smell",
                        "sense of taste",
                        "sixth sense",
                        "five senses"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одно из пяти природных чувств (зрение, слух, обоняние, вкус и осязание), с помощью которых люди и животные воспринимают физический мир."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чу́вство",
                                "target_lang_sense_clarification": "способность восприятия (зрение, слух и т.д.)"
                            },
                            {
                                "target_lang_word": "ощуще́ние",
                                "target_lang_sense_clarification": "процесс восприятия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9a531ac-5444-396b-8428-eace1a8108d1",
                    "sense_definition": "A general feeling, awareness, or understanding of a situation or quality.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a wonderful <w>sense</w> of humor.",
                            "target_lang_translations": {
                                "ru": "У неё прекрасное <w>чувство</w> юмора."
                            }
                        },
                        {
                            "text": "There was a <w>sense</w> of relief when the plane finally landed.",
                            "target_lang_translations": {
                                "ru": "Когда самолёт наконец приземлился, возникло <w>чувство</w> облегчения."
                            }
                        }
                    ],
                    "synonyms": [
                        "sense",
                        "sense"
                    ],
                    "common_phrases": [
                        "sense of humor",
                        "sense of direction",
                        "sense of security",
                        "sense of loss"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общее ощущение, осознание или понимание ситуации, атмосферы или качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чу́вство",
                                "target_lang_sense_clarification": "эмоция или осознание (напр. юмора, вины)"
                            },
                            {
                                "target_lang_word": "ощуще́ние",
                                "target_lang_sense_clarification": "внутреннее переживание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d0601b9-10db-35ee-bd55-95c8b6b41788",
                    "sense_definition": "The quality of having a clear meaning, being logical, or being easy to understand.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "logic_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It makes <w>sense</w> to save some money for emergencies.",
                            "target_lang_translations": {
                                "ru": "Имеет <w>смысл</w> откладывать деньги на случай непредвиденных обстоятельств."
                            }
                        },
                        {
                            "text": "His explanation didn't make any <w>sense</w> to me.",
                            "target_lang_translations": {
                                "ru": "Его объяснение не имело для меня никакого <w>смысла</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sense",
                        "sense"
                    ],
                    "antonyms": [
                        "nonsense"
                    ],
                    "common_phrases": [
                        "make sense",
                        "make sense of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство быть логичным, понятным или иметь ясное значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смысл",
                                "target_lang_sense_clarification": "разумность, логичность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "964959d4-5491-3646-b251-5bd991017598",
                    "sense_definition": "Good practical judgment and the ability to make reasonable decisions.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "logic_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Use your common <w>sense</w> and don't touch the hot stove.",
                            "target_lang_translations": {
                                "ru": "Используй свой <w>здравый смысл</w> и не трогай горячую плиту."
                            }
                        },
                        {
                            "text": "He had the <w>sense</w> to bring an umbrella when he saw the dark clouds.",
                            "target_lang_translations": {
                                "ru": "Ему хватило <w>ума</w> взять зонт, когда он увидел тёмные тучи."
                            }
                        }
                    ],
                    "synonyms": [
                        "sense",
                        "sense"
                    ],
                    "antonyms": [
                        "stupidity",
                        "foolishness"
                    ],
                    "common_phrases": [
                        "common sense",
                        "have the sense to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность рассуждать здраво и принимать разумные практические решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здра́вый смысл",
                                "target_lang_sense_clarification": "практическая рассудительность"
                            },
                            {
                                "target_lang_word": "ум",
                                "target_lang_sense_clarification": "сообразительность"
                            },
                            {
                                "target_lang_word": "ра́зум",
                                "target_lang_sense_clarification": "способность мыслить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c04dbde0-0f12-37e1-a091-bec72c4574dd",
                    "sense_definition": "A specific meaning of a word, phrase, or concept.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "logic_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The word \"bank\" has more than one <w>sense</w> in English.",
                            "target_lang_translations": {
                                "ru": "Слово \"bank\" имеет более одного <w>значения</w> в английском языке."
                            }
                        },
                        {
                            "text": "In a <w>sense</w>, we are all responsible for the environment.",
                            "target_lang_translations": {
                                "ru": "В каком-то <w>смысле</w> мы все несём ответственность за окружающую среду."
                            }
                        }
                    ],
                    "synonyms": [
                        "sense",
                        "sense"
                    ],
                    "common_phrases": [
                        "in a sense",
                        "in the strict sense"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное значение слова, фразы или понятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "значе́ние",
                                "target_lang_sense_clarification": "лингвистическое значение"
                            },
                            {
                                "target_lang_word": "смысл",
                                "target_lang_sense_clarification": "суть, содержание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdb3d63a-6d61-3bb6-a138-6947c36606f9",
                    "sense_definition": "One of two opposite directions in which a force, vector, or rotation can move.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gear rotates in a clockwise <w>sense</w>.",
                            "target_lang_translations": {
                                "ru": "Шестерня вращается в <w>направлении</w> по часовой стрелке."
                            }
                        },
                        {
                            "text": "Vectors have both magnitude and <w>sense</w>.",
                            "target_lang_translations": {
                                "ru": "Векторы имеют как величину, так и <w>направление</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sense",
                        "sense"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одно из двух противоположных направлений, в которых может двигаться сила, вектор или вращение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "направле́ние",
                                "target_lang_sense_clarification": "математический/физический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d63e7bfa-ea81-39c4-ac4c-5165dab26844",
                    "sense_definition": "To become aware of something or feel something, often without seeing or hearing it clearly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I could <w>sense</w> that she was hiding something from me.",
                            "target_lang_translations": {
                                "ru": "Я <w>чувствовал</w>, что она что-то от меня скрывает."
                            }
                        },
                        {
                            "text": "The security system <w>senses</w> motion and turns on the lights.",
                            "target_lang_translations": {
                                "ru": "Система безопасности <w>улавливает</w> движение и включает свет."
                            }
                        }
                    ],
                    "synonyms": [
                        "sense",
                        "sense"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осознавать или ощущать что-либо, часто интуитивно, без явного зрительного или слухового восприятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чу́вствовать",
                                "target_lang_sense_clarification": "ощущать интуитивно или физически"
                            },
                            {
                                "target_lang_word": "ощуща́ть",
                                "target_lang_sense_clarification": "воспринимать органами чувств"
                            },
                            {
                                "target_lang_word": "ула́вливать",
                                "target_lang_sense_clarification": "распознавать (о приборах или чувствах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "target_lang_definitions": {
                        "ru": "Роскошный автомобиль, произведенный в другой стране."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инома́рка",
                                "target_lang_sense_clarification": "разговорное название автомобиля иностранного производства"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
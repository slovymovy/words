    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a2cb5fa2-78ac-36af-8f62-ed5d3294e3e2",
                    "sense_definition": "A central point, such as an airport or station, where many routes meet and traffic or goods are distributed.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "center_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city airport serves as a major <w>hub</w> for international flights.",
                            "target_lang_translations": {
                                "ru": "Городской аэропорт служит крупным <w>узлом</w> для международных рейсов."
                            }
                        },
                        {
                            "text": "Chicago has long been a vital railway <w>hub</w> connecting the east and west.",
                            "target_lang_translations": {
                                "ru": "Чикаго долгое время был важнейшим железнодорожным <w>узлом</w>, соединяющим восток и запад."
                            }
                        }
                    ],
                    "synonyms": [
                        "hub world"
                    ],
                    "common_phrases": [
                        "transport hub",
                        "transit hub",
                        "airline hub",
                        "logistics hub"
                    ],
                    "target_lang_definitions": {
                        "ru": "Центральный пункт (например, аэропорт или вокзал), где сходятся маршруты и распределяются потоки транспорта или грузов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "транспортный узел",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "хаб",
                                "target_lang_sense_clarification": "современный термин, часто в авиации"
                            },
                            {
                                "target_lang_word": "узловой аэропорт",
                                "target_lang_sense_clarification": "в контексте авиации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2206c85-85b3-3a61-838b-7d98e37c197d",
                    "sense_definition": "A device used to connect multiple computers or electronic peripherals together in a network.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "center_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can use a USB <w>hub</w> to connect your mouse, keyboard, and printer to one port.",
                            "target_lang_translations": {
                                "ru": "Вы можете использовать USB-<w>хаб</w> для подключения мыши, клавиатуры и принтера к одному порту."
                            }
                        },
                        {
                            "text": "The technician replaced the old network <w>hub</w> to improve the connection speed.",
                            "target_lang_translations": {
                                "ru": "Техник заменил старый сетевой <w>концентратор</w>, чтобы улучшить скорость соединения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "USB hub",
                        "network hub"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство для соединения нескольких компьютеров или периферийных устройств в сеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "концентратор",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "хаб",
                                "target_lang_sense_clarification": "профессиональный жаргон"
                            },
                            {
                                "target_lang_word": "разветвитель",
                                "target_lang_sense_clarification": "часто для USB"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2fe770b-3123-3ef7-b7d7-619bda6025e4",
                    "sense_definition": "A central place or facility of great activity, interest, or importance.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "center_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silicon Valley is known as a global <w>hub</w> for technology and innovation.",
                            "target_lang_translations": {
                                "ru": "Кремниевая долина известна как мировой <w>центр</w> технологий и инноваций."
                            }
                        },
                        {
                            "text": "The new community center became a vibrant <w>hub</w> of social life for the neighborhood.",
                            "target_lang_translations": {
                                "ru": "Новый общественный центр стал оживленным <w>средоточием</w> общественной жизни района."
                            }

{
    "word_family": [
        "ойкать",
        "ойканье"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "8aa08459-23c5-3257-b962-df3168f2a28e",
                    "sense_definition": "Выражение внезапной физической боли или резкого дискомфорта.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pain_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ой</w>, я сильно ударилась локтем об стол!",
                            "target_lang_translations": {
                                "en": "<w>Ouch</w>, I hit my elbow hard against the table!"
                            }
                        },
                        {
                            "text": "<w>Ой</w>, как больно наступать на эту ногу.",
                            "target_lang_translations": {
                                "en": "<w>Ow</w>, it hurts so much to step on this foot."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ой-ой-ой",
                        "ой больно"
                    ],
                    "target_lang_definitions": {
                        "en": "Expression of sudden physical pain or sharp discomfort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ouch",
                                "target_lang_sense_clarification": "standard expression of sudden pain"
                            },
                            {
                                "target_lang_word": "ow",
                                "target_lang_sense_clarification": "short expression of sharp pain"
                            },
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "general exclamation often used for pain"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2366f17a-9414-34fd-a0f4-be8416dbe5dc",
                    "sense_definition": "Выражение испуга, страха или внезапного удивления.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ой</w>, ты меня так напугал, не подходи так тихо!",
                            "target_lang_translations": {
                                "en": "<w>Oh</w>, you scared me so much, don't sneak up like that!"
                            }
                        },
                        {
                            "text": "<w>Ой</w>, мамочки, что это за страшный звук?",
                            "target_lang_translations": {
                                "en": "<w>Oh</w> my, what is that scary sound?"
                            }
                        }
                    ],
                    "synonyms": [
                        "мамочки"
                    ],
                    "common_phrases": [
                        "ой мамочки",
                        "ой господи"
                    ],
                    "target_lang_definitions": {
                        "en": "Expression of fright, fear, or sudden surprise."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "expression of surprise or fear"
                            },
                            {
                                "target_lang_word": "oh my",
                                "target_lang_sense_clarification": "expression of shock or concern"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b692ee7b-f96a-3a2a-9d35-818481faa15d",
                    "sense_definition": "Выражение сожаления при осознании допущенной ошибки, оплошности или неловкости.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mistake_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ой</w>, я случайно уронила твою кружку на пол.",
                            "target_lang_translations": {
                                "en": "<w>Oops</w>, I accidentally dropped your mug on the floor."
                            }
                        },
                        {
                            "text": "<w>Ой</w>, извините, я кажется набрал неправильный номер.",
                            "target_lang_translations": {
                                "en": "<w>Oh</w>, sorry, I think I dialed the wrong number."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ой прости",
                        "ой забыл"
                    ],
                    "target_lang_definitions": {
                        "en": "Expression of regret upon realizing a mistake, blunder, or awkwardness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oops",
                                "target_lang_sense_clarification": "acknowledging a minor mistake or accident"
                            },
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "realization of an error, often preceding an apology"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae995e44-d6e2-3f5c-be94-5ca9d3bedc24",
                    "sense_definition": "Используется для эмоционального усиления значения следующего слова, подчёркивая высокую степень признака или действия.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На улице сегодня <w>ой</w> какой сильный мороз.",
                            "target_lang_translations": {
                                "en": "It is <w>oh</w> so freezing outside today."
                            }
                        },
                        {
                            "text": "<w>Ой</w> как мне не хочется вставать так рано утром!",
                            "target_lang_translations": {
                                "en": "<w>Oh</w> how I don't want to get up so early in the morning!"
                            }
                        }
                    ],
                    "synonyms": [
                        "ух"
                    ],
                    "common_phrases": [
                        "ой как",
                        "ой какой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to emotionally emphasize the meaning of the following word, highlighting a high degree of a trait or action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "used for emphasis, often in phrases like 'oh how' or 'oh so'"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c345d4b5-c202-36ef-9013-c09af9e83392",
                    "sense_definition": "Het uiterlijk dat iets of iemand heeft, vaak in tegenstelling tot de werkelijkheid; een bedrieglijke indruk.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze dacht in de verte kunstlicht te zien, maar het was slechts <w>schijn</w>.",
                            "target_lang_translations": {
                                "en": "She thought she saw artificial light in the distance, but it was merely an <w>illusion</w>.",
                                "ru": "Ей показалось, что она видит вдали искусственный свет, но это была лишь <w>иллюзия</w>."
                            }
                        },
                        {
                            "text": "Hij probeerde de <w>schijn</w> op te houden dat hij gelukkig was, ondanks zijn verdriet.",
                            "target_lang_translations": {
                                "en": "He tried to keep up the <w>appearance</w> that he was happy, despite his grief.",
                                "ru": "Он пытался создать <w>видимость</w>, что счастлив, несмотря на своё горе."
                            }
                        }
                    ],
                    "synonyms": [
                        "uiterlijk",
                        "voorkomen",
                        "illusie"
                    ],
                    "antonyms": [
                        "werkelijkheid",
                        "realiteit"
                    ],
                    "common_phrases": [
                        "de schijn ophouden",
                        "schijn bedriegt",
                        "voor de schijn"
                    ],
                    "target_lang_definitions": {
                        "en": "The outward look of something or someone, often contrasting with reality; a deceptive impression.",
                        "ru": "Внешний вид чего-либо или кого-либо, часто противоречащий действительности; обманчивое впечатление."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appearance",
                                "target_lang_sense_clarification": "the way something looks to others, often hiding the truth"
                            },
                            {
                                "target_lang_word": "semblance",
                                "target_lang_sense_clarification": "an outward appearance or apparent form of something"
                            },
                            {
                                "target_lang_word": "illusion",
                                "target_lang_sense_clarification": "a deceptive appearance or impression"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "видимость",
                                "target_lang_sense_clarification": "внешнее впечатление, часто не соответствующее истине"
                            },
                            {
                                "target_lang_word": "иллюзия",
                                "target_lang_sense_clarification": "обманчивое восприятие действительности"
                            },
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "внешность или облик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9ae7e9c-0279-32fc-8649-65c4df202913",
                    "sense_definition": "Een zacht licht of een gloed; een zwakke lichtuitstraling.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de <w>schijn</w> van de kaars zag ik de tranen op haar gezicht.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "98a78d0a-1867-3193-bd4a-dbec3300373a",
                    "sense_definition": "The act of encouraging interest or activity, or the feeling of being excited and interested.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new job offers the intellectual <w>stimulation</w> I was missing before.",
                            "target_lang_translations": {
                                "ru": "Новая работа предлагает интеллектуальную <w>стимуляцию</w>, которой мне раньше не хватало."
                            }
                        },
                        {
                            "text": "Children need plenty of mental <w>stimulation</w> to help their brains develop.",
                            "target_lang_translations": {
                                "ru": "Детям необходимо много умственной <w>стимуляции</w>, чтобы помочь их мозгу развиваться."
                            }
                        }
                    ],
                    "synonyms": [
                        "encouragement",
                        "inspiration",
                        "motivation"
                    ],
                    "antonyms": [
                        "boredom",
                        "discouragement"
                    ],
                    "common_phrases": [
                        "intellectual stimulation",
                        "mental stimulation",
                        "provide stimulation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, направленное на пробуждение интереса или активности, или ощущение воодушевления и заинтересованности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стимуляция",
                                "target_lang_sense_clarification": "активизация умственной деятельности или интереса"
                            },
                            {
                                "target_lang_word": "стимулирование",
                                "target_lang_sense_clarification": "процесс побуждения к действию или развитию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d42b13bc-564d-3ba8-b8ec-b60a3bdc4b01",
                    "sense_definition": "The application of a stimulus to a body part or organism to produce a reaction.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The therapy involves electrical <w>stimulation</w> of the muscles to improve strength.",
                            "target_lang_translations": {
                                "ru": "Терапия включает электрическую <w>стимуляцию</w> мышц для увеличения силы."
                            }
                        },
                        {
                            "text": "Babies respond well to visual <w>stimulation</w> like bright colors and moving objects.",
                            "target_lang_translations": {
                                "ru": "Младенцы хорошо реагируют на зрительную <w>стимуляцию</w>, такую как яркие цвета и движущиеся предметы."
                            }
                        }
                    ],
                    "synonyms": [
                        "excitation",
                        "provocation"
                    ],
                    "antonyms": [
                        "inhibition"
                    ],
                    "common_phrases": [
                        "sensory stimulation",
                        "electrical stimulation",
                        "visual stimulation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздействие раздражителя на часть тела или организм для вызова ответной реакции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стимуляция",
                                "target_lang_sense_clarification": "физиологическое или медицинское воздействие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

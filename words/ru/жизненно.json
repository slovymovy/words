                        "неправдоподобно",
                        "искусственно"
                    ],
                    "common_phrases": [
                        "очень жизненно",
                        "получилось жизненно"
                    ],
                    "target_lang_definitions": {
                        "en": "Truthfully, authentically, in full accordance with reality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "realistically",
                                "target_lang_sense_clarification": "in a way that demonstrates a realistic outlook or portrayal"
                            },
                            {
                                "target_lang_word": "true to life",
                                "target_lang_sense_clarification": "faithfully representing reality"
                            },
                            {
                                "target_lang_word": "authentically",
                                "target_lang_sense_clarification": "in a genuine or original way"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [
        "okazywać",
        "okazanie",
        "okazały",
        "okazałość"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3fdac774-814a-3ca7-93ef-8b4ec45cddf4",
                    "sense_definition": "Stać się jawnym, zostać poznanym lub zrozumianym po pewnym czasie lub w wyniku sprawdzenia (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "discovery_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety <w>okazało</w> się, że sklep jest już zamknięty.",
                            "target_lang_translations": {
                                "ru": "К сожалению, <w>оказалось</w>, что магазин уже закрыт."
                            }
                        },
                        {
                            "text": "Ten pomysł może <w>okazać</w> się strzałem w dziesiątkę.",
                            "target_lang_translations": {
                                "ru": "Эта идея может <w>оказаться</w> попаданием в десятку."
                            }
                        },
                        {
                            "text": "Wkrótce <w>okaże</w> się, kto miał rację w tym sporze.",
                            "target_lang_translations": {
                                "ru": "Скоро <w>выяснится</w>, кто был прав в этом споре."
                            }
                        }
                    ],
                    "synonyms": [
                        "powstać",
                        "udać"
                    ],
                    "common_phrases": [
                        "to się okaże",
                        "okazało się że",
                        "okazać się prawdą",
                        "okazać się skutecznym"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wymaga zaimka zwrotnego „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать явным, известным или понятным в результате проверки или с течением времени (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оказаться",
                                "target_lang_sense_clarification": "стать явным, подтвердиться"
                            },
                            {
                                "target_lang_word": "выясниться",
                                "target_lang_sense_clarification": "стать известным, проясниться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed6ebefd-e5da-3f9a-85f1-e6f424163bc4",
                    "sense_definition": "Uzewnętrznić swoje uczucia, nastawienie lub cechy charakteru; dać dowód czegoś swoim zachowaniem lub udzielić czegoś niematerialnego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotions_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powinieneś <w>okazać</w> rodzicom więcej szacunku i zrozumienia.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>проявить</w> к родителям больше уважения и понимания."
                            }
                        },
                        {
                            "text": "Chcemy <w>okazać</w> wdzięczność za waszą ciężką pracę i poświęcenie.",
                            "target_lang_translations": {
                                "ru": "Мы хотим <w>выразить</w> благодарность за ваш тяжелый труд и самоотверженность."
                            }
                        },
                        {
                            "text": "Musisz <w>okazać</w> trochę cierpliwości, zanim zobaczysz efekty.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>проявить</w> немного терпения, прежде чем увидишь результаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokazać",
                        "popisać się"
                    ],
                    "antonyms": [
                        "ukryć",
                        "zataić"
                    ],
                    "common_phrases": [
                        "okazać szacunek",
                        "okazać pomoc",
                        "okazać zainteresowanie",
                        "okazać serce",
                        "okazać skruchę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продемонстрировать чувства, отношение или черты характера; предоставить что-то нематериальное (например, помощь или уважение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проявить",
                                "target_lang_sense_clarification": "чувства, качества, характер"
                            },
                            {
                                "target_lang_word": "оказать",
                                "target_lang_sense_clarification": "помощь, уважение, честь"
                            },
                            {
                                "target_lang_word": "выразить",
                                "target_lang_sense_clarification": "благодарность, признание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67231fea-da7e-3b93-82eb-bd903d587672",
                    "sense_definition": "Przedłożyć coś uprawnionej osobie w celu sprawdzenia, weryfikacji lub identyfikacji (np. dokument, bilet).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "official_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas kontroli drogowej kierowca musiał <w>okazać</w> prawo jazdy.",
                            "target_lang_translations": {
                                "ru": "Во время проверки на дороге водитель должен был <w>предъявить</w> водительские права."
                            }
                        },
                        {
                            "text": "Proszę przygotować i <w>okazać</w> bilety do kontroli.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, приготовьте и <w>предъявите</w> билеты для проверки."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokazać"
                    ],
                    "common_phrases": [
                        "okazać dowód osobisty",
                        "okazać bilet",
                        "okazać przepustkę",
                        "okazać dokumenty"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często używane w kontekście urzędowym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представить что-либо уполномоченному лицу для проверки, верификации или идентификации (например, документы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предъявить",
                                "target_lang_sense_clarification": "документы, билеты"
                            },
                            {
                                "target_lang_word": "показать",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
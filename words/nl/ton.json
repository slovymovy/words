            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a6235c33-e8cf-3687-85f2-e04b172530f5",
                    "sense_definition": "Een groot, cilindervormig vat om vloeistoffen of goederen in te bewaren, vaak gemaakt van hout, metaal of kunststof.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de tuin staat een grote <w>ton</w> om het regenwater op te vangen.",
                            "target_lang_translations": {
                                "en": "There is a large <w>barrel</w> in the garden to collect rainwater.",
                                "ru": "В саду стоит большая <w>бочка</w> для сбора дождевой воды."
                            }
                        },
                        {
                            "text": "De wijn moet jarenlang rijpen in een houten <w>ton</w> voor de beste smaak.",
                            "target_lang_translations": {
                                "en": "The wine must mature for years in a wooden <w>barrel</w> for the best taste.",
                                "ru": "Вино должно годами созревать в деревянной <w>бочке</w> для лучшего вкуса."
                            }
                        }
                    ],
                    "synonyms": [
                        "metrische ton"
                    ],
                    "common_phrases": [
                        "een houten ton",
                        "de regenton"
                    ],
                    "target_lang_definitions": {
                        "en": "A large, cylindrical container for storing liquids or goods, often made of wood, metal, or plastic.",
                        "ru": "Большой цилиндрический сосуд для хранения жидкостей или товаров, часто сделанный из дерева, металла или пластика."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "barrel",
                                "target_lang_sense_clarification": "cylindrical container"
                            },
                            {

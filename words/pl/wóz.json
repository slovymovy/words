            "senses": [
                {
                    "sense_id": "31781938-5e01-3438-b256-681767261557",
                    "sense_definition": "Pojazd o specjalnym przeznaczeniu, używany przez służby takie jak straż pożarna, policja czy wojsko.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czerwony <w>wóz</w> strażacki pędził do pożaru na sygnale.",
                            "target_lang_translations": {
                                "ru": "Красная пожарная <w>машина</w> мчалась на пожар с сиреной."
                            }
                        },
                        {
                            "text": "Policyjny <w>wóz</w> patrolował ulice miasta przez całą noc.",
                            "target_lang_translations": {
                                "ru": "Полицейская <w>машина</w> патрулировала улицы города всю ночь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wóz strażacki",
                        "wóz policyjny",
                        "wóz bojowy",
                        "wóz transmisyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортное средство специального назначения, используемое такими службами, как пожарная охрана, полиция или армия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "машина",
                                "target_lang_sense_clarification": "общее название для автомобиля спецслужб"
                            },
                            {
                                "target_lang_word": "автомобиль",
                                "target_lang_sense_clarification": "транспортное средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92bc0f65-301e-39f1-ad5f-eef34e27c91b",
                    "sense_definition": "Pojazd na czterech kołach ciągnięty przez konie lub inne zwierzęta, używany tradycyjnie do transportu na wsi.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik załadował świeże siano na drewniany <w>wóz</w>.",
                            "target_lang_translations": {
                                "ru": "Фермер погрузил свежее сено на деревянную <w>телегу</w>."
                            }
                        },
                        {
                            "text": "Dwa konie z trudem ciągnęły ciężki <w>wóz</w> pod górę.",
                            "target_lang_translations": {
                                "ru": "Две лошади с трудом тащили тяжелый <w>воз</w> в гору."
                            }
                        }
                    ],
                    "synonyms": [
                        "furmanka",
                        "fura"
                    ],
                    "common_phrases": [
                        "wóz drabiniasty",
                        "wóz konny",
                        "wóz z sianem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Четырехколесная повозка, запряженная лошадьми или другими животными, традиционно используемая для перевозки в сельской местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телега",
                                "target_lang_sense_clarification": "открытая повозка для грузов"
                            },
                            {
                                "target_lang_word": "воз",
                                "target_lang_sense_clarification": "крытая или груженая повозка"
                            },
                            {
                                "target_lang_word": "повозка",
                                "target_lang_sense_clarification": "общее название гужевого транспорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47d8c07d-b13b-3aa0-8797-4bdb7851ac23",
                    "sense_definition": "Samochód osobowy (określenie potoczne).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad kupił sobie nowy, sportowy <w>wóz</w>.",
                            "target_lang_translations": {
                                "ru": "Мой сосед купил себе новую спортивную <w>тачку</w>."
                            }
                        },
                        {
                            "text": "Nie wiem, gdzie zaparkowałem swój <w>wóz</w>.",
                            "target_lang_translations": {
                                "ru": "Не знаю, где я припарковал свою <w>машину</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "samochód",
                        "auto",
                        "fura",
                        "bryka"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "175bb157-def7-3eff-82b4-64aabe5005db",
                    "sense_definition": "Able to be carried or moved easily from one place to another.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We brought a <w>portable</w> heater to keep the tent warm during the night.",
                            "target_lang_translations": {
                                "ru": "Мы взяли с собой <w>переносной</w> обогреватель, чтобы поддерживать тепло в палатке ночью."
                            }
                        },
                        {
                            "text": "This <w>portable</w> speaker is small enough to fit in your backpack.",
                            "target_lang_translations": {
                                "ru": "Эта <w>портативная</w> колонка достаточно мала, чтобы поместиться в твой рюкзак."
                            }
                        }
                    ],
                    "synonyms": [
                        "movable",
                        "transportable",
                        "mobile"
                    ],
                    "antonyms": [
                        "fixed",
                        "stationary",
                        "immovable",
                        "immobile",
                        "permanent",
                        "anchored"
                    ],
                    "common_phrases": [

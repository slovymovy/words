{
    "word_family": [
        "rijstrook",
        "vluchtstrook",
        "landingsstrook"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e0bf10f0-5044-3d96-b685-9af969729fae",
                    "sense_definition": "Een lang, smal stuk materiaal, zoals papier, stof of land.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material_strip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij knipte een lange <w>strook</w> van het rode papier.",
                            "target_lang_translations": {
                                "en": "She cut a long <w>strip</w> off the red paper."
                            }
                        },
                        {
                            "text": "Langs de rivier ligt een groene <w>strook</w> gras.",
                            "target_lang_translations": {
                                "en": "Along the river lies a green <w>strip</w> of grass."
                            }
                        }
                    ],
                    "synonyms": [
                        "reep"
                    ],
                    "common_phrases": [
                        "strook papier",
                        "smalle strook"
                    ],
                    "target_lang_definitions": {
                        "en": "A long, narrow piece of material, such as paper, fabric, or land."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strip",
                                "target_lang_sense_clarification": "piece of material or land"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "917d0481-a710-3efd-8fbe-37f639674010",
                    "sense_definition": "Een gemarkeerd deel van de weg bestemd voor één rij voertuigen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "traffic_lane",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de snelweg moet je de rechter <w>strook</w> aanhouden.",
                            "target_lang_translations": {
                                "en": "On the highway you must keep to the right <w>lane</w>."
                            }
                        },
                        {
                            "text": "De middelste <w>strook</w> was afgesloten door een ongeluk.",
                            "target_lang_translations": {
                                "en": "The middle <w>lane</w> was closed due to an accident."
                            }
                        }
                    ],
                    "synonyms": [
                        "rijstrook"
                    ],
                    "common_phrases": [
                        "linker strook",
                        "rechter strook"
                    ],
                    "target_lang_definitions": {
                        "en": "A marked part of the road intended for one line of vehicles."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lane",
                                "target_lang_sense_clarification": "traffic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "718e7118-1aa2-3b91-96de-5620c504eb70",
                    "sense_definition": "Een langwerpige baan of lijn die qua kleur of patroon afwijkt van de ondergrond.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "visual_stripe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vlag van dat land heeft een blauwe <w>strook</w>.",
                            "target_lang_translations": {
                                "en": "That country's flag has a blue <w>stripe</w>."
                            }
                        },
                        {
                            "text": "Er viel een <w>strook</w> licht door de gordijnen naar binnen.",
                            "target_lang_translations": {
                                "en": "A <w>strip</w> of light fell in through the curtains."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An elongated band or line that differs in color or pattern from the background."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stripe",
                                "target_lang_sense_clarification": "color pattern"
                            },
                            {
                                "target_lang_word": "band",
                                "target_lang_sense_clarification": "broad stripe"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "28716e7a-efd9-36b8-966d-6f7229add94a",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>stroken</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>strook</w> de onderkant van de rok met kant.",
                            "target_lang_translations": {
                                "en": "I <w>trim</w> the bottom of the skirt with lace."
                            }
                        },
                        {
                            "text": "Ik <w>strook</w> niet met de heersende opvattingen.",
                            "target_lang_translations": {
                                "en": "I do not <w>agree</w> with the prevailing views."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'stroken' (to agree or to fit with strips)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "agree",
                                "target_lang_sense_clarification": "to be consistent with"
                            },
                            {
                                "target_lang_word": "tally",
                                "target_lang_sense_clarification": "to match"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
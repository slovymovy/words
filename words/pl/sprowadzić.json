    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1d590da1-0221-385c-b819-bfaa05b162d5",
                    "sense_definition": "Sprawić, że ktoś lub coś znajdzie się w określonym miejscu; przywieźć lub przyprowadzić kogoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy szybko <sprowadzić> lekarza do chorego dziecka.",
                            "target_lang_translations": {
                                "ru": "Нам нужно быстро <вызвать> врача к больному ребенку."
                            }
                        },
                        {
                            "text": "Udało mi się <sprowadzić> całą rodzinę na święta do domu.",
                            "target_lang_translations": {
                                "ru": "Мне удалось <собрать> всю семью домой на праздники."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywieźć",
                        "przyprowadzić",
                        "ściągnąć"
                    ],
                    "antonyms": [
                        "odprowadzić",
                        "wywieźć",
                        "odesłać"
                    ],
                    "common_phrases": [
                        "sprowadzić pomoc",
                        "sprowadzić lekarza",
                        "sprowadzić kłopoty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести или привезти кого-то или что-то в определенное место; вызвать кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привезти",
                                "target_lang_sense_clarification": "доставить с помощью транспорта"
                            },
                            {
                                "target_lang_word": "привести",
                                "target_lang_sense_clarification": "сопровождая, помочь добраться"
                            },
                            {
                                "target_lang_word": "вызвать",
                                "target_lang_sense_clarification": "попросить прийти (напр. врача)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c79a5cd7-5fd0-3ef3-aa8b-0f853e14ed0c",
                    "sense_definition": "Zakupić i przywieźć towary lub produkty z innego kraju (z zagranicy).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "trade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma postanowiła <sprowadzić> nowe maszyny z Niemiec.",
                            "target_lang_translations": {
                                "ru": "Фирма решила <привезти> новые станки из Германии."
                            }
                        },
                        {
                            "text": "Czy opłaca się teraz <sprowadzić> samochód ze Stanów Zjednoczonych?",
                            "target_lang_translations": {
                                "ru": "Выгодно ли сейчас <пригнать> машину из Соединенных Штатов?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zaimportować"
                    ],
                    "antonyms": [
                        "wyeksportować",
                        "wywieźć"
                    ],
                    "common_phrases": [
                        "sprowadzić z zagranicy",
                        "sprowadzić samochód",
                        "sprowadzić towary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Купить и доставить товары или продукты из другой страны (из-за границы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "импортировать",
                                "target_lang_sense_clarification": "ввозить товары из-за границы"
                            },
                            {
                                "target_lang_word": "привезти",
                                "target_lang_sense_clarification": "доставить из другого места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f05f088-744f-3f85-928a-f5d2f6265d4b",
                    "sense_definition": "Pomóc komuś zejść z wyższego poziomu na niższy lub skierować coś w dół.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_movement_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Strażak pomógł <sprowadzić> przerażonego kota z wysokiego drzewa.",
                            "target_lang_translations": {
                                "ru": "Пожарный помог <снять> испуганного кота с высокого дерева."
                            }
                        },
                        {
                            "text": "Musisz ostrożnie <sprowadzić> ten ciężki rower po schodach.",
                            "target_lang_translations": {
                                "ru": "Ты должен осторожно <спустить> этот тяжелый велосипед по лестнице."
                            }
                        }
                    ],
                    "synonyms": [
                        "spuścić",
                        "znieść"
                    ],
                    "antonyms": [
                        "wprowadzić",
                        "wnieść"
                    ],
                    "common_phrases": [
                        "sprowadzić na dół",
                        "sprowadzić na ziemię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помочь кому-то спуститься с высоты или переместить что-то вниз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спустить",
                                "target_lang_sense_clarification": "переместить вниз"
                            },
                            {
                                "target_lang_word": "свести",
                                "target_lang_sense_clarification": "помочь сойти вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13167c37-8897-35eb-9516-a5844ba7911a",
                    "sense_definition": "Ograniczyć coś do prostszej formy, mniejszej liczby elementów lub przedstawić w uproszczony sposób.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reduction_simplification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym zadaniu należy <sprowadzić> ułamki do wspólnego mianownika.",
                            "target_lang_translations": {
                                "ru": "В этом задании нужно <привести> дроби к общему знаменателю."
                            }
                        },
                        {
                            "text": "Nie można <sprowadzić> tego skomplikowanego problemu tylko do braku pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Нельзя <свести> эту сложную проблему только к нехватке денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "uprościć",
                        "zredukować",
                        "ograniczyć"
                    ],
                    "antonyms": [
                        "skomplikować",
                        "rozbudować"
                    ],
                    "common_phrases": [
                        "sprowadzić do wspólnego mianownika",
                        "sprowadzić do parteru",
                        "sprowadzić do absurdu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свести что-то к более простой форме или представить упрощенно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свести",
                                "target_lang_sense_clarification": "упростить до чего-то"
                            },
                            {
                                "target_lang_word": "привести",
                                "target_lang_sense_clarification": "математический термин (к знаменателю)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a931835a-2d05-32b7-92db-4576f1c2a793",
                    "sense_definition": "Zamieszkać w nowym miejscu; wprowadzić się do mieszkania lub domu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi nowi sąsiedzi postanowili <sprowadzić się> tutaj w zeszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Мои новые соседи решили <переехать> сюда на прошлой неделе."
                            }
                        },
                        {
                            "text": "Kiedy zamierzasz ostatecznie <sprowadzić się> do swojego nowego domu?",
                            "target_lang_translations": {
                                "ru": "Когда ты собираешься окончательно <въехать> в свой новый дом?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wprowadzić się",
                        "zamieszkać"
                    ],
                    "antonyms": [
                        "wyprowadzić się"
                    ],
                    "common_phrases": [
                        "sprowadzić się do miasta",
                        "sprowadzić się do mieszkania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поселиться на новом месте; въехать в квартиру или дом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переехать",
                                "target_lang_sense_clarification": "сменить место жительства"
                            },
                            {
                                "target_lang_word": "поселиться",
                                "target_lang_sense_clarification": "начать жить в новом месте"
                            },
                            {
                                "target_lang_word": "въехать",
                                "target_lang_sense_clarification": "заселиться в помещение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
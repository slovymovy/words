    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3c6aa94b-a1c2-3bbd-a03e-ef8a7e5e6b72",
                    "sense_definition": "Gemakkelijk om doorheen te kijken omdat het niet troebel is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_transparency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water in het bergmeer was ijskoud en volkomen <w>helder</w>.",
                            "target_lang_translations": {
                                "ru": "Вода в горном озере была ледяной и совершенно <w>прозрачной</w>.",
                                "en": "The water in the mountain lake was ice-cold and completely <w>clear</w>."
                            }
                        },
                        {
                            "text": "Door het <w>heldere</w> glas kon ik de vogels in de tuin goed zien.",
                            "target_lang_translations": {
                                "ru": "Через <w>прозрачное</w> стекло я хорошо видел птиц в саду.",
                                "en": "Through the <w>clear</w> glass I could see the birds in the garden well."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorzichtig",
                        "transparant"
                    ],
                    "antonyms": [
                        "troebel",
                        "ondoorzichtig"
                    ],
                    "common_phrases": [
                        "helder water",
                        "helder glas",
                        "heldere vloeistof"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легко просматриваемый насквозь, не мутный; прозрачный.",
                        "en": "Easy to look through because it is not cloudy or turbid."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачный",
                                "target_lang_sense_clarification": "о воде, стекле или материале"
                            },
                            {
                                "target_lang_word": "чистый",
                                "target_lang_sense_clarification": "без примесей, не мутный"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "transparent, not cloudy"
                            },
                            {
                                "target_lang_word": "transparent",
                                "target_lang_sense_clarification": "allowing light to pass through"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef81445f-93c9-35a4-b4d9-bd6e5871304a",
                    "sense_definition": "Veel licht uitstralend of weerkaatsend; fel en intens van kleur of licht.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_brightness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zon scheen met een <w>helder</w> licht door de ramen naar binnen.",
                            "target_lang_translations": {
                                "ru": "Солнце светило <w>ярким</w> светом сквозь окна внутрь.",
                                "en": "The sun shone with a <w>bright</w> light through the windows."
                            }
                        },
                        {
                            "text": "Zij draagt graag kleding met <w>heldere</w> kleuren om op te vallen.",
                            "target_lang_translations": {
                                "ru": "Она любит носить одежду <w>ярких</w> цветов, чтобы выделяться.",
                                "en": "She likes wearing clothes with <w>bright</w> colors to stand out."
                            }
                        }
                    ],
                    "synonyms": [
                        "fel",
                        "stralend",
                        "licht"
                    ],
                    "antonyms": [
                        "dof",
                        "donker"
                    ],
                    "common_phrases": [
                        "helder licht",
                        "heldere kleuren",
                        "helder wit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Излучающий или отражающий много света; интенсивный по цвету или свету.",
                        "en": "Emitting or reflecting a lot of light; intense in color or light."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яркий",
                                "target_lang_sense_clarification": "о свете или цвете"
                            },
                            {
                                "target_lang_word": "светлый",
                                "target_lang_sense_clarification": "хорошо освещенный, сияющий"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "bright",
                                "target_lang_sense_clarification": "emitting much light or vivid in color"
                            },
                            {
                                "target_lang_word": "vivid",
                                "target_lang_sense_clarification": "intense color"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5925eda-8d18-31fd-a5eb-2537dd517921",
                    "sense_definition": "Gemakkelijk te begrijpen; zonder onduidelijkheden of verwarring.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar gaf een <w>heldere</w> uitleg over het moeilijke wiskundige probleem.",
                            "target_lang_translations": {
                                "ru": "Учитель дал <w>ясное</w> объяснение сложной математической задачи.",
                                "en": "The teacher gave a <w>clear</w> explanation of the difficult math problem."
                            }
                        },
                        {
                            "text": "Het is mij nog niet helemaal <w>helder</w> wat we precies moeten doen.",
                            "target_lang_translations": {
                                "ru": "Мне ещё не совсем <w>ясно</w>, что именно мы должны делать.",
                                "en": "It is not yet entirely <w>clear</w> to me what exactly we have to do."
                            }
                        }
                    ],
                    "synonyms": [
                        "duidelijk",
                        "begrijpelijk",
                        "evident"
                    ],
                    "antonyms": [
                        "vaag",
                        "onduidelijk",
                        "warrig"
                    ],
                    "common_phrases": [
                        "helder maken",
                        "helder antwoord",
                        "helder plan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкий для понимания; без неясностей или путаницы.",
                        "en": "Easy to understand; without ambiguity or confusion."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ясный",
                                "target_lang_sense_clarification": "понятный, очевидный"
                            },
                            {
                                "target_lang_word": "понятный",
                                "target_lang_sense_clarification": "доступный для понимания"
                            },
                            {
                                "target_lang_word": "чёткий",
                                "target_lang_sense_clarification": "определённый, конкретный"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "easy to understand"
                            },
                            {
                                "target_lang_word": "plain",
                                "target_lang_sense_clarification": "simple and distinct"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b695bb95-557e-3d15-97c4-b1595a071956",
                    "sense_definition": "Zonder wolken of mist; goed zichtbaar (vooral gezegd van de lucht of hemellichamen).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op een <w>heldere</w> dag kun je de bergen in de verte zien liggen.",
                            "target_lang_translations": {
                                "ru": "В <w>ясный</w> день можно увидеть горы вдалеке.",
                                "en": "On a <w>clear</w> day you can see the mountains in the distance."
                            }
                        },
                        {
                            "text": "Gisteravond was de maan erg <w>helder</w> aan de donkere hemel.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером луна была очень <w>яркой</w> на тёмном небе.",
                                "en": "Last night the moon was very <w>bright</w> in the dark sky."
                            }
                        }
                    ],
                    "synonyms": [
                        "onbewolkt",
                        "klaar"
                    ],
                    "antonyms": [
                        "bewolkt",
                        "betrokken"
                    ],
                    "common_phrases": [
                        "heldere hemel",
                        "heldere lucht",
                        "heldere nacht"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без облаков или тумана; хорошо видимый (о небе или небесных телах).",
                        "en": "Without clouds or fog; clearly visible (especially of the sky or celestial bodies)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ясный",
                                "target_lang_sense_clarification": "безоблачный, о погоде"
                            },
                            {
                                "target_lang_word": "безоблачный",
                                "target_lang_sense_clarification": "без облаков"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "free from clouds or mist"
                            },
                            {
                                "target_lang_word": "bright",
                                "target_lang_sense_clarification": "shining strongly (of stars/moon)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35b3aebc-06de-3aeb-a6b1-5868319b18ca",
                    "sense_definition": "Goed hoorbaar en zuiver van klank; niet hees of vervormd.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "auditory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zangeres heeft een prachtige, <w>heldere</w> stem die de hele zaal vult.",
                            "target_lang_translations": {
                                "ru": "У певицы прекрасный, <w>чистый</w> голос, который наполняет весь зал.",
                                "en": "The singer has a beautiful, <w>clear</w> voice that fills the whole hall."
                            }
                        },
                        {
                            "text": "Het geluid van de nieuwe luidsprekers is verrassend <w>helder</w> en gedetailleerd.",
                            "target_lang_translations": {
                                "ru": "Звук новых колонок удивительно <w>чистый</w> и детальный.",
                                "en": "The sound of the new speakers is surprisingly <w>clear</w> and detailed."
                            }
                        }
                    ],
                    "synonyms": [
                        "zuiver"
                    ],
                    "antonyms": [
                        "hees",
                        "schor"
                    ],
                    "common_phrases": [
                        "heldere stem",
                        "heldere toon",
                        "helder geluid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошо слышимый и чистый по звучанию; не хриплый и не искаженный.",
                        "en": "Clearly audible and pure in tone; not hoarse or distorted."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чистый",
                                "target_lang_sense_clarification": "о звуке, без помех"
                            },
                            {
                                "target_lang_word": "звонкий",
                                "target_lang_sense_clarification": "о голосе, звучный"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "distinct and pure sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3670ac10-dc52-30f5-8ad5-82b6027f2303",
                    "sense_definition": "In staat om goed, logisch en scherp na te denken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een kop sterke koffie kon hij eindelijk weer <w>helder</w> nadenken.",
                            "target_lang_translations": {
                                "ru": "После чашки крепкого кофе он наконец снова смог <w>ясно</w> мыслить.",
                                "en": "After a cup of strong coffee he could finally think <w>clearly</w> again."
                            }
                        },
                        {
                            "text": "Ondanks haar hoge leeftijd is mijn grootmoeder nog steeds <w>helder</w> van geest.",
                            "target_lang_translations": {
                                "ru": "Несмотря на преклонный возраст, у моей бабушки всё ещё <w>ясный</w> ум.",
                                "en": "Despite her old age, my grandmother is still <w>clear</w> of mind."
                            }
                        }
                    ],
                    "synonyms": [
                        "scherpzinnig",
                        "nuchter"
                    ],
                    "antonyms": [
                        "verward",
                        "suf"
                    ],
                    "common_phrases": [
                        "helder denken",
                        "helder hoofd",
                        "helder van geest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный хорошо, логично и остро мыслить.",
                        "en": "Capable of thinking well, logically, and sharply."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ясный",
                                "target_lang_sense_clarification": "о сознании или уме"
                            },
                            {
                                "target_lang_word": "трезвый",
                                "target_lang_sense_clarification": "рассудительный"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "clear-headed",
                                "target_lang_sense_clarification": "able to think clearly"
                            },
                            {
                                "target_lang_word": "lucid",
                                "target_lang_sense_clarification": "mentally sound and rational"
                            }
                        ]
                    }
                }
            ]

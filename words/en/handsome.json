{
    "word_family": [
        "handsomely",
        "handsomeness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3ebb5595-d8b9-300a-bc83-5c0d434a8e29",
                    "sense_definition": "Physically attractive and good-looking (usually describing a man).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He looked very <w>handsome</w> in his new dark suit.",
                            "target_lang_translations": {
                                "ru": "В своем новом темном костюме он выглядел очень <w>красивым</w>."
                            }
                        },
                        {
                            "text": "The actor is known for being a tall and <w>handsome</w> man.",
                            "target_lang_translations": {
                                "ru": "Этот актер известен как высокий и <w>красивый</w> мужчина."
                            }
                        }
                    ],
                    "synonyms": [
                        "substantial",
                        "hefty"
                    ],
                    "common_phrases": [
                        "handsome man",
                        "tall and handsome",
                        "handsome prince"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически привлекательный и приятной внешности (обычно о мужчине)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красивый",
                                "target_lang_sense_clarification": "о мужчине"
                            },
                            {
                                "target_lang_word": "привлекательный",
                                "target_lang_sense_clarification": "располагающий к себе"
                            },
                            {
                                "target_lang_word": "статный",
                                "target_lang_sense_clarification": "о фигуре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b054e3a7-7744-3df1-aa72-8f404d748d2f",
                    "sense_definition": "Impressive, well-made, and pleasing to look at (describing an object, building, or place).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city hall is a <w>handsome</w> building made of red brick.",
                            "target_lang_translations": {
                                "ru": "Ратуша — это <w>красивое</w> здание из красного кирпича."
                            }
                        },
                        {
                            "text": "They bought a <w>handsome</w> oak table for their dining room.",
                            "target_lang_translations": {
                                "ru": "Они купили <w>добротный</w> дубовый стол для своей столовой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "handsome building",
                        "handsome house",
                        "handsome volume"
                    ],
                    "target_lang_definitions": {
                        "ru": "Впечатляющий, добротный и приятный на вид (о предмете, здании или месте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красивый",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "великолепный",
                                "target_lang_sense_clarification": "впечатляющий"
                            },
                            {
                                "target_lang_word": "добротный",
                                "target_lang_sense_clarification": "качественно сделанный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02f89833-2c7f-38e5-aa73-d553d6eddf18",
                    "sense_definition": "Large in amount or quantity; generous.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company made a <w>handsome</w> profit from the new product launch.",
                            "target_lang_translations": {
                                "ru": "Компания получила <w>солидную</w> прибыль от запуска нового продукта."
                            }
                        },
                        {
                            "text": "She received a <w>handsome</w> salary for her work as a consultant.",
                            "target_lang_translations": {
                                "ru": "Она получала <w>щедрую</w> зарплату за свою работу консультантом."
                            }
                        }
                    ],
                    "synonyms": [
                        "substantial",
                        "hefty"
                    ],
                    "common_phrases": [
                        "handsome profit",
                        "handsome salary",
                        "handsome sum",
                        "handsome reward"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой по количеству или размеру; щедрый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "значительный",
                                "target_lang_sense_clarification": "о количестве"
                            },
                            {
                                "target_lang_word": "щедрый",
                                "target_lang_sense_clarification": "о награде или оплате"
                            },
                            {
                                "target_lang_word": "солидный",
                                "target_lang_sense_clarification": "внушительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc8866f4-5533-3fa1-9c3d-a47e88105181",
                    "sense_definition": "Attractive in a strong, imposing, or statuesque way (describing a woman).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was a <w>handsome</w> woman with striking features and confident posture.",
                            "target_lang_translations": {
                                "ru": "Она была <w>статной</w> женщиной с яркими чертами лица и уверенной осанкой."
                            }
                        },
                        {
                            "text": "The portrait depicted a <w>handsome</w> lady dressed in velvet.",
                            "target_lang_translations": {
                                "ru": "На портрете была изображена <w>видная</w> дама, одетая в бархат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "handsome woman"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привлекательная, обладающая сильной, внушительной или статной красотой (о женщине)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "статный",
                                "target_lang_sense_clarification": "о женщине с хорошей осанкой"
                            },
                            {
                                "target_lang_word": "видный",
                                "target_lang_sense_clarification": "заметный, представительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e32dd3f7-5dd7-3a69-bd8b-7f17c98dcbbc",
                    "sense_definition": "Generous or noble in character.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He made a <w>handsome</w> apology for his mistake.",
                            "target_lang_translations": {
                                "ru": "Он принес <w>благородные</w> извинения за свою ошибку."
                            }
                        },
                        {
                            "text": "It was very <w>handsome</w> of him to pay for everyone's dinner.",
                            "target_lang_translations": {
                                "ru": "С его стороны было очень <w>благородно</w> оплатить всем ужин."
                            }
                        }
                    ],
                    "common_phrases": [
                        "handsome is as handsome does"
                    ],
                    "target_lang_definitions": {
                        "ru": "Щедрый или благородный по характеру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благородный",
                                "target_lang_sense_clarification": "о поступке"
                            },
                            {
                                "target_lang_word": "красивый",
                                "target_lang_sense_clarification": "перен. о жесте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "49f18d9e-3e4c-3969-b1d9-1e282f61be3f",
                    "sense_definition": "To make someone or something look handsome.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new haircut really <w>handsomed</w> him for the occasion.",
                            "target_lang_translations": {
                                "ru": "Новая стрижка действительно <w>украсила</w> его по этому случаю."
                            }
                        },
                        {
                            "text": "They <w>handsomed</w> the room with fresh flowers and decorations.",
                            "target_lang_translations": {
                                "ru": "Они <w>украсили</w> комнату свежими цветами и декорациями."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать кого-то или что-то красивым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украшать",
                                "target_lang_sense_clarification": "делать красивее"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "af61d6a8-0f67-308e-ae90-f0b0d8026955",
                    "sense_definition": "Całkowity, zupełny; używane najczęściej jako wzmocnienie negatywnego określenia osoby.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "emphatic_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie słuchaj go, to <w>skończony</w> idiota, który nie wie, o czym mówi.",
                            "target_lang_translations": {
                                "ru": "Не слушай его, это <w>законченный</w> идиот, который не знает, о чём говорит."
                            }
                        },
                        {
                            "text": "Jego zachowanie przy stole dowodzi, że jest <w>skończonym</w> chamem.",
                            "target_lang_translations": {
                                "ru": "Его поведение за столом доказывает, что он <w>законченный</w> хам."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrujnowany"
                    ],
                    "common_phrases": [
                        "skończony idiota",
                        "skończony dureń",
                        "skończony łajdak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej do wyrażania emocji"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный, абсолютный; часто используется для усиления негативной характеристики человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "законченный",
                                "target_lang_sense_clarification": "полный, совершенный (обычно о негативных качествах)"
                            },
                            {
                                "target_lang_word": "отпетый",
                                "target_lang_sense_clarification": "безнадежный, неисправимый (о мошенниках, негодяях)"
                            },
                            {
                                "target_lang_word": "набитый",
                                "target_lang_sense_clarification": "используется в сочетании «набитый дурак»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec70e9c5-c34e-339c-9b76-39eeb5354bf4",
                    "sense_definition": "Mający kres, ograniczony pod względem liczby, czasu lub wielkości; przeciwieństwo nieskończonego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "finite_limited",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zasoby naturalne Ziemi są <w>skończone</w>, dlatego musimy o nie dbać.",
                            "target_lang_translations": {
                                "ru": "Природные ресурсы Земли <w>конечны</w>, поэтому мы должны беречь их."
                            }
                        },
                        {
                            "text": "Każdy człowiek ma <w>skończoną</w> ilość czasu do wykorzystania w swoim życiu.",
                            "target_lang_translations": {
                                "ru": "У каждого человека есть <w>конечное</w> количество времени, которое можно использовать в своей жизни."
                            }
                        }
                    ],
                    "antonyms": [
                        "nieskończony"
                    ],
                    "common_phrases": [
                        "zbiór skończony",
                        "liczba skończona",
                        "w skończonym czasie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий предел, ограниченный в количестве, времени или размере; противоположность бесконечному."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конечный",
                                "target_lang_sense_clarification": "имеющий конец, предел"
                            },
                            {
                                "target_lang_word": "ограниченный",
                                "target_lang_sense_clarification": "имеющий границы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "481e67d7-13bd-3b86-befa-b7a6e78d8cf1",
                    "sense_definition": "Taki, który poniósł ostateczną klęskę, stracił pozycję lub szanse na sukces.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ruined_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ujawnieniu skandalu korupcyjnego ten polityk jest już <w>skończony</w>.",
                            "target_lang_translations": {
                                "ru": "После раскрытия коррупционного скандала этот политик уже <w>конченый</w> человек."
                            }
                        },
                        {
                            "text": "Jeśli nie oddam długu do jutra, jestem <w>skończony</w> w tym mieście.",
                            "target_lang_translations": {
                                "ru": "Если я не верну долг до завтра, я <w>конченый</w> человек в этом городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrujnowany"
                    ],
                    "common_phrases": [
                        "być skończonym",
                        "człowiek skończony"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często w kontekście dramatycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерпевший окончательное поражение, утративший положение или шансы на успех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конченый",
                                "target_lang_sense_clarification": "опустившийся, ни на что не годный (о человеке)"
                            },
                            {
                                "target_lang_word": "пропащий",
                                "target_lang_sense_clarification": "безнадежный, погибший"
                            }

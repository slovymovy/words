                        "development environment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Программное или аппаратное обеспечение компьютерной системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "среда",
                                "target_lang_sense_clarification": "компьютерная"

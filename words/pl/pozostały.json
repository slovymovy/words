    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c6d33b1a-7ce5-3ad0-9507-d2854e59278c",
                    "sense_definition": "Taki, który stanowi resztę; wszyscy lub wszystko oprócz wymienionych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten mały egoista sam zjadł wszystkie ciastka, nie myśląc o <w>pozostałych</w> dzieciach.",
                            "target_lang_translations": {
                                "ru": "Этот маленький эгоист сам съел все печенья, не думая об <w>остальных</w> детях."
                            }
                        },
                        {
                            "text": "Okolicę dworca oświetlała wieczorem tylko jedna latarnia, a wszystkie <w>pozostałe</w> były zepsute.",
                            "target_lang_translations": {
                                "ru": "Вечером окрестности вокзала освещал только один фонарь, а все <w>остальные</w> были сломаны."
                            }
                        }
                    ],
                    "synonyms": [
                        "inny"
                    ],
                    "common_phrases": [
                        "pozostała część",
                        "pozostałe osoby",
                        "w pozostałych przypadkach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, который составляет остаток; все или всё, кроме упомянутых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остальной",
                                "target_lang_sense_clarification": "все другие, прочие"
                            },
                            {
                                "target_lang_word": "прочий",
                                "target_lang_sense_clarification": "другой, остальной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7392b9e2-9cc7-39fc-ae61-70049c88de0c",
                    "sense_definition": "Taki, który nie został zużyty, zniszczony lub zabrany; taki, który się uchował.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy mądrze wykorzystać <w>pozostały</w> nam czas na przygotowania.",
                            "target_lang_translations": {
                                "ru": "Мы должны мудро использовать <w>оставшееся</w> у нас время на подготовку."
                            }
                        },
                        {
                            "text": "Na talerzu leżał ostatni <w>pozostały</w> kawałek pizzy.",
                            "target_lang_translations": {
                                "ru": "На тарелке лежал последний <w>оставшийся</w> кусок пиццы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pozostały czas",
                        "pozostała kwota"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, который не был использован, уничтожен или забран; сохранившийся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставшийся",
                                "target_lang_sense_clarification": "который остался, сохранился"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dbf81342-a456-3d4f-8e1a-302784832594",
                    "sense_definition": "Forma czasu przeszłego (3. osoba liczby mnogiej, rodzaj niemęskoosobowy) czasownika <w>pozostać</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej słowa na długo <w>pozostały</w> w mojej pamięci.",
                            "target_lang_translations": {
                                "ru": "Её слова надолго <w>остались</w> в моей памяти."
                            }
                        },
                        {
                            "text": "Wszystkie miejsca w kinie <w>pozostały</w> zajęte do końca seansu.",
                            "target_lang_translations": {
                                "ru": "Все места в кинотеатре <w>остались</w> заняты до конца сеанса."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прошедшего времени (3-е лицо множественного числа) глагола <w>pozostać</w> (остаться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остались",
                                "target_lang_sense_clarification": "форма глагола остаться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
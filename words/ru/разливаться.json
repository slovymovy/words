    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b3dcc492-d818-324b-9d5c-b2954787d5b8",
                    "sense_definition": "Вытекать из ёмкости, проливаться или расплёскиваться по поверхности.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "liquid_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вода из переполненного ведра <разливается> по всему полу.",
                            "target_lang_translations": {
                                "en": "Water from the overflowing bucket <spills> all over the floor."
                            }
                        },
                        {
                            "text": "Если неаккуратно нести чашку, чай легко <разливается>.",
                            "target_lang_translations": {
                                "en": "If you carry the cup carelessly, the tea easily <spills>."
                            }
                        }
                    ],
                    "synonyms": [
                        "проливаться",
                        "расплёскиваться"
                    ],
                    "common_phrases": [
                        "вода разливается",
                        "быстро разливаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To flow out of a container, spill, or splash over a surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spill",
                                "target_lang_sense_clarification": "accidental flow out of a container"
                            },
                            {
                                "target_lang_word": "slosh",
                                "target_lang_sense_clarification": "liquid moving clumsily over the edge"
                            },
                            {
                                "target_lang_word": "splash",
                                "target_lang_sense_clarification": "scatter liquid in drops"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "957aa956-cd58-3498-bff2-0d02c8ec902a",
                    "sense_definition": "Выходить из берегов, затопляя прилегающую местность (о реке, озере и других водоёмах).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "liquid_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждую весну наша река широко <разливается> и затапливает луга.",
                            "target_lang_translations": {
                                "en": "Every spring our river <overflows> widely and floods the meadows."
                            }
                        },
                        {
                            "text": "Озеро <разливалось> всё больше из-за сильных дождей.",
                            "target_lang_translations": {
                                "en": "The lake <overflowed> more and more due to heavy rains."
                            }
                        }
                    ],
                    "common_phrases": [
                        "река разливается",
                        "широко разливаться",
                        "море разливается"
                    ],
                    "target_lang_definitions": {
                        "en": "To go over the banks, flooding the surrounding area (about a river, lake, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overflow",
                                "target_lang_sense_clarification": "exceed limits or banks"
                            },
                            {
                                "target_lang_word": "burst its banks",
                                "target_lang_sense_clarification": "idiomatic phrase for rivers"
                            },
                            {
                                "target_lang_word": "flood",
                                "target_lang_sense_clarification": "cover land with water"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6491edd-c393-3330-b001-329481a25341",
                    "sense_definition": "Распространяться в пространстве (о запахе, свете) или охватывать тело (о чувствах, ощущениях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_spread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Приятный аромат свежего хлеба <разливался> по всей кухне.",
                            "target_lang_translations": {
                                "en": "The pleasant aroma of fresh bread <spread> throughout the kitchen."
                            }
                        },
                        {
                            "text": "По её телу медленно <разливалось> приятное тепло.",
                            "target_lang_translations": {
                                "en": "A pleasant warmth slowly <spread> through her body."
                            }
                        }
                    ],
                    "common_phrases": [
                        "свет разливается",
                        "тепло разливается",
                        "румянец разливается",
                        "запах разливается"
                    ],
                    "target_lang_definitions": {
                        "en": "To spread through space (smell, light) or encompass the body (feelings, sensations)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spread",
                                "target_lang_sense_clarification": "extend over a large area"
                            },
                            {
                                "target_lang_word": "diffuse",
                                "target_lang_sense_clarification": "spread out freely (light, scent)"
                            },
                            {
                                "target_lang_word": "wash over",
                                "target_lang_sense_clarification": "suddenly affect someone (feelings)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09387900-a937-32ae-ade6-1beffeedf28d",
                    "sense_definition": "Страдательная форма от глагола <разливать>: распределяться по сосудам или ёмкостям.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "passive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На этом заводе минеральная вода <разливается> в стеклянные бутылки.",
                            "target_lang_translations": {
                                "en": "At this factory, mineral water <is bottled> in glass bottles."
                            }
                        },
                        {
                            "text": "Горячий суп <разливается> по тарелкам перед подачей на стол.",
                            "target_lang_translations": {
                                "en": "Hot soup <is poured> into plates before serving."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вино разливается",
                        "суп разливается"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Passive form of 'to pour': to be distributed into vessels or containers."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be bottled",
                                "target_lang_sense_clarification": "put into bottles commercially"
                            },
                            {
                                "target_lang_word": "be poured",
                                "target_lang_sense_clarification": "transferred into a container"
                            },
                            {
                                "target_lang_word": "be filled",
                                "target_lang_sense_clarification": "put into containers"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd80a6fd-4e7d-3c71-9179-40d0aaf26a3b",
                    "sense_definition": "Издавать мелодичные, переливчатые звуки; петь звонко и увлечённо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В весеннем лесу звонко <разливался> соловей.",
                            "target_lang_translations": {
                                "en": "In the spring forest, a nightingale <trilled> loudly."
                            }
                        },
                        {
                            "text": "Гармонь весело <разливалась> на деревенском празднике.",
                            "target_lang_translations": {
                                "en": "The accordion <rang out> merrily at the village festival."
                            }
                        }
                    ],
                    "common_phrases": [
                        "соловей разливается",
                        "звонко разливаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To produce melodious, trilling sounds; to sing loudly and enthusiastically."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trill",
                                "target_lang_sense_clarification": "produce a quavering sound (birds)"
                            },
                            {
                                "target_lang_word": "warble",
                                "target_lang_sense_clarification": "sing softly and with a succession of changing notes"
                            },
                            {
                                "target_lang_word": "ring out",
                                "target_lang_sense_clarification": "sound loudly and clearly"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
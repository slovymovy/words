            "senses": [
                {
                    "sense_id": "0d49c106-e8b3-30e5-9f16-0c461ee1b736",
                    "sense_definition": "Издавать характерные глухие, отрывистые звуки, похожие на «ух» (обычно о совах или филинах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "animal_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В тёмном лесу всю ночь страшно <ухал> филин."
                        },
                        {
                            "text": "Где-то вдалеке <ухала> одинокая сова, предвещая ночь."
                        }
                    ],
                    "synonyms": [
                        "кричать"
                    ],
                    "common_phrases": [
                        "сова ухает",
                        "филин ухает"
                    ]
                },
                {
                    "sense_id": "1d388a38-7000-35a5-b9d5-944e4a13a826",
                    "sense_definition": "Издавать сильные, глухие и резкие звуки (о взрывах, выстрелах, ударах или работе тяжёлых механизмов).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "loud_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За горизонтом глухо <ухали> пушки, и земля дрожала."
                        },
                        {
                            "text": "На соседней стройке ритмично <ухал> сваебойный молот."
                        }
                    ],
                    "synonyms": [
                        "грохотать",
                        "бухать",
                        "бабахать"
                    ],
                    "common_phrases": [
                        "пушки ухают",
                        "земля ухает",
                        "сердце ухает"
                    ]
                },
                {
                    "sense_id": "5d35909c-cb5d-3cb0-b729-89eced305a39",
                    "sense_definition": "Громко и отрывисто выдыхать воздух со звуком «ух» при тяжёлой физической работе, усилии или от сильных эмоций.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "human_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Грузчики громко <ухали>, поднимая тяжёлый рояль на третий этаж."
                        },
                        {
                            "text": "Мужики выбегали из бани и <ухали> от удовольствия, ныряя в снег."
                        }
                    ],
                    "synonyms": [
                        "кряхтеть",
                        "выдыхать"
                    ],
                    "common_phrases": [
                        "ухать от натуги",
                        "ухать от холода"
                    ]
                },
                {
                    "sense_id": "49a39ae3-810f-32df-951b-9fd188016d80",
                    "sense_definition": "С шумом падать, проваливаться вниз или резко бросать что-либо тяжёлое.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "heavy_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня сердце <ухало> в пятки каждый раз, когда машина резко тормозила."
                        },
                        {
                            "text": "Дети с разбегу <ухали> в мягкие сугробы и смеялись."
                        }
                    ],
                    "synonyms": [
                        "падать",
                        "плюхаться",
                        "валиться"
                    ],
                    "common_phrases": [
                        "ухать вниз",
                        "ухать в воду",
                        "сердце ухает"
                    ],

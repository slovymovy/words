                    "synonyms": [
                        "coke",
                        "snow"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кокаин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кокаин",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "014abea1-37f8-338d-98b2-636e0113cd6f",
                    "sense_definition": "Предмет или вещь, которую дарят по собственному желанию, безвозмездно, с целью доставить удовольствие или пользу.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На день рождения я получил долгожданный <w>подарок</w>.",
                            "target_lang_translations": {
                                "en": "For my birthday, I received a long-awaited <w>present</w>."
                            }
                        },
                        {
                            "text": "Мы решили сделать учителю небольшой <w>подарок</w>.",
                            "target_lang_translations": {
                                "en": "We decided to give the teacher a small <w>gift</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "презент",
                        "сувенир"
                    ],
                    "common_phrases": [
                        "сделать подарок",
                        "получить подарок",
                        "купить подарок",
                        "новогодний подарок"
                    ],
                    "target_lang_definitions": {
                        "en": "An object given voluntarily to someone without payment, usually to please them or show affection."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gift",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "often for birthdays or holidays"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "108b021f-e214-33d9-8da1-56c91fa8be97",
                    "sense_definition": "Событие, явление или обстоятельство, которое доставляет большую радость и удовольствие (обычно в переносном смысле).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Встреча со старым другом стала для меня настоящим <w>подарком</w>.",
                            "target_lang_translations": {
                                "en": "Meeting an old friend was a real <w>gift</w> for me."
                            }
                        },
                        {
                            "text": "Этот солнечный день — просто <w>подарок</w> после недели дождей.",
                            "target_lang_translations": {
                                "en": "This sunny day is simply a <w>gift</w> after a week of rain."
                            }
                        }
                    ],
                    "common_phrases": [
                        "подарок судьбы",
                        "настоящий подарок"
                    ],
                    "target_lang_definitions": {
                        "en": "An event, circumstance, or experience that brings great joy, relief, or pleasure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gift",
                                "target_lang_sense_clarification": "figurative meaning"
                            },
                            {
                                "target_lang_word": "godsend",
                                "target_lang_sense_clarification": "unexpected blessing or relief"
                            }
                        ]
                    }
                }

{
    "word_family": [
        "youth",
        "youngster",
        "youngling",
        "youngish"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "34a5a2c2-cd48-39cd-b970-754f777a300c",
                    "sense_definition": "In the early part of growth or life; born not long ago.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "age_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>young</w> boy played happily in the park with his friends.",
                            "target_lang_translations": {
                                "ru": "<w>Юный</w> мальчик весело играл в парке со своими друзьями."
                            }
                        },
                        {
                            "text": "Many <w>young</w> people enjoy listening to loud music and dancing.",
                            "target_lang_translations": {
                                "ru": "Многим <w>молодым</w> людям нравится слушать громкую музыку и танцевать."
                            }
                        }
                    ],
                    "synonyms": [
                        "youthful",
                        "juvenile"
                    ],
                    "antonyms": [
                        "old",
                        "elderly",
                        "aged",
                        "senior"
                    ],
                    "common_phrases": [
                        "young man",
                        "young woman",
                        "young people",
                        "young children"
                    ],
                    "target_lang_definitions": {
                        "ru": "В раннем периоде роста или жизни; родившийся недавно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодой",
                                "target_lang_sense_clarification": "общий термин для описания возраста"
                            },
                            {
                                "target_lang_word": "юный",
                                "target_lang_sense_clarification": "относящийся к юности, часто подразумевает свежесть и невинность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59c12fb0-a73b-32b1-b5ff-f3be4e26453f",
                    "sense_definition": "At an early stage of existence or development; not yet established.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "age_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The night is <w>young</w>, so we still have plenty of time to have fun.",
                            "target_lang_translations": {
                                "ru": "Ночь ещё <w>молода</w>, так что у нас ещё куча времени, чтобы повеселиться."
                            }
                        },
                        {
                            "text": "This <w>young</w> company has already achieved great success in the market.",
                            "target_lang_translations": {
                                "ru": "Эта <w>молодая</w> компания уже добилась большого успеха на рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "undeveloped"
                    ],
                    "antonyms": [
                        "old"
                    ],
                    "common_phrases": [
                        "night is young",
                        "young industry",
                        "young country"
                    ],
                    "target_lang_definitions": {
                        "ru": "На ранней стадии существования или развития; ещё не устоявшийся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодой",
                                "target_lang_sense_clarification": "недавно созданный или начавшийся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bcfdcf5-2bae-3dd6-a90c-b23210f8c44a",
                    "sense_definition": "Having the look, qualities, or attitude of a young person.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother is eighty, but she is still very <w>young</w> at heart.",
                            "target_lang_translations": {
                                "ru": "Моей бабушке восемьдесят, но она всё ещё очень <w>молода</w> душой."
                            }
                        },
                        {
                            "text": "She uses special cream to make her skin look <w>young</w> and fresh.",
                            "target_lang_translations": {
                                "ru": "Она использует специальный крем, чтобы её кожа выглядела <w>молодой</w> и свежей."
                            }
                        }
                    ],
                    "synonyms": [
                        "youthful",
                        "baby-faced"
                    ],
                    "antonyms": [
                        "mature",
                        "old"
                    ],
                    "common_phrases": [
                        "young at heart",
                        "look young",
                        "feel young"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий вид, качества или отношение к жизни, свойственные молодому человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодой",
                                "target_lang_sense_clarification": "о состоянии души или внешности"
                            },
                            {
                                "target_lang_word": "моложавый",
                                "target_lang_sense_clarification": "выглядящий моложе своих лет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69ae2a3b-4345-363b-86f5-f70c81137c36",
                    "sense_definition": "Used to distinguish the younger of two people with the same name (often a parent and child).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "distinction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>young</w> Mr. Jones will take over the family business soon.",
                            "target_lang_translations": {
                                "ru": "<w>Младший</w> мистер Джонс скоро возглавит семейный бизнес."
                            }
                        },
                        {
                            "text": "I am looking for the <w>young</w> doctor, not his father.",
                            "target_lang_translations": {
                                "ru": "Я ищу <w>молодого</w> доктора, а не его отца."
                            }
                        }
                    ],
                    "synonyms": [
                        "junior"
                    ],
                    "antonyms": [
                        "senior",
                        "old"
                    ],
                    "common_phrases": [
                        "the young Mr",
                        "the young Mrs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для различения младшего из двух людей с одинаковым именем (часто родителя и ребёнка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "младший",
                                "target_lang_sense_clarification": "добавляется к имени или титулу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "841129ca-1b98-3f0b-8665-b843eaeeb0cd",
                    "sense_definition": "Having little experience or practice; new to a specific activity.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is still <w>young</w> in his career and has much to learn.",
                            "target_lang_translations": {
                                "ru": "Он всё ещё <w>неопытен</w> в своей карьере, и ему многому нужно научиться."
                            }
                        },
                        {
                            "text": "Do not be too hard on him, as he is <w>young</w> at this game.",
                            "target_lang_translations": {
                                "ru": "Не будь к нему слишком строг, так как он <w>новичок</w> в этом деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "immature",
                        "underdeveloped"
                    ],
                    "antonyms": [
                        "experienced",
                        "veteran",
                        "mature"
                    ],
                    "common_phrases": [
                        "young in the business"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий мало опыта или практики; новичок в определённой деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неопытный",
                                "target_lang_sense_clarification": "не имеющий достаточного опыта"
                            },
                            {
                                "target_lang_word": "зелёный",
                                "target_lang_sense_clarification": "разговорное; новичок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "234befa4-0f33-33c4-b1d9-bca97b571fe7",
                    "sense_definition": "Offspring, especially the immature offspring of animals.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "offspring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mother bear fought fiercely to protect her <w>young</w> from danger.",
                            "target_lang_translations": {
                                "ru": "Медведица яростно сражалась, чтобы защитить своих <w>детёнышей</w> от опасности."
                            }
                        },
                        {
                            "text": "Many animals carry their <w>young</w> on their backs while traveling.",
                            "target_lang_translations": {
                                "ru": "Многие животные носят своих <w>детёнышей</w> на спинах во время путешествий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "feed the young",
                        "protect the young",
                        "carry the young"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потомство, особенно незрелое потомство животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детёныш",
                                "target_lang_sense_clarification": "молодое животное"
                            },
                            {
                                "target_lang_word": "молодняк",
                                "target_lang_sense_clarification": "собирательное; молодые животные"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "79e53362-6218-3e89-b540-02b3a64d1ba7",
                    "sense_definition": "A common surname of English or Chinese origin.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Young</w> is our new mathematics teacher at school.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Янг</w> — наш новый учитель математики в школе."
                            }
                        },
                        {
                            "text": "I have an appointment with Dr. <w>Young</w> tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "У меня назначен приём у доктора <w>Янга</w> завтра утром."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространённая фамилия английского или китайского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Янг",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "299f9b09-d74a-3e02-88e2-9a8ac03e830a",
                    "sense_definition": "A place name used for various towns and communities in English-speaking countries.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They visited the small town of <w>Young</w> during their holiday.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска они посетили маленький городок <w>Янг</w>."
                            }
                        },
                        {
                            "text": "My cousins live in a community called <w>Young</w> in Arizona.",
                            "target_lang_translations": {
                                "ru": "Мои кузены живут в общине под названием <w>Янг</w> в Аризоне."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Географическое название различных городов и общин в англоязычных странах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Янг",
                                "target_lang_sense_clarification": "название города"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5578c84c-caf5-3b18-8e55-90ebaa41feaa",
                    "sense_definition": "To exhibit characteristics of becoming younger or more recent (often used in geology).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geology_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rock formations <w>young</w> towards the south in this region.",
                            "target_lang_translations": {
                                "ru": "Горные породы <w>омолаживаются</w> к югу в этом регионе."
                            }
                        },
                        {
                            "text": "Geologists observed how the strata <w>young</w> in that specific direction.",
                            "target_lang_translations": {
                                "ru": "Геологи наблюдали, как пласты <w>становятся моложе</w> в этом конкретном направлении."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical/geology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявлять признаки омоложения породы в определённом направлении (геологический термин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "омолаживаться",
                                "target_lang_sense_clarification": "становиться моложе (о геологических слоях)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
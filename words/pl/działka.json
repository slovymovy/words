    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f3afe975-8396-355e-a586-69251ba33be8",
                    "sense_definition": "Niewielki ogród, często wchodzący w skład pracowniczych lub rodzinnych ogrodów działkowych, służący do rekreacji i uprawy roślin",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "land_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdy słoneczny weekend całą rodziną jeździmy na naszą <działkę>.",
                            "target_lang_translations": {
                                "ru": "Каждые солнечные выходные мы всей семьей ездим на нашу <w>дачу</w>."
                            }
                        },
                        {
                            "text": "Mój dziadek ma mały domek na <działce> i uprawia tam warzywa.",
                            "target_lang_translations": {
                                "ru": "У моего дедушки есть маленький домик на <w>дачном участке</w>, и он выращивает там овощи."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogród działkowy",
                        "ogródek działkowy"
                    ],
                    "common_phrases": [
                        "jechać na działkę",
                        "pracować na działce",
                        "domek na działce",
                        "ogródek na działce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой сад, часто входящий в состав дачных или семейных садовых товариществ, служащий для отдыха и выращивания растений"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дача",
                                "target_lang_sense_clarification": "загородный дом с участком"
                            },
                            {
                                "target_lang_word": "садовый участок",
                                "target_lang_sense_clarification": "территория для садоводства"
                            },
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "кусок земли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "941e733d-4486-3575-9a19-8ca4bc6c49da",
                    "sense_definition": "Wyznaczony obszar ziemi, będący odrębną własnością, przeznaczony pod zabudowę lub uprawę",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "land_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice kupili <działkę> budowlaną na obrzeżach miasta.",
                            "target_lang_translations": {
                                "ru": "Родители купили строительный <w>участок</w> на окраине города."
                            }
                        },
                        {
                            "text": "Ta <działka> ma świetną lokalizację blisko lasu i jeziora.",
                            "target_lang_translations": {
                                "ru": "Этот <w>участок</w> имеет отличное расположение рядом с лесом и озером."
                            }
                        }
                    ],
                    "synonyms": [
                        "parcela",
                        "grunt",
                        "ziemia"
                    ],
                    "common_phrases": [
                        "działka budowlana",
                        "kupić działkę",
                        "granica działki",
                        "numer działki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделенная территория земли, являющаяся отдельной собственностью, предназначенная для застройки или выращивания культур"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "кусок земли"
                            },
                            {
                                "target_lang_word": "земельный участок",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef6b70db-1d23-3bdd-8ea6-b028f3b190ea",
                    "sense_definition": "Forma fleksyjna (dopełniacz liczby pojedynczej lub mianownik liczby mnogiej) słowa <w>działko</w> oznaczającego małe działo lub element uzbrojenia",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kaliber tego <działka> przeciwlotniczego wynosi dwadzieścia milimetrów.",
                            "target_lang_translations": {
                                "ru": "Калибр этой зенитной <w>пушки</w> составляет двадцать миллиметров."
                            }
                        },
                        {
                            "text": "Myśliwiec był uzbrojony w dwa <działka> zamontowane pod skrzydłami.",
                            "target_lang_translations": {
                                "ru": "Истребитель был вооружен двумя <w>пушками</w>, установленными под крыльями."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection of działko"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (родительный падеж единственного числа или именительный падеж множественного числа) слова <w>działko</w>, означающего малокалиберную пушку или элемент вооружения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пушка",
                                "target_lang_sense_clarification": "орудие"
                            },
                            {
                                "target_lang_word": "малокалиберная пушка",
                                "target_lang_sense_clarification": "небольшое орудие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5e1297b-b7db-38fb-8f4f-f07095418e47",
                    "sense_definition": "Część zysków, pieniędzy lub łupu, która przypada jednej osobie w ramach podziału",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "share",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po udanej transakcji każdy wspólnik otrzymał swoją <działkę>.",
                            "target_lang_translations": {
                                "ru": "После успешной сделки каждый партнер получил свою <w>долю</w>."
                            }
                        },
                        {
                            "text": "Musisz mi odpalić <działkę> za pomoc w załatwieniu tej sprawy.",
                            "target_lang_translations": {
                                "ru": "Ты должен отстегнуть мне <w>долю</w> за помощь в решении этого дела."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dostać swoją działkę",
                        "odpalić komuś działkę",
                        "działka z zysków"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть прибыли, денег или добычи, причитающаяся одному человеку при разделе"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доля",
                                "target_lang_sense_clarification": "часть чего-либо"
                            },
                            {
                                "target_lang_word": "часть",
                                "target_lang_sense_clarification": "кусок, фрагмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a256278f-e6e5-3508-b79c-a9492a8a453c",
                    "sense_definition": "Jednorazowa porcja narkotyku przygotowana do zażycia",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja zatrzymała mężczyznę, który posiadał przy sobie jedną <działkę> marihuany.",
                            "target_lang_translations": {
                                "ru": "Полиция задержала мужчину, у которого была при себе одна <w>доза</w> марихуаны."
                            }
                        },
                        {
                            "text": "Uzależniony zrobiłby wszystko, żeby zdobyć kolejną <działkę>.",
                            "target_lang_translations": {
                                "ru": "Зависимый сделал бы всё, чтобы достать очередную <w>дозу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kupić działkę",
                        "pierwsza działka"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разовая порция наркотика, подготовленная для употребления"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доза",
                                "target_lang_sense_clarification": "порция вещества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0490e29-770a-331b-9d96-81907225e41e",
                    "sense_definition": "Pojedynczy listek kielicha kwiatu, będący częścią jego okwiatu",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każda <działka> kielicha chroni pąk kwiatowy przed uszkodzeniem.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>чашелистик</w> защищает цветочный бутон от повреждений."
                            }
                        },
                        {
                            "text": "U tego gatunku <działki> są jaskrawo ubarwione i przypominają płatki.",
                            "target_lang_translations": {
                                "ru": "У этого вида <w>чашелистики</w> ярко окрашены и напоминают лепестки."
                            }
                        }
                    ],
                    "synonyms": [
                        "działka kielicha"
                    ],
                    "common_phrases": [
                        "działka kielicha",
                        "wolna działka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный листок чашечки цветка, являющийся частью его околоцветника"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чашелистик",
                                "target_lang_sense_clarification": "ботанический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

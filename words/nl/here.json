                    "sense_id": "0c5f28b8-07f9-3b01-a30b-e79adc9ddf30",
                    "sense_definition": "Aanduiding voor God, vaak gebruikt in de Bijbel en christelijke taal.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_entity",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Wij bidden elke avond tot de <w>Here</w> voor bescherming.",
                            "target_lang_translations": {
                                "ru": "Мы каждый вечер молимся <w>Господу</w> о защите."
                            }
                        },
                        {
                            "text": "De <w>Here</w> is mijn herder, mij ontbreekt niets.",
                            "target_lang_translations": {
                                "ru": "<w>Господь</w> — Пастырь мой; я ни в чем не буду нуждаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "God",
                        "Heer",
                        "Eeuwige",
                        "Adonai",
                        "Jahwe"
                    ],
                    "common_phrases": [
                        "de Here God",
                        "de Here zij met u",
                        "in de naam des Heren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначение Бога, часто используемое в Библии и христианском языке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Господь",
                                "target_lang_sense_clarification": "Бог"

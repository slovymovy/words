    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6cfb3089-e0c3-3317-aa01-f87f6b5b6321",
                    "sense_definition": "Одержать верх над противником в бою, соревновании или споре; выиграть.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша команда <w>победила</w> в важном матче со счётом два ноль.",
                            "target_lang_translations": {
                                "en": "Our team <w>won</w> the important match with a score of two-nil."
                            }
                        },
                        {
                            "text": "Чтобы <w>победить</w> сильного соперника, нужно много тренироваться.",
                            "target_lang_translations": {
                                "en": "To <w>defeat</w> a strong opponent, you need to train a lot."
                            }
                        }
                    ],
                    "synonyms": [
                        "выиграть",
                        "одержать победу"
                    ],
                    "antonyms": [
                        "проиграть",
                        "потерпеть поражение"
                    ],
                    "common_phrases": [
                        "победить врага",
                        "победить в войне",
                        "победить в конкурсе",
                        "победить соперника"
                    ],
                    "target_lang_definitions": {
                        "en": "To gain the upper hand over an opponent in a fight, competition, or argument; to win."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "win",
                                "target_lang_sense_clarification": "a game, a competition, a war"
                            },
                            {
                                "target_lang_word": "defeat",
                                "target_lang_sense_clarification": "an opponent, an enemy"
                            },
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "an opponent (informal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9ba36d9-4498-373a-9d75-17e42da9ca81",
                    "sense_definition": "Справиться с чем-либо неприятным или трудным (болезнью, страхом, препятствиями); преодолеть.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он смог <w>победить</w> свой страх высоты и поднялся на гору.",
                            "target_lang_translations": {
                                "en": "He was able to <w>conquer</w> his fear of heights and climbed the mountain."
                            }
                        },
                        {
                            "text": "Врачи делают всё возможное, чтобы <w>победить</w> опасную болезнь.",
                            "target_lang_translations": {
                                "en": "Doctors are doing everything possible to <w>beat</w> the dangerous disease."
                            }
                        }
                    ],
                    "synonyms": [
                        "преодолеть",
                        "побороть",
                        "превозмочь"
                    ],
                    "common_phrases": [
                        "победить страх",
                        "победить болезнь",
                        "победить себя",
                        "победить бедность"
                    ],
                    "target_lang_definitions": {
                        "en": "To successfully deal with something difficult or unpleasant (like illness, fear, or obstacles); to overcome."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overcome",
                                "target_lang_sense_clarification": "difficulties, obstacles"
                            },
                            {
                                "target_lang_word": "conquer",
                                "target_lang_sense_clarification": "fear, a habit, a peak"
                            },
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "a disease, an addiction"
                            }
                        ]
                    }
                }
            ]
        }
    ]

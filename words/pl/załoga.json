{
    "word_family": [
        "załogowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "26cb96a6-8fd7-36b1-bbae-cec5cceb29db",
                    "sense_definition": "Zespół ludzi wyznaczony do obsługi statku, samolotu, czołgu lub innego dużego pojazdu",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kapitan rozkazał, aby cała <w>załoga</w> natychmiast zeszła na ląd.",
                            "target_lang_translations": {
                                "ru": "Капитан приказал, чтобы вся <w>команда</w> немедленно сошла на берег."
                            }
                        },
                        {
                            "text": "Dzielna <w>załoga</w> czołgu walczyła na froncie do ostatniej chwili.",
                            "target_lang_translations": {
                                "ru": "Отважный <w>экипаж</w> танка сражался на фронте до последнего момента."
                            }
                        }
                    ],
                    "synonyms": [
                        "obsada",
                        "ekipa",
                        "drużyna",
                        "skład"
                    ],
                    "common_phrases": [
                        "załoga statku",
                        "załoga samolotu",
                        "członek załogi",
                        "kapitan i załoga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, назначенная для обслуживания судна, самолета, танка или другого крупного транспортного средства"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экипаж",
                                "target_lang_sense_clarification": "персонал транспортного средства (самолета, танка, судна)"
                            },
                            {
                                "target_lang_word": "команда",
                                "target_lang_sense_clarification": "личный состав судна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "631b27f3-11aa-3e85-bcf8-b9663149a7df",
                    "sense_definition": "Ogół pracowników zatrudnionych w jednym zakładzie pracy, fabryce lub instytucji",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor podziękował całej <w>załodze</w> za ciężką pracę w minionym roku.",
                            "target_lang_translations": {
                                "ru": "Директор поблагодарил весь <w>коллектив</w> за тяжелую работу в прошлом году."
                            }
                        },
                        {
                            "text": "<w>Załoga</w> kopalni ogłosiła strajk i przerwała wydobycie węgla.",
                            "target_lang_translations": {
                                "ru": "<w>Коллектив</w> шахты объявил забастовку и прекратил добычу угля."
                            }
                        }
                    ],
                    "synonyms": [
                        "personel",
                        "kadra",
                        "pracownicy"
                    ],
                    "common_phrases": [
                        "załoga fabryki",
                        "spotkanie z załogą",
                        "strajk załogi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность работников, занятых на одном предприятии, заводе или в учреждении"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коллектив",
                                "target_lang_sense_clarification": "группа людей, объединенных общей работой"
                            },
                            {
                                "target_lang_word": "персонал",
                                "target_lang_sense_clarification": "штат сотрудников"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b1d780a-8d9d-3b6a-9ab5-619c02cd3e4f",
                    "sense_definition": "Oddział wojskowy stacjonujący w twierdzy, zamku lub na placówce obronnej; garnizon",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nieliczna <w>załoga</w> broniła murów zamku przed atakiem wroga przez wiele tygodni.",
                            "target_lang_translations": {
                                "ru": "Малочисленный <w>гарнизон</w> защищал стены замка от атаки врага в течение многих недель."
                            }
                        },
                        {
                            "text": "Komendant poddał miasto, aby ocalić życie swojej wycieńczonej <w>załogi</w>.",
                            "target_lang_translations": {
                                "ru": "Комендант сдал город, чтобы спасти жизнь своего измученного <w>гарнизона</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "obsada"
                    ],
                    "common_phrases": [
                        "załoga twierdzy",
                        "załoga zamku",
                        "załoga wojskowa"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "used mostly in historical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Воинское подразделение, размещенное в крепости, замке или на оборонительном пункте; гарнизон"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гарнизон",
                                "target_lang_sense_clarification": "войска, занимающие укрепленный пункт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20663325-c724-384b-b60e-9cf30e5cde5e",
                    "sense_definition": "Dawniej: zabezpieczenie, gwarancja lub rękojmia",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz dał swoje słowo jako <w>załogę</w>, że dotrzyma warunków rozejmu.",
                            "target_lang_translations": {
                                "ru": "Рыцарь дал свое слово в качестве <w>залога</w>, что соблюдет условия перемирия."
                            }
                        },
                        {
                            "text": "Kupiec zażądał majątku dłużnika jako pewnej <w>załogi</w> spłaty długu.",
                            "target_lang_translations": {
                                "ru": "Купец потребовал имущество должника в качестве надежного <w>залога</w> возврата долга."
                            }
                        }
                    ],
                    "synonyms": [
                        "gwarancja",
                        "zapewnienie"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устар.: обеспечение, гарантия или поручительство"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "залог",
                                "target_lang_sense_clarification": "гарантия обязательств"
                            },
                            {
                                "target_lang_word": "гарантия",
                                "target_lang_sense_clarification": "ручательство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eaae4b84-70c2-357a-93aa-8cfd708949ac",
                    "sense_definition": "Dawniej: przeszkoda, zawada lub coś, co tamuje ruch",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powalone przez wichurę drzewo stanowiło niebezpieczną <w>załogę</w> na leśnej drodze.",
                            "target_lang_translations": {
                                "ru": "Поваленное бурей дерево представляло собой опасную <w>преграду</w> на лесной дороге."
                            }
                        },
                        {
                            "text": "Wędrowcy musieli ominąć rzeczną <w>załogę</w>, aby kontynuować podróż łodzią.",
                            "target_lang_translations": {
                                "ru": "Путники должны были обогнуть речную <w>преграду</w>, чтобы продолжить путешествие на лодке."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeszkoda",
                        "zapora",
                        "zator",
                        "utrudnienie"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устар.: препятствие, помеха или что-то, что блокирует движение"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преграда",
                                "target_lang_sense_clarification": "препятствие на пути"
                            },
                            {
                                "target_lang_word": "помеха",
                                "target_lang_sense_clarification": "то, что мешает"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
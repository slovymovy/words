    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7e9a2ff3-d5a9-30ea-8d63-9b3d7d771189",
                    "sense_definition": "Zakończyć się pomyślnie, przynieść oczekiwany rezultat (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "success_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam nadzieję, że nasz plan <uda> się zrealizować bez problemów.",
                            "target_lang_translations": {
                                "ru": "Я надеюсь, что наш план <удастся> реализовать без проблем."
                            }
                        },
                        {
                            "text": "Ciasto <udało> się znakomicie i wszystkim gościom bardzo smakowało.",
                            "target_lang_translations": {
                                "ru": "Пирог <получился> на славу, и всем гостям очень понравился."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdarzyć",
                        "spodobać się"
                    ],
                    "common_phrases": [
                        "to się uda",
                        "wszystko się udało",
                        "nic się nie udało"
                    ],
                    "target_lang_definitions": {
                        "ru": "Завершиться успешно, принести ожидаемый результат (используется с возвратной частицей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удаться",
                                "target_lang_sense_clarification": "завершиться успехом"
                            },
                            {
                                "target_lang_word": "получиться",
                                "target_lang_sense_clarification": "выйти хорошо, дать результат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb14bdde-9e51-3646-a670-c1bd1cab6947",
                    "sense_definition": "Stworzyć mylne wrażenie, że coś jest prawdą, lub zachować się nienaturalnie, aby kogoś zmylić.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "feign_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopiec postanowił <udać> chorobę, żeby nie musieć iść do szkoły.",
                            "target_lang_translations": {
                                "ru": "Мальчик решил <симулировать> болезнь, чтобы не идти в школу."
                            }
                        },
                        {
                            "text": "Kiedy go zobaczyła, próbowała <udać>, że go nie poznaje.",
                            "target_lang_translations": {
                                "ru": "Когда она его увидела, то попыталась <сделать вид>, что не узнаёт его."
                            }
                        }
                    ],
                    "synonyms": [
                        "oszukać"
                    ],
                    "common_phrases": [
                        "udać Greka",
                        "udać zdziwienie",
                        "udać niewiniątko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создать ложное впечатление, что что-то является правдой, или вести себя неестественно, чтобы ввести кого-то в заблуждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "притвориться",
                                "target_lang_sense_clarification": "сделать вид"
                            },
                            {
                                "target_lang_word": "изобразить",
                                "target_lang_sense_clarification": "разыграть, представить собой"
                            },
                            {
                                "target_lang_word": "симулировать",
                                "target_lang_sense_clarification": "ложно воспроизвести (например, болезнь)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c4eb97e-da8c-3153-be73-9d5e51ccd22d",
                    "sense_definition": "Pójść lub pojechać w określone miejsce (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po uroczystym obiedzie wszyscy goście <udali> się na spacer do ogrodu.",
                            "target_lang_translations": {
                                "ru": "После торжественного обеда все гости <отправились> на прогулку в сад."
                            }
                        },
                        {
                            "text": "Musisz <udać> się do urzędu miasta, aby złożyć ten wniosek.",
                            "target_lang_translations": {
                                "ru": "Ты должен <обратиться> в городскую администрацию, чтобы подать это заявление."
                            }
                        }
                    ],
                    "common_phrases": [
                        "udać się w podróż",
                        "udać się na spoczynek",
                        "udać się do lekarza"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пойти или поехать в определённое место (используется с возвратной частицей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправиться",
                                "target_lang_sense_clarification": "пойти или поехать"
                            },
                            {
                                "target_lang_word": "направиться",
                                "target_lang_sense_clarification": "взять курс куда-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]

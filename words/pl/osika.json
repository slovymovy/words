    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f2a4d709-d1e9-3a14-9ee0-1a9f42e84ee2",
                    "sense_definition": "Gatunek drzewa liściastego z rodziny wierzbowatych, charakteryzującego się liśćmi, które poruszają się nawet przy najmniejszym wietrze; topola drżąca.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za moim oknem rośnie pięć wysokich <osik>.",
                            "target_lang_translations": {
                                "ru": "За моим окном растет пять высоких <осин>."
                            }
                        },
                        {
                            "text": "Liście <osiki> zaczęły szumieć, zapowiadając nadchodzącą burzę.",
                            "target_lang_translations": {
                                "ru": "Листья <осины> зашумели, предвещая приближающуюся грозу."
                            }
                        }
                    ],
                    "synonyms": [
                        "topola drżąca",
                        "osina"
                    ],
                    "common_phrases": [
                        "drżeć jak osika",
                        "trząść się jak osika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид лиственного дерева из семейства ивовых, отличающийся листьями, которые трепещут даже при малейшем ветре; тополь дрожащий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осина",
                                "target_lang_sense_clarification": "дерево"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7eecf30-10f4-3d73-99e0-211a699d4804",
                    "sense_definition": "Drewno pozyskiwane z drzewa osiki, miękkie i lekkie, wykorzystywane w stolarstwie i rzeźbiarstwie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mebel został wykonany z jasnej i lekkiej <osiki>.",
                            "target_lang_translations": {
                                "ru": "Мебель была изготовлена из светлой и легкой <осины>."
                            }
                        },
                        {
                            "text": "Do rzeźbienia figurek najlepiej nadaje się miękka <osika>.",
                            "target_lang_translations": {
                                "ru": "Для вырезания фигурок лучше всего подходит мягкая <осина>."
                            }
                        }
                    ],
                    "synonyms": [
                        "osina"
                    ],
                    "common_phrases": [
                        "kołek z osiki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древесина осины, мягкая и легкая, используемая в столярном деле и резьбе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осина",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "75666dd4-73ff-33f2-9a2f-5131aba6cd7c",
                    "sense_definition": "Trzecia osoba liczby pojedynczej czasu przyszłego od <w>osikać</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli nie wypuścisz psa na dwór, to <osika> dywan w salonie.",
                            "target_lang_translations": {
                                "ru": "Если не выпустишь собаку на улицу, она <описает> ковер в гостиной."
                            }
                        },
                        {
                            "text": "Uważaj przy przewijaniu, bo niemowlę cię <osika>.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен при пеленании, иначе младенец тебя <описает>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "future tense form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма третьего лица единственного числа будущего времени глагола <w>osikać</w> (описать)."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]
}
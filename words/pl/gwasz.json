    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "12056fd8-e970-3bed-9db7-bd3ad8714419",
                    "sense_definition": "Kryjąca farba wodna z domieszką kredy lub bieli, która po wyschnięciu staje się matowa.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Komplet dwunastu <w>gwaszy</w> w tubach kosztuje w tym sklepie tylko piętnaście złotych.",
                            "target_lang_translations": {
                                "ru": "Набор из двенадцати тюбиков <w>гуаши</w> стоит в этом магазине всего пятнадцать злотых."
                            }
                        },
                        {
                            "text": "Do namalowania tła artysta użył gęstego, ciemnego <w>gwaszu</w>.",
                            "target_lang_translations": {
                                "ru": "Для написания фона художник использовал густую, темную <w>гуашь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "plakatówka"
                    ],
                    "antonyms": [
                        "akwarela"
                    ],
                    "common_phrases": [
                        "malować gwaszem",
                        "gwasz w tubce",
                        "zestaw gwaszy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кроющая водорастворимая краска с примесью мела или белил, которая после высыхания становится матовой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гуашь",
                                "target_lang_sense_clarification": "краска"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f970955a-4c43-3f04-a78b-f3f90d3ab0e2",
                    "sense_definition": "Technika malarska wykorzystująca kryjące farby wodne, różniąca się od akwareli możliwością nanoszenia poprawek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten znany malarz najchętniej tworzy swoje dzieła w technice <w>gwaszu</w>.",
                            "target_lang_translations": {
                                "ru": "Этот известный художник охотнее всего создает свои произведения в технике <w>гуаши</w>."
                            }
                        },
                        {
                            "text": "Biegle posługiwał się <w>gwaszem</w>, stosując szerokie i pewne pociągnięcia pędzla.",
                            "target_lang_translations": {
                                "ru": "Он мастерски владел <w>гуашью</w>, нанося широкие и уверенные мазки кистью."
                            }
                        }
                    ],
                    "antonyms": [
                        "akwarela"
                    ],
                    "common_phrases": [
                        "technika gwaszu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Техника живописи, использующая кроющие водорастворимые краски, отличающаяся от акварели возможностью внесения исправлений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гуашь",
                                "target_lang_sense_clarification": "техника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f88f1db4-8bf8-358b-a8f1-92eee958a336",
                    "sense_definition": "Obraz namalowany techniką gwaszu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum narodowym oglądaliśmy piękny, osiemnastowieczny <w>gwasz</w>.",
                            "target_lang_translations": {
                                "ru": "В национальном музее мы рассматривали прекрасную <w>гуашь</w> восемнадцатого века."
                            }
                        },
                        {
                            "text": "Na ścianie w salonie wisi kolorowy <w>gwasz</w> przedstawiający letni krajobraz.",
                            "target_lang_translations": {
                                "ru": "На стене в гостиной висит красочная <w>гуашь</w>, изображающая летний пейзаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "malowidło",
                        "obraz"
                    ],
                    "common_phrases": [
                        "wystawa gwaszy",
                        "kolekcja gwaszy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Картина, написанная в технике гуаши."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гуашь",
                                "target_lang_sense_clarification": "картина"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "hemelen",
        "hemels"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f61f8f0e-1268-3b29-8d6f-0d2f9f39ea88",
                    "sense_definition": "De ruimte boven de aarde waar men de wolken, de zon, de maan en de sterren ziet.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>hemel</w> was helder en vol fonkelende sterren.",
                            "target_lang_translations": {
                                "en": "The <w>sky</w> was clear and full of twinkling stars."
                            }
                        },
                        {
                            "text": "Er is vandaag geen enkel wolkje aan de <w>hemel</w> te zien.",
                            "target_lang_translations": {
                                "en": "There is not a single cloud to be seen in the <w>sky</w> today."
                            }
                        }
                    ],
                    "synonyms": [
                        "lucht",
                        "firmament",
                        "uitspansel",
                        "zwerk"
                    ],
                    "antonyms": [
                        "aarde"
                    ],
                    "common_phrases": [
                        "aan de hemel",
                        "strakblauwe hemel",
                        "open hemel",
                        "onder de blote hemel"
                    ],
                    "target_lang_definitions": {
                        "en": "The space above the earth where one sees clouds, the sun, the moon, and the stars."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sky",
                                "target_lang_sense_clarification": "The atmosphere and outer space viewed from the earth"
                            },
                            {
                                "target_lang_word": "firmament",
                                "target_lang_sense_clarification": "Literary or poetic term for the sky"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7470a4b3-e611-3254-9eda-81762f44d211",
                    "sense_definition": "De plaats waar God of goden wonen en waar gelovigen hopen te komen na de dood.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel mensen geloven dat goede zielen naar de <w>hemel</w> gaan.",
                            "target_lang_translations": {
                                "en": "Many people believe that good souls go to <w>heaven</w>."
                            }
                        },
                        {
                            "text": "Hij vindt troost in de gedachte dat zijn ouders in de <w>hemel</w> zijn.",
                            "target_lang_translations": {
                                "en": "He finds comfort in the thought that his parents are in <w>heaven</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "hel"
                    ],
                    "common_phrases": [
                        "in de hemel",
                        "naar de hemel gaan",
                        "koninkrijk der hemelen"
                    ],
                    "target_lang_definitions": {
                        "en": "The place where God or gods live and where believers hope to go after death."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heaven",
                                "target_lang_sense_clarification": "The abode of God and the afterlife"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3eb7303d-b127-3bbb-8309-72364b94db01",
                    "sense_definition": "Een toestand van groot geluk of een zeer aangename plek of ervaring.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was de <w>hemel</w> om na de lange reis eindelijk in een zacht bed te liggen.",
                            "target_lang_translations": {
                                "en": "It was <w>heaven</w> to finally lie in a soft bed after the long journey."
                            }
                        },
                        {
                            "text": "Voor liefhebbers van chocolade is deze winkel de ware <w>hemel</w>.",
                            "target_lang_translations": {
                                "en": "For chocolate lovers, this shop is true <w>heaven</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "hel"
                    ],
                    "common_phrases": [
                        "de hemel op aarde",
                        "in de zevende hemel"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of great happiness or a very pleasant place or experience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heaven",
                                "target_lang_sense_clarification": "A state of supreme happiness"
                            },
                            {
                                "target_lang_word": "paradise",
                                "target_lang_sense_clarification": "A place or state of perfect happiness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a034f69-b88c-300d-b399-06eda254aee9",
                    "sense_definition": "Een overkapping van stof boven een bed, troon of altaar.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het antieke bed in het kasteel had een zware fluwelen <w>hemel</w>.",
                            "target_lang_translations": {
                                "en": "The antique bed in the castle had a heavy velvet <w>canopy</w>."
                            }
                        },
                        {
                            "text": "Tijdens de processie liep de priester onder een gouden <w>hemel</w>.",
                            "target_lang_translations": {
                                "en": "During the procession, the priest walked under a golden <w>canopy</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "draaghemel",
                        "troonhemel",
                        "hoogte"
                    ],
                    "target_lang_definitions": {
                        "en": "A fabric covering above a bed, throne, or altar."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "canopy",
                                "target_lang_sense_clarification": "An ornamental cloth covering hung over a bed or throne"
                            },
                            {
                                "target_lang_word": "tester",
                                "target_lang_sense_clarification": "A canopy over a four-poster bed"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "4db6381d-1a89-3a5d-8678-0e139ba7465d",
                    "sense_definition": "Een uitroep van verbazing, schrik, ongeloof of wanhoop.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hemel</w>, wat is er hier voor een rommel gemaakt!",
                            "target_lang_translations": {
                                "en": "<w>Heavens</w>, what a mess has been made here!"
                            }
                        },
                        {
                            "text": "Grote <w>hemel</w>, ik ben helemaal vergeten de deur op slot te doen.",
                            "target_lang_translations": {
                                "en": "Good <w>heavens</w>, I completely forgot to lock the door."
                            }
                        }
                    ],
                    "synonyms": [
                        "mijn hemel"
                    ],
                    "common_phrases": [
                        "lieve hemel",
                        "grote hemel"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation of surprise, shock, disbelief, or despair."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heavens",
                                "target_lang_sense_clarification": "Exclamation of surprise or dismay"
                            },
                            {
                                "target_lang_word": "good heavens",
                                "target_lang_sense_clarification": "Emphatic exclamation of surprise"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6b7a823f-1bcb-3beb-aa97-8ba58f91a2f7",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>hemelen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>hemel</w> het plafond van de kinderkamer met blauwe stof.",
                            "target_lang_translations": {
                                "en": "I <w>canopy</w> the nursery ceiling with blue fabric."
                            }
                        },
                        {
                            "text": "Als ik zo doorga met werken, <w>hemel</w> ik nog voor mijn pensioen.",
                            "target_lang_translations": {
                                "en": "If I keep working like this, I'll <w>kick the bucket</w> before my retirement."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'hemelen'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kick the bucket",
                                "target_lang_sense_clarification": "To die (informal/slang)"
                            },
                            {
                                "target_lang_word": "canopy",
                                "target_lang_sense_clarification": "To cover with a canopy (rare)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
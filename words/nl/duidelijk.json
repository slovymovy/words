{
    "word_family": [
        "duidelijkheid",
        "verduidelijken"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5dd3fea3-3b9d-3ddc-870e-befe65618162",
                    "sense_definition": "Gemakkelijk te begrijpen en niet ingewikkeld",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "begrip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar gaf een <w>duidelijke</w> uitleg aan de klas.",
                            "target_lang_translations": {
                                "en": "The teacher gave a <w>clear</w> explanation to the class.",
                                "ru": "Учитель дал классу <w>понятное</w> объяснение."
                            }
                        },
                        {
                            "text": "Is het voor iedereen <w>duidelijk</w> wat we gaan doen?",
                            "target_lang_translations": {
                                "en": "Is it <w>clear</w> to everyone what we are going to do?",
                                "ru": "Всем <w>понятно</w>, что мы будем делать?"
                            }
                        }
                    ],
                    "synonyms": [
                        "begrijpelijk",
                        "helder"
                    ],
                    "antonyms": [
                        "onduidelijk"
                    ],
                    "common_phrases": [
                        "heel duidelijk",
                        "alles duidelijk",
                        "niet duidelijk"
                    ],
                    "target_lang_definitions": {
                        "en": "Easy to understand and not complicated",
                        "ru": "Легкий для понимания, несложный"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "Free of ambiguity or doubt; easily understood"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "понятный",
                                "target_lang_sense_clarification": "доступный для понимания"
                            },
                            {
                                "target_lang_word": "ясный",
                                "target_lang_sense_clarification": "недвусмысленный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5016887f-4670-3df1-80ff-afead50e5c67",
                    "sense_definition": "Waarover geen twijfel bestaat; onmiskenbaar",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "zekerheid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is een <w>duidelijk</w> verschil tussen deze twee kleuren.",
                            "target_lang_translations": {
                                "en": "There is a <w>clear</w> difference between these two colors.",
                                "ru": "Между этими двумя цветами есть <w>явная</w> разница."
                            }
                        },
                        {
                            "text": "Het is <w>duidelijk</w> dat hij geen zin heeft om te werken.",
                            "target_lang_translations": {
                                "en": "It is <w>clear</w> that he doesn't feel like working.",
                                "ru": "<w>Очевидно</w>, что у него нет желания работать."
                            }
                        }
                    ],
                    "synonyms": [
                        "onmiskenbaar",
                        "overduidelijk"
                    ],
                    "antonyms": [
                        "onduidelijk"
                    ],
                    "common_phrases": [
                        "een duidelijke zaak",
                        "volkomen duidelijk"
                    ],
                    "target_lang_definitions": {
                        "en": "About which there is no doubt; unmistakable",
                        "ru": "Не вызывающий сомнений; очевидный"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "Free of doubt; certain"
                            },
                            {
                                "target_lang_word": "obvious",
                                "target_lang_sense_clarification": "Easily discovered, seen, or understood; self-explanatory"
                            },
                            {
                                "target_lang_word": "plain",
                                "target_lang_sense_clarification": "Evident to one's senses or reason; manifest"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "очевидный",
                                "target_lang_sense_clarification": "несомненный"
                            },
                            {
                                "target_lang_word": "явный",
                                "target_lang_sense_clarification": "открытый, нескрываемый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f41c4915-bc7a-37ae-bd03-8cbd6de78fbe",
                    "sense_definition": "Goed te zien, te horen of te herkennen",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "waarneming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de foto is zijn gezicht heel <w>duidelijk</w> te zien.",
                            "target_lang_translations": {
                                "en": "His face is very <w>clearly</w> visible in the photo.",
                                "ru": "На фотографии его лицо видно очень <w>чётко</w>."
                            }
                        },
                        {
                            "text": "De zanger was achter in de zaal niet <w>duidelijk</w> te horen.",
                            "target_lang_translations": {
                                "en": "The singer could not be heard <w>clearly</w> in the back of the hall.",
                                "ru": "Певца в конце зала было слышно не <w>отчётливо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "scherp",
                        "zichtbaar"
                    ],
                    "antonyms": [
                        "onduidelijk"
                    ],
                    "common_phrases": [
                        "duidelijk zichtbaar",
                        "duidelijk beeld"
                    ],
                    "target_lang_definitions": {
                        "en": "Easy to see, hear, or recognize",
                        "ru": "Хорошо видимый, слышимый или различимый"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "Distinct, sharp, or easily heard"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чёткий",
                                "target_lang_sense_clarification": "резкий, хорошо видимый"
                            },
                            {
                                "target_lang_word": "отчётливый",
                                "target_lang_sense_clarification": "хорошо различимый (звук/образ)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "58d8b96b-6247-3d39-8c4d-82790a6ee1d4",
                    "sense_definition": "Op een heldere en begrijpelijke manier",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "wijze",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet <w>duidelijk</w> praten zodat iedereen je verstaat.",
                            "target_lang_translations": {
                                "en": "You must speak <w>clearly</w> so that everyone understands you.",
                                "ru": "Ты должен говорить <w>внятно</w>, чтобы все тебя понимали."
                            }
                        },
                        {
                            "text": "Hij heeft <w>duidelijk</w> gemaakt dat hij het er niet mee eens is.",
                            "target_lang_translations": {
                                "en": "He has made it <w>clear</w> that he does not agree.",
                                "ru": "Он <w>ясно</w> дал понять, что не согласен с этим."
                            }
                        }
                    ],
                    "synonyms": [
                        "helder"
                    ],
                    "antonyms": [

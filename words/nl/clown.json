    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6fb6da58-9246-3869-aba6-b9d8e0afe77a",
                    "sense_definition": "Een komische artiest met een geschminkt gezicht en opvallende kleding, die mensen vermaakt in een circus of tijdens optredens.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "entertainer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen lachten hard om de grappen van de <w>clown</w>.",
                            "target_lang_translations": {
                                "en": "The children laughed loudly at the <w>clown</w>'s jokes.",
                                "ru": "Дети громко смеялись над шутками <w>клоуна</w>."
                            }
                        },
                        {
                            "text": "In het circus zagen we een <w>clown</w> met hele grote schoenen.",
                            "target_lang_translations": {
                                "en": "In the circus, we saw a <w>clown</w> with very big shoes.",
                                "ru": "В цирке мы видели <w>клоуна</w> в огромных ботинках."
                            }
                        }
                    ],
                    "synonyms": [
                        "pipo"
                    ],
                    "common_phrases": [
                        "de clown uithangen",
                        "een lachende clown"
                    ],
                    "target_lang_definitions": {
                        "en": "A comic entertainer with a painted face and striking clothes, who entertains people in a circus or during performances.",
                        "ru": "Комический артист в гриме и яркой одежде, развлекающий публику в цирке или на представлениях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clown",
                                "target_lang_sense_clarification": "circus entertainer"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "клоун",
                                "target_lang_sense_clarification": "артист цирка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0abda0c0-eb80-3be9-a694-9d17669298ad",
                    "sense_definition": "Iemand die zich dwaas of belachelijk gedraagt en daardoor niet serieus genomen wordt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gedraagt zich als een <w>clown</w> tijdens de belangrijke vergadering.",
                            "target_lang_translations": {
                                "en": "He behaves like a <w>clown</w> during the important meeting.",
                                "ru": "Он ведет себя как <w>клоун</w> на важном совещании."
                            }
                        },
                        {
                            "text": "Neem hem niet te serieus, hij is soms echt een <w>clown</w>.",
                            "target_lang_translations": {
                                "en": "Don't take him too seriously, he is sometimes a real <w>clown</w>.",
                                "ru": "Не принимай его всерьез, он иногда настоящий <w>клоун</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pipo"
                    ],
                    "common_phrases": [
                        "wat een clown"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe a foolish person"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who behaves foolishly or ridiculously and is therefore not taken seriously.",
                        "ru": "Человек, который ведет себя глупо или нелепо, из-за чего его не принимают всерьез."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clown",
                                "target_lang_sense_clarification": "foolish person"
                            },
                            {
                                "target_lang_word": "buffoon",
                                "target_lang_sense_clarification": "ridiculous person"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "клоун",
                                "target_lang_sense_clarification": "несерьезный человек"
                            },
                            {
                                "target_lang_word": "шут",
                                "target_lang_sense_clarification": "паяц, кривляка"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8879b763-1cfa-34ba-99f7-d1ced1c66071",
                    "sense_definition": "Przemieścić się gwałtownie w dół pod wpływem grawitacji; stracić równowagę i upaść.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_gravity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj na schodach, żebyś przypadkiem nie <spadł>.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен на лестнице, чтобы случайно не <упасть>."
                            }
                        },
                        {
                            "text": "Duże jabłko <spadło> prosto na trawę pod drzewem.",
                            "target_lang_translations": {
                                "ru": "Большое яблоко <упало> прямо на траву под деревом."
                            }
                        },
                        {
                            "text": "Wazon zachwiał się i <spadł> ze stołu na podłogę.",
                            "target_lang_translations": {
                                "ru": "Ваза покачнулась и <свалилась> со стола на пол."
                            }
                        }
                    ],
                    "synonyms": [
                        "upaść",
                        "runąć",
                        "zlecieć"
                    ],
                    "antonyms": [
                        "wznieść się",
                        "podnieść się",
                        "wlecieć"
                    ],
                    "common_phrases": [
                        "spaść z krzesła",
                        "spaść na ziemię",
                        "spaść z deszczem",
                        "spaść na cztery łapy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместиться резко вниз под действием гравитации; потерять равновесие и упасть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упасть",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "свалиться",
                                "target_lang_sense_clarification": "упасть сверху или потерять равновесие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3534a020-74d0-3273-aed6-ca438897f60a",
                    "sense_definition": "Szybko opuścić jakieś miejsce, uciec lub odejść.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "departure_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Impreza jest nudna, więc zaraz stąd <spadamy>.",
                            "target_lang_translations": {
                                "ru": "Вечеринка скучная, так что мы скоро отсюда <сваливаем>."
                            }
                        },
                        {
                            "text": "Kiedy zobaczył policję, postanowił szybko <spaść>.",
                            "target_lang_translations": {
                                "ru": "Увидев полицию, он решил быстро <свалить>."
                            }
                        }
                    ],
                    "synonyms": [
                        "uciec",
                        "zmyć się",
                        "zwiać",
                        "ulotnić się",
                        "wynieść się"
                    ],
                    "antonyms": [
                        "zostać",
                        "przyjść"
                    ],
                    "common_phrases": [
                        "spadaj stąd",
                        "muszę spadać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро покинуть какое-либо место, сбежать или уйти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свалить",
                                "target_lang_sense_clarification": "разг. уйти"
                            },
                            {
                                "target_lang_word": "смыться",
                                "target_lang_sense_clarification": "разг. исчезнуть"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "885f8660-8f96-35e8-9c9a-be16afe3da3d",
                    "sense_definition": "Zużyć zapas paszy na karmienie zwierząt lub wypaść zwierzęta na pastwisku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture_feeding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik <spasł> krowom całą koniczynę z tego pola.",
                            "target_lang_translations": {
                                "ru": "Фермер <скормил> коровам весь клевер с этого поля."
                            }
                        },
                        {
                            "text": "Musimy <spaść> owce na łące, zanim przyjdzie zima.",
                            "target_lang_translations": {
                                "ru": "Мы должны <выпасти> овец на лугу до наступления зимы."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypaść",
                        "skarmić"
                    ],
                    "common_phrases": [
                        "spaść bydło",
                        "spaść łąkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Израсходовать запас корма для животных или использовать пастбище для выпаса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скормить",
                                "target_lang_sense_clarification": "израсходовать корм"
                            },
                            {
                                "target_lang_word": "выпасти",
                                "target_lang_sense_clarification": "о животных на пастбище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eaa71960-10e4-3cd7-807f-dba78b370aef",
                    "sense_definition": "Mocno przytyć, nabrać dużej masy ciała (zwykle o zwierzętach, rzadziej o ludziach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "body_weight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój pies strasznie się <spasł> przez ostatnie miesiące.",
                            "target_lang_translations": {
                                "ru": "Твой пес страшно <растолстел> за последние месяцы."
                            }
                        },
                        {
                            "text": "Nie powinieneś tak się <spaść> na tych słodyczach.",
                            "target_lang_translations": {
                                "ru": "Тебе не стоит так <разжиреть> на этих сладостях."
                            }
                        }
                    ],
                    "synonyms": [
                        "utyć",
                        "przytyć",
                        "roztyć się"
                    ],
                    "antonyms": [
                        "schudnąć",
                        "zmizernieć"
                    ],
                    "common_phrases": [
                        "spaść się jak wieprz"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often pejorative when used for humans"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно прибавить в весе, набрать большую массу тела (обычно о животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растолстеть",
                                "target_lang_sense_clarification": "набрать вес"
                            },
                            {
                                "target_lang_word": "разжиреть",
                                "target_lang_sense_clarification": "сильно потолстеть"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2582f6f4-47ea-33f1-af95-18ba00428d77",
                    "sense_definition": "Płynna lub półpłynna potrawa, często stanowiąca pierwsze danie obiadu, przygotowywana zazwyczaj na wywarze z mięsa lub warzyw.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "potrawa",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wstaniesz od stołu, dopóki nie zjesz <w>zupy</w>!"
                        },
                        {
                            "text": "Na dzisiejszy obiad przygotowałem pyszną <w>zupę</w> pomidorową z ryżem."
                        }
                    ],
                    "synonyms": [
                        "polewka",
                        "supa"
                    ],
                    "common_phrases": [
                        "zupa pomidorowa",
                        "talerz zupy",
                        "gorąca zupa",
                        "łyżka zupy"
                    ]
                },
                {
                    "sense_id": "d5f3b0f0-d289-3728-8ab2-8e52e043fed5",
                    "sense_definition": "Porcja zupy podana w naczyniu, zwykle zamawiana w restauracji.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "potrawa",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner, dla nas dwie <w>zupy</w> dnia proszę!"
                        },
                        {
                            "text": "Zamówiłem drugie danie, ale zrezygnowałem z <w>zupy</w>, bo nie byłem głodny."
                        }
                    ],
                    "common_phrases": [
                        "zupa dnia",
                        "dwie zupy"
                    ]
                },
                {
                    "sense_id": "043385c2-257f-3b13-af93-e742423a1657",
                    "sense_definition": "Masa o konsystencji zbyt rzadkiej, rozgotowanej lub papkowatej; breja.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "konsystencja",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ziemniaki mi się całkiem rozgotowały i zrobiła się z nich <w>zupa</w>."
                        },
                        {
                            "text": "Śnieg stopniał i na chodnikach zrobiła się brudna, błotnista <w>zupa</w>."
                        }
                    ],
                    "synonyms": [
                        "breja",
                        "bryja",
                        "ciapa",
                        "papka",
                        "pulpa"
                    ],
                    "common_phrases": [
                        "zrobiła się zupa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "236f59b0-4976-343a-a3d4-9abc05fb4566",
                    "sense_definition": "Mętna, brudna woda w zbiorniku (np. w jeziorze), często pełna glonów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "konsystencja",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W jeziorze mamy teraz <w>zupę</w>, bo zakwitły glony."
                        },
                        {
                            "text": "Nie wchodź do tej wody, to jakaś zielona <w>zupa</w>, w której nic nie widać."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "96c5e304-09ca-342d-be60-3f903c33b55c",
                    "sense_definition": "Rosół z kury.",

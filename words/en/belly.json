    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a06d6015-9d28-3add-9045-8586b29d4ee0",
                    "sense_definition": "The front part of the human body between the chest and the legs.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He rubbed his round <w>belly</w> after eating a very large dinner.",
                            "target_lang_translations": {
                                "ru": "Он потёр свой круглый <w>живот</w> после очень плотного ужина."
                            }
                        },
                        {
                            "text": "The little boy has a ticklish <w>belly</w> that makes him laugh.",
                            "target_lang_translations": {
                                "ru": "У маленького мальчика щекотный <w>животик</w>, отчего он смеётся."
                            }
                        }
                    ],
                    "synonyms": [
                        "abdomen",
                        "stomach",
                        "tummy",
                        "tum",
                        "midriff"
                    ],
                    "common_phrases": [
                        "beer belly",
                        "belly button",
                        "belly fat",
                        "belly laugh"
                    ],
                    "target_lang_definitions": {

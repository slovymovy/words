{
    "word_family": [
        "enrage",
        "outrage",
        "rager",
        "raging"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "02fe1184-36cc-37b6-90ad-f90e37fd6971",
                    "sense_definition": "Violent uncontrolled anger.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "anger_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He flew into a <w>rage</w> when he discovered that his car had been stolen.",
                            "target_lang_translations": {
                                "ru": "Он пришел в <w>ярость</w>, когда обнаружил, что его машину угнали."
                            }
                        },
                        {
                            "text": "Her face turned red with <w>rage</w> as she read the insulting letter.",
                            "target_lang_translations": {
                                "ru": "Её лицо покраснело от <w>ярости</w>, когда она читала это оскорбительное письмо."
                            }
                        }
                    ],
                    "synonyms": [
                        "fury",
                        "ire"
                    ],
                    "common_phrases": [
                        "road rage",
                        "fly into a rage",
                        "blind rage",
                        "shaking with rage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный, неконтролируемый гнев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ярость",
                                "target_lang_sense_clarification": "сильный гнев"
                            },
                            {
                                "target_lang_word": "бешенство",
                                "target_lang_sense_clarification": "крайняя степень раздражения"
                            },
                            {
                                "target_lang_word": "гнев",
                                "target_lang_sense_clarification": "чувство сильного возмущения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "989b7917-6148-3cf1-986e-bf1fb6dba909",
                    "sense_definition": "A current fashion or fad that is very popular for a short time.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "fashion_trend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Those colorful sneakers were all the <w>rage</w> last summer.",
                            "target_lang_translations": {
                                "ru": "Те разноцветные кроссовки были <w>последним писком моды</w> прошлым летом."
                            }
                        },
                        {
                            "text": "Video games are the current <w>rage</w> among teenagers in the city.",
                            "target_lang_translations": {
                                "ru": "Видеоигры — это нынешний <w>писк моды</w> среди подростков в городе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "all the rage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Модное увлечение, которое очень популярно в течение короткого времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последний крик",
                                "target_lang_sense_clarification": "о моде"
                            },
                            {
                                "target_lang_word": "писк моды",
                                "target_lang_sense_clarification": "идиома"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0a75207-3867-31b7-807e-0ee78cea8dac",
                    "sense_definition": "A subgenre of trap music characterized by aggressive, distorted synthesizers.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The DJ played a set of <w>rage</w> tracks that kept the crowd moving all night.",
                            "target_lang_translations": {
                                "ru": "Диджей сыграл сет из <w>рейдж</w>-треков, которые заставляли толпу двигаться всю ночь."
                            }
                        },
                        {
                            "text": "Many new artists are experimenting with the <w>rage</w> sound in their latest albums.",
                            "target_lang_translations": {
                                "ru": "Многие новые артисты экспериментируют со звучанием <w>рейдж</w> в своих последних альбомах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "music"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поджанр трэп-музыки, характеризующийся агрессивными, искаженными синтезаторами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рейдж",
                                "target_lang_sense_clarification": "музыкальный стиль"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a6e33578-e501-3f4e-88d3-3b8d954f3d20",
                    "sense_definition": "To move or continue with great violence or intensity, such as a storm, fire, or conflict.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "violent_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The storm <w>raged</w> outside the cabin for three days without stopping.",
                            "target_lang_translations": {
                                "ru": "Шторм <w>бушевал</w> снаружи хижины три дня без остановки."
                            }
                        },
                        {
                            "text": "A fierce debate has <w>raged</w> for years about the best way to solve the problem.",
                            "target_lang_translations": {
                                "ru": "Ожесточенные споры <w>бушевали</w> годами о том, как лучше всего решить эту проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "storm"
                    ],
                    "common_phrases": [
                        "rage on",
                        "battle raged",
                        "fire raged"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить или продолжаться с большой силой или интенсивностью (о шторме, огне или конфликте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бушевать",
                                "target_lang_sense_clarification": "о стихии или ветре"
                            },
                            {
                                "target_lang_word": "свирепствовать",
                                "target_lang_sense_clarification": "о шторме, болезни или войне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ad3058f-4d8f-3c09-90a1-d086c4a09e8e",
                    "sense_definition": "To act or speak in heightened anger.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "anger_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>raged</w> against the unfair rules imposed by the new management.",
                            "target_lang_translations": {
                                "ru": "Он <w>неистовствовал</w> против несправедливых правил, введенных новым руководством."
                            }
                        },
                        {
                            "text": "The customer <w>raged</w> at the waiter because his soup was cold.",
                            "target_lang_translations": {
                                "ru": "Клиент <w>набросился с криками</w> на официанта, потому что его суп был холодным."
                            }
                        }
                    ],
                    "synonyms": [
                        "rant",
                        "rave",
                        "fume",
                        "seethe"
                    ],
                    "common_phrases": [
                        "rage against",
                        "rage at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действовать или говорить в состоянии сильного гнева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неистовствовать",
                                "target_lang_sense_clarification": "проявлять крайнюю ярость"
                            },
                            {
                                "target_lang_word": "беситься",
                                "target_lang_sense_clarification": "разг. злиться"
                            },
                            {
                                "target_lang_word": "рвать и метать",
                                "target_lang_sense_clarification": "идиома"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19447799-6b70-35cc-9b0f-02996826739c",
                    "sense_definition": "To party hard or have a wild, exciting time.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "party_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The students <w>raged</w> all night after finishing their final exams.",
                            "target_lang_translations": {
                                "ru": "Студенты <w>отрывались</w> всю ночь после сдачи выпускных экзаменов."
                            }
                        },
                        {
                            "text": "We plan to <w>rage</w> at the concert this weekend.",
                            "target_lang_translations": {
                                "ru": "Мы планируем <w>оторваться</w> на концерте в эти выходные."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бурно веселиться, проводить время дико и активно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрываться",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "тусить",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
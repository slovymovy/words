            "senses": [
                {
                    "sense_id": "2c605398-5a04-394a-b1ab-0d436acec392",
                    "sense_definition": "Liked or wanted more than others; favoured.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Email is my <w>preferred</w> method of communication.",
                            "target_lang_translations": {
                                "ru": "Электронная почта — мой <w>предпочитаемый</w> способ общения."
                            }
                        },
                        {
                            "text": "The <w>preferred</w> option is to finish the project early.",
                            "target_lang_translations": {
                                "ru": "<w>Предпочтительный</w> вариант — завершить проект досрочно."

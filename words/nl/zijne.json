{
    "word_family": [
        "zijn"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "1cc1f3a8-8d48-33ec-a946-fb36231a01d9",
                    "sense_definition": "Zelfstandig gebruikt bezittelijk voornaamwoord dat verwijst naar iets dat eigendom is van een mannelijke persoon (of een woord met mannelijk woordgeslacht).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "independent_possessive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn fiets is kapot, mag ik vandaag de <w>zijne</w> even lenen?",
                            "target_lang_translations": {
                                "en": "My bike is broken, may I borrow <w>his</w> for a moment today?"
                            }
                        },
                        {
                            "text": "Zij heeft haar huiswerk al af, maar hij is het <w>zijne</w> vergeten te maken.",
                            "target_lang_translations": {
                                "en": "She has already finished her homework, but he forgot to do <w>his</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "van hem"
                    ],
                    "common_phrases": [
                        "de zijne",
                        "het zijne"
                    ],
                    "target_lang_definitions": {
                        "en": "Independently used possessive pronoun referring to something owned by a male person (or a masculine noun)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "his",
                                "target_lang_sense_clarification": "independent possessive"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "7b76ab8c-eacd-3498-ab83-2835e7b1f6f0",
                    "sense_definition": "Formele verbuiging van 'zijn', uitsluitend gebruikt in vaste titels en aanduidingen van hoogwaardigheidsbekleders.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "honorific_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de ceremonie werd <w>Zijne</w> Majesteit de Koning luid toegejuicht door het publiek.",
                            "target_lang_translations": {
                                "en": "During the ceremony, <w>His</w> Majesty the King was loudly cheered by the audience."
                            }
                        },
                        {
                            "text": "De brief was persoonlijk ondertekend door <w>Zijne</w> Excellentie de minister.",
                            "target_lang_translations": {
                                "en": "The letter was personally signed by <w>His</w> Excellency the minister."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Zijne Majesteit",
                        "Zijne Excellentie",
                        "Zijne Koninklijke Hoogheid",
                        "Zijne Heiligheid"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "honorific"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "archaic outside of titles"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Formal inflection of 'zijn', used exclusively in fixed titles and designations of dignitaries."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "His",
                                "target_lang_sense_clarification": "in titles"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d38a915a-bd08-3c8f-8174-3e18a4344859",
                    "sense_definition": "Ouderwetse of dialectische verbuiging van 'zijn' die voor een zelfstandig naamwoord staat.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_attributive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude teksten leest men vaak over <w>zijne</w> grote daden en overwinningen.",
                            "target_lang_translations": {
                                "en": "In old texts, one often reads about <w>his</w> great deeds and victories."
                            }
                        },
                        {
                            "text": "Hij pakte <w>zijne</w> jas en vertrok zonder nog een woord te zeggen.",
                            "target_lang_translations": {
                                "en": "He grabbed <w>his</w> coat and left without saying another word."
                            }
                        }
                    ],
                    "synonyms": [
                        "zijn"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "standard Dutch uses 'zijn'"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "common in Flemish"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Old-fashioned or dialectal inflection of 'zijn' placed before a noun."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "his",
                                "target_lang_sense_clarification": "archaic or dialectal form"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "67490f4e-ec89-3e56-8519-3bcdc16d81d7",
                    "sense_definition": "Datgene wat iemand toekomt, zijn aandeel, zijn plicht of zijn mening.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_substantivized",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iedereen in het team moet het <w>zijne</w> bijdragen om het project te laten slagen.",
                            "target_lang_translations": {
                                "en": "Everyone on the team must contribute <w>his part</w> to make the project succeed."
                            }
                        },
                        {
                            "text": "Hij luisterde aandachtig naar het vreemde verhaal en dacht er het <w>zijne</w> van.",
                            "target_lang_translations": {
                                "en": "He listened attentively to the strange story and had <w>his own thoughts</w> about it."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het zijne doen",
                        "het zijne ervan denken",
                        "het zijne hebben"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "substantivized"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "That which is due to someone, his share, his duty, or his opinion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "his part",
                                "target_lang_sense_clarification": "share or contribution"
                            },
                            {
                                "target_lang_word": "his own opinion",
                                "target_lang_sense_clarification": "thoughts or view"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c84dca64-fae6-34de-9ab4-157d40cd7a5c",
                    "sense_definition": "Zbiórka określonej grupy osób, na przykład uczniów w szkole lub żołnierzy, w wyznaczonym miejscu w celu sprawdzenia obecności lub przekazania informacji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "assembly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy uczniowie musieli stawić się na poranny <w>apel</w> na boisku szkolnym.",
                            "target_lang_translations": {
                                "ru": "Все ученики должны были явиться на утреннюю <w>линейку</w> на школьном дворе."
                            }
                        },
                        {
                            "text": "Podczas uroczystego <w>apelu</w> dyrektor wręczył nagrody najlepszym sportowcom.",
                            "target_lang_translations": {
                                "ru": "Во время торжественной <w>линейки</w> директор вручил награды лучшим спортсменам."
                            }
                        }
                    ],
                    "synonyms": [
                        "proklamacja"
                    ],
                    "common_phrases": [
                        "poranny apel",
                        "uroczysty apel",
                        "stawić się na apel",
                        "apel poległych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбор определенной группы людей (например, школьников или солдат) в назначенном месте для проверки присутствия или объявления информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "линейка",
                                "target_lang_sense_clarification": "в школе"
                            },
                            {
                                "target_lang_word": "построение",
                                "target_lang_sense_clarification": "военное или официальное"
                            },
                            {
                                "target_lang_word": "перекличка",
                                "target_lang_sense_clarification": "проверка присутствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbf1d93e-4205-3796-a522-97d873352ce2",
                    "sense_definition": "Publiczne wezwanie skierowane do ogółu lub konkretnej grupy osób, mające na celu zachęcenie do podjęcia jakiegoś działania lub udzielenia pomocy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "call_to_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lokalne media opublikowały <w>apel</w> policji o pomoc w znalezieniu zaginionego dziecka.",
                            "target_lang_translations": {
                                "ru": "Местные СМИ опубликовали <w>призыв</w> полиции о помощи в поиске пропавшего ребенка."
                            }
                        },
                        {
                            "text": "Prezydent wystosował dramatyczny <w>apel</w> do mieszkańców o zachowanie spokoju w trudnej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Президент выступил с драматическим <w>обращением</w> к жителям с просьбой сохранять спокойствие в сложной ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "odezwa",
                        "orędzie",
                        "posłanie"
                    ],
                    "common_phrases": [
                        "apel o pomoc",
                        "wystosować apel",
                        "odpowiedzieć na apel",
                        "podpisać apel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичное обращение к обществу или группе лиц с целью побудить к действию или просьбой о помощи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "призыв",
                                "target_lang_sense_clarification": "к действию или помощи"
                            },
                            {
                                "target_lang_word": "обращение",
                                "target_lang_sense_clarification": "официальное заявление"
                            },
                            {
                                "target_lang_word": "воззвание",
                                "target_lang_sense_clarification": "торжественное или письменное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d20492fe-f4f3-387e-bde5-4c12e579ea09",
                    "sense_definition": "Wieczorna modlitwa w Kościele katolickim kierowana do Matki Bożej, tradycyjnie związana z sanktuarium na Jasnej Górze.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "prayer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pielgrzymi zgromadzili się przed obrazem, aby wspólnie odśpiewać wieczorny <w>apel</w>.",
                            "target_lang_translations": {
                                "ru": "Паломники собрались перед иконой, чтобы вместе пропеть вечернюю <w>молитву</w>."
                            }
                        },
                        {
                            "text": "Każdego dnia o godzinie dwudziestej pierwszej w telewizji transmitowany jest <w>apel</w> z Częstochowy.",
                            "target_lang_translations": {
                                "ru": "Каждый день в двадцать один ноль-ноль по телевидению транслируется <w>молитва</w> из Ченстоховы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Apel Jasnogórski"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "religious"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вечерняя молитва в католической церкви, обращенная к Богоматери, традиционно связанная со святилищем на Ясной Горе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молитва",
                                "target_lang_sense_clarification": "религиозная"
                            },
                            {
                                "target_lang_word": "апель",
                                "target_lang_sense_clarification": "специфический католический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cc85488-9b77-30d7-a27c-b05b430115f2",
                    "sense_definition": "W terminologii łowieckiej: posłuszeństwo psa myśliwskiego i jego gotowość do natychmiastowego reagowania na komendy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "hunting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Apel</w> mojej suczki jest bardzo słaby, bo lubi ona biegać po lesie i nie wraca na zawołanie.",
                            "target_lang_translations": {
                                "ru": "<w>Послушание</w> моей собаки очень слабое, так как она любит бегать по лесу и не возвращается по зову."
                            }
                        },
                        {
                            "text": "Dobry <w>apel</w> jest kluczowy dla bezpieczeństwa psa podczas polowania w gęstym lesie.",
                            "target_lang_translations": {
                                "ru": "Хорошее <w>послушание</w> имеет ключевое значение для безопасности собаки во время охоты в густом лесу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć dobry apel",
                        "pies bez apelu"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "hunting"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В охотничьей терминологии: послушание собаки и её готовность немедленно реагировать на команды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "послушание",
                                "target_lang_sense_clarification": "дисциплина собаки"
                            },
                            {
                                "target_lang_word": "отзывчивость",
                                "target_lang_sense_clarification": "реакция на зов"
                            }
                        ]
                    }
                }
            ]
        }

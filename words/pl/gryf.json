                    "sense_definition": "Nazwa polskiego herbu szlacheckiego przedstawiającego mitycznego gryfa.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "heraldry",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "Rodzina ta od wieków pieczętowała się herbem <Gryf>.",
                            "target_lang_translations": {
                                "ru": "Эта семья веками использовала герб <Гриф>."
                            }
                        },
                        {
                            "text": "Na tarczy rycerza widniał wizerunek herbu <Gryf>.",
                            "target_lang_translations": {
                                "ru": "На щите рыцаря было изображение герба <Гриф>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "herb Gryf"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название польского дворянского герба с изображением мифического грифона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гриф",
                                "target_lang_sense_clarification": "герб"
                            }
                        ]
                    }
                }
            ]
        }

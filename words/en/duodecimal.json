    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9fedeef9-a155-363d-9c49-24ae1f7c0882",
                    "sense_definition": "Relating to or expressed in a system of counting based on the number twelve.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>duodecimal</w> system is often considered more efficient because the number twelve has many factors.",
                            "target_lang_translations": {
                                "ru": "<w>Двенадцатеричная</w> система часто считается более эффективной, так как число двенадцать имеет много делителей."
                            }
                        },
                        {
                            "text": "6B4 is the <w>duodecimal</w> notation for the decimal number 1000.",
                            "target_lang_translations": {
                                "ru": "6B4 — это <w>двенадцатеричная</w> запись десятичного числа 1000."
                            }
                        }
                    ],
                    "synonyms": [
                        "base twelve",
                        "dozenal"
                    ],
                    "common_phrases": [
                        "duodecimal system",
                        "duodecimal notation",
                        "duodecimal scale"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к системе счисления, основанной на числе двенадцать, или выраженный в ней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двенадцатеричный",
                                "target_lang_sense_clarification": "математика"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0552668b-0eca-32b2-84d8-f3453c81b043",
                    "sense_definition": "A number system that uses twelve as its base.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Calculations in <w>duodecimal</w> can be simpler for division since twelve is divisible by two, three, four, and six.",
                            "target_lang_translations": {
                                "ru": "Вычисления в <w>двенадцатеричной системе</w> могут быть проще при делении, так как двенадцать делится на два, три, четыре и шесть."
                            }
                        },
                        {
                            "text": "The student learned to convert integers from decimal to <w>duodecimal</w>.",
                            "target_lang_translations": {
                                "ru": "Студент учился переводить целые числа из десятичной системы в <w>двенадцатеричную</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "base twelve",
                        "dozenal"
                    ],
                    "common_phrases": [
                        "convert to duodecimal",
                        "in duodecimal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система счисления, использующая двенадцать в качестве основания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двенадцатеричная система",
                                "target_lang_sense_clarification": "система счисления"
                            }
                        ]
                    }
                }
            ]
        }

                    "synonyms": [
                        "крупица"
                    ],
                    "common_phrases": [
                        "хлебные крошки",
                        "ни крошки",
                        "собирать крошки"
                    ],
                    "target_lang_definitions": {
                        "en": "A small particle, fragment, or piece that has broken off from something (most often from bread or other food)."

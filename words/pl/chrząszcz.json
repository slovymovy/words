            "senses": [
                {
                    "sense_id": "d99d50a4-b36f-3a05-b21a-f343dfe9ea83",
                    "sense_definition": "Owad posiadający twarde skrzydła przednie, które osłaniają skrzydła tylne i odwłok; występuje w wielu gatunkach i środowiskach.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "insect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Szczebrzeszynie <chrząszcz> brzmi w trzcinie.",
                            "target_lang_translations": {
                                "ru": "В Щебжешине <w>жук</w> жужжит в тростнике."
                            }
                        },
                        {
                            "text": "Na liściu dębu siedział duży, błyszczący <chrząszcz>.",
                            "target_lang_translations": {
                                "ru": "На дубовом листе сидел большой блестящий <w>жук</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "chrabąszcz"
                    ],
                    "common_phrases": [
                        "chrząszcz brzmi w trzcinie",
                        "majowy chrząszcz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Насекомое с жесткими передними крыльями, которые защищают задние крылья и брюшко; встречается во многих видах и средах обитания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жук",
                                "target_lang_sense_clarification": "насекомое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af0f7a7f-56e8-33e2-a04b-9040a0c09bb0",
                    "sense_definition": "Potoczne określenie modelu samochodu marki Volkswagen o charakterystycznym, zaokrąglonym kształcie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "car",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad odnawia starego <chrząszcza> w swoim garażu.",
                            "target_lang_translations": {
                                "ru": "Мой сосед реставрирует старого <w>Жука</w> в своем гараже."
                            }
                        },
                        {
                            "text": "Na ulicy zaparkował żółty <chrząszcz>, przyciągając wzrok przechodniów.",
                            "target_lang_translations": {
                                "ru": "На улице припарковался желтый <w>Жук</w>, притягивая взгляды прохожих."
                            }
                        }
                    ],
                    "synonyms": [
                        "garbus"
                    ],
                    "traits": [

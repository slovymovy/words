        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "449113a0-ea48-3a26-b69f-fb1e824c58b0",
                    "sense_definition": "Higher in rank, standing, or authority within an organization or profession.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rank_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was promoted to a <w>senior</w> management position after five years.",
                            "target_lang_translations": {
                                "ru": "Через пять лет ее повысили до <w>руководящей</w> должности."
                            }

                {
                    "sense_id": "7497a896-b374-3582-9819-9f07e8fe08cf",
                    "sense_definition": "Wulgarne wykrzyknienie wyrażające nagłe, silne emocje, takie jak gniew, ból, frustracja, zdziwienie lub zachwyt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Kurwa</w>, mówiłem ci sto razy, żebyś tego nie dotykał!",
                            "target_lang_translations": {
                                "ru": "<w>Блядь</w>, я же говорил тебе сто раз, чтобы ты этого не трогал!"
                            }
                        },
                        {
                            "text": "O <w>kurwa</w>, ale mnie przestraszyłeś wyskakując zza rogu.",
                            "target_lang_translations": {
                                "ru": "О <w>блядь</w>, ну и напугал ты меня, выскочив из-за угла."
                            }
                        }
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8251ac96-dcc3-341f-afdc-9c8bedbf2e78",
                    "sense_definition": "Disposition or state of mind.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You have got a nice <w>attitude</w> today.",
                            "target_lang_translations": {
                                "ru": "У тебя сегодня хороший <w>настрой</w>."
                            }
                        },
                        {
                            "text": "She has a very positive <w>attitude</w> towards her new job.",
                            "target_lang_translations": {
                                "ru": "У неё очень позитивное <w>отношение</w> к новой работе."
                            }

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8bf08ab0-0586-3fa1-bec6-25cd362276ab",
                    "sense_definition": "W sposób bezkrytyczny, bez zastanowienia, nie zważając na rozsądek lub fakty; często w odniesieniu do silnych uczuć lub posłuszeństwa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody chłopak <ślepo> zakochał się w koleżance z pracy i nie widział poza nią świata.",
                            "target_lang_translations": {
                                "ru": "Молодой парень <слепо> влюбился в коллегу по работе и не видел никого, кроме неё."
                            }
                        },
                        {
                            "text": "Nie powinieneś <ślepo> wierzyć we wszystko, co przeczytasz w internecie.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <слепо> верить всему, что ты читаешь в интернете."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezkrytycznie",
                        "bezrefleksyjnie",
                        "bezmyślnie"
                    ],
                    "antonyms": [
                        "krytycznie",
                        "rozważnie"
                    ],
                    "common_phrases": [
                        "ślepo wierzyć",
                        "ślepo posłuszny",
                        "ślepo zakochany",
                        "ślepo oddany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Некритично, без раздумий, игнорируя здравый смысл или факты; часто в отношении сильных чувств или послушания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слепо",
                                "target_lang_sense_clarification": "перен. не рассуждая, безотчетно"
                            },
                            {
                                "target_lang_word": "безоговорочно",
                                "target_lang_sense_clarification": "без сомнений и возражений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51b0def6-1db0-34b7-aa52-654590cb3c23",
                    "sense_definition": "W sposób niewidzący, bez użycia wzroku lub z pustym wyrazem oczu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starzec siedział na ławce i wpatrywał się <ślepo> w przestrzeń przed sobą.",
                            "target_lang_translations": {
                                "ru": "Старик сидел на скамейке и <слепо> смотрел в пространство перед собой."
                            }
                        },
                        {
                            "text": "Jej oczy były otwarte, ale patrzyły <ślepo>, jakby nikogo nie poznawała.",
                            "target_lang_translations": {
                                "ru": "Её глаза были открыты, но смотрели <слепо>, словно она никого не узнавала."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wpatrywać się ślepo",
                        "patrzeć ślepo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Невидящим взглядом, не используя зрение или с пустым выражением глаз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слепо",
                                "target_lang_sense_clarification": "прям. не видя, подобно слепому"
                            }
                        ]
                    }
                }
            ]
        }
    ]

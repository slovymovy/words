        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "03f51730-e4dc-3867-b89e-c71663d9879d",
                    "sense_definition": "Having a good chance of happening or being true; probable or expected.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "probability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rain is <w>likely</w> later this afternoon.",
                            "target_lang_translations": {
                                "ru": "Во второй половине дня <w>вероятен</w> дождь."
                            }
                        },
                        {
                            "text": "He is <w>likely</w> to succeed at anything he tries.",
                            "target_lang_translations": {
                                "ru": "Он, <w>вероятно</w>, добьется успеха во всем, за что ни возьмется."
                            }
                        },
                        {
                            "text": "It is more <w>likely</w> to be tastier if you eat it fresh.",
                            "target_lang_translations": {
                                "ru": "Это, <w>вероятнее</w> всего, будет вкуснее, если съесть его свежим."
                            }
                        }
                    ],
                    "synonyms": [
                        "liable",
                        "see synonyms at probably"
                    ],
                    "antonyms": [
                        "unlikely"
                    ],
                    "common_phrases": [
                        "highly likely",
                        "likely to",
                        "more likely than not",
                        "most likely"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий хорошие шансы произойти или оказаться правдой; вероятный или ожидаемый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вероятный",
                                "target_lang_sense_clarification": "возможный, ожидаемый"
                            }
                        ]
                    }
                },
                {

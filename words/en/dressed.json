                {
                    "sense_id": "9786af20-ea6e-3bc9-9a62-f577e85e22f6",
                    "sense_definition": "Wearing clothes.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to get <w>dressed</w> before we leave for the party.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>одеться</w>, прежде чем мы уедем на вечеринку."
                            }
                        },
                        {
                            "text": "He was <w>dressed</w> in a smart black suit.",
                            "target_lang_translations": {
                                "ru": "Он был <w>одет</w> в элегантный черный костюм."
                            }
                        }
                    ],
                    "synonyms": [
                        "clothed"
                    ],
                    "antonyms": [
                        "undressed",
                        "naked"
                    ],
                    "common_phrases": [
                        "get dressed",
                        "well dressed",
                        "fully dressed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одетый; тот, на ком есть одежда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одетый",
                                "target_lang_sense_clarification": "носящий одежду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4439c974-27e5-3dc4-8398-16be21b1a376",
                    "sense_definition": "Prepared for eating, especially by the addition of specific condiments or dressing.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "food_prep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>dressed</w> salad was served with fresh bread.",
                            "target_lang_translations": {
                                "ru": "<w>Заправленный</w> салат подали со свежим хлебом."
                            }
                        },
                        {
                            "text": "We ordered a <w>dressed</w> crab for the main course.",
                            "target_lang_translations": {
                                "ru": "Мы заказали <w>разделанного</w> краба на горячее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dressed salad",
                        "dressed crab"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приготовленный к подаче на стол (заправленный соусом или разделанный)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заправленный",
                                "target_lang_sense_clarification": "о салате или блюде с соусом"
                            },
                            {
                                "target_lang_word": "разделанный",
                                "target_lang_sense_clarification": "о рыбе, крабе или птице"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76897e77-1650-3f45-a2e9-45aea8e31b22",
                    "sense_definition": "Having been subjected to a preparatory process or treatment; treated, prepared.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "material_prep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wall was constructed from carefully <w>dressed</w> stone.",
                            "target_lang_translations": {
                                "ru": "Стена была построена из тщательно <w>отёсанного</w> камня."
                            }
                        },
                        {
                            "text": "They used <w>dressed</w> leather to make the fine book covers.",
                            "target_lang_translations": {
                                "ru": "Они использовали <w>выделанную</w> кожу для изготовления изящных книжных обложек."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dressed stone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергшийся специальной обработке; обработанный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обработанный",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "отёсанный",
                                "target_lang_sense_clarification": "о камне"
                            },
                            {
                                "target_lang_word": "выделанный",
                                "target_lang_sense_clarification": "о коже"
                            }
                        ]
                    }
                }
            ]
        },
        {

            "senses": [
                {
                    "sense_id": "b88c59ec-256a-3bad-9b1f-31f2a869e7c2",
                    "sense_definition": "Вырастить ребенка, заботясь о его физическом и нравственном развитии, обучая правилам поведения и давая образование.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "education_formation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители смогли <w>воспитать</w> своих детей честными и добрыми людьми.",
                            "target_lang_translations": {
                                "en": "The parents were able to <w>raise</w> their children to be honest and kind people."
                            }
                        },
                        {
                            "text": "Бабушка <w>воспитала</w> внука, когда его родители были в длительной командировке.",
                            "target_lang_translations": {
                                "en": "The grandmother <w>brought up</w> her grandson while his parents were on a long business trip."
                            }
                        }
                    ],
                    "synonyms": [
                        "вырастить",
                        "взрастить"
                    ],
                    "common_phrases": [
                        "воспитать детей",
                        "воспитать сына",
                        "воспитать дочь",
                        "правильно воспитать",
                        "строго воспитать"
                    ],
                    "target_lang_definitions": {
                        "en": "To raise a child, caring for their physical and moral development, teaching rules of behavior, and providing education."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "raise",
                                "target_lang_sense_clarification": "a child"
                            },
                            {
                                "target_lang_word": "bring up",
                                "target_lang_sense_clarification": "a child"
                            },
                            {
                                "target_lang_word": "rear",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45431bf8-ac82-36b6-b588-4a05d8eb7f92",
                    "sense_definition": "Развить в человеке определенные черты характера, чувства, навыки или привычки путем систематического воздействия.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "education_formation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец старался <w>воспитать</w> в сыне смелость и решительность с ранних лет.",
                            "target_lang_translations": {
                                "en": "The father tried to <w>instill</w> courage and determination in his son from an early age."
                            }
                        },
                        {
                            "text": "Спорт помогает <w>воспитать</w> силу воли и дисциплину.",
                            "target_lang_translations": {
                                "en": "Sport helps to <w>cultivate</w> willpower and discipline."
                            }
                        }
                    ],
                    "synonyms": [
                        "взрастить",
                        "вырастить",
                        "взлелеять",
                        "выхолить",
                        "вынянчить"
                    ],
                    "common_phrases": [
                        "воспитать характер",
                        "воспитать силу воли",
                        "воспитать вкус",
                        "воспитать чувство ответственности",
                        "воспитать привычку"
                    ],
                    "target_lang_definitions": {
                        "en": "To develop specific character traits, feelings, skills, or habits in a person through systematic influence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "instill",
                                "target_lang_sense_clarification": "qualities or values"
                            },

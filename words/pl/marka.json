                },
                {
                    "sense_id": "f1242bc5-71e4-3dcd-aa88-dd337504366f",
                    "sense_definition": "Stalowy element zatopiony w betonie, służący do mocowania innych konstrukcji.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Belki stalowe są oparte na solidnych <w>markach</w>.",
                            "target_lang_translations": {
                                "ru": "Стальные балки опираются на прочные <w>закладные детали</w>."
                            }
                        },
                        {
                            "text": "Robotnicy muszą precyzyjnie osadzić <w>marki</w> w stropie.",
                            "target_lang_translations": {
                                "ru": "Рабочие должны точно установить <w>закладные детали</w> в перекрытии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "osadzić markę"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "termin techniczny/budowlany"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стальной элемент, закрепленный в бетоне, служащий для крепления других конструкций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закладная деталь",
                                "target_lang_sense_clarification": "строительный термин"

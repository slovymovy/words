    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0bd45b1e-eca6-3f54-be42-655d85c46a0e",
                    "sense_definition": "Wyjąć coś z wnętrza (np. torby, szafy) i umieścić w widocznym miejscu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprzedawca <wyłożył> świeże owoce na stragan.",
                            "target_lang_translations": {
                                "ru": "Продавец <выложил> свежие фрукты на прилавок."
                            }
                        },
                        {
                            "text": "Musisz <wyłożyć> wszystkie metalowe przedmioty na tacę podczas kontroli.",
                            "target_lang_translations": {
                                "ru": "Вы должны <выложить> все металлические предметы на поднос во время досмотра."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystawić"
                    ],
                    "common_phrases": [
                        "wyłożyć towar",
                        "wyłożyć na stół",
                        "wyłożyć karty na stół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вынуть что-то изнутри (например, из сумки) и положить на видное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выложить",
                                "target_lang_sense_clarification": "достать и положить"
                            },
                            {
                                "target_lang_word": "выставить",
                                "target_lang_sense_clarification": "товар на продажу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bfa3aaf-b27c-3eb1-bf55-2f251b5126e5",
                    "sense_definition": "Wytłumaczyć coś komuś, przedstawić i wyjaśnić jakieś zagadnienie lub temat.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Profesor w prosty sposób <wyłożył> studentom skomplikowaną teorię.",
                            "target_lang_translations": {
                                "ru": "Профессор простым языком <изложил> студентам сложную теорию."
                            }
                        },
                        {
                            "text": "Chcę ci <wyłożyć> kawę na ławę, żeby nie było niedomówień.",
                            "target_lang_translations": {
                                "ru": "Я хочу <выложить> тебе всё начистоту, чтобы не было недомолвок."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedstawić",
                        "przetłumaczyć"
                    ],
                    "common_phrases": [
                        "wyłożyć kawę na ławę",
                        "wyłożyć swoje racje",
                        "wyłożyć temat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объяснить что-то кому-то, представить и разъяснить какую-либо тему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изложить",
                                "target_lang_sense_clarification": "материал, теорию"
                            },
                            {
                                "target_lang_word": "разъяснить",
                                "target_lang_sense_clarification": "объяснить суть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e13c465-390c-3bd8-b1e1-22ac81ada57c",
                    "sense_definition": "Pokryć powierzchnię warstwą jakiegoś materiału (np. kafelkami, papierem).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Postanowiliśmy <wyłożyć> łazienkę nowymi płytkami ceramicznymi.",
                            "target_lang_translations": {
                                "ru": "Мы решили <выложить> ванную новой керамической плиткой."
                            }
                        },
                        {
                            "text": "Przed pieczeniem należy <wyłożyć> blachę papierem.",
                            "target_lang_translations": {
                                "ru": "Перед выпечкой следует <застелить> противень бумагой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyłożyć kafelkami",
                        "wyłożyć papierem",
                        "wyłożyć kostką"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрыть поверхность слоем какого-либо материала (например, плиткой, бумагой)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выложить",
                                "target_lang_sense_clarification": "плиткой, камнем"
                            },
                            {
                                "target_lang_word": "застелить",
                                "target_lang_sense_clarification": "бумагой, тканью"
                            },
                            {
                                "target_lang_word": "облицевать",
                                "target_lang_sense_clarification": "строительный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b708f790-0d12-3513-a536-96a65c73f131",
                    "sense_definition": "Przeznaczyć i wydać pieniądze na jakiś cel lub pokryć czyjeś koszty.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ojciec musiał <wyłożyć> dużą sumę na naprawę samochodu.",
                            "target_lang_translations": {
                                "ru": "Отец должен был <выложить> крупную сумму на ремонт машины."
                            }
                        },
                        {
                            "text": "Firma zgodziła się <wyłożyć> fundusze na ten innowacyjny projekt.",
                            "target_lang_translations": {
                                "ru": "Фирма согласилась <выделить> средства на этот инновационный проект."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyłożyć gotówkę",
                        "wyłożyć pieniądze",
                        "wyłożyć z własnej kieszeni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделить и потратить деньги на какую-либо цель или покрыть чьи-то расходы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выложить",
                                "target_lang_sense_clarification": "потратить деньги (разг.)"
                            },
                            {
                                "target_lang_word": "внести",
                                "target_lang_sense_clarification": "оплату, средства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "915e60d1-465a-335e-84f8-20326a287885",
                    "sense_definition": "Przewrócić się i upaść (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "reflexive_fail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biegacz <wyłożył> się na ostatnim zakręcie przed metą.",
                            "target_lang_translations": {
                                "ru": "Бегун <растянулся> на последнем повороте перед финишем."
                            }
                        },
                        {
                            "text": "Uważaj, żebyś nie <wyłożył> się na tym śliskim chodniku.",
                            "target_lang_translations": {
                                "ru": "Смотри, не <растянись> на этом скользком тротуаре."
                            }
                        }
                    ],
                    "synonyms": [
                        "przewrócić się",
                        "wywalić się",
                        "wytrąbić się",
                        "położyć się"
                    ],
                    "common_phrases": [
                        "wyłożyć się jak długi",
                        "wyłożyć się na ziemię"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Упасть во весь рост (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растянуться",
                                "target_lang_sense_clarification": "упасть (разг.)"
                            },
                            {
                                "target_lang_word": "шлёпнуться",
                                "target_lang_sense_clarification": "упасть с шумом (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11e4aa0f-2c8d-33ed-876d-8e23c07c3639",
                    "sense_definition": "Nie odnieść sukcesu, nie zdać egzaminu (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reflexive_fail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety <wyłożyłem> się na pytaniu z matematyki.",
                            "target_lang_translations": {
                                "ru": "К сожалению, я <завалился> на вопросе по математике."
                            }
                        },
                        {
                            "text": "Cały nasz plan <wyłożył> się przez brak czasu.",
                            "target_lang_translations": {
                                "ru": "Весь наш план <провалился> из-за нехватки времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "oblać",
                        "rozłożyć się"
                    ],
                    "common_phrases": [
                        "wyłożyć się na egzaminie",
                        "wyłożyć się na zadaniu"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "używane w mowie potocznej, często uczniowskiej lub studenckiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерпеть неудачу, не сдать экзамен (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провалиться",
                                "target_lang_sense_clarification": "потерпеть фиаско"
                            },
                            {
                                "target_lang_word": "завалиться",
                                "target_lang_sense_clarification": "на экзамене (разг.)"
                            },
                            {
                                "target_lang_word": "срезаться",
                                "target_lang_sense_clarification": "на экзамене (разг.)"
                            }
                        ]
                    }
                }
            ]
        }

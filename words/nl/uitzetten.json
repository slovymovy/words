                {
                    "sense_id": "6542597a-87dc-3d09-bc4a-86d4b3e9fa15",
                    "sense_definition": "Groter worden in omvang of volume, vaak door warmte of vocht",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "expand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de extreme hitte kan het metaal van de brug <w>uitzetten</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за сильной жары металл моста может <w>расширяться</w>.",
                                "en": "Due to the extreme heat, the metal of the bridge can <w>expand</w>."
                            }
                        },
                        {
                            "text": "Als water bevriest, zal het <w>uitzetten</w> en kan de leiding barsten.",
                            "target_lang_translations": {
                                "ru": "Когда вода замерзает, она <w>расширяется</w>, и труба может лопнуть.",
                                "en": "When water freezes, it will <w>expand</w> and the pipe may burst."
                            }
                        }
                    ],
                    "synonyms": [
                        "expanderen"
                    ],
                    "antonyms": [
                        "krimpen"
                    ],
                    "common_phrases": [
                        "uitzetten door warmte",
                        "krimpen en uitzetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличиваться в объеме или размере, часто из-за тепла или влаги",
                        "en": "To increase in size or volume, often due to heat or moisture"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расширяться",
                                "target_lang_sense_clarification": "физика/материалы"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "expand",
                                "target_lang_sense_clarification": "physics/materials"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "648eda90-4fda-389d-9485-b0cca349afc7",
                    "sense_definition": "Iemand dwingen een woning, gebouw of land te verlaten",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "expel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De huurders werden hun huis <w>uitgezet</w> omdat ze de huur niet betaalden.",
                            "target_lang_translations": {
                                "ru": "Жильцов <w>выселили</w> из дома, так как они не платили аренду.",
                                "en": "The tenants were <w>evicted</w> from their house because they didn't pay the rent."
                            }
                        },
                        {
                            "text": "De overheid kan vreemdelingen zonder verblijfsvergunning het land <w>uitzetten</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство может <w>выдворить</w> из страны иностранцев без вида на жительство.",
                                "en": "The government can <w>deport</w> foreigners without a residence permit from the country."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het land uitzetten",
                        "iemand zijn huis uitzetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принуждать кого-либо покинуть жилье, здание или страну",
                        "en": "To force someone to leave a dwelling, building, or country"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выселять",
                                "target_lang_sense_clarification": "из жилья"
                            },
                            {
                                "target_lang_word": "выдворять",
                                "target_lang_sense_clarification": "из страны"
                            },
                            {
                                "target_lang_word": "депортировать",
                                "target_lang_sense_clarification": "официально"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "evict",
                                "target_lang_sense_clarification": "from housing"
                            },
                            {
                                "target_lang_word": "deport",
                                "target_lang_sense_clarification": "from a country"
                            },
                            {
                                "target_lang_word": "expel",
                                "target_lang_sense_clarification": "general/school/country"
                            }
                        ]
                    }

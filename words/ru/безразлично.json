            "senses": [
                {
                    "sense_id": "95a34d2c-7de1-3e67-8dd8-a9a3377dc695",
                    "sense_definition": "Краткая форма среднего рода прилагательного <w>безразличный</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Выражение его лица было холодно и <w>безразлично</w>.",
                            "target_lang_translations": {
                                "en": "The expression on his face was cold and <w>indifferent</w>."
                            }
                        },
                        {
                            "text": "Это решение для нас совершенно <w>безразлично</w>.",
                            "target_lang_translations": {
                                "en": "This decision is completely <w>indifferent</w> to us."
                            }

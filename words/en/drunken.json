    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1f5aee2e-e922-388c-9da7-791d9c56a3cb",
                    "sense_definition": "Affected by alcohol; intoxicated.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_alcohol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>drunken</w> man stumbled out of the bar and called a taxi.",
                            "target_lang_translations": {
                                "ru": "<w>Пьяный</w> мужчина, спотыкаясь, вышел из бара и вызвал такси."
                            }
                        },
                        {
                            "text": "She helped her <w>drunken</w> brother get home safely after the party.",
                            "target_lang_translations": {
                                "ru": "Она помогла своему <w>пьяному</w> брату благополучно добраться домой после вечеринки."
                            }
                        }
                    ],
                    "synonyms": [
                        "drunk"
                    ],
                    "antonyms": [
                        "sober"
                    ],
                    "common_phrases": [
                        "drunken sailor",
                        "drunken stupor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся под воздействием алкоголя; опьяневший."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пьяный",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "нетрезвый",
                                "target_lang_sense_clarification": "более формально или вежливо"
                            },
                            {
                                "target_lang_word": "выпивший",
                                "target_lang_sense_clarification": "слегка пьяный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce8616ba-b1b6-3e24-bab3-db91aab481a7",
                    "sense_definition": "Characterized by, resulting from, or involving alcohol intoxication.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_alcohol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was arrested for <w>drunken</w> driving late last night.",
                            "target_lang_translations": {
                                "ru": "Его арестовали за вождение в <w>нетрезвом</w> виде поздно ночью."
                            }
                        },
                        {
                            "text": "The neighbors complained about the <w>drunken</w> brawl in the street.",
                            "target_lang_translations": {
                                "ru": "Соседи жаловались на <w>пьяную</w> драку на улице."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drunken driving",
                        "drunken brawl",
                        "drunken rage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся опьянением, происходящий в состоянии опьянения или вызванный им."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пьяный",
                                "target_lang_sense_clarification": "о действиях, событиях или состояниях"
                            },
                            {
                                "target_lang_word": "нетрезвый",
                                "target_lang_sense_clarification": "часто используется в юридическом контексте (напр. вождение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f2b480d-c22b-360f-a201-60b5d133cad9",
                    "sense_definition": "Used in the names of certain spicy dishes, often cooked with alcohol.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I ordered spicy <w>drunken</w> noodles for dinner at the Thai restaurant.",
                            "target_lang_translations": {
                                "ru": "Я заказал острую «<w>пьяную</w>» лапшу на ужин в тайском ресторане."
                            }
                        },
                        {
                            "text": "The chef prepared a delicious plate of <w>drunken</w> chicken.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар приготовил вкусное блюдо из «<w>пьяной</w>» курицы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drunken noodles",
                        "drunken chicken"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Asian cuisine"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в названиях блюд, приготовленных с добавлением алкоголя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пьяный",
                                "target_lang_sense_clarification": "кулинарный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {

                    ],
                    "synonyms": [
                        "attempt",
                        "endeavor",
                        "strive",
                        "work hard",
                        "apply oneself"
                    ],
                    "common_phrases": [
                        "try to",
                        "try hard",
                        "try one's best"
                    ],

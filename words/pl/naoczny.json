    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "af2782c8-9e68-3058-95c9-523057e1b44a",
                    "sense_definition": "Taki, który widział coś, zazwyczaj jakieś zdarzenie, na własne oczy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "witness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja wciąż poszukuje <w>naocznego</w> świadka tego wypadku.",
                            "target_lang_translations": {
                                "ru": "Полиция всё ещё ищет <w>очевидца</w> этой аварии."
                            }
                        },
                        {
                            "text": "Jako <w>naoczny</w> świadek mogę potwierdzić, że to nie była jego wina.",
                            "target_lang_translations": {
                                "ru": "Как <w>очевидец</w>, я могу подтвердить, что это была не его вина."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezpośredni"
                    ],
                    "common_phrases": [
                        "naoczny świadek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто видел что-то, обычно какое-то событие, своими глазами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очевидец",
                                "target_lang_sense_clarification": "существительное; используется для перевода словосочетания 'naoczny świadek'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85a56af3-961a-3be1-8dba-eeaaa809617c",
                    "sense_definition": "Taki, który jest oparty na obserwacji wzrokowej lub stanowi widoczny, niepodważalny dowód.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "evidence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To nagranie stanowi <w>naoczny</w> dowód popełnionego przestępstwa.",
                            "target_lang_translations": {
                                "ru": "Эта запись представляет собой <w>наглядное</w> доказательство совершенного преступления."
                            }
                        },
                        {
                            "text": "Musisz dostarczyć <w>naoczne</w> potwierdzenie, że towar dotarł do magazynu.",
                            "target_lang_translations": {
                                "ru": "Ты должен предоставить <w>наглядное</w> подтверждение того, что товар прибыл на склад."
                            }
                        }
                    ],
                    "synonyms": [
                        "widoczny",
                        "ewidentny",
                        "namacalny"
                    ],
                    "common_phrases": [
                        "naoczny dowód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основанный на визуальном наблюдении или представляющий собой видимое, неопровержимое доказательство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наглядный",
                                "target_lang_sense_clarification": "о доказательстве или примере"
                            },
                            {
                                "target_lang_word": "очевидный",
                                "target_lang_sense_clarification": "явный, бесспорный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

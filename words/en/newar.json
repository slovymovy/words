{
    "word_family": [
        "Newari"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "95e05d7a-eb00-376a-b610-96fd3760276c",
                    "sense_definition": "A member of the historical inhabitants of the Kathmandu Valley and its surrounding areas in Nepal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "people",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "The <w>Newar</w> community is famous for its intricate wood carvings.",
                            "target_lang_translations": {
                                "ru": "Община <w>неваров</w> славится своей искусной резьбой по дереву."
                            }
                        },
                        {
                            "text": "She met a <w>Newar</w> who told her about the history of the valley.",
                            "target_lang_translations": {
                                "ru": "Она встретила <w>невара</w>, который рассказал ей об истории долины."
                            }
                        }
                    ],
                    "synonyms": [
                        "Newa"
                    ],
                    "common_phrases": [
                        "Newar community",
                        "Newar culture",
                        "Newar people"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель коренного населения долины Катманду и прилегающих районов Непала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невар",
                                "target_lang_sense_clarification": "представитель народа"
                            },
                            {
                                "target_lang_word": "невары",
                                "target_lang_sense_clarification": "народ (мн. ч.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7263164a-fec0-3fd2-ae42-8c84456164c9",
                    "sense_definition": "The Sino-Tibetan language spoken by the Newar people.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "language",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "He is learning to speak <w>Newar</w> to communicate with his relatives.",
                            "target_lang_translations": {
                                "ru": "Он учится говорить на <w>невари</w>, чтобы общаться со своими родственниками."
                            }
                        },
                        {
                            "text": "Many traditional songs are sung in <w>Newar</w>.",
                            "target_lang_translations": {
                                "ru": "Многие традиционные песни исполняются на <w>невари</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Newa"
                    ],
                    "common_phrases": [
                        "speak Newar",
                        "in Newar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сино-тибетский язык, на котором говорят невары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невари",
                                "target_lang_sense_clarification": "язык"
                            },
                            {
                                "target_lang_word": "неварский язык",
                                "target_lang_sense_clarification": "язык"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76e7c05d-c5ed-360a-a7d4-dc3385e5094d",
                    "sense_definition": "A synonym of <w>Newa</w> referring to the script used to write the language.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "script",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "The ancient manuscript was written using the <w>Newar</w> script.",
                            "target_lang_translations": {
                                "ru": "Древняя рукопись была написана с использованием <w>неварского</w> письма."
                            }
                        },
                        {
                            "text": "Scholars can identify the age of the text by the style of the <w>Newar</w> characters.",
                            "target_lang_translations": {
                                "ru": "Ученые могут определить возраст текста по стилю <w>неварских</w> символов."
                            }
                        }
                    ],
                    "synonyms": [
                        "Newa"
                    ],
                    "common_phrases": [
                        "Newar script"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность письменности, используемая для записи неварского языка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неварский",
                                "target_lang_sense_clarification": "о письменности (прилагательное)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
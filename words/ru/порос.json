            "senses": [
                {
                    "sense_id": "59a44911-dd02-39ef-8968-d0c0d42e4246",
                    "sense_definition": "Река в России, протекающая в Кемеровской области, правый приток реки Пурла.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geo_names",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Река <w>Порос</w> известна местным рыбакам своими живописными берегами.",
                            "target_lang_translations": {
                                "en": "The <w>Poros</w> river is known to local fishermen for its picturesque banks."
                            }
                        },
                        {
                            "text": "На карте области можно найти небольшой приток под названием <w>Порос</w>.",
                            "target_lang_translations": {
                                "en": "On the map of the region, one can find a small tributary named <w>Poros</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A river in Russia, flowing in the Kemerovo region, a right tributary of the Purla River."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Poros",
                                "target_lang_sense_clarification": "river name"
                            }

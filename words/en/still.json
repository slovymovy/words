                },
                {
                    "sense_id": "419c5bda-515b-3159-993c-d61d2adb9081",
                    "sense_definition": "Not fizzy or carbonated; without bubbles (used for drinks).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "beverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I prefer <w>still</w> water over sparkling water.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю <w>негазированную</w> воду газированной."
                            }
                        },
                        {
                            "text": "They ordered a bottle of <w>still</w> white wine.",
                            "target_lang_translations": {
                                "ru": "Они заказали бутылку <w>тихого</w> белого вина."
                            }
                        }
                    ],
                    "synonyms": [
                        "flat",
                        "non-carbonated",
                        "uneffervescent"
                    ],
                    "antonyms": [
                        "sparkling",
                        "carbonated",
                        "fizzy"
                    ],
                    "common_phrases": [
                        "still water",
                        "still wine",
                        "sparkling or still"
                    ],
                    "target_lang_definitions": {
                        "ru": "Негазированный; без пузырьков (о напитках)."

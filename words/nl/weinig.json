                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <w>weinig</w> zin om mijn vingers daaraan te branden.",
                            "target_lang_translations": {

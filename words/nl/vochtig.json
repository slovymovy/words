{
    "word_family": [
        "vocht",
        "bevochtigen",
        "vochtigheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c5014bfe-b1aa-3726-9ad5-38e2fe9c2fdf",
                    "sense_definition": "Een beetje nat; niet helemaal droog (gebruikt voor voorwerpen, oppervlakken of lichaamsdelen).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "wetness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Maak de tafel schoon met een <w>vochtige</w> doek.",
                            "target_lang_translations": {
                                "ru": "Протри стол <w>влажной</w> тряпкой."
                            }
                        },
                        {
                            "text": "Mijn kleren zijn nog steeds <w>vochtig</w> van de regen.",
                            "target_lang_translations": {
                                "ru": "Моя одежда всё ещё <w>влажная</w> от дождя."
                            }
                        }
                    ],
                    "synonyms": [
                        "klam",
                        "nattig"
                    ],
                    "antonyms": [
                        "droog"
                    ],
                    "common_phrases": [
                        "vochtige doekjes",
                        "vochtige ogen",
                        "vochtige muren",
                        "vochtige grond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слегка мокрый; не совсем сухой (используется для предметов, поверхностей или частей тела)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влажный",
                                "target_lang_sense_clarification": "слегка мокрый"
                            },
                            {
                                "target_lang_word": "сырой",
                                "target_lang_sense_clarification": "не высохший до конца (например, об одежде)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a92e76a9-a144-345d-be47-d9a075b643dd",
                    "sense_definition": "Met veel waterdamp in de lucht (gebruikt voor weer, klimaat of ruimtes).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "wetness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is vandaag erg warm en <w>vochtig</w> buiten.",
                            "target_lang_translations": {
                                "ru": "Сегодня на улице очень тепло и <w>влажно</w>."
                            }
                        },
                        {
                            "text": "In de kelder hangt vaak een <w>vochtige</w> lucht.",
                            "target_lang_translations": {
                                "ru": "В подвале часто стоит <w>сырой</w> воздух."
                            }
                        }
                    ],
                    "synonyms": [
                        "klam"
                    ],
                    "antonyms": [
                        "droog"
                    ],
                    "common_phrases": [
                        "vochtige lucht",
                        "vochtig klimaat",
                        "warm en vochtig"
                    ],
                    "target_lang_definitions": {
                        "ru": "С большим содержанием водяного пара в воздухе (используется для погоды, климата или помещений)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влажный",
                                "target_lang_sense_clarification": "о климате или воздухе"
                            },
                            {
                                "target_lang_word": "сырой",
                                "target_lang_sense_clarification": "о холодном, промозглом воздухе или помещении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
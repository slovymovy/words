                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заросли",
                                "target_lang_sense_clarification": "густой кустарник"
                            },
                            {
                                "target_lang_word": "чаща",
                                "target_lang_sense_clarification": "густой лес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "349dcdc6-5252-3528-a039-d0432e9e8b8b",

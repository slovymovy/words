            "senses": [
                {
                    "sense_id": "5cd5347d-e890-3669-aafe-fdd5570e836a",
                    "sense_definition": "Rażący błąd, gafa lub pomyłka, często popełniona przez nieuwagę, zwłaszcza w sporcie lub pracy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mistake_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bramkarz puścił strasznego <w>babola</w> w ostatnich minutach meczu.",
                            "target_lang_translations": {
                                "ru": "Вратарь допустил ужасный <w>ляп</w> на последних минутах матча."
                            }
                        },
                        {
                            "text": "W moim wypracowaniu znalazłem jednego ortograficznego <w>babola</w>.",
                            "target_lang_translations": {
                                "ru": "В своём сочинении я нашёл один орфографический <w>ляп</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "błąd"
                    ],
                    "common_phrases": [
                        "strzelić babola",
                        "puścić babola",
                        "zrobić babola"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубая ошибка, оплошность или ляп, часто совершённые по невнимательности, особенно в спорте или работе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ляп",
                                "target_lang_sense_clarification": "разг. грубая ошибка, промах"
                            },
                            {
                                "target_lang_word": "косяк",
                                "target_lang_sense_clarification": "сленг. ошибка, неправильное действие"
                            },
                            {
                                "target_lang_word": "пенка",
                                "target_lang_sense_clarification": "спорт. сленг. грубая ошибка вратаря или игрока"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35802741-c2b4-37e4-bcbe-caa49872ad0e",
                    "sense_definition": "Zaschnięta wydzielina z nosa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nasal_secretion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko dłubało w nosie i wyciągnęło wielkiego <w>babola</w>.",
                            "target_lang_translations": {
                                "ru": "Ребёнок ковырял в носу и вытащил огромную <w>козявку</w>."
                            }
                        },
                        {
                            "text": "Masz <w>babola</w> w nosie, weź chusteczkę i go wytrzyj.",
                            "target_lang_translations": {
                                "ru": "У тебя <w>козявка</w> в носу, возьми платок и вытри её."
                            }
                        }
                    ],
                    "synonyms": [
                        "koza",
                        "gil",
                        "glut",
                        "smark",
                        "śpik"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Засохшие выделения из носа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "козявка",
                                "target_lang_sense_clarification": "разг. засохшая слизь в носу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a5d0eb8-b8e3-3fa1-867e-86d2f5e97fbb",
                    "sense_definition": "Pogardliwe określenie kobiety, która jest postrzegana jako nieatrakcyjna, wredna lub zaniedbana.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "pejorative_woman",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary <w>babol</w> znowu krzyczał na dzieci bawiące się pod blokiem.",
                            "target_lang_translations": {
                                "ru": "Эта старая <w>карга</w> снова кричала на детей, играющих у дома."
                            }
                        },
                        {
                            "text": "Nie chcę mieć z tym wrednym <w>babolem</w> nic wspólnego.",
                            "target_lang_translations": {
                                "ru": "Я не хочу иметь ничего общего с этой вредной <w>мымрой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "babon",
                        "babsko",
                        "babsztyl",
                        "babus",
                        "kartofel",
                        "szmata"
                    ],
                    "common_phrases": [
                        "stary babol",
                        "wredny babol"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Презрительное название женщины, которую считают непривлекательной, вредной или неухоженной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мымра",

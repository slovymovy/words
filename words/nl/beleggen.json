    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c06b39d1-d3d2-3738-ac31-7347eaa73fc2",
                    "sense_definition": "Brood of beschuit bedekken met beleg zoals kaas, vleeswaren of zoetigheid.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga de broodjes <w>beleggen</w> met kaas en komkommer voor de lunch.",
                            "target_lang_translations": {
                                "en": "I am going to <w>top</w> the rolls with cheese and cucumber for lunch.",
                                "ru": "Я собираюсь <w>положить</w> сыр и огурцы на булочки к обеду."
                            }
                        },
                        {
                            "text": "Zij heeft haar beschuit dik <w>belegd</w> met aardbeien en suiker.",
                            "target_lang_translations": {
                                "en": "She has <w>topped</w> her rusk thickly with strawberries and sugar.",
                                "ru": "Она густо <w>покрыла</w> свой сухарь клубникой с сахаром."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedekken"
                    ],
                    "common_phrases": [
                        "een boterham beleggen",
                        "rijkelijk belegd",
                        "vers belegd broodje"
                    ],
                    "target_lang_definitions": {
                        "en": "To cover bread or a rusk with a topping such as cheese, cold cuts, or sweets.",
                        "ru": "Накладывать начинку (сыр, колбасу и т.д.) на хлеб или сухари."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "top",
                                "target_lang_sense_clarification": "food/sandwiches"
                            },
                            {
                                "target_lang_word": "put toppings on",
                                "target_lang_sense_clarification": "food"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "класть на хлеб",
                                "target_lang_sense_clarification": "general action"
                            },
                            {
                                "target_lang_word": "делать бутерброд",
                                "target_lang_sense_clarification": "functional equivalent"
                            },
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "to cover with toppings"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a791323f-c0b5-3f2f-8d1d-e6ef6c035bc8",
                    "sense_definition": "Geld besteden aan bijvoorbeeld aandelen, obligaties of vastgoed met de verwachting dat het meer waard wordt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "investing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij besloot zijn spaargeld te <w>beleggen</w> in duurzame energieprojecten.",
                            "target_lang_translations": {
                                "en": "He decided to <w>invest</w> his savings in sustainable energy projects.",
                                "ru": "Он решил <w>вложить</w> свои сбережения в проекты по устойчивой энергетике."
                            }
                        },
                        {
                            "text": "Als je veilig wilt <w>beleggen</w>, moet je de risico's goed spreiden.",
                            "target_lang_translations": {
                                "en": "If you want to <w>invest</w> safely, you must spread the risks well.",
                                "ru": "Если ты хочешь безопасно <w>инвестировать</w>, нужно хорошо распределять риски."
                            }
                        }
                    ],
                    "synonyms": [
                        "investeren"
                    ],
                    "common_phrases": [
                        "geld beleggen",
                        "in aandelen beleggen",
                        "risicovol beleggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend money on assets like stocks, bonds, or real estate with the expectation that they will increase in value.",
                        "ru": "Вкладывать деньги в акции, облигации или недвижимость с целью получения прибыли."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "invest",
                                "target_lang_sense_clarification": "financial"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вкладывать",
                                "target_lang_sense_clarification": "money/capital"
                            },
                            {
                                "target_lang_word": "инвестировать",
                                "target_lang_sense_clarification": "financial context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "665c96e0-c017-3764-89b6-0816cf486aaa",

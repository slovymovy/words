            "senses": [
                {
                    "sense_id": "3798a4a6-2a15-3314-b83a-aa156354c6eb",
                    "sense_definition": "Een zeer grote hoeveelheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft in korte tijd een <w>massa</w> geld verdiend met zijn bedrijf.",
                            "target_lang_translations": {
                                "en": "He earned a <w>heap</w> of money with his company in a short time.",
                                "ru": "За короткое время он заработал <w>кучу</w> денег на своей фирме."
                            }
                        },
                        {
                            "text": "Er lag een <w>massa</w> werk op zijn bureau te wachten.",
                            "target_lang_translations": {
                                "en": "A <w>pile</w> of work was waiting on his desk.",
                                "ru": "На его столе ждала <w>масса</w> работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "boel",
                        "hoop",
                        "veel",
                        "overvloed"
                    ],
                    "antonyms": [
                        "beetje"
                    ],
                    "common_phrases": [
                        "een massa geld",
                        "massa's mensen"
                    ],
                    "target_lang_definitions": {
                        "en": "A very large quantity or amount.",
                        "ru": "Очень большое количество."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mass",
                                "target_lang_sense_clarification": "A large quantity or sum"
                            },
                            {

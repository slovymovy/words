    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "930945bb-0fce-3c7c-ad54-44221fe2fec1",
                    "sense_definition": "Slecht volk of ongure personen die zich misdragen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bad_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bewoners klagen over het <w>ontuig</w> dat 's nachts de bushokjes vernielt.",
                            "target_lang_translations": {
                                "en": "The residents complain about the <w>scum</w> that vandalizes the bus shelters at night.",
                                "ru": "Жители жалуются на <w>шпану</w>, которая по ночам громит автобусные остановки."
                            }
                        },
                        {
                            "text": "Hij wil niet dat zijn kinderen omgaan met dat criminele <w>ontuig</w>.",
                            "target_lang_translations": {
                                "en": "He does not want his children associating with that criminal <w>riffraff</w>.",
                                "ru": "Он не хочет, чтобы его дети общались с этим криминальным <w>отребьем</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tuig",
                        "schorremorrie",
                        "gepeupel",
                        "uitvaagsel",
                        "schuim"
                    ],
                    "common_phrases": [
                        "dat ontuig"
                    ],
                    "target_lang_definitions": {
                        "en": "Bad people or unsavory individuals who misbehave.",
                        "ru": "Плохие люди, сброд или хулиганы, которые ведут себя неподобающе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scum",
                                "target_lang_sense_clarification": "despicable people"
                            },
                            {
                                "target_lang_word": "riffraff",
                                "target_lang_sense_clarification": "disreputable people"
                            },
                            {
                                "target_lang_word": "vermin",
                                "target_lang_sense_clarification": "people perceived as despicable"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сброд",
                                "target_lang_sense_clarification": "группа людей низкого социального статуса или поведения"
                            },
                            {
                                "target_lang_word": "отребье",
                                "target_lang_sense_clarification": "деклассированные, опустившиеся люди"
                            },
                            {
                                "target_lang_word": "шпана",
                                "target_lang_sense_clarification": "мелкие хулиганы (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad27b858-5fdd-3868-b5a2-11226c0d28ca",
                    "sense_definition": "Waardeloze spullen, rotzooi of afval.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "trash",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ruim dat oude <w>ontuig</w> in de schuur eens op.",
                            "target_lang_translations": {
                                "en": "Clean up that old <w>junk</w> in the shed for once.",
                                "ru": "Убери наконец этот старый <w>хлам</w> в сарае."
                            }
                        },
                        {
                            "text": "Er ligt allerlei <w>ontuig</w> op de vloer dat weggegooid moet worden.",
                            "target_lang_translations": {
                                "en": "There is all sorts of <w>rubbish</w> on the floor that needs to be thrown out.",
                                "ru": "На полу валяется всякое <w>барахло</w>, которое нужно выбросить."
                            }
                        }
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d636d6b1-1775-3ff8-abad-7adb44d2a7d2",
                    "sense_definition": "Een mechanisme waarmee men iets, zoals een deur, raam of kist, kan afsluiten zodat het niet zomaar geopend kan worden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "lock_mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik draai de sleutel om in het <w>slot</w>.",
                            "target_lang_translations": {
                                "ru": "Я поворачиваю ключ в <w>замке</w>.",
                                "en": "I turn the key in the <w>lock</w>."
                            }
                        },
                        {
                            "text": "Zorg ervoor dat je fiets goed op <w>slot</w> staat als je weggaat.",
                            "target_lang_translations": {
                                "ru": "Убедись, что твой велосипед надежно заперт на <w>замок</w>, когда уходишь.",
                                "en": "Make sure your bike is securely <w>locked</w> when you leave."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op slot",
                        "achter slot en grendel",
                        "van het slot halen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Механизм, с помощью которого можно закрыть что-либо, например дверь, окно или ящик, чтобы их нельзя было легко открыть.",
                        "en": "A mechanism used to close something, such as a door, window, or box, so that it cannot be opened easily."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замок",
                                "target_lang_sense_clarification": "устройство для запирания"
                            },
                            {
                                "target_lang_word": "запор",
                                "target_lang_sense_clarification": "засов, задвижка"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "lock",
                                "target_lang_sense_clarification": "fastening device"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1fb2982-c93b-32ad-8d12-d8c97ff92fb3",
                    "sense_definition": "Het laatste gedeelte of de afronding van een tekst, toespraak, muziekstuk of gebeurtenis.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ending",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tot <w>slot</w> wil ik iedereen bedanken voor hun aanwezigheid.",
                            "target_lang_translations": {
                                "ru": "В <w>заключение</w> я хочу поблагодарить всех за присутствие.",
                                "en": "In <w>conclusion</w>, I would like to thank everyone for their presence."
                            }
                        },
                        {
                            "text": "Het <w>slot</w> van het boek was erg verrassend en spannend.",
                            "target_lang_translations": {
                                "ru": "<w>Конец</w> книги был очень неожиданным и захватывающим.",
                                "en": "The <w>ending</w> of the book was very surprising and exciting."
                            }
                        }
                    ],
                    "synonyms": [
                        "eind"
                    ],
                    "common_phrases": [
                        "tot slot",
                        "als slot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последняя часть или завершение текста, речи, музыкального произведения или события.",
                        "en": "The final part or completion of a text, speech, piece of music, or event."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конец",
                                "target_lang_sense_clarification": "завершение"
                            },
                            {
                                "target_lang_word": "заключение",
                                "target_lang_sense_clarification": "финальная часть"
                            },
                            {
                                "target_lang_word": "финал",
                                "target_lang_sense_clarification": "в музыке или событиях"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "conclusion",
                                "target_lang_sense_clarification": "final part"
                            },
                            {
                                "target_lang_word": "end",
                                "target_lang_sense_clarification": "termination"
                            },
                            {
                                "target_lang_word": "ending",
                                "target_lang_sense_clarification": "last part of a story/event"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f42d810d-aa78-3725-b84b-f58e289c71b3",
                    "sense_definition": "Een groot, versterkt gebouw waar vroeger adellijke personen woonden; een kasteel.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koning woonde vroeger in een prachtig <w>slot</w> op de heuvel.",
                            "target_lang_translations": {
                                "ru": "Король раньше жил в прекрасном <w>замке</w> на холме.",
                                "en": "The king used to live in a beautiful <w>castle</w> on the hill."
                            }
                        },
                        {
                            "text": "Tijdens onze vakantie bezochten we een middeleeuws <w>slot</w>.",
                            "target_lang_translations": {
                                "ru": "Во время нашего отпуска мы посетили средневековый <w>замок</w>.",
                                "en": "During our vacation, we visited a medieval <w>castle</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kasteel",
                        "burcht"
                    ],
                    "common_phrases": [
                        "Slot Loevestein"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое укрепленное здание, где раньше жили знатные особы; замок.",
                        "en": "A large, fortified building where noble persons used to live; a castle."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замок",
                                "target_lang_sense_clarification": "здание, дворец"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "castle",
                                "target_lang_sense_clarification": "fortified building"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95e6267c-2ce3-36dd-86b5-38782878feaf",
                    "sense_definition": "Een toegewezen periode of tijdruimte waarbinnen een bepaalde activiteit, zoals het landen van een vliegtuig, mag plaatsvinden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_interval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het vliegtuig miste zijn <w>slot</w> en moest wachten op een nieuwe vertrektijd.",
                            "target_lang_translations": {
                                "ru": "Самолет упустил свой <w>слот</w> и был вынужден ждать нового времени вылета.",
                                "en": "The plane missed its <w>slot</w> and had to wait for a new departure time."
                            }
                        },
                        {
                            "text": "We hebben een <w>slot</w> van dertig minuten gereserveerd voor de presentatie.",
                            "target_lang_translations": {
                                "ru": "Мы зарезервировали тридцатиминутный <w>слот</w> для презентации.",
                                "en": "We have reserved a thirty-minute <w>slot</w> for the presentation."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een slot toewijzen",
                        "beschikbaar slot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделенный период времени, в течение которого может происходить определенное действие, например, посадка самолета.",
                        "en": "An allocated period of time within which a certain activity, such as the landing of an aircraft, may take place."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слот",
                                "target_lang_sense_clarification": "временной интервал"
                            },
                            {
                                "target_lang_word": "окно",
                                "target_lang_sense_clarification": "промежуток времени"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "slot",
                                "target_lang_sense_clarification": "time interval"
                            },
                            {
                                "target_lang_word": "time slot",
                                "target_lang_sense_clarification": "allocated time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9491b278-89c2-3340-b7f5-80acaa42940f",
                    "sense_definition": "Een smalle opening of gleuf, vaak in elektronische apparatuur, waar een kaart of stekker in past.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "opening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de geheugenkaart voorzichtig in het juiste <w>slot</w> duwen.",
                            "target_lang_translations": {
                                "ru": "Нужно осторожно вставить карту памяти в правильный <w>слот</w>.",
                                "en": "You must carefully push the memory card into the correct <w>slot</w>."
                            }
                        },
                        {
                            "text": "Mijn computer heeft geen vrij <w>slot</w> meer voor extra uitbreidingen.",
                            "target_lang_translations": {
                                "ru": "В моем компьютере больше нет свободного <w>слота</w> для дополнительных расширений.",
                                "en": "My computer has no free <w>slot</w> left for extra expansions."
                            }
                        }
                    ],
                    "synonyms": [
                        "sleuf"
                    ],
                    "common_phrases": [
                        "CI-slot",
                        "USB-slot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкое отверстие или щель, часто в электронном оборудовании, куда подходит карта или штекер.",
                        "en": "A narrow opening or slit, often in electronic equipment, into which a card or plug fits."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слот",
                                "target_lang_sense_clarification": "разъём расширения"
                            },
                            {
                                "target_lang_word": "разъём",
                                "target_lang_sense_clarification": "гнездо"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "slot",
                                "target_lang_sense_clarification": "opening for insertion"
                            },
                            {
                                "target_lang_word": "socket",
                                "target_lang_sense_clarification": "receptacle"
                            }
                        ]
                    }
                }
            ]
        }
    ]

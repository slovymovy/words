                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of *leuren*.",
                        "ru": "Первое лицо единственного числа настоящего времени от глагола *leuren*."
                    },
                    "translations": {

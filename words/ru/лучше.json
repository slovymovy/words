    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "207fad48-e6cd-3fc0-aa8b-f3dde81ed9ba",
                    "sense_definition": "Сравнительная степень прилагательного <w>хороший</w>: более высокого качества, более полезный, приятный или подходящий.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "comparison_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта новая модель телефона намного <лучше> старой.",
                            "target_lang_translations": {
                                "en": "This new phone model is much <better> than the old one."
                            }
                        },
                        {
                            "text": "Погода сегодня <лучше>, чем была вчера.",
                            "target_lang_translations": {
                                "en": "The weather today is <better> than it was yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "полу́чше"
                    ],
                    "antonyms": [
                        "хуже"
                    ],
                    "common_phrases": [
                        "намного лучше",
                        "всё лучше и лучше",
                        "чем лучше, тем"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "comparative degree"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Comparative form of \"good\": of higher quality, more useful, pleasant, or suitable."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "better",
                                "target_lang_sense_clarification": "comparative of good"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcaf6ba0-dcd6-3a81-9c42-d1d516cddbbd",
                    "sense_definition": "Об улучшении физического состояния, здоровья или самочувствия.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "health_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После приёма лекарства пациенту стало <лучше>.",
                            "target_lang_translations": {
                                "en": "After taking the medicine, the patient felt <better>."
                            }
                        },
                        {
                            "text": "Мне уже <лучше>, голова больше не болит.",
                            "target_lang_translations": {
                                "en": "I feel <better> already, my head doesn't ache anymore."
                            }
                        }
                    ],
                    "antonyms": [
                        "хуже"
                    ],
                    "common_phrases": [
                        "мне лучше",
                        "стало лучше",
                        "чувствовать себя лучше"
                    ],
                    "target_lang_definitions": {
                        "en": "Referring to an improvement in physical condition, health, or well-being."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "better",
                                "target_lang_sense_clarification": "improved health/recovery"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1052e919-71f0-32c5-8291-d5520c9dce2c",
                    "sense_definition": "Сравнительная степень наречия <w>хорошо</w>: более успешно, качественно, умело или удачно.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "comparison_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Теперь я понимаю эту тему гораздо <лучше>.",
                            "target_lang_translations": {
                                "en": "Now I understand this topic much <better>."
                            }
                        },
                        {
                            "text": "Он говорит по-русски <лучше>, чем я.",
                            "target_lang_translations": {
                                "en": "He speaks Russian <better> than I do."
                            }
                        }
                    ],
                    "synonyms": [
                        "полу́чше"
                    ],
                    "antonyms": [
                        "хуже"
                    ],
                    "common_phrases": [
                        "как можно лучше",
                        "знать лучше",
                        "сделать лучше"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "comparative degree"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Comparative form of \"well\": more successfully, skillfully, or effectively."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "better",
                                "target_lang_sense_clarification": "comparative of well"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92fb5431-131e-38c0-82aa-819131cea082",
                    "sense_definition": "Употребляется как вводное слово для уточнения или исправления сказанного ранее; точнее говоря.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "clarification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы встретимся вечером или, <лучше>, завтра утром.",
                            "target_lang_translations": {
                                "en": "We will meet in the evening or, <better>, tomorrow morning."
                            }
                        },
                        {
                            "text": "Он устал, или <лучше> сказать, совершенно выбился из сил.",
                            "target_lang_translations": {
                                "en": "He was tired, or <better> to say, completely exhausted."
                            }
                        }
                    ],
                    "synonyms": [
                        "точнее",
                        "вернее",
                        "лучше сказать"
                    ],
                    "common_phrases": [
                        "или лучше",
                        "лучше сказать"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to clarify or correct a previous statement; or rather."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "better",
                                "target_lang_sense_clarification": "correction/clarification"
                            },
                            {
                                "target_lang_word": "rather",
                                "target_lang_sense_clarification": "more precisely"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "12c6d62a-b4f5-375f-a25f-0c72fe1ce58f",
                    "sense_definition": "Употребляется для выражения совета, рекомендации или предпочтения в данной ситуации.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "preference_advice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вы <лучше> спросите у учителя, он всё объяснит.",
                            "target_lang_translations": {
                                "en": "You'd <better> ask the teacher, he will explain everything."
                            }
                        },
                        {
                            "text": "Давай <лучше> останемся сегодня дома и посмотрим фильм.",
                            "target_lang_translations": {
                                "en": "We'd <better> stay home today and watch a movie."
                            }
                        }
                    ],
                    "synonyms": [
                        "скорее",
                        "предпочтительнее"
                    ],
                    "common_phrases": [
                        "лучше бы",
                        "тебе лучше",
                        "лучше не"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express advice, a recommendation, or a preference in a given situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "had better",
                                "target_lang_sense_clarification": "advice/recommendation"
                            },
                            {
                                "target_lang_word": "preferably",
                                "target_lang_sense_clarification": "preference"
                            }
                        ]
                    }
                }
            ]
        }
    ]

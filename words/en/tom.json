            "senses": [
                {
                    "sense_id": "f0e29f20-b8f8-3e66-bf80-6383541a21e1",
                    "sense_definition": "The male of the domesticated cat, especially if not neutered.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "male_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old <w>tom</w> sat on the fence and howled at the moon.",
                            "target_lang_translations": {
                                "ru": "Старый <w>кот</w> сидел на заборе и выл на луну."
                            }
                        },
                        {
                            "text": "We need to get our <w>tom</w> neutered before he gets into more fights.",
                            "target_lang_translations": {
                                "ru": "Нам нужно кастрировать нашего <w>кота</w>, пока он снова не ввязался в драку."
                            }

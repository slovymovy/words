    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9bb7d1a6-9667-3d01-8df7-8d702a6214f6",
                    "sense_definition": "Место, специально отведенное для посадки и высадки пассажиров общественного транспорта (автобуса, трамвая и т. п.).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Твой автобус уже подъезжает на <w>остановку</w>.",
                            "target_lang_translations": {
                                "en": "Your bus is already approaching the <w>stop</w>."
                            }
                        },
                        {
                            "text": "Мы долго ждали трамвай на <w>остановке</w>.",
                            "target_lang_translations": {
                                "en": "We waited a long time for the tram at the <w>stop</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "пункт",
                        "станция"
                    ],
                    "common_phrases": [
                        "автобусная остановка",
                        "конечная остановка",
                        "ждать на остановке",
                        "выйти на остановке"
                    ],
                    "target_lang_definitions": {
                        "en": "A place specially designated for boarding and alighting passengers of public transport (bus, tram, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "public transport"
                            },
                            {
                                "target_lang_word": "bus stop",
                                "target_lang_sense_clarification": "specific type"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44efbdff-8cd1-3325-aa52-8bf084cf7ee4",
                    "sense_definition": "Расстояние между двумя соседними пунктами, где останавливается общественный транспорт.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам нужно проехать еще одну <w>остановку</w>.",
                            "target_lang_translations": {
                                "en": "We need to go one more <w>stop</w>."
                            }
                        },
                        {
                            "text": "Мой дом находится через две <w>остановки</w> от метро.",
                            "target_lang_translations": {
                                "en": "My house is two <w>stops</w> from the metro."
                            }
                        }
                    ],
                    "synonyms": [
                        "перегон"
                    ],
                    "common_phrases": [
                        "проехать остановку",
                        "через одну остановку",
                        "следующая остановка"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to measure distance in cities"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The distance between two neighboring points where public transport stops."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "distance unit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1864365-1894-359d-a91e-69a83c4f98ee",
                    "sense_definition": "Временное прекращение движения или какого-либо действия; перерыв.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_cessation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы работали три часа без <w>остановки</w>.",
                            "target_lang_translations": {
                                "en": "We worked for three hours without a <w>break</w>."
                            }
                        },
                        {
                            "text": "Поезд сделал короткую <w>остановку</w> в поле.",
                            "target_lang_translations": {
                                "en": "The train made a short <w>stop</w> in the field."
                            }
                        }
                    ],
                    "synonyms": [
                        "пауза",
                        "перерыв",
                        "стоянка",
                        "задержка"
                    ],
                    "antonyms": [
                        "движение"

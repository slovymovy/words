    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "6dcb4c1b-6c03-3325-b248-3897ecfe862c",
                    "sense_definition": "По всей видимости, наверное, кажется; используется как вводное слово для выражения предположения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "assumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты, <w>видать</w>, очень устал после долгой дороги.",
                            "target_lang_translations": {
                                "en": "You <w>must be</w> very tired after the long journey."
                            }
                        },
                        {
                            "text": "Хозяин дома, <w>видать</w>, не ждал гостей в такой поздний час.",
                            "target_lang_translations": {
                                "en": "The host, <w>apparently</w>, was not expecting guests at such a late hour."
                            }
                        },
                        {
                            "text": "Дела у них, <w>видать</w>, идут совсем не так хорошо, как они говорят.",
                            "target_lang_translations": {
                                "en": "Things, <w>it seems</w>, are not going nearly as well for them as they say."
                            }
                        }
                    ],
                    "synonyms": [
                        "по-видимому",
                        "очевидно",
                        "видно",
                        "знать"
                    ],
                    "common_phrases": [
                        "видать по глазам",
                        "как видать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "характерно для устной речи"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Apparently, probably, it seems; used as a parenthetical word to express an assumption."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "apparently",
                                "target_lang_sense_clarification": "based on what is seen or heard"
                            },
                            {
                                "target_lang_word": "it seems",
                                "target_lang_sense_clarification": "used to say that something appears to be true"
                            },
                            {
                                "target_lang_word": "evidently",
                                "target_lang_sense_clarification": "clearly seen or understood"
                            },
                            {
                                "target_lang_word": "looks like",
                                "target_lang_sense_clarification": "colloquial expression of probability"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a0d5376-c9d2-35bf-8a5c-4698f97f3c60",
                    "sense_definition": "Можно увидеть или разглядеть; доступно для зрения (часто используется в безличных предложениях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "visibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В густом тумане ничего не <w>видать</w> даже на расстоянии вытянутой руки.",
                            "target_lang_translations": {
                                "en": "In the thick fog, nothing <w>is visible</w> even at arm's length."
                            }
                        },
                        {
                            "text": "С вершины горы <w>видать</w> всю нашу маленькую деревню.",
                            "target_lang_translations": {
                                "en": "From the top of the mountain, one <w>can see</w> our whole little village."
                            }
                        }
                    ],
                    "synonyms": [
                        "видно"
                    ],
                    "common_phrases": [
                        "ничего не видать",
                        "отсюда видать",
                        "не видать как своих ушей"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Can be seen or discerned; visible (often used in impersonal sentences)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "visible",
                                "target_lang_sense_clarification": "able to be seen"
                            },
                            {
                                "target_lang_word": "can be seen",
                                "target_lang_sense_clarification": "possibility of seeing something"
                            },
                            {
                                "target_lang_word": "in sight",
                                "target_lang_sense_clarification": "within the field of vision"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "12685cbe-3fe8-3cad-b51e-bcf93e535e1d",
                    "sense_definition": "Видеть, встречать кого-либо или что-либо (обычно многократно или в прошлом).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я много странствовал, но таких красивых мест никогда не <w>видал</w>.",
                            "target_lang_translations": {
                                "en": "I have traveled a lot, but I have never <w>seen</w> such beautiful places."
                            }
                        },
                        {
                            "text": "Мы с ним давно не <w>видались</w>, наверное, уже лет пять.",
                            "target_lang_translations": {
                                "en": "We haven't <w>seen</w> each other for a long time, probably about five years."
                            }
                        },
                        {
                            "text": "Ты <w>видал</w> когда-нибудь настоящего живого медведя в лесу?",
                            "target_lang_translations": {
                                "en": "Have you ever <w>seen</w> a real live bear in the forest?"
                            }
                        }
                    ],
                    "synonyms": [
                        "видеть",
                        "встречать"
                    ],
                    "common_phrases": [
                        "видать виды",
                        "глаза бы мои не видали"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To see, to meet someone or something (usually repeatedly or in the past)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to see",
                                "target_lang_sense_clarification": "to perceive with eyes or meet"
                            },
                            {
                                "target_lang_word": "to encounter",
                                "target_lang_sense_clarification": "to meet someone unexpectedly"
                            },
                            {
                                "target_lang_word": "to witness",
                                "target_lang_sense_clarification": "to see an event happen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1fe0e03-f303-3888-9c6a-39e68a0cd042",
                    "sense_definition": "Смотри! Посмотри! (используется для привлечения внимания собеседника к чему-то удивительному или возмутительному).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Видал</w>, какую дорогую машину купил наш сосед?",
                            "target_lang_translations": {
                                "en": "<w>Did you see</w> what an expensive car our neighbor bought?"
                            }
                        },
                        {
                            "text": "<w>Видал</w>, как ловко он перепрыгнул через этот высокий забор?",
                            "target_lang_translations": {
                                "en": "<w>Did you see</w> how nimbly he jumped over that high fence?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "ты видал",
                        "видал миндал"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "просторечное выражение"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Look! Did you see that? (used to draw attention to something surprising or outrageous)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "did you see",
                                "target_lang_sense_clarification": "asking for confirmation of a surprising sight"
                            },
                            {
                                "target_lang_word": "look at",
                                "target_lang_sense_clarification": "directing attention to something"
                            },
                            {
                                "target_lang_word": "check out",
                                "target_lang_sense_clarification": "slang; look at something interesting"
                            }
                        ]
                    }
                }
            ]
        }
    ]

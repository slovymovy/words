{
    "word_family": [
        "aangifte",
        "aangever"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1db19346-0048-3a81-92cb-926af446d871",
                    "sense_definition": "Iets in iemands hand leggen of aanreiken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "handing_over",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je me alsjeblieft het zout even <w>aangeven</w>?",
                            "target_lang_translations": {
                                "ru": "Можешь, пожалуйста, <w>передать</w> мне соль?"
                            }
                        },
                        {
                            "text": "Hij <w>gaf</w> de hamer <w>aan</w> zodat ik de spijker kon slaan.",
                            "target_lang_translations": {
                                "ru": "Он <w>подал</w> молоток, чтобы я мог забить гвоздь."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanreiken",
                        "overhandigen",
                        "geven"
                    ],
                    "antonyms": [
                        "aannemen",
                        "aanpakken"
                    ],
                    "common_phrases": [
                        "het zout aangeven",
                        "iets aangeven",
                        "gereedschap aangeven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положить что-то кому-то в руку или передать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подавать",
                                "target_lang_sense_clarification": "передавать предмет в руки"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "вручать что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e79ed521-9622-39f4-9ab0-d9e31b045a00",
                    "sense_definition": "Laten zien, aanwijzen of markeren waar iets is of wat iets betekent.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "indicating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De borden langs de weg <w>geven</w> de richting naar het centrum <w>aan</w>.",
                            "target_lang_translations": {
                                "ru": "Дорожные знаки <w>указывают</w> направление к центру."
                            }
                        },
                        {
                            "text": "Op de kaart kun je precies <w>aangeven</w> waar het hotel zich bevindt.",
                            "target_lang_translations": {
                                "ru": "На карте можно точно <w>отметить</w> (или указать), где находится отель."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanwijzen",
                        "aanduiden",
                        "tonen",
                        "markeren"
                    ],
                    "common_phrases": [
                        "de richting aangeven",
                        "de weg aangeven",
                        "de tijd aangeven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывать, указывать или отмечать местоположение или значение чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "указывать",
                                "target_lang_sense_clarification": "показывать направление или факт"
                            },
                            {
                                "target_lang_word": "обозначать",
                                "target_lang_sense_clarification": "отмечать или символизировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e500a88c-7efe-34c0-a9c7-fd80c88f7ae2",
                    "sense_definition": "Te kennen geven of uiten wat men bedoelt, wil of voelt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "expressing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar <w>gaf</w> duidelijk <w>aan</w> wat de regels in de klas zijn.",
                            "target_lang_translations": {
                                "ru": "Учитель четко <w>обозначил</w> правила поведения в классе."
                            }
                        },
                        {
                            "text": "Zij <w>gaf</w> tijdens het gesprek <w>aan</w> dat ze zich niet lekker voelde.",
                            "target_lang_translations": {
                                "ru": "В ходе разговора она <w>сообщила</w>, что плохо себя чувствует."
                            }
                        }
                    ],
                    "synonyms": [
                        "uiten",
                        "laten weten",
                        "melden",
                        "vertellen"
                    ],
                    "antonyms": [
                        "verzwijgen"
                    ],
                    "common_phrases": [
                        "duidelijk aangeven",
                        "een grens aangeven",
                        "zijn mening aangeven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать знать или выражать то, что кто-то имеет в виду, хочет или чувствует."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообщать",
                                "target_lang_sense_clarification": "информировать о чем-либо"
                            },
                            {
                                "target_lang_word": "давать понять",
                                "target_lang_sense_clarification": "выражать намерение или чувство"
                            },
                            {
                                "target_lang_word": "обозначать",
                                "target_lang_sense_clarification": "очерчивать или выражать позицию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d46970f-a6f7-35af-9fa5-4fb39f86bb4e",
                    "sense_definition": "Officieel melden of registreren bij een bevoegde instantie, zoals de politie of de belastingdienst.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reporting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je bent verplicht om al je inkomsten bij de belastingdienst <w>aan</w> te <w>geven</w>.",
                            "target_lang_translations": {
                                "ru": "Вы обязаны <w>декларировать</w> все свои доходы в налоговой службе."
                            }
                        },
                        {
                            "text": "Na de inbraak zijn ze direct naar het politiebureau gegaan om de diefstal <w>aan</w> te <w>geven</w>.",
                            "target_lang_translations": {
                                "ru": "После взлома они сразу пошли в полицейский участок, чтобы <w>заявить</w> о краже."
                            }
                        }
                    ],
                    "synonyms": [
                        "melden",
                        "opgeven",
                        "declareren"
                    ],
                    "common_phrases": [
                        "belasting aangeven",
                        "een geboorte aangeven",
                        "diefstal aangeven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально сообщать или регистрировать в компетентных органах (полиция, налоговая)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "декларировать",
                                "target_lang_sense_clarification": "о налогах или доходах"
                            },
                            {
                                "target_lang_word": "заявлять",
                                "target_lang_sense_clarification": "официально сообщать, например, в полицию"
                            },
                            {
                                "target_lang_word": "регистрировать",
                                "target_lang_sense_clarification": "официально оформлять событие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
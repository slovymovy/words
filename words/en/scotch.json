        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e552873b-2135-39e4-9363-826bc4dffb1c",
                    "sense_definition": "Whisky distilled in Scotland, especially from malted barley.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_whisky",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He poured himself a glass of <w>Scotch</w> and sat by the fire.",
                            "target_lang_translations": {
                                "ru": "Он налил себе стакан <w>скотча</w> и сел у камина."
                            }
                        },
                        {
                            "text": "The bartender recommended a single malt <w>Scotch</w> for the special occasion.",
                            "target_lang_translations": {
                                "ru": "Бармен порекомендовал односолодовый <w>скотч</w> для особого случая."
                            }
                        }
                    ],
                    "synonyms": [
                        "malt",
                        "Scotch whisky"
                    ],
                    "common_phrases": [
                        "single malt Scotch",
                        "bottle of Scotch",
                        "glass of Scotch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Виски, произведенный в Шотландии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скотч",
                                "target_lang_sense_clarification": "разговорное название шотландского виски"
                            },
                            {
                                "target_lang_word": "шотландский виски",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2cdb1ae-3331-3870-a320-869f19f88ee5",
                    "sense_definition": "Transparent adhesive tape used for fastening paper or other light materials.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_tape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Could you pass me the <w>Scotch</w> so I can wrap this birthday present?",
                            "target_lang_translations": {
                                "ru": "Не мог бы ты передать мне <w>скотч</w>, чтобы я упаковал этот подарок на день рождения?"
                            }
                        },
                        {
                            "text": "She repaired the torn page of the book with a small piece of <w>Scotch</w>.",
                            "target_lang_translations": {
                                "ru": "Она заклеила порванную страницу книги маленьким кусочком <w>скотча</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "roll of Scotch"

                {
                    "sense_id": "8a866bc2-2a41-3e3c-aaeb-cce56b53434a",
                    "sense_definition": "Potoczne określenie gry sportowej w koszykówkę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopaki spotkali się na boisku szkolnym, żeby pograć w <w>kosza</w>.",
                            "target_lang_translations": {
                                "ru": "Парни встретились на школьной площадке, чтобы поиграть в <w>баскетбол</w>."
                            }

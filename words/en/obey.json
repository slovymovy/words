    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a31718dd-e060-3f47-9f0d-e8a2f4be4526",
                    "sense_definition": "To do what a person, law, or rule says you must do.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "All drivers must <w>obey</w> the speed limit to ensure safety on the road.",
                            "target_lang_translations": {
                                "ru": "Все водители должны <w>соблюдать</w> ограничение скорости, чтобы обеспечить безопасность на дороге."
                            }
                        },
                        {
                            "text": "The students were expected to <w>obey</w> the rules of the school at all times.",
                            "target_lang_translations": {
                                "ru": "От учеников ожидалось, что они будут всегда <w>соблюдать</w> школьные правила."
                            }
                        }
                    ],
                    "synonyms": [
                        "comply",
                        "conform",
                        "submit",
                        "yield",
                        "mind"
                    ],
                    "antonyms": [
                        "disobey",
                        "defy",
                        "violate"
                    ],
                    "common_phrases": [
                        "obey the law",
                        "obey the rules",
                        "obey orders",
                        "obey a command"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять то, что требует человек, закон или правило."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подчиняться",
                                "target_lang_sense_clarification": "следовать правилам, законам или воле кого-либо"
                            },
                            {
                                "target_lang_word": "соблюдать",
                                "target_lang_sense_clarification": "придерживаться законов, правил или норм"
                            },
                            {
                                "target_lang_word": "слушаться",
                                "target_lang_sense_clarification": "выполнять требования (обычно о людях)"
                            },
                            {
                                "target_lang_word": "повиноваться",
                                "target_lang_sense_clarification": "беспрекословно выполнять приказы (формально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23004070-52c3-3227-9422-0ba08f01d493",
                    "sense_definition": "To do as one is told without a specific object mentioned.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dog was trained to <w>obey</w> instantly when it heard the whistle.",
                            "target_lang_translations": {
                                "ru": "Собаку приучили мгновенно <w>слушаться</w>, как только она услышит свисток."
                            }
                        },
                        {
                            "text": "Soldiers are taught to <w>obey</w> without asking any questions.",
                            "target_lang_translations": {
                                "ru": "Солдат учат <w>повиноваться</w>, не задавая лишних вопросов."
                            }
                        }
                    ],
                    "synonyms": [
                        "comply",
                        "yield",
                        "acquiesce",
                        "accede"
                    ],
                    "antonyms": [
                        "rebel",
                        "resist"
                    ],
                    "common_phrases": [
                        "learn to obey",
                        "refuse to obey",
                        "forced to obey"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поступать так, как велено; проявлять послушание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слушаться",
                                "target_lang_sense_clarification": "проявлять послушание (часто о детях или животных)"
                            },
                            {
                                "target_lang_word": "повиноваться",
                                "target_lang_sense_clarification": "действовать по приказу без возражений"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
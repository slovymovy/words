                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical or literary context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Capital punishment by beheading."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the axe",
                                "target_lang_sense_clarification": "execution method"
                            }
                        ]
                    }
                }
            ]
        }
    ]

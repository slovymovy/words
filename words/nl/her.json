    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "421c561a-2cc4-3243-ba29-fc408cd6e7c8",
                    "sense_definition": "Geleden; duidt een periode aan die verstreken is sinds een moment in het verleden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "time_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is inmiddels alweer jaren <w>her</w> dat wij elkaar voor het laatst spraken.",
                            "target_lang_translations": {
                                "en": "It is now years <w>ago</w> since we last spoke.",
                                "ru": "Мы в последний раз разговаривали уже много лет <w>назад</w>."
                            }
                        },
                        {
                            "text": "Deze traditie stamt uit tijden van lang <w>her</w>.",
                            "target_lang_translations": {
                                "en": "This tradition dates from times long <w>past</w>.",
                                "ru": "Эта традиция восходит к временам, что были давным-<w>давно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "geleden"
                    ],
                    "common_phrases": [
                        "jaren her",
                        "lang her"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "voornamelijk literair of plechtig gebruik"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Ago; indicates a period of time that has passed since a moment in the past.",
                        "ru": "Назад; указывает на период времени, прошедший с момента в прошлом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ago",
                                "target_lang_sense_clarification": "time passed"
                            },
                            {
                                "target_lang_word": "past",
                                "target_lang_sense_clarification": "long ago"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "назад",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "тому назад",
                                "target_lang_sense_clarification": "устаревшее или литературное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a89963a-8663-3a69-9b78-2769b16b816e",
                    "sense_definition": "Naar links; een oud commando dat koetsiers gebruikten voor hun trekdieren.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koetsier riep luidkeels <w>her</w> om het paard de juiste kant op te sturen.",
                            "target_lang_translations": {
                                "en": "The coachman shouted <w>left</w> loudly to steer the horse in the right direction.",
                                "ru": "Кучер громко крикнул «<w>влево</w>», чтобы направить лошадь в нужную сторону."
                            }
                        },
                        {
                            "text": "Hij werd van hot naar <w>her</w> gestuurd zonder enig resultaat.",
                            "target_lang_translations": {
                                "en": "He was sent from pillar to <w>post</w> without any result.",
                                "ru": "Его посылали туда и <w>сюда</w> без какого-либо результата."
                            }
                        }
                    ],
                    "common_phrases": [
                        "van hot naar her"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "ouderwetse koetsiersterm"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To the left; an old command used by coachmen for their draft animals.",
                        "ru": "Влево; старая команда, которую кучеры использовали для управления упряжными животными."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "left",
                                "target_lang_sense_clarification": "command for animals"
                            },
                            {
                                "target_lang_word": "thither",
                                "target_lang_sense_clarification": "in idioms like 'hither and thither'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "влево",
                                "target_lang_sense_clarification": "направление"
                            },
                            {
                                "target_lang_word": "цоб",
                                "target_lang_sense_clarification": "команда животным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74706ba5-a4e3-3cf0-9680-72c2b3964d28",
                    "sense_definition": "Hierheen; naar deze plaats toe.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kom <w>her</w> en luister goed naar wat ik te zeggen heb.",
                            "target_lang_translations": {
                                "en": "Come <w>hither</w> and listen closely to what I have to say.",
                                "ru": "Иди <w>сюда</w> и слушай внимательно, что я скажу."
                            }
                        },
                        {
                            "text": "Het schip deinde heen en <w>her</w> op de woeste golven.",
                            "target_lang_translations": {
                                "en": "The ship swayed to and <w>fro</w> on the rough waves.",
                                "ru": "Корабль качался туда и <w>сюда</w> на бурных волнах."
                            }
                        }
                    ],
                    "synonyms": [
                        "hierheen"
                    ],
                    "antonyms": [
                        "heen"
                    ],
                    "common_phrases": [
                        "heen en her"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Hither; towards this place.",
                        "ru": "Сюда; по направлению к этому месту."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hither",
                                "target_lang_sense_clarification": "archaic direction"
                            },
                            {
                                "target_lang_word": "fro",
                                "target_lang_sense_clarification": "in 'to and fro'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сюда",
                                "target_lang_sense_clarification": "направление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "365bed71-2142-3cce-aabe-aabaaddcfe47",
                    "sense_definition": "Een herkansing; het opnieuw maken van een toets of examen na een onvoldoende resultaat.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omdat hij ziek was tijdens de toetsweek, mag hij een <w>her</w> doen.",
                            "target_lang_translations": {
                                "en": "Because he was ill during test week, he is allowed to do a <w>resit</w>.",
                                "ru": "Так как он болел во время экзаменационной недели, он может прийти на <w>пересдачу</w>."
                            }
                        },
                        {
                            "text": "Zij studeert hard voor de <w>her</w> van haar wiskunde-examen.",
                            "target_lang_translations": {
                                "en": "She is studying hard for the <w>retake</w> of her math exam.",
                                "ru": "Она усердно готовится к <w>пересдаче</w> экзамена по математике."
                            }
                        }
                    ],
                    "synonyms": [
                        "herkansing"
                    ],
                    "common_phrases": [
                        "een her doen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "studententaal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A resit; retaking a test or exam after an insufficient result.",
                        "ru": "Пересдача; повторное прохождение теста или экзамена после получения неудовлетворительной оценки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resit",
                                "target_lang_sense_clarification": "exam"
                            },
                            {
                                "target_lang_word": "retake",
                                "target_lang_sense_clarification": "exam"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пересдача",
                                "target_lang_sense_clarification": "экзамена"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "manner_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat najlepiej pływa <w>stylem</w> grzbietowym.",
                            "target_lang_translations": {
                                "ru": "Мой брат лучше всего плавает <w>стилем</w> на спине."

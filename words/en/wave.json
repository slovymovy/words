                {
                    "sense_id": "effd5347-0cf5-3cbf-83d0-11dd67963937",
                    "sense_definition": "A raised line of water that moves across the surface of the sea, ocean, or a lake.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "water_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>wave</w> traveled from the center of the lake to the shore.",
                            "target_lang_translations": {
                                "ru": "<w>Волна</w> прошла от центра озера к берегу."
                            }
                        },
                        {
                            "text": "Surfers were waiting for a big <w>wave</w> to ride.",
                            "target_lang_translations": {
                                "ru": "Серферы ждали большую <w>волну</w>, чтобы оседлать её."
                            }

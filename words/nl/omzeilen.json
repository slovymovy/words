            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d00ae407-afe8-3912-82f8-0cd35f778652",
                    "sense_definition": "Een probleem, regel of obstakel ontwijken of er op een slimme manier omheen gaan.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vaak loont het te veel om de regels te <w>omzeilen</w>.",
                            "target_lang_translations": {
                                "ru": "Часто слишком выгодно <w>обходить</w> правила.",
                                "en": "It often pays off too much to <w>circumvent</w> the rules."
                            }
                        },
                        {
                            "text": "Zo hebben we die file netjes <w>omzeild</w>.",
                            "target_lang_translations": {
                                "ru": "Так мы ловко <w>обошли</w> эту пробку.",
                                "en": "That is how we neatly <w>bypassed</w> that traffic jam."
                            }
                        },
                        {
                            "text": "Bedrijven proberen soms de belastingwetgeving te <w>omzeilen</w>.",
                            "target_lang_translations": {
                                "ru": "Компании иногда пытаются <w>обойти</w> налоговое законодательство.",
                                "en": "Companies sometimes try to <w>circumvent</w> tax legislation."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontwijken",
                        "vermijden",
                        "ontduiken"
                    ],
                    "antonyms": [
                        "naleven",
                        "confronteren"
                    ],
                    "common_phrases": [
                        "de regels omzeilen",
                        "een probleem omzeilen",
                        "de wet omzeilen",
                        "beveiliging omzeilen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "onscheidbaar werkwoord (omzeilde, heeft omzeild)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Избегать проблемы, правила или препятствия, либо хитро обходить их.",
                        "en": "To avoid a problem, rule, or obstacle, or to go around it in a clever way."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обходить",
                                "target_lang_sense_clarification": "препятствие, закон"
                            },
                            {
                                "target_lang_word": "уклоняться",
                                "target_lang_sense_clarification": "от налогов, ответственности"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "circumvent",
                                "target_lang_sense_clarification": "rules, laws, or systems"
                            },
                            {
                                "target_lang_word": "bypass",
                                "target_lang_sense_clarification": "obstacles or security measures"
                            },
                            {
                                "target_lang_word": "get around",
                                "target_lang_sense_clarification": "a problem or restriction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de96c42d-a04b-36fd-b443-954873f3ed05",
                    "sense_definition": "Met een zeilboot een omweg maken of ergens omheen varen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben enkele kilometers moeten <w>omzeilen</w> om de haven te bereiken.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>сделать крюк</w> в несколько километров, чтобы добраться до гавани.",
                                "en": "We had to <w>sail a detour</w> of a few kilometers to reach the harbor."
                            }
                        },
                        {
                            "text": "Door de onverwachte storm <w>zeilden</w> we een groot stuk <w>om</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за неожиданного шторма мы <w>сделали</w> большой <w>крюк</w>.",
                                "en": "Due to the unexpected storm, we <w>sailed</w> a large <w>detour</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "omvaren"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "scheidbaar werkwoord (zeilde om, heeft omgezeild)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать крюк на парусной лодке или огибать что-либо.",
                        "en": "To make a detour with a sailboat or to sail around something."
                    },
                    "translations": {

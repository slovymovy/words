                {
                    "sense_id": "cab4452b-db66-38fa-a596-d4f2cac732f3",
                    "sense_definition": "O dziecku: nieposłuszny, sprawiający problemy wychowawcze.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {

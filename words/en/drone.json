{
    "word_family": [],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7f4161e3-617f-3ce6-a055-42e0d442a309",
                    "sense_definition": "An aircraft without a human pilot on board, operated by remote control or computers.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_uav",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The military used a <w>drone</w> to survey the area.",
                            "target_lang_translations": {
                                "ru": "Военные использовали <w>дрон</w> для осмотра местности."
                            }
                        },
                        {
                            "text": "He bought a camera <w>drone</w> to take aerial photos of the wedding.",
                            "target_lang_translations": {
                                "ru": "Он купил <w>дрон</w> с камерой, чтобы сделать фотографии свадьбы с воздуха."
                            }
                        }
                    ],
                    "synonyms": [
                        "UAV"
                    ],
                    "common_phrases": [
                        "fly a drone",
                        "drone strike",
                        "surveillance drone",
                        "camera drone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Беспилотный летательный аппарат, управляемый дистанционно или компьютером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дрон",
                                "target_lang_sense_clarification": "летательный аппарат"
                            },
                            {
                                "target_lang_word": "беспилотник",
                                "target_lang_sense_clarification": "разг. беспилотный аппарат"
                            },
                            {
                                "target_lang_word": "БПЛА",
                                "target_lang_sense_clarification": "воен. аббревиатура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1158fcb0-38cf-3d2e-a7e7-f3610cc8eaba",
                    "sense_definition": "A continuous low humming or buzzing sound.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We could hear the steady <w>drone</w> of the airplane engine.",
                            "target_lang_translations": {
                                "ru": "Мы слышали ровный <w>гул</w> двигателя самолета."
                            }
                        },
                        {
                            "text": "The monotonous <w>drone</w> of the fan made it hard to concentrate.",
                            "target_lang_translations": {
                                "ru": "Монотонное <w>жужжание</w> вентилятора мешало сосредоточиться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "monotonous drone",
                        "low drone",
                        "steady drone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непрерывный низкий гудящий или жужжащий звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гул",
                                "target_lang_sense_clarification": "низкий, монотонный звук"
                            },
                            {
                                "target_lang_word": "жужжание",
                                "target_lang_sense_clarification": "звук насекомых или механизмов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "972178e7-78a9-3b9f-a5f5-f53f9b9b18a0",
                    "sense_definition": "A male bee, ant, or wasp that does not gather food but functions to fertilize the queen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_insect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>drone</w> does not collect pollen like the worker bees.",
                            "target_lang_translations": {
                                "ru": "<w>Трутень</w> не собирает пыльцу, как рабочие пчелы."
                            }
                        },
                        {
                            "text": "In the hive, the <w>drones</w> are often driven out before winter.",
                            "target_lang_translations": {
                                "ru": "В улье <w>трутней</w> часто изгоняют перед зимой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "worker bees and drones"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самец пчелы, муравья или осы, который не собирает пищу, а служит для оплодотворения матки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трутень",
                                "target_lang_sense_clarification": "самец пчелы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "242e4845-2e60-3956-b949-f4e8d762aea8",
                    "sense_definition": "A person who performs menial, tedious work.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_insect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt like just another corporate <w>drone</w> sitting in a cubicle all day.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя просто очередным корпоративным <w>винтиком</w>, сидящим в кабинке весь день."
                            }
                        },
                        {
                            "text": "The manager treated them like mindless <w>drones</w> rather than skilled employees.",
                            "target_lang_translations": {
                                "ru": "Менеджер обращался с ними как с безмозглыми <w>роботами</w>, а не как с квалифицированными сотрудниками."
                            }
                        }
                    ],
                    "synonyms": [
                        "drudge"
                    ],
                    "common_phrases": [
                        "office drone",
                        "corporate drone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, выполняющий черную, утомительную работу; бездумный исполнитель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "винтик",
                                "target_lang_sense_clarification": "перен. мелкий работник"
                            },
                            {
                                "target_lang_word": "робот",
                                "target_lang_sense_clarification": "перен. бездумный исполнитель"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "22ed5194-3ef9-3d22-a2d8-a582e7116077",
                    "sense_definition": "To make a continuous low humming sound.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The refrigerator <w>droned</w> loudly in the quiet kitchen.",
                            "target_lang_translations": {
                                "ru": "Холодильник громко <w>гудел</w> в тихой кухне."
                            }
                        },
                        {
                            "text": "An airplane <w>droned</w> overhead as we lay on the grass.",
                            "target_lang_translations": {
                                "ru": "Самолет <w>гудел</w> над головой, пока мы лежали на траве."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать непрерывный низкий гудящий звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гудеть",
                                "target_lang_sense_clarification": "издавать низкий звук"
                            },
                            {
                                "target_lang_word": "жужжать",
                                "target_lang_sense_clarification": "издавать вибрирующий звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1404ba5-3adb-37b2-9c4c-403988a3b6f7",
                    "sense_definition": "To speak in a boring, monotonous tone for a long time.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lecturer <w>droned</w> on for hours about the history of taxation.",
                            "target_lang_translations": {
                                "ru": "Лектор часами <w>бубнил</w> об истории налогообложения."
                            }
                        },
                        {
                            "text": "She stopped listening as he continued to <w>drone</w> about his problems.",
                            "target_lang_translations": {
                                "ru": "Она перестала слушать, так как он продолжал <w>нудить</w> о своих проблемах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drone on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить скучным, монотонным тоном в течение долгого времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бубнить",
                                "target_lang_sense_clarification": "говорить монотонно"
                            },
                            {
                                "target_lang_word": "нудить",
                                "target_lang_sense_clarification": "разг. надоедливо говорить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3bcc3d5-cfbf-37b1-8474-08130db59bf1",
                    "sense_definition": "To attack or target with an unmanned aerial vehicle.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_uav",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government was criticized for <w>droning</w> targets in civilian areas.",
                            "target_lang_translations": {
                                "ru": "Правительство критиковали за <w>атаки дронами</w> по целям в гражданских районах."
                            }
                        },
                        {
                            "text": "Modern warfare increasingly involves <w>droning</w> enemy positions from a distance.",
                            "target_lang_translations": {
                                "ru": "Современная война все чаще включает <w>удары беспилотниками</w> по позициям врага с расстояния."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Атаковать или выслеживать с помощью беспилотного летательного аппарата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атаковать дроном",
                                "target_lang_sense_clarification": "использовать БПЛА для удара"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "b9b27faa-3cd0-370b-9331-fec8879466cf",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Drone</w> signed the documents yesterday.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Дроун</w> подписал документы вчера."
                            }
                        },
                        {
                            "text": "I met a family named <w>Drone</w> while visiting the village.",
                            "target_lang_translations": {
                                "ru": "Я встретил семью по фамилии <w>Дроун</w>, когда посещал деревню."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дроун",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "33c57047-e934-374b-8f2d-db693e38e540",
                    "sense_definition": "Wyczerpywać zasób czegoś, zmniejszać tego ilość przez używanie (np. energii, materiałów, pieniędzy).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stara lodówka <zużywa> znacznie więcej prądu niż nowoczesne modele.",
                            "target_lang_translations": {
                                "ru": "Старый холодильник <потребляет> значительно больше электричества, чем современные модели."
                            }
                        },
                        {
                            "text": "Musimy oszczędzać wodę i nie <zużywać> jej bez potrzeby.",
                            "target_lang_translations": {
                                "ru": "Мы должны экономить воду и не <расходовать> её без необходимости."
                            }
                        }
                    ],
                    "synonyms": [
                        "eksploatować"
                    ],
                    "common_phrases": [
                        "zużywać energię",
                        "zużywać prąd",
                        "zużywać wodę",
                        "zużywać paliwo",
                        "zużywać zapasy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тратить или уменьшать запас чего-либо (например, энергии, материалов, денег) в процессе использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расходовать",
                                "target_lang_sense_clarification": "тратить запас"
                            },
                            {
                                "target_lang_word": "потреблять",
                                "target_lang_sense_clarification": "использовать ресурс (энергию, воду)"
                            },
                            {
                                "target_lang_word": "тратить",
                                "target_lang_sense_clarification": "расходовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c3e106f-d1ad-32de-a7f7-1932026cf9a9",
                    "sense_definition": "Niszczyć się lub tracić swoje właściwości wskutek długiego używania i funkcjonowania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "deterioration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Opony w samochodzie wyścigowym <zużywają się> w bardzo szybkim tempie.",
                            "target_lang_translations": {
                                "ru": "Шины на гоночном автомобиле <изнашиваются> очень быстро."
                            }
                        },
                        {
                            "text": "Tanie ubrania często <zużywają się> już po kilku praniach.",
                            "target_lang_translations": {
                                "ru": "Дешёвая одежда часто <изнашивается> уже после нескольких стирок."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyczerpywać się",
                        "eksploatować"
                    ],
                    "antonyms": [
                        "regenerować się"
                    ],
                    "common_phrases": [
                        "szybko się zużywać",
                        "materiał się zużywa",
                        "części się zużywają"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить в негодность или терять свои свойства в результате длительного использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изнашиваться",
                                "target_lang_sense_clarification": "приходить в негодность (о вещах)"
                            },
                            {
                                "target_lang_word": "изнашивать",
                                "target_lang_sense_clarification": "портить использованием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92370a1a-bb59-3b0c-ac9a-a4a2541186c6",
                    "sense_definition": "Wyczerpywać czyjeś siły fizyczne lub psychiczne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "exhaustion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciągły stres w pracy powoli go <zużywa> i odbiera radość z życia.",
                            "target_lang_translations": {
                                "ru": "Постоянный стресс на работе медленно его <истощает> и отнимает радость жизни."
                            }
                        },
                        {
                            "text": "Nie warto <zużywać> zdrowia na zamartwianie się rzeczami, na które nie mamy wpływu.",
                            "target_lang_translations": {
                                "ru": "Не стоит <тратить> здоровье на переживания о вещах, на которые мы не можем повлиять."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zużywać siły",
                        "zużywać zdrowie",
                        "zużywać nerwy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишать кого-либо физических или душевных сил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "истощать",
                                "target_lang_sense_clarification": "лишать сил"
                            },
                            {
                                "target_lang_word": "изматывать",
                                "target_lang_sense_clarification": "сильно утомлять"
                            },
                            {
                                "target_lang_word": "тратить",
                                "target_lang_sense_clarification": "расходовать (здоровье, нервы)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
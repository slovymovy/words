{
    "word_family": [
        "dół",
        "dolny",
        "dołować"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0d309ab5-368e-3e39-b569-99d5b66ac98a",
                    "sense_definition": "Małe zagłębienie w ziemi lub na powierzchni; zdrobnienie od słowa <w>dół</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_depression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pies wykopał głęboki <w>dołek</w> pod płotem w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Собака вырыла глубокую <w>ямку</w> под забором в саду."
                            }
                        },
                        {
                            "text": "Musisz uważać, żeby nie wpaść w ten <w>dołek</w> na ścieżce.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть осторожен, чтобы не угодить в эту <w>ямку</w> на тропинке."
                            }
                        }
                    ],
                    "synonyms": [
                        "zagłębienie",
                        "dziura",
                        "jamka",
                        "wgłębienie",
                        "lej"
                    ],
                    "common_phrases": [
                        "wykopać dołek",
                        "zasypać dołek",
                        "wpaść w dołek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое углубление в земле или на поверхности; уменьшительная форма от слова «dół» (яма)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ямка",
                                "target_lang_sense_clarification": "маленькая яма"
                            },
                            {
                                "target_lang_word": "углубление",
                                "target_lang_sense_clarification": "в поверхности"
                            },
                            {
                                "target_lang_word": "лунка",
                                "target_lang_sense_clarification": "небольшое отверстие в земле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "727d6ef5-a45d-3161-9869-6bf4113c7a0d",
                    "sense_definition": "Stan obniżonego nastroju, smutku, braku energii lub lekkiej depresji.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Od kilku dni mam straszny <w>dołek</w> i nic mnie nie cieszy.",
                            "target_lang_translations": {
                                "ru": "Уже несколько дней у меня жуткая <w>хандра</w>, и ничего меня не радует."
                            }
                        },
                        {
                            "text": "Przyjaciele pomogli mi wyjść z tego psychicznego <w>dołka</w>.",
                            "target_lang_translations": {
                                "ru": "Друзья помогли мне выбраться из этой психологической <w>ямы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przygnębienie",
                        "melancholia",
                        "splin",
                        "kryzys"
                    ],
                    "common_phrases": [
                        "mieć dołek",
                        "wpaść w dołek",
                        "psychiczny dołek"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech to describe mood"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние пониженного настроения, грусти, отсутствия энергии или легкой депрессии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хандра",
                                "target_lang_sense_clarification": "уныние, тоска"
                            },
                            {
                                "target_lang_word": "депрессия",
                                "target_lang_sense_clarification": "подавленное состояние"
                            },
                            {
                                "target_lang_word": "упадок сил",
                                "target_lang_sense_clarification": "отсутствие энергии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa994f85-4c38-3db9-9fe0-495f9f0c0c27",
                    "sense_definition": "Naturalne, drobne wgłębienie na ciele człowieka, na przykład na policzku lub brodzie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_depression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyna uśmiechnęła się szeroko, pokazując urocze <w>dołki</w> w policzkach.",
                            "target_lang_translations": {
                                "ru": "Девушка широко улыбнулась, показывая очаровательные <w>ямочки</w> на щеках."
                            }
                        },
                        {
                            "text": "To małe dziecko ma charakterystyczny <w>dołek</w> w brodzie.",
                            "target_lang_translations": {
                                "ru": "У этого маленького ребенка есть характерная <w>ямочка</w> на подбородке."
                            }
                        }
                    ],
                    "synonyms": [
                        "dołeczek"
                    ],
                    "common_phrases": [
                        "dołki w policzkach",
                        "mieć dołki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественное небольшое углубление на теле человека, например, на щеке или подбородке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ямочка",
                                "target_lang_sense_clarification": "на теле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "560bc71d-e687-34b5-874c-757ca62bb2ef",
                    "sense_definition": "Pomieszczenie w komisariacie policji służące do tymczasowego przetrzymywania zatrzymanych osób.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podejrzany spędził całą noc na <w>dołku</w> w oczekiwaniu na przesłuchanie.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый провел всю ночь в <w>обезьяннике</w> в ожидании допроса."
                            }
                        },
                        {
                            "text": "Policjanci zabrali awanturującego się mężczyznę prosto na <w>dołek</w>.",
                            "target_lang_translations": {
                                "ru": "Полицейские забрали буянившего мужчину прямо в <w>кутузку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "izba zatrzymań",
                        "cela"
                    ],
                    "common_phrases": [
                        "trafić na dołek",
                        "zabrać na dołek",
                        "siedzieć na dołku"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "police and criminal slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещение в полицейском участке, служащее для временного содержания задержанных лиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обезьянник",
                                "target_lang_sense_clarification": "сленг, камера в полиции"
                            },
                            {
                                "target_lang_word": "кутузка",
                                "target_lang_sense_clarification": "разг., место заключения"
                            },
                            {
                                "target_lang_word": "камера",
                                "target_lang_sense_clarification": "изолятор временного содержания"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обеспечивать",
                                "target_lang_sense_clarification": "предоставлять средства"
                            },
                            {
                                "target_lang_word": "устраивать",
                                "target_lang_sense_clarification": "создавать условия"

                {
                    "sense_id": "6280087f-0fe2-37c0-b186-25d5bdd8f53b",
                    "sense_definition": "Martwe ciało człowieka lub zwierzęcia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "corpse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja znalazła <trupa> w opuszczonym budynku na obrzeżach miasta.",
                            "target_lang_translations": {
                                "ru": "Полиция нашла <труп> в заброшенном здании на окраине города."
                            }
                        },
                        {
                            "text": "Nie pozwolę ci tam iść, chyba że po moim <trupie>.",
                            "target_lang_translations": {
                                "ru": "Я не позволю тебе туда пойти, разве что только через мой <труп>."
                            }
                        },
                        {
                            "text": "Wyglądasz jak <trup>, powinieneś wziąć wolne i odpocząć.",
                            "target_lang_translations": {
                                "ru": "Ты выглядишь как <труп>, тебе стоит взять выходной и отдохнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwłoki",
                        "nieboszczyk",
                        "zmarły",
                        "denat",
                        "ciało"
                    ],
                    "common_phrases": [
                        "po moim trupie",
                        "blady jak trup",
                        "trup w szafie",
                        "ścielić się trupem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мёртвое тело человека или животного."
                    },
                    "translations": {

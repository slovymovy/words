{
    "word_family": [
        "tourism",
        "tourist",
        "touring"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b5e71490-903e-3d2b-b042-a647b74f032d",
                    "sense_definition": "A journey through a particular building, estate, country, or other place, usually for pleasure or education.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "journey_visit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went on a guided <w>tour</w> of the historic castle.",
                            "target_lang_translations": {
                                "ru": "Мы отправились на <w>экскурсию</w> с гидом по старинному замку."
                            }
                        },
                        {
                            "text": "The bus <w>tour</w> of the city covers all the major landmarks.",
                            "target_lang_translations": {
                                "ru": "Автобусный <w>тур</w> по городу охватывает все основные достопримечательности."
                            }
                        }
                    ],
                    "synonyms": [
                        "trip",
                        "excursion",
                        "journey",
                        "visit"
                    ],
                    "common_phrases": [
                        "guided tour",
                        "walking tour",
                        "city tour",
                        "go on a tour"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествие по определенному зданию, поместью, стране или другому месту, обычно с целью отдыха или образования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экскурсия",
                                "target_lang_sense_clarification": "посещение достопримечательностей, обычно с гидом"
                            },
                            {
                                "target_lang_word": "тур",
                                "target_lang_sense_clarification": "туристическая поездка по маршруту"
                            },
                            {
                                "target_lang_word": "поездка",
                                "target_lang_sense_clarification": "общее название для путешествия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e23fe18-8cf1-3514-a2fb-993617017b08",
                    "sense_definition": "A planned series of performances, sports matches, or similar events made in different places.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "performance_series",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band is currently on a world <w>tour</w> to promote their new album.",
                            "target_lang_translations": {
                                "ru": "Группа сейчас находится в мировом <w>турне</w> в поддержку своего нового альбома."
                            }
                        },
                        {
                            "text": "Tickets for the comedian's national <w>tour</w> sold out in minutes.",
                            "target_lang_translations": {
                                "ru": "Билеты на национальный <w>тур</w> комика были распроданы за считанные минуты."
                            }
                        }
                    ],
                    "synonyms": [
                        "circuit"
                    ],
                    "common_phrases": [
                        "world tour",
                        "on tour",
                        "concert tour",
                        "book tour"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запланированная серия выступлений, спортивных матчей или подобных мероприятий, проводимых в разных местах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "турне",
                                "target_lang_sense_clarification": "поездка артистов или спортсменов"
                            },
                            {
                                "target_lang_word": "гастроли",
                                "target_lang_sense_clarification": "выступления артистов в поездке"
                            },
                            {
                                "target_lang_word": "тур",
                                "target_lang_sense_clarification": "серия концертов или матчей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd3076e5-3046-3568-a5d1-342204841e3c",
                    "sense_definition": "A period of time spent in military service or on a specific assignment.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "duty_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He served two <w>tours</w> in the army before returning to civilian life.",
                            "target_lang_translations": {
                                "ru": "Он отслужил два <w>срока</w> в армии, прежде чем вернуться к гражданской жизни."
                            }
                        },
                        {
                            "text": "The soldier found his second <w>tour</w> of duty much more difficult than the first.",
                            "target_lang_translations": {
                                "ru": "Солдат счел свою вторую <w>командировку</w> намного более сложной, чем первую."
                            }
                        }
                    ],
                    "synonyms": [
                        "stint",
                        "assignment"
                    ],
                    "common_phrases": [
                        "tour of duty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени, проведенный на военной службе или на конкретном задании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срок службы",
                                "target_lang_sense_clarification": "период пребывания в армии"
                            },
                            {
                                "target_lang_word": "командировка",
                                "target_lang_sense_clarification": "служебная поездка, часто военная"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a2f72e99-10e1-3fce-afc3-93a8be5afc01",
                    "sense_definition": "To travel around a place, visiting different areas or performing in different locations.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They plan to <w>tour</w> Europe by train next summer.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>путешествовать</w> по Европе на поезде следующим летом."
                            }
                        },
                        {
                            "text": "The theater company has <w>toured</w> the country for over a decade.",
                            "target_lang_translations": {
                                "ru": "Театральная труппа <w>гастролировала</w> по стране более десяти лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "travel",
                        "visit",
                        "explore"
                    ],
                    "common_phrases": [
                        "tour the country",
                        "tour the world"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествовать по какому-либо месту, посещая разные районы или выступая в разных локациях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путешествовать",
                                "target_lang_sense_clarification": "ездить по разным местам"
                            },
                            {
                                "target_lang_word": "гастролировать",
                                "target_lang_sense_clarification": "ездить с выступлениями"
                            },
                            {
                                "target_lang_word": "совершать турне",
                                "target_lang_sense_clarification": "ездить по маршруту"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d03b946e-a31b-3d11-9006-ebc717cc5e25",
                    "sense_definition": "The Tour de France, a famous annual bicycle race.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "event_reference",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "Every July, millions of fans watch the <w>Tour</w> on television.",
                            "target_lang_translations": {
                                "ru": "Каждый июль миллионы фанатов смотрят <w>Тур</w> по телевизору."
                            }
                        },
                        {
                            "text": "Winning a stage in the <w>Tour</w> is a dream for many professional cyclists.",
                            "target_lang_translations": {
                                "ru": "Победа на этапе <w>Тура</w> — мечта многих профессиональных велосипедистов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "«Тур де Франс», знаменитая ежегодная велогонка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тур",
                                "target_lang_sense_clarification": "сокращенное название гонки"
                            },
                            {
                                "target_lang_word": "Тур де Франс",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
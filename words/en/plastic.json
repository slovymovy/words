                        },
                        {
                            "text": "I am tired of these <w>plastic</w> celebrities who never show their real selves.",
                            "target_lang_translations": {
                                "ru": "Я устал от этих <w>ненатуральных</w> знаменитостей, которые никогда не показывают свое истинное лицо."
                            }
                        }
                    ],
                    "synonyms": [
                        "fake",
                        "insincere",
                        "ersatz"
                    ],
                    "antonyms": [
                        "genuine",
                        "sincere"
                    ],
                    "common_phrases": [
                        "plastic smile"

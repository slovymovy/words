{
    "word_family": [
        "inhaalslag",
        "inhaalverbod",
        "inhaalmanoeuvre",
        "inhaalrace"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "afaa3675-da5d-3e10-b9e1-45c13da69b8d",
                    "sense_definition": "Iemand of een voertuig dat voor je is voorbijgaan door sneller te bewegen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op deze smalle weg is het gevaarlijk om een vrachtwagen in te halen.",
                            "target_lang_translations": {
                                "ru": "На этой узкой дороге опасно <w>обгонять</w> грузовик."
                            }
                        },
                        {
                            "text": "De snelle auto wilde de fietser <w>inhalen</w>.",
                            "target_lang_translations": {
                                "ru": "Быстрая машина хотела <w>обогнать</w> велосипедиста."
                            }
                        }
                    ],
                    "synonyms": [
                        "passeren",
                        "voorbijrijden",
                        "voorbijgaan"
                    ],
                    "common_phrases": [
                        "rechts inhalen",
                        "een auto inhalen",
                        "iemand inhalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опередить кого-то или транспортное средство, двигаясь быстрее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обгонять",
                                "target_lang_sense_clarification": "транспортное средство или человека"
                            },
                            {
                                "target_lang_word": "опережать",
                                "target_lang_sense_clarification": "двигаться быстрее кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "537f3da1-560b-3bc2-8cd9-5c2ad3e434d3",
                    "sense_definition": "Een achterstand wegwerken om weer op het gewenste niveau te komen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_progress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na mijn vakantie moest ik veel werk <w>inhalen</w>.",
                            "target_lang_translations": {
                                "ru": "После отпуска мне пришлось <w>наверстывать</w> много работы."
                            }
                        },
                        {
                            "text": "In het weekend ga ik wat slaap <w>inhalen</w>.",
                            "target_lang_translations": {
                                "ru": "В выходные я собираюсь <w>наверстать</w> упущенный сон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "schade inhalen",
                        "slaap inhalen",
                        "werk inhalen",
                        "de tijd inhalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ликвидировать отставание, чтобы вернуться на желаемый уровень."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наверстывать",
                                "target_lang_sense_clarification": "упущенное время, работу"
                            },
                            {
                                "target_lang_word": "нагонять",
                                "target_lang_sense_clarification": "ликвидировать отставание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef4ff689-b258-34b7-a806-faca1f14f814",
                    "sense_definition": "Iets naar binnen halen of naar je toe trekken, zoals een net, anker of vlag.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vissers waren druk bezig om de zware netten <w>in te halen</w>.",
                            "target_lang_translations": {
                                "ru": "Рыбаки были заняты тем, что <w>вытаскивали</w> тяжелые сети."
                            }
                        },
                        {
                            "text": "Voor zonsondergang moeten we de vlag <w>inhalen</w>.",
                            "target_lang_translations": {
                                "ru": "До заката мы должны <w>спустить</w> флаг."
                            }
                        }
                    ],
                    "antonyms": [
                        "uitwerpen"
                    ],
                    "common_phrases": [
                        "de netten inhalen",
                        "de vlag inhalen",
                        "het anker inhalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Втягивать внутрь или подтягивать к себе (сети, якорь, флаг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "втягивать",
                                "target_lang_sense_clarification": "тянуть внутрь"
                            },
                            {
                                "target_lang_word": "выбирать",
                                "target_lang_sense_clarification": "сети, якорь (морской термин)"
                            },
                            {
                                "target_lang_word": "спускать",
                                "target_lang_sense_clarification": "флаг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee69f3bd-b0e1-3365-9be7-c1c2ea887619",
                    "sense_definition": "Iemand feestelijk ontvangen bij aankomst, vaak gebruikt voor belangrijke personen of evenementen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hele stad liep uit om de kampioenen feestelijk <w>in te halen</w>.",
                            "target_lang_translations": {
                                "ru": "Весь город вышел, чтобы торжественно <w>встретить</w> чемпионов."
                            }
                        },
                        {
                            "text": "Elk jaar in november zullen we Sinterklaas weer <w>inhalen</w>.",
                            "target_lang_translations": {
                                "ru": "Каждый год в ноябре мы снова будем <w>встречать</w> Синтаклааса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de Sint inhalen",
                        "de kampioen inhalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественно встречать кого-либо по прибытии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречать",
                                "target_lang_sense_clarification": "торжественный прием"
                            },
                            {
                                "target_lang_word": "чествовать",
                                "target_lang_sense_clarification": "приветствовать героя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "437cd8b1-5590-3084-9a62-063ca967d35b",
                    "sense_definition": "Przybyć w określone miejsce w wyznaczonym czasie, często oficjalnie lub na wezwanie (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <w>stawić</w> się w biurze jutro rano punktualnie o ósmej.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>явиться</w> в офис завтра утром ровно в восемь."
                            }
                        },
                        {
                            "text": "Oskarżony nie <w>stawił</w> się na rozprawie sądowej z powodu choroby.",
                            "target_lang_translations": {
                                "ru": "Обвиняемый не <w>явился</w> на судебное заседание из-за болезни."
                            }
                        }
                    ],
                    "synonyms": [
                        "zjawić się",
                        "stawiać"
                    ],
                    "common_phrases": [
                        "stawić się w sądzie",
                        "stawić się na wezwanie",
                        "stawić się do pracy",
                        "stawić się osobiście"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибыть в определенное место в назначенное время, часто официально или по вызову (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "явиться",
                                "target_lang_sense_clarification": "прийти официально (в суд, на службу)"
                            },
                            {
                                "target_lang_word": "предстать",
                                "target_lang_sense_clarification": "появиться перед кем-то (напр. перед судом)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10ae3a77-f811-3b59-933d-7facfcfda99e",
                    "sense_definition": "Wykazać sprzeciw, odwagę lub podjąć walkę wobec kogoś lub czegoś (występuje głównie w stałych zwrotach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "confrontation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze postanowili <w>stawić</w> zacięty opór przeważającym siłom wroga.",
                            "target_lang_translations": {
                                "ru": "Солдаты решили <w>оказать</w> ожесточенное сопротивление превосходящим силам врага."
                            }
                        },
                        {
                            "text": "Musimy wspólnie <w>stawić</w> czoła nadchodzącym problemom ekonomicznym.",
                            "target_lang_translations": {
                                "ru": "Мы должны сообща <w>противостоять</w> грядущим экономическим проблемам."
                            }
                        }
                    ],
                    "synonyms": [
                        "zjawić się",
                        "stawiać"
                    ],
                    "common_phrases": [
                        "stawić opór",
                        "stawić czoła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявить сопротивление, смелость или вступить в борьбу с кем-то или чем-то (встречается в основном в устойчивых выражениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оказать",
                                "target_lang_sense_clarification": "в сочетании «оказать сопротивление» (stawić opór)"
                            },
                            {
                                "target_lang_word": "противостоять",
                                "target_lang_sense_clarification": "в значении «смело встретить» (stawić czoła)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d80c269f-8017-3015-bf2f-14b0339c13e9",
                    "sense_definition": "Umieścić coś w pozycji stojącej lub zbudować coś (obecnie rzadkie, w tym znaczeniu częściej używa się słowa „postawić”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych czasach zwykło się <w>stawić</w> straże przy bramach miasta.",
                            "target_lang_translations": {
                                "ru": "В давние времена было принято <w>ставить</w> стражу у городских ворот."
                            }
                        },
                        {
                            "text": "Gospodarz kazał <w>stawić</w> przed gośćmi najlepsze wino i potrawy.",
                            "target_lang_translations": {
                                "ru": "Хозяин приказал <w>поставить</w> перед гостями лучшее вино и блюда."
                            }
                        }
                    ],
                    "synonyms": [
                        "stawiać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить что-либо в стоячее положение или построить что-либо (сейчас редко, в этом значении чаще используется слово «postawić»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставить",
                                "target_lang_sense_clarification": "помещать, располагать (устаревшее или редкое)"
                            },
                            {
                                "target_lang_word": "выставить",
                                "target_lang_sense_clarification": "разместить (например, охрану или угощение)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
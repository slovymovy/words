            "senses": [
                {
                    "sense_id": "7b2384f8-797e-3f8f-93fc-94b40ba1337f",
                    "sense_definition": "Bezbarwna, bezwonna ciecz niezbędna do życia wszystkich organizmów; chemicznie tlenek wodoru.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano wypijam szklankę ciepłej <w>wody</w> z cytryną.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я выпиваю стакан тёплой <w>воды</w> с лимоном."
                            }
                        },
                        {
                            "text": "W tym regionie <w>woda</w> w kranie jest czysta i zdatna do picia.",
                            "target_lang_translations": {
                                "ru": "В этом регионе <w>вода</w> из-под крана чистая и пригодна для питья."
                            }
                        }
                    ],
                    "synonyms": [
                        "tlenek wodoru",
                        "H₂O",
                        "monotlenek diwodoru"
                    ],
                    "antonyms": [
                        "ogień"
                    ],
                    "common_phrases": [
                        "woda mineralna",
                        "woda gazowana",
                        "woda pitna",
                        "szklanka wody",
                        "bieżąca woda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бесцветная жидкость без запаха, необходимая для жизни всех организмов; химически оксид водорода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вода",
                                "target_lang_sense_clarification": "жидкость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "765d2acd-1851-3043-8f13-86d3e2660d7e",
                    "sense_definition": "Naturalny lub sztuczny zbiornik wodny, na przykład morze, jezioro lub rzeka.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wakacje najchętniej wypoczywamy całą rodziną nad <w>wodą</w>.",
                            "target_lang_translations": {
                                "ru": "В каникулы мы всей семьёй предпочитаем отдыхать у <w>воды</w>."
                            }
                        },
                        {
                            "text": "Te <w>wody</w> są pełne rzadkich gatunków ryb i roślin.",
                            "target_lang_translations": {
                                "ru": "Эти <w>воды</w> полны редких видов рыб и растений."
                            }
                        }
                    ],
                    "synonyms": [
                        "akwen",
                        "zbiornik wodny"
                    ],
                    "common_phrases": [
                        "nad wodą",
                        "wody terytorialne",
                        "wody śródlądowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественный или искусственный водоём, например, море, озеро или река."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вода",
                                "target_lang_sense_clarification": "водоём"
                            },
                            {
                                "target_lang_word": "воды",
                                "target_lang_sense_clarification": "водное пространство"
                            }

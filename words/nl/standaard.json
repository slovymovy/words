                        "ru": [
                            {
                                "target_lang_word": "по умолчанию",
                                "target_lang_sense_clarification": "автоматически"
                            },
                            {
                                "target_lang_word": "стандартно",
                                "target_lang_sense_clarification": "обычно"
                            },
                            {
                                "target_lang_word": "всегда",

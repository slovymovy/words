            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6412d7a8-aec3-33cd-b81b-b1bd7cffb187",
                    "sense_definition": "Zdjąć lub poluzować element gwintowany (np. śrubę, nakrętkę, pokrywkę) poprzez kręcenie nim w odpowiednią stronę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_rotation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <odkręcić> ten słoik, ale wieczka nie da się ruszyć.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <открыть> эту банку, но крышка не поддается."
                            }
                        },
                        {
                            "text": "Mechanik pomógł mi <odkręcić> koło w samochodzie, bo śruby były zapieczone.",
                            "target_lang_translations": {
                                "ru": "Механик помог мне <открутить> колесо на машине, потому что болты прикипели."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykręcić się",
                        "otworzyć się"
                    ],
                    "antonyms": [
                        "dokręcić",
                        "przykręcić"
                    ],
                    "common_phrases": [
                        "odkręcić słoik",
                        "odkręcić śrubę",
                        "odkręcić butelkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снять или ослабить резьбовой элемент (например, винт, гайку, крышку), вращая его в соответствующую сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открутить",
                                "target_lang_sense_clarification": "общий термин для снятия чего-либо вращением"
                            },
                            {
                                "target_lang_word": "отвинтить",
                                "target_lang_sense_clarification": "касательно винтов и болтов"
                            },
                            {
                                "target_lang_word": "открыть",
                                "target_lang_sense_clarification": "касательно банок или бутылок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1cf3e11-5541-3174-be0f-d7c45227a311",
                    "sense_definition": "Otworzyć dopływ czegoś (np. wody, gazu, ciepła) poprzez przekręcenie zaworu lub kurka.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_rotation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <odkręcić> kran, żeby sprawdzić ciśnienie wody w rurach.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <откройте> кран, чтобы проверить давление воды в трубах."
                            }
                        },
                        {
                            "text": "Zrobiło się zimno, więc muszę <odkręcić> kaloryfer w sypialni.",
                            "target_lang_translations": {
                                "ru": "Стало холодно, поэтому мне нужно <включить> батарею в спальне."
                            }
                        }
                    ],
                    "synonyms": [
                        "otworzyć się",
                        "obrócić"
                    ],
                    "antonyms": [
                        "przykręcić"
                    ],
                    "common_phrases": [

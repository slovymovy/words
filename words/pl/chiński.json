    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2375130c-5f43-355f-a1e3-33c8facf87d3",
                    "sense_definition": "związany z Chinami (państwem), ich mieszkańcami lub kulturą",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "country_rel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielki Mur <Chiński> jest jedną z największych atrakcji turystycznych świata.",
                            "target_lang_translations": {
                                "ru": "Великая <Китайская> стена является одной из величайших туристических достопримечательностей мира."
                            }
                        },
                        {
                            "text": "Wczoraj na obiad jedliśmy ostrą <chińską> zupę z makaronem.",
                            "target_lang_translations": {
                                "ru": "Вчера на обед мы ели острый <китайский> суп с лапшой."
                            }
                        }
                    ],
                    "synonyms": [
                        "kitajski"
                    ],
                    "antonyms": [
                        "antychiński"
                    ],
                    "common_phrases": [
                        "Mur Chiński",
                        "kuchnia chińska",
                        "chiński smok",
                        "chiński zodiak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с Китаем (государством), его жителями или культурой"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китайский",
                                "target_lang_sense_clarification": "о стране, культуре, людях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7cda8ba-8553-383f-a7c3-5d2c081e964e",
                    "sense_definition": "związany z językiem używanym w Chinach",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "lang_rel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znaki <chińskie> są bardzo skomplikowane dla początkujących uczniów.",
                            "target_lang_translations": {
                                "ru": "<Китайские> иероглифы очень сложны для начинающих учеников."
                            }
                        },
                        {
                            "text": "Kupiłem nowy słownik <chiński>, aby poszerzyć swoje słownictwo.",
                            "target_lang_translations": {
                                "ru": "Я купил новый <китайский> словарь, чтобы расширить свой словарный запас."
                            }
                        }
                    ],
                    "common_phrases": [
                        "znaki chińskie",
                        "słownik chiński"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с языком, используемым в Китае"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китайский",
                                "target_lang_sense_clarification": "о языке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3891dbf-6002-304a-9af3-721f52ffe12b",
                    "sense_definition": "język urzędowy w Chinach; grupa dialektów używanych przez Chińczyków",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "lang_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Leon nauczył się <chińskiego> na studiach i teraz tam pracuje.",
                            "target_lang_translations": {
                                "ru": "Леон выучил <китайский> в университете и теперь там работает."
                            }
                        },
                        {
                            "text": "Uważam, że <chiński> jest trudniejszy do nauki niż angielski.",
                            "target_lang_translations": {
                                "ru": "Я считаю, что <китайский> труднее для изучения, чем английский."
                            }
                        }
                    ],
                    "synonyms": [
                        "chińszczyzna"
                    ],
                    "common_phrases": [
                        "uczyć się chińskiego",
                        "znać chiński",
                        "język chiński"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный язык Китая; группа диалектов, на которых говорят китайцы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китайский язык",
                                "target_lang_sense_clarification": "язык"
                            },
                            {
                                "target_lang_word": "китайский",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

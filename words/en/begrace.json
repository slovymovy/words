    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7db9d881-3035-35d3-b7b2-f67caa70048e",
                    "sense_definition": "To bestow grace upon someone; to endow or gift with a quality or blessing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist was <w>begraced</w> with an extraordinary talent for painting landscapes.",
                            "target_lang_translations": {
                                "ru": "Художник был <w>одарён</w> необычайным талантом к написанию пейзажей."
                            }
                        },
                        {
                            "text": "They believed the sacred ceremony would <w>begrace</w> the participants with divine favor.",
                            "target_lang_translations": {
                                "ru": "Они верили, что священная церемония <w>одарит</w> участников божественной милостью."
                            }
                        }
                    ],
                    "synonyms": [
                        "endow",
                        "bless",
                        "grace"
                    ],
                    "common_phrases": [
                        "begraced with"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одарить благодатью; наделить каким-либо качеством или даром."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одарить",
                                "target_lang_sense_clarification": "наделить качеством или талантом"
                            },
                            {
                                "target_lang_word": "наделить",
                                "target_lang_sense_clarification": "дать что-либо в качестве дара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0e4c925-dbb1-3a48-85d2-c53cbe2597a7",
                    "sense_definition": "To address a person using the title \"Grace\".",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nervous servant did not know how to properly <w>begrace</w> the visiting duke.",
                            "target_lang_translations": {
                                "ru": "Нервный слуга не знал, как правильно <w>величать</w> прибывшего герцога."
                            }
                        },
                        {
                            "text": "It was customary to <w>begrace</w> the archbishop respectfully when he entered the room.",
                            "target_lang_translations": {
                                "ru": "Было принято почтительно <w>титуловать</w> архиепископа, когда он входил в комнату."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться к человеку, используя титул «Ваша Светлость» или «Ваша Милость»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "величать",
                                "target_lang_sense_clarification": "называть по титулу"
                            },
                            {
                                "target_lang_word": "титуловать",
                                "target_lang_sense_clarification": "использовать титул при обращении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "ondergang"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1f52401a-2cb2-37a4-a4fe-afa8abb77465",
                    "sense_definition": "Achter de horizon verdwijnen (van hemellichamen zoals de zon of maan).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verdwijnen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer <w>gaat</w> de zon pas laat <w>onder</w>.",
                            "target_lang_translations": {
                                "en": "In the summer, the sun <w>sets</w> late."
                            }
                        },
                        {
                            "text": "We keken samen naar de prachtige <w>ondergaande</w> zon.",
                            "target_lang_translations": {
                                "en": "We watched the beautiful <w>setting</w> sun together."
                            }
                        }
                    ],
                    "antonyms": [
                        "opkomen"
                    ],
                    "common_phrases": [
                        "zon gaat onder"
                    ],
                    "target_lang_definitions": {
                        "en": "To disappear behind the horizon (of celestial bodies like the sun or moon)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "of the sun or moon"
                            },
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "disappear below the horizon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e7e1539-e2c7-35ff-99ba-62b5606c8a56",
                    "sense_definition": "Een handeling, behandeling of verandering onderworpen worden of meemaken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ervaren",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader <w>onderging</w> gisteren een zware operatie in het ziekenhuis.",
                            "target_lang_translations": {
                                "en": "My father <w>underwent</w> a major surgery in the hospital yesterday."
                            }
                        },
                        {
                            "text": "Het gebouw heeft de laatste jaren een grote verandering <w>ondergaan</w>.",
                            "target_lang_translations": {
                                "en": "The building has <w>undergone</w> a major change in recent years."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdragen",
                        "uithouden",
                        "uitstaan",
                        "velen",
                        "harden"
                    ],
                    "common_phrases": [
                        "operatie ondergaan",
                        "verandering ondergaan",
                        "test ondergaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To be subjected to or experience an action, treatment, or change."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "undergo",
                                "target_lang_sense_clarification": "experience a change or process"
                            },
                            {
                                "target_lang_word": "experience",
                                "target_lang_sense_clarification": "go through"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5f67bb9-82de-31f8-b4ff-784e2ee49fed",
                    "sense_definition": "Onder het wateroppervlak verdwijnen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verdwijnen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De duiker <w>ging</w> snel <w>onder</w> om het koraal te bekijken.",
                            "target_lang_translations": {
                                "en": "The diver quickly <w>went under</w> to look at the coral."
                            }
                        },
                        {
                            "text": "Het kleine bootje is tijdens de storm volledig <w>ondergegaan</w>.",
                            "target_lang_translations": {
                                "en": "The small boat completely <w>went under</w> during the storm."
                            }
                        }
                    ],
                    "synonyms": [
                        "harden",
                        "uithouden",
                        "uitstaan",
                        "velen",
                        "verdragen"
                    ],
                    "antonyms": [
                        "bovenkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To disappear below the surface of the water."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go under",
                                "target_lang_sense_clarification": "submerge"
                            },
                            {
                                "target_lang_word": "sink",
                                "target_lang_sense_clarification": "descend to the bottom"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2687c35f-a5a7-3530-a358-6a4615f863d5",
                    "sense_definition": "Iets vervelends of zwaars verdragen en doorstaan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "ervaren",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil die verschrikkelijke herrie van de buren niet meer <w>ondergaan</w>.",
                            "target_lang_translations": {
                                "en": "I don't want to <w>endure</w> that terrible noise from the neighbors anymore."
                            }
                        },
                        {
                            "text": "De gevangenen moesten hun straf in stilte <w>ondergaan</w>.",
                            "target_lang_translations": {
                                "en": "The prisoners had to <w>undergo</w> their punishment in silence."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdragen",
                        "uithouden",
                        "uitstaan",
                        "velen"
                    ],
                    "common_phrases": [
                        "straf ondergaan",
                        "lot ondergaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To endure and withstand something unpleasant or difficult."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "endure",
                                "target_lang_sense_clarification": "bear patiently"
                            },
                            {
                                "target_lang_word": "suffer",
                                "target_lang_sense_clarification": "undergo pain or distress"
                            },
                            {
                                "target_lang_word": "undergo",
                                "target_lang_sense_clarification": "be subjected to something unpleasant"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c457d041-36a3-3987-b23f-315a6467419f",
                    "sense_definition": "Ophouden te bestaan; vernietigd worden of te gronde gaan.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verdwijnen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vele oude beschavingen zijn door oorlogen en rampen <w>ondergegaan</w>.",
                            "target_lang_translations": {
                                "en": "Many ancient civilizations <w>perished</w> due to wars and disasters."
                            }
                        },
                        {
                            "text": "Als er geen hulp komt, zal het bedrijf zeker <w>ondergaan</w>.",
                            "target_lang_translations": {
                                "en": "If no help comes, the company will certainly <w>go under</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdragen",
                        "uithouden",
                        "uitstaan",
                        "velen",
                        "harden"
                    ],
                    "common_phrases": [
                        "ten onder gaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To cease to exist; to be destroyed or go to ruin."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "perish",
                                "target_lang_sense_clarification": "cease to exist or die"
                            },
                            {
                                "target_lang_word": "go under",
                                "target_lang_sense_clarification": "fail or collapse (e.g. a business)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
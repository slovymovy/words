            "senses": [
                {
                    "sense_id": "8d652ed5-5802-3d1d-ad7c-42d0761eaeff",
                    "sense_definition": "Zout toevoegen aan voedsel om de smaak te verbeteren.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cooking_flavor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet het water eerst <w>zouten</w> voordat je de pasta toevoegt.",
                            "target_lang_translations": {
                                "ru": "Нужно сначала <w>посолить</w> воду, прежде чем добавлять макароны."
                            }
                        },
                        {
                            "text": "De kok <w>zoutte</w> de soep een beetje bij omdat deze te flauw was.",
                            "target_lang_translations": {
                                "ru": "Повар немного <w>подсолил</w> суп, так как он был слишком пресным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aardappels zouten",
                        "licht zouten",
                        "het eten zouten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавлять соль в пищу для улучшения вкуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солить",
                                "target_lang_sense_clarification": "добавлять соль"
                            },
                            {
                                "target_lang_word": "посолить",
                                "target_lang_sense_clarification": "сделать соленым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b7d2bd5-a61f-390c-a1ea-525d041a7de4",
                    "sense_definition": "Voedsel conserveren door het met zout te behandelen of in te leggen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cooking_preserve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Haring moet licht <w>gezouten</w> worden om een parasiet te doden.",
                            "target_lang_translations": {
                                "ru": "Сельдь должна быть слегка <w>просолена</w>, чтобы убить паразитов."
                            }
                        },
                        {
                            "text": "Vroeger <w>zoutten</w> mensen vlees en vis om het de hele winter te kunnen bewaren.",
                            "target_lang_translations": {
                                "ru": "Раньше люди <w>солили</w> мясо и рыбу, чтобы хранить их всю зиму."
                            }
                        }
                    ],
                    "synonyms": [
                        "pekelen",
                        "inleggen"
                    ],
                    "common_phrases": [
                        "vis zouten",
                        "vlees zouten",
                        "haring zouten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Консервировать продукты, обрабатывая их солью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засаливать",
                                "target_lang_sense_clarification": "консервировать"
                            },
                            {
                                "target_lang_word": "солить",
                                "target_lang_sense_clarification": "заготавливать впрок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4918ba40-1185-345b-90c0-f56d5badfbd4",
                    "sense_definition": "De meervoudsvorm van <w>zout</w>; chemische verbindingen die bestaan uit ionen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>zouten</w> van natrium en kalium zijn meestal goed oplosbaar.",
                            "target_lang_translations": {
                                "ru": "<w>Соли</w> натрия и калия обычно хорошо растворимы."
                            }
                        },
                        {
                            "text": "Tijdens de scheikundeles leerden we hoe verschillende <w>zouten</w> reageren met water.",
                            "target_lang_translations": {
                                "ru": "На уроке химии мы узнали, как различные <w>соли</w> реагируют с водой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "opgeloste zouten",
                        "anorganische zouten"
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ca07178-9bfb-3392-87b6-288626f755b9",
                    "sense_definition": "Urządzenie sanitarne w kształcie misy z odpływem, zwykle montowane w kuchni, służące do mycia naczyń i produktów spożywczych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sanitary_fixture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Włóż brudne talerze i kubki do <w>zlewu</w>.",
                            "target_lang_translations": {
                                "ru": "Положи грязные тарелки и чашки в <w>раковину</w>."
                            }
                        },
                        {
                            "text": "Muszę naprawić cieknący kran nad <w>zlewem</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно починить протекающий кран над <w>раковиной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zlewozmywak"
                    ],
                    "common_phrases": [
                        "zlew kuchenny",
                        "pełny zlew",
                        "włożyć do zlewu",
                        "zatkany zlew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Санитарное приспособление в форме чаши со сливом, обычно устанавливаемое на кухне, служащее для мытья посуды и продуктов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раковина",
                                "target_lang_sense_clarification": "общее название"
                            },
                            {
                                "target_lang_word": "мойка",
                                "target_lang_sense_clarification": "кухонная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eff98c45-8f6a-38da-a8b0-263d0c4649a3",
                    "sense_definition": "Forma gramatyczna (dopełniacz liczby mnogiej) rzeczownika <w>zlewa</w> (oznaczającego ulewny deszcz).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po tygodniu ciągłych <w>zlew</w> rzeka wylała z koryta.",
                            "target_lang_translations": {
                                "ru": "После недели непрерывных <w>ливней</w> река вышла из берегов."
                            }
                        },
                        {
                            "text": "Nie spodziewaliśmy się aż tak gwałtownych <w>zlew</w> w lipcu.",
                            "target_lang_translations": {
                                "ru": "Мы не ожидали таких сильных <w>ливней</w> в июле."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural of zlewa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (родительный падеж множественного числа) существительного <w>zlewa</w> (означающего ливень)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ливней",
                                "target_lang_sense_clarification": "форма слова"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "180ce4e4-22d0-3b83-8351-5a17d9e72958",
                    "sense_definition": "Sytuacja bardzo zabawna, powód do śmiechu lub drwiny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "amusement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ale był <w>zlew</w>, gdy opowiedział nam tę historię.",
                            "target_lang_translations": {
                                "ru": "Ну и <w>ржака</w> была, когда он рассказал нам эту историю."
                            }
                        },
                        {
                            "text": "Wszyscy mieli niezły <w>zlew</w> z jego nowego pomysłu.",
                            "target_lang_translations": {
                                "ru": "Все устроили неплохой <w>стёб</w> над его новой идеей."
                            }
                        }
                    ],
                    "synonyms": [
                        "beka",
                        "polewka",
                        "ubaw",
                        "zwała",
                        "brecha"
                    ],
                    "common_phrases": [
                        "mieć zlew",
                        "niezły zlew"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "youth slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень смешная ситуация, повод для смеха или насмешки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ржака",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "умора",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "стёб",
                                "target_lang_sense_clarification": "насмешка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6faf85b2-c1dc-3c23-89f8-3424cc7dcb05",
                    "sense_definition": "Miejsce, w którym gromadzą się spływające ciecze, lub sam proces ich spływania.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "liquid_accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym obniżeniu terenu znajduje się naturalny <w>zlew</w> wód opadowych.",
                            "target_lang_translations": {
                                "ru": "В этом понижении местности находится естественный <w>сток</w> дождевых вод."
                            }
                        },
                        {
                            "text": "Zanieczyszczenia trafiają do głównego <w>zlewu</w> ścieków.",
                            "target_lang_translations": {
                                "ru": "Загрязнения попадают в главный <w>слив</w> сточных вод."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где скапливаются стекающие жидкости, или сам процесс их стекания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сток",
                                "target_lang_sense_clarification": "место сбора воды"
                            },
                            {
                                "target_lang_word": "слив",
                                "target_lang_sense_clarification": "отверстие или процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05adf365-9cc6-365a-92f0-c7e51485abba",
                    "sense_definition": "Zawodowy podoficer w wojsku.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military_personnel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <w>zlew</w> głośno wydawał rozkazy na placu apelowym.",
                            "target_lang_translations": {
                                "ru": "Старый <w>кусок</w> громко отдавал приказы на плацу."
                            }
                        },
                        {
                            "text": "Młodzi żołnierze starali się nie podpaść temu <w>zlewowi</w>.",
                            "target_lang_translations": {
                                "ru": "Молодые солдаты старались не попасться этому <w>куску</w>."
                            }
                        }

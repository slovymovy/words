    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7901dba7-840e-3209-991b-80b6aef5f247",
                    "sense_definition": "Stawać się widocznym, powoli ukazywać się oczom, często wychodząc zza przeszkody, z ciemności lub z oddali.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z gęstej mgły powoli <w>wyłaniał</w> się zarys starego zamku.",
                            "target_lang_translations": {
                                "ru": "Из густого тумана медленно <w>выплывал</w> силуэт старого замка."
                            }
                        },
                        {
                            "text": "Księżyc zaczął <w>wyłaniać</w> się zza ciemnych chmur po burzy.",
                            "target_lang_translations": {
                                "ru": "Луна начала <w>выглядывать</w> из-за темных туч после грозы."
                            }
                        }
                    ],
                    "synonyms": [
                        "pojawiać się",
                        "wyzierać"
                    ],
                    "common_phrases": [
                        "wyłaniać się z mgły",
                        "wyłaniać się z cienia",
                        "wyłaniać się z mroku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться видимым, показываться из-за преграды, из темноты или издалека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "появляться",
                                "target_lang_sense_clarification": "становиться видимым"
                            },
                            {
                                "target_lang_word": "выплывать",
                                "target_lang_sense_clarification": "плавно появляться (из тумана, темноты)"
                            },
                            {
                                "target_lang_word": "вырисовываться",
                                "target_lang_sense_clarification": "становиться различимым по очертаниям"
                            },
                            {
                                "target_lang_word": "показываться",
                                "target_lang_sense_clarification": "выходить на видное место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "571f2de6-c6e6-30f0-9d51-f8238fa59d71",
                    "sense_definition": "Dokonywać wyboru kogoś lub czegoś z większej grupy, decydować o wyniku selekcji.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Komisja konkursowa właśnie <w>wyłania</w> najlepszego projektanta mody.",

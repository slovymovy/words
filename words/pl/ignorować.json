                {
                    "sense_id": "d19550b7-13f3-375e-b348-efbc3906039d",
                    "sense_definition": "celowo nie zwracać na kogoś lub na coś uwagi; udawać, że się kogoś nie widzi lub nie słyszy",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ignoring_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca nie powinien <ignorować> znaków drogowych ustawionych przy ulicy.",
                            "target_lang_translations": {
                                "ru": "Водитель не должен <игнорировать> дорожные знаки, установленные на улице."
                            }
                        },
                        {
                            "text": "Mój brat postanowił <ignorować> moje dobre rady i zrobił po swojemu.",
                            "target_lang_translations": {
                                "ru": "Мой брат решил <игнорировать> мои добрые советы и сделал по-своему."
                            }
                        },
                        {
                            "text": "Nie możesz ciągle <ignorować> problemu, bo on sam nie zniknie.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь постоянно <игнорировать> проблему, потому что она сама не исчезнет."
                            }
                        }
                    ],
                    "synonyms": [
                        "lekceważyć",
                        "bagatelizować",
                        "traktować jak powietrze",
                        "olewać"
                    ],
                    "antonyms": [
                        "interesować się"
                    ],
                    "common_phrases": [
                        "ignorować ostrzeżenia",
                        "ignorować fakty",
                        "ignorować przepisy",
                        "ignorować zakazy"
                    ],
                    "target_lang_definitions": {
                        "ru": "намеренно не обращать внимания на кого-либо или что-либо; делать вид, что не видишь или не слышишь кого-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игнорировать",
                                "target_lang_sense_clarification": "умышленно не замечать"
                            },
                            {
                                "target_lang_word": "не замечать",
                                "target_lang_sense_clarification": "делать вид, что не видишь"
                            },
                            {
                                "target_lang_word": "пренебрегать",
                                "target_lang_sense_clarification": "относиться без внимания, не учитывать"

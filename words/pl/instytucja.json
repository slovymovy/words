            "pos": "noun",
            "senses": [
                {
                    "sense_id": "da2af03b-9bbf-30a5-9251-d9102aa3c20f",
                    "sense_definition": "Zakład o charakterze publicznym zajmujący się określonym zakresem spraw, np. administracyjnych, kulturalnych lub społecznych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "organization_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każda <w>instytucja</w> państwowa musi działać zgodnie z obowiązującym prawem.",
                            "target_lang_translations": {
                                "ru": "Каждое государственное <w>учреждение</w> должно действовать в соответствии с действующим законом."
                            }
                        },
                        {
                            "text": "Bank to <w>instytucja</w> finansowa, która cieszy się dużym zaufaniem klientów.",
                            "target_lang_translations": {
                                "ru": "Банк — это финансовое <w>учреждение</w>, которое пользуется большим доверием клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "organizacja"
                    ],
                    "common_phrases": [
                        "instytucja państwowa",
                        "instytucja kultury",
                        "instytucja finansowa",
                        "instytucja zaufania publicznego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государственное или общественное заведение, занимающееся определенной сферой деятельности (административной, культурной или социальной)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учреждение",
                                "target_lang_sense_clarification": "государственное или общественное"
                            },
                            {
                                "target_lang_word": "организация",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfae52ab-a405-3332-aab3-f779709b9d47",
                    "sense_definition": "Trwały zespół norm prawnych lub obyczajowych regulujących ważną dziedzinę życia społecznego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Współczesne zmiany społeczne wpływają na tradycyjną <w>instytucję</w> rodziny.",
                            "target_lang_translations": {
                                "ru": "Современные социальные изменения влияют на традиционный <w>институт</w> семьи."
                            }
                        },
                        {
                            "text": "W prawie cywilnym istnieje <w>instytucja</w> przedawnienia roszczeń.",
                            "target_lang_translations": {
                                "ru": "В гражданском праве существует <w>институт</w> исковой давности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "instytucja małżeństwa",
                        "instytucja rodziny",
                        "instytucja prawna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность правовых или социальных норм, регулирующих важную область общественной жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "институт",
                                "target_lang_sense_clarification": "социальный или правовой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd471ec4-3ae4-3f62-9468-314b726c270f",
                    "sense_definition": "Osoba lub funkcja społeczna pełniąca ważną, stałą i powszechnie uznaną rolę w danej społeczności.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten starszy portier to prawdziwa <w>instytucja</w> w naszym biurowcu.",
                            "target_lang_translations": {
                                "ru": "Этот пожилой швейцар — настоящее <w>учреждение</w> в нашем офисном здании."
                            }
                        },
                        {
                            "text": "W małych miasteczkach listonosz jest często traktowany jak lokalna <w>instytucja</w>.",
                            "target_lang_translations": {
                                "ru": "В маленьких городках к почтальону часто относятся как к местной <w>достопримечательности</w>."
                            }

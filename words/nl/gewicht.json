            "senses": [
                {
                    "sense_id": "6d1dcc4e-b285-3d0b-a232-58f47bfc43e2",
                    "sense_definition": "De zwaarte van een persoon of voorwerp; de kracht waarmee iets op de ondergrond drukt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn <w>gewicht</w> is de laatste maanden stabiel gebleven.",
                            "target_lang_translations": {
                                "en": "My <w>weight</w> has remained stable over the last few months."
                            }
                        },
                        {
                            "text": "Het totale <w>gewicht</w> van de koffer mag niet meer dan twintig kilo zijn.",
                            "target_lang_translations": {
                                "en": "The total <w>weight</w> of the suitcase must not exceed twenty kilos."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwaarte"
                    ],
                    "common_phrases": [
                        "op gewicht blijven",
                        "gewicht verliezen",
                        "in de schaal leggen"
                    ],
                    "target_lang_definitions": {
                        "en": "The heaviness of a person or object; the force with which something presses on a surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weight",
                                "target_lang_sense_clarification": "heaviness or mass"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fe95a3e-e01d-3a40-ad16-432a5714e0f5",
                    "sense_definition": "Een voorwerp met een vastgestelde zwaarte, vaak gebruikt bij sport of om te wegen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de sportschool traint zij elke dag met zware <w>gewichten</w>.",
                            "target_lang_translations": {
                                "en": "In the gym, she trains with heavy <w>weights</w> every day."
                            }
                        },
                        {
                            "text": "De oude weegschaal werkt nog met losse koperen <w>gewichten</w>.",
                            "target_lang_translations": {
                                "en": "The old scale still works with loose copper <w>weights</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gewichten heffen"
                    ],
                    "target_lang_definitions": {
                        "en": "An object with a fixed heaviness, often used in sports or for weighing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weight",
                                "target_lang_sense_clarification": "object used for exercise or weighing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4a9c5bf-4425-34d5-a8f3-52c95ca6a209",
                    "sense_definition": "Het gewei van een hert of ree.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jager bewonderde het indrukwekkende <w>gewicht</w> van het hert.",
                            "target_lang_translations": {
                                "en": "The hunter admired the deer's impressive <w>antlers</w>."
                            }
                        },
                        {
                            "text": "Aan de muur van de hut hing een oud <w>gewicht</w> als trofee.",
                            "target_lang_translations": {
                                "en": "An old set of <w>antlers</w> hung on the wall of the cabin as a trophy."
                            }
                        }
                    ],
                    "synonyms": [
                        "gewei"
                    ],
                    "target_lang_definitions": {
                        "en": "The antlers of a deer or roe deer."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "antlers",
                                "target_lang_sense_clarification": "horns of a deer"
                            }

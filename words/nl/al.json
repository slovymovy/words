    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ccf60fce-24e0-328b-98d7-8b13deb7ac25",
                    "sense_definition": "Aanduiding dat iets eerder heeft plaatsgevonden dan verwacht of op dit moment reeds een feit is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_already",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <w>al</w> negen uur geweest.",
                            "target_lang_translations": {
                                "ru": "Было <w>уже</w> девять часов.",
                                "en": "It is <w>already</w> past nine o'clock."
                            }
                        },
                        {
                            "text": "Ik heb mijn huiswerk <w>al</w> afgemaakt.",
                            "target_lang_translations": {
                                "ru": "Я <w>уже</w> сделал домашнее задание.",
                                "en": "I have <w>already</w> finished my homework."
                            }
                        }
                    ],
                    "synonyms": [
                        "wel",
                        "ook al"
                    ],
                    "antonyms": [
                        "geen"
                    ],
                    "common_phrases": [
                        "nu al",
                        "al lang"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указание на то, что что-то произошло раньше, чем ожидалось, или уже является фактом.",
                        "en": "Indication that something happened earlier than expected or is already a fact."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уже",
                                "target_lang_sense_clarification": "наречие времени"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "already",
                                "target_lang_sense_clarification": "adverb of time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6958d59-0eec-32b3-9d09-eb20972320f9",
                    "sense_definition": "Gebruikt in vragen om te informeren of iets inmiddels gebeurd is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_yet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb je <w>al</w> een keuze gemaakt?",
                            "target_lang_translations": {
                                "ru": "Ты <w>уже</w> сделал выбор?",
                                "en": "Have you made a choice <w>yet</w>?"
                            }
                        },
                        {
                            "text": "Is de postbode <w>al</w> langs geweest?",
                            "target_lang_translations": {
                                "ru": "Почтальон <w>уже</w> приходил?",
                                "en": "Has the mailman come <w>yet</w>?"
                            }
                        }
                    ],
                    "antonyms": [
                        "geen"
                    ],
                    "common_phrases": [
                        "heb je al",
                        "ben je al"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в вопросах, чтобы узнать, произошло ли что-то к данному моменту.",
                        "en": "Used in questions to inquire if something has happened by now."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уже",
                                "target_lang_sense_clarification": "в вопросительных предложениях"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "yet",
                                "target_lang_sense_clarification": "in questions"
                            },
                            {
                                "target_lang_word": "already",
                                "target_lang_sense_clarification": "expressing surprise or expectation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "249d633f-7d7a-3cde-b1f5-4b0b54d6075b",
                    "sense_definition": "Versterkend woord dat een overmaat aanduidt, meestal voorafgaand aan 'te'.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emphatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is <w>al</w> te gek voor woorden.",
                            "target_lang_translations": {
                                "ru": "Это <w>слишком</w> безумно, чтобы выразить словами.",
                                "en": "That is <w>all</w> too crazy for words."
                            }
                        },
                        {
                            "text": "Hij wilde <w>al</w> te graag meedoen met het spel.",
                            "target_lang_translations": {
                                "ru": "Он <w>слишком</w> сильно хотел участвовать в игре.",
                                "en": "He wanted <w>all</w> too eagerly to join the game."
                            }
                        }
                    ],
                    "common_phrases": [
                        "al te",
                        "al te graag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Усилительное слово, обозначающее чрезмерность; обычно используется перед 'te' (слишком).",
                        "en": "Intensifying word indicating excess, usually preceding 'te' (too)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слишком",
                                "target_lang_sense_clarification": "усилительная частица"
                            },
                            {
                                "target_lang_word": "чересчур",
                                "target_lang_sense_clarification": "излишне"
                            },
                            {
                                "target_lang_word": "уж",
                                "target_lang_sense_clarification": "разговорное усиление"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "all",
                                "target_lang_sense_clarification": "intensifier (all too)"
                            },
                            {
                                "target_lang_word": "far",
                                "target_lang_sense_clarification": "intensifier"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2325ad23-e05b-3267-8b77-4951fe21610d",
                    "sense_definition": "Terwijl; aanduiding van gelijktijdigheid in combinatie met een onvoltooid deelwoord.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "simultaneity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Al</w> doende leert men.",
                            "target_lang_translations": {
                                "ru": "<w>В процессе</w> дела человек учится.",
                                "en": "One learns <w>by</w> doing."
                            }
                        },
                        {
                            "text": "<w>Al</w> wandelend kwamen ze bij het bos aan.",
                            "target_lang_translations": {
                                "ru": "<w>Пока</w> они гуляли, они подошли к лесу.",
                                "en": "<w>While</w> walking, they arrived at the forest."
                            }
                        }
                    ],
                    "common_phrases": [
                        "al doende",
                        "al pratende"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "with participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В то время как; указание на одновременность действия в сочетании с причастием.",
                        "en": "While; indication of simultaneity in combination with a present participle."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока",
                                "target_lang_sense_clarification": "союз времени"
                            },
                            {
                                "target_lang_word": "в процессе",
                                "target_lang_sense_clarification": "во время действия"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "while",
                                "target_lang_sense_clarification": "conjunction of time"
                            },
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "method or means"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "8b864d72-d0b6-37a8-bfb3-3a96d1c4419a",

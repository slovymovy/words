{
    "word_family": [
        "spreader",
        "spreading",
        "spreadable",
        "widespread",
        "bedspread",
        "spreadsheet"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c7e0a5d5-b65c-35fd-b8c1-c6fcd7695073",
                    "sense_definition": "To cover a surface with a thin layer of a soft substance.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She likes to <w>spread</w> strawberry jam on her toast.",
                            "target_lang_translations": {
                                "ru": "Она любит <w>намазывать</w> клубничный джем на тост."
                            }
                        },
                        {
                            "text": "He <w>spread</w> the glue evenly over the back of the paper.",
                            "target_lang_translations": {
                                "ru": "Он равномерно <w>намазал</w> клей на обратную сторону бумаги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spread butter",
                        "spread evenly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать поверхность тонким слоем мягкого вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "намазывать",
                                "target_lang_sense_clarification": "наносить слой (джем, масло)"
                            },
                            {
                                "target_lang_word": "мазать",
                                "target_lang_sense_clarification": "разг. покрывать чем-то жирным или липким"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "070f688e-c625-3732-8eb5-bed2bca71e23",
                    "sense_definition": "To affect a larger area or a greater number of people; to become widely known or felt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_expansion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fire began to <w>spread</w> rapidly due to the strong wind.",
                            "target_lang_translations": {
                                "ru": "Огонь начал быстро <w>распространяться</w> из-за сильного ветра."
                            }
                        },
                        {
                            "text": "News of the victory <w>spread</w> quickly through the town.",
                            "target_lang_translations": {
                                "ru": "Новость о победе быстро <w>распространилась</w> по городу."
                            }
                        }
                    ],
                    "synonyms": [
                        "circulate",
                        "disseminate",
                        "propagate",
                        "diffuse",
                        "put about"
                    ],
                    "common_phrases": [
                        "spread the word",
                        "spread like wildfire",
                        "spread quickly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Затрагивать большую область или большее количество людей; становиться широко известным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распространяться",
                                "target_lang_sense_clarification": "охватывать всё большее пространство (огонь, новости)"
                            },
                            {
                                "target_lang_word": "расходиться",
                                "target_lang_sense_clarification": "быстро передаваться (о слухах, новостях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c265582-650d-34da-a8df-afd15043a55b",
                    "sense_definition": "To open, unfold, or stretch something out so that it covers a larger area.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>spread</w> a blanket on the grass for our picnic.",
                            "target_lang_translations": {
                                "ru": "Мы <w>расстелили</w> одеяло на траве для пикника."
                            }
                        },
                        {
                            "text": "The eagle <w>spread</w> its wings and soared into the sky.",
                            "target_lang_translations": {
                                "ru": "Орел <w>расправил</w> крылья и взмыл в небо."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spread out",
                        "spread one's wings"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскрывать, разворачивать или вытягивать что-либо, чтобы оно покрывало большую площадь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расстилать",
                                "target_lang_sense_clarification": "раскладывать по поверхности (одеяло, ковер)"
                            },
                            {
                                "target_lang_word": "расправлять",
                                "target_lang_sense_clarification": "выпрямлять сложенное (крылья, плечи)"
                            },
                            {
                                "target_lang_word": "разворачивать",
                                "target_lang_sense_clarification": "раскрывать свернутое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3676a15-cbed-3b57-a57c-a6670fb86828",
                    "sense_definition": "To distribute or scatter things over a wide area.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer <w>spread</w> seeds across the plowed field.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>разбросал</w> семена по вспаханному полю."
                            }
                        },
                        {
                            "text": "The explosion <w>spread</w> debris all over the street.",
                            "target_lang_translations": {
                                "ru": "Взрыв <w>раскидал</w> обломки по всей улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "sow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распределять или разбрасывать предметы по широкой площади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбрасывать",
                                "target_lang_sense_clarification": "кидать в разные стороны"
                            },
                            {
                                "target_lang_word": "раскидывать",
                                "target_lang_sense_clarification": "разг. разбрасывать хаотично"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8830118-6044-3d86-86b0-f95c491b3c32",
                    "sense_definition": "To arrange for something to happen over a period of time rather than all at once.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_expansion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>spread</w> the cost of the car over twelve monthly payments.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>распределить</w> стоимость автомобиля на двенадцать ежемесячных платежей."
                            }
                        },
                        {
                            "text": "The course is <w>spread</w> over three weeks.",
                            "target_lang_translations": {
                                "ru": "Курс <w>распределен</w> на три недели."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spread the cost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организовывать что-либо так, чтобы оно происходило в течение определенного периода времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распределять",
                                "target_lang_sense_clarification": "делить на части по времени"
                            },
                            {
                                "target_lang_word": "растягивать",
                                "target_lang_sense_clarification": "удлинять срок выполнения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "99b26b69-ff40-3f1b-9194-7e27b29469da",
                    "sense_definition": "A soft food product designed to be spread on bread or crackers.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I bought a jar of chocolate <w>spread</w> for the children.",
                            "target_lang_translations": {
                                "ru": "Я купил банку шоколадной <w>пасты</w> для детей."
                            }
                        },
                        {
                            "text": "Do you prefer butter or a low-fat <w>spread</w> on your sandwich?",
                            "target_lang_translations": {
                                "ru": "Ты предпочитаешь сливочное масло или обезжиренный <w>спред</w> на бутерброде?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "cheese spread",
                        "chocolate spread"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мягкий пищевой продукт, предназначенный для намазывания на хлеб или крекеры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паста",
                                "target_lang_sense_clarification": "шоколадная или ореховая масса"
                            },
                            {
                                "target_lang_word": "спред",
                                "target_lang_sense_clarification": "жировой продукт, заменитель масла"
                            },
                            {
                                "target_lang_word": "намазка",
                                "target_lang_sense_clarification": "разг. то, что намазывают на хлеб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98926f8c-a28d-337e-add1-7c03ada6064c",
                    "sense_definition": "The process of reaching a larger area or affecting more people; the extent of this expansion.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_expansion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doctors are working hard to stop the <w>spread</w> of the disease.",
                            "target_lang_translations": {
                                "ru": "Врачи усердно работают, чтобы остановить <w>распространение</w> болезни."
                            }
                        },
                        {
                            "text": "The rapid <w>spread</w> of mobile technology has changed how we communicate.",
                            "target_lang_translations": {
                                "ru": "Быстрое <w>распространение</w> мобильных технологий изменило то, как мы общаемся."
                            }
                        }
                    ],
                    "common_phrases": [
                        "slow the spread",
                        "prevent the spread"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс охвата большей территории или воздействия на большее количество людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распространение",
                                "target_lang_sense_clarification": "процесс расширения зоны действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52a897bb-4f20-3d0b-925b-3214e7a844b2",
                    "sense_definition": "An article, advertisement, or image that covers two facing pages in a magazine or newspaper.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The magazine featured a colorful two-page <w>spread</w> on holiday destinations.",
                            "target_lang_translations": {
                                "ru": "В журнале был красочный <w>разворот</w> на две страницы о местах для отдыха."
                            }
                        },
                        {
                            "text": "They paid for a center <w>spread</w> to advertise the new product.",
                            "target_lang_translations": {
                                "ru": "Они заплатили за центральный <w>разворот</w> для рекламы нового продукта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "double-page spread",
                        "center spread"
                    ],
                    "target_lang_definitions": {
                        "ru": "Статья или реклама, занимающая две соседние страницы в журнале или газете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разворот",
                                "target_lang_sense_clarification": "две смежные страницы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91fda272-a39f-328d-8943-3e792b490068",
                    "sense_definition": "A decorative cover for a bed.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "household",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She chose a quilted <w>spread</w> that matched the bedroom curtains.",
                            "target_lang_translations": {
                                "ru": "Она выбрала стеганое <w>покрывало</w>, которое сочеталось со шторами в спальне."
                            }
                        },
                        {
                            "text": "The maid smoothed the <w>spread</w> over the pillows.",
                            "target_lang_translations": {
                                "ru": "Горничная разгладила <w>покрывало</w> поверх подушек."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративное покрывало для кровати."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрывало",
                                "target_lang_sense_clarification": "ткань для накрывания постели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02ca66b0-e0df-383b-82a5-d4b741fbe825",
                    "sense_definition": "The difference between two amounts, prices, rates, or scores.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Investors profit from the <w>spread</w> between the buying and selling price.",
                            "target_lang_translations": {
                                "ru": "Инвесторы получают прибыль от <w>спреда</w> между ценой покупки и продажи."
                            }
                        },
                        {
                            "text": "The team covered the point <w>spread</w> in the final seconds of the game.",
                            "target_lang_translations": {
                                "ru": "Команда покрыла <w>разрыв</w> в очках на последних секундах игры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "point spread",
                        "bid-ask spread"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often used in finance or sports betting"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разница между двумя суммами, ценами, ставками или показателями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спред",
                                "target_lang_sense_clarification": "фин. разность цен"
                            },
                            {
                                "target_lang_word": "разница",
                                "target_lang_sense_clarification": "отличие в величинах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c689ea0e-4bb0-38aa-a718-9db03173b937",
                    "sense_definition": "A large, elaborate meal laid out on a table.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They put on a huge <w>spread</w> for the wedding anniversary party.",
                            "target_lang_translations": {
                                "ru": "Они устроили огромное <w>пиршество</w> в честь годовщины свадьбы."
                            }
                        },
                        {
                            "text": "I have never seen such a delicious <w>spread</w> of cakes and pastries.",
                            "target_lang_translations": {
                                "ru": "Я никогда не видел такого вкусного <w>угощения</w> из тортов и пирожных."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обильное, богатое угощение, выставленное на стол."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угощение",
                                "target_lang_sense_clarification": "еда для гостей"
                            },
                            {
                                "target_lang_word": "пиршество",
                                "target_lang_sense_clarification": "богатый стол, пир"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44cd481b-5bd1-3ee6-82b0-94fb6c5fc65a",
                    "sense_definition": "A large tract of land used to raise livestock; a ranch.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He dreams of owning a cattle <w>spread</w> in Texas someday.",
                            "target_lang_translations": {
                                "ru": "Он мечтает когда-нибудь владеть скотоводческим <w>ранчо</w> в Техасе."
                            }
                        },
                        {
                            "text": "The family has lived on this <w>spread</w> for three generations.",
                            "target_lang_translations": {
                                "ru": "Семья жила на этой <w>ферме</w> в течение трех поколений."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой участок земли, используемый для животноводства; ранчо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ранчо",
                                "target_lang_sense_clarification": "ферма (обычно в Америке)"
                            },
                            {
                                "target_lang_word": "ферма",
                                "target_lang_sense_clarification": "сельскохозяйственное предприятие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
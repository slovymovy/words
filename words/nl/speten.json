    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8d4769bf-cd48-3b8d-85c9-876fa42dab8c",
                    "sense_definition": "verleden tijd meervoud van <w>spijten</w>",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De harde woorden <w>speten</w> hem onmiddellijk na het uitspreken.",
                            "target_lang_translations": {
                                "ru": "Он сразу же <пожалел> о резких словах, как только произнес их."
                            }
                        },
                        {
                            "text": "Zijn overhaaste beslissingen <w>speten</w> de commissie achteraf.",
                            "target_lang_translations": {
                                "ru": "Комиссия впоследствии <сожалела> о его поспешных решениях."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время во множественном числе от глагола spijten (сожалеть)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сожалеть",
                                "target_lang_sense_clarification": "испытывать сожаление"
                            },
                            {
                                "target_lang_word": "раскаиваться",
                                "target_lang_sense_clarification": "чувствовать вину"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "266b7d01-1e13-35a1-8126-0e51f598cd52",
                    "sense_definition": "voedsel aan een scherpe pin of stok rijgen om het te bereiden",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kok moest de paling <w>speten</w> voordat deze de rookoven in ging.",
                            "target_lang_translations": {
                                "ru": "Повар должен был <нанизать> угря, прежде чем отправить его в коптильню."
                            }
                        },
                        {
                            "text": "De rollade werd <w>gespeet</w> en geroosterd boven het vuur.",
                            "target_lang_translations": {
                                "ru": "Рулет <насадили на вертел> и зажарили над огнем."
                            }
                        }
                    ],
                    "synonyms": [
                        "spietsen",
                        "rijgen"
                    ],
                    "common_phrases": [
                        "vis speten",
                        "vlees speten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанизывать еду на острый штырь или палку для приготовления"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "насаживать на вертел",
                                "target_lang_sense_clarification": "для жарки над огнем"
                            },
                            {
                                "target_lang_word": "нанизывать",
                                "target_lang_sense_clarification": "продевать сквозь что-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3769e0d1-4768-3fca-bea5-dba1b0731225",

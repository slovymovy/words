    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b24ca84c-b7b4-3236-a588-f23e9bb1ac19",
                    "sense_definition": "Ухудшать состояние чего-либо, делать плохим, негодным или некрасивым.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот глубокий шрам сильно <w>портит</w> его лицо.",
                            "target_lang_translations": {
                                "en": "This deep scar significantly <w>mars</w> his face."
                            }
                        },
                        {
                            "text": "Постоянные дожди могут <w>испортить</w> нам весь отпуск.",
                            "target_lang_translations": {
                                "en": "Constant rains can <w>ruin</w> our whole vacation."
                            }
                        }
                    ],
                    "synonyms": [
                        "ухудшать",
                        "повреждать",
                        "вредить"
                    ],
                    "antonyms": [
                        "улучшать",
                        "налаживать",
                        "украшать"
                    ],
                    "common_phrases": [
                        "портить зрение",
                        "портить настроение",
                        "портить отношения",
                        "портить имущество"
                    ],
                    "target_lang_definitions": {
                        "en": "To worsen the condition of something, to make it bad, useless, or ugly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spoil",
                                "target_lang_sense_clarification": "general sense: to diminish the value or quality of"
                            },
                            {
                                "target_lang_word": "ruin",
                                "target_lang_sense_clarification": "to destroy or damage severely"
                            },
                            {
                                "target_lang_word": "damage",
                                "target_lang_sense_clarification": "to cause physical harm to"
                            },
                            {
                                "target_lang_word": "mar",
                                "target_lang_sense_clarification": "to impair the appearance of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19a20637-db96-37e1-a8f0-949538399960",
                    "sense_definition": "Оказывать дурное влияние на чей-либо характер или поведение, нравственно развращать.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чрезмерная опека родителей часто <w>портит</w> детей.",
                            "target_lang_translations": {
                                "en": "Excessive parental care often <w>spoils</w> children."
                            }
                        },
                        {
                            "text": "Говорят, что большие деньги и власть <w>портят</w> человека.",
                            "target_lang_translations": {
                                "en": "They say that big money and power <w>corrupt</w> a person."
                            }
                        }
                    ],
                    "synonyms": [
                        "губить"
                    ],
                    "common_phrases": [
                        "портить ребёнка",
                        "портить характер",
                        "портить людей"
                    ],
                    "target_lang_definitions": {
                        "en": "To exert a bad influence on someone's character or behavior, to morally corrupt."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spoil",
                                "target_lang_sense_clarification": "to harm character through overindulgence"
                            },
                            {
                                "target_lang_word": "corrupt",
                                "target_lang_sense_clarification": "to cause to become morally depraved"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7be97521-9518-3d40-8f0d-64aaeea68eb8",
                    "sense_definition": "Лишать девственности, вступать в половую связь (обычно вне брака).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старину барин мог безнаказанно <w>портить</w> дворовых девок.",
                            "target_lang_translations": {
                                "en": "In the old days, a master could <w>ruin</w> servant girls with impunity."
                            }
                        },
                        {
                            "text": "Он не собирался жениться, а только хотел <w>портить</w> молодых девушек.",
                            "target_lang_translations": {
                                "en": "He did not intend to marry, but only wanted to <w>ruin</w> young girls."
                            }
                        }
                    ],
                    "common_phrases": [
                        "портить девок"
                    ],
                    "traits": [
                        {

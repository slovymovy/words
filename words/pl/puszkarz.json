            "senses": [
                {
                    "sense_id": "1d89a85c-6bc7-3670-a83a-c52d50b6f5f5",
                    "sense_definition": "Osoba zbierająca puszki aluminiowe, zwykle w celu ich sprzedaży w punkcie skupu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miejscowy <w>puszkarz</w> codziennie rano sprawdza zawartość koszy na śmieci w poszukiwaniu aluminium.",
                            "target_lang_translations": {
                                "ru": "Местный <w>сборщик банок</w> каждое утро проверяет содержимое мусорных баков в поисках алюминия."
                            }
                        },
                        {
                            "text": "Ten <w>puszkarz</w> traktuje zbieranie opakowań jako swoje główne źródło dochodu i sposób na życie.",
                            "target_lang_translations": {
                                "ru": "Этот <w>сборщик банок</w> рассматривает сбор тары как свой основной источник дохода и образ жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbieracz",
                        "złomiarz"
                    ],
                    "common_phrases": [
                        "osiedlowy puszkarz",
                        "puszkarz złomiarz"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "współcześnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, собирающий алюминиевые банки, обычно для сдачи их в пункт приема."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сборщик банок",
                                "target_lang_sense_clarification": "человек, собирающий тару"
                            },
                            {
                                "target_lang_word": "баночник",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6053b49-9ccc-3b2a-bd5c-d83d9a6d8f6f",
                    "sense_definition": "Rzemieślnik wyrabiający dawną broń palną, w szczególności armaty i działa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_guns",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany <w>puszkarz</w> otrzymał królewskie zlecenie na wykonanie nowych armat dla wojska.",
                            "target_lang_translations": {
                                "ru": "Известный <w>пушечный мастер</w> получил королевский заказ на изготовление новых пушек для войска."
                            }
                        },
                        {
                            "text": "W średniowieczu dobry <w>puszkarz</w> cieszył się dużym poważaniem i wysokimi zarobkami na dworze.",
                            "target_lang_translations": {
                                "ru": "В Средневековье хороший <w>пушечный мастер</w> пользовался большим уважением и имел высокие заработки при дворе."
                            }
                        }
                    ],
                    "synonyms": [
                        "ludwisarz",
                        "rusznikarz"
                    ],
                    "common_phrases": [
                        "królewski puszkarz",
                        "cech puszkarzy",
                        "mistrz puszkarz"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "rzemiosło historyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремесленник, изготавливающий старинное огнестрельное оружие, в частности пушки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пушечный мастер",
                                "target_lang_sense_clarification": "изготовитель пушек"
                            },
                            {
                                "target_lang_word": "оружейник",
                                "target_lang_sense_clarification": "мастер по оружию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a123856e-0b13-357f-9d7a-96cca58568d2",
                    "sense_definition": "Dawne określenie żołnierza obsługującego działa artyleryjskie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_guns",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas oblężenia twierdzy każdy <w>puszkarz</w> miał pełne ręce roboty przy ładowaniu dział.",
                            "target_lang_translations": {
                                "ru": "Во время осады крепости у каждого <w>пушкаря</w> было полно работы с заряжанием орудий."
                            }
                        },
                        {
                            "text": "Stary <w>puszkarz</w> precyzyjnie wycelował lufę w bramę miasta, czekając na rozkaz strzału.",
                            "target_lang_translations": {
                                "ru": "Старый <w>пушкарь</w> точно навел ствол на городские ворота, ожидая приказа стрелять."
                            }
                        }
                    ],
                    "synonyms": [

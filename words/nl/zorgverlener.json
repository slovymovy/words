    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "db8a897a-e642-3ab2-bc38-dd44f1688b59",
                    "sense_definition": "Een persoon die voor zijn beroep medische of verzorgende hulp biedt aan patiënten of cliënten",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "person_provider",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>zorgverlener</w> komt elke ochtend langs om de medicijnen toe te dienen.",
                            "target_lang_translations": {
                                "en": "The <w>caregiver</w> comes by every morning to administer the medication.",
                                "ru": "<w>Медицинский работник</w> приходит каждое утро, чтобы дать лекарства."
                            }
                        },
                        {
                            "text": "Het is belangrijk dat er een goede vertrouwensband is tussen de patiënt en de <w>zorgverlener</w>.",
                            "target_lang_translations": {
                                "en": "It is important that there is a good bond of trust between the patient and the <w>care provider</w>.",
                                "ru": "Важно, чтобы между пациентом и <w>специалистом по уходу</w> были хорошие доверительные отношения."
                            }
                        }
                    ],
                    "synonyms": [
                        "hulpverlener",
                        "verzorger"
                    ],
                    "antonyms": [
                        "zorgbehoevende",
                        "patiënt",
                        "cliënt"
                    ],
                    "common_phrases": [
                        "professionele zorgverlener",
                        "medische zorgverlener"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who provides medical or nursing care to patients or clients as a profession",
                        "ru": "Человек, профессионально оказывающий медицинскую помощь или уход пациентам"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "caregiver",
                                "target_lang_sense_clarification": "person providing direct care"
                            },
                            {
                                "target_lang_word": "healthcare provider",
                                "target_lang_sense_clarification": "formal, professional context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "медицинский работник",
                                "target_lang_sense_clarification": "общий термин для профессионала"
                            },
                            {
                                "target_lang_word": "специалист по уходу",
                                "target_lang_sense_clarification": "акцент на уход и заботу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "096baacd-58e0-3f7d-ad78-a70e1e02292c",
                    "sense_definition": "Een organisatie, bedrijf of instelling die diensten op het gebied van gezondheidszorg aanbiedt",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "org_provider",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je zorgverzekeraar kan je vertellen welke <w>zorgverlener</w> gecontracteerd is.",
                            "target_lang_translations": {
                                "en": "Your health insurer can tell you which <w>healthcare provider</w> is contracted.",
                                "ru": "Ваша страховая компания может сообщить, с каким <w>поставщиком медицинских услуг</w> заключен договор."
                            }
                        },
                        {
                            "text": "Als erkende <w>zorgverlener</w> moet de instelling aan strenge kwaliteitseisen voldoen.",
                            "target_lang_translations": {
                                "en": "As a recognized <w>care provider</w>, the institution must meet strict quality requirements.",
                                "ru": "Как признанное <w>медицинское учреждение</w>, организация должна соответствовать строгим требованиям качества."
                            }
                        }
                    ],
                    "synonyms": [
                        "zorgaanbieder",
                        "zorginstelling"
                    ],
                    "common_phrases": [
                        "gecontracteerde zorgverlener",
                        "erkende zorgverlener",
                        "keuze voor zorgverlener"
                    ],
                    "target_lang_definitions": {
                        "en": "An organization, company, or institution that offers healthcare services",
                        "ru": "Организация или учреждение, предоставляющее услуги в сфере здравоохранения"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "healthcare provider",
                                "target_lang_sense_clarification": "organization or institution"
                            },
                            {
                                "target_lang_word": "care provider",
                                "target_lang_sense_clarification": "organization or institution"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поставщик медицинских услуг",
                                "target_lang_sense_clarification": "официальный термин, часто в страховании"
                            },
                            {
                                "target_lang_word": "медицинское учреждение",
                                "target_lang_sense_clarification": "организация (больница, клиника)"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "rotten",
        "rotter",
        "rottenness"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f51cb74c-0c34-3635-b484-22cfa8a41dd1",
                    "sense_definition": "To suffer decomposition due to biological action, especially by fungi or bacteria.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "decay_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The apple left in the cupboard all that time had started to <w>rot</w>.",
                            "target_lang_translations": {
                                "ru": "Яблоко, оставленное в шкафу на всё это время, начало <w>гнить</w>."
                            }
                        },
                        {
                            "text": "If you leave the wood out in the rain, it will eventually <w>rot</w>.",
                            "target_lang_translations": {
                                "ru": "Если оставить древесину под дождем, она в конце концов <w>сгниёт</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "putrefy",
                        "molder"
                    ],
                    "common_phrases": [
                        "start to rot",
                        "rot away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разлагаться под воздействием бактерий или грибков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнить",
                                "target_lang_sense_clarification": "процесс разложения"
                            },
                            {
                                "target_lang_word": "сгнить",
                                "target_lang_sense_clarification": "результат разложения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c1f74af-6096-3af3-a5b0-03c2fe866862",
                    "sense_definition": "To decline in function or utility.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "decay_metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your brain will <w>rot</w> if you spend so much time on the computer.",
                            "target_lang_translations": {
                                "ru": "Твой мозг <w>сгниёт</w>, если будешь проводить столько времени за компьютером."
                            }
                        },
                        {
                            "text": "The old car was left to <w>rot</w> in the garage for twenty years.",
                            "target_lang_translations": {
                                "ru": "Старую машину оставили <w>гнить</w> в гараже на двадцать лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rot your brain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить в упадок, терять функциональность или качество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнить",
                                "target_lang_sense_clarification": "о вещах или метафорически"
                            },
                            {
                                "target_lang_word": "деградировать",
                                "target_lang_sense_clarification": "об умственных способностях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d31d06d7-e10a-3160-a186-f89bdd9ad873",
                    "sense_definition": "To spend a long period of time in an unpleasant place or state.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "decay_metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The criminal was sentenced to <w>rot</w> in prison for the rest of his life.",
                            "target_lang_translations": {
                                "ru": "Преступника приговорили <w>гнить</w> в тюрьме до конца своих дней."
                            }
                        },
                        {
                            "text": "I refuse to sit here and <w>rot</w> in this boring job forever.",
                            "target_lang_translations": {
                                "ru": "Я отказываюсь сидеть здесь и <w>гнить</w> на этой скучной работе вечно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rot in jail",
                        "rot in hell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить долгое время в невыносимых или скучных условиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнить",
                                "target_lang_sense_clarification": "страдать в заточении или бездействии"
                            },
                            {
                                "target_lang_word": "прозябать",
                                "target_lang_sense_clarification": "вести жалкое существование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88cbf681-7bb2-30c4-8bbb-32b6eb0cec0d",
                    "sense_definition": "To cause to be wholly or partially decomposed by natural processes.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "decay_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Too much water can <w>rot</w> the roots of your house plants.",
                            "target_lang_translations": {
                                "ru": "Избыток воды может <w>сгноить</w> корни ваших комнатных растений."
                            }
                        },
                        {
                            "text": "The humidity in the jungle will <w>rot</w> your boots if you are not careful.",
                            "target_lang_translations": {
                                "ru": "Влажность в джунглях <w>испортит</w> твои ботинки, если не будешь осторожен."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать разложение или гниение чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гноить",
                                "target_lang_sense_clarification": "заставлять гнить"
                            },
                            {
                                "target_lang_word": "сгноить",
                                "target_lang_sense_clarification": "довести до полного разложения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0b034e9-f891-333f-8b6f-ce03c29756ce",
                    "sense_definition": "To talk nonsense.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nonsense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stop <w>rotting</w> and tell us what really happened yesterday.",
                            "target_lang_translations": {
                                "ru": "Хватит <w>нести чушь</w> и расскажи нам, что на самом деле произошло вчера."
                            }
                        },
                        {
                            "text": "He was just <w>rotting</w> about his imaginary achievements to impress us.",
                            "target_lang_translations": {
                                "ru": "Он просто <w>трепался</w> о своих воображаемых достижениях, чтобы впечатлить нас."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        },
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить ерунду или вздор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нести чушь",
                                "target_lang_sense_clarification": "разг."
                            },
                            {
                                "target_lang_word": "трепаться",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "08955bfe-a8f5-364f-91e4-580262d0a9f9",
                    "sense_definition": "The process of becoming rotten; putrefaction.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "decay_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The smell of <w>rot</w> coming from the garbage bin was unbearable.",
                            "target_lang_translations": {
                                "ru": "Запах <w>гнили</w>, исходящий из мусорного бака, был невыносим."
                            }
                        },
                        {
                            "text": "We need to stop the <w>rot</w> in the wooden beams before the roof collapses.",
                            "target_lang_translations": {
                                "ru": "Нам нужно остановить <w>гниение</w> деревянных балок, пока крыша не рухнула."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dry rot",
                        "root rot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс разложения органических веществ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гниение",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "гниль",
                                "target_lang_sense_clarification": "состояние или запах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c902a1a9-75c7-36ff-bc50-2d9069600b3a",
                    "sense_definition": "Decaying matter.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "decay_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The forest floor was covered in damp leaves and <w>rot</w>.",
                            "target_lang_translations": {
                                "ru": "Лесная подстилка была покрыта влажными листьями и <w>гнилью</w>."
                            }
                        },
                        {
                            "text": "When a turkey vulture detects the scent of <w>rot</w>, it circles down to find the source.",
                            "target_lang_translations": {
                                "ru": "Когда гриф-индейка чует запах <w>падали</w>, он кружит вниз, чтобы найти источник."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гниющая материя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гниль",
                                "target_lang_sense_clarification": "разложившаяся масса"
                            },
                            {
                                "target_lang_word": "перегной",
                                "target_lang_sense_clarification": "сгнившие листья, гумус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21e15333-7969-37b7-90ad-616932ebdb2b",
                    "sense_definition": "Verbal nonsense.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nonsense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are talking absolute <w>rot</w> and I do not believe a word of it.",
                            "target_lang_translations": {
                                "ru": "Ты несешь полную <w>чушь</w>, и я не верю ни единому слову."
                            }
                        },
                        {
                            "text": "Don't listen to the <w>rot</w> he says about politics.",
                            "target_lang_translations": {
                                "ru": "Не слушай тот <w>вздор</w>, что он говорит о политике."
                            }
                        }
                    ],
                    "common_phrases": [
                        "talk rot"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Словесная бессмыслица, вздор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чушь",
                                "target_lang_sense_clarification": "абсурд"
                            },
                            {
                                "target_lang_word": "вздор",
                                "target_lang_sense_clarification": "глупости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c6e1674-29b2-3c88-8873-e12fa07c8794",
                    "sense_definition": "Any of several diseases in which breakdown of tissue occurs.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "decay_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer was worried that foot <w>rot</w> would spread through his herd.",
                            "target_lang_translations": {
                                "ru": "Фермер беспокоился, что копытная <w>гниль</w> распространится на всё стадо."
                            }
                        },
                        {
                            "text": "Various types of <w>rot</w> can destroy the entire harvest if not treated.",
                            "target_lang_translations": {
                                "ru": "Различные виды <w>гнили</w> могут уничтожить весь урожай, если их не лечить."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Болезнь растений или животных, вызывающая разложение тканей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гниль",
                                "target_lang_sense_clarification": "в названиях болезней"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4df595fd-cb7f-3e6c-84e5-8529f930df9e",
                    "sense_definition": "Abbreviation of Refugee Olympic Team (country code for the team).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "The athletes from the <w>ROT</w> received a standing ovation at the ceremony.",
                            "target_lang_translations": {
                                "ru": "Спортсмены из <w>Сборной беженцев</w> удостоились оваций на церемонии."
                            }
                        },
                        {
                            "text": "She was proud to compete under the banner of the <w>ROT</w>.",
                            "target_lang_translations": {
                                "ru": "Она гордилась тем, что выступает под флагом <w>Сборной беженцев</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ROA"
                    ],
                    "target_lang_definitions": {
                        "ru": "Олимпийская сборная беженцев (код команды)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сборная беженцев",
                                "target_lang_sense_clarification": "спортивная команда"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
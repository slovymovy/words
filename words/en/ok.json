{
    "word_family": [
        "okay",
        "okey-dokey"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "f3ee97b9-a6a3-3b4c-b8d9-a176fbac3f99",
                    "sense_definition": "Used to agree to a suggestion, request, or statement.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>OK</w>, I will see you tomorrow.",
                            "target_lang_translations": {
                                "ru": "<w>Хорошо</w>, увидимся завтра."
                            }
                        },
                        {
                            "text": "Can you help me with this? — <w>OK</w>.",
                            "target_lang_translations": {
                                "ru": "Ты можешь мне помочь с этим? — <w>Ладно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "all right",
                        "okey-dokey"
                    ],
                    "common_phrases": [
                        "it is OK",
                        "that is OK"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения согласия с предложением, просьбой или утверждением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "нейтральное согласие"
                            },
                            {
                                "target_lang_word": "ладно",
                                "target_lang_sense_clarification": "согласие, иногда с оттенком уступки"
                            },
                            {
                                "target_lang_word": "окей",
                                "target_lang_sense_clarification": "разговорное заимствование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ca005ee-b839-3aad-b86a-60f246f6d7a4",
                    "sense_definition": "Used to introduce a new topic, pause for thought, or get attention before speaking.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "discourse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>OK</w>, let's move on to the next question.",
                            "target_lang_translations": {
                                "ru": "<w>Так</w>, давайте перейдем к следующему вопросу."
                            }
                        },
                        {
                            "text": "<w>OK</w>, I think I understand what you mean now.",
                            "target_lang_translations": {
                                "ru": "<w>Хорошо</w>, я думаю, теперь я понимаю, что вы имеете в виду."
                            }
                        }
                    ],
                    "synonyms": [
                        "now",
                        "now then",
                        "all right"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения новой темы, паузы для размышления или привлечения внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "вводное слово для смены темы"
                            },
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "ладно",
                                "target_lang_sense_clarification": "вводное слово, часто при переходе к действию"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d83c883a-5dfc-3f65-b706-c907d86f3369",
                    "sense_definition": "Allowed or permitted; acceptable to do.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "permitted",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is it <w>ok</w> if I open the window?",
                            "target_lang_translations": {
                                "ru": "<w>Ничего</w>, если я открою окно?"
                            }
                        },
                        {
                            "text": "It is not <w>ok</w> to run in the hallway.",
                            "target_lang_translations": {
                                "ru": "Бегать в коридоре <w>нельзя</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "allowed",
                        "permissible",
                        "all right"
                    ],
                    "antonyms": [
                        "forbidden"
                    ],
                    "common_phrases": [
                        "is it OK",
                        "it is OK"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрешенный или допустимый; приемлемый для выполнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нормально",
                                "target_lang_sense_clarification": "допустимо, приемлемо"
                            },
                            {
                                "target_lang_word": "можно",
                                "target_lang_sense_clarification": "разрешено (в роли сказуемого)"
                            },
                            {
                                "target_lang_word": "ничего",
                                "target_lang_sense_clarification": "разг. допустимо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "525cf729-4663-31f0-aa5a-30099e58e8a0",
                    "sense_definition": "In good health, happy, or safe.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Are you <w>ok</w> after the accident?",
                            "target_lang_translations": {
                                "ru": "Ты <w>в порядке</w> после аварии?"
                            }
                        },
                        {
                            "text": "He was sick yesterday, but he feels <w>ok</w> now.",
                            "target_lang_translations": {
                                "ru": "Вчера он болел, но сейчас чувствует себя <w>нормально</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "fine",
                        "well",
                        "all right"
                    ],
                    "antonyms": [
                        "ill",
                        "sick",
                        "unwell",
                        "poorly"
                    ],
                    "common_phrases": [
                        "are you OK",
                        "I am OK"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здоровый, счастливый или находящийся в безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в порядке",
                                "target_lang_sense_clarification": "о состоянии или самочувствии"
                            },
                            {
                                "target_lang_word": "нормально",
                                "target_lang_sense_clarification": "разг. о самочувствии"
                            },
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "о хорошем самочувствии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "376737a9-ec87-3f42-b0b1-5a117e9e6d67",
                    "sense_definition": "Good enough but not excellent; satisfactory.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie was just <w>ok</w>, so I would not watch it again.",
                            "target_lang_translations": {
                                "ru": "Фильм был так себе, просто <w>нормальный</w>, так что я бы не стал смотреть его снова."
                            }
                        },
                        {
                            "text": "Her grades are <w>ok</w>, but she could do better.",
                            "target_lang_translations": {
                                "ru": "Ее оценки <w>нормальные</w>, но она могла бы учиться лучше."
                            }
                        }
                    ],
                    "synonyms": [
                        "satisfactory",
                        "adequate",
                        "not bad",
                        "all right"
                    ],
                    "antonyms": [
                        "bad",
                        "poor",
                        "unsatisfactory",
                        "inadequate"
                    ],
                    "common_phrases": [
                        "just OK"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достаточно хороший, но не отличный; удовлетворительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нормальный",
                                "target_lang_sense_clarification": "удовлетворительный"
                            },
                            {
                                "target_lang_word": "ничего",
                                "target_lang_sense_clarification": "разг. неплохой"
                            },
                            {
                                "target_lang_word": "так себе",
                                "target_lang_sense_clarification": "средний, не очень хороший"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c821f5ca-b196-3cee-ac87-a060c16296a3",
                    "sense_definition": "Willing to accept a situation or decision.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "willingness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>ok</w> with staying home tonight if you are tired.",
                            "target_lang_translations": {
                                "ru": "Я <w>не против</w> остаться дома сегодня вечером, если ты устал."
                            }
                        },
                        {
                            "text": "Are you <w>ok</w> with the new schedule?",
                            "target_lang_translations": {
                                "ru": "Ты <w>согласен</w> с новым расписанием?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "OK with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовый принять ситуацию или решение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не против",
                                "target_lang_sense_clarification": "выражение согласия"
                            },
                            {
                                "target_lang_word": "согласен",
                                "target_lang_sense_clarification": "выражение согласия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e2dd4a87-8e26-3fb1-b167-adc5c68a3cec",
                    "sense_definition": "In a satisfactory manner; well enough.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "adverb_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team played <w>ok</w> but still lost the game.",
                            "target_lang_translations": {
                                "ru": "Команда сыграла <w>нормально</w>, но все равно проиграла."
                            }
                        },
                        {
                            "text": "Everything went <w>ok</w> at the meeting.",
                            "target_lang_translations": {
                                "ru": "На собрании все прошло <w>хорошо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "adequately",
                        "satisfactorily"
                    ],
                    "antonyms": [
                        "badly",
                        "poorly",
                        "unsatisfactorily",
                        "inadequately"
                    ],
                    "common_phrases": [
                        "do OK"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удовлетворительным образом; достаточно хорошо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нормально",
                                "target_lang_sense_clarification": "удовлетворительно"
                            },
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "вполне успешно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d095795e-6860-34ac-a4e5-fb4fe89ddd2b",
                    "sense_definition": "To give official approval or permission for something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_approve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager <w>OK'd</w> the budget for the new project.",
                            "target_lang_translations": {
                                "ru": "Менеджер <w>одобрил</w> бюджет нового проекта."
                            }
                        },
                        {
                            "text": "I need my parents to <w>OK</w> my trip before I can buy tickets.",
                            "target_lang_translations": {
                                "ru": "Мне нужно, чтобы родители <w>дали добро</w> на мою поездку, прежде чем я смогу купить билеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "approve",
                        "greenlight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать официальное одобрение или разрешение на что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одобрить",
                                "target_lang_sense_clarification": "дать официальное согласие"
                            },
                            {
                                "target_lang_word": "дать добро",
                                "target_lang_sense_clarification": "разг. разрешить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a82fb64d-53c3-38c4-b423-9b2457264933",
                    "sense_definition": "To confirm an action on a computer by clicking a button labeled OK.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "action_click",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please select your options and then <w>OK</w> the dialog box.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, выберите опции, а затем <w>нажмите ОК</w> в диалоговом окне."
                            }
                        },
                        {
                            "text": "Once you <w>OK</w> the changes, the file will be saved.",
                            "target_lang_translations": {
                                "ru": "Как только вы <w>подтвердите</w> изменения, файл будет сохранен."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in computing contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подтвердить действие на компьютере, нажав кнопку с надписью OK."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нажать ОК",
                                "target_lang_sense_clarification": "выбрать кнопку подтверждения"
                            },
                            {
                                "target_lang_word": "подтвердить",
                                "target_lang_sense_clarification": "согласиться с действием"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b8e2dace-ddac-3f61-91eb-bd11a852f206",
                    "sense_definition": "An official agreement or permission to proceed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_approval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are just waiting for the <w>OK</w> from the boss to start.",
                            "target_lang_translations": {
                                "ru": "Мы просто ждем <w>добра</w> от начальника, чтобы начать."
                            }
                        },
                        {
                            "text": "They gave the <w>OK</w> to build the new school.",
                            "target_lang_translations": {
                                "ru": "Они дали <w>добро</w> на строительство новой школы."
                            }
                        }
                    ],
                    "synonyms": [
                        "approval",
                        "endorsement",
                        "green light",
                        "go-ahead"
                    ],
                    "common_phrases": [
                        "get the OK",
                        "give the OK"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное соглашение или разрешение на продолжение действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одобрение",
                                "target_lang_sense_clarification": "официальное согласие"
                            },
                            {
                                "target_lang_word": "добро",
                                "target_lang_sense_clarification": "разг. разрешение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c4e1c85d-03bf-392d-973c-efc43beae918",
                    "sense_definition": "The state of Oklahoma in the United States.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "He was born in Tulsa, <w>OK</w>.",
                            "target_lang_translations": {
                                "ru": "Он родился в Талсе, <w>Оклахома</w>."
                            }
                        },
                        {
                            "text": "The package was shipped to an address in <w>OK</w>.",
                            "target_lang_translations": {
                                "ru": "Посылка была отправлена по адресу в <w>Оклахоме</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Штат Оклахома в США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Оклахома",
                                "target_lang_sense_clarification": "штат США"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
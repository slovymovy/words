{
    "word_family": [
        "tousle"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bd4554a5-6892-3b82-a048-e094ada7d862",
                    "sense_definition": "To make hair or clothing untidy by handling it roughly; to tousle.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The strong wind began to <w>touse</w> her hair as she walked along the cliff.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер начал <w>трепать</w> её волосы, когда она шла вдоль обрыва."
                            }
                        },
                        {
                            "text": "He likes to <w>touse</w> the dog's fur when they play together.",
                            "target_lang_translations": {
                                "ru": "Ему нравится <w>взъерошивать</w> шерсть собаки, когда они играют вместе."
                            }
                        }
                    ],
                    "synonyms": [
                        "tousle",
                        "rumple",
                        "dishevel",
                        "muss"
                    ],
                    "antonyms": [
                        "smooth",
                        "tidy",
                        "neaten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить волосы или одежду в беспорядок грубым обращением; взъерошивать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взъерошивать",
                                "target_lang_sense_clarification": "приводить волосы в беспорядок"
                            },
                            {
                                "target_lang_word": "трепать",
                                "target_lang_sense_clarification": "небрежно трогать или дергать (о ветре или руках)"
                            },
                            {
                                "target_lang_word": "лохматить",
                                "target_lang_sense_clarification": "делать лохматым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddf19fb5-9882-3eac-ac4e-6b6c7b43a42f",
                    "sense_definition": "To pull, drag, or tear something roughly or into pieces.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The playful puppy began to <w>touse</w> the old slipper until it was ruined.",
                            "target_lang_translations": {
                                "ru": "Игривый щенок начал <w>терзать</w> старый тапочек, пока окончательно его не испортил."
                            }
                        },
                        {
                            "text": "She watched the kittens <w>touse</w> the ball of yarn across the floor.",
                            "target_lang_translations": {
                                "ru": "Она наблюдала, как котята <w>таскают</w> клубок пряжи по полу."
                            }
                        }
                    ],
                    "synonyms": [
                        "tear",
                        "rend",
                        "drag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубо тянуть, тащить или рвать что-либо на части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терзать",
                                "target_lang_sense_clarification": "рвать или мучить (часто о животных)"
                            },
                            {
                                "target_lang_word": "таскать",
                                "target_lang_sense_clarification": "грубо волочить или тянуть"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1cdf1d78-c62c-3023-8062-da6fe060a258",
                    "sense_definition": "A noisy disturbance or commotion.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The neighbors caused quite a <w>touse</w> late last night.",
                            "target_lang_translations": {
                                "ru": "Соседи устроили настоящий <w>переполох</w> поздно ночью."
                            }
                        },
                        {
                            "text": "There was a sudden <w>touse</w> in the street that woke everyone up.",
                            "target_lang_translations": {
                                "ru": "На улице внезапно поднялся <w>шум</w>, который всех разбудил."
                            }
                        }
                    ],
                    "synonyms": [
                        "commotion",
                        "disturbance",
                        "ruckus",
                        "row"
                    ],
                    "antonyms": [
                        "calm",
                        "peace",
                        "quiet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шумное нарушение спокойствия, суматоха или скандал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переполох",
                                "target_lang_sense_clarification": "суматоха, тревога"
                            },
                            {
                                "target_lang_word": "шум",
                                "target_lang_sense_clarification": "громкие звуки, скандал"
                            },
                            {
                                "target_lang_word": "возня",
                                "target_lang_sense_clarification": "шумное беспорядочное движение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
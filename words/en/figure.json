                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "иметь мнение"
                            },
                            {
                                "target_lang_word": "рассчитывать",
                                "target_lang_sense_clarification": "ожидать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84bd8e61-5a21-3d52-87a2-03a59f21b069",
                    "sense_definition": "To calculate an amount or solve a mathematical problem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>figure</w> the total cost before we sign the contract.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>подсчитать</w> общую стоимость, прежде чем мы подпишем контракт."
                            }
                        },
                        {
                            "text": "She tried to <w>figure</w> the ratio of expenses to income.",
                            "target_lang_translations": {
                                "ru": "Она пыталась <w>вычислить</w> соотношение расходов к доходам."
                            }
                        }
                    ],
                    "synonyms": [
                        "calculate",
                        "compute",
                        "work out"
                    ],
                    "common_phrases": [
                        "figure the cost",
                        "figure the odds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вычислять сумму или решать математическую задачу."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8045d7e2-d452-3a77-873b-391f85ecb6f6",
                    "sense_definition": "Dorosły samiec owcy, zwierzę hodowlane charakteryzujące się często zakręconymi rogami.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zielonej łące pasł się duży <w>baran</w> z zakręconymi rogami.",
                            "target_lang_translations": {
                                "ru": "На зеленом лугу пасся большой <w>баран</w> с закрученными рогами."
                            }
                        },
                        {
                            "text": "Gospodarz musiał oddzielić agresywnego <w>barana</w> od reszty stada.",
                            "target_lang_translations": {
                                "ru": "Хозяин должен был отделить агрессивного <w>барана</w> от остального стада."
                            }
                        }
                    ],
                    "synonyms": [
                        "tryk"
                    ],
                    "antonyms": [
                        "owca"
                    ],
                    "common_phrases": [
                        "uparty jak baran",
                        "stado baranów",
                        "brać na barana",
                        "strzyc barany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрослый самец овцы, домашнее животное, часто характеризующееся закрученными рогами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баран",
                                "target_lang_sense_clarification": "животное / самец овцы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c238e138-df0a-3700-b50d-ac018240e3b2",
                    "sense_definition": "Obraźliwe określenie osoby, którą uważa się za głupią, bezmyślną, naiwną lub upartą.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "insult_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź takim <w>baranem</w> i pomyśl chwilę, zanim coś zrobisz.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>бараном</w> и подумай немного, прежде чем что-то сделать."
                            }
                        },
                        {
                            "text": "Ten kierowca to kompletny <w>baran</w>, zajechał mi drogę na skrzyżowaniu!",
                            "target_lang_translations": {
                                "ru": "Этот водитель — полный <w>баран</w>, подрезал меня на перекрестке!"
                            }
                        }
                    ],
                    "synonyms": [
                        "idiota",
                        "głupek",
                        "tępak",
                        "osioł",
                        "bałwan",
                        "kretyn"
                    ],
                    "common_phrases": [
                        "skończony baran",
                        "głupi baran",
                        "uparty jak baran"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "used as an insult"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оскорбительное название человека, которого считают глупым, бездумным, наивным или упрямым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баран",
                                "target_lang_sense_clarification": "глупый или упрямый человек"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "05a17fe7-8c6d-3e0e-b757-a6da396c031f",
                    "sense_definition": "Pierwszy znak zodiaku w astrologii, przypisywany okresowi od 21 marca do 19 kwietnia.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "astrology_sense",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Mówi się, że <w>Baran</w> jest znakiem ognistym i pełnym energii.",
                            "target_lang_translations": {
                                "ru": "Говорят, что <w>Овен</w> — это огненный знак, полный энергии."
                            }
                        },
                        {
                            "text": "Słońce wchodzi w znak <w>Barana</w> wraz z początkiem astronomicznej wiosny.",
                            "target_lang_translations": {
                                "ru": "Солнце входит в знак <w>Овна</w> вместе с началом астрономической весны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "znak Barana",
                        "zodiakalny Baran"
                    ],
                    "target_lang_definitions": {
                        "ru": "Первый знак зодиака в астрологии, приписываемый периоду с 21 марта по 19 апреля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Овен",
                                "target_lang_sense_clarification": "знак зодиака"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c86a4681-e0da-35a3-b53b-77da2c78f8db",
                    "sense_definition": "Osoba urodzona w okresie, któremu patronuje ten znak zodiaku.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "astrology_sense",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja siostra to typowy <w>Baran</w>, zawsze musi postawić na swoim.",
                            "target_lang_translations": {
                                "ru": "Моя сестра — типичный <w>Овен</w>, она всегда должна настоять на своем."
                            }
                        },
                        {
                            "text": "Jako zodiakalny <w>Baran</w>, Marek jest bardzo ambitny i odważny.",
                            "target_lang_translations": {
                                "ru": "Как зодиакальный <w>Овен</w>, Марек очень амбициозен и смел."
                            }
                        }
                    ],
                    "common_phrases": [
                        "typowy Baran",
                        "zodiakalny Baran"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, родившийся в период, которому покровительствует этот знак зодиака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Овен",
                                "target_lang_sense_clarification": "человек, рожденный под этим знаком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31cc4891-1e28-3401-b824-437ba2587ef6",
                    "sense_definition": "Popularne nazwisko polskie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "surname_sense",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Baran</w> mieszka w tym bloku od ponad dwudziestu lat.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Баран</w> живет в этом доме уже более двадцати лет."
                            }
                        },
                        {
                            "text": "Na liście obecności w naszej klasie jest dwóch uczniów o nazwisku <w>Baran</w>.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих в нашем классе есть два ученика по фамилии <w>Баран</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Популярная польская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Баран",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1eda0d01-ad7f-3afa-82e9-93ab33f2f391",
                    "sense_definition": "Gwiazdozbiór nieba północnego, w Polsce najlepiej widoczny jesienią i zimą.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy_sense",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Podczas nocnych obserwacji udało nam się zlokalizować gwiazdozbiór <w>Barana</w>.",
                            "target_lang_translations": {
                                "ru": "Во время ночных наблюдений нам удалось найти созвездие <w>Овна</w>."
                            }
                        },
                        {
                            "text": "Najjaśniejszą gwiazdą w konstelacji <w>Barana</w> jest Hamal.",
                            "target_lang_translations": {
                                "ru": "Самой яркой звездой в созвездии <w>Овна</w> является Хамаль."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gwiazdozbiór Barana",
                        "konstelacja Barana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Созвездие северного неба, в Польше лучше всего видимое осенью и зимой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Овен",
                                "target_lang_sense_clarification": "созвездие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
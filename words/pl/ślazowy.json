            "senses": [
                {
                    "sense_id": "cb1887f3-382f-3a68-b8f4-237b48e744a3",
                    "sense_definition": "Otrzymywany z rośliny zwanej ślazem lub zawierający ją w swoim składzie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botany_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia zawsze kupuje na gardło cukierki <w>ślazowe</w>.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка всегда покупает для горла <w>мальвовые</w> конфеты."
                            }
                        },
                        {
                            "text": "W aptece dostępny jest syrop <w>ślazowy</w>, który łagodzi podrażnienia.",
                            "target_lang_translations": {
                                "ru": "В аптеке продается <w>мальвовый</w> сироп, который снимает раздражение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cukierki ślazowe",
                        "syrop ślazowy",
                        "napar ślazowy",
                        "herbata ślazowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получаемый из растения мальва или содержащий его в своем составе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мальвовый",
                                "target_lang_sense_clarification": "относящийся к растению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c29ac20-a342-3fe4-a697-9b9f5b09147e",
                    "sense_definition": "Taki, który ma smak lub zapach charakterystyczny dla ślazu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sensory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W kuchni unosił się delikatny, ziołowy i nieco <w>ślazowy</w> zapach.",
                            "target_lang_translations": {
                                "ru": "На кухне стоял тонкий, травяной и слегка <w>мальвовый</w> запах."
                            }
                        },
                        {
                            "text": "Ten napój ma specyficzny, <w>ślazowy</w> posmak, który nie każdemu odpowiada.",
                            "target_lang_translations": {
                                "ru": "У этого напитка специфический <w>мальвовый</w> привкус, который не всем нравится."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ślazowy zapach",
                        "ślazowy smak",
                        "ślazowy aromat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий вкус или запах, характерный для мальвы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мальвовый",
                                "target_lang_sense_clarification": "о запахе или вкусе"
                            }
                        ]
                    }
                }
            ]
        }

                    "sense_id": "57ead0e6-1cde-3d7f-956b-f0d81f479e34",
                    "sense_definition": "Быстро, проворно, не теряя времени.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <w>живо</w> собрал свои вещи и вышел из комнаты.",
                            "target_lang_translations": {
                                "en": "He <w>quickly</w> gathered his things and left the room."
                            }
                        },
                        {
                            "text": "Мы <w>живо</w> добрались до вокзала и успели на поезд.",
                            "target_lang_translations": {
                                "en": "We <w>quickly</w> got to the station and caught the train."
                            }
                        }
                    ],
                    "synonyms": [
                        "быстро",
                        "проворно",
                        "поспешно",
                        "скоро",
                        "торопливо"
                    ],
                    "antonyms": [
                        "медленно"
                    ],
                    "common_phrases": [
                        "живо собраться",
                        "живо ответить",
                        "живо сделать"
                    ],
                    "target_lang_definitions": {
                        "en": "Quickly, nimbly, without losing time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quickly",
                                "target_lang_sense_clarification": "general speed"
                            },
                            {

{
    "word_family": [
        "beunhaas",
        "beunhazen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f9ab8f63-a8c7-31ab-b705-13c86c985ea2",
                    "sense_definition": "Het verrichten van werk onder de marktprijs, vaak illegaal of als oneerlijke concurrentie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "economic_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De overheid wil <w>beunhazerij</w> in de bouwsector streng bestrijden.",
                            "target_lang_translations": {
                                "en": "The government wants to strictly combat <w>moonlighting</w> in the construction sector.",
                                "ru": "Правительство хочет жестко бороться с <w>шабашничеством</w> в строительном секторе."
                            }
                        },
                        {
                            "text": "Eerlijke ondernemers lijden schade door grootschalige <w>beunhazerij</w>.",
                            "target_lang_translations": {
                                "en": "Honest entrepreneurs suffer damage due to large-scale <w>illicit work</w>.",
                                "ru": "Честные предприниматели несут убытки из-за масштабного <w>нелегального предпринимательства</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwartwerkerij"
                    ],
                    "common_phrases": [
                        "beunhazerij bestrijden",
                        "grootschalige beunhazerij"
                    ],
                    "target_lang_definitions": {
                        "en": "Performing work below market price, often illegally or as unfair competition.",
                        "ru": "Выполнение работы по цене ниже рыночной, часто нелегально или в качестве недобросовестной конкуренции."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "moonlighting",
                                "target_lang_sense_clarification": "working secretly or illegally"
                            },
                            {
                                "target_lang_word": "illicit work",
                                "target_lang_sense_clarification": "undeclared or illegal labor"
                            },
                            {
                                "target_lang_word": "unlicensed practice",
                                "target_lang_sense_clarification": "working without required credentials"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шабашничество",
                                "target_lang_sense_clarification": "нелегальная работа, часто в строительстве"
                            },
                            {
                                "target_lang_word": "нелегальное предпринимательство",
                                "target_lang_sense_clarification": "незаконная деятельность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "564a4551-51c9-3339-bd6d-b461dc821711",
                    "sense_definition": "Het leveren van slecht werk door een gebrek aan vakkennis of kwaliteit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality_of_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De patiënt diende een klacht in wegens medische <w>beunhazerij</w>.",
                            "target_lang_translations": {
                                "en": "The patient filed a complaint regarding medical <w>quackery</w>.",
                                "ru": "Пациент подал жалобу на медицинское <w>шарлатанство</w>."
                            }
                        },
                        {
                            "text": "Door gebrek aan ervaring verviel het project al snel in <w>beunhazerij</w>.",
                            "target_lang_translations": {
                                "en": "Due to a lack of experience, the project quickly descended into <w>bungling</w>.",
                                "ru": "Из-за недостатка опыта проект быстро скатился в <w>дилетантство</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "knoeier",
                        "knoeier"
                    ],
                    "common_phrases": [
                        "medische beunhazerij"
                    ],
                    "target_lang_definitions": {
                        "en": "Delivering poor work due to a lack of professional knowledge or quality.",
                        "ru": "Выполнение некачественной работы из-за недостатка профессиональных знаний или навыков."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bungling",
                                "target_lang_sense_clarification": "clumsy or incompetent work"
                            },
                            {
                                "target_lang_word": "quackery",
                                "target_lang_sense_clarification": "specifically in medical contexts"
                            },
                            {
                                "target_lang_word": "amateurism",
                                "target_lang_sense_clarification": "lack of professional skill"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "халтура",
                                "target_lang_sense_clarification": "небрежная, некачественная работа"
                            },
                            {
                                "target_lang_word": "шарлатанство",
                                "target_lang_sense_clarification": "обман, выдача себя за специалиста"
                            },
                            {
                                "target_lang_word": "дилетантство",
                                "target_lang_sense_clarification": "непрофессионализм"
                            }
                        ]
                    }
                }
            ]
        }
    ]

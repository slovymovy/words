        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b5adfb40-2da1-38ed-bac1-41112e6ac7b3",
                    "sense_definition": "The quality of doing something well and effectively without wasting time, energy, or money.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new software improved the <w>efficiency</w> of the entire department.",
                            "target_lang_translations": {
                                "ru": "Новое программное обеспечение повысило <w>эффективность</w> работы всего отдела."
                            }
                        },
                        {
                            "text": "We were impressed by the speed and <w>efficiency</w> of the service staff.",
                            "target_lang_translations": {
                                "ru": "Мы были впечатлены скоростью и <w>эффективностью</w> обслуживающего персонала."
                            }
                        }
                    ],
                    "antonyms": [
                        "inefficiency",
                        "wastefulness"
                    ],
                    "common_phrases": [
                        "improve efficiency",
                        "maximum efficiency",
                        "operational efficiency",
                        "energy efficiency"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность выполнять работу качественно и результативно, не тратя зря время, энергию или деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эффективность",
                                "target_lang_sense_clarification": "действенность, результативность"
                            },
                            {
                                "target_lang_word": "продуктивность",
                                "target_lang_sense_clarification": "производительность труда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b28e21b5-ed38-3159-87f7-30ba094282a6",
                    "sense_definition": "The ratio of useful work produced by a machine or system to the energy supplied to it.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_ratio",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Engineers are working to increase the fuel <w>efficiency</w> of the new car model.",
                            "target_lang_translations": {
                                "ru": "Инженеры работают над повышением топливной <w>экономичности</w> новой модели автомобиля."
                            }
                        },
                        {
                            "text": "Solar panels have reached a higher level of <w>efficiency</w> in recent years.",
                            "target_lang_translations": {
                                "ru": "В последние годы солнечные панели достигли более высокого уровня <w>эффективности</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fuel efficiency",
                        "thermal efficiency",
                        "high efficiency"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отношение полезной работы, выполняемой машиной или системой, к затраченной на неё энергии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "КПД",

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9f906902-b62e-34d3-be1f-0f27a634d936",
                    "sense_definition": "Wyodrębniona, ułamkowa część jakiejś całości, fragment przedmiotu lub materiału.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "part_of_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Janek urwał <w>kawałek</w> kartki, żeby zapisać numer telefonu.",
                            "target_lang_translations": {
                                "ru": "Янек оторвал <w>кусок</w> бумаги, чтобы записать номер телефона."
                            }
                        },
                        {
                            "text": "Na podłodze leżał mały <w>kawałek</w> rozbitego szkła.",
                            "target_lang_translations": {
                                "ru": "На полу лежал маленький <w>кусок</w> разбитого стекла."
                            }
                        }
                    ],
                    "synonyms": [
                        "część",
                        "fragment",
                        "cząstka",
                        "kawałeczek"
                    ],
                    "antonyms": [
                        "całość"
                    ],
                    "common_phrases": [
                        "kawałek papieru",
                        "kawałek drewna",
                        "w kawałkach",
                        "rozbić na kawałki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная часть целого, фрагмент предмета или материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кусок",
                                "target_lang_sense_clarification": "отделенная часть"
                            },
                            {
                                "target_lang_word": "фрагмент",
                                "target_lang_sense_clarification": "обломок или часть"
                            },
                            {
                                "target_lang_word": "часть",
                                "target_lang_sense_clarification": "доля целого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91adfda5-ebfb-325d-a1c7-7ad44b828d9c",
                    "sense_definition": "Porcja jedzenia odkrojona lub oderwana z większej całości, na przykład plaster ciasta lub pizzy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "part_of_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Został mi ostatni <w>kawałek</w> pizzy na kolację.",
                            "target_lang_translations": {
                                "ru": "У меня остался последний <w>кусок</w> пиццы на ужин."
                            }
                        },
                        {
                            "text": "Czy masz ochotę na <w>kawałek</w> domowego ciasta?",
                            "target_lang_translations": {
                                "ru": "Хочешь <w>кусочек</w> домашнего пирога?"
                            }
                        }
                    ],
                    "synonyms": [
                        "kęs",
                        "kąsek",
                        "kawałeczek"
                    ],
                    "common_phrases": [
                        "kawałek tortu",
                        "kawałek chleba",
                        "smaczny kawałek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Порция еды, отрезанная или оторванная от большего целого, например, кусок пирога или пиццы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кусок",
                                "target_lang_sense_clarification": "порция еды"
                            },
                            {
                                "target_lang_word": "кусочек",
                                "target_lang_sense_clarification": "небольшая порция"
                            },
                            {
                                "target_lang_word": "ломтик",

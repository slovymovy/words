    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "adf6dfbc-c7c7-3a7c-9fbd-8224d54da564",
                    "sense_definition": "Zdrobnienie od: <w>oko</w>; małe oko (np. u dziecka) lub pieszczotliwe określenie narządu wzroku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko przetarło rączką zaspane <w>oczko</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок потер ручкой заспанный <w>глазок</w>."
                            }
                        },
                        {
                            "text": "To małe szczenię ma jedno <w>oczko</w> brązowe, a drugie niebieskie.",
                            "target_lang_translations": {
                                "ru": "У этого маленького щенка один <w>глазок</w> коричневый, а другой голубой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oczko w głowie",
                        "puszczać oczko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительно-ласкательное от слова \"глаз\"; маленький глаз (например, у ребенка) или ласковое название органа зрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глазок",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5a7c7d2-90ee-36f6-a7a3-e0db78510fa0",
                    "sense_definition": "Pionowe rozdarcie w dzianinie, najczęściej w rajstopach lub pończosze, powstałe w wyniku pęknięcia nitki.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "textile_defect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić nowe rajstopy, bo w tych poszło mi <w>oczko</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новые колготки, потому что на этих у меня пошла <w>стрелка</w>."
                            }
                        },
                        {
                            "text": "Zauważyła <w>oczko</w> na nodze tuż przed wejściem na ważne spotkanie.",
                            "target_lang_translations": {
                                "ru": "Она заметила <w>стрелку</w> на ноге прямо перед входом на важную встречу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "poszło oczko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальный разрыв на трикотаже, чаще всего на колготках или чулках, возникший в результате обрыва нити."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стрелка",
                                "target_lang_sense_clarification": "на колготках"
                            },
                            {
                                "target_lang_word": "спущенная петля",
                                "target_lang_sense_clarification": "дефект ткани"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a11e82ce-7f96-3800-a5a5-c1f51132943c",
                    "sense_definition": "Ozdobny kamień, szlachetny lub syntetyczny, umieszczony w oprawie pierścionka.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "jewelry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zgubiła <w>oczko</w> ze swojego ulubionego srebrnego pierścionka.",
                            "target_lang_translations": {
                                "ru": "Она потеряла <w>камень</w> из своего любимого серебряного кольца."
                            }
                        },
                        {
                            "text": "Ten pierścionek ma piękne, szafirowe <w>oczko</w>, które błyszczy w słońcu.",
                            "target_lang_translations": {
                                "ru": "В этом кольце красивый сапфировый <w>камень</w>, который сверкает на солнце."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pierścionek z oczkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративный камень, драгоценный или синтетический, вставленный в оправу кольца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "камень",
                                "target_lang_sense_clarification": "в ювелирном изделии"
                            },
                            {
                                "target_lang_word": "вставка",
                                "target_lang_sense_clarification": "ювелирная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcf181c8-fecd-3725-af74-f56f89cffc47",
                    "sense_definition": "Niewielki zbiornik wodny, naturalny lub sztuczny, często pełniący funkcję ozdobną w ogrodzie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym ogrodzie wykopaliśmy małe <w>oczko</w> wodne dla rybek i żab.",
                            "target_lang_translations": {
                                "ru": "В нашем саду мы выкопали маленький <w>прудик</w> для рыбок и лягушек."
                            }
                        },
                        {
                            "text": "Latem nad leśnym <w>oczkiem</w> można spotkać wiele ważek.",
                            "target_lang_translations": {
                                "ru": "Летом над лесным <w>озерцом</w> можно встретить много стрекоз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oczko wodne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой водоем, естественный или искусственный, часто выполняющий декоративную функцию в саду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прудик",
                                "target_lang_sense_clarification": "небольшой водоем"
                            },
                            {
                                "target_lang_word": "озерцо",
                                "target_lang_sense_clarification": "маленькое озеро"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02f6118d-99bc-3af2-8ea4-baccd031a6c1",
                    "sense_definition": "Gra karciana polegająca na dobieraniu kart, aby uzyskać sumę punktów jak najbardziej zbliżoną do liczby 21.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczorami często graliśmy z dziadkiem w <w>oczko</w> na zapałki.",
                            "target_lang_translations": {
                                "ru": "По вечерам мы часто играли с дедушкой в <w>очко</w> на спички."
                            }
                        },
                        {
                            "text": "A na plaży zagramy w <w>oczko</w>, co ty na to?",
                            "target_lang_translations": {
                                "ru": "А на пляже сыграем в <w>очко</w>, что скажешь?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "grać w oczko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Карточная игра, в которой нужно набрать количество очков, максимально близкое к 21."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очко",
                                "target_lang_sense_clarification": "карточная игра"
                            },
                            {
                                "target_lang_word": "двадцать одно",
                                "target_lang_sense_clarification": "игра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ca261ee-a7d0-3c62-8451-af3eb1d5b5d1",
                    "sense_definition": "Punkt na ściance kostki do gry lub na kamieniu domina, oznaczający wartość liczbową.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "games_points",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzuciłem kostką i wypadło mi pięć <w>oczek</w>.",
                            "target_lang_translations": {
                                "ru": "Я бросил кость, и у меня выпало пять <w>очков</w>."
                            }
                        },
                        {
                            "text": "Policz wszystkie <w>oczka</w> na kostkach, żeby wiedzieć, o ile pól się przesunąć.",
                            "target_lang_translations": {
                                "ru": "Посчитай все <w>очки</w> на кубиках, чтобы знать, на сколько полей передвинуться."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Точка на грани игральной кости или на костяшке домино, обозначающая числовое значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очко",
                                "target_lang_sense_clarification": "единица счета в игре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93c15164-2121-3248-a6fb-1f5d6d17a5e9",
                    "sense_definition": "Pojedyncza pętla z nitki tworząca podstawowy element struktury dzianiny, sieci lub koronki.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "textile_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia uczyła mnie, jak nabierać <w>oczka</w> na druty, żeby zrobić szalik.",
                            "target_lang_translations": {
                                "ru": "Бабушка учила меня, как набирать <w>петли</w> на спицы, чтобы связать шарф."
                            }
                        },
                        {
                            "text": "Musisz uważać, żeby nie zgubić żadnego <w>oczka</w> podczas robótki.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть осторожен, чтобы не потерять ни одной <w>петли</w> во время вязания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nabierać oczka",
                        "gubić oczka",
                        "prawe oczko",
                        "lewe oczko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единичная петля из нити, образующая основной элемент структуры трикотажа, сети или кружева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "петля",
                                "target_lang_sense_clarification": "в вязании"
                            },
                            {
                                "target_lang_word": "ячея",
                                "target_lang_sense_clarification": "сети"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68d0fdd8-3e3c-3609-babb-af58c3cb3b01",
                    "sense_definition": "Potoczne określenie dwudziestych pierwszych urodzin.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "events",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro idę na dużą imprezę, bo Marek świętuje swoje <w>oczko</w>.",
                            "target_lang_translations": {
                                "ru": "Завтра я иду на большую вечеринку, потому что Марек празднует свой <w>двадцать первый день рождения</w>."
                            }
                        },
                        {
                            "text": "Dostał w prezencie na <w>oczko</w> swój pierwszy samochód.",
                            "target_lang_translations": {
                                "ru": "Он получил в подарок на <w>двадцать один год</w> свою первую машину."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название двадцать первого дня рождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двадцать один год",
                                "target_lang_sense_clarification": "возраст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aadc36ef-e1ed-3076-a0f1-f083baccf15f",
                    "sense_definition": "Otwór w ulu, przez który pszczoły wylatują na zewnątrz i wracają do środka.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "beekeeping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pszczoły tłoczyły się przed <w>oczkiem</w>, wracając z nektarem do ula.",
                            "target_lang_translations": {
                                "ru": "Пчелы толпились перед <w>летком</w>, возвращаясь с нектаром в улей."
                            }
                        },
                        {
                            "text": "Pszczelarz zwęził <w>oczko</w> w ulu, aby zabezpieczyć rodzinę pszczelą przed zimnem.",
                            "target_lang_translations": {
                                "ru": "Пчеловод сузил <w>леток</w> в улье, чтобы защитить пчелиную семью от холода."
                            }
                        }
                    ],
                    "synonyms": [
                        "wylotek",
                        "wlotek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отверстие в улье, через которое пчелы вылетают наружу и возвращаются внутрь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "леток",
                                "target_lang_sense_clarification": "в улье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e2bd684-78eb-3d71-b42f-f858e1139169",
                    "sense_definition": "Proste oko występujące u wielu bezkręgowców, reagujące na natężenie światła.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Owady posiadają często zarówno oczy złożone, jak i proste <w>oczka</w>.",
                            "target_lang_translations": {
                                "ru": "Насекомые часто имеют как сложные глаза, так и простые <w>глазки</w>."
                            }
                        },
                        {
                            "text": "To prymitywne <w>oczko</w> pozwala zwierzęciu jedynie odróżniać światło od ciemności.",
                            "target_lang_translations": {
                                "ru": "Этот примитивный <w>глазок</w> позволяет животному лишь отличать свет от тьмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyoczko"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Простой глаз, встречающийся у многих беспозвоночных, реагирующий на интенсивность света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глазок",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                }
            ]
        }

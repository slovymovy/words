{
    "word_family": [
        "juniorka",
        "juniorski"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d122ebbd-34b5-390c-8660-67aaf00e7865",
                    "sense_definition": "Zawodnik sportowy, który nie przekroczył określonego wieku (zwykle 18 lub 19 lat) i startuje w kategorii młodzieżowej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sports_category",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz najlepszy <w>junior</w> zdobył wczoraj złoty medal na mistrzostwach.",
                            "target_lang_translations": {
                                "ru": "Наш лучший <w>юниор</w> вчера завоевал золотую медаль на чемпионате."
                            }
                        },
                        {
                            "text": "Trener zdecydował się przenieść dwóch zdolnych <w>juniorów</w> do pierwszego składu.",
                            "target_lang_translations": {
                                "ru": "Тренер решил перевести двух способных <w>юниоров</w> в основной состав."
                            }
                        }
                    ],
                    "antonyms": [
                        "senior"
                    ],
                    "common_phrases": [
                        "mistrz świata juniorów",
                        "kategoria juniorów",
                        "reprezentacja juniorów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортсмен, не достигший определенного возраста (обычно 18 или 19 лет) и выступающий в молодежной категории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юниор",
                                "target_lang_sense_clarification": "спортсмен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e8f5714-60e5-3426-8c77-3a5bb1ef8910",
                    "sense_definition": "Określenie dodawane do imienia lub nazwiska syna, który nosi to samo imię co ojciec, w celu ich rozróżnienia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "naming_convention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zaproszeniu ślubnym widniało nazwisko pana młodego z dopiskiem <w>junior</w>.",
                            "target_lang_translations": {
                                "ru": "На свадебном приглашении стояла фамилия жениха с припиской <w>младший</w>."
                            }
                        },
                        {
                            "text": "To jest Tomasz <w>junior</w>, a tam przy stole siedzi jego ojciec.",
                            "target_lang_translations": {
                                "ru": "Это Томаш-<w>младший</w>, а там за столом сидит его отец."
                            }
                        }
                    ],
                    "synonyms": [
                        "jr",
                        "jun."
                    ],
                    "antonyms": [
                        "senior"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначение, добавляемое к имени или фамилии сына, который носит то же имя, что и отец, для их различения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "младший",
                                "target_lang_sense_clarification": "приставка к имени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "945c077b-f523-3bd9-9ac1-529db45fe1ab",
                    "sense_definition": "Najmłodszy członek rodziny lub grupy osób.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "age_hierarchy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz rodzinny <w>junior</w> właśnie nauczył się jeździć na rowerze.",
                            "target_lang_translations": {
                                "ru": "Наш семейный <w>младшенький</w> только что научился кататься на велосипеде."
                            }
                        },
                        {
                            "text": "Jako <w>junior</w> w zespole musiał często słuchać rad starszych kolegów.",
                            "target_lang_translations": {
                                "ru": "Как <w>младший</w> в команде, он часто должен был слушать советы старших коллег."
                            }
                        }
                    ],
                    "antonyms": [
                        "nestor",
                        "senior"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый младший член семьи или группы людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "младший",
                                "target_lang_sense_clarification": "по возрасту или положению"
                            },
                            {
                                "target_lang_word": "самый маленький",
                                "target_lang_sense_clarification": "в семье"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
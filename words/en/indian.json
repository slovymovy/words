                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "индийский",
                                "target_lang_sense_clarification": "относящийся к государству Индия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ee04636-2ced-364f-9017-17dc068be914",
                    "sense_definition": "Of or relating to the indigenous peoples of the Americas.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "native_american_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We visited an <w>Indian</w> reservation in the southwest.",

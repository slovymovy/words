    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "edcca2c8-cf07-35a2-8d89-6c6adb7e3942",
                    "sense_definition": "Able to be treated as fact; real or concrete.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police need <w>tangible</w> evidence before they can make an arrest.",
                            "target_lang_translations": {
                                "ru": "Полиции нужны <w>конкретные</w> доказательства, прежде чем они смогут произвести арест."
                            }
                        },
                        {
                            "text": "After months of hard work, the team finally saw <w>tangible</w> results.",
                            "target_lang_translations": {
                                "ru": "После месяцев упорной работы команда наконец увидела <w>ощутимые</w> результаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "concrete",
                        "real",
                        "physical",
                        "substantial",
                        "solid",
                        "material"
                    ],
                    "antonyms": [
                        "intangible"
                    ],
                    "common_phrases": [
                        "tangible evidence",
                        "tangible results",
                        "tangible benefits",
                        "tangible proof",
                        "tangible reward"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что можно считать фактом; реальный, конкретный или существенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ощутимый",
                                "target_lang_sense_clarification": "заметный, существенный (о результатах, пользе)"
                            },
                            {
                                "target_lang_word": "реальный",
                                "target_lang_sense_clarification": "существующий на самом деле, не воображаемый"
                            },
                            {
                                "target_lang_word": "конкретный",
                                "target_lang_sense_clarification": "точный, определенный (о доказательствах, фактах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89d4ba3a-9747-35ca-a451-475ac63efcb3",
                    "sense_definition": "Able to be touched or felt; perceptible by the sense of touch.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A sculpture is a <w>tangible</w> object that occupies physical space.",
                            "target_lang_translations": {
                                "ru": "Скульптура — это <w>осязаемый</w> объект, занимающий физическое пространство."
                            }
                        },
                        {
                            "text": "Light is visible to the eye, but it is not <w>tangible</w>.",
                            "target_lang_translations": {
                                "ru": "Свет виден глазу, но он не <w>осязаем</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "touchable",
                        "tactile",
                        "palpable",
                        "physical"
                    ],
                    "antonyms": [
                        "intangible"
                    ],
                    "common_phrases": [
                        "tangible object",
                        "tangible form",
                        "tangible thing"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, к чему можно прикоснуться; воспринимаемый на ощупь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осязаемый",
                                "target_lang_sense_clarification": "воспринимаемый тактильно"
                            },
                            {
                                "target_lang_word": "материальный",
                                "target_lang_sense_clarification": "физический, вещественный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5fe64400-7648-320d-a3d9-fe275e333b9a",
                    "sense_definition": "A physical object or asset; something that can be touched.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company's <w>tangibles</w> include real estate and heavy machinery.",
                            "target_lang_translations": {
                                "ru": "<w>Материальные активы</w> компании включают недвижимость и тяжелую технику."
                            }
                        },
                        {
                            "text": "Investors often prefer <w>tangibles</w> like gold during times of economic crisis.",
                            "target_lang_translations": {
                                "ru": "Инвесторы часто предпочитают <w>материальные ценности</w>, такие как золото, во время экономического кризиса."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "26493564-7b82-3370-9695-a6972706fcd2",
                    "sense_definition": "De tijd die nog moet komen; de toekomst of vooruitzichten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "toekomst",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er ligt een prachtige vakantie voor ons in het <w>verschiet</w>.",
                            "target_lang_translations": {
                                "ru": "У нас в <w>перспективе</w> прекрасный отпуск.",
                                "en": "A wonderful holiday lies in <w>store</w> for us."
                            }
                        },
                        {
                            "text": "Met deze nieuwe baan liggen er veel kansen in het <w>verschiet</w>.",
                            "target_lang_translations": {
                                "ru": "С этой новой работой открываются большие <w>перспективы</w>.",
                                "en": "With this new job, many opportunities are in the <w>offing</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "toekomst",
                        "vooruitzicht",
                        "perspectief"
                    ],
                    "antonyms": [
                        "verleden"
                    ],
                    "common_phrases": [
                        "in het verschiet liggen",
                        "in het verschiet hebben"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время, которое еще предстоит; будущее или перспективы.",
                        "en": "The time yet to come; the future or prospects."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перспектива",
                                "target_lang_sense_clarification": "ожидаемое будущее или возможность"
                            },
                            {
                                "target_lang_word": "будущее",
                                "target_lang_sense_clarification": "время, которое наступит"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "prospect",
                                "target_lang_sense_clarification": "expectation of something to happen"
                            },
                            {
                                "target_lang_word": "offing",
                                "target_lang_sense_clarification": "near future (often used in 'in the offing')"
                            },
                            {
                                "target_lang_word": "store",
                                "target_lang_sense_clarification": "what is waiting to happen (used in 'in store')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "714ac831-3b81-3c32-bdf6-caa9082e6ccf",
                    "sense_definition": "Een plek of gebied dat ver weg ligt; de verte.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "afstand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het verre <w>verschiet</w> zagen we de contouren van de bergen.",
                            "target_lang_translations": {
                                "ru": "В далекой <w>дали</w> мы увидели очертания гор.",
                                "en": "In the far <w>distance</w>, we saw the contours of the mountains."
                            }
                        },
                        {
                            "text": "Het schip verdween langzaam in het grijze <w>verschiet</w>.",
                            "target_lang_translations": {
                                "ru": "Корабль медленно исчез в серой <w>дали</w>.",
                                "en": "The ship slowly disappeared into the grey <w>distance</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verte",
                        "achtergrond",
                        "horizon"
                    ],
                    "antonyms": [
                        "voorgrond",
                        "nabijheid"
                    ],
                    "common_phrases": [
                        "in het verre verschiet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или область, находящиеся далеко; даль.",
                        "en": "A place or area situated far away; the distance."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "даль",
                                "target_lang_sense_clarification": "далекое пространство"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "distance",
                                "target_lang_sense_clarification": "far away point or area"
                            },
                            {
                                "target_lang_word": "horizon",
                                "target_lang_sense_clarification": "limit of one's vision"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bd6c4734-4c28-32f3-9b85-664f57cf78e7",
                    "sense_definition": "Vervoeging van <w>verschieten</w>: eerste, tweede en derde persoon enkelvoud tegenwoordige tijd en gebiedende wijs.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleur van deze stof <w>verschiet</w> snel als hij in de zon hangt.",
                            "target_lang_translations": {
                                "ru": "Цвет этой ткани быстро <w>выцветает</w>, если она висит на солнце.",
                                "en": "The color of this fabric <w>fades</w> quickly when it hangs in the sun."
                            }
                        },
                        {
                            "text": "Hij <w>verschiet</w> al zijn kruit in de eerste ronde van het debat.",
                            "target_lang_translations": {
                                "ru": "Он <w>расстреливает</w> весь свой порох в первом раунде дебатов.",
                                "en": "He <w>uses up</w> all his powder in the first round of the debate."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола 'verschieten': выцветать, тратить или пугаться.",
                        "en": "Conjugated form of 'verschieten': to fade, use up, or startle."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выцветать",
                                "target_lang_sense_clarification": "терять яркость цвета"
                            },
                            {
                                "target_lang_word": "расстреливать",
                                "target_lang_sense_clarification": "тратить весь запас (например, патронов)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fades",
                                "target_lang_sense_clarification": "loses color or brightness"
                            },
                            {
                                "target_lang_word": "uses up",
                                "target_lang_sense_clarification": "expends completely (e.g., ammunition)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

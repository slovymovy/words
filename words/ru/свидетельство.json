    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a48c2b05-2d15-3df4-908c-6f523a07ea99",
                    "sense_definition": "Официальный документ, удостоверяющий какой-либо факт, личность или права человека (например, о рождении, о браке, о праве собственности).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "doc_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для оформления паспорта вам понадобится <w>свидетельство</w> о рождении.",
                            "target_lang_translations": {
                                "en": "To apply for a passport, you will need a birth <w>certificate</w>."
                            }
                        },
                        {
                            "text": "Молодожёны с радостью получили своё <w>свидетельство</w> о браке в загсе.",
                            "target_lang_translations": {
                                "en": "The newlyweds happily received their marriage <w>certificate</w> at the registry office."
                            }
                        }
                    ],
                    "synonyms": [
                        "удостоверение",
                        "документ",
                        "сертификат"
                    ],
                    "common_phrases": [
                        "свидетельство о рождении",
                        "свидетельство о браке",
                        "свидетельство о смерти",
                        "авторское свидетельство"
                    ],
                    "target_lang_definitions": {
                        "en": "An official document certifying a fact, identity, or rights (e.g., birth, marriage, property ownership)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "certificate",
                                "target_lang_sense_clarification": "official document"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e24670b3-5b56-369b-9b2e-6ca38210323d",
                    "sense_definition": "То, что служит подтверждением, доказательством какого-либо факта или события.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "proof_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его искренняя улыбка была лучшим <w>свидетельством</w> того, что он счастлив.",
                            "target_lang_translations": {
                                "en": "His sincere smile was the best <w>evidence</w> that he was happy."
                            }
                        },
                        {
                            "text": "Эти древние руины служат <w>свидетельством</w> былого величия империи.",
                            "target_lang_translations": {
                                "en": "These ancient ruins serve as <w>evidence</w> of the empire's former greatness."
                            }
                        }
                    ],
                    "synonyms": [
                        "доказательство",
                        "подтверждение"
                    ],
                    "antonyms": [
                        "опровержение"
                    ],
                    "common_phrases": [
                        "яркое свидетельство",
                        "наглядное свидетельство",
                        "служить свидетельством"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that serves as confirmation or proof of a fact or event."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "evidence",
                                "target_lang_sense_clarification": "proof or indication"
                            },
                            {
                                "target_lang_word": "testimony",
                                "target_lang_sense_clarification": "figurative proof"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8dd3a00d-b0f2-3065-ab23-20ef9a6cc079",
                    "sense_definition": "Сообщение сведений о чём-либо очевидцем или осведомлённым лицом (часто в юридическом или религиозном контексте).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "proof_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Судья внимательно выслушал <w>свидетельство</w> главного очевидца происшествия.",
                            "target_lang_translations": {
                                "en": "The judge listened carefully to the <w>testimony</w> of the main eyewitness to the incident."
                            }
                        },
                        {
                            "text": "Закон строго запрещает давать ложное <w>свидетельство</w> в суде.",
                            "target_lang_translations": {
                                "en": "The law strictly forbids giving false <w>testimony</w> in court."
                            }
                        }
                    ],
                    "synonyms": [
                        "показание"
                    ],
                    "common_phrases": [
                        "ложное свидетельство",
                        "дать свидетельство"
                    ],
                    "target_lang_definitions": {
                        "en": "A statement of facts provided by an eyewitness or knowledgeable person (often in a legal or religious context)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "testimony",
                                "target_lang_sense_clarification": "witness statement"
                            },
                            {
                                "target_lang_word": "evidence",
                                "target_lang_sense_clarification": "legal proof"
                            }
                        ]
                    }
                }
            ]
        }
    ]

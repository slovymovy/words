    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c36a1b5-4338-36aa-b3d1-3307c2d16f0d",
                    "sense_definition": "Een vervelende maatregel die iemand moet ondergaan als gevolg van een overtreding of misdaad.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "punishment_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter legde de dief een zware <w>straf</w> op voor zijn daden.",
                            "target_lang_translations": {
                                "en": "The judge imposed a heavy <w>punishment</w> on the thief for his actions.",
                                "ru": "Судья назначил вору суровое <w>наказание</w> за его действия."
                            }
                        },
                        {
                            "text": "Als <w>straf</w> mocht het kind die avond geen televisie kijken.",
                            "target_lang_translations": {
                                "en": "As <w>punishment</w>, the child was not allowed to watch television that evening.",
                                "ru": "В <w>наказание</w> ребенку не разрешили смотреть телевизор в тот вечер."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zware straf",
                        "straf krijgen",
                        "straf uitzitten"
                    ],
                    "target_lang_definitions": {
                        "en": "An unpleasant penalty or measure imposed on someone as a consequence of an offense or crime.",
                        "ru": "Неприятная мера или взыскание, применяемое к кому-либо как последствие нарушения или преступления."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "punishment",
                                "target_lang_sense_clarification": "general term for a penalty imposed for wrongdoing"
                            },
                            {
                                "target_lang_word": "penalty",
                                "target_lang_sense_clarification": "often legal, formal, or in sports"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наказание",
                                "target_lang_sense_clarification": "общий термин для меры воздействия за проступок"
                            },
                            {
                                "target_lang_word": "кара",
                                "target_lang_sense_clarification": "возмездие, часто в высоком или строгом стиле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db71c26e-c095-3a9e-969b-33b4d00f7f4c",
                    "sense_definition": "Een vervelende ervaring of toestand, vaak gebruikt om aan te geven dat iets juist aangenaam is.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "punishment_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is absoluut geen <w>straf</w> om met dit mooie weer buiten te werken.",
                            "target_lang_translations": {
                                "en": "It is absolutely no <w>punishment</w> to work outside in this beautiful weather.",
                                "ru": "Работать на улице в такую прекрасную погоду — это совершенно не <w>наказание</w>."
                            }
                        },
                        {
                            "text": "Wachten in de stromende regen is echt een <w>straf</w>.",
                            "target_lang_translations": {
                                "en": "Waiting in the pouring rain is truly a <w>punishment</w>.",
                                "ru": "Ожидание под проливным дождем — это настоящее <w>наказание</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "geen straf zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "An unpleasant experience or hardship; often used negatively to indicate that something is actually enjoyable.",
                        "ru": "Неприятный опыт или трудность; часто используется в отрицательном контексте, чтобы указать, что что-то на самом деле приятно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "punishment",
                                "target_lang_sense_clarification": "figurative use for a harsh or difficult experience"
                            },
                            {
                                "target_lang_word": "hardship",
                                "target_lang_sense_clarification": "something that causes suffering or difficulty"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наказание",
                                "target_lang_sense_clarification": "в переносном смысле: тяжелое или неприятное занятие"
                            },
                            {
                                "target_lang_word": "мучение",
                                "target_lang_sense_clarification": "страдание или крайне неприятное переживание"
                            }
                        ]
                    }
                }
            ]
        },

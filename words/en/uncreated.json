            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c18c20f2-80de-3033-a1d8-668bf7ce4574",
                    "sense_definition": "Not having been created or brought into existence yet.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author died suddenly, leaving his planned future novels <w>uncreated</w>.",
                            "target_lang_translations": {
                                "ru": "Автор внезапно умер, оставив свои запланированные будущие романы <w>несозданными</w>."
                            }
                        },
                        {
                            "text": "Many potential masterpieces remain <w>uncreated</w> because of fear and hesitation.",
                            "target_lang_translations": {
                                "ru": "Многие потенциальные шедевры остаются <w>несозданными</w> из-за страха и нерешительности."
                            }
                        }
                    ],
                    "antonyms": [
                        "created",
                        "made"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не созданный или еще не вызванный к существованию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несозданный",
                                "target_lang_sense_clarification": "еще не существующий"
                            },
                            {
                                "target_lang_word": "ненаписанный",
                                "target_lang_sense_clarification": "о книгах или произведениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa14efdf-90ce-34e5-9c7c-86c5ab59b865",
                    "sense_definition": "Existing eternally without having been created or having a beginning.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The theology describes an <w>uncreated</w> deity who exists outside of time.",
                            "target_lang_translations": {
                                "ru": "Теология описывает <w>несотворенное</w> божество, существующее вне времени."
                            }
                        },
                        {
                            "text": "Monks in this tradition meditate on the nature of <w>uncreated</w> light.",
                            "target_lang_translations": {
                                "ru": "Монахи этой традиции медитируют на природу <w>нетварного</w> света."
                            }
                        }
                    ],
                    "synonyms": [
                        "beginningless",
                        "unbeginning"
                    ],
                    "antonyms": [
                        "created"
                    ],
                    "common_phrases": [
                        "uncreated light",
                        "uncreated deity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий вечно, не имеющий начала или акта творения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нетварный",
                                "target_lang_sense_clarification": "теологический термин"
                            },
                            {
                                "target_lang_word": "несотворенный",
                                "target_lang_sense_clarification": "не имеющий начала"
                            }
                        ]
                    }
                }
            ]
        },

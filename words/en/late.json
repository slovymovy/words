{
    "word_family": [
        "lately",
        "lateness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b1511e89-e76e-30a0-9c54-4bf908cb9e1a",
                    "sense_definition": "Not arriving, happening, or done until after the expected, proper, or usual time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "delayed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bus was <w>late</w> this morning, so I missed my first class.",
                            "target_lang_translations": {
                                "ru": "Сегодня утром автобус <w>опоздал</w>, поэтому я пропустил первый урок."
                            }
                        },
                        {
                            "text": "Please call me if you are going to be <w>late</w> for the meeting.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, позвони мне, если будешь <w>опаздывать</w> на встречу."
                            }
                        }
                    ],
                    "synonyms": [
                        "belatedly",
                        "late"
                    ],
                    "antonyms": [
                        "early"
                    ],
                    "common_phrases": [
                        "be late",
                        "running late",
                        "too late",
                        "late fee"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несвоевременный; происходящий, прибывающий или сделанный позже ожидаемого или обычного времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поздний",
                                "target_lang_sense_clarification": "происходящий с задержкой или в конце периода"
                            },
                            {
                                "target_lang_word": "запоздалый",
                                "target_lang_sense_clarification": "пришедший или сделанный слишком поздно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c375de7-5711-3547-9146-487ec1964705",
                    "sense_definition": "Happening or existing near the end of a period of time, such as a day, season, or life.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We enjoyed a beautiful walk on the beach in the <w>late</w> afternoon.",
                            "target_lang_translations": {
                                "ru": "Мы наслаждались прекрасной прогулкой по пляжу <w>поздним</w> днём."
                            }
                        },
                        {
                            "text": "It is getting <w>late</w>, so we should probably start heading home now.",
                            "target_lang_translations": {
                                "ru": "Уже становится <w>поздно</w>, так что нам, наверное, пора домой."
                            }
                        }
                    ],
                    "antonyms": [
                        "early",
                        "middle"
                    ],
                    "common_phrases": [
                        "late night",
                        "late summer",
                        "late in the day",
                        "it is getting late"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий или существующий ближе к концу определенного периода времени (дня, сезона, жизни)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поздний",
                                "target_lang_sense_clarification": "относящийся к концу дня, года или другого периода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b072daea-4354-3a32-af97-5cdf7e216f6a",
                    "sense_definition": "No longer alive; referring to a person who has died, especially recently.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "deceased",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum was founded by the <w>late</w> billionaire to display his art collection.",
                            "target_lang_translations": {
                                "ru": "Музей был основан <w>покойным</w> миллиардером для демонстрации его коллекции произведений искусства."
                            }
                        },
                        {
                            "text": "She still keeps a photo of her <w>late</w> husband on her bedside table.",
                            "target_lang_translations": {
                                "ru": "Она до сих пор хранит фотографию своего <w>покойного</w> мужа на прикроватном столике."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the late",
                        "my late husband",
                        "her late father"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "attributive only; usually preceded by 'the' or a possessive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Умерший; используется для уважительного упоминания о человеке, который ушел из жизни (обычно недавно)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покойный",
                                "target_lang_sense_clarification": "умерший (уважительная форма)"
                            },
                            {
                                "target_lang_word": "усопший",
                                "target_lang_sense_clarification": "умерший (книжная, торжественная форма)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b501dec4-78e7-3b8c-9368-91a4e7cd106c",
                    "sense_definition": "After the scheduled, expected, or usual time has passed.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "delayed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I stayed up <w>late</w> last night to finish reading my new book.",
                            "target_lang_translations": {
                                "ru": "Вчера я засиделся до <w>поздна</w>, чтобы дочитать новую книгу."
                            }
                        },
                        {
                            "text": "They arrived <w>late</w> to the party because of the heavy traffic.",
                            "target_lang_translations": {
                                "ru": "Они приехали на вечеринку <w>поздно</w> из-за сильных пробок."
                            }
                        }
                    ],
                    "synonyms": [
                        "belatedly"
                    ],
                    "antonyms": [
                        "early"
                    ],
                    "common_phrases": [
                        "arrive late",
                        "stay up late",
                        "sleep late",
                        "better late than never"
                    ],
                    "target_lang_definitions": {
                        "ru": "После того, как прошло назначенное, ожидаемое или обычное время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поздно",
                                "target_lang_sense_clarification": "с опозданием, не вовремя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9f8f86e-c30e-3b6f-ac92-2fcb7bb28127",
                    "sense_definition": "Formerly; used to indicate a past position, residence, or status.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "former",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new manager is Mr. Smith, <w>late</w> of the marketing department.",
                            "target_lang_translations": {
                                "ru": "Новый менеджер — мистер Смит, <w>бывший</w> сотрудник отдела маркетинга."
                            }
                        },
                        {
                            "text": "She introduced us to Colonel Jones, <w>late</w> of the Royal Air Force.",
                            "target_lang_translations": {
                                "ru": "Она представила нас полковнику Джонсу, <w>ранее</w> служившему в Королевских ВВС."
                            }
                        }
                    ],
                    "synonyms": [
                        "formerly"
                    ],
                    "common_phrases": [
                        "late of"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бывший; используется для указания на прошлую должность, статус или место жительства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бывший",
                                "target_lang_sense_clarification": "занимавший должность в прошлом"
                            },
                            {
                                "target_lang_word": "ранее",
                                "target_lang_sense_clarification": "в прошлом (наречие)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
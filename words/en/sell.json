    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1862ab7f-6d2b-3dec-9566-eb9df1b81e12",
                    "sense_definition": "To transfer goods or provide services in exchange for money.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "trade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>sold</w> her old car very quickly.",
                            "target_lang_translations": {
                                "ru": "Она очень быстро <w>продала</w> свою старую машину."
                            }
                        },
                        {
                            "text": "They <w>sell</w> fresh vegetables at the local market.",
                            "target_lang_translations": {
                                "ru": "Они <w>продают</w> свежие овощи на местном рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "vend",
                        "retail",
                        "peddle",
                        "hawk"
                    ],
                    "antonyms": [
                        "buy"
                    ],
                    "common_phrases": [
                        "buy and sell",
                        "sell for a price",
                        "sell online"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать товары или оказывать услуги в обмен на деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продава́ть",
                                "target_lang_sense_clarification": "несовершенный вид, процесс обмена товара на деньги"
                            },
                            {
                                "target_lang_word": "прода́ть",
                                "target_lang_sense_clarification": "совершенный вид, завершенное действие"
                            },
                            {
                                "target_lang_word": "торгова́ть",
                                "target_lang_sense_clarification": "заниматься продажей товаров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8c53067-6f9b-3bb9-8aaf-01fa0c917df7",
                    "sense_definition": "To be bought by customers in specific quantities or at a specific price.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "trade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This new book <w>sells</w> very well in Europe.",
                            "target_lang_translations": {
                                "ru": "Эта новая книга очень хорошо <w>продается</w> в Европе."
                            }
                        },
                        {
                            "text": "The tickets <w>sold</w> out in just a few minutes.",
                            "target_lang_translations": {
                                "ru": "Билеты <w>разошлись</w> всего за несколько минут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sell well",
                        "sell out",
                        "sell like hotcakes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пользоваться спросом; покупаться клиентами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продава́ться",
                                "target_lang_sense_clarification": "быть в продаже, иметь спрос"
                            },
                            {
                                "target_lang_word": "расходи́ться",
                                "target_lang_sense_clarification": "быстро распродаваться (о товаре)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3df37d06-0099-3a4b-b10a-922f42e9fddf",
                    "sense_definition": "To persuade someone to accept an idea, plan, or product.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "persuasion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to <w>sell</w> me on the idea of moving to the city.",
                            "target_lang_translations": {
                                "ru": "Он пытался <w>убедить</w> меня в идее переезда в город."
                            }
                        },
                        {
                            "text": "You need to <w>sell</w> yourself confidently during the job interview.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно уверенно <w>преподнести</w> себя на собеседовании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sell an idea",
                        "sell yourself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убедить кого-то принять идею, план или продукт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убеди́ть",
                                "target_lang_sense_clarification": "заставить поверить в идею или план"
                            },
                            {
                                "target_lang_word": "преподнести́",
                                "target_lang_sense_clarification": "выгодно представить себя или идею"
                            },
                            {
                                "target_lang_word": "прода́ть",
                                "target_lang_sense_clarification": "переносное значение: успешно внедрить идею"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c5cf38b-f01d-3c34-8eec-cfc6fcd4c2c9",
                    "sense_definition": "To betray a person, group, or country for money or personal gain.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "betrayal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The spy <w>sold</w> state secrets to the enemy.",
                            "target_lang_translations": {
                                "ru": "Шпион <w>продал</w> государственные тайны врагу."
                            }
                        },
                        {
                            "text": "He would never <w>sell</w> his friends down the river.",
                            "target_lang_translations": {
                                "ru": "Он бы никогда не <w>предал</w> своих друзей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sell out",
                        "sell down the river",
                        "sell one's soul"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предать человека, группу или страну ради денег или личной выгоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прода́ть",
                                "target_lang_sense_clarification": "переносное значение: совершить предательство за выгоду"
                            },
                            {
                                "target_lang_word": "преда́ть",
                                "target_lang_sense_clarification": "нарушить верность кому-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c1a1ebe8-c1c1-33ab-86ac-8e3ac616ed0b",
                    "sense_definition": "A method of persuading someone to buy a product or accept an idea.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "persuasion_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Convincing the board to change the policy will be a tough <w>sell</w>.",
                            "target_lang_translations": {
                                "ru": "Убедить совет директоров изменить политику будет непростой <w>задачей</w>."
                            }
                        },
                        {
                            "text": "The salesman used a hard <w>sell</w> to make us buy the car.",
                            "target_lang_translations": {
                                "ru": "Продавец использовал агрессивную <w>тактику</w>, чтобы заставить нас купить машину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hard sell",
                        "soft sell",
                        "tough sell",
                        "easy sell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Метод убеждения или попытка заставить кого-то что-то купить или принять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прода́жа",
                                "target_lang_sense_clarification": "процесс или метод реализации"
                            },
                            {
                                "target_lang_word": "убежде́ние",
                                "target_lang_sense_clarification": "процесс склонения к своей точке зрения"
                            }
                        ]
                    }
                }
            ]
        },

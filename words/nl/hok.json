    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0342d124-f8fd-3088-931a-262b7e45f24c",
                    "sense_definition": "Een onderkomen of verblijfplaats voor dieren, zoals een kooi of een klein stalletje",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal_shelter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hond slaapt elke nacht rustig in zijn <w>hok</w> in de tuin.",
                            "target_lang_translations": {
                                "en": "The dog sleeps quietly in his <w>kennel</w> in the garden every night.",
                                "ru": "Собака каждую ночь спокойно спит в своей <w>будке</w> в саду."
                            }
                        },
                        {
                            "text": "We moeten het <w>hok</w> van de konijnen schoonmaken omdat het vies is.",
                            "target_lang_translations": {
                                "en": "We need to clean the rabbit <w>hutch</w> because it is dirty.",
                                "ru": "Нам нужно почистить кроличью <w>клетку</w>, потому что она грязная."
                            }
                        }
                    ],
                    "synonyms": [
                        "kooi"
                    ],
                    "common_phrases": [
                        "in zijn hok",
                        "in het hok"
                    ],
                    "target_lang_definitions": {
                        "en": "A shelter or accommodation for animals, such as a cage or a small stall",
                        "ru": "Укрытие или жилище для животных, например, клетка или маленький сарай"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kennel",
                                "target_lang_sense_clarification": "specifically for dogs"
                            },
                            {
                                "target_lang_word": "hutch",
                                "target_lang_sense_clarification": "specifically for rabbits or small animals"
                            },
                            {
                                "target_lang_word": "pen",
                                "target_lang_sense_clarification": "enclosure for animals"
                            },
                            {
                                "target_lang_word": "cage",
                                "target_lang_sense_clarification": "barred enclosure"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "будка",
                                "target_lang_sense_clarification": "для собаки"
                            },
                            {
                                "target_lang_word": "клетка",
                                "target_lang_sense_clarification": "для кроликов или птиц"
                            },
                            {
                                "target_lang_word": "сарай",
                                "target_lang_sense_clarification": "для домашнего скота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7a712e6-5d59-3eb8-b72d-be903aeaae90",
                    "sense_definition": "Een kleine, vaak afgesloten of benauwde ruimte in een gebouw, vaak gebruikt voor opslag",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "small_room",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt de bezem en de emmer vinden in het <w>hok</w> onder de trap.",
                            "target_lang_translations": {
                                "en": "You can find the broom and the bucket in the <w>cubbyhole</w> under the stairs.",
                                "ru": "Ты можешь найти метлу и ведро в <w>чулане</w> под лестницей."
                            }
                        },
                        {
                            "text": "Hij werkt de hele dag in een donker <w>hok</w> zonder ramen.",
                            "target_lang_translations": {
                                "en": "He works all day in a dark <w>cubicle</w> without windows.",
                                "ru": "Он весь день работает в темной <w>каморке</w> без окон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "donker hok",
                        "klein hok"
                    ],
                    "target_lang_definitions": {
                        "en": "A small, often enclosed or stuffy space in a building, often used for storage",
                        "ru": "Небольшое, часто замкнутое или тесное помещение, используемое для хранения или работы"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cubbyhole",
                                "target_lang_sense_clarification": "small enclosed space"
                            },
                            {

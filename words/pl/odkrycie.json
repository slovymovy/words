    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "113fc224-6fec-3511-84bc-b11d2e84266a",
                    "sense_definition": "Napotkanie i opisanie prawa, zjawiska lub rzeczy istniejącej w naturze, ale przez nikogo do tej pory nieznanej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "discovery_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To przełomowe <odkrycie> zmieniło całkowicie współczesną medycynę.",
                            "target_lang_translations": {
                                "ru": "Это прорывное <w>открытие</w> полностью изменило современную медицину."
                            }
                        },
                        {
                            "text": "Wielkie <odkrycie> geograficzne pozwoliło na stworzenie dokładniejszych map świata.",
                            "target_lang_translations": {
                                "ru": "Великое географическое <w>открытие</w> позволило создать более точные карты мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "znalezisko"
                    ],
                    "common_phrases": [
                        "dokonać odkrycia",
                        "wielkie odkrycie",
                        "odkrycie naukowe",
                        "odkrycie geograficzne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнаружение и описание закона, явления или предмета, существующего в природе, но ранее неизвестного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытие",
                                "target_lang_sense_clarification": "научное или географическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "350e9c44-27ce-3b53-9c48-eef9875b1b96",
                    "sense_definition": "Ujawnienie czegoś, co dotąd było skrywane, tajne lub nieznane.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "revelation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej przypadkowe <odkrycie> prawdy o rodzinie było dla wszystkich szokiem.",
                            "target_lang_translations": {
                                "ru": "Её случайное <w>открытие</w> правды о семье стало для всех шоком."
                            }
                        },
                        {
                            "text": "Dziennikarze opisali <odkrycie> tajnych dokumentów w archiwum.",
                            "target_lang_translations": {
                                "ru": "Журналисты описали <w>обнаружение</w> секретных документов в архиве."
                            }
                        }
                    ],
                    "synonyms": [
                        "ujawnienie",
                        "rewelacja"
                    ],
                    "antonyms": [
                        "zatajenie"
                    ],
                    "common_phrases": [
                        "szokujące odkrycie",
                        "przypadkowe odkrycie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выявление чего-либо, что до сих пор было скрыто, тайно или неизвестно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнаружение",
                                "target_lang_sense_clarification": "находка"
                            },
                            {
                                "target_lang_word": "раскрытие",
                                "target_lang_sense_clarification": "тайны, правды"
                            },
                            {
                                "target_lang_word": "открытие",
                                "target_lang_sense_clarification": "новой информации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74e52eee-5053-369b-a15d-4a79c43bf52a",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>odkryć</w>: fizyczna czynność zdjęcia zasłony lub pokazania czegoś, co było zakryte.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_uncovering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zapraszamy wszystkich mieszkańców na uroczyste <odkrycie> nowego pomnika.",
                            "target_lang_translations": {
                                "ru": "Приглашаем всех жителей на торжественное <w>открытие</w> нового памятника."
                            }
                        },
                        {
                            "text": "Nagłe <odkrycie> kart przez gracza zakończyło emocjonującą rozgrywkę.",
                            "target_lang_translations": {
                                "ru": "Внезапное <w>раскрытие</w> карт игроком завершило захватывающую игру."
                            }
                        }
                    ],
                    "synonyms": [
                        "odsłonięcie"
                    ],
                    "antonyms": [
                        "zakrycie",
                        "przykrycie"
                    ],
                    "common_phrases": [
                        "odkrycie pomnika",
                        "odkrycie kart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое действие снятия покрова или демонстрации того, что было закрыто."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытие",
                                "target_lang_sense_clarification": "памятника, торжественное"
                            },
                            {
                                "target_lang_word": "раскрытие",
                                "target_lang_sense_clarification": "карт, снятие покрова"
                            }
                        ]
                    }
                }
            ]
        }
    ]

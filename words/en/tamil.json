                        "Indian Tamils"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, происходящий из штата Тамилнад (Индия) или Шри-Ланки, или имеющий тамильское происхождение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тами́л",
                                "target_lang_sense_clarification": "мужской род / общее"
                            },
                            {
                                "target_lang_word": "тами́лка",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8346b8cd-1404-32a3-9f18-8320075a7353",
                    "sense_definition": "vloeistof of gas doorlatend omdat er een gat of scheur in zit",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan niet met de fiets naar school omdat ik een <w>lekke</w> band heb.",
                            "target_lang_translations": {
                                "en": "I can't go to school by bike because I have a <w>flat</w> tire.",
                                "ru": "Я не могу поехать в школу на велосипеде, потому что у меня <w>спущенное</w> колесо."
                            }
                        },
                        {
                            "text": "Door het <w>lekke</w> dak kwam er veel water de zolder op.",
                            "target_lang_translations": {
                                "en": "A lot of water came into the attic through the <w>leaky</w> roof.",
                                "ru": "Через <w>дырявую</w> крышу на чердак попало много воды."
                            }
                        }
                    ],
                    "antonyms": [
                        "waterdicht",
                        "luchtdicht"
                    ],
                    "common_phrases": [
                        "lekke band",
                        "lek stoten",
                        "lek schieten"
                    ],
                    "target_lang_definitions": {
                        "en": "Permeable to liquid or gas due to a hole or crack",
                        "ru": "пропускающий жидкость или газ из-за наличия отверстия или трещины"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leaky",
                                "target_lang_sense_clarification": "allowing fluid or gas to pass through"
                            },
                            {
                                "target_lang_word": "punctured",
                                "target_lang_sense_clarification": "having a hole (e.g., a tire)"
                            },
                            {
                                "target_lang_word": "flat",
                                "target_lang_sense_clarification": "deflated due to a leak (specifically for tires)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дырявый",
                                "target_lang_sense_clarification": "имеющий дыры"
                            },
                            {
                                "target_lang_word": "протекающий",
                                "target_lang_sense_clarification": "пропускающий воду"
                            },
                            {
                                "target_lang_word": "спущенный",
                                "target_lang_sense_clarification": "о шине или мяче"
                            }
                        ]
                    }
                }
            ]
        },
        {

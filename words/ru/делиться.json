            "senses": [
                {
                    "sense_id": "4bbfef4e-444d-3856-916e-6aea096e8011",
                    "sense_definition": "Давать часть того, что имеешь, другим людям для совместного использования.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "share_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Брат всегда <делится> своими игрушками с младшей сестрой."
                        },
                        {
                            "text": "Мы привыкли <делиться> едой во время похода."
                        }
                    ],
                    "common_phrases": [
                        "делиться едой",
                        "делиться всем",
                        "делиться деньгами"
                    ]
                },
                {
                    "sense_id": "5fe23e1b-f64a-3d7b-bf7c-c2384bd8ca63",
                    "sense_definition": "Быть кратным какому-либо числу, то есть давать целое число при делении (в математике).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "division_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Число десять <делится> на пять без остатка."
                        },
                        {
                            "text": "Это сложное число не <делится> на три."
                        }
                    ],
                    "common_phrases": [
                        "делиться на два",
                        "делиться без остатка",
                        "делиться на три"
                    ]
                },
                {
                    "sense_id": "47ff507f-3699-3fef-a5b9-dc840b4420cc",
                    "sense_definition": "Рассказывать кому-либо о своих мыслях, чувствах, планах или новостях.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "share_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она любит <делиться> впечатлениями после каждого путешествия."
                        },
                        {
                            "text": "Коллеги часто <делятся> опытом друг с другом."
                        }
                    ],
                    "synonyms": [
                        "рассказывать",
                        "сообщать"
                    ],
                    "common_phrases": [
                        "делиться впечатлениями",
                        "делиться опытом",
                        "делиться новостями",
                        "делиться секретами"
                    ]
                },
                {
                    "sense_id": "7eb7e0c9-7b5b-377c-8664-4aac81718e7c",
                    "sense_definition": "Распадаться на отдельные части или группы; разъединяться.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "division_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Большая река <делится> на несколько маленьких рукавов."
                        },
                        {
                            "text": "Класс <делится> на две группы для урока английского языка."
                        }
                    ],
                    "synonyms": [
                        "разделяться",
                        "распадаться",
                        "разъединяться"
                    ],
                    "antonyms": [
                        "сохраняться"
                    ],
                    "common_phrases": [
                        "делиться на группы",
                        "делиться на части",
                        "делиться на лагеря"
                    ]
                }
            ]

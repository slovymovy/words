                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "видеть",
                                "target_lang_sense_clarification": "смотреть (фильм, спектакль)"
                            },
                            {
                                "target_lang_word": "смотреть",
                                "target_lang_sense_clarification": "наблюдать за чем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26186355-67af-3c55-af68-0ee1731361e2",
                    "sense_definition": "Rozumieć coś, uświadamiać sobie pewne fakty lub sytuację",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Widzę</w>, że jesteś dzisiaj bardzo zmęczony i potrzebujesz odpoczynku.",
                            "target_lang_translations": {
                                "ru": "<w>Вижу</w>, что ты сегодня очень устал и нуждаешься в отдыхе."
                            }
                        },
                        {
                            "text": "Zupełnie nie <w>widzę</w> sensu w kontynuowaniu tej bezowocnej dyskusji.",
                            "target_lang_translations": {
                                "ru": "Я совершенно не <w>вижу</w> смысла в продолжении этой бесплодной дискуссии."
                            }
                        }
                    ],
                    "synonyms": [
                        "zauważać"
                    ],
                    "common_phrases": [
                        "jak widzę",
                        "widzieć różnicę",
                        "widzieć problem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать что-либо, осознавать факты или ситуацию"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "видеть",
                                "target_lang_sense_clarification": "понимать, осознавать"
                            },
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "постигать смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfcf685b-db9b-362d-a7d4-17506eb03486",
                    "sense_definition": "Wyobrażać sobie kogoś lub coś w określonej roli; mieć pogląd na przyszłość lub rozwój sytuacji",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo jego starań, nie <w>widzę</w> go na stanowisku kierownika zespołu.",
                            "target_lang_translations": {
                                "ru": "Несмотря на его старания, я не <w>вижу</w> его на должности руководителя команды."
                            }
                        },
                        {
                            "text": "Rodzice od zawsze <w>widzieli</w> moją przyszłość w medycynie lub prawie.",
                            "target_lang_translations": {
                                "ru": "Родители всегда <w>видели</w> моё будущее в медицине или праве."
                            }
                        }
                    ],
                    "common_phrases": [
                        "widzieć to czarno",
                        "widzieć kogoś w roli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представлять кого-то или что-то в определённой роли; иметь мнение о будущем"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "видеть",
                                "target_lang_sense_clarification": "представлять, воображать"
                            },
                            {
                                "target_lang_word": "представлять",
                                "target_lang_sense_clarification": "мысленно воображать"
                            }
                        ]
                    }
                }

            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "04c4974c-dc1e-36ef-b654-00b86f13eed1",
                    "sense_definition": "Не постоянно, в течение ограниченного, короткого срока.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Магазин <w>временно</w> закрыт на ремонт до следующего понедельника.",
                            "target_lang_translations": {
                                "en": "The shop is <w>temporarily</w> closed for repairs until next Monday."
                            }
                        },
                        {
                            "text": "Он <w>временно</w> живёт у друзей, пока ищет новую квартиру.",
                            "target_lang_translations": {
                                "en": "He is <w>temporarily</w> living with friends while looking for a new apartment."
                            }
                        }
                    ],
                    "antonyms": [
                        "постоянно"
                    ],
                    "common_phrases": [
                        "временно недоступен",
                        "временно не работает",
                        "временно исполняющий обязанности",
                        "временно закрыто"
                    ],
                    "target_lang_definitions": {
                        "en": "Not permanently; for a limited, short period of time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "temporarily",
                                "target_lang_sense_clarification": "for a limited time"
                            },
                            {
                                "target_lang_word": "provisionally",
                                "target_lang_sense_clarification": "subject to later change"
                            },
                            {
                                "target_lang_word": "for the time being",
                                "target_lang_sense_clarification": "until a situation changes"
                            }
                        ]
                    }
                }
            ]
        },
        {

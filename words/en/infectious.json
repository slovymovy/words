{
    "word_family": [
        "infect",
        "infection",
        "infective",
        "infectiousness",
        "infectiously",
        "disinfect",
        "disinfectant",
        "disinfection"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "31caba06-79b8-3abd-8924-a386b738076f",
                    "sense_definition": "Caused by bacteria or viruses and capable of being transmitted to others.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medical_transmission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Malaria is a serious <w>infectious</w> disease spread by mosquitoes.",
                            "target_lang_translations": {
                                "ru": "Малярия — это серьезное <w>инфекционное</w> заболевание, распространяемое комарами."
                            }
                        },
                        {
                            "text": "Cancer is usually not <w>infectious</w>.",
                            "target_lang_translations": {
                                "ru": "Рак обычно не является <w>инфекционным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "contagious",
                        "communicable"
                    ],
                    "antonyms": [
                        "non-infectious"
                    ],
                    "common_phrases": [
                        "infectious disease",
                        "infectious agent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызываемый бактериями или вирусами и способный передаваться другим (о болезни)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инфекцио́нный",
                                "target_lang_sense_clarification": "медицинский термин"
                            },
                            {
                                "target_lang_word": "зара́зный",
                                "target_lang_sense_clarification": "общеупотребительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a4bf046-640e-3237-ba5c-8d08fa2066de",
                    "sense_definition": "Able to pass a disease or virus to other people.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medical_transmission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Despite feeling better, the patient is still <w>infectious</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на улучшение самочувствия, пациент все еще <w>заразен</w>."
                            }
                        },
                        {
                            "text": "You should stay home from school while you are highly <w>infectious</w>.",
                            "target_lang_translations": {
                                "ru": "Тебе следует оставаться дома и не ходить в школу, пока ты очень <w>заразен</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "contagious"
                    ],
                    "antonyms": [
                        "non-infectious"
                    ],
                    "common_phrases": [
                        "highly infectious",
                        "remain infectious"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный передавать болезнь или вирус другим людям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зара́зный",
                                "target_lang_sense_clarification": "о человеке или животном"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f576a3a1-7ec2-3526-a901-093a8f238200",
                    "sense_definition": "Likely to spread quickly to others, such as laughter, enthusiasm, or a mood.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her enthusiasm for the new project can be really <w>infectious</w>.",
                            "target_lang_translations": {
                                "ru": "Ее энтузиазм по поводу нового проекта может быть действительно <w>заразительным</w>."
                            }
                        },
                        {
                            "text": "He had such an <w>infectious</w> laugh that everyone in the room started giggling.",
                            "target_lang_translations": {
                                "ru": "У него был такой <w>заразительный</w> смех, что все в комнате начали хихикать."
                            }
                        }
                    ],
                    "synonyms": [
                        "catching"
                    ],
                    "common_phrases": [
                        "infectious laugh",
                        "infectious smile",
                        "infectious enthusiasm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легко передающийся окружающим (о смехе, энтузиазме или настроении)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарази́тельный",
                                "target_lang_sense_clarification": "об эмоциях и поведении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "371dc47e-48e9-3a38-b2b5-ee0a689853db",
                    "sense_definition": "Exciting and easy to remember, often referring to music or rhythm.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band played a new song with an <w>infectious</w> rhythm.",
                            "target_lang_translations": {
                                "ru": "Группа сыграла новую песню с <w>заразительным</w> ритмом."
                            }
                        },
                        {
                            "text": "Pop music is often more <w>infectious</w> than classical music.",
                            "target_lang_translations": {
                                "ru": "Поп-музыка часто более <w>заразительна</w>, чем классическая."
                            }
                        }
                    ],
                    "synonyms": [
                        "catchy"
                    ],
                    "common_phrases": [
                        "infectious rhythm",
                        "infectious beat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Захватывающий и легко запоминающийся (часто о музыке или ритме)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарази́тельный",
                                "target_lang_sense_clarification": "о ритме"
                            },
                            {
                                "target_lang_word": "зажига́тельный",
                                "target_lang_sense_clarification": "о музыке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
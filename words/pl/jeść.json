            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b1a6c901-feb8-3792-8ff6-52f7b965d07f",
                    "sense_definition": "Przyjmować pokarm do ust, gryźć go i połykać w celu zaspokojenia głodu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano <jem> pożywne śniadanie przed wyjściem do pracy.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <ем> питательный завтрак перед уходом на работу."
                            }
                        },
                        {
                            "text": "Lekarz zalecił mi, abym <jadł> więcej świeżych warzyw i owoców.",
                            "target_lang_translations": {
                                "ru": "Врач посоветовал мне <есть> больше свежих овощей и фруктов."
                            }
                        },
                        {
                            "text": "W niedzielę zazwyczaj <jemy> obiad wspólnie z całą rodziną.",
                            "target_lang_translations": {
                                "ru": "В воскресенье мы обычно <едим> обед всей семьёй."
                            }
                        }
                    ],
                    "synonyms": [
                        "spożywać",
                        "konsumować",
                        "posilać się",
                        "zjadać",
                        "wcinać",
                        "szamać"
                    ],
                    "antonyms": [
                        "pić"
                    ],
                    "common_phrases": [
                        "jeść śniadanie",
                        "jeść obiad",
                        "jeść kolację",
                        "jeść oczami",
                        "jeść za dwóch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать пищу, пережёвывать и глотать её для утоления голода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "основное слово"
                            },
                            {
                                "target_lang_word": "кушать",
                                "target_lang_sense_clarification": "вежливая или разговорная форма"
                            },
                            {
                                "target_lang_word": "питаться",
                                "target_lang_sense_clarification": "принимать пищу регулярно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "483424ff-2718-325f-a950-d77246adbb01",
                    "sense_definition": "Niszczyć strukturę czegoś poprzez powolne działanie czynników chemicznych lub fizycznych (np. o rdzy lub kwasie).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wilgoć i brak zabezpieczenia sprawiły, że rdza powoli <je> metalowe ogrodzenie.",
                            "target_lang_translations": {
                                "ru": "Влага и отсутствие защиты привели к тому, что ржавчина медленно <разъедает> металлический забор."
                            }
                        },
                        {
                            "text": "Ten silny kwas może <jeść> nawet bardzo twarde materiały.",
                            "target_lang_translations": {
                                "ru": "Эта сильная кислота может <разъедать> даже очень твёрдые материалы."
                            }
                        }
                    ],
                    "synonyms": [
                        "niszczyć",
                        "trawić",
                        "korodować",
                        "nadżerać",
                        "wżerać się"
                    ],
                    "common_phrases": [
                        "rdza je metal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушать структуру чего-либо под воздействием химических или физических факторов (например, о ржавчине или кислоте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разъедать",
                                "target_lang_sense_clarification": "о химическом воздействии или коррозии"
                            },
                            {
                                "target_lang_word": "проедать",
                                "target_lang_sense_clarification": "делать дыру, разрушая материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "848dba26-0334-341b-9175-3796d02cb58b",
                    "sense_definition": "Dręczyć kogoś psychicznie, wywoływać ciągły niepokój, smutek lub wyrzuty sumienia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "psychological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ogromne poczucie winy <jadło> go od środka przez wiele lat.",
                            "target_lang_translations": {
                                "ru": "Огромное чувство вины <глодало> его изнутри много лет."
                            }
                        },
                        {
                            "text": "Niepewność o losy bliskich <jadła> jej serce każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Неизвестность о судьбе близких <терзала> её сердце каждый день."
                            }
                        }
                    ],
                    "synonyms": [
                        "dręczyć",
                        "trapić",
                        "gryźć",
                        "nękać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used metaphorically"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мучить кого-либо морально, вызывать постоянную тревогу, грусть или угрызения совести."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грызть",
                                "target_lang_sense_clarification": "перен. мучить, не давать покоя"
                            },
                            {
                                "target_lang_word": "терзать",
                                "target_lang_sense_clarification": "сильно мучить"
                            },
                            {
                                "target_lang_word": "съедать",
                                "target_lang_sense_clarification": "изводить (обычно о тоске или болезни)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bec5931f-e5d3-3a2e-a4cf-8dbf00efaaa4",
                    "sense_definition": "Kąsać, gryźć lub żądlić, powodując ból i swędzenie (zwykle o owadach).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy uciekać z lasu, bo komary strasznie tu <jedzą>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно бежать из леса, потому что комары здесь страшно <едят>."
                            }
                        },
                        {
                            "text": "Posmaruj się środkiem odstraszającym, żeby meszki cię nie <jadły>.",
                            "target_lang_translations": {
                                "ru": "Намажься репеллентом, чтобы мошки тебя не <кусали>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gryźć",
                        "kąsać",
                        "ciąć",
                        "żądlić"
                    ],
                    "common_phrases": [
                        "komary jedzą"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "standard word is 'gryźć'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусать или жалить, причиняя боль и зуд (обычно о насекомых)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кусать",
                                "target_lang_sense_clarification": "о насекомых или животных"
                            },
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "разг. о множественных укусах насекомых"
                            },
                            {
                                "target_lang_word": "жалить",
                                "target_lang_sense_clarification": "о пчёлах, осах, крапиве"
                            }
                        ]
                    }
                }
            ]
        }

                {
                    "sense_id": "10aa76b5-cccd-3757-b33d-fae98a67728f",
                    "sense_definition": "Płaski, prostokątny element konstrukcyjny lub dekoracyjny, używany najczęściej do wykańczania podłóg lub ścian.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "construction_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ten weekend planujemy położyć nowe <panele> w salonie.",
                            "target_lang_translations": {
                                "ru": "В эти выходные мы планируем уложить новый <ламинат> в гостиной."
                            }
                        },
                        {
                            "text": "Jasne <panele> podłogowe sprawiają, że pokój wydaje się większy.",
                            "target_lang_translations": {
                                "ru": "Светлые напольные <панели> делают комнату визуально больше."
                            }
                        }
                    ],
                    "synonyms": [
                        "płyta",
                        "deszczułka"
                    ],
                    "common_phrases": [
                        "panele podłogowe",
                        "panele ścienne",
                        "układać panele",
                        "panele laminowane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский прямоугольный конструктивный или декоративный элемент, используемый чаще всего для отделки полов или стен."

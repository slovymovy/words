                    "target_lang_definitions": {
                        "ru": "Бывшее рыбацкое поселение и природный заповедник в Польше, в Поморском воеводстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бека",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        }

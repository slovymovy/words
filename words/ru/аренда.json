    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c41273b2-5e4f-318f-8cd7-77f3b38f8b87",
                    "sense_definition": "Наём имущества, помещения или территории во временное пользование за определённую плату.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "lease_contract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили взять в <аренду> просторный офис в центре города.",
                            "target_lang_translations": {
                                "en": "We decided to <lease> a spacious office in the city center."
                            }
                        },
                        {
                            "text": "Срок <аренды> квартиры истекает в следующем месяце.",
                            "target_lang_translations": {
                                "en": "The apartment <lease> expires next month."
                            }
                        }
                    ],
                    "synonyms": [
                        "наём"
                    ],
                    "common_phrases": [
                        "сдать в аренду",
                        "взять в аренду",
                        "договор аренды",
                        "срок аренды"
                    ],
                    "target_lang_definitions": {
                        "en": "The hiring of property, premises, or territory for temporary use for a certain fee."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lease",
                                "target_lang_sense_clarification": "contract for renting property"
                            },
                            {
                                "target_lang_word": "tenancy",
                                "target_lang_sense_clarification": "possession of property as a tenant"
                            },
                            {
                                "target_lang_word": "rental",
                                "target_lang_sense_clarification": "act of renting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a20d019a-2a9c-340f-824d-d5282ff02df9",
                    "sense_definition": "Плата за временное пользование чужим имуществом или помещением.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "lease_payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Владелец магазина поднял <аренду> на десять процентов.",
                            "target_lang_translations": {
                                "en": "The shop owner raised the <rent> by ten percent."
                            }
                        },
                        {
                            "text": "Нам нужно заплатить <аренду> до пятого числа.",
                            "target_lang_translations": {
                                "en": "We need to pay the <rent> by the fifth."
                            }
                        }
                    ],
                    "common_phrases": [
                        "платить аренду",
                        "стоимость аренды",
                        "высокая аренда"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное употребление в значении «плата»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Payment for the temporary use of someone else's property or premises."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rent",
                                "target_lang_sense_clarification": "payment for the use of property"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40f51da6-1890-348d-871c-0c001d8f9a81",
                    "sense_definition": "Вид государственной награды в дореволюционной России, дававший право на получение дохода с казённых земель.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_award",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За свои заслуги генерал получил богатую <аренду>.",
                            "target_lang_translations": {
                                "en": "For his services, the general received a rich <lease>."
                            }
                        },
                        {
                            "text": "Император пожаловал ему <аренду> вместо ордена.",
                            "target_lang_translations": {
                                "en": "The emperor granted him a <lease> instead of an order."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "исторический термин"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A type of state award in pre-revolutionary Russia granting the right to receive income from state lands."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lease",
                                "target_lang_sense_clarification": "historical grant of income from state lands"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
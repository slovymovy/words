                    "semantic_group_id": "praise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Поэты любили <w>петь</w> красоту родной природы.",
                            "target_lang_translations": {
                                "en": "Poets loved to <w>sing</w> the beauty of their native nature."
                            }

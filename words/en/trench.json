    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a0dfb1bd-07d6-34c1-aa40-6fcdf4204982",
                    "sense_definition": "A long, narrow ditch or hole dug in the ground, often for laying pipes or drainage.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "excavation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The workers dug a deep <w>trench</w> to lay the new water pipes.",
                            "target_lang_translations": {
                                "ru": "Рабочие вырыли глубокую <w>траншею</w>, чтобы проложить новые водопроводные трубы."
                            }
                        },
                        {
                            "text": "Rainwater flowed along the <w>trench</w> at the side of the road.",
                            "target_lang_translations": {
                                "ru": "Дождевая вода текла по <w>канаве</w> на обочине дороги."
                            }
                        }
                    ],
                    "synonyms": [
                        "ditch",
                        "channel",
                        "furrow",
                        "cut"
                    ],
                    "antonyms": [
                        "embankment",
                        "mound"
                    ],
                    "common_phrases": [
                        "dig a trench",
                        "deep trench",
                        "drainage trench"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная узкая выемка в земле, часто используемая для прокладки труб или дренажа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "траншея",
                                "target_lang_sense_clarification": "строительство, коммуникации"
                            },
                            {
                                "target_lang_word": "канава",
                                "target_lang_sense_clarification": "дренаж, сток воды"
                            },
                            {
                                "target_lang_word": "ров",
                                "target_lang_sense_clarification": "глубокая яма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32982bc4-452d-3fe4-b836-eff867812ab9",
                    "sense_definition": "A system of deep, narrow excavations used in warfare to protect soldiers from enemy fire.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "excavation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Soldiers lived in the muddy <w>trenches</w> for weeks during the war.",
                            "target_lang_translations": {
                                "ru": "Солдаты неделями жили в грязных <w>окопах</w> во время войны."
                            }
                        },
                        {
                            "text": "The general ordered the troops to stay in the <w>trenches</w> until nightfall.",
                            "target_lang_translations": {
                                "ru": "Генерал приказал войскам оставаться в <w>окопах</w> до наступления темноты."
                            }
                        }
                    ],
                    "synonyms": [
                        "fortification",
                        "dugout"
                    ],
                    "common_phrases": [
                        "in the trenches",
                        "trench warfare"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "military context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Система глубоких узких укреплений, используемых в военное время для защиты солдат от огня противника."
                    },
                    "translations": {

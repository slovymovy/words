{
    "word_family": [
        "kapłanka",
        "kapłaństwo",
        "kapłański"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5e2ad603-0ba3-31d4-83cb-a2cd2e2b8bf3",
                    "sense_definition": "Duchowny chrześcijański, który przyjął święcenia i ma prawo do odprawiania mszy oraz udzielania sakramentów",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody <kapłan> wygłosił dzisiaj bardzo poruszające kazanie.",
                            "target_lang_translations": {
                                "ru": "Молодой <священник> произнес сегодня очень трогательную проповедь."
                            }
                        },
                        {
                            "text": "Wierni czekali w kościele, aż <kapłan> rozpocznie uroczystą mszę.",
                            "target_lang_translations": {
                                "ru": "Верующие ждали в церкви, когда <священник> начнет торжественную мессу."
                            }
                        }
                    ],
                    "synonyms": [
                        "ksiądz",
                        "duchowny"
                    ],
                    "common_phrases": [
                        "kapłan diecezjalny",
                        "posługa kapłana",
                        "święcenia kapłana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Христианский священнослужитель, принявший сан и имеющий право совершать богослужения и таинства"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "священник",
                                "target_lang_sense_clarification": "общее название служителя церкви"
                            },
                            {
                                "target_lang_word": "священнослужитель",
                                "target_lang_sense_clarification": "более формальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58d10c6a-e63e-33d0-8110-8d2cd116d61c",
                    "sense_definition": "Mężczyzna pełniący funkcje religijne, sprawujący kult i składający ofiary, zwłaszcza w religiach starożytnych lub politeistycznych",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starożytny <kapłan> złożył dary na ołtarzu, aby przebłagać bogów.",
                            "target_lang_translations": {
                                "ru": "Древний <жрец> возложил дары на алтарь, чтобы умилостивить богов."
                            }
                        },
                        {
                            "text": "Najwyższy <kapłan> świątyni był odpowiedzialny za przebieg całego rytuału.",
                            "target_lang_translations": {
                                "ru": "Верховный <жрец> храма отвечал за ход всего ритуала."
                            }
                        }
                    ],
                    "synonyms": [
                        "duchowny"
                    ],
                    "common_phrases": [
                        "najwyższy kapłan",
                        "kapłan świątyni",
                        "szaty kapłana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служитель культа, совершающий обряды и жертвоприношения, особенно в древних или языческих религиях"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жрец",
                                "target_lang_sense_clarification": "в языческих культах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8b3c8c0-ebcf-3981-9f3f-311b564b651a",
                    "sense_definition": "Osoba z wielkim oddaniem służąca jakiejś wzniosłej idei, sztuce lub sprawie",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Był to prawdziwy <kapłan> sztuki, który całe życie poświęcił malarstwu.",
                            "target_lang_translations": {
                                "ru": "Это был настоящий <жрец> искусства, посвятивший всю жизнь живописи."
                            }
                        },
                        {
                            "text": "Jako <kapłan> sprawiedliwości zawsze stawał w obronie pokrzywdzonych.",
                            "target_lang_translations": {
                                "ru": "Как <служитель> справедливости, он всегда вставал на защиту обиженных."
                            }
                        }
                    ],
                    "synonyms": [
                        "głosiciel",
                        "apostoł",
                        "krzewiciel"
                    ],
                    "common_phrases": [
                        "kapłan sztuki",
                        "kapłan nauki",
                        "kapłan ogniska domowego"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often used in literary or lofty contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, с великой преданностью служащий какой-либо возвышенной идее, искусству или делу"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жрец",
                                "target_lang_sense_clarification": "перен. высокий стиль (напр. жрец искусства)"
                            },
                            {
                                "target_lang_word": "служитель",
                                "target_lang_sense_clarification": "преданный последователь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
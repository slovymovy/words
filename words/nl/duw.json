            "pos": "noun",
            "senses": [
                {
                    "sense_id": "57058f7b-f17f-3416-b1be-2f36943b184a",
                    "sense_definition": "Een fysieke zet of stoot om iets of iemand te verplaatsen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gaf de auto een harde <w>duw</w> om hem te starten.",
                            "target_lang_translations": {
                                "en": "He gave the car a hard <w>push</w> to start it.",
                                "ru": "Он дал машине сильный <w>толчок</w>, чтобы завести её."
                            }
                        },
                        {
                            "text": "Tijdens het spel kreeg ik per ongeluk een <w>duw</w> van een andere speler.",
                            "target_lang_translations": {
                                "en": "During the game, I accidentally got a <w>push</w> from another player.",
                                "ru": "Во время игры я случайно получил <w>толчок</w> от другого игрока."
                            }
                        }
                    ],
                    "synonyms": [
                        "zet",
                        "stoot"
                    ],
                    "antonyms": [
                        "trek"
                    ],
                    "common_phrases": [
                        "een duw geven",
                        "een duwtje in de rug"
                    ],
                    "target_lang_definitions": {
                        "en": "A physical push or shove to move something or someone.",
                        "ru": "Физический толчок или удар с целью сдвинуть что-то или кого-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "push",
                                "target_lang_sense_clarification": "general act of applying force"
                            },
                            {
                                "target_lang_word": "shove",
                                "target_lang_sense_clarification": "rough or forceful push"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "толчок",
                                "target_lang_sense_clarification": "резкое движение от себя"
                            }
                        ]
                    }
                }
            ]
        },

                    ],
                    "common_phrases": [
                        "снарядить невесту"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "regional or archaic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To dress someone beautifully or elaborately."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dress up",
                                "target_lang_sense_clarification": "to dress in smart or formal clothes"
                            },
                            {
                                "target_lang_word": "deck out",
                                "target_lang_sense_clarification": "to dress or adorn, especially in a showy way"
                            }
                        ]
                    }
                }
            ]
        }
    ]

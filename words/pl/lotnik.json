                },
                {
                    "sense_id": "bf5c898f-da66-32e8-878d-6d90e7448147",
                    "sense_definition": "Żołnierz pełniący służbę w wojskach lotniczych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_aviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody <w>lotnik</w> zasalutował dowódcy przed wejściem na pokład myśliwca.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>лётчик</w> отдал честь командиру перед тем, как сесть в истребитель."
                            }
                        },
                        {
                            "text": "W tej bazie wojskowej stacjonują najlepsi <w>lotnicy</w> w całym kraju.",
                            "target_lang_translations": {
                                "ru": "На этой военной базе базируются лучшие <w>лётчики</w> страны."
                            }
                        }
                    ],

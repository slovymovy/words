            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0bac25dc-ad42-3116-aed5-72bc4ae02d9a",
                    "sense_definition": "Частое судорожное сокращение мышц, возникающее от холода, страха или сильного волнения",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От пронизывающего ветра меня пробирала сильная <w>дрожь</w>.",
                            "target_lang_translations": {
                                "en": "A strong <w>shiver</w> ran through me from the piercing wind."
                            }
                        },
                        {
                            "text": "Неожиданная новость вызвала у неё нервную <w>дрожь</w> в руках.",
                            "target_lang_translations": {
                                "en": "The unexpected news caused a nervous <w>trembling</w> in her hands."
                            }
                        }
                    ],
                    "synonyms": [
                        "трепет",
                        "дрожание"
                    ],
                    "common_phrases": [
                        "бросает в дрожь",
                        "мелкая дрожь",
                        "дрожь по телу",
                        "бить дрожь"
                    ],
                    "target_lang_definitions": {
                        "en": "A trembling or shivering of the body caused by cold, fear, or excitement"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shiver",
                                "target_lang_sense_clarification": "caused by cold or fear"
                            },
                            {
                                "target_lang_word": "trembling",
                                "target_lang_sense_clarification": "caused by emotion or weakness"
                            },
                            {
                                "target_lang_word": "shudder",
                                "target_lang_sense_clarification": "sudden and violent, often due to disgust or fear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04fc5c76-1b09-3d79-9a5a-8ec9798c106b",
                    "sense_definition": "Ритмичное сотрясение или вибрация твердых предметов, механизмов или поверхностей",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При запуске мощного двигателя по корпусу корабля прошла <w>дрожь</w>.",
                            "target_lang_translations": {
                                "en": "A <w>shudder</w> ran through the ship's hull when the powerful engine started."
                            }
                        },
                        {
                            "text": "От громкой музыки ощущалась неприятная <w>дрожь</w> в стенах комнаты.",
                            "target_lang_translations": {
                                "en": "An unpleasant <w>vibration</w> was felt in the room's walls from the loud music."
                            }
                        }
                    ],
                    "synonyms": [
                        "сотрясение",
                        "тряска"
                    ],
                    "common_phrases": [
                        "дрожь земли",
                        "дрожь стекол",
                        "дрожь стен"
                    ],
                    "target_lang_definitions": {
                        "en": "Rhythmic shaking or vibration of solid objects or surfaces"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vibration",
                                "target_lang_sense_clarification": "mechanical or physical movement"
                            },
                            {
                                "target_lang_word": "shudder",
                                "target_lang_sense_clarification": "of heavy objects or structures"
                            },
                            {
                                "target_lang_word": "tremor",
                                "target_lang_sense_clarification": "of the ground or walls"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d6debe7-ec08-313e-9c84-dd935c7ff0b6",
                    "sense_definition": "Неустойчивое движение света, звука или поверхности воды; мерцание",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Над раскаленной дорогой в полдень была видна струящаяся <w>дрожь</w> воздуха.",
                            "target_lang_translations": {
                                "en": "A flowing <w>shimmer</w> of air was visible over the scorching road at noon."
                            }
                        },
                        {
                            "text": "Легкая <w>дрожь</w> свечи создавала таинственные тени на стене.",
                            "target_lang_translations": {

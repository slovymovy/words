    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c278b9f-bc5e-390d-af66-fa7504820fd1",
                    "sense_definition": "Osoba będąca dzieckiem, wnukiem lub dalszym następcą w linii prostej w stosunku do swoich przodków.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Król martwił się brakiem męskiego <w>potomka</w>, który mógłby przejąć tron.",
                            "target_lang_translations": {
                                "ru": "Король беспокоился об отсутствии <w>потомка</w> мужского пола, который мог бы унаследовать трон."
                            }
                        },
                        {
                            "text": "Jako jedyny <w>potomek</w> słynnego malarza, odziedziczył wszystkie jego obrazy.",
                            "target_lang_translations": {
                                "ru": "Как единственный <w>потомок</w> известного художника, он унаследовал все его картины."
                            }
                        }
                    ],
                    "synonyms": [
                        "zstępny",
                        "descendent"
                    ],
                    "antonyms": [
                        "przodek",
                        "antenat",
                        "ascendent"
                    ],
                    "common_phrases": [
                        "męski potomek",
                        "bezpośredni potomek",
                        "ostatni potomek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, происходящий по рождению от кого-либо (ребенок, внук и т. д. по отношению к предкам)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потомок",
                                "target_lang_sense_clarification": "прямой родственник"
                            },
                            {
                                "target_lang_word": "наследник",
                                "target_lang_sense_clarification": "продолжатель рода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2667fe8f-0d7d-302a-9e20-8bb44877dc00",
                    "sense_definition": "Język, który wyewoluował z wcześniejszego języka, zachowując z nim ciągłość historyczną.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "językoznawstwo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Języki romańskie są <w>potomkami</w> języka łacińskiego używanego w starożytności.",
                            "target_lang_translations": {
                                "ru": "Романские языки являются <w>потомками</w> латинского языка, использовавшегося в древности."
                            }
                        },
                        {
                            "text": "Współczesny język polski jest <w>potomkiem</w> języka prasłowiańskiego.",
                            "target_lang_translations": {
                                "ru": "Современный польский язык является <w>потомком</w> праславянского языка."
                            }
                        }
                    ],
                    "synonyms": [
                        "kontynuant"
                    ],
                    "common_phrases": [
                        "język potomek",
                        "potomek łaciny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Язык, который развился из более раннего языка, сохраняя с ним историческую преемственность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потомок",
                                "target_lang_sense_clarification": "язык, произошедший от другого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "243dc747-1d8c-3b92-8e22-389bf2d84221",
                    "sense_definition": "Słowo lub forma gramatyczna, która wywodzi się z odpowiednika w języku przodków.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "językoznawstwo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czasownik ten jest bezpośrednim <w>potomkiem</w> prasłowiańskiego przymiotnika.",
                            "target_lang_translations": {
                                "ru": "Этот глагол является прямым <w>потомком</w> праславянского прилагательного."
                            }
                        },
                        {
                            "text": "Wiele angielskich słów to <w>potomkowie</w> terminów pochodzących z języka francuskiego.",
                            "target_lang_translations": {
                                "ru": "Многие английские слова — это <w>потомки</w> терминов, происходящих из французского языка."
                            }
                        }
                    ],
                    "synonyms": [
                        "kontynuant"
                    ],
                    "common_phrases": [
                        "fonetyczny potomek",
                        "potomek słowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово или грамматическая форма, происходящие от соответствующей единицы в языке-предке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потомок",
                                "target_lang_sense_clarification": "лингвистический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

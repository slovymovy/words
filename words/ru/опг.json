            "senses": [
                {
                    "sense_id": "9eed2d16-054d-39f8-842e-0da3b2f026dc",
                    "sense_definition": "Сокращение от <w>организованная</w> <w>преступная</w> <w>группировка</w>; устойчивая группа лиц, заранее объединившихся для совершения преступлений.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "criminal_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция задержала лидера крупной <w>ОПГ</w> при попытке пересечь границу.",
                            "target_lang_translations": {
                                "en": "The police detained the leader of a large <w>organized crime group</w> while he was attempting to cross the border."
                            }
                        },
                        {
                            "text": "Члены этой <w>ОПГ</w> занимались вымогательством и финансовыми махинациями.",
                            "target_lang_translations": {
                                "en": "Members of this <w>criminal gang</w> were engaged in extortion and financial fraud."
                            }
                        }
                    ],
                    "synonyms": [
                        "банда",
                        "группировка",
                        "мафия"
                    ],
                    "common_phrases": [
                        "лидер ОПГ",
                        "члены ОПГ",
                        "ликвидация ОПГ",
                        "состав ОПГ"
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for \"organized criminal group\"; a stable group of individuals united in advance to commit crimes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "organized crime group",
                                "target_lang_sense_clarification": "formal term for a criminal organization"
                            },
                            {
                                "target_lang_word": "criminal gang",
                                "target_lang_sense_clarification": "general term for a group of criminals"
                            },
                            {
                                "target_lang_word": "OCG",
                                "target_lang_sense_clarification": "acronym for Organized Crime Group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "801654bf-d08e-3ea3-b2d3-cb498558f3f3",
                    "sense_definition": "Сокращение от <w>оперативно-поисковая</w> <w>группа</w>; подразделение силовых структур для розыска и задержания.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "law_enforcement_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На место происшествия срочно выехала <w>ОПГ</w> с собаками.",
                            "target_lang_translations": {
                                "en": "An <w>operational search group</w> with dogs was urgently dispatched to the scene."
                            }
                        },
                        {
                            "text": "В задачи <w>ОПГ</w> входило прочесывание лесного массива.",
                            "target_lang_translations": {
                                "en": "The <w>search party</w>'s tasks included combing the forest area."
                            }
                        }
                    ],
                    "synonyms": [
                        "опергруппа"
                    ],
                    "common_phrases": [
                        "выезд ОПГ",
                        "работа ОПГ"
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for \"operational search group\"; a law enforcement unit for search and detention operations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "operational search group",
                                "target_lang_sense_clarification": "literal translation, law enforcement unit"
                            },
                            {
                                "target_lang_word": "search party",
                                "target_lang_sense_clarification": "group organized to search for someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1129c1f6-cfa1-3008-9f74-cc632e3da7f0",
                    "sense_definition": "Медицинское сокращение от <w>ортопантомография</w>; панорамный рентгеновский снимок всех зубов и челюстей.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical_imaging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Стоматолог направил пациента сделать <w>ОПГ</w> для уточнения диагноза.",
                            "target_lang_translations": {
                                "en": "The dentist referred the patient to get an <w>OPG</w> to clarify the diagnosis."
                            }
                        },
                        {
                            "text": "На снимке <w>ОПГ</w> врач увидел воспаление у корня зуба.",
                            "target_lang_translations": {
                                "en": "On the <w>panoramic X-ray</w>, the doctor saw inflammation at the root of the tooth."
                            }
                        }
                    ],
                    "synonyms": [
                        "панорамный снимок"
                    ],
                    "common_phrases": [
                        "сделать ОПГ",
                        "снимок ОПГ",
                        "направление на ОПГ"
                    ],
                    "target_lang_definitions": {
                        "en": "Medical abbreviation for \"orthopantomography\"; a panoramic X-ray of all teeth and jaws."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "OPG",
                                "target_lang_sense_clarification": "orthopantomogram, dental X-ray"
                            },
                            {
                                "target_lang_word": "panoramic dental X-ray",
                                "target_lang_sense_clarification": "descriptive term for the image"
                            }
                        ]
                    }
                }
            ]
        }

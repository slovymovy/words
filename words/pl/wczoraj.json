                    "sense_id": "efe424ca-c538-3aa6-962f-4b47acd4f682",
                    "sense_definition": "w dniu, który bezpośrednio poprzedzał dzień dzisiejszy",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "literal_yesterday",
                    "name_type": "no",

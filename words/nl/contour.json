                {
                    "sense_id": "7173afdc-bad5-3174-b901-4f536c48e2f0",
                    "sense_definition": "De buitenste lijn of vorm van een voorwerp, persoon of landschap; het silhouet.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "shape_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de mist zagen we alleen de <w>contouren</w> van het flatgebouw.",
                            "target_lang_translations": {
                                "en": "Through the mist, we only saw the <w>outlines</w> of the apartment building.",
                                "ru": "Сквозь туман мы видели лишь <w>очертания</w> многоэтажного дома."
                            }
                        },
                        {
                            "text": "De scherpe <w>contouren</w> van de bergen staken mooi af tegen de blauwe lucht.",
                            "target_lang_translations": {
                                "en": "The sharp <w>contours</w> of the mountains stood out beautifully against the blue sky.",
                                "ru": "Резкие <w>контуры</w> гор красиво выделялись на фоне голубого неба."
                            }
                        }
                    ],
                    "synonyms": [
                        "omtrek"
                    ],
                    "common_phrases": [
                        "scherpe contouren",
                        "vage contouren",
                        "contouren van het lichaam"
                    ],
                    "target_lang_definitions": {
                        "en": "The outer line or shape of an object, person, or landscape; the silhouette.",
                        "ru": "Внешняя линия или форма предмета, человека или ландшафта; силуэт."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6a0e4b75-4531-36b7-bc92-51c0b13d562a",
                    "sense_definition": "An increase in prices and a fall in the purchasing value of money.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "economics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government is taking steps to control the rising rate of <w>inflation</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство принимает меры для контроля растущего уровня <w>инфляции</w>."
                            }
                        },
                        {
                            "text": "Due to high <w>inflation</w>, the cost of groceries has gone up significantly this year.",
                            "target_lang_translations": {
                                "ru": "Из-за высокой <w>инфляции</w> стоимость продуктов в этом году значительно выросла."
                            }
                        }
                    ],
                    "antonyms": [
                        "deflation"
                    ],
                    "common_phrases": [
                        "rate of inflation",
                        "high inflation",
                        "curb inflation",
                        "combat inflation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рост цен и падение покупательной способности денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инфляция",
                                "target_lang_sense_clarification": "экономический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b52e33d7-cd9b-370b-8409-c9a8f4f68b4c",
                    "sense_definition": "The act of expanding something by filling it with air or gas.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_expansion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>inflation</w> of the large balloon took five hours to complete.",
                            "target_lang_translations": {
                                "ru": "<w>Надувание</w> большого воздушного шара заняло пять часов."
                            }
                        },
                        {
                            "text": "Proper tire <w>inflation</w> is essential for safe driving and fuel efficiency.",
                            "target_lang_translations": {
                                "ru": "Правильное <w>накачивание</w> шин необходимо для безопасного вождения и экономии топлива."
                            }
                        }
                    ],
                    "antonyms": [
                        "deflation"
                    ],
                    "common_phrases": [
                        "tire inflation",
                        "balloon inflation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс наполнения чего-либо воздухом или газом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надувание",
                                "target_lang_sense_clarification": "воздушного шара, матраса"
                            },
                            {
                                "target_lang_word": "накачивание",
                                "target_lang_sense_clarification": "шин, мяча"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71a0cbb6-ab4c-31ef-9664-536d6f1ef886",
                    "sense_definition": "An excessive increase in something, particularly academic grades.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_increase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Universities are debating how to handle grade <w>inflation</w> among the student body.",
                            "target_lang_translations": {
                                "ru": "Университеты обсуждают, как бороться с <w>инфляцией</w> оценок среди студентов."
                            }
                        },
                        {
                            "text": "The <w>inflation</w> of job titles meant that almost everyone was a manager.",
                            "target_lang_translations": {
                                "ru": "<w>Раздувание</w> названий должностей означало, что почти каждый был менеджером."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grade inflation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезмерное увеличение чего-либо, особенно оценок или званий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инфляция",
                                "target_lang_sense_clarification": "обесценивание (оценок)"
                            },
                            {
                                "target_lang_word": "раздувание",
                                "target_lang_sense_clarification": "штатов, названий должностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03120bf5-68a3-34ac-b407-e125918e7f3b",
                    "sense_definition": "A very rapid expansion of the universe theorized to have occurred shortly after the Big Bang.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cosmology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cosmic <w>inflation</w> helps explain why the universe looks the same in every direction.",
                            "target_lang_translations": {
                                "ru": "Космическая <w>инфляция</w> помогает объяснить, почему Вселенная выглядит одинаково во всех направлениях."
                            }
                        },
                        {
                            "text": "Physicists study the period of <w>inflation</w> to understand the origins of the cosmos.",
                            "target_lang_translations": {
                                "ru": "Физики изучают период <w>инфляции</w>, чтобы понять происхождение космоса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cosmic inflation"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень быстрое расширение Вселенной, которое, согласно теории, произошло вскоре после Большого взрыва."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инфляция",
                                "target_lang_sense_clarification": "космологическая"
                            },
                            {
                                "target_lang_word": "расширение",
                                "target_lang_sense_clarification": "вселенной"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cebf99b5-eb24-32d0-8078-da46d162f7a4",
                    "sense_definition": "The specific epoch in the early universe characterized by expansion faster than the speed of light.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cosmology_proper",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "Most models of the Big Bang include <w>Inflation</w> as a key early stage.",
                            "target_lang_translations": {
                                "ru": "Большинство моделей Большого взрыва включают <w>Инфляцию</w> как ключевую раннюю стадию."
                            }
                        },
                        {
                            "text": "The theory of <w>Inflation</w> solves the horizon problem in cosmology.",
                            "target_lang_translations": {
                                "ru": "Теория <w>Инфляции</w> решает проблему горизонта в космологии."
                            }
                        }
                    ],
                    "synonyms": [
                        "Inflationary Epoch",
                        "Era of Inflation"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Эпоха в ранней Вселенной, характеризующаяся расширением быстрее скорости света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Инфляция",
                                "target_lang_sense_clarification": "название эпохи/теории"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "132b8f9f-561a-3626-bc5b-2924cd1d465b",
                    "sense_definition": "Een steun om iets rechtop te laten staan, bijvoorbeeld bij een fiets of microfoon",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "support_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet je fiets op de <w>standaard</w> zodat hij niet omvalt.",
                            "target_lang_translations": {
                                "en": "Put your bike on the <w>kickstand</w> so it doesn't fall over.",
                                "ru": "Поставь велосипед на <w>подножку</w>, чтобы он не упал."
                            }
                        },
                        {
                            "text": "De zanger zette de microfoon terug in de <w>standaard</w>.",
                            "target_lang_translations": {
                                "en": "The singer put the microphone back in the <w>stand</w>.",
                                "ru": "Певец вернул микрофон на <w>стойку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "steun",
                        "houder"
                    ],
                    "common_phrases": [
                        "op de standaard zetten",
                        "de standaard uitklappen"
                    ],
                    "target_lang_definitions": {
                        "en": "A support to keep something upright, for example with a bicycle or microphone",
                        "ru": "Опора для удержания предмета в вертикальном положении, например, у велосипеда или микрофона"
                    },

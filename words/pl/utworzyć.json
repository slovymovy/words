                    "sense_id": "c21429bf-82c7-327f-92e2-b7dbe604ed9c",
                    "sense_definition": "powołać coś do istnienia, zorganizować lub stworzyć nową całość, taką jak instytucja, grupa lub obiekt cyfrowy",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prezydent powierzył mu misję, aby <utworzył> nowy rząd w ciągu dwóch tygodni.",
                            "target_lang_translations": {
                                "ru": "Президент поручил ему миссию <w>сформировать</w> новое правительство в течение двух недель."
                            }
                        },
                        {
                            "text": "Musisz <utworzyć> bezpieczne hasło, aby móc zalogować się do systemu bankowego.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>создать</w> надежный пароль, чтобы войти в банковскую систему."
                            }
                        }
                    ],
                    "synonyms": [
                        "stworzyć",
                        "zorganizować",
                        "założyć",
                        "powołać"
                    ],
                    "antonyms": [
                        "zlikwidować",
                        "rozwiązać",
                        "usunąć"
                    ],
                    "common_phrases": [
                        "utworzyć rząd",
                        "utworzyć konto",
                        "utworzyć nowy folder",
                        "utworzyć komisję"
                    ],
                    "target_lang_definitions": {
                        "ru": "вызвать к жизни, организовать или создать что-то новое, например, учреждение, группу или цифровой объект"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "создать",
                                "target_lang_sense_clarification": "общее значение: сделать существующим"
                            },
                            {
                                "target_lang_word": "сформировать",
                                "target_lang_sense_clarification": "организовать группу, правительство или структуру"
                            },
                            {
                                "target_lang_word": "образовать",
                                "target_lang_sense_clarification": "учредить или составить целое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bfb7b71-738d-3046-a289-78383df0f87d",
                    "sense_definition": "nadać czemuś określony kształt lub ułożyć elementy w pewną formę",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "shaping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci na placu zabaw chwyciły się za ręce, aby <utworzyć> wielkie koło.",
                            "target_lang_translations": {
                                "ru": "Дети на игровой площадке взялись за руки, чтобы <w>образовать</w> большой круг."
                            }
                        },
                        {
                            "text": "Policjanci musieli szybko <utworzyć> szczelny kordon, by oddzielić tłum od budynku.",
                            "target_lang_translations": {
                                "ru": "Полицейским пришлось быстро <w>сформировать</w> плотный кордон, чтобы отделить толпу от здания."
                            }

                    "synonyms": [
                        "mature",
                        "ready"
                    ],
                    "antonyms": [
                        "unripe"
                    ],
                    "common_phrases": [
                        "ripe fruit",
                        "ripe banana",
                        "ripe tomato"
                    ],

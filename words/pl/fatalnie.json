{
    "word_family": [
        "fatalny",
        "fatalność",
        "fatum"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "65954a4c-b4b7-3d25-bcf8-0f00a185a58c",
                    "sense_definition": "W sposób bardzo zły, okropny lub beznadziejny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziś rano czuję się <w>fatalnie</w> i chyba zostanę w łóżku.",
                            "target_lang_translations": {
                                "ru": "Сегодня утром я чувствую себя <w>ужасно</w> и, пожалуй, останусь в постели."
                            }
                        },
                        {
                            "text": "Ten nowy film jest zrobiony <w>fatalnie</w>, szkoda czasu na oglądanie.",
                            "target_lang_translations": {
                                "ru": "Этот новый фильм сделан <w>отвратительно</w>, жаль времени на просмотр."
                            }
                        }
                    ],
                    "synonyms": [
                        "strasznie",
                        "beznadziejnie",
                        "okropnie",
                        "katastrofalnie",
                        "do kitu"
                    ],
                    "antonyms": [
                        "świetnie",
                        "wspaniale",
                        "doskonale"
                    ],
                    "common_phrases": [
                        "czuć się fatalnie",
                        "wyglądać fatalnie",
                        "spać fatalnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень плохо, ужасно или безнадежно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ужасно",
                                "target_lang_sense_clarification": "о самочувствии или качестве"
                            },
                            {
                                "target_lang_word": "отвратительно",
                                "target_lang_sense_clarification": "крайне плохо"
                            },
                            {
                                "target_lang_word": "скверно",
                                "target_lang_sense_clarification": "плохо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb7fc230-b11d-3d7a-8b65-eb73cda9e93c",
                    "sense_definition": "W sposób pechowy, nieszczęśliwy, wynikający ze złego zbiegu okoliczności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Fatalnie</w> się złożyło, że zgubiłem klucze tuż przed wyjazdem.",
                            "target_lang_translations": {
                                "ru": "<w>Неудачно</w> сложилось, что я потерял ключи прямо перед отъездом."
                            }
                        },
                        {
                            "text": "Nasze plany ułożyły się <w>fatalnie</w> przez nagłą zmianę pogody.",
                            "target_lang_translations": {
                                "ru": "Наши планы сложились <w>скверно</w> из-за внезапной перемены погоды."
                            }
                        }
                    ],
                    "synonyms": [
                        "pechowo"
                    ],
                    "antonyms": [
                        "szczęśliwie"
                    ],
                    "common_phrases": [
                        "fatalnie się złożyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неудачно, несчастливо, в результате плохого стечения обстоятельств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неудачно",
                                "target_lang_sense_clarification": "о стечении обстоятельств"
                            },
                            {
                                "target_lang_word": "некстати",
                                "target_lang_sense_clarification": "в неподходящий момент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6663713-fb85-3c74-8845-ccb6e0fd7b17",
                    "sense_definition": "W sposób nieuchronny, zdeterminowany przez los (często tragiczny).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta błędna decyzja zaważyła <w>fatalnie</w> na losach całego królestwa.",
                            "target_lang_translations": {
                                "ru": "Это ошибочное решение <w>роковым образом</w> сказалось на судьбе всего королевства."
                            }
                        },
                        {
                            "text": "Wydarzenia potoczyły się <w>fatalnie</w>, prowadząc do nieuniknionej klęski.",
                            "target_lang_translations": {
                                "ru": "События развивались <w>фатально</w>, ведя к неизбежному краху."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieuchronnie",
                        "nieuniknienie"
                    ],
                    "common_phrases": [
                        "zaważyć fatalnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неизбежно, предопределено судьбой (часто трагически)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фатально",
                                "target_lang_sense_clarification": "с трагическими последствиями"
                            },
                            {
                                "target_lang_word": "роковым образом",
                                "target_lang_sense_clarification": "повлияв на судьбу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
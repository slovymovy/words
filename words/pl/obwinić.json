{
    "word_family": [
        "obwiniać",
        "wina",
        "winny",
        "winić",
        "obwinienie",
        "uniewinnić"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a7447241-5340-30e5-8298-94d1759506b6",
                    "sense_definition": "Przypisać komuś winę za zrobienie czegoś złego lub za niepowodzenie; uznać kogoś za winnego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "blaming_actions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel niesłusznie <w>obwinił</w> ucznia o hałasowanie na lekcji.",
                            "target_lang_translations": {
                                "ru": "Учитель несправедливо <w>обвинил</w> ученика в шуме на уроке."
                            }
                        },
                        {
                            "text": "Nie możesz <w>obwinić</w> go za błędy całego zespołu.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь <w>обвинить</w> его в ошибках всей команды."
                            }
                        }
                    ],
                    "synonyms": [
                        "oskarżyć"
                    ],
                    "antonyms": [
                        "uniewinnić",
                        "usprawiedliwić"
                    ],
                    "common_phrases": [
                        "obwinić kogoś o kradzież",
                        "niesłusznie obwinić",
                        "obwinić za porażkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приписать кому-то вину за проступок или неудачу; счесть кого-то виновным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обвинить",
                                "target_lang_sense_clarification": "счесть виновным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27788442-f938-35cf-8c26-90d786672dbe",
                    "sense_definition": "Przypisać winę samemu sobie; uznać siebie za winnego (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "blaming_actions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wypadku kierowca <w>obwinił</w> się o spowodowanie kolizji.",
                            "target_lang_translations": {
                                "ru": "После аварии водитель <w>обвинил</w> себя в том, что стал причиной столкновения."
                            }
                        },
                        {
                            "text": "Zamiast szukać rozwiązania, on od razu <w>obwinił</w> się za całą sytuację.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы искать решение, он сразу же <w>обвинил</w> себя во всей ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "oskarżyć się"
                    ],
                    "antonyms": [
                        "usprawiedliwić się"
                    ],
                    "common_phrases": [
                        "obwinić się o wszystko",
                        "niepotrzebnie się obwinić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приписать вину самому себе; признать себя виновным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обвинить себя",
                                "target_lang_sense_clarification": "взять вину на себя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
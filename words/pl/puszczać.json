                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "release_hold_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko bało się <w>puszczać</w> ręki mamy na spacerze.",
                            "target_lang_translations": {

                            "target_lang_translations": {
                                "ru": "Моряки подняли <w>паруса</w> на мачтах, чтобы ускорить рейс."
                            }
                        },
                        {
                            "text": "Silny wiatr rozerwał główny <w>żagiel</w> na drobne kawałki.",
                            "target_lang_translations": {

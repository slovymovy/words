    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "47bd4445-f5e5-3dad-90c6-18f14c6781fd",
                    "sense_definition": "Podstawowy czasownik łączący podmiot z orzecznikiem, używany do określania tożsamości, zawodu, cech lub przynależności do grupy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "copula_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój starszy brat <jest> bardzo doświadczonym lekarzem.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат <является> очень опытным врачом."
                            }
                        },
                        {
                            "text": "Dzisiaj <jesteśmy> wyjątkowo zmęczeni po długiej podróży.",
                            "target_lang_translations": {
                                "ru": "Сегодня мы <очень устали> после долгой поездки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "być pewnym siebie",
                        "być w błędzie",
                        "być dobrej myśli",
                        "być człowiekiem sukcesu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основной глагол-связка, соединяющий подлежащее со сказуемым; используется для определения личности, профессии, качеств или принадлежности к группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "глагол-связка (часто опускается в настоящем времени)"
                            },
                            {
                                "target_lang_word": "являться",
                                "target_lang_sense_clarification": "формальный синоним глагола-связки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01937cb8-dfb4-3847-ab66-57148d23fe18",
                    "sense_definition": "Znajdować się fizycznie w określonym miejscu lub istnieć w rzeczywistości.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klucze od twojego samochodu <są> na stole w kuchni.",
                            "target_lang_translations": {
                                "ru": "Ключи от твоей машины <находятся> на столе в кухне."
                            }
                        },
                        {
                            "text": "W tej starej bibliotece <jest> wiele ciekawych książek.",
                            "target_lang_translations": {
                                "ru": "В этой старой библиотеке <есть> много интересных книг."
                            }
                        }
                    ],
                    "synonyms": [
                        "znajdować się",
                        "istnieć",
                        "znajdować się"
                    ],
                    "antonyms": [
                        "nie ma"
                    ],
                    "common_phrases": [
                        "być w domu",
                        "być na miejscu",
                        "być pod ręką",
                        "być w pobliżu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться физически в определённом месте или существовать в реальности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "о местоположении"
                            },
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "существовать или находиться"
                            },
                            {
                                "target_lang_word": "иметься",
                                "target_lang_sense_clarification": "о наличии"
                            },
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "форма настоящего времени (о наличии)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efd9053c-5786-3705-bc54-0c783e5c7575",
                    "sense_definition": "Uczestniczyć w jakimś wydarzeniu, spotkaniu lub odwiedzać kogoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "participation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj wieczorem <byliśmy> z przyjaciółmi w kinie na nowym filmie.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы <были> с друзьями в кино на новом фильме."
                            }
                        },
                        {
                            "text": "Czy <będziesz> jutro na urodzinach swojej babci?",
                            "target_lang_translations": {
                                "ru": "Ты <будешь> завтра на дне рождения своей бабушки?"
                            }
                        }
                    ],
                    "synonyms": [
                        "znajdować się",
                        "istnieć"
                    ],
                    "common_phrases": [
                        "być na spotkaniu",
                        "być na wakacjach",
                        "być u lekarza",
                        "być w pracy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участвовать в каком-либо событии, встрече или посещать кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "присутствовать где-либо"
                            },
                            {
                                "target_lang_word": "присутствовать",
                                "target_lang_sense_clarification": "официальное участие"
                            },
                            {
                                "target_lang_word": "побывать",
                                "target_lang_sense_clarification": "посетить (совершенный вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba342407-e333-38fd-b46c-8ab13be390b8",
                    "sense_definition": "Czasownik posiłkowy służący do tworzenia czasu przyszłego złożonego w połączeniu z bezokolicznikiem lub formą czasu przeszłego czasownika niedokonanego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "auxiliary_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym tygodniu <będziemy> malować ściany w naszym salonie.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе мы <будем> красить стены в нашей гостиной."
                            }
                        },
                        {
                            "text": "Jutro o tej porze <będę> już leciał samolotem do Hiszpanii.",
                            "target_lang_translations": {
                                "ru": "Завтра в это время я уже <буду> лететь на самолёте в Испанию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "będę robić",
                        "będzie czekać",
                        "będziemy musieli"
                    ],
                    "traits": [

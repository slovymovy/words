                {
                    "sense_id": "b3c2e12a-247d-3642-b031-399e729415df",
                    "sense_definition": "Voldoende luid kunnen horen om te begrijpen wat er gezegd wordt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "acoustic_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je me achterin de zaal ook goed <w>verstaan</w>?",
                            "target_lang_translations": {
                                "ru": "Ты меня хорошо <w>слышишь</w> даже в конце зала?",
                                "en": "Can you <w>hear</w> me clearly at the back of the room too?"
                            }
                        },
                        {
                            "text": "Door de harde muziek kon ik hem nauwelijks <w>verstaan</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за громкой музыки я едва мог его <w>расслышать</w>.",
                                "en": "Because of the loud music, I could hardly <w>hear</w> him."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niet te verstaan",
                        "slecht verstaan",
                        "goed verstaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слышать достаточно громко, чтобы разобрать сказанное.",
                        "en": "To be able to hear clearly enough to understand what is being said."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расслышать",
                                "target_lang_sense_clarification": "разобрать слова на слух"
                            },
                            {
                                "target_lang_word": "слышать",
                                "target_lang_sense_clarification": "воспринимать звуки речи"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "hear",
                                "target_lang_sense_clarification": "perceive sound clearly"
                            },
                            {
                                "target_lang_word": "make out",
                                "target_lang_sense_clarification": "manage to hear or distinguish"
                            },
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "hear something said"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c47cad1-ee5d-3102-af19-365898df9c0e",
                    "sense_definition": "De betekenis van woorden, taal of tekens begrijpen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cognitive_understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik spreek geen Spaans, maar ik kan het wel een beetje <w>verstaan</w>.",
                            "target_lang_translations": {
                                "ru": "Я не говорю по-испански, но немного его <w>понимаю</w>.",
                                "en": "I don't speak Spanish, but I can <w>understand</w> it a little."
                            }
                        },
                        {
                            "text": "Heb je <w>verstaan</w> wat de leraar net uitlegde?",
                            "target_lang_translations": {
                                "ru": "Ты <w>понял</w>, что только что объяснил учитель?",
                                "en": "Did you <w>understand</w> what the teacher just explained?"
                            }
                        }
                    ],
                    "synonyms": [
                        "begrijpen",
                        "snappen"
                    ],
                    "antonyms": [
                        "misverstaan"
                    ],
                    "common_phrases": [
                        "elkaar verstaan",
                        "een taal verstaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать смысл слов, языка или знаков.",
                        "en": "To understand the meaning of words, language, or signs."

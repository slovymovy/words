                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of the noun 'smelt' (a small silvery fish).",
                        "ru": "Множественное число существительного «smelt» (рыба песчанка)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smelts",
                                "target_lang_sense_clarification": "small fish"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "песчанки",
                                "target_lang_sense_clarification": "рыба"
                            }
                        ]
                    }
                }
            ]
        }
    ]

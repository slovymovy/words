    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c89077ce-86fa-34b1-aa42-8ce54529a8cf",
                    "sense_definition": "Не подвергаясь наказанию за совершённый поступок или действие.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "no_punishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Никто не имеет права <w>безнаказанно</w> нарушать закон в нашем государстве.",
                            "target_lang_translations": {
                                "en": "No one has the right to break the law <w>with impunity</w> in our state."
                            }
                        },
                        {
                            "text": "Он ошибочно полагал, что сможет <w>безнаказанно</w> грубить старшим.",
                            "target_lang_translations": {
                                "en": "He mistakenly believed that he could be rude to his elders <w>with impunity</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "безнаказанно грабить",
                        "безнаказанно убивать",
                        "действовать безнаказанно"
                    ],
                    "target_lang_definitions": {
                        "en": "Without receiving punishment for an action or behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "with impunity",
                                "target_lang_sense_clarification": "without fear of punishment"
                            },
                            {
                                "target_lang_word": "scot-free",
                                "target_lang_sense_clarification": "completely without punishment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51de6cb2-f2f1-3d40-9de1-4315a0bf0fc4",
                    "sense_definition": "Оставаясь без неприятных, вредных или тяжёлых последствий (обычно для здоровья или благополучия).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "no_consequences",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Постоянное недосыпание не проходит <w>безнаказанно</w> для нервной системы.",
                            "target_lang_translations": {
                                "en": "Constant lack of sleep does not pass <w>without consequences</w> for the nervous system."
                            }
                        },
                        {
                            "text": "Нельзя <w>безнаказанно</w> эксплуатировать природные ресурсы бесконечно долго.",
                            "target_lang_translations": {
                                "en": "One cannot exploit natural resources endlessly <w>without consequences</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "безболезненно",
                        "даром"
                    ],
                    "common_phrases": [
                        "не пройти безнаказанно",
                        "не проходит безнаказанно"
                    ],
                    "target_lang_definitions": {
                        "en": "Without negative consequences or harm, typically regarding health or long-term effects."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "without consequences",
                                "target_lang_sense_clarification": "without negative effects"
                            },
                            {
                                "target_lang_word": "without paying the price",
                                "target_lang_sense_clarification": "idiomatic, regarding negative outcome"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ba29b664-58fc-32c1-bea6-ab7c37176ed7",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>безнаказанный</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это дерзкое преступление так и осталось <w>безнаказанно</w> властями.",
                            "target_lang_translations": {
                                "en": "This brazen crime remained <w>unpunished</w> by the authorities."
                            }
                        },
                        {
                            "text": "Зло не должно быть <w>безнаказанно</w>, справедливость должна восторжествовать.",
                            "target_lang_translations": {
                                "en": "Evil must not be <w>unpunished</w>; justice must prevail."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "State of not having been punished (short adjective form)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unpunished",
                                "target_lang_sense_clarification": "adjective describing a state"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
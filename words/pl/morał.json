            "senses": [
                {
                    "sense_id": "6b726ab0-d84d-30a9-b1d0-5b6437c16a16",
                    "sense_definition": "Zwięzły, pouczający wniosek wynikający z jakiegoś zdarzenia, historii lub bajki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "lesson",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jaki <w>morał</w> płynie z tej smutnej historii?",
                            "target_lang_translations": {
                                "ru": "Какая <w>мораль</w> следует из этой печальной истории?"
                            }
                        },
                        {
                            "text": "Morał z tego taki, że warto być uczciwym człowiekiem.",
                            "target_lang_translations": {
                                "ru": "<w>Мораль</w> такова, что стоит быть честным человеком."
                            }
                        }
                    ],
                    "synonyms": [
                        "wniosek",
                        "nauka",
                        "pouczenie"
                    ],
                    "common_phrases": [
                        "morał bajki",
                        "morał z tego taki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткий поучительный вывод из какого-либо события, истории или басни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мораль",
                                "target_lang_sense_clarification": "поучительный вывод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "318666e4-84d1-3ee2-b3fd-d057e497da02",
                    "sense_definition": "Długa, często nudna mowa lub uwagi na temat właściwego zachowania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sermon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mam ochoty słuchać twoich <w>morałów</w> na temat mojego życia.",
                            "target_lang_translations": {
                                "ru": "Я не хочу слушать твои <w>нравоучения</w> о моей жизни."
                            }
                        },
                        {
                            "text": "Zamiast pomóc, zaczął prawić mi <w>morały</w> o odpowiedzialności.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы помочь, он начал читать мне <w>нотации</w> об ответственности."
                            }
                        }
                    ],
                    "synonyms": [
                        "kazanie",
                        "pouczenie"
                    ],
                    "common_phrases": [
                        "prawić morały",
                        "czytać morały"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная, часто скучная речь или замечания о правильном поведении; нравоучение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нравоучение",
                                "target_lang_sense_clarification": "наставление"
                            },
                            {
                                "target_lang_word": "нотация",
                                "target_lang_sense_clarification": "выговор, поучение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

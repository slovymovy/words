                            {
                                "target_lang_word": "no less than",
                                "target_lang_sense_clarification": "emphasizing a significant amount"
                            },
                            {
                                "target_lang_word": "fully",
                                "target_lang_sense_clarification": "emphasizing completeness or amount"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "целых",
                                "target_lang_sense_clarification": "подчеркивает большое количество"
                            },
                            {
                                "target_lang_word": "аж",
                                "target_lang_sense_clarification": "разг. усиление удивления числом"
                            },
                            {
                                "target_lang_word": "не менее",
                                "target_lang_sense_clarification": "указание на значительный минимум"
                            }

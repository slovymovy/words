    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d18c8f63-7366-30c8-b247-c52f0b71d323",
                    "sense_definition": "Нанести ущерб, ослабить или расшатать что-либо (обычно нематериальное: авторитет, доверие, экономику).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот скандал может серьёзно <подорвать> авторитет директора среди сотрудников.",
                            "target_lang_translations": {
                                "en": "This scandal could seriously <undermine> the director's authority among the employees."
                            }
                        },
                        {
                            "text": "Постоянная ложь способна <подорвать> доверие даже самых близких друзей.",
                            "target_lang_translations": {
                                "en": "Constant lying is capable of <undermining> the trust of even the closest friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "ослабить",
                        "пошатнуть",
                        "расшатать"
                    ],
                    "antonyms": [
                        "укрепить",
                        "усилить"
                    ],
                    "common_phrases": [
                        "подорвать доверие",
                        "подорвать авторитет",
                        "подорвать экономику",
                        "подорвать репутацию"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause damage, weaken, or destabilize something (usually intangible: authority, trust, economy)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "undermine",
                                "target_lang_sense_clarification": "authority, trust, reputation"
                            },
                            {
                                "target_lang_word": "damage",
                                "target_lang_sense_clarification": "reputation, economy"
                            },
                            {
                                "target_lang_word": "weaken",
                                "target_lang_sense_clarification": "position, economy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c36f4ace-de46-3f68-bfee-1b13151f6fb6",
                    "sense_definition": "Значительно ухудшить состояние здоровья или лишить физических сил.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тяжёлая работа на севере окончательно <подорвала> его здоровье.",
                            "target_lang_translations": {
                                "en": "Hard work in the North finally <ruined> his health."
                            }
                        },
                        {
                            "text": "Бессонные ночи и постоянный стресс могут быстро <подорвать> силы организма.",
                            "target_lang_translations": {
                                "en": "Sleepless nights and constant stress can quickly <sap> the body's strength."
                            }
                        }
                    ],
                    "synonyms": [
                        "ухудшить",
                        "расстроить",
                        "надломить"
                    ],
                    "antonyms": [
                        "поправить",
                        "восстановить",
                        "укрепить"
                    ],
                    "common_phrases": [
                        "подорвать здоровье",
                        "подорвать силы"
                    ],
                    "target_lang_definitions": {
                        "en": "To significantly worsen health or drain physical strength."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ruin",
                                "target_lang_sense_clarification": "health"
                            },
                            {
                                "target_lang_word": "undermine",
                                "target_lang_sense_clarification": "health"
                            },
                            {
                                "target_lang_word": "sap",
                                "target_lang_sense_clarification": "strength, energy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33e1a5b9-1c45-356c-8289-ad8e83ac9c19",
                    "sense_definition": "Разрушить или повредить что-либо с помощью взрыва.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Партизаны решили <подорвать> железнодорожный мост, чтобы остановить вражеский поезд.",
                            "target_lang_translations": {
                                "en": "The partisans decided to <blow up> the railway bridge to stop the enemy train."
                            }
                        },
                        {
                            "text": "Сапёры смогли безопасно <подорвать> старый боеприпас на полигоне.",
                            "target_lang_translations": {
                                "en": "The sappers were able to safely <detonate> the old munition at the testing ground."
                            }
                        }
                    ],
                    "synonyms": [
                        "взорвать"
                    ],
                    "common_phrases": [
                        "подорвать мост",
                        "подорвать танк",
                        "подорвать на мине"
                    ],
                    "target_lang_definitions": {
                        "en": "To destroy or damage something using an explosion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blow up",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "detonate",
                                "target_lang_sense_clarification": "explosives"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42f36461-83dc-3ae2-a226-26702b0fcd30",
                    "sense_definition": "Резко и быстро уйти или убежать откуда-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Как только началась драка, они решили <подорвать> из клуба.",
                            "target_lang_translations": {
                                "en": "As soon as the fight started, they decided to <bolt> from the club."
                            }
                        },
                        {
                            "text": "Нам нужно срочно <подорвать> отсюда, пока никто не заметил.",
                            "target_lang_translations": {
                                "en": "We need to urgently <take off> from here before anyone notices."
                            }
                        }
                    ],
                    "synonyms": [
                        "убежать",
                        "сбежать",
                        "сорваться"
                    ],
                    "antonyms": [
                        "остаться"
                    ],
                    "common_phrases": [
                        "подорвать с места"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To leave or run away from somewhere abruptly and quickly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bolt",
                                "target_lang_sense_clarification": "run away suddenly"
                            },
                            {
                                "target_lang_word": "take off",
                                "target_lang_sense_clarification": "leave quickly"
                            },
                            {
                                "target_lang_word": "bail",
                                "target_lang_sense_clarification": "leave a situation"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
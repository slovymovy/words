            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b1aa0b07-3819-30eb-910c-d12d2efb886f",
                    "sense_definition": "Zator na drodze spowodowany dużą liczbą samochodów, które poruszają się bardzo wolno lub stoją w miejscu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "traffic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spóźniłem się dzisiaj do pracy, ponieważ utknąłem w ogromnym <w>korku</w>.",
                            "target_lang_translations": {
                                "ru": "Я сегодня опоздал на работу, потому что застрял в огромной <w>пробке</w>."
                            }
                        },
                        {
                            "text": "W godzinach szczytu w centrum miasta zawsze tworzą się <w>korki</w>.",
                            "target_lang_translations": {
                                "ru": "В часы пик в центре города всегда образуются <w>пробки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zator",
                        "zator drogowy"
                    ],
                    "common_phrases": [
                        "stać w korku",
                        "gigantyczny korek",
                        "korek uliczny",
                        "rozładować korek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Затор на дороге, вызванный большим количеством автомобилей, которые движутся очень медленно или стоят."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробка",
                                "target_lang_sense_clarification": "дорожная"
                            },
                            {
                                "target_lang_word": "затор",
                                "target_lang_sense_clarification": "скопление транспорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d51ea68e-f23e-38ef-a6ac-54e60bc62366",
                    "sense_definition": "Przedmiot służący do zamykania butelki, słoika lub zatykania odpływu w zlewie czy wannie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner sprawnie wyciągnął <w>korek</w> z butelki czerwonego wina.",
                            "target_lang_translations": {
                                "ru": "Официант ловко вытащил <w>пробку</w> из бутылки красного вина."
                            }
                        },
                        {
                            "text": "Woda ucieka z wanny, ponieważ <w>korek</w> jest nieszczelny.",
                            "target_lang_translations": {
                                "ru": "Вода уходит из ванны, потому что <w>пробка</w> пропускает воду."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatyczka",
                        "zamknięcie"
                    ],
                    "common_phrases": [
                        "korek od butelki",
                        "wyciągnąć korek",
                        "korek do wanny",
                        "strzelić korkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, используемый для закрывания бутылки, банки или затыкания слива в раковине или ванне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробка",
                                "target_lang_sense_clarification": "для бутылки"
                            },
                            {
                                "target_lang_word": "затычка",
                                "target_lang_sense_clarification": "для ванны/раковины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "232c840a-218a-3d5e-bf45-167dc9a1cc89",
                    "sense_definition": "Płatne lekcje dodatkowe udzielane uczniom poza szkołą; potoczne określenie korepetycji.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę zapisać się na <w>korki</w> z matematyki, żeby zdać maturę.",
                            "target_lang_translations": {
                                "ru": "Мне нужно записаться на <w>дополнительные занятия</w> по математике, чтобы сдать выпускные экзамены."
                            }
                        },
                        {
                            "text": "Moja starsza siostra daje <w>korki</w> z języka angielskiego dzieciom z sąsiedztwa.",
                            "target_lang_translations": {
                                "ru": "Моя старшая сестра дает <w>частные уроки</w> английского языка соседским детям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chodzić na korki",
                        "dawać korki",
                        "brać korki",
                        "korki z fizyki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common student slang for 'korepetycje'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Платные дополнительные уроки для учеников вне школы; разговорное название репетиторства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "частные уроки",
                                "target_lang_sense_clarification": "занятия"
                            },
                            {
                                "target_lang_word": "репетиторство",
                                "target_lang_sense_clarification": "деятельность"
                            },
                            {
                                "target_lang_word": "дополнительные занятия",
                                "target_lang_sense_clarification": "учеба"
                            }
                        ]

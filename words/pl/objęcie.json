                        {
                            "trait_type": "form",
                            "comment": "verbal noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отглагольное существительное, означающее действие по охвату чего-либо (например, взглядом, умом, зоной действия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охват",

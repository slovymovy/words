    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7bf18e19-0b04-3ce1-9f7e-48b808d5ca92",
                    "sense_definition": "Conscious or having knowledge of something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Are you <w>aware</w> of the risks involved in this decision?",
                            "target_lang_translations": {
                                "ru": "Вы <w>осознаёте</w> риски, связанные с этим решением?"
                            }
                        },
                        {
                            "text": "I was not <w>aware</w> that the meeting had been cancelled.",
                            "target_lang_translations": {
                                "ru": "Я был не <w>в курсе</w>, что встречу отменили."
                            }
                        }
                    ],
                    "synonyms": [
                        "conscious",
                        "informed",
                        "mindful",
                        "cognizant",
                        "apprised"
                    ],
                    "antonyms": [
                        "unaware"
                    ],
                    "common_phrases": [
                        "aware of",
                        "well aware",
                        "become aware",
                        "make aware"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знающий или осведомлённый о чём-либо; понимающий ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осведомлённый",
                                "target_lang_sense_clarification": "обладающий информацией о чём-либо"
                            },
                            {
                                "target_lang_word": "в курсе",
                                "target_lang_sense_clarification": "разговорное выражение; знающий о текущей ситуации"
                            },
                            {
                                "target_lang_word": "осознающий",
                                "target_lang_sense_clarification": "понимающий смысл или последствия чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcc12e07-4516-3982-80e9-c46bb6940df2",
                    "sense_definition": "Vigilant or on one's guard against danger or difficulty.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vigilance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to stay <w>aware</w> of your surroundings when walking alone at night.",
                            "target_lang_translations": {
                                "ru": "Нужно быть <w>внимательным</w> к происходящему вокруг, когда гуляешь один ночью."
                            }
                        },
                        {
                            "text": "Drivers must be <w>aware</w> of pedestrians crossing the street.",
                            "target_lang_translations": {
                                "ru": "Водители должны быть <w>внимательны</w> к пешеходам, переходящим улицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "alert",
                        "watchful",
                        "wary",
                        "attentive",
                        "observant"
                    ],
                    "common_phrases": [
                        "stay aware",
                        "fully aware"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бдительный, настороже по отношению к опасности или трудностям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бдительный",
                                "target_lang_sense_clarification": "сохраняющий настороженность"
                            },
                            {
                                "target_lang_word": "внимательный",
                                "target_lang_sense_clarification": "обращающий внимание на окружение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2c13b1c8-adb7-3288-a4ce-588551a617cc",
                    "sense_definition": "To make someone aware of something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The messenger <w>awared</w> the king of the approaching army.",
                            "target_lang_translations": {
                                "ru": "Гонец <w>осведомил</w> короля о приближающейся армии."
                            }
                        },
                        {
                            "text": "His conscience <w>awares</w> him of the difference between right and wrong.",
                            "target_lang_translations": {
                                "ru": "Его совесть <w>даёт</w> ему <w>понять</w> разницу между добром и злом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This usage is very rare in modern English."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ставить кого-либо в известность о чём-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осведомлять",
                                "target_lang_sense_clarification": "сообщать сведения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
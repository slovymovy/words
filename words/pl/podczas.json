        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "0eda55a8-f90f-3b75-9ab2-43068da0f0cc",
                    "sense_definition": "Przyimek wskazujący na to, że jakaś czynność dzieje się w tym samym czasie, co inne zdarzenie; w trakcie trwania czegoś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wolno rozmawiać <w>podczas</w> egzaminu.",
                            "target_lang_translations": {
                                "ru": "Нельзя разговаривать <w>во время</w> экзамена."
                            }
                        },
                        {
                            "text": "Zgubiłem telefon <w>podczas</w> spaceru w parku.",
                            "target_lang_translations": {
                                "ru": "Я потерял телефон <w>во время</w> прогулки в парке."
                            }
                        },
                        {
                            "text": "<w>Podczas</w> wakacji często pływaliśmy w morzu.",
                            "target_lang_translations": {
                                "ru": "<w>Во время</w> каникул мы часто плавали в море."
                            }
                        }
                    ],
                    "synonyms": [
                        "w czasie",
                        "w trakcie"
                    ],
                    "antonyms": [
                        "po",
                        "przed"
                    ],
                    "common_phrases": [
                        "podczas wakacji",
                        "podczas pracy",
                        "podczas rozmowy",
                        "podczas deszczu",
                        "podczas wojny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлог, указывающий на то, что действие происходит одновременно с другим событием; в процессе чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "во время",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "в ходе",
                                "target_lang_sense_clarification": "в процессе развития события"
                            },
                            {
                                "target_lang_word": "в течение",
                                "target_lang_sense_clarification": "на протяжении времени"
                            }
                        ]
                    }
                }
            ]
        },

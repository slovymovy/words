                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The zipper <w>pull</w> broke off my winter jacket.",
                            "target_lang_translations": {
                                "ru": "<w>Язычок</w> молнии отломился от моей зимней куртки."
                            }

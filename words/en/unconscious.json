{
    "word_family": [
        "consciousness",
        "unconsciously",
        "unconsciousness",
        "subconscious"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ea5fe5ce-e0ae-3fed-88b2-3ed5836b8558",
                    "sense_definition": "Not awake and not aware of one's surroundings, usually because of an injury, illness, or anesthesia.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "state_of_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The driver was knocked <w>unconscious</w> during the car accident.",
                            "target_lang_translations": {
                                "ru": "Во время автомобильной аварии водитель потерял <w>сознание</w>."
                            }
                        },
                        {
                            "text": "Doctors waited for the patient to wake up after he had been <w>unconscious</w> for two days.",
                            "target_lang_translations": {
                                "ru": "Врачи ждали, когда пациент очнется, после того как он два дня пробыл <w>без сознания</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "senseless",
                        "out of it"
                    ],
                    "antonyms": [
                        "conscious",
                        "awake"
                    ],
                    "common_phrases": [
                        "knock unconscious",
                        "fall unconscious",
                        "lie unconscious"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не бодрствующий и не воспринимающий окружающее, обычно из-за травмы, болезни или анестезии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "без сознания",
                                "target_lang_sense_clarification": "состояние (предикатив)"
                            },
                            {
                                "target_lang_word": "бессознательный",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff9857a6-d98f-3d49-999f-4f3bf3d8547b",
                    "sense_definition": "Existing or happening without the person realizing or intending it; not controlled by conscious thought.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She had an <w>unconscious</w> habit of tapping her foot when she was nervous.",
                            "target_lang_translations": {
                                "ru": "У нее была <w>неосознанная</w> привычка постукивать ногой, когда она нервничала."
                            }
                        },
                        {
                            "text": "The manager promised to address any <w>unconscious</w> bias in the hiring process.",
                            "target_lang_translations": {
                                "ru": "Менеджер пообещал устранить любую <w>неосознанную</w> предвзятость в процессе найма."
                            }
                        }
                    ],
                    "synonyms": [
                        "subconscious",
                        "unaware",
                        "oblivious"
                    ],
                    "antonyms": [
                        "conscious",
                        "deliberate",
                        "intentional"
                    ],
                    "common_phrases": [
                        "unconscious bias",
                        "unconscious desire",
                        "unconscious reaction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий без осознания или намерения; не контролируемый сознательной мыслью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неосознанный",
                                "target_lang_sense_clarification": "непреднамеренный, спонтанный"
                            },
                            {
                                "target_lang_word": "бессознательный",
                                "target_lang_sense_clarification": "инстинктивный, рефлекторный"
                            },
                            {
                                "target_lang_word": "подсознательный",
                                "target_lang_sense_clarification": "глубинный, скрытый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95b78a3c-f1b0-30b9-a58b-f214ac114148",
                    "sense_definition": "Performing extremely well in a sport or activity without having to think about it; in a state of high performance.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The basketball player was absolutely <w>unconscious</w> in the final quarter, scoring from everywhere.",
                            "target_lang_translations": {
                                "ru": "В последней четверти баскетболист был просто <w>в ударе</w>, забивая отовсюду."
                            }
                        },
                        {
                            "text": "He went <w>unconscious</w> from the three-point line and led his team to victory.",
                            "target_lang_translations": {
                                "ru": "Он стал <w>неудержим</w> на трехочковой линии и привел свою команду к победе."
                            }
                        }
                    ],
                    "synonyms": [
                        "in the zone",
                        "on a roll"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывающий невероятные результаты в игре, действуя интуитивно и безошибочно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в ударе",
                                "target_lang_sense_clarification": "идиома: на пике формы"
                            },
                            {
                                "target_lang_word": "неудержимый",
                                "target_lang_sense_clarification": "которого невозможно остановить"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2167ba72-6e95-3cb6-a011-726f9d93ace1",
                    "sense_definition": "The part of the mind containing thoughts, memories, and desires that a person is not aware of but which influence their behavior.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "psychology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dreams are often believed to be messages from the <w>unconscious</w>.",
                            "target_lang_translations": {
                                "ru": "Сны часто считают посланиями из <w>бессознательного</w>."
                            }
                        },
                        {
                            "text": "The therapist helped him explore the <w>unconscious</w> to understand his fears.",
                            "target_lang_translations": {
                                "ru": "Терапевт помог ему исследовать <w>бессознательное</w>, чтобы понять свои страхи."
                            }
                        }
                    ],
                    "synonyms": [
                        "subconscious",
                        "subconsciousness",
                        "subconscious mind"
                    ],
                    "common_phrases": [
                        "collective unconscious",
                        "the unconscious"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть психики, содержащая неосознаваемые мысли, воспоминания и желания, влияющие на поведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бессознательное",
                                "target_lang_sense_clarification": "психологический термин"
                            },
                            {
                                "target_lang_word": "подсознание",
                                "target_lang_sense_clarification": "общеупотребительный синоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
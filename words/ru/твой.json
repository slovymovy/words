    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "4d2b132a-b530-3541-b21b-b723c3949ed2",
                    "sense_definition": "Принадлежащий тебе, относящийся к тебе или связанный с тобой (используется при обращении к одному лицу, с которым говорящий на «ты»).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Где сейчас живёт <w>твоя</w> семья?",
                            "target_lang_translations": {
                                "en": "Where does <w>your</w> family live now?"
                            }
                        },
                        {
                            "text": "Мне очень нравится <w>твой</w> новый дом.",
                            "target_lang_translations": {
                                "en": "I really like <w>your</w> new house."
                            }
                        },
                        {
                            "text": "Это <w>твоё</w> решение, и я его уважаю.",
                            "target_lang_translations": {
                                "en": "It is <w>your</w> decision, and I respect it."
                            }
                        }
                    ],
                    "antonyms": [
                        "мой",
                        "чужой"
                    ],
                    "common_phrases": [
                        "твоё имя",
                        "твоя очередь",
                        "твоё здоровье",
                        "по твоему мнению"
                    ],
                    "target_lang_definitions": {
                        "en": "Belonging to you, relating to you, or associated with you (used when addressing one person informally)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "your",
                                "target_lang_sense_clarification": "possessive determiner"
                            },
                            {
                                "target_lang_word": "yours",
                                "target_lang_sense_clarification": "possessive pronoun"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4b7580f-f0d0-3556-b4b1-eba1d6d68772",
                    "sense_definition": "Родственники, члены семьи или близкие люди (употребляется во множественном числе как существительное).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "substantivized_family",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Как там <w>твои</w>, все здоровы?",
                            "target_lang_translations": {
                                "en": "How are <w>your folks</w>, is everyone healthy?"
                            }
                        },
                        {
                            "text": "Передавай большой привет <w>твоим</w> от меня.",
                            "target_lang_translations": {
                                "en": "Give a big hello to <w>your folks</w> for me."
                            }
                        }
                    ],
                    "synonyms": [
                        "родные",
                        "домашние"
                    ],
                    "common_phrases": [
                        "привет твоим",
                        "как твои"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Relatives, family members, or close people (used in the plural as a noun)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "your folks",
                                "target_lang_sense_clarification": "colloquial"
                            },
                            {
                                "target_lang_word": "your family",
                                "target_lang_sense_clarification": "general term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "438819e2-a588-3d17-85b6-22fd64122be7",
                    "sense_definition": "Муж, жених или романтический партнёр (употребляется как существительное).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "substantivized_partner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вчера <w>твой</w> заходил к нам в офис.",
                            "target_lang_translations": {
                                "en": "Yesterday <w>your man</w> came by our office."
                            }
                        },
                        {
                            "text": "А <w>твой</w> не будет против, если мы пойдём в кино?",
                            "target_lang_translations": {
                                "en": "And won't <w>your guy</w> mind if we go to the movies?"

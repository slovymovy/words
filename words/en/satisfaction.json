                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Компенсация или возмещение за нанесенный ущерб, потерю или обиду"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компенса́ция",

                        {
                            "text": "Settlers arrived to <w>plant</w> a new colony on the distant shore.",
                            "target_lang_translations": {
                                "ru": "Поселенцы прибыли, чтобы <w>основать</w> новую колонию на далеком берегу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plant an idea",
                        "plant a colony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждать или обосновывать идею, колонию или группу людей."
                    },
                    "translations": {
                        "ru": [

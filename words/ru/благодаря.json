        "благодарный"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "91d5e378-c141-336c-8538-78006341aa12",
                    "sense_definition": "Употребляется при указании на причину, которая привела к желаемому, положительному результату; по причине, вследствие чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Только <благодаря> вашей поддержке я смог выиграть выборы.",
                            "target_lang_translations": {
                                "en": "Only <w>thanks to</w> your support was I able to win the election."
                            }
                        },
                        {
                            "text": "Мы закончили этот сложный проект вовремя <благодаря> слаженной работе команды.",
                            "target_lang_translations": {
                                "en": "We finished this complex project on time <w>thanks to</w> the coordinated work of the team."
                            }
                        }
                    ],
                    "synonyms": [
                        "по причине",
                        "вследствие",
                        "в результате",
                        "при помощи"
                    ],
                    "antonyms": [
                        "наперекор"
                    ],
                    "common_phrases": [
                        "благодаря этому",
                        "благодаря помощи",
                        "благодаря случаю",
                        "благодаря тебе"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to indicate a cause that led to a desired, positive result; because of or as a consequence of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thanks to",
                                "target_lang_sense_clarification": "primary translation, implies a positive outcome"
                            },
                            {
                                "target_lang_word": "due to",
                                "target_lang_sense_clarification": "indicates cause, often formal"
                            },
                            {
                                "target_lang_word": "owing to",
                                "target_lang_sense_clarification": "formal synonym for 'because of'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d560489-25f6-3d52-9b49-42f7d0a06476",
                    "sense_definition": "Деепричастие настоящего времени от глагола <w>благодарить</w>; действие, выражающее признательность.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гость ушёл, тепло <благодаря> хозяйку за вкусный ужин.",
                            "target_lang_translations": {
                                "en": "The guest left, warmly <w>thanking</w> the hostess for a delicious dinner."
                            }
                        },
                        {
                            "text": "Она улыбнулась, <благодаря> друга за неожиданный подарок.",
                            "target_lang_translations": {
                                "en": "She smiled, <w>thanking</w> her friend for the unexpected gift."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "gerund"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Present participle (gerund) of the verb 'to thank'; an action expressing gratitude."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thanking",
                                "target_lang_sense_clarification": "gerund form describing the action of expressing thanks"
                            }
                        ]
                    }
                }
            ]
        }
    ]

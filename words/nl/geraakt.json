                        "slaags geraakt"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Past participle of 'geraken'.",
                        "ru": "Причастие прошедшего времени от <w>geraken</w> (добираться, оказываться)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "got",
                                "target_lang_sense_clarification": "managed to reach a place or state"
                            },
                            {
                                "target_lang_word": "touched",
                                "target_lang_sense_clarification": "emotionally affected or moved"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "добрался",

    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "3f17e7cf-125c-3675-819a-d07f3cb2d229",
                    "sense_definition": "The star around which the Earth orbits; the Sun, especially when used in a scientific or science fiction context.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sun_concept",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "The spaceship engaged its engines to leave the orbit of <w>Sol</w>.",
                            "target_lang_translations": {
                                "ru": "Космический корабль включил двигатели, чтобы покинуть орбиту <w>Солнца</w>."
                            }
                        },
                        {
                            "text": "Astronomers sometimes refer to our local star as <w>Sol</w> to distinguish it from other suns.",
                            "target_lang_translations": {
                                "ru": "Астрономы иногда называют нашу звезду <w>Сол</w>, чтобы отличить её от других солнц."
                            }
                        }
                    ],
                    "synonyms": [
                        "Sun"
                    ],
                    "common_phrases": [
                        "system of Sol",
                        "orbit of Sol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звезда, вокруг которой вращается Земля; Солнце, особенно в научном или научно-фантастическом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Солнце",
                                "target_lang_sense_clarification": "астрономическое тело"
                            },
                            {
                                "target_lang_word": "Сол",
                                "target_lang_sense_clarification": "в научной фантастике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d98811ea-6b83-3366-951e-d1a3d85a298c",
                    "sense_definition": "A short form of the male given name Solomon.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My uncle <w>Sol</w> has owned this bakery for over forty years.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Сол</w> владеет этой пекарней уже более сорока лет."
                            }
                        },
                        {
                            "text": "Everyone in the neighborhood knew <w>Sol</w> as a kind and generous man.",
                            "target_lang_translations": {
                                "ru": "Все в округе знали <w>Сола</w> как доброго и щедрого человека."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткая форма мужского имени Соломон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сол",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e861938-15d4-3e8f-9388-b0ecd013994b",
                    "sense_definition": "The ancient Roman god of the Sun.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sun_concept",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "The festival was held in honor of <w>Sol</w> and the return of longer days.",
                            "target_lang_translations": {
                                "ru": "Праздник проводился в честь <w>Сола</w> и возвращения длинных дней."
                            }
                        },
                        {
                            "text": "In the ancient mosaic, <w>Sol</w> is depicted driving a chariot across the sky.",
                            "target_lang_translations": {
                                "ru": "На древней мозаике <w>Сол</w> изображен управляющим колесницей в небе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Sol Invictus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древнеримский бог Солнца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сол",
                                "target_lang_sense_clarification": "мифология"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6bd7fdca-8201-33b9-8f11-5d2f80b8aaca",
                    "sense_definition": "The fifth note in a musical scale, often represented by the syllable 'so'.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The choir warmed up by singing the scale from do to <w>sol</w>.",
                            "target_lang_translations": {
                                "ru": "Хор распевался, исполняя гамму от до до <w>соль</w>."
                            }
                        },
                        {
                            "text": "In the key of C major, the note <w>sol</w> corresponds to G.",
                            "target_lang_translations": {
                                "ru": "В тональности до мажор нота <w>соль</w> соответствует G."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do re mi fa sol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пятая нота музыкальной гаммы, часто обозначаемая слогом «соль»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соль",
                                "target_lang_sense_clarification": "нота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f008a6a1-f33a-3790-959f-8988c94a5a9b",
                    "sense_definition": "A solar day on the planet Mars, which is slightly longer than an Earth day.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rover has been exploring the Martian surface for over one hundred <w>sols</w>.",
                            "target_lang_translations": {
                                "ru": "Марсоход исследует поверхность Марса уже более ста <w>солов</w>."
                            }
                        },
                        {
                            "text": "A <w>sol</w> lasts about twenty-four hours and thirty-nine minutes.",
                            "target_lang_translations": {
                                "ru": "<w>Сол</w> длится около двадцати четырех часов и тридцати девяти минут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Martian sol",
                        "sols on Mars"
                    ],
                    "target_lang_definitions": {
                        "ru": "Солнечные сутки на планете Марс, которые немного длиннее земных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сол",
                                "target_lang_sense_clarification": "марсианские сутки"
                            },
                            {
                                "target_lang_word": "марсианские сутки",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a6b82cb-99ec-3130-98d1-269a0519ae4a",
                    "sense_definition": "The basic monetary unit of Peru.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The taxi ride to the airport cost thirty <w>sols</w>.",
                            "target_lang_translations": {
                                "ru": "Поездка на такси в аэропорт обошлась в тридцать <w>солей</w>."
                            }
                        },
                        {
                            "text": "I need to exchange my dollars for <w>sols</w> before we travel to Lima.",
                            "target_lang_translations": {
                                "ru": "Мне нужно обменять доллары на <w>соли</w> перед поездкой в Лиму."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Peruvian sol"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Peru"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная денежная единица Перу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соль",
                                "target_lang_sense_clarification": "валюта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a492073-c098-34d3-a407-a0039e8c86b1",
                    "sense_definition": "A fluid suspension of a solid substance in a liquid; a type of colloid.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chemist carefully heated the <w>sol</w> to observe the phase change.",
                            "target_lang_translations": {
                                "ru": "Химик осторожно нагрел <w>золь</w>, чтобы пронаблюдать фазовый переход."
                            }
                        },
                        {
                            "text": "Under certain conditions, the <w>sol</w> can transform into a gel structure.",
                            "target_lang_translations": {
                                "ru": "При определенных условиях <w>золь</w> может превратиться в гель."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sol-gel process"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкая взвесь твердого вещества в жидкости; тип коллоидной системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "золь",
                                "target_lang_sense_clarification": "химия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "921cb67d-9e2a-33c6-bbce-919704a6e4a8",
                    "sense_definition": "An abbreviation for 'shit out of luck', meaning to be in a hopeless situation without a solution.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you miss the last train home, you are completely <w>SOL</w>.",
                            "target_lang_translations": {
                                "ru": "Если ты опоздаешь на последнюю электричку домой, ты в полном <w>пролёте</w>."
                            }
                        },
                        {
                            "text": "We forgot to make a reservation, so we were <w>SOL</w> for dinner.",
                            "target_lang_translations": {
                                "ru": "Мы забыли забронировать столик, так что с ужином мы оказались <w>в пролёте</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "totally SOL"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от 'shit out of luck', означающая безнадежную ситуацию без решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в пролёте",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "в безвыходном положении",
                                "target_lang_sense_clarification": "ситуация"
                            }
                        ]
                    }
                }
            ]
        }
    ]

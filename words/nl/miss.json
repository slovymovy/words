                    "sense_id": "bb88bee0-de62-3a3d-b626-7006ee7626a6",
                    "sense_definition": "Winnares van een schoonheidswedstrijd.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "beauty_queen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij werd vorig jaar gekroond tot <w>Miss</w> België.",
                            "target_lang_translations": {

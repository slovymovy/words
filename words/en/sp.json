                    "sense_id": "8e88ebd9-499c-3466-a05d-ccab67f71b24",
                    "sense_definition": "Abbreviation of Spanish (language).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dictionary includes both English and <w>Sp</w> definitions.",
                            "target_lang_translations": {
                                "ru": "Словарь включает определения как на английском, так и на <w>исп.</w>"
                            }
                        },
                        {
                            "text": "He is taking an introductory <w>Sp</w> course at the college.",
                            "target_lang_translations": {
                                "ru": "Он посещает вводный курс <w>исп.</w> языка в колледже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Eng and Sp",
                        "Sp translation"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }

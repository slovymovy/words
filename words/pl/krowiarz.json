                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Krowiarz</w> mieszka w naszym bloku od wielu lat.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Кровяж</w> живет в нашем доме уже много лет."
                            }
                        },
                        {
                            "text": "Nie znam nikogo o nazwisku <w>Krowiarz</w> w tej okolicy.",
                            "target_lang_translations": {

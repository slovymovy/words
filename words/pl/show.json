    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9fba57d1-e29e-34d2-925e-58cc89009a2c",
                    "sense_definition": "Duże, efektowne widowisko artystyczno-rozrywkowe, często o charakterze rewiowym, z udziałem muzyków, tancerzy i prowadzącego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "entertainment_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj wieczorem oglądaliśmy w telewizji nowe, fascynujące <w>show</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы смотрели по телевизору новое увлекательное <w>шоу</w>."
                            }
                        },
                        {
                            "text": "To wielkie <w>show</w> przyciągnęło na stadion tysiące fanów muzyki.",
                            "target_lang_translations": {
                                "ru": "Это грандиозное <w>шоу</w> привлекло на стадион тысячи музыкальных фанатов."
                            }
                        }
                    ],
                    "synonyms": [
                        "widowisko",
                        "spektakl",
                        "przedstawienie"
                    ],
                    "common_phrases": [
                        "telewizyjne show",
                        "wielkie show",
                        "wieczorne show"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное, зрелищное развлекательное представление, часто с участием музыкантов, танцоров и ведущего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоу",
                                "target_lang_sense_clarification": "развлекательная программа"
                            },
                            {
                                "target_lang_word": "представление",
                                "target_lang_sense_clarification": "сценическое действие"
                            },
                            {
                                "target_lang_word": "зрелище",
                                "target_lang_sense_clarification": "эффектное событие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "911a402f-2037-37bb-8eda-814300e515e0",
                    "sense_definition": "Efektowny pokaz niezwykłych umiejętności lub brawurowe wystąpienie artystyczne.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "skill_display",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gitarzysta dał niesamowite <w>show</w> podczas solówki na koncercie.",
                            "target_lang_translations": {
                                "ru": "Гитарист устроил невероятное <w>шоу</w> во время соло на концерте."
                            }
                        },
                        {
                            "text": "Jego występ kulinarny to było prawdziwe <w>show</w> dla wszystkich gości.",
                            "target_lang_translations": {
                                "ru": "Его кулинарное выступление стало настоящим <w>шоу</w> для всех гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokaz"
                    ],
                    "common_phrases": [
                        "dać show",
                        "niezłe show",
                        "zrobić show"
                    ],
                    "target_lang_definitions": {
                        "ru": "Эффектная демонстрация необычных навыков или яркое артистическое выступление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоу",
                                "target_lang_sense_clarification": "яркое выступление"
                            },
                            {
                                "target_lang_word": "перформанс",
                                "target_lang_sense_clarification": "выступление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "307fbb3e-17c6-34ca-9250-4df44b5f25d3",
                    "sense_definition": "Wydarzenie w życiu społecznym lub politycznym mające charakter sensacyjnego spektaklu, często obliczone na przyciągnięcie uwagi.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "public_spectacle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obrady parlamentu zamieniły się w żenujące <w>show</w> dla mediów.",
                            "target_lang_translations": {
                                "ru": "Заседания парламента превратились в постыдное <w>шоу</w> для СМИ."
                            }
                        },
                        {
                            "text": "To całe polityczne <w>show</w> ma na celu jedynie odwrócenie uwagi od problemów.",
                            "target_lang_translations": {
                                "ru": "Всё это политическое <w>шоу</w> нацелено лишь на отвлечение внимания от проблем."
                            }
                        }
                    ],
                    "synonyms": [
                        "widowisko"
                    ],
                    "common_phrases": [
                        "medialne show",
                        "polityczne show",
                        "tanie show"
                    ],
                    "target_lang_definitions": {
                        "ru": "Событие в общественной или политической жизни, имеющее характер сенсационного спектакля, часто рассчитанное на привлечение внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоу",
                                "target_lang_sense_clarification": "показуха"
                            },
                            {
                                "target_lang_word": "спектакль",
                                "target_lang_sense_clarification": "разыгранное действие"
                            },
                            {
                                "target_lang_word": "цирк",
                                "target_lang_sense_clarification": "балаган, фарс"
                            }
                        ]
                    }
                }
            ]
        }
    ]

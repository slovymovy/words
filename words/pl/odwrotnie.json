    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "eefa2770-f2ec-31fa-85ff-e8d1f437d911",
                    "sense_definition": "W sposób przeciwstawny do normalnego, w innym kierunku, w innej kolejności lub na drugą stronę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Założyłeś sweter <w>odwrotnie</w> i teraz metka wystaje ci na zewnątrz.",
                            "target_lang_translations": {
                                "ru": "Ты надел свитер <w>наизнанку</w>, и теперь бирка торчит наружу."
                            }
                        },
                        {
                            "text": "Aby otworzyć te drzwi, musisz przekręcić klucz <w>odwrotnie</w> do ruchu wskazówek zegara.",
                            "target_lang_translations": {
                                "ru": "Чтобы открыть эту дверь, нужно повернуть ключ <w>против</w> часовой стрелки."
                            }
                        }
                    ],
                    "synonyms": [
                        "na odwrót",
                        "na opak",
                        "wspak"
                    ],
                    "common_phrases": [
                        "zupełnie odwrotnie",
                        "dokładnie odwrotnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Противоположным образом, в обратном направлении, в другом порядке или другой стороной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наоборот",
                                "target_lang_sense_clarification": "противоположно"
                            },
                            {
                                "target_lang_word": "наизнанку",
                                "target_lang_sense_clarification": "об одежде"
                            },
                            {
                                "target_lang_word": "задом наперёд",
                                "target_lang_sense_clarification": "задней стороной вперёд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad268f52-2223-3161-a007-2263755105e2",
                    "sense_definition": "W sposób sprzeczny z tym, co było wcześniej powiedziane, zrobione lub czego oczekiwano.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "logical_opposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myślałem, że ten egzamin będzie bardzo trudny, ale było <w>odwrotnie</w>.",
                            "target_lang_translations": {
                                "ru": "Я думал, что этот экзамен будет очень трудным, но вышло <w>наоборот</w>."
                            }
                        },
                        {
                            "text": "Czasami to uczniowie uczą nauczycieli, a nie <w>odwrotnie</w>.",
                            "target_lang_translations": {
                                "ru": "Иногда ученики учат учителей, а не <w>наоборот</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeciwnie",
                        "inaczej",
                        "odmiennie"
                    ],
                    "common_phrases": [
                        "i odwrotnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Противоположно тому, что было сказано ранее, сделано или ожидалось."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наоборот",
                                "target_lang_sense_clarification": "вопреки ожиданиям"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "f1c5832e-1da1-3a9f-8193-beaf2fc1a18a",
                    "sense_definition": "Słowo używane do zaprzeczenia poprzedniej wypowiedzi i wskazania, że prawda jest zupełnie inna.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "logical_opposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie jestem na ciebie zły, wręcz <w>odwrotnie</w>, bardzo się cieszę z twojego sukcesu.",
                            "target_lang_translations": {
                                "ru": "Я не сержусь на тебя, совсем <w>наоборот</w>, я очень рад твоему успеху."
                            }
                        },
                        {
                            "text": "To wcale nie jest nudna książka, <w>odwrotnie</w>, czyta się ją jednym tchem.",
                            "target_lang_translations": {
                                "ru": "Это вовсе не скучная книга, <w>наоборот</w>, она читается на одном дыхании."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeciwnie"
                    ],
                    "common_phrases": [
                        "wręcz odwrotnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое для отрицания предыдущего высказывания и указания на то, что истина совершенно ина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наоборот",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "напротив",
                                "target_lang_sense_clarification": "вводное слово"
                            }
                        ]
                    }
                }
            ]
        }
    ]

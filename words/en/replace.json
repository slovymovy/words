{
    "word_family": [
        "replacement",
        "replaceable",
        "irreplaceable",
        "replacer"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "364558ed-5a5b-39d1-9cea-ae27ef3fa31a",
                    "sense_definition": "To remove something that is old, broken, or lost and put a new one in its place.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "substitution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The batteries were dead, so I <w>replaced</w> them immediately.",
                            "target_lang_translations": {
                                "ru": "Батарейки сели, поэтому я сразу же <w>заменил</w> их."
                            }
                        },
                        {
                            "text": "I decided to <w>replace</w> my old car with a newer model.",
                            "target_lang_translations": {
                                "ru": "Я решил <w>заменить</w> свою старую машину на более новую модель."
                            }
                        }
                    ],
                    "synonyms": [
                        "exchange",
                        "swap"
                    ],
                    "common_phrases": [
                        "replace with",
                        "replace by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убрать что-то старое, сломанное или потерянное и поместить на его место новое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заменить",
                                "target_lang_sense_clarification": "поменять на новое"
                            },
                            {
                                "target_lang_word": "сменить",
                                "target_lang_sense_clarification": "произвести замену"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ba8b20e-f06b-3488-9503-665d66bebba1",
                    "sense_definition": "To take the place of someone or something; to be used instead of something else.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "substitution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This new security pass <w>replaces</w> the one you were given earlier.",
                            "target_lang_translations": {
                                "ru": "Этот новый пропуск <w>заменяет</w> тот, который вам выдали ранее."
                            }
                        },
                        {
                            "text": "Robots will never fully <w>replace</w> human workers in this industry.",
                            "target_lang_translations": {
                                "ru": "Роботы никогда полностью не <w>заменят</w> людей в этой отрасли."
                            }
                        }
                    ],
                    "common_phrases": [
                        "replace someone",
                        "hard to replace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занять место кого-то или чего-то; использоваться вместо чего-то другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заменить",
                                "target_lang_sense_clarification": "использоваться вместо"
                            },
                            {
                                "target_lang_word": "заместить",
                                "target_lang_sense_clarification": "выполнять функции вместо кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ec028f7-97bc-3191-8534-b87ddf74297f",
                    "sense_definition": "To put something back in its correct or original position.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "restoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When you have finished using the telephone, please <w>replace</w> the handset.",
                            "target_lang_translations": {
                                "ru": "Когда закончите разговор, пожалуйста, <w>положите</w> трубку <w>на место</w>."
                            }
                        },
                        {
                            "text": "She carefully <w>replaced</w> the book on the shelf after reading it.",
                            "target_lang_translations": {
                                "ru": "Прочитав книгу, она аккуратно <w>поставила</w> её <w>на место</w> на полку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "replace the receiver",
                        "replace the lid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вернуть что-то в правильное или исходное положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вернуть на место",
                                "target_lang_sense_clarification": "положить обратно"
                            },
                            {
                                "target_lang_word": "поставить на место",
                                "target_lang_sense_clarification": "разместить в исходном положении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9bfcb90-c808-3e2a-8c74-8da34188c84b",
                    "sense_definition": "To pay back money or provide a new item for one that was used or lost.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "restoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can take cash from the box, but you must <w>replace</w> it tomorrow.",
                            "target_lang_translations": {
                                "ru": "Ты можешь взять наличные из коробки, но завтра ты должен их <w>вернуть</w>."
                            }
                        },
                        {
                            "text": "The insurance company agreed to <w>replace</w> the stolen jewelry.",
                            "target_lang_translations": {
                                "ru": "Страховая компания согласилась <w>возместить</w> стоимость украденных драгоценностей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вернуть деньги или предоставить новый предмет взамен использованного или потерянного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возместить",
                                "target_lang_sense_clarification": "компенсировать убыток"
                            },
                            {
                                "target_lang_word": "вернуть",
                                "target_lang_sense_clarification": "отдать взятое"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
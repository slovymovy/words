                    "common_phrases": [
                        "at war",
                        "at peace",
                        "at rest",
                        "at play",
                        "at risk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания состояния, условия или непрерывной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "находиться в определенном состоянии"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "находиться на войне/службе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf14f885-dd5b-3139-bda7-03be41730041",
                    "sense_definition": "Used to indicate a cause, source, or reaction to something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cause_reaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We were all surprised <w>at</w> the sudden change in the weather.",

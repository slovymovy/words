            "senses": [
                {
                    "sense_id": "79210308-10fd-373b-991b-82f48f094155",
                    "sense_definition": "Vervoeging van <w>vlotten</w>: eerste, tweede en derde persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het werk <w>vlot</w> vandaag erg goed, we zijn bijna klaar.",
                            "target_lang_translations": {
                                "ru": "Работа сегодня <w>спорится</w> очень хорошо, мы почти закончили.",
                                "en": "The work is <w>progressing</w> very well today, we are almost done."
                            }
                        },
                        {
                            "text": "Als de samenwerking niet <w>vlot</w>, moeten we duidelijke afspraken maken.",
                            "target_lang_translations": {
                                "ru": "Если сотрудничество не <w>ладится</w>, нам нужно заключить четкие соглашения.",
                                "en": "If the cooperation does not <w>run smoothly</w>, we must make clear agreements."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола vlotten: идти успешно, продвигаться вперед (о работе или деле).",
                        "en": "Verb form of vlotten: to proceed successfully, to progress well (of work or affairs)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спориться",
                                "target_lang_sense_clarification": "идти успешно (о работе)"
                            },
                            {
                                "target_lang_word": "ладиться",
                                "target_lang_sense_clarification": "получаться хорошо"
                            },
                            {
                                "target_lang_word": "продвигаться",
                                "target_lang_sense_clarification": "идти вперед"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "progress",
                                "target_lang_sense_clarification": "to move forward successfully"
                            },
                            {
                                "target_lang_word": "go smoothly",
                                "target_lang_sense_clarification": "to happen without problems"
                            }

            "senses": [
                {
                    "sense_id": "8945a09d-e97a-3ab2-b1a7-891bc577269e",
                    "sense_definition": "But sportowy na miękkiej podeszwie, przeznaczony do uprawiania sportu lub codziennego użytku (nazwa pochodzi od marki Adidas, ale jest używana potocznie dla wszystkich butów tego typu).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shoes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lekcje wychowania fizycznego musisz przynieść czyste <adidasy>.",
                            "target_lang_translations": {
                                "ru": "На урок физкультуры ты должен принести чистые <w>кроссовки</w>."
                            }
                        },
                        {
                            "text": "Kupiłem sobie nową parę <adidasów> do biegania po parku.",
                            "target_lang_translations": {
                                "ru": "Я купил себе новую пару <w>кроссовок</w> для бега в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "but sportowy",
                        "cichobieg",
                        "sneakers"
                    ],
                    "antonyms": [
                        "pantofel",
                        "szpilka",
                        "kozak"
                    ],
                    "common_phrases": [
                        "nowe adidasy",
                        "para adidasów",
                        "chodzić w adidasach"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "genericized trademark"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивная обувь на мягкой подошве, предназначенная для занятий спортом или повседневного использования (название происходит от бренда Adidas, но используется в разговорной речи для любой обуви этого типа)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроссовки",
                                "target_lang_sense_clarification": "спортивная обувь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79251092-cfed-3291-8769-78db5172e476",
                    "sense_definition": "Slangowe określenie zespołu nabytego niedoboru odporności (AIDS).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "disease_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tamtych czasach wiedza o profilaktyce była mała i ludzie bali się <adidasa>.",
                            "target_lang_translations": {
                                "ru": "В те времена знаний о профилактике было мало, и люди боялись <w>СПИДа</w>."
                            }
                        },
                        {
                            "text": "Krążyły plotki, że jeden z muzyków złapał <adidasa>.",
                            "target_lang_translations": {
                                "ru": "Ходили слухи, что один из музыкантов подхватил <w>СПИД</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "AIDS"
                    ],
                    "common_phrases": [

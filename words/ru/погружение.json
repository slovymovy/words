    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "94366d0e-6dde-35d4-a29a-b6926794472f",
                    "sense_definition": "Действие по значению глагола <w>погружать</w>; опускание предмета в жидкость, сыпучее вещество или иную среду.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для эксперимента потребовалось полное <w>погружение</w> детали в масло.",
                            "target_lang_translations": {
                                "en": "The experiment required the complete <w>immersion</w> of the part in oil."
                            }
                        },
                        {
                            "text": "Мы наблюдали за медленным <w>погружением</w> тяжелого камня в воду.",
                            "target_lang_translations": {
                                "en": "We watched the slow <w>submersion</w> of the heavy stone into the water."
                            }
                        }
                    ],
                    "synonyms": [
                        "опускание",
                        "окунание"
                    ],
                    "antonyms": [
                        "всплытие",
                        "извлечение"
                    ],
                    "common_phrases": [
                        "полное погружение",
                        "погружение в воду",
                        "глубина погружения"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of lowering an object into a liquid or other medium."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "immersion",
                                "target_lang_sense_clarification": "act of dipping into liquid"
                            },
                            {
                                "target_lang_word": "submersion",
                                "target_lang_sense_clarification": "act of placing completely under liquid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11b7f9cd-bb6f-3597-a9fc-70f7f950033a",
                    "sense_definition": "Спуск под воду на глубину (обычно о водолазах, подводных лодках или батискафах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водолазы начали <w>погружение</w> для осмотра морского дна.",
                            "target_lang_translations": {
                                "en": "The divers began their <w>descent</w> to inspect the seabed."
                            }
                        },
                        {
                            "text": "Подводная лодка совершила аварийное <w>погружение</w> во время учений.",
                            "target_lang_translations": {
                                "en": "The submarine performed an emergency <w>dive</w> during the exercises."
                            }
                        }
                    ],
                    "synonyms": [
                        "ныряние"
                    ],
                    "antonyms": [
                        "всплытие"
                    ],
                    "common_phrases": [
                        "погружение с аквалангом",
                        "скорость погружения",
                        "аварийное погружение"
                    ],
                    "target_lang_definitions": {
                        "en": "Descent underwater to a depth (usually regarding divers or submarines)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dive",
                                "target_lang_sense_clarification": "act of going underwater"
                            },
                            {
                                "target_lang_word": "descent",
                                "target_lang_sense_clarification": "movement downwards underwater"
                            },
                            {
                                "target_lang_word": "submersion",
                                "target_lang_sense_clarification": "process of going below the surface"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43471a2c-9e9a-3a60-91b9-0c3effb14388",
                    "sense_definition": "Переход в какое-либо состояние или глубокая сосредоточенность на каком-либо занятии.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для быстрого изучения языка полезно полное <w>погружение</w> в языковую среду.",
                            "target_lang_translations": {
                                "en": "For learning a language quickly, total <w>immersion</w> in the language environment is useful."
                            }
                        },
                        {
                            "text": "Его <w>погружение</w> в чтение было настолько глубоким, что он не слышал звонка.",
                            "target_lang_translations": {
                                "en": "His <w>absorption</w> in reading was so deep that he didn't hear the bell."
                            }
                        }
                    ],
                    "synonyms": [
                        "углубление"
                    ],
                    "common_phrases": [
                        "погружение в сон",
                        "погружение в мысли",
                        "языковое погружение",
                        "погружение в темноту"
                    ],
                    "target_lang_definitions": {
                        "en": "Transition into a specific state or deep concentration on an activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "immersion",
                                "target_lang_sense_clarification": "deep mental involvement or environment"
                            },
                            {
                                "target_lang_word": "absorption",
                                "target_lang_sense_clarification": "deep concentration or engrossment"
                            }
                        ]
                    }
                }
            ]
        }
    ]

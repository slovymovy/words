    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8461868c-d82c-391d-bb0a-cfc86973bbb0",
                    "sense_definition": "An irrational constant representing the ratio of a circle's circumference to its diameter; approximately 3.14.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "math_symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Students are often asked to calculate the area of a circle using <w>pi</w>.",
                            "target_lang_translations": {
                                "ru": "Студентов часто просят вычислить площадь круга, используя <w>пи</w>."
                            }
                        },
                        {
                            "text": "The value of <w>pi</w> cannot be expressed as a simple fraction.",
                            "target_lang_translations": {
                                "ru": "Значение <w>пи</w> невозможно выразить простой дробью."
                            }
                        }
                    ],
                    "synonyms": [
                        "Archimedes' constant"
                    ],
                    "common_phrases": [
                        "value of pi",
                        "digits of pi",
                        "calculate pi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Математическая константа, выражающая отношение длины окружности к длине её диаметра; приблизительно 3,14."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "число пи",
                                "target_lang_sense_clarification": "математическая константа"
                            },
                            {
                                "target_lang_word": "пи",
                                "target_lang_sense_clarification": "число"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea55117e-7f75-3f69-bcf6-130e13575c0e",
                    "sense_definition": "The sixteenth letter of the Greek alphabet (Π, π).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "math_symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The symbol for <w>pi</w> is widely recognized in mathematics and physics.",
                            "target_lang_translations": {
                                "ru": "Символ <w>пи</w> широко узнаваем в математике и физике."
                            }
                        },
                        {
                            "text": "He wrote a small Greek <w>pi</w> next to the equation.",
                            "target_lang_translations": {
                                "ru": "Он написал маленькую греческую букву <w>пи</w> рядом с уравнением."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шестнадцатая буква греческого алфавита (Π, π)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пи",
                                "target_lang_sense_clarification": "буква греческого алфавита"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bf4b416-5e35-3525-a770-efd49896a461",
                    "sense_definition": "Initialism of <w>private investigator</w>.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "acronyms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family hired a <w>PI</w> to find their missing relative.",
                            "target_lang_translations": {
                                "ru": "Семья наняла <w>частного детектива</w>, чтобы найти пропавшего родственника."
                            }
                        },
                        {
                            "text": "In the movie, the <w>PI</w> took photographs from a parked car.",
                            "target_lang_translations": {
                                "ru": "В фильме <w>частный детектив</w> делал фотографии из припаркованной машины."
                            }

{
    "word_family": [
        "kieca"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f9e3e680-63c4-3d4d-94f0-1dd6f740dad9",
                    "sense_definition": "Potoczne określenie sukienki, czyli damskiego ubioru okrywającego tułów i nogi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_dress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić sobie jakąś nową <w>kieckę</w> na wesele mojej siostry.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить себе какое-нибудь новое <w>платье</w> на свадьбу сестры."
                            }
                        },
                        {
                            "text": "Dziewczyny pokazały się na balu w takich samych <w>kieckach</w>.",
                            "target_lang_translations": {
                                "ru": "Девушки появились на балу в одинаковых <w>платьях</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sukienka"
                    ],
                    "common_phrases": [
                        "krótka kiecka",
                        "nowa kiecka",
                        "letnia kiecka",
                        "wbić się w kieckę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название платья — женской одежды, закрывающей туловище и ноги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платье",
                                "target_lang_sense_clarification": "женская одежда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9681ab7d-826b-31d9-97f6-7b9bf432db45",
                    "sense_definition": "Regionalne, używane głównie na Górnym Śląsku, określenie spódnicy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_skirt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia nosiła szeroką, wełnianą <w>kieckę</w> sięgającą aż do kostek.",
                            "target_lang_translations": {
                                "ru": "Бабушка носила широкую шерстяную <w>юбку</w> до самых щиколоток."
                            }
                        },
                        {
                            "text": "W tym regionie na każdą spódnicę mówi się po prostu <w>kiecka</w>.",
                            "target_lang_translations": {
                                "ru": "В этом регионе любую юбку называют просто <w>юбкой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "spódnica",
                        "gw. (Górny Śląsk) kecka"
                    ],
                    "common_phrases": [
                        "szeroka kiecka",
                        "śląska kiecka"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Górny Śląsk"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное название юбки (используется в основном в Верхней Силезии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юбка",
                                "target_lang_sense_clarification": "региональное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19759588-1e9a-30b9-8258-c02ab332857c",
                    "sense_definition": "Dawne określenie kapoty lub luźnego wierzchniego okrycia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_outerwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłop narzucił na ramiona starą <w>kieckę</w> i wyszedł przed chatę.",
                            "target_lang_translations": {
                                "ru": "Мужик набросил на плечи старый <w>кафтан</w> и вышел из хаты."
                            }
                        },
                        {
                            "text": "Pod płotem leżała zniszczona, szara <w>kiecka</w>, której nikt już nie chciał.",
                            "target_lang_translations": {
                                "ru": "Под забором лежал изношенный серый <w>кафтан</w>, который никому уже был не нужен."
                            }
                        }
                    ],
                    "synonyms": [
                        "kapota"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "określenie dawne, rzadko spotykane współcześnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название кафтана или свободной верхней одежды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кафтан",
                                "target_lang_sense_clarification": "устаревшее"
                            },
                            {
                                "target_lang_word": "балахон",
                                "target_lang_sense_clarification": "свободная одежда"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
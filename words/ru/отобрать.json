    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fee0c423-b42d-325d-820f-7c29e391fbf9",
                    "sense_definition": "Взять что-либо у кого-либо обратно или насильно; лишить кого-либо чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хулиганы попытались <отобрать> у школьника новый телефон.",
                            "target_lang_translations": {
                                "en": "The hooligans tried to <take away> the student's new phone."
                            }
                        },
                        {
                            "text": "Если ты будешь плохо себя вести, мама может <отобрать> игрушку.",
                            "target_lang_translations": {
                                "en": "If you behave badly, mom might <take away> the toy."
                            }
                        }
                    ],
                    "synonyms": [
                        "отнять",
                        "изъять",
                        "конфисковать"
                    ],
                    "antonyms": [
                        "дать"
                    ],
                    "common_phrases": [
                        "отобрать права",
                        "отобрать паспорт",
                        "отобрать имущество",
                        "отобрать деньги"
                    ],
                    "target_lang_definitions": {
                        "en": "To take something back or forcibly from someone; to deprive someone of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take away",
                                "target_lang_sense_clarification": "general usage, removing possession"
                            },
                            {
                                "target_lang_word": "seize",
                                "target_lang_sense_clarification": "forcibly or suddenly"
                            },
                            {
                                "target_lang_word": "confiscate",
                                "target_lang_sense_clarification": "by authority"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a902aca-c943-36df-a457-bf6477c577bd",
                    "sense_definition": "Выделить или выбрать кого-либо или что-либо из множества по какому-либо признаку.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам нужно <отобрать> самые лучшие фотографии для выставки.",
                            "target_lang_translations": {
                                "en": "We need to <select> the best photos for the exhibition."
                            }
                        },
                        {
                            "text": "Жюри предстоит <отобрать> победителей из сотни участников.",
                            "target_lang_translations": {
                                "en": "The jury has to <select> the winners from hundreds of participants."
                            }
                        }
                    ],
                    "synonyms": [
                        "выбрать",
                        "выделить",
                        "подобрать",
                        "избрать"
                    ],
                    "common_phrases": [
                        "отобрать кандидатов",
                        "отобрать образцы",
                        "тщательно отобрать",
                        "отобрать лучшие"
                    ],
                    "target_lang_definitions": {
                        "en": "To select or choose someone or something from a multitude based on some criteria."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "select",
                                "target_lang_sense_clarification": "to choose carefully from a group"
                            },
                            {
                                "target_lang_word": "choose",
                                "target_lang_sense_clarification": "general selection"
                            },
                            {
                                "target_lang_word": "pick out",
                                "target_lang_sense_clarification": "to distinguish from a group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8897f3c-3c66-37a1-94b4-24f5cc42ff35",
                    "sense_definition": "Собрать или получить сведения, показания или объяснения путём опроса (обычно в официальной речи).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Следователь должен <отобрать> показания у всех свидетелей происшествия.",
                            "target_lang_translations": {
                                "en": "The investigator must <take> statements from all witnesses of the incident."
                            }
                        },
                        {
                            "text": "С сотрудника решили <отобрать> объяснительную записку по факту нарушения.",
                            "target_lang_translations": {
                                "en": "They decided to <obtain> a written explanation from the employee regarding the violation."
                            }
                        }
                    ],
                    "common_phrases": [
                        "отобрать объяснение",
                        "отобрать показания",
                        "отобрать подписку"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "в общем значении сбора сведений"
                        },
                        {
                            "trait_type": "form",
                            "comment": "официально-деловой стиль (о показаниях, подписках)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To collect or obtain information, testimony, or explanations via questioning (usually in official contexts)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "used with statements, testimony, or depositions"
                            },
                            {
                                "target_lang_word": "obtain",
                                "target_lang_sense_clarification": "formal acquisition of documents or statements"
                            }
                        ]
                    }
                }
            ]
        }
    ]

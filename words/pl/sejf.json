            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8efd4bd0-0b42-31e2-b03f-6118733db6ef",
                    "sense_definition": "Mocna, zamykana szafa lub skrzynia, zwykle metalowa, służąca do bezpiecznego przechowywania pieniędzy, dokumentów i cennych przedmiotów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "safe_storage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor schował ważne umowy do pancernego <w>sejfu</w>.",
                            "target_lang_translations": {
                                "ru": "Директор спрятал важные договоры в бронированный <w>сейф</w>."
                            }
                        },
                        {
                            "text": "Złodziej próbował otworzyć <w>sejf</w>, ale nie znał szyfru.",
                            "target_lang_translations": {
                                "ru": "Вор пытался открыть <w>сейф</w>, но не знал шифра."
                            }
                        }
                    ],
                    "synonyms": [
                        "kasa pancerna",
                        "szafa pancerna"
                    ],
                    "common_phrases": [
                        "otworzyć sejf",
                        "zamknąć w sejfie",
                        "szyfr do sejfu",
                        "klucz do sejfu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочный запираемый шкаф или ящик, обычно металлический, предназначенный для безопасного хранения денег, документов и ценных предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сейф",
                                "target_lang_sense_clarification": "металлический шкаф для ценностей"
                            },
                            {
                                "target_lang_word": "несгораемый шкаф",
                                "target_lang_sense_clarification": "формальное или устаревшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "579a67ba-a2bb-3d8f-8159-1efec1018a3d",
                    "sense_definition": "Skrytka w banku lub hotelu, którą można wynająć do przechowywania wartościowych rzeczy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "safe_storage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wakacji zawsze zostawiamy paszporty w hotelowym <w>sejfie</w>.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска мы всегда оставляем паспорта в гостиничном <w>сейфе</w>."
                            }
                        },
                        {
                            "text": "Klient banku poprosił o dostęp do swojego prywatnego <w>sejfu</w>.",
                            "target_lang_translations": {
                                "ru": "Клиент банка попросил доступ к своему личному <w>сейфу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sejf bankowy",
                        "sejf hotelowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ячейка в банке или небольшой сейф в отеле, которые можно арендовать для хранения ценных вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сейф",
                                "target_lang_sense_clarification": "в отеле или банке"
                            },
                            {
                                "target_lang_word": "ячейка",

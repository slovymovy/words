            "senses": [
                {
                    "sense_id": "b2e6646d-27dc-3e4b-8858-21f0a4bbb0b8",
                    "sense_definition": "Een afzonderlijk deel dat samen met andere delen een groter geheel of een constructie vormt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf richt zich op een specifiek <w>segment</w> van de markt.",
                            "target_lang_translations": {
                                "en": "The company focuses on a specific <w>segment</w> of the market.",
                                "ru": "Компания ориентируется на конкретный <w>сегмент</w> рынка."
                            }
                        },
                        {
                            "text": "Dit <w>segment</w> van de brug moet binnenkort worden vervangen.",
                            "target_lang_translations": {
                                "en": "This <w>section</w> of the bridge needs to be replaced soon.",
                                "ru": "Эта <w>секция</w> моста должна быть вскоре заменена."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderdeel",
                        "sectie",
                        "deel",
                        "part"
                    ],
                    "antonyms": [
                        "geheel"
                    ],
                    "common_phrases": [
                        "een specifiek segment",
                        "segment van de markt",
                        "bepaald segment"
                    ],
                    "target_lang_definitions": {
                        "en": "A separate part that forms a larger whole or construction together with other parts.",
                        "ru": "Отдельная часть, которая вместе с другими частями образует более крупное целое или конструкцию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "segment",
                                "target_lang_sense_clarification": "part of a whole"
                            },
                            {
                                "target_lang_word": "section",
                                "target_lang_sense_clarification": "distinct part or division"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сегмент",
                                "target_lang_sense_clarification": "часть рынка или общества"
                            },
                            {
                                "target_lang_word": "секция",
                                "target_lang_sense_clarification": "конструктивная часть"
                            },
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "отрезок, зона"

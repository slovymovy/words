                {
                    "sense_id": "f8e2e922-9322-30ab-a4ce-0c9f48309fda",
                    "sense_definition": "Een lid van het dagelijks bestuur van een Nederlandse provincie (Gedeputeerde Staten).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "political_office",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>gedeputeerde</w> voor ruimtelijke ordening presenteerde gisteren de nieuwe plannen.",
                            "target_lang_translations": {
                                "en": "The <w>member of the Provincial Executive</w> for spatial planning presented the new plans yesterday.",
                                "ru": "<w>Член исполнительного совета</w> по вопросам пространственного планирования вчера представил новые планы."
                            }
                        },
                        {
                            "text": "Na de verkiezingen werd zij benoemd tot <w>gedeputeerde</w> in de provincie Utrecht.",
                            "target_lang_translations": {
                                "en": "After the elections, she was appointed <w>member of the Provincial Executive</w> in the province of Utrecht.",
                                "ru": "После выборов она была назначена <w>членом администрации</w> провинции Утрехт."
                            }
                        }
                    ],
                    "synonyms": [
                        "provinciebestuurder"
                    ],
                    "common_phrases": [
                        "gedeputeerde staten",
                        "provinciale gedeputeerde",
                        "gedeputeerde voor verkeer"
                    ],
                    "target_lang_definitions": {
                        "en": "A member of the executive board of a Dutch province.",
                        "ru": "Член исполнительного органа власти нидерландской провинции (Gedeputeerde Staten)."
                    },

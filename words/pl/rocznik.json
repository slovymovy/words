    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6852549e-4e85-3089-ae22-89427b3b4395",
                    "sense_definition": "Ogół osób urodzonych w tym samym roku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_people_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>rocznik</w> musiał zdawać trudniejszą maturę z matematyki.",
                            "target_lang_translations": {
                                "ru": "Моему <w>году</w> пришлось сдавать более сложный экзамен по математике."
                            }
                        },
                        {
                            "text": "To jest bardzo udany <w>rocznik</w>, wielu ludzi odniosło sukces zawodowy.",
                            "target_lang_translations": {
                                "ru": "Это очень успешный <w>год</w>, многие люди добились профессионального успеха."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mój rocznik",
                        "starszy rocznik",
                        "rocznik osiemdziesiąty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность людей, родившихся в одном и том же году."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "год рождения",
                                "target_lang_sense_clarification": "характеристика возраста"
                            },
                            {
                                "target_lang_word": "год",
                                "target_lang_sense_clarification": "разг. о людях одного года рождения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9051c103-abb9-34ea-a3ec-a9de104b6ffe",
                    "sense_definition": "Rok wyprodukowania danego przedmiotu (np. samochodu) lub wina.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_object_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten samochód to <w>rocznik</w> dwa tysiące dziesiąty, ale jest w świetnym stanie.",
                            "target_lang_translations": {
                                "ru": "У этой машины <w>год выпуска</w> — две тысячи десятый, но она в отличном состоянии."
                            }
                        },
                        {
                            "text": "Kelner polecił nam wyśmienity <w>rocznik</w> czerwonego wina do kolacji.",
                            "target_lang_translations": {
                                "ru": "Официант порекомендовал нам красное вино отличного <w>года урожая</w> к ужину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rocznik wina",
                        "rocznik samochodu",
                        "dobry rocznik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Год производства предмета или год урожая вина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "год выпуска",
                                "target_lang_sense_clarification": "техника, предметы"
                            },
                            {
                                "target_lang_word": "год урожая",
                                "target_lang_sense_clarification": "вино"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec4e13e0-5d66-3cb4-abc1-e8ff1bdf5135",
                    "sense_definition": "Grupa uczniów, studentów lub żołnierzy rozpoczynających lub kończących naukę albo służbę w tym samym czasie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_people_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały nasz <w>rocznik</w> spotkał się na uroczystym zjeździe absolwentów.",
                            "target_lang_translations": {
                                "ru": "Весь наш <w>выпуск</w> встретился на торжественном собрании выпускников."
                            }
                        },
                        {
                            "text": "Jesteśmy pierwszym <w>rocznikiem</w>, który uczy się według nowego programu studiów.",
                            "target_lang_translations": {
                                "ru": "Мы являемся первым <w>курсом</w>, который учится по новой программе обучения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rocznik studiów",
                        "spotkanie rocznika",
                        "zjazd rocznika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа учащихся или военнослужащих одного года набора или выпуска."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курс",
                                "target_lang_sense_clarification": "студенты"
                            },
                            {
                                "target_lang_word": "выпуск",
                                "target_lang_sense_clarification": "окончившие обучение"
                            },
                            {
                                "target_lang_word": "призыв",
                                "target_lang_sense_clarification": "военнослужащие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a80feaf-7265-3a9e-933e-0ce9d2a0d375",
                    "sense_definition": "Czasopismo naukowe, informacyjne lub statystyczne ukazujące się raz w roku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_publication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W bibliotece uniwersyteckiej znalazłem stary <w>rocznik</w> czasopisma medycznego.",
                            "target_lang_translations": {
                                "ru": "В университетской библиотеке я нашел старый <w>ежегодник</w> медицинского журнала."
                            }
                        },
                        {
                            "text": "Główny Urząd Statystyczny opublikował nowy <w>rocznik</w> demograficzny z aktualnymi danymi.",
                            "target_lang_translations": {
                                "ru": "Главное статистическое управление опубликовало новый демографический <w>ежегодник</w> с актуальными данными."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rocznik statystyczny",
                        "czytać rocznik",
                        "rocznik literacki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Периодическое издание научного или информационного характера, выходящее раз в год."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ежегодник",
                                "target_lang_sense_clarification": "издание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74e4713b-b303-379d-84cf-be42326075cc",
                    "sense_definition": "Zwierzę, zazwyczaj koń lub źrebię, które ma rok.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zielonym pastwisku biegał wesoły <w>rocznik</w> o kasztanowej maści.",
                            "target_lang_translations": {
                                "ru": "На зеленом пастбище бегал веселый <w>годовик</w> каштановой масти."
                            }
                        },
                        {
                            "text": "Doświadczony hodowca postanowił sprzedać ten <w>rocznik</w> innemu gospodarzowi.",
                            "target_lang_translations": {
                                "ru": "Опытный заводчик решил продать этого <w>годовика</w> другому хозяину."
                            }
                        }
                    ],
                    "synonyms": [
                        "roczniak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Животное (обычно лошадь) в возрасте одного года."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "годовик",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "34013cb9-6928-3b6a-80d5-783875bb05a2",
                    "sense_definition": "Pracownik zajmujący się zawodowo sporządzaniem wycen, kosztorysów i kalkulacji finansowych.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "professions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony <w>kalkulator</w> potrafi precyzyjnie oszacować koszty budowy domu.",

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1447ada8-d5ae-3cac-b062-633dc12af909",
                    "sense_definition": "Iets met de handen of armen vastpakken en niet loslaten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_holding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je mijn tas even voor me <w>vasthouden</w>?",
                            "target_lang_translations": {
                                "en": "Can you <w>hold</w> my bag for me for a moment?",
                                "ru": "Ты можешь <w>подержать</w> мою сумку?"
                            }
                        },
                        {
                            "text": "De bange kleuter wilde de hand van zijn moeder <w>vasthouden</w>.",
                            "target_lang_translations": {
                                "en": "The scared toddler wanted to <w>hold</w> his mother's hand.",
                                "ru": "Испуганный малыш хотел <w>держать</w> маму за руку."
                            }
                        }
                    ],
                    "synonyms": [
                        "beethouden",
                        "omklemmen"
                    ],
                    "antonyms": [
                        "loslaten"
                    ],
                    "common_phrases": [
                        "iemands hand vasthouden",
                        "stevig vasthouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To grasp something with the hands or arms and not let go.",
                        "ru": "Схватить что-то руками или обнять и не отпускать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "to grasp or grip with hands"
                            },
                            {
                                "target_lang_word": "hold on to",
                                "target_lang_sense_clarification": "to keep a grip on something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "держать",
                                "target_lang_sense_clarification": "физически в руках"
                            },
                            {
                                "target_lang_word": "удерживать",
                                "target_lang_sense_clarification": "не отпускать силой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbd4b9fc-2b69-3fb5-a143-9aa5903a72c0",
                    "sense_definition": "Iets bewaren of niet laten ontsnappen, zoals aandacht, vocht of warmte.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "retaining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze grondsoort kan heel goed regenwater <w>vasthouden</w>.",
                            "target_lang_translations": {
                                "en": "This type of soil is very good at <w>retaining</w> rainwater.",
                                "ru": "Этот тип почвы может очень хорошо <w>удерживать</w> дождевую воду."
                            }
                        },
                        {
                            "text": "De spannende film kon mijn aandacht tot het einde <w>vasthouden</w>.",
                            "target_lang_translations": {
                                "en": "The exciting movie managed to <w>hold</w> my attention until the end.",
                                "ru": "Захватывающий фильм смог <w>удерживать</w> мое внимание до самого конца."
                            }
                        }
                    ],
                    "synonyms": [
                        "behouden",
                        "bewaren"
                    ],
                    "antonyms": [
                        "verliezen",
                        "kwijtraken"
                    ],
                    "common_phrases": [
                        "aandacht vasthouden",
                        "vocht vasthouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To keep something or prevent it from escaping, such as attention, moisture, or heat.",
                        "ru": "Сохранять что-то или не давать этому исчезнуть (внимание, влагу, тепло)."
                    },
                    "translations": {
                        "en": [
                            {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "71d9b4c7-28ad-36c2-b9b1-e3cd155491c2",
                    "sense_definition": "A strong feeling of displeasure, hostility, or antagonism towards someone or something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to learn how to control your <w>anger</w>.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно научиться контролировать свой <w>гнев</w>."
                            }
                        },
                        {
                            "text": "His face turned red with <w>anger</w> when he heard the news.",
                            "target_lang_translations": {
                                "ru": "Его лицо покраснело от <w>гнева</w>, когда он услышал новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "fury",
                        "rage",
                        "wrath",
                        "ire",
                        "resentment",
                        "indignation",
                        "displeasure",
                        "vexation"
                    ],
                    "antonyms": [
                        "calmness",
                        "peacefulness"
                    ],
                    "common_phrases": [
                        "control anger",
                        "vent anger",
                        "anger management",
                        "deep anger",
                        "express anger"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное чувство недовольства, враждебности или неприязни по отношению к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнев",
                                "target_lang_sense_clarification": "сильное чувство возмущения"
                            },
                            {
                                "target_lang_word": "злость",
                                "target_lang_sense_clarification": "чувство раздражения и недоброжелательности"
                            },
                            {
                                "target_lang_word": "злоба",
                                "target_lang_sense_clarification": "затаенное чувство вражды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddf99fba-4710-308e-9494-14c06b4eefe4",
                    "sense_definition": "Pain or inflammation of a sore or wound.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The medicine helped to take away the <w>anger</w> of the wound.",
                            "target_lang_translations": {
                                "ru": "Лекарство помогло снять <w>воспаление</w> раны."
                            }
                        },
                        {
                            "text": "He waited for the <w>anger</w> in his sore foot to subside.",
                            "target_lang_translations": {
                                "ru": "Он ждал, пока утихнет <w>боль</w> в его больной ноге."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "implied by context and examples"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Боль или воспаление раны или ушиба."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспаление",
                                "target_lang_sense_clarification": "болезненный процесс в тканях"
                            },
                            {
                                "target_lang_word": "боль",
                                "target_lang_sense_clarification": "физическое страдание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4bce7dc6-53a2-3d07-9388-a6556159f9dd",
                    "sense_definition": "To make someone feel angry or annoyed.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cause_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The unfair decision <w>angered</w> the entire staff.",
                            "target_lang_translations": {
                                "ru": "Несправедливое решение <w>разозлило</w> весь персонал."
                            }
                        },
                        {
                            "text": "It <w>angers</w> me when people are rude to waiters.",
                            "target_lang_translations": {
                                "ru": "Меня <w>злит</w>, когда люди грубы с официантами."
                            }
                        }
                    ],
                    "synonyms": [
                        "enrage",
                        "infuriate",
                        "annoy",
                        "irritate",
                        "vex",
                        "displease",
                        "aggravate"
                    ],
                    "common_phrases": [
                        "anger someone",
                        "easily angered"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать у кого-либо чувство злости или раздражения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "злить",
                                "target_lang_sense_clarification": "вызывать злость"
                            },
                            {
                                "target_lang_word": "сердить",
                                "target_lang_sense_clarification": "приводить в раздражение"
                            },
                            {
                                "target_lang_word": "гневить",
                                "target_lang_sense_clarification": "вызывать гнев (книжн.)"
                            }
                        ]

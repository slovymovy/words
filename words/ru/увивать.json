            "pos": "verb",
            "senses": [
                {
                    "sense_id": "200a8ca1-97ce-3f22-a5fe-e870c7045ec3",
                    "sense_definition": "Обвивать, плотно окружая или покрывая собой поверхность чего-либо (обычно о вьющихся растениях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "entwine_cover",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Густой зеленый плющ полностью <увивал> стены старого замка.",
                            "target_lang_translations": {
                                "en": "Thick green ivy completely <wreathed> the walls of the old castle."
                            }
                        },
                        {
                            "text": "Дикий виноград плотно <увивает> беседку, создавая приятную тень.",
                            "target_lang_translations": {
                                "en": "Wild grapes tightly <entwine> the gazebo, creating a pleasant shade."
                            }
                        }
                    ],
                    "synonyms": [
                        "обвивать",
                        "оплетать"
                    ],
                    "common_phrases": [
                        "увивать плющом",
                        "увивать зеленью",
                        "увивать беседку"
                    ],
                    "target_lang_definitions": {
                        "en": "To wrap around, tightly surrounding or covering the surface of something (usually referring to climbing plants)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "entwine",
                                "target_lang_sense_clarification": "to wind or twist around"
                            },
                            {
                                "target_lang_word": "wreathe",
                                "target_lang_sense_clarification": "to cover or surround completely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9427ba3-ac8a-33a6-87c5-40e105f7068c",
                    "sense_definition": "Украшать, обматывая или оплетая чем-либо (цветами, лентами, гирляндами).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "decorate_wreathe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К празднику колонны в зале <увивали> яркими лентами и цветами.",
                            "target_lang_translations": {
                                "en": "For the holiday, the columns in the hall were <festooned> with bright ribbons and flowers."
                            }
                        },
                        {
                            "text": "Девушки <увивали> свои головы венками из полевых ромашек.",
                            "target_lang_translations": {
                                "en": "The girls <wreathed> their heads with garlands of field daisies."
                            }
                        }
                    ],
                    "synonyms": [
                        "украшать",
                        "увенчивать"
                    ],
                    "common_phrases": [
                        "увивать цветами",
                        "увивать лентами",
                        "увивать гирляндами",
                        "увивать голову"
                    ],
                    "target_lang_definitions": {
                        "en": "To decorate by wrapping or braiding with something (such as flowers, ribbons, or garlands)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wreathe",
                                "target_lang_sense_clarification": "to adorn with flowers or greenery"
                            },
                            {
                                "target_lang_word": "festoon",
                                "target_lang_sense_clarification": "to decorate with hanging ribbons or garlands"
                            },
                            {
                                "target_lang_word": "garland",
                                "target_lang_sense_clarification": "to deck with a garland"
                            }
                        ]
                    }
                }
            ]
        }
    ]

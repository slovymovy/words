{
    "word_family": [
        "iffy"
    ],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "8f4fb9fc-3a3c-33ec-ac58-ebf828d9c2e1",
                    "sense_definition": "Used to introduce a condition that must happen or be true for something else to happen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "conditional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will only go to the party <w>if</w> you come with me.",
                            "target_lang_translations": {
                                "ru": "Я пойду на вечеринку, только <w>если</w> ты пойдешь со мной."
                            }
                        },
                        {
                            "text": "Please call the doctor <w>if</w> you feel sick tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, позвони врачу, <w>если</w> завтра утром почувствуешь себя плохо."
                            }
                        }
                    ],
                    "synonyms": [
                        "provided that"
                    ],
                    "antonyms": [
                        "unless"
                    ],
                    "common_phrases": [
                        "if possible",
                        "if necessary",
                        "if so",
                        "if not"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения условия, которое должно произойти или быть истинным, чтобы случилось что-то другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "е́сли",
                                "target_lang_sense_clarification": "союз, вводящий реальное условие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d2644f5-f6f4-32bf-add2-3ae25bf6e118",
                    "sense_definition": "Used to introduce a choice between alternatives, often after verbs like know, ask, see, or wonder.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "indirect_question",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not know <w>if</w> she is coming to the meeting or not.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, придет <w>ли</w> она на собрание или нет."
                            }
                        },
                        {
                            "text": "Can you check to see <w>if</w> the mail has arrived yet?",
                            "target_lang_translations": {
                                "ru": "Можешь проверить, пришла <w>ли</w> уже почта?"
                            }
                        }
                    ],
                    "synonyms": [
                        "whether"
                    ],
                    "common_phrases": [
                        "see if",
                        "ask if",
                        "wonder if",
                        "know if"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения выбора между альтернативами, часто после глаголов «знать», «спрашивать», «видеть» или «интересоваться»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ли",
                                "target_lang_sense_clarification": "частица для косвенного вопроса (соответствует 'whether')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2883ae7a-5c7c-37f1-bac8-77c3c700abdc",
                    "sense_definition": "Used to introduce an imaginary, impossible, or hypothetical situation and its consequences.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "conditional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>If</w> I were you, I would accept that job offer immediately.",
                            "target_lang_translations": {
                                "ru": "<w>Если</w> бы я был на твоем месте, я бы немедленно принял это предложение о работе."
                            }
                        },
                        {
                            "text": "She would travel the world <w>if</w> she had more money.",
                            "target_lang_translations": {
                                "ru": "Она бы путешествовала по миру, <w>если</w> бы у нее было больше денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "supposing"
                    ],
                    "common_phrases": [
                        "if I were you",
                        "if only"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения воображаемой, невозможной или гипотетической ситуации и её последствий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "е́сли",
                                "target_lang_sense_clarification": "обычно используется с частицей «бы» для гипотетических условий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a18574b5-4e43-3ba3-ba1b-1618579bb558",
                    "sense_definition": "Used to mean \"every time that\" or \"whenever\" regarding general facts, habits, or scientific truths.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "conditional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>If</w> you mix blue and yellow paint, you get green.",
                            "target_lang_translations": {
                                "ru": "<w>Если</w> смешать синюю и желтую краску, получится зеленый цвет."
                            }
                        },
                        {
                            "text": "<w>If</w> it rains heavily, the roof always leaks a little bit.",
                            "target_lang_translations": {
                                "ru": "<w>Если</w> идет сильный дождь, крыша всегда немного протекает."
                            }
                        }
                    ],
                    "synonyms": [
                        "when",
                        "whenever"
                    ],
                    "common_phrases": [
                        "if and when"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в значении «каждый раз, когда» или «когда» применительно к общим фактам, привычкам или научным истинам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "е́сли",
                                "target_lang_sense_clarification": "в значении «когда», для констатации фактов"
                            },
                            {
                                "target_lang_word": "когда́",
                                "target_lang_sense_clarification": "синоним для повторяющихся действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f1c70a6-73ef-3964-8233-7e578e93375d",
                    "sense_definition": "Used to introduce a statement that is relevant only in a specific circumstance or to offer something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "conditional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is some leftover pizza in the fridge <w>if</w> you are hungry.",
                            "target_lang_translations": {
                                "ru": "В холодильнике есть остатки пиццы, <w>если</w> ты голоден."
                            }
                        },
                        {
                            "text": "I am going to the library now <w>if</w> anyone needs me.",
                            "target_lang_translations": {
                                "ru": "Я иду в библиотеку, <w>если</w> я кому-то нужен."
                            }
                        }
                    ],
                    "synonyms": [
                        "in case"
                    ],
                    "common_phrases": [
                        "if you like",
                        "if you want"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения утверждения, уместного только в определенных обстоятельствах, или для предложения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "е́сли",
                                "target_lang_sense_clarification": "вводное условие вежливости или уместности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57744589-0869-30f0-b130-7736acf6c3ad",
                    "sense_definition": "Considering the fact that; used when a condition is already known to be true.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "conditional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>If</w> the drain is blocked, we must call a plumber immediately.",
                            "target_lang_translations": {
                                "ru": "<w>Если</w> сток засорился, нужно немедленно вызвать сантехника."
                            }
                        },
                        {
                            "text": "<w>If</w> you already knew the truth, why did you ask me?",
                            "target_lang_translations": {
                                "ru": "<w>Если</w> ты уже знал правду, почему ты спросил меня?"
                            }
                        }
                    ],
                    "synonyms": [
                        "since",
                        "given that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учитывая тот факт, что; используется, когда условие уже известно как истинное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "е́сли",
                                "target_lang_sense_clarification": "в значении «раз» или «поскольку»"
                            },
                            {
                                "target_lang_word": "раз",
                                "target_lang_sense_clarification": "разговорный синоним для известных фактов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "948f01fb-eeaa-35cd-89ec-c168fc5b2ab3",
                    "sense_definition": "Used to introduce a contrasting fact or opinion; meaning \"although\" or \"even though\".",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "concession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a very useful, <w>if</w> somewhat expensive, lesson to learn.",
                            "target_lang_translations": {
                                "ru": "Это был очень полезный, <w>пусть</w> и несколько дорогой, урок."
                            }
                        },
                        {
                            "text": "The team played with great passion, <w>if</w> not with much skill.",
                            "target_lang_translations": {
                                "ru": "Команда играла с большой страстью, <w>хотя</w> и не очень умело."
                            }
                        }
                    ],
                    "synonyms": [
                        "although",
                        "even though"
                    ],
                    "common_phrases": [
                        "if anything"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения противопоставляемого факта или мнения; в значении «хотя» или «даже если»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотя́",
                                "target_lang_sense_clarification": "уступительный союз"
                            },
                            {
                                "target_lang_word": "пусть",
                                "target_lang_sense_clarification": "в значении «пусть и», «даже если»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d2af9164-d298-38a8-b65b-3b9aed824159",
                    "sense_definition": "A condition, uncertainty, or possibility regarding a future event.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are too many <w>ifs</w> in this plan for me to approve it.",
                            "target_lang_translations": {
                                "ru": "В этом плане слишком много <w>если</w>, чтобы я мог его утвердить."
                            }
                        },
                        {
                            "text": "I want the work done today with no <w>ifs</w> or buts.",
                            "target_lang_translations": {
                                "ru": "Я хочу, чтобы работа была сделана сегодня без всяких <w>если</w> и но."
                            }
                        }
                    ],
                    "synonyms": [
                        "condition",
                        "uncertainty"
                    ],
                    "antonyms": [
                        "certainty"
                    ],
                    "common_phrases": [
                        "no ifs or buts",
                        "the big if"
                    ],
                    "target_lang_definitions": {
                        "ru": "Условие, неопределенность или возможность относительно будущего события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "е́сли",
                                "target_lang_sense_clarification": "субстантивированное существительное (несклоняемое)"
                            },
                            {
                                "target_lang_word": "усло́вие",
                                "target_lang_sense_clarification": "оговорка или требование"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
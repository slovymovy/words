    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8815cca9-b309-3e5f-afb7-f0b2fa10e688",
                    "sense_definition": "Надавить на что-либо (кнопку, клавишу, рычаг и т. п.), приводя в действие механизм или сжимая поверхность.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чтобы включить компьютер, нужно <нажать> кнопку питания.",
                            "target_lang_translations": {
                                "en": "To turn on the computer, you need to <press> the power button."
                            }
                        },
                        {
                            "text": "Водитель резко <нажал> на тормоз, увидев пешехода.",
                            "target_lang_translations": {
                                "en": "The driver sharply <pressed> the brake when he saw the pedestrian."
                            }
                        }
                    ],
                    "synonyms": [
                        "надавить"
                    ],
                    "antonyms": [
                        "отпустить"
                    ],
                    "common_phrases": [
                        "нажать кнопку",
                        "нажать на тормоз",
                        "нажать на газ",
                        "нажать клавишу"
                    ],
                    "target_lang_definitions": {
                        "en": "To apply force to something (a button, key, lever, etc.) to activate a mechanism or compress a surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "apply force to a button or object"
                            },
                            {
                                "target_lang_word": "push",
                                "target_lang_sense_clarification": "a button or key"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48f50f3e-2749-375e-86b3-bb94ab223562",
                    "sense_definition": "Оказать сильное воздействие или давление на кого-либо, настойчиво требуя или заставляя сделать что-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник решил <нажать> на сотрудников, чтобы ускорить сдачу проекта.",
                            "target_lang_translations": {
                                "en": "The boss decided to <pressure> the employees to speed up the project submission."
                            }
                        },
                        {
                            "text": "Если на него немного <нажать>, он согласится помочь нам.",
                            "target_lang_translations": {
                                "en": "If we <push> him a little, he will agree to help us."
                            }
                        }
                    ],
                    "synonyms": [
                        "надавить"
                    ],
                    "common_phrases": [
                        "нажать на жалость",
                        "нажать авторитетом",
                        "нажать на должника"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To exert strong influence or pressure on someone, insistently demanding or forcing them to do something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pressure",
                                "target_lang_sense_clarification": "exert influence on someone"
                            },
                            {
                                "target_lang_word": "lean on",
                                "target_lang_sense_clarification": "someone (informal)"
                            },
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "someone to do something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16723d84-42b8-3fac-8a68-a50cef52cf8b",
                    "sense_definition": "Увеличить темп, скорость или интенсивность выполнения какого-либо действия.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам нужно <нажать> на учёбу, так как экзамены уже скоро.",
                            "target_lang_translations": {
                                "en": "We need to <push> hard on our studies since exams are coming soon."
                            }
                        },
                        {
                            "text": "Команда <нажала> в конце матча и смогла сравнять счёт.",
                            "target_lang_translations": {
                                "en": "The team <pressed> at the end of the match and managed to equalize."
                            }
                        }
                    ],
                    "common_phrases": [
                        "нажать на работу",
                        "нажать на темп"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To increase the tempo, speed, or intensity of an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "push",
                                "target_lang_sense_clarification": "increase effort or intensity"
                            },
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "increase pressure or speed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ecc3d29-33d0-3b15-9dad-7e7c725515f4",
                    "sense_definition": "Срезать серпом или специальной машиной какое-либо количество злаков (ржи, пшеницы и т. п.) при уборке урожая.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Крестьяне успели <нажать> много снопов до начала дождя.",
                            "target_lang_translations": {
                                "en": "The peasants managed to <reap> many sheaves before the rain started."
                            }
                        },
                        {
                            "text": "За день работы в поле они <нажали> целую гору пшеницы.",
                            "target_lang_translations": {
                                "en": "During the day's work in the field, they <reaped> a whole mountain of wheat."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cut a quantity of grain (rye, wheat, etc.) with a sickle or machine during harvest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reap",
                                "target_lang_sense_clarification": "harvest grain"
                            },
                            {
                                "target_lang_word": "harvest",
                                "target_lang_sense_clarification": "crops"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
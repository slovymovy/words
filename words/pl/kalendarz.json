{
    "word_family": [
        "kalendarzyk",
        "kalendarzowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e134106a-d383-3a92-903b-d74c036f37f0",
                    "sense_definition": "Spis dni całego roku z podziałem na miesiące i tygodnie, często w formie drukowanej lub elektronicznej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "object_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na ścianie w kuchni wisi duży <kalendarz> z widokami gór.",
                            "target_lang_translations": {
                                "ru": "На стене в кухне висит большой <календарь> с видами гор."
                            }
                        },
                        {
                            "text": "Muszę kupić nowy <kalendarz> kieszonkowy na przyszły rok.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый карманный <календарь> на следующий год."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kalendarz ścienny",
                        "kartka z kalendarza",
                        "kalendarz biurkowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Список дней всего года с делением на месяцы и недели, часто в печатном или электронном виде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "календарь",
                                "target_lang_sense_clarification": "печатное или электронное издание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d32a53bf-c5d4-3431-9bdf-e73aa68a0427",
                    "sense_definition": "Plan zawierający ustalone terminy wydarzeń, spotkań lub zadań do wykonania.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "schedule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój szef ma bardzo napięty <kalendarz> w tym tygodniu.",
                            "target_lang_translations": {
                                "ru": "У моего начальника очень плотный <график> на этой неделе."
                            }
                        },
                        {
                            "text": "Wpisałem naszą wizytę u lekarza w <kalendarz> w telefonie.",
                            "target_lang_translations": {
                                "ru": "Я записал наш визит к врачу в <календарь> в телефоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "terminarz"
                    ],
                    "common_phrases": [
                        "napięty kalendarz",
                        "kalendarz spotkań",
                        "wpisać w kalendarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "План, содержащий установленные сроки событий, встреч или задач для выполнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "график",
                                "target_lang_sense_clarification": "расписание дел"
                            },
                            {
                                "target_lang_word": "расписание",
                                "target_lang_sense_clarification": "план действий"
                            },
                            {
                                "target_lang_word": "календарь",
                                "target_lang_sense_clarification": "список встреч и событий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d2258ca-20fe-3f1d-a79b-7de60397b3c3",
                    "sense_definition": "System rachuby czasu dzielący go na lata, miesiące i dni, oparty na zjawiskach astronomicznych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Większość krajów na świecie stosuje obecnie <kalendarz> gregoriański.",
                            "target_lang_translations": {
                                "ru": "Большинство стран мира использует сейчас григорианский <календарь>."
                            }
                        },
                        {
                            "text": "Niektóre święta religijne są ruchome i zależą od <kalendarza> księżycowego.",
                            "target_lang_translations": {
                                "ru": "Некоторые религиозные праздники — переходящие и зависят от лунного <календаря>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kalendarz gregoriański",
                        "kalendarz juliański",
                        "kalendarz księżycowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система исчисления времени, делящая его на годы, месяцы и дни, основанная на астрономических явлениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "календарь",
                                "target_lang_sense_clarification": "система летоисчисления"
                            },
                            {
                                "target_lang_word": "летоисчисление",
                                "target_lang_sense_clarification": "способ подсчета времени"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "02cc98f4-4b77-34bf-bfa4-3d7637eae4b0",
                    "sense_definition": "Wyrażać określoną treść, być symbolem, nazwą lub informacją o czymś",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "meaning_implication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Co <w>oznacza</w> to słowo w języku polskim?",
                            "target_lang_translations": {
                                "ru": "Что <w>означает</w> это слово на польском языке?"
                            }
                        },
                        {
                            "text": "Czerwone światło na sygnalizatorze <w>oznacza</w> bezwzględny zakaz wjazdu.",
                            "target_lang_translations": {
                                "ru": "Красный свет на светофоре <w>означает</w> категорический запрет въезда."
                            }
                        }
                    ],
                    "synonyms": [
                        "znaczyć"
                    ],
                    "common_phrases": [
                        "co to oznacza",
                        "oznaczać to samo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать определенное содержание, быть символом, названием или информацией о чем-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "означать",
                                "target_lang_sense_clarification": "иметь смысл или значение"
                            },
                            {
                                "target_lang_word": "значить",
                                "target_lang_sense_clarification": "выражать смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eccc4687-0fe7-3877-96e9-49ea7723314b",
                    "sense_definition": "Być równoznacznym z czymś, pociągać za sobą określone skutki lub być zapowiedzią czegoś",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "meaning_implication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nagła rezygnacja <w>oznacza</w> poważne kłopoty dla całej firmy.",
                            "target_lang_translations": {
                                "ru": "Его внезапная отставка <w>означает</w> серьезные проблемы для всей фирмы."
                            }
                        },
                        {
                            "text": "Dla wielu ludzi sukces zawodowy nie zawsze <w>oznacza</w> szczęście w życiu prywatnym.",
                            "target_lang_translations": {
                                "ru": "Для многих людей профессиональный успех не всегда <w>означает</w> счастье в личной жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "świadczyć",
                        "znamionować"
                    ],
                    "common_phrases": [
                        "milczenie oznacza zgodę",
                        "to oznacza wojnę",
                        "oznaczać kłopoty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть равносильным чему-либо, влечь за собой определенные последствия или предвещать что-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "означать",
                                "target_lang_sense_clarification": "влечь за собой последствия"
                            },
                            {
                                "target_lang_word": "значить",
                                "target_lang_sense_clarification": "быть равносильным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6a1ab94-0c1e-3aa7-b364-77e7ceb46d42",
                    "sense_definition": "Umieszczać na czymś znak, symbol lub etykietę w celu identyfikacji lub wyróżnienia",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "marking_determining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik <w>oznaczał</w> swoje owce kolorową farbą, aby odróżnić je od stada sąsiada.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>помечал</w> своих овец цветной краской, чтобы отличить их от стада соседа."
                            }
                        },
                        {
                            "text": "Na mapie turystycznej dokładnie <w>oznaczono</w> wszystkie szlaki i schroniska.",
                            "target_lang_translations": {
                                "ru": "На туристической карте были точно <w>обозначены</w> все маршруты и приюты."
                            }
                        }
                    ],
                    "synonyms": [
                        "znakować",
                        "oznakowywać",
                        "naznaczać",
                        "cechować"
                    ],
                    "common_phrases": [
                        "oznaczać teren",
                        "oznaczać produkty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить на что-либо знак, символ или этикетку с целью идентификации или выделения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помечать",
                                "target_lang_sense_clarification": "ставить метку"
                            },
                            {
                                "target_lang_word": "маркировать",
                                "target_lang_sense_clarification": "наносить маркировку"
                            },
                            {
                                "target_lang_word": "обозначать",
                                "target_lang_sense_clarification": "выделять знаком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5d39062-6963-309f-b18f-5bd78e51ca27",
                    "sense_definition": "Ustalać, wytyczać lub precyzować coś, np. granice, termin, wartość lub miejsce",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "marking_determining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geodeci przyjechali rano, aby <w>oznaczyć</w> granice nowej działki budowlanej.",
                            "target_lang_translations": {
                                "ru": "Геодезисты приехали утром, чтобы <w>обозначить</w> границы нового строительного участка."
                            }
                        },
                        {
                            "text": "Lekarz zlecił badania, aby <w>oznaczyć</w> poziom cukru we krwi pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врач назначил анализы, чтобы <w>определить</w> уровень сахара в крови пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyznaczać",
                        "wskazywać"
                    ],
                    "common_phrases": [
                        "oznaczać granice",
                        "oznaczać termin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устанавливать, намечать или уточнять что-либо, например, границы, сроки, значение или место"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "выявлять значение или параметр"
                            },
                            {
                                "target_lang_word": "обозначать",
                                "target_lang_sense_clarification": "указывать границы или место"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a002b22e-0f0d-30a4-b330-8ad59091c196",
                    "sense_definition": "Always doing things, moving around, or involved in activities; full of energy and not resting.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother stays **active** by gardening and walking every day.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка остается <w>активной</w>, занимаясь садоводством и гуляя каждый день."
                            }
                        },
                        {
                            "text": "He is an **active** member of the local chess club.",
                            "target_lang_translations": {
                                "ru": "Он <w>активный</w> член местного шахматного клуба."
                            }
                        }
                    ],
                    "synonyms": [
                        "energetic",
                        "busy",
                        "lively",
                        "dynamic",
                        "agile"
                    ],
                    "antonyms": [
                        "inactive",
                        "sluggish",
                        "sedentary",
                        "passive",
                        "indolent"
                    ],
                    "common_phrases": [
                        "stay active",
                        "active lifestyle",
                        "active participant",
                        "active role"
                    ],
                    "target_lang_definitions": {
                        "ru": "Всегда занятый делом, подвижный или вовлеченный в деятельность; полный энергии и не сидящий на месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активный",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "деятельный",
                                "target_lang_sense_clarification": "постоянно занятый делом"
                            },
                            {
                                "target_lang_word": "энергичный",
                                "target_lang_sense_clarification": "полный энергии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a16da0ee-084b-3a6f-961a-aa64dad8b7e6",
                    "sense_definition": "Currently functioning, operating, or in effect; not stopped, paused, or extinct.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mount Etna is one of the most **active** volcanoes in Europe.",
                            "target_lang_translations": {
                                "ru": "Этна — один из самых <w>активных</w> вулканов в Европе."
                            }
                        },
                        {
                            "text": "You need an **active** internet connection to use this application.",
                            "target_lang_translations": {
                                "ru": "Для использования этого приложения вам нужно <w>активное</w> интернет-соединение."
                            }
                        }
                    ],
                    "synonyms": [
                        "working",
                        "operative",
                        "running",
                        "in force"
                    ],
                    "antonyms": [
                        "inactive",
                        "dormant",
                        "extinct",
                        "quiescent"
                    ],
                    "common_phrases": [
                        "active volcano",
                        "active account",
                        "active duty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Функционирующий, работающий или имеющий силу в данный момент; не остановленный и не исчезнувший."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "действующий",
                                "target_lang_sense_clarification": "функционирующий (о вулкане, законе)"
                            },
                            {
                                "target_lang_word": "активный",
                                "target_lang_sense_clarification": "находящийся в действии (о счете, подключении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c730114b-d1ca-3fff-a9ab-0c2862cdb889",
                    "sense_definition": "Relating to a verb form or voice in which the subject performs the action rather than receiving it.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Teachers often encourage students to write in the **active** voice for greater clarity.",
                            "target_lang_translations": {
                                "ru": "Учителя часто советуют студентам писать в <w>действительном</w> залоге для большей ясности."
                            }
                        },
                        {
                            "text": "In the sentence 'The cat chased the mouse', the verb is **active**.",
                            "target_lang_translations": {
                                "ru": "В предложении «Кошка погналась за мышью» глагол стоит в <w>действительном</w> залоге."
                            }
                        }
                    ],
                    "antonyms": [
                        "passive"
                    ],
                    "common_phrases": [
                        "active voice",
                        "active verb"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "grammar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к грамматическому залогу, в котором подлежащее выполняет действие, а не испытывает его на себе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "действительный",
                                "target_lang_sense_clarification": "грамматический термин (залог)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa1348b9-72f0-3607-974b-ed5db2b6e282",
                    "sense_definition": "Having a specific chemical, biological, or physical effect; capable of causing a reaction.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Always check the label to see the **active** ingredients in the medicine.",
                            "target_lang_translations": {
                                "ru": "Всегда проверяйте этикетку, чтобы узнать <w>действующие</w> вещества в лекарстве."
                            }
                        },
                        {
                            "text": "Yeast is an **active** culture that causes the bread dough to rise.",
                            "target_lang_translations": {
                                "ru": "Дрожжи — это <w>активная</w> культура, которая заставляет тесто подниматься."
                            }
                        }
                    ],
                    "synonyms": [
                        "potent",
                        "effective"
                    ],
                    "antonyms": [
                        "inert",
                        "inactive"
                    ],
                    "common_phrases": [
                        "active ingredient",
                        "active substance",
                        "active agent"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий определенным химическим, биологическим или физическим действием; способный вызывать реакцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "действующий",
                                "target_lang_sense_clarification": "о веществе или ингредиенте"
                            },
                            {
                                "target_lang_word": "активный",
                                "target_lang_sense_clarification": "вызывающий реакцию"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "83fe0d18-1b21-3dc6-ab11-7ed14cd53672",
                    "sense_definition": "A member of an organization, especially a fraternity or sorority, who is currently participating fully.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The **actives** organized a welcome party for the new pledges.",
                            "target_lang_translations": {
                                "ru": "<w>Активные члены</w> организовали приветственную вечеринку для новичков."
                            }
                        },
                        {
                            "text": "Only **actives** are allowed to vote on the proposed changes to the chapter rules.",
                            "target_lang_translations": {
                                "ru": "Только <w>активным членам</w> разрешено голосовать по предложенным изменениям в правилах отделения."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mostly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член организации (особенно студенческого братства), который в данный момент полноценно участвует в её деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активный член",
                                "target_lang_sense_clarification": "участник организации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "366ef85d-40b1-3a52-8364-611dc54157c9",
                    "sense_definition": "An electronic component, such as a transistor, that can control the flow of electricity.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The circuit board contains a mix of passives and **actives**.",
                            "target_lang_translations": {
                                "ru": "Печатная плата содержит смесь пассивных и <w>активных элементов</w>."
                            }
                        },
                        {
                            "text": "Engineers tested the **actives** to ensure the signal was amplified correctly.",
                            "target_lang_translations": {
                                "ru": "Инженеры протестировали <w>активные компоненты</w>, чтобы убедиться в правильном усилении сигнала."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "electronics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Электронный компонент, такой как транзистор, который может управлять потоком электричества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активный элемент",
                                "target_lang_sense_clarification": "в электронике"
                            },
                            {
                                "target_lang_word": "активный компонент",
                                "target_lang_sense_clarification": "в электронике"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
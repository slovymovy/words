{
    "word_family": [
        "dagtekening"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "63448f7a-21a8-3d45-94dd-19f77cb27964",
                    "sense_definition": "Uit een bepaalde periode in het verleden stammen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze oude gebruiken <w>dagtekenen</w> nog uit de middeleeuwen.",
                            "target_lang_translations": {
                                "en": "These old customs <w>date</w> back to the Middle Ages.",
                                "ru": "Эти старые обычаи <w>восходят</w> еще к средневековью."
                            }
                        },
                        {
                            "text": "Het oudste deel van de kerk <w>dagtekent</w> van de twaalfde eeuw.",
                            "target_lang_translations": {
                                "en": "The oldest part of the church <w>dates</w> from the twelfth century.",
                                "ru": "Старейшая часть церкви <w>датируется</w> двенадцатым веком."
                            }
                        }
                    ],
                    "synonyms": [
                        "dateren"
                    ],
                    "common_phrases": [
                        "dagtekenen uit",
                        "dagtekenen van"
                    ],
                    "target_lang_definitions": {
                        "en": "To originate from a certain period in the past.",
                        "ru": "Происходить из определенного периода в прошлом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "date from",
                                "target_lang_sense_clarification": "to have an origin at a specific time"
                            },
                            {
                                "target_lang_word": "date back to",
                                "target_lang_sense_clarification": "to exist since a specific time in the past"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "датироваться",
                                "target_lang_sense_clarification": "относиться к определенному времени"
                            },
                            {
                                "target_lang_word": "восходить к",
                                "target_lang_sense_clarification": "брать начало в прошлом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9c8d514-7d20-38ef-879d-d38be336f35c",
                    "sense_definition": "Een datum vermelden op een geschrift of document.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "marking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "U moet het formulier ondertekenen en <w>dagtekenen</w> voordat u het opstuurt.",
                            "target_lang_translations": {
                                "en": "You must sign and <w>date</w> the form before you send it.",
                                "ru": "Вы должны подписать и <w>датировать</w> бланк перед отправкой."
                            }
                        },
                        {
                            "text": "De officiële brief was <w>gedagtekend</w> op 1 januari.",
                            "target_lang_translations": {
                                "en": "The official letter was <w>dated</w> January 1st.",
                                "ru": "Официальное письмо было <w>датировано</w> 1 января."
                            }
                        }
                    ],
                    "synonyms": [
                        "dateren"
                    ],
                    "common_phrases": [
                        "gedagtekend te"
                    ],
                    "target_lang_definitions": {
                        "en": "To write or mention a date on a document.",
                        "ru": "Ставить дату на документе или письме."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "date",
                                "target_lang_sense_clarification": "to mark with a date"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "датировать",
                                "target_lang_sense_clarification": "обозначать дату"
                            },
                            {
                                "target_lang_word": "поставить дату",
                                "target_lang_sense_clarification": "указать число на документе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6317d86-1bab-340c-b8f2-8de2bb67f82f",
                    "sense_definition": "Iets vastleggen of markeren om het voor de toekomst te bewaren.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "recording",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schrijver wilde zijn woede in dit gedicht <w>dagtekenen</w>.",
                            "target_lang_translations": {
                                "en": "The writer wanted to <w>record</w> his anger in this poem.",
                                "ru": "Писатель хотел <w>запечатлеть</w> свой гнев в этом стихотворении."
                            }
                        },
                        {
                            "text": "Zij probeerde het vluchtige moment in haar geheugen te <w>dagtekenen</w>.",
                            "target_lang_translations": {
                                "en": "She tried to <w>record</w> the fleeting moment in her memory.",
                                "ru": "Она пыталась <w>запечатлеть</w> мимолетное мгновение в своей памяти."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To record or mark something to preserve it for the future.",
                        "ru": "Записывать или фиксировать что-либо для сохранения в будущем."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "record",
                                "target_lang_sense_clarification": "to set down in writing or memory"
                            },
                            {
                                "target_lang_word": "chronicle",
                                "target_lang_sense_clarification": "to record events or feelings"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "запечатлеть",
                                "target_lang_sense_clarification": "сохранить в памяти или произведении"
                            },
                            {
                                "target_lang_word": "зафиксировать",
                                "target_lang_sense_clarification": "отметить для памяти"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
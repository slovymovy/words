    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b12753fd-8815-36db-a440-8ac6e2d462cc",
                    "sense_definition": "Повернуть голову или туловище назад или в сторону, чтобы посмотреть на кого-либо или что-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Услышав своё имя, он резко <w>обернулся</w>.",
                            "target_lang_translations": {
                                "en": "Hearing his name, he sharply <w>turned around</w>."
                            }
                        },
                        {
                            "text": "Девушка <w>обернулась</w> и помахала нам рукой на прощание.",
                            "target_lang_translations": {
                                "en": "The girl <w>turned around</w> and waved goodbye to us."
                            }
                        }
                    ],
                    "synonyms": [
                        "оглянуться",
                        "повернуться"
                    ],
                    "antonyms": [
                        "отвернуться"
                    ],
                    "common_phrases": [
                        "резко обернуться",
                        "обернуться назад",
                        "обернуться на звук"
                    ],
                    "target_lang_definitions": {
                        "en": "To turn one's head or body to look back or to the side."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn around",
                                "target_lang_sense_clarification": "physical movement of the body"
                            },
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "change orientation"
                            },
                            {
                                "target_lang_word": "look back",
                                "target_lang_sense_clarification": "glance behind oneself"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d7299fe-4c9a-3422-beb5-b9b731d2ac18",
                    "sense_definition": "Привести к какому-либо результату, закончиться чем-либо (обычно неожиданным или неприятным) для кого-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта маленькая ошибка может <w>обернуться</w> большой проблемой для всей команды.",
                            "target_lang_translations": {
                                "en": "This small mistake can <w>turn into</w> a big problem for the whole team."
                            }
                        },
                        {
                            "text": "Поездка в горы <w>обернулась</w> настоящим приключением.",
                            "target_lang_translations": {
                                "en": "The trip to the mountains <w>turned into</w> a real adventure."
                            }
                        }
                    ],
                    "synonyms": [
                        "оказаться",
                        "стать",
                        "вылиться"
                    ],
                    "common_phrases": [
                        "обернуться бедой",
                        "обернуться трагедией",
                        "обернуться удачей"
                    ],
                    "target_lang_definitions": {
                        "en": "To result in a specific outcome or turn into a situation, often unexpectedly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn into",
                                "target_lang_sense_clarification": "develop into a situation"
                            },
                            {
                                "target_lang_word": "result in",
                                "target_lang_sense_clarification": "lead to a consequence"
                            },
                            {
                                "target_lang_word": "turn out",
                                "target_lang_sense_clarification": "prove to be"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d84367d-2f9a-38ad-aea8-8f9e5dfd2795",
                    "sense_definition": "Быстро сходить или съездить куда-либо и вернуться обратно.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_trip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я мигом <w>обернусь</w> до магазина, жди меня здесь.",
                            "target_lang_translations": {
                                "en": "I'll quickly <w>run</w> to the shop and back, wait for me here."
                            }
                        },
                        {
                            "text": "Отец обещал <w>обернуться</w> за один день и привезти подарки.",
                            "target_lang_translations": {
                                "en": "Father promised to <w>make the round trip</w> in one day and bring gifts."
                            }
                        }
                    ],
                    "synonyms": [
                        "сбегать",
                        "съездить"
                    ],
                    "common_phrases": [
                        "быстро обернуться",
                        "обернуться за час",
                        "мигом обернуться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To go somewhere and return quickly; to make a quick round trip."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be back",
                                "target_lang_sense_clarification": "return quickly"
                            },
                            {
                                "target_lang_word": "make a round trip",
                                "target_lang_sense_clarification": "go and return"
                            },
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "colloquial: go somewhere quickly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd2c7dc9-0646-3c5b-8db2-ea9c3a8d27f4",
                    "sense_definition": "Превратиться в кого-либо или что-либо (часто в сказках, мифах или переносном смысле).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "transformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В русской сказке Иван-царевич <w>обернулся</w> серым волком.",
                            "target_lang_translations": {
                                "en": "In the Russian fairy tale, Ivan Tsarevich <w>turned into</w> a grey wolf."
                            }
                        },
                        {
                            "text": "Злая колдунья ударилась о землю и <w>обернулась</w> чёрной птицей.",
                            "target_lang_translations": {
                                "en": "The evil witch hit the ground and <w>turned into</w> a black bird."
                            }
                        }
                    ],
                    "synonyms": [
                        "превратиться"
                    ],
                    "common_phrases": [
                        "обернуться зверем",
                        "обернуться птицей"
                    ],
                    "target_lang_definitions": {
                        "en": "To transform or shapeshift into someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn into",
                                "target_lang_sense_clarification": "transform physically or magically"
                            },
                            {
                                "target_lang_word": "transform into",
                                "target_lang_sense_clarification": "change form"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10fe4645-b8f6-37f0-8c58-40e62b669116",
                    "sense_definition": "Замотать себя во что-либо (в ткань, одеяло), плотно укрыться.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От холода она решила <w>обернуться</w> в тёплый шерстяной плед.",
                            "target_lang_translations": {
                                "en": "Because of the cold, she decided to <w>wrap herself</w> in a warm wool blanket."
                            }
                        },
                        {
                            "text": "После душа ребёнок <w>обернулся</w> большим полотенцем.",
                            "target_lang_translations": {
                                "en": "After the shower, the child <w>wrapped himself</w> in a large towel."
                            }
                        }
                    ],
                    "synonyms": [
                        "закутаться",
                        "завернуться"
                    ],
                    "common_phrases": [
                        "обернуться одеялом",
                        "обернуться в плащ"
                    ],
                    "target_lang_definitions": {
                        "en": "To wrap oneself in something (like a blanket or towel)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wrap oneself",
                                "target_lang_sense_clarification": "cover oneself in fabric"
                            },
                            {
                                "target_lang_word": "wrap up",
                                "target_lang_sense_clarification": "bundle up for warmth"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2e2fccef-fc6b-3638-bb04-2d00a100ed40",
                    "sense_definition": "A sequence of numbers, letters, or symbols used to identify something, gain access, or represent a specific item.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "identification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please enter your four-digit access <w>code</w> to unlock the door.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, введите ваш четырехзначный <w>код</w> доступа, чтобы открыть дверь."
                            }
                        },
                        {
                            "text": "Do you know the area <w>code</w> for this part of the city?",
                            "target_lang_translations": {
                                "ru": "Вы знаете телефонный <w>код</w> этой части города?"
                            }
                        }
                    ],
                    "synonyms": [
                        "number",
                        "ID"
                    ],
                    "common_phrases": [
                        "zip code",
                        "area code",
                        "bar code",
                        "access code",
                        "country code"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последовательность цифр, букв или символов, используемая для идентификации чего-либо, получения доступа или обозначения конкретного предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "код",
                                "target_lang_sense_clarification": "для доступа или идентификации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c4cec21-59e2-3600-ab0e-a6bfc15891a7",
                    "sense_definition": "Instructions for a computer written in a programming language.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She spent the whole weekend writing <w>code</w> for her new mobile application.",
                            "target_lang_translations": {
                                "ru": "Она провела все выходные, создавая <w>код</w> для своего нового мобильного приложения."
                            }
                        },
                        {
                            "text": "The programmer checked the source <w>code</w> to find the error.",
                            "target_lang_translations": {
                                "ru": "Программист проверил исходный <w>код</w>, чтобы найти ошибку."
                            }
                        }
                    ],
                    "synonyms": [
                        "program",
                        "script"
                    ],
                    "common_phrases": [
                        "source code",
                        "machine code",
                        "write code",
                        "binary code"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструкции для компьютера, написанные на языке программирования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "код",
                                "target_lang_sense_clarification": "программа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49da9dc3-eda8-3701-8da6-ced193e20657",
                    "sense_definition": "A set of rules, laws, or principles that govern behavior or a specific subject.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rules",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school has a strict dress <w>code</w> that all students must follow.",
                            "target_lang_translations": {
                                "ru": "В школе действует строгий дресс-<w>код</w>, которому должны следовать все ученики."
                            }
                        },
                        {
                            "text": "Doctors must adhere to a professional <w>code</w> of ethics when treating patients.",
                            "target_lang_translations": {
                                "ru": "Врачи должны придерживаться профессионального <w>кодекса</w> этики при лечении пациентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "rules",
                        "regulations",
                        "laws"
                    ],
                    "common_phrases": [
                        "dress code",
                        "code of conduct",
                        "penal code",
                        "moral code"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свод правил, законов или принципов, регулирующих поведение или определенную область."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кодекс",
                                "target_lang_sense_clarification": "свод законов или правил"
                            },
                            {
                                "target_lang_word": "устав",
                                "target_lang_sense_clarification": "свод правил организации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4c8e5e3-f29b-342d-87a3-7b9523e77fa4",
                    "sense_definition": "A system of replacing words, letters, or signs with others to keep a message secret or short.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cryptography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The spies communicated using a secret <w>code</w> that no one else could understand.",
                            "target_lang_translations": {
                                "ru": "Шпионы общались, используя секретный <w>шифр</w>, который никто другой не мог понять."
                            }
                        },
                        {
                            "text": "It took the experts a long time to crack the enemy <w>code</w>.",
                            "target_lang_translations": {
                                "ru": "Экспертам потребовалось много времени, чтобы взломать вражеский <w>код</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cipher"
                    ],
                    "common_phrases": [
                        "secret code",
                        "Morse code",
                        "crack the code"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система замены слов, букв или знаков другими для сохранения тайны или сокращения сообщения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шифр",
                                "target_lang_sense_clarification": "секретная система"
                            },
                            {
                                "target_lang_word": "код",
                                "target_lang_sense_clarification": "система знаков"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb6e5c14-d5ba-3ec4-ad42-5d3b1b81e3d7",
                    "sense_definition": "To write instructions or programs for a computer.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "computing_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I want to learn to <w>code</w> so I can build my own website.",
                            "target_lang_translations": {
                                "ru": "Я хочу научиться <w>программировать</w>, чтобы создать свой собственный веб-сайт."
                            }
                        },
                        {
                            "text": "He <w>coded</w> a simple game in just a few hours.",
                            "target_lang_translations": {
                                "ru": "Он <w>написал</w> простую игру всего за несколько часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "program"
                    ],
                    "common_phrases": [
                        "learn to code",
                        "code in Python"
                    ],
                    "target_lang_definitions": {
                        "ru": "Писать инструкции или программы для компьютера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "программировать",
                                "target_lang_sense_clarification": "составлять программы"
                            },
                            {
                                "target_lang_word": "кодить",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "кодировать",
                                "target_lang_sense_clarification": "писать код"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7a0b766-9698-3106-b6f1-af1fb207e3ba",
                    "sense_definition": "To convert a message or information into a code to keep it secret or for transmission.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "encoding_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The military <w>coded</w> the message before sending it over the radio.",
                            "target_lang_translations": {
                                "ru": "Военные <w>зашифровали</w> сообщение перед отправкой по радио."
                            }
                        },
                        {
                            "text": "Data is <w>coded</w> to ensure privacy during the transfer.",
                            "target_lang_translations": {
                                "ru": "Данные <w>кодируются</w> для обеспечения конфиденциальности при передаче."
                            }
                        }
                    ],
                    "synonyms": [
                        "encode",
                        "encrypt"
                    ],
                    "antonyms": [
                        "decode"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преобразовывать сообщение или информацию в код для сохранения тайны или передачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шифровать",
                                "target_lang_sense_clarification": "скрывать смысл"
                            },
                            {
                                "target_lang_word": "кодировать",
                                "target_lang_sense_clarification": "преобразовывать данные"
                            },
                            {
                                "target_lang_word": "зашифровать",
                                "target_lang_sense_clarification": "сделать секретным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00f1e811-f1d0-3484-9129-b1e08d211962",
                    "sense_definition": "To suffer a sudden medical emergency, such as cardiac arrest.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "medical_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient in room four <w>coded</w> suddenly, and the team rushed in to help.",
                            "target_lang_translations": {
                                "ru": "У пациента в четвертой палате внезапно <w>остановилось сердце</w>, и бригада врачей бросилась на помощь."
                            }
                        },
                        {
                            "text": "Nurses are trained to react instantly if a patient <w>codes</w>.",
                            "target_lang_translations": {
                                "ru": "Медсестры обучены реагировать мгновенно, если у пациента <w>происходит остановка сердца</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "medical jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пережить внезапное критическое состояние, например, остановку сердца (медицинский жаргон)."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },

                    "target_lang_definitions": {
                        "ru": "Наемный убийца, головорез или смельчак (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "браво",
                                "target_lang_sense_clarification": "исторический термин, наемный убийца"

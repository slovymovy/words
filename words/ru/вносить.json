            "senses": [
                {
                    "sense_id": "40e3abb9-d483-3bc3-8c44-7e4f7e352fd2",
                    "sense_definition": "Перемещать кого-либо или что-либо внутрь помещения или пространства.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Грузчики начали осторожно <вносить> новую мебель в квартиру.",
                            "target_lang_translations": {
                                "en": "The movers started to carefully <carry> the new furniture into the apartment."
                            }
                        },
                        {
                            "text": "Зимой цветы с балкона нужно <вносить> в тёплую комнату.",
                            "target_lang_translations": {
                                "en": "In winter, flowers from the balcony need to be <brought> into a warm room."
                            }
                        }
                    ],
                    "synonyms": [
                        "заносить"
                    ],
                    "antonyms": [
                        "выносить"
                    ],
                    "common_phrases": [
                        "вносить вещи",
                        "вносить мебель",
                        "вносить в дом"
                    ],
                    "target_lang_definitions": {
                        "en": "To move someone or something inside a room or space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bring in",
                                "target_lang_sense_clarification": "general movement inside"
                            },
                            {
                                "target_lang_word": "carry in",
                                "target_lang_sense_clarification": "while holding something"
                            },
                            {
                                "target_lang_word": "take in",
                                "target_lang_sense_clarification": "move inside"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8f1cc8d-ad9c-3372-aacd-0326ec0650c7",
                    "sense_definition": "Вписывать или включать информацию, исправления или дополнения в документ, список или текст.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Секретарь должен <вносить> все важные встречи в календарь директора.",
                            "target_lang_translations": {
                                "en": "The secretary must <enter> all important meetings into the director's calendar."
                            }
                        },
                        {
                            "text": "Редактор попросил автора <вносить> изменения прямо в рукопись.",
                            "target_lang_translations": {
                                "en": "The editor asked the author to <make> changes directly to the manuscript."
                            }
                        }
                    ],
                    "synonyms": [
                        "вписывать",
                        "включать",
                        "добавлять"
                    ],
                    "common_phrases": [
                        "вносить изменения",
                        "вносить в список",
                        "вносить поправки",
                        "вносить данные"
                    ],
                    "target_lang_definitions": {
                        "en": "To write in or include information, corrections, or additions into a document, list, or text."
                    },
                    "translations": {

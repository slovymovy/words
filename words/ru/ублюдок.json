                },
                {
                    "sense_id": "c09eda78-adb3-39a2-b895-7e0e40bf29ea",
                    "sense_definition": "Животное, рождённое от скрещивания разных пород; непородистое животное.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "origin",
                    "name_type": "no",
                    "examples": [
                        {

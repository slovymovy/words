        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "d07fed9b-e84b-35cd-96f3-b8b855d845ce",
                    "sense_definition": "Een kleine hoeveelheid; niet veel (gebruikt bij niet-telbare woorden).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_mass",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is nog maar <w>weinig</w> boter over in de koelkast.",
                            "target_lang_translations": {
                                "en": "There is only <w>little</w> butter left in the fridge.",
                                "ru": "В холодильнике осталось совсем <w>мало</w> масла."
                            }
                        },
                        {
                            "text": "Ik heb helaas <w>weinig</w> tijd om je te helpen.",
                            "target_lang_translations": {
                                "en": "Unfortunately, I have <w>little</w> time to help you.",
                                "ru": "К сожалению, у меня <w>мало</w> времени, чтобы помочь тебе."
                            }
                        }
                    ],
                    "synonyms": [
                        "beetje"
                    ],
                    "antonyms": [
                        "veel"
                    ],
                    "common_phrases": [
                        "weinig tijd",
                        "weinig geld",
                        "weinig kans"
                    ],
                    "target_lang_definitions": {
                        "en": "A small amount; not much (used with uncountable words).",
                        "ru": "Небольшое количество; немного (используется с неисчисляемыми словами)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "little",
                                "target_lang_sense_clarification": "not much; for uncountable nouns"
                            },
                            {
                                "target_lang_word": "not much",
                                "target_lang_sense_clarification": "a small quantity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мало",
                                "target_lang_sense_clarification": "небольшое количество"
                            },
                            {
                                "target_lang_word": "немного",
                                "target_lang_sense_clarification": "некоторое количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6e195c5-1a1a-3bbf-863b-e68bc4dcb111",
                    "sense_definition": "Een klein aantal; niet veel (gebruikt bij telbare woorden).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_count",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zijn vandaag maar <w>weinig</w> toeristen in de stad.",
                            "target_lang_translations": {

                                "target_lang_sense_clarification": "питание/связь"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "turn off",
                                "target_lang_sense_clarification": "device/machine"
                            },
                            {
                                "target_lang_word": "switch off",
                                "target_lang_sense_clarification": "device/machine"
                            }
                        ]
                    }

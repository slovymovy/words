    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2e5e8127-29a2-325e-a849-269324810539",
                    "sense_definition": "Акцентирование внимания на чём-либо, выделение чего-либо как самого важного.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В своём докладе директор сделал основной <упор> на безопасность сотрудников.",
                            "target_lang_translations": {
                                "en": "In his report, the director placed the main <emphasis> on employee safety."
                            }
                        },
                        {
                            "text": "При изучении языка нужно делать <упор> на разговорную практику.",
                            "target_lang_translations": {
                                "en": "When learning a language, you need to <focus> on speaking practice."
                            }
                        }
                    ],
                    "synonyms": [
                        "акцент",
                        "ударение"
                    ],
                    "common_phrases": [
                        "делать упор",
                        "основной упор",
                        "упор на качество"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of focusing attention on something or highlighting it as the most important part."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "emphasis",
                                "target_lang_sense_clarification": "special importance or attention given to something"
                            },
                            {
                                "target_lang_word": "focus",
                                "target_lang_sense_clarification": "the center of interest or activity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09f2c1c4-47ae-3b7f-b40b-299bd2b2ba34",
                    "sense_definition": "Положение тела при выполнении физических упражнений с опорой на руки или ноги.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тренер скомандовал принять <упор> лёжа и начать отжимания.",
                            "target_lang_translations": {
                                "en": "The coach commanded us to assume the <push-up position> and start push-ups."
                            }
                        },
                        {
                            "text": "Гимнаст выполнял сложный <упор> на брусьях.",
                            "target_lang_translations": {
                                "en": "The gymnast performed a difficult <support> on the parallel bars."
                            }
                        }
                    ],
                    "common_phrases": [
                        "упор лёжа",
                        "принять упор",
                        "упор присев"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "спорт"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A body position in physical exercises where the weight is supported by the hands or arms."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "support",
                                "target_lang_sense_clarification": "gymnastics position"
                            },
                            {
                                "target_lang_word": "rest",
                                "target_lang_sense_clarification": "static position in exercises (e.g., front leaning rest)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b35458d8-dbce-3d05-8490-fedea95c9edb",
                    "sense_definition": "Предельно близкое расстояние, почти вплотную (часто используется в устойчивых выражениях о стрельбе или пристальном взгляде).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "idiomatic_distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он смотрел на меня в <упор>, и мне стало не по себе.",
                            "target_lang_translations": {
                                "en": "He looked at me <point-blank>, and I felt uneasy."
                            }
                        },
                        {
                            "text": "Я в <упор> не вижу здесь никакой ошибки, всё кажется правильным.",
                            "target_lang_translations": {
                                "en": "I don't see any mistake here <at all>; everything seems correct."
                            }
                        }
                    ],
                    "common_phrases": [
                        "стрелять в упор",
                        "смотреть в упор",
                        "в упор не видеть"
                    ],
                    "target_lang_definitions": {
                        "en": "Extremely close range, almost touching; often used in phrases regarding shooting or staring."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "point-blank range",
                                "target_lang_sense_clarification": "very close distance"
                            },
                            {
                                "target_lang_word": "point-blank",
                                "target_lang_sense_clarification": "adverb describing very close proximity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdff055a-a4f7-369e-af08-389c180c982c",
                    "sense_definition": "Деталь, предмет или устройство, которое поддерживает что-либо или ограничивает чьё-либо движение.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дверь с шумом ударилась о металлический <упор> на полу.",
                            "target_lang_translations": {
                                "en": "The door banged noisily against the metal <stop> on the floor."
                            }
                        },
                        {
                            "text": "Рабочий установил <упор>, чтобы закрепить деталь в станке.",
                            "target_lang_translations": {
                                "en": "The worker installed a <stop> to secure the part in the machine."
                            }
                        }
                    ],
                    "synonyms": [
                        "ограничитель",
                        "стопор",
                        "опора"
                    ],
                    "common_phrases": [
                        "дверной упор",
                        "до упора"
                    ],
                    "target_lang_definitions": {
                        "en": "A part, object, or device that supports something or limits movement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "device intended to block the path of a moving object"
                            },
                            {
                                "target_lang_word": "support",
                                "target_lang_sense_clarification": "something which holds up a structure"
                            },
                            {
                                "target_lang_word": "rest",
                                "target_lang_sense_clarification": "device used to support an object"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
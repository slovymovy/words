                },
                {
                    "sense_id": "979e2e61-96c3-3563-8199-4d73b59b8743",
                    "sense_definition": "Odpowiednia chwila lub moment na zrobienie czegoś",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To najwyższy <w>czas</w>, żebyśmy zaczęli przygotowania do wyjazdu.",
                            "target_lang_translations": {
                                "ru": "Уже самое <w>время</w> начать подготовку к отъезду."
                            }
                        },
                        {
                            "text": "Wszystko wydarzyło się w sam <w>czas</w>, dzięki czemu uniknęliśmy kłopotów.",
                            "target_lang_translations": {
                                "ru": "Всё произошло как раз во<w>время</w>, благодаря чему мы избежали неприятностей."
                            }

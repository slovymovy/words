        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fa11e6f5-64c7-33fc-afa2-8d4a4f44e61d",
                    "sense_definition": "Wyrażający miłość, troskę, delikatność i ciepłe emocje wobec innej osoby.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego głos był cichy i <w>czuły</w>, gdy opowiadał jej o swoich uczuciach.",
                            "target_lang_translations": {
                                "ru": "Его голос был тихим и <w>нежным</w>, когда он рассказывал ей о своих чувствах."
                            }
                        },
                        {
                            "text": "Matka przytuliła płaczące dziecko w bardzo <w>czuły</w> i uspokajający sposób.",
                            "target_lang_translations": {
                                "ru": "Мать обняла плачущего ребенка очень <w>ласковым</w> и успокаивающим образом."
                            }
                        }
                    ],
                    "synonyms": [
                        "serdeczny",
                        "tkliwy",
                        "troskliwy",
                        "delikatny"
                    ],
                    "antonyms": [
                        "oschły",
                        "chłodny",
                        "obojętny",
                        "szorstki"
                    ],
                    "common_phrases": [
                        "czułe słowa",
                        "czuły gest",
                        "czułe pożegnanie",
                        "czuły dotyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражающий любовь, заботу, нежность и теплые эмоции по отношению к другому человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нежный",
                                "target_lang_sense_clarification": "выражающий любовь и мягкость"
                            },
                            {
                                "target_lang_word": "ласковый",
                                "target_lang_sense_clarification": "проявляющий ласку и теплоту"
                            },
                            {
                                "target_lang_word": "чуткий",
                                "target_lang_sense_clarification": "внимательный к нуждам других"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68dee431-efff-33e6-babb-ac056237ec75",
                    "sense_definition": "O urządzeniach lub instrumentach: reagujący precyzyjnie nawet na niewielkie bodźce, zmiany lub sygnały.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To niezwykle <w>czuły</w> instrument, który wykrywa nawet najmniejsze drgania podłoża.",
                            "target_lang_translations": {
                                "ru": "Это чрезвычайно <w>чувствительный</w> прибор, который обнаруживает даже малейшие колебания почвы."
                            }
                        },
                        {
                            "text": "Mikrofon w studiu jest tak <w>czuły</w>, że rejestruje każdy szept.",
                            "target_lang_translations": {
                                "ru": "Микрофон в студии настолько <w>чувствительный</w>, что регистрирует каждый шепот."
                            }
                        }
                    ],
                    "synonyms": [
                        "precyzyjny",
                        "wrażliwy",
                        "dokładny"
                    ],
                    "common_phrases": [
                        "czuły instrument",
                        "czuły mikrofon",
                        "czuły na światło",
                        "czuły sensor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Об устройствах или инструментах: точно реагирующий даже на незначительные стимулы, изменения или сигналы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствительный",
                                "target_lang_sense_clarification": "о приборах и датчиках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48570590-7b66-3ef6-a2fb-b9488124dbcb",
                    "sense_definition": "O częściach ciała lub organizmie: silnie reagujący na bodźce fizyczne, takie jak dotyk, temperatura czy ból.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje zęby są bardzo <w>czułe</w> na zimne napoje i lody.",
                            "target_lang_translations": {
                                "ru": "Мои зубы очень <w>чувствительны</w> к холодным напиткам и мороженому."
                            }
                        },
                        {
                            "text": "Lekarz ostrożnie zbadał <w>czułe</w> miejsce na brzuchu pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врач осторожно осмотрел <w>болезненное</w> место на животе пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "wrażliwy",
                        "delikatny"
                    ],
                    "antonyms": [
                        "odporny",
                        "niewrażliwy"
                    ],
                    "common_phrases": [
                        "czuły punkt",
                        "czułe miejsce",
                        "czuła skóra"
                    ],
                    "target_lang_definitions": {
                        "ru": "О частях тела или организме: сильно реагирующий на физические раздражители, такие как прикосновение, температура или боль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствительный",
                                "target_lang_sense_clarification": "о реакции на внешние раздражители"
                            },
                            {
                                "target_lang_word": "болезненный",
                                "target_lang_sense_clarification": "вызывающий боль при прикосновении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5dacb735-488a-3f80-85f7-04d7a5a8c7ee",
                    "sense_definition": "Łatwo ulegający urazie, przesadnie reagujący emocjonalnie na krytykę lub uwagi dotyczące własnej osoby.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój szef jest bardzo <w>czuły</w> na punkcie swojego autorytetu i nie lubi sprzeciwu.",
                            "target_lang_translations": {
                                "ru": "Мой начальник очень <w>чувствителен</w> к вопросам своего авторитета и не любит возражений."
                            }
                        },
                        {
                            "text": "Nie bądź taki <w>czuły</w>, to był tylko niewinny żart.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>обидчивым</w>, это была всего лишь невинная шутка."
                            }
                        }
                    ],
                    "synonyms": [
                        "przewrażliwiony",
                        "drażliwy"
                    ],
                    "antonyms": [
                        "gruboskórny",
                        "dystansowny"
                    ],
                    "common_phrases": [
                        "czuły na krytykę",
                        "czuły na swoim punkcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легко обижающийся, чрезмерно эмоционально реагирующий на критику или замечания в свой адрес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обидчивый",
                                "target_lang_sense_clarification": "легко поддающийся обиде"
                            },
                            {
                                "target_lang_word": "чувствительный",
                                "target_lang_sense_clarification": "остро воспринимающий критику"
                            },
                            {
                                "target_lang_word": "восприимчивый",
                                "target_lang_sense_clarification": "легко поддающийся влиянию или эмоциям"
                            }
                        ]
                    }
                }
            ]
        },
        {

                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>stof</w> elke zaterdag de woonkamer en de slaapkamers.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6a8a2b4c-297b-3822-a900-25e12a494f2f",
                    "sense_definition": "Перестать удерживать кого-либо или что-либо физически; разжать пальцы или хватку.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "release_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты делаешь мне больно, <w>отпусти</w> мою руку!",
                            "target_lang_translations": {
                                "en": "You're hurting me, <w>let go</w> of my hand!"
                            }
                        },
                        {
                            "text": "Чтобы механизм сработал, нужно нажать и резко <w>отпустить</w> кнопку.",
                            "target_lang_translations": {
                                "en": "For the mechanism to work, you need to press and sharply <w>release</w> the button."
                            }
                        }
                    ],
                    "synonyms": [
                        "выпустить"
                    ],
                    "antonyms": [
                        "схватить",
                        "задержать",
                        "сжать"
                    ],
                    "common_phrases": [
                        "отпустить руку",
                        "отпустить кнопку",
                        "отпустить тормоз"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop holding someone or something physically; to unclench fingers or grip."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let go",
                                "target_lang_sense_clarification": "general physical release"
                            },
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "more formal or mechanical"
                            },
                            {
                                "target_lang_word": "unhand",
                                "target_lang_sense_clarification": "archaic or dramatic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7af244c-ce88-3b07-baaf-a6216dda886f",
                    "sense_definition": "Разрешить уйти или уехать; предоставить свободу действий.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "release_permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник согласился <w>отпустить</w> меня домой пораньше из-за плохого самочувствия.",
                            "target_lang_translations": {
                                "en": "The boss agreed to <w>let</w> me <w>go</w> home early because I wasn't feeling well."
                            }
                        },
                        {
                            "text": "Полиция решила <w>отпустить</w> задержанного за отсутствием улик.",
                            "target_lang_translations": {
                                "en": "The police decided to <w>release</w> the detainee due to lack of evidence."
                            }
                        }
                    ],
                    "synonyms": [
                        "освободить",
                        "выпустить"
                    ],
                    "antonyms": [
                        "задержать",
                        "арестовать",
                        "удержать"
                    ],
                    "common_phrases": [
                        "отпустить домой",
                        "отпустить с работы",
                        "отпустить на волю",
                        "отпустить гулять"
                    ],
                    "target_lang_definitions": {
                        "en": "To allow to leave or go away; to grant freedom of action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let go",
                                "target_lang_sense_clarification": "allow to leave"
                            },
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "from custody or confinement"
                            },
                            {
                                "target_lang_word": "dismiss",
                                "target_lang_sense_clarification": "from work or class"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7be213d-8845-35f6-85e5-332aabfc920b",
                    "sense_definition": "Сделать что-либо менее тугим или натянутым; ослабить.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "loosen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После плотного обеда ему пришлось немного <w>отпустить</w> ремень.",
                            "target_lang_translations": {
                                "en": "After a heavy lunch, he had to <w>loosen</w> his belt a little."
                            }
                        },
                        {
                            "text": "Попробуй <w>отпустить</w> струну, она натянута слишком сильно и может лопнуть.",
                            "target_lang_translations": {
                                "en": "Try to <w>loosen</w> the string; it's stretched too tight and might snap."
                            }
                        }
                    ],
                    "synonyms": [
                        "ослабить"
                    ],
                    "antonyms": [
                        "затянуть",
                        "натянуть"
                    ],
                    "common_phrases": [
                        "отпустить ремень",
                        "отпустить гайку",
                        "отпустить вожжи"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something less tight or stretched; to loosen."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "loosen",
                                "target_lang_sense_clarification": "make less tight"
                            },
                            {
                                "target_lang_word": "slacken",
                                "target_lang_sense_clarification": "reduce tension"
                            },
                            {
                                "target_lang_word": "let out",
                                "target_lang_sense_clarification": "clothing or line"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3af70856-bbcf-3cff-ba52-f5edfa163c78",
                    "sense_definition": "Перестать стричь волосы, усы или бороду, давая им вырасти.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "grow_hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За время долгого путешествия он успел <w>отпустить</w> густую бороду.",
                            "target_lang_translations": {
                                "en": "During the long trip, he managed to <w>grow</w> a thick beard."
                            }
                        },
                        {
                            "text": "Маша решила больше не стричься коротко и <w>отпустить</w> длинные волосы.",
                            "target_lang_translations": {
                                "en": "Masha decided not to cut her hair short anymore and to <w>grow out</w> her hair."
                            }
                        }
                    ],
                    "synonyms": [
                        "отрастить"
                    ],
                    "antonyms": [
                        "сбрить",
                        "остричь"
                    ],
                    "common_phrases": [
                        "отпустить бороду",
                        "отпустить усы",
                        "отпустить волосы",
                        "отпустить живот"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop cutting hair, mustache, or beard, allowing them to grow."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grow",
                                "target_lang_sense_clarification": "hair or beard"
                            },
                            {
                                "target_lang_word": "grow out",
                                "target_lang_sense_clarification": "hair"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e1a8477-5b36-3956-9a8b-8681dbc36055",
                    "sense_definition": "Стать менее сильным или прекратиться (о боли, болезни, чувствах или природных явлениях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "subside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Выпей обезболивающее, и через полчаса боль обязательно <w>отпустит</w>.",
                            "target_lang_translations": {
                                "en": "Take a painkiller, and in half an hour the pain will definitely <w>let up</w>."
                            }
                        },
                        {
                            "text": "Только когда мы вышли из темного леса, страх наконец <w>отпустил</w> нас.",
                            "target_lang_translations": {
                                "en": "Only when we got out of the dark forest did the fear finally <w>let go</w> of us."
                            }
                        }
                    ],
                    "synonyms": [
                        "ослабнуть",
                        "утихнуть",
                        "пройти"
                    ],
                    "antonyms": [
                        "усилиться"
                    ],
                    "common_phrases": [
                        "боль отпустила",
                        "мороз отпустил",
                        "сердце отпустило"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often impersonal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become less intense or cease (regarding pain, illness, feelings, or natural phenomena)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let up",
                                "target_lang_sense_clarification": "pain or weather"
                            },
                            {
                                "target_lang_word": "subside",
                                "target_lang_sense_clarification": "pain or emotion"
                            },
                            {
                                "target_lang_word": "ease",
                                "target_lang_sense_clarification": "become less severe"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eadf060d-b4a9-36af-b02f-b3ab4c2677d1",
                    "sense_definition": "Сказать что-либо (обычно шутку, колкость или комплимент).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "say_phrase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он любил <w>отпустить</w> едкую шутку в самый неподходящий момент.",
                            "target_lang_translations": {
                                "en": "He loved to <w>crack</w> a caustic joke at the most inappropriate moment."
                            }
                        },
                        {
                            "text": "При встрече с дамой вежливый кавалер должен <w>отпустить</w> комплимент.",
                            "target_lang_translations": {
                                "en": "When meeting a lady, a polite gentleman should <w>pay</w> a compliment."
                            }
                        }
                    ],
                    "synonyms": [
                        "высказать",
                        "сказать"
                    ],
                    "common_phrases": [
                        "отпустить шутку",
                        "отпустить комплимент",
                        "отпустить замечание",
                        "отпустить остроту"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To say something (usually a joke, barb, or compliment)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crack",
                                "target_lang_sense_clarification": "a joke"
                            },
                            {
                                "target_lang_word": "pay",
                                "target_lang_sense_clarification": "a compliment"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "a remark or comment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "051e6458-632d-3bd9-ab75-d8476f0abb16",
                    "sense_definition": "Выдать, выделить или продать что-либо (средства, товары) в официальном порядке.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "allocate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Министерство распорядилось <w>отпустить</w> дополнительные средства на ремонт школы.",
                            "target_lang_translations": {
                                "en": "The ministry ordered to <w>allocate</w> additional funds for the school renovation."
                            }
                        },
                        {
                            "text": "Продавец отказался <w>отпустить</w> товар в долг без разрешения управляющего.",
                            "target_lang_translations": {
                                "en": "The seller refused to <w>sell</w> the goods on credit without the manager's permission."
                            }
                        }
                    ],
                    "synonyms": [
                        "выделить",
                        "выдать",
                        "предоставить"
                    ],
                    "common_phrases": [
                        "отпустить средства",
                        "отпустить товар",
                        "отпустить кредит"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "official or business"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To issue, allocate, or sell something (funds, goods) in an official manner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "allocate",
                                "target_lang_sense_clarification": "funds or resources"
                            },
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "goods or funds"
                            },
                            {
                                "target_lang_word": "dispense",
                                "target_lang_sense_clarification": "goods or medicine"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83ed1d06-dfb4-3416-83b0-af80d812eec1",
                    "sense_definition": "Простить вину (в церковном или высоком контексте).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "forgive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Верующие приходят в церковь на исповедь, чтобы священник мог <w>отпустить</w> им грехи.",
                            "target_lang_translations": {
                                "en": "Believers come to church for confession so the priest can <w>absolve</w> their sins."
                            }
                        },
                        {
                            "text": "Старец пообещал помолиться за него и <w>отпустить</w> душу с миром.",
                            "target_lang_translations": {
                                "en": "The elder promised to pray for him and <w>let</w> his soul <w>go</w> in peace."
                            }
                        }
                    ],
                    "synonyms": [
                        "простить"
                    ],
                    "common_phrases": [
                        "отпустить грехи"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "religious"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To forgive guilt (in a religious or high context)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "absolve",
                                "target_lang_sense_clarification": "sins"
                            },
                            {
                                "target_lang_word": "forgive",
                                "target_lang_sense_clarification": "general"
                            }
                        ]
                    }
                }
            ]
        }
    ]

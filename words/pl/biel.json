                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бели",
                                "target_lang_sense_clarification": "крась в белый"
                            },
                            {
                                "target_lang_word": "отбеливай",
                                "target_lang_sense_clarification": "делай белым"
                            }
                        ]
                    }
                }
            ]
        },
        {

                {
                    "sense_id": "2dfa1b65-38b2-3b21-9873-f0906279e9cb",
                    "sense_definition": "Iedereen; alle personen in een groep zonder uitzondering.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_persons",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij gaan <allemaal> naar het feest vanavond.",
                            "target_lang_translations": {
                                "en": "We are <all> going to the party tonight.",
                                "ru": "Мы <все> идём сегодня вечером на вечеринку."
                            }
                        },
                        {
                            "text": "De studenten hebben het examen <allemaal> gehaald.",
                            "target_lang_translations": {
                                "en": "The students <all> passed the exam.",
                                "ru": "Студенты <все> сдали экзамен."
                            }
                        }

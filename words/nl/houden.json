    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d113aafb-3fce-341d-90e2-f85cac519032",
                    "sense_definition": "Veel genegenheid of liefde voelen voor iemand of iets (altijd gebruikt met 'van').",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <hou> zielsveel van jou en de kinderen.",
                            "target_lang_translations": {
                                "en": "I <love> you and the children with all my heart.",
                                "ru": "Я всей душой <люблю> тебя и детей."
                            }
                        },
                        {
                            "text": "Mijn vader <houdt> helemaal niet van moderne kunst.",
                            "target_lang_translations": {
                                "en": "My father does not <like> modern art at all.",
                                "ru": "Мой отец совсем не <любит> современное искусство."
                            }
                        }
                    ],
                    "antonyms": [
                        "haten"
                    ],
                    "common_phrases": [
                        "houden van",
                        "van iemand houden"
                    ],
                    "target_lang_definitions": {
                        "en": "To feel deep affection or love for someone or something (always used with 'van').",
                        "ru": "Чувствовать сильную привязанность или любовь к кому-то или чему-то (всегда используется с 'van')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "love",
                                "target_lang_sense_clarification": "deep affection for people"
                            },
                            {
                                "target_lang_word": "like",
                                "target_lang_sense_clarification": "enjoyment or preference for things"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "любить",
                                "target_lang_sense_clarification": "испытывать любовь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d92ba9d2-28a2-3b56-b724-573f6f739dd2",
                    "sense_definition": "Iets in bezit hebben en niet weggeven of wegdoen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je mag het wisselgeld <houden>.",
                            "target_lang_translations": {
                                "en": "You can <keep> the change.",
                                "ru": "Ты можешь <оставить> сдачу себе."
                            }
                        },
                        {
                            "text": "Hij <hield> het huis, maar zij kreeg de auto.",
                            "target_lang_translations": {
                                "en": "He <kept> the house, but she got the car.",
                                "ru": "Он <оставил> себе дом, а она получила машину."
                            }
                        }
                    ],
                    "synonyms": [
                        "bewaren",
                        "behouden"
                    ],
                    "antonyms": [
                        "weggeven",
                        "verliezen",
                        "wegdoen"
                    ],
                    "common_phrases": [
                        "mogen houden",
                        "zelf houden"
                    ],
                    "target_lang_definitions": {
                        "en": "To have something in one's possession and not give it away or dispose of it.",
                        "ru": "Иметь что-то во владении и не отдавать или не выбрасывать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "retain possession"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "сохранить у себя"
                            },
                            {
                                "target_lang_word": "хранить",
                                "target_lang_sense_clarification": "беречь, не выбрасывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44d1d723-06f6-3231-b26a-5a8ca8e568d1",
                    "sense_definition": "Zorgen dat iets of iemand in een bepaalde toestand blijft.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de wond goed schoon en droog <houden>.",
                            "target_lang_translations": {
                                "en": "You must <keep> the wound clean and dry.",
                                "ru": "Нужно <держать> рану чистой и сухой."
                            }
                        },
                        {
                            "text": "Kun jij even je mond <houden> tijdens de film?",
                            "target_lang_translations": {
                                "en": "Can you just <keep> your mouth shut during the movie?",
                                "ru": "Ты можешь <держать> язык за зубами во время фильма?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "schoon houden",
                        "mond houden",
                        "rekening houden met",
                        "in de gaten houden"
                    ],
                    "target_lang_definitions": {
                        "en": "To ensure that something or someone remains in a particular state.",
                        "ru": "Обеспечивать, чтобы что-то или кто-то оставался в определенном состоянии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "maintain a condition"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "держать",
                                "target_lang_sense_clarification": "поддерживать состояние"
                            },
                            {
                                "target_lang_word": "сохранять",
                                "target_lang_sense_clarification": "сберегать в определенном виде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f2ed190-690b-3c81-8c8d-a07ab08a2dc6",
                    "sense_definition": "Een evenement, bijeenkomst of activiteit organiseren en laten plaatsvinden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <houden> morgen een groot feest voor zijn verjaardag.",
                            "target_lang_translations": {
                                "en": "We are <holding> a big party for his birthday tomorrow.",
                                "ru": "Мы <устраиваем> завтра большой праздник в честь его дня рождения."
                            }
                        },
                        {
                            "text": "De directeur <hield> een lange toespraak voor het personeel.",
                            "target_lang_translations": {
                                "en": "The director <gave> a long speech to the staff.",
                                "ru": "Директор <произнес> длинную речь перед персоналом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een toespraak houden",
                        "een feest houden",
                        "een lezing houden"
                    ],
                    "target_lang_definitions": {
                        "en": "To organize an event, meeting, or activity and cause it to take place.",
                        "ru": "Организовывать мероприятие, встречу или деятельность и проводить их."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "organize an event"
                            },
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "deliver a speech or host a party"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "проводить",
                                "target_lang_sense_clarification": "осуществлять мероприятие"
                            },
                            {
                                "target_lang_word": "устраивать",
                                "target_lang_sense_clarification": "организовывать праздник или встречу"
                            },
                            {
                                "target_lang_word": "произносить",
                                "target_lang_sense_clarification": "говорить речь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb8054f2-a433-360a-a569-73557c8c1f7c",
                    "sense_definition": "Dieren verzorgen en laten leven in een bepaalde ruimte, vaak als hobby of voor veeteelt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buren <houden> kippen in hun achtertuin.",
                            "target_lang_translations": {
                                "en": "My neighbors <keep> chickens in their backyard.",
                                "ru": "Мои соседи <держат> кур на заднем дворе."
                            }
                        },
                        {
                            "text": "Het is verboden om hier exotische dieren te <houden>.",
                            "target_lang_translations": {
                                "en": "It is forbidden to <keep> exotic animals here.",
                                "ru": "Здесь запрещено <держать> экзотических животных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vee houden",
                        "huisdieren houden"
                    ],
                    "target_lang_definitions": {
                        "en": "To care for animals and have them live in a specific space, often as a hobby or for farming.",
                        "ru": "Ухаживать за животными и содержать их в определенном месте, часто как хобби или для животноводства."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "care for animals"
                            },
                            {
                                "target_lang_word": "raise",
                                "target_lang_sense_clarification": "breed and rear livestock"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "держать",
                                "target_lang_sense_clarification": "иметь животных"
                            },
                            {
                                "target_lang_word": "разводить",
                                "target_lang_sense_clarification": "выращивать животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3405f8f0-736e-3c19-98ed-2815cad46ed8",
                    "sense_definition": "Een bepaalde veronderstelling hebben of een conclusie trekken (vaak in combinatie met 'op' of 'voor').",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie <hield> het op een ongeluk, maar er was twijfel.",
                            "target_lang_translations": {
                                "en": "The police <put> it down to an accident, but there was doubt.",
                                "ru": "Полиция <считала> это несчастным случаем, но были сомнения."
                            }
                        },
                        {
                            "text": "Ik <hou> het voor gezien en ga naar huis.",
                            "target_lang_translations": {
                                "en": "I'm <calling> it a day and going home.",
                                "ru": "Я <заканчиваю> с этим и иду домой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het houden op",
                        "voor mogelijk houden",
                        "voor gezien houden"
                    ],
                    "target_lang_definitions": {
                        "en": "To hold a certain assumption or draw a conclusion (often used with 'op' or 'voor').",
                        "ru": "Иметь определенное предположение или делать вывод (часто в сочетании с 'op' или 'voor')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "consider",
                                "target_lang_sense_clarification": "regard as"
                            },
                            {
                                "target_lang_word": "take for",
                                "target_lang_sense_clarification": "assume to be"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "полагать, расценивать"
                            },
                            {
                                "target_lang_word": "придерживаться",
                                "target_lang_sense_clarification": "мнения или версии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
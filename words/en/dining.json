                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "званый ужин",
                                "target_lang_sense_clarification": "мероприятие с приглашением гостей"
                            },
                            {
                                "target_lang_word": "застолье",
                                "target_lang_sense_clarification": "праздничный стол с угощением"
                            }
                        ]
                    }
                }
            ]
        }
    ]

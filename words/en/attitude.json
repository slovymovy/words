                            },
                            {
                                "target_lang_word": "настрой",
                                "target_lang_sense_clarification": "душевное состояние, настроение"
                            },
                            {
                                "target_lang_word": "позиция",
                                "target_lang_sense_clarification": "точка зрения, принцип"
                            }
                        ]

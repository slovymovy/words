{
    "word_family": [
        "przypadek",
        "przypadkowy",
        "przypadkowo",
        "przypadłość"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "61fdcf87-bd7b-3f4c-ba3e-518f028e779d",
                    "sense_definition": "Zdarzać się lub mieć miejsce w określonym terminie (dotyczy dat, świąt, rocznic).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym roku Wielkanoc <w>przypada</w> wyjątkowo wcześnie, bo już w marcu.",
                            "target_lang_translations": {
                                "ru": "В этом году Пасха <w>выпадает</w> исключительно рано, уже в марте."
                            }
                        },
                        {
                            "text": "Jutro <w>przypada</w> dziesiąta rocznica naszego ślubu, więc planujemy uroczystą kolację.",
                            "target_lang_translations": {
                                "ru": "На завтра <w>приходится</w> десятая годовщина нашей свадьбы, поэтому мы планируем праздничный ужин."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypadać",
                        "zdarzać się"
                    ],
                    "common_phrases": [
                        "przypadać na dzień",
                        "przypadać w weekend",
                        "święto przypada"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случаться или иметь место в определенный срок (касается дат, праздников, годовщин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приходиться",
                                "target_lang_sense_clarification": "о датах и событиях"
                            },
                            {
                                "target_lang_word": "выпадать",
                                "target_lang_sense_clarification": "совпадать с датой или днем недели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b68d30f-f95c-30a6-8a33-2a511ac0808d",
                    "sense_definition": "Być przydzielonym komuś jako udział, obowiązek lub nagroda; statystycznie dotyczyć jednej jednostki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "allocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Według najnowszych danych na jednego mieszkańca <w>przypada</w> średnio dwa samochody.",
                            "target_lang_translations": {
                                "ru": "Согласно последним данным, на одного жителя <w>приходится</w> в среднем два автомобиля."
                            }
                        },
                        {
                            "text": "Główna nagroda w loterii <w>przypada</w> osobie, która wylosowała szczęśliwy numer.",
                            "target_lang_translations": {
                                "ru": "Главный приз в лотерее <w>достается</w> человеку, который вытянул счастливый номер."
                            }
                        }
                    ],
                    "synonyms": [
                        "należeć się",
                        "dostawać się",
                        "trafiać się"
                    ],
                    "common_phrases": [
                        "przypadać w udziale",
                        "przypadać na głowę",
                        "przypadać na osobę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть выделенным кому-то как доля, обязанность или награда; статистически относиться к одной единице."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приходиться",
                                "target_lang_sense_clarification": "статистически или как доля"
                            },
                            {
                                "target_lang_word": "доставаться",
                                "target_lang_sense_clarification": "получать в качестве доли или награды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95a1a1fc-c4bf-3084-a99b-3c47dcf8b605",
                    "sense_definition": "Gwałtownie pochylać się i mocno przywierać ciałem do ziemi lub innej powierzchni, często w celu ukrycia się.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestraszony zając <w>przypada</w> do miedzy, próbując stać się niewidocznym dla drapieżnika.",
                            "target_lang_translations": {
                                "ru": "Испуганный заяц <w>припадает</w> к меже, пытаясь стать невидимым для хищника."
                            }
                        },
                        {
                            "text": "Żołnierz na dźwięk wybuchu natychmiast <w>przypadał</w> do ziemi.",
                            "target_lang_translations": {
                                "ru": "Солдат при звуке взрыва немедленно <w>припадал</w> к земле."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywierać",
                        "tulić się"
                    ],
                    "common_phrases": [
                        "przypadać do ziemi",
                        "przypadać do muru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко наклоняться и плотно прижиматься телом к земле или другой поверхности, часто с целью укрытия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "припадать",
                                "target_lang_sense_clarification": "плотно прижиматься к поверхности"
                            },
                            {
                                "target_lang_word": "прижиматься",
                                "target_lang_sense_clarification": "тесно прилегать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
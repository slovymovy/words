            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cc958673-901b-346c-92bb-2990c603d033",
                    "sense_definition": "Solidnie wykonany, dobrej jakości, trwały i spełniający swoje zadanie (o rzeczy).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quality_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić sobie <w>porządne</w> buty na zimę, bo stare już przemakają.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить себе <w>добротные</w> ботинки на зиму, потому что старые уже промокают."
                            }
                        },
                        {
                            "text": "To jest naprawdę <w>porządny</w> sprzęt, który posłuży ci przez wiele lat.",
                            "target_lang_translations": {
                                "ru": "Это действительно <w>качественное</w> оборудование, которое прослужит тебе много лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "solidny",
                        "wytrzymały",
                        "dobry"
                    ],
                    "antonyms": [
                        "nieporządny",
                        "nieuczciwy"
                    ],
                    "common_phrases": [
                        "porządny samochód",
                        "porządna robota",
                        "porządny sprzęt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный на совесть, хорошего качества, прочный и надежный (о вещах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добротный",
                                "target_lang_sense_clarification": "прочный, качественно сделанный"
                            },
                            {
                                "target_lang_word": "качественный",
                                "target_lang_sense_clarification": "высокого качества"
                            },
                            {
                                "target_lang_word": "хороший",
                                "target_lang_sense_clarification": "общий термин, выражающий положительные свойства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51966a4c-528a-350a-98f5-7b71bb64cec4",
                    "sense_definition": "Uczciwy, postępujący zgodnie z zasadami moralnymi i ogólnie przyjętymi normami (o człowieku).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quality_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad to bardzo <w>porządny</w> człowiek, zawsze wszystkim pomaga.",
                            "target_lang_translations": {
                                "ru": "Мой сосед — очень <w>порядочный</w> человек, он всегда всем помогает."
                            }
                        },
                        {
                            "text": "Szukam <w>porządnego</w> pracownika, któremu będę mógł w pełni zaufać w firmie.",
                            "target_lang_translations": {
                                "ru": "Я ищу <w>порядочного</w> сотрудника, которому смогу полностью доверять в фирме."
                            }
                        }
                    ],
                    "synonyms": [
                        "uczciwy",
                        "przyzwoity",
                        "prawy",
                        "zacny"
                    ],
                    "antonyms": [
                        "nieuczciwy"
                    ],
                    "common_phrases": [
                        "porządny człowiek",
                        "porządny obywatel",
                        "porządna rodzina"
                    ],
                    "target_lang_definitions": {
                        "ru": "Честный, соблюдающий моральные нормы и правила поведения (о человеке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порядочный",
                                "target_lang_sense_clarification": "честный, достойный уважения"
                            },
                            {
                                "target_lang_word": "честный",
                                "target_lang_sense_clarification": "не склонный к обману"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1c50d5a-27c9-3ca6-be23-c57fc9cb66ab",
                    "sense_definition": "Dbający o czystość, ład i organizację; utrzymujący porządek wokół siebie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "neatness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bądź <w>porządny</w> i posprzątaj swój pokój, zanim wyjdziesz do kolegów.",
                            "target_lang_translations": {
                                "ru": "Будь <w>аккуратным</w> и уберись в своей комнате, прежде чем идти к друзьям."
                            }
                        },
                        {
                            "text": "Ona jest bardzo <w>porządna</w>, w jej domu nigdy nie zobaczysz kurzu.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>чистоплотная</w>, в её доме никогда не увидишь пыли."
                            }
                        }
                    ],
                    "synonyms": [
                        "schludny",
                        "poukładany"
                    ],
                    "antonyms": [
                        "nieporządny"
                    ],
                    "common_phrases": [
                        "bądź porządny",
                        "porządna gospodyni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любящий чистоту и порядок, организованный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аккуратный",
                                "target_lang_sense_clarification": "соблюдающий порядок"
                            },
                            {
                                "target_lang_word": "опрятный",
                                "target_lang_sense_clarification": "чистый, следящий за своим внешним видом"
                            },
                            {
                                "target_lang_word": "чистоплотный",
                                "target_lang_sense_clarification": "любящий чистоту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c952f14b-6d7a-3027-aa93-da6f6dce040f",
                    "sense_definition": "O dużej intensywności, wielkości lub sile.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zjadłem dzisiaj <w>porządny</w> kawał ciasta i nie mam już miejsca na obiad.",
                            "target_lang_translations": {
                                "ru": "Я съел сегодня <w>приличный</w> кусок пирога, и в меня уже не лезет обед."
                            }
                        },
                        {
                            "text": "Wczoraj w nocy spadł <w>porządny</w> deszcz i zalało nam piwnicę.",
                            "target_lang_translations": {
                                "ru": "Вчера ночью прошел <w>сильный</w> дождь, и нам затопило подвал."
                            }
                        }

    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "24d2648c-a05a-3df9-893d-6d9a5ddb61a7",
                    "sense_definition": "Verbogen vorm van <w>elk</w>: gebruikt voor een zelfstandig naamwoord (de-woord) om naar alle personen of dingen in een groep afzonderlijk te verwijzen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gaat <elke> dag een stuk wandelen in het park.",
                            "target_lang_translations": {
                                "en": "He goes for a walk in the park <w>every</w> day."
                            }
                        },
                        {
                            "text": "Niet <elke> verandering is een verbetering voor het bedrijf.",
                            "target_lang_translations": {
                                "en": "Not <w>every</w> change is an improvement for the company."
                            }
                        },
                        {
                            "text": "Ze probeert <elke> keer weer haar record te verbreken.",
                            "target_lang_translations": {
                                "en": "She tries to break her record <w>each</w> time."
                            }
                        }
                    ],
                    "synonyms": [
                        "iedere"
                    ],
                    "common_phrases": [
                        "elke dag",
                        "elke keer",
                        "elke week",
                        "elke ochtend"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "van elk"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Inflected form of 'elk': used before a noun to refer to all people or things in a group individually."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "every",
                                "target_lang_sense_clarification": "referring to all members of a group"
                            },
                            {
                                "target_lang_word": "each",
                                "target_lang_sense_clarification": "emphasizing individual members"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "754c2b0a-5d86-3700-8201-710055a45d51",
                    "sense_definition": "Verbogen vorm van <w>elk</w>: gebruikt voor een meervoudig zelfstandig naamwoord, meestal in combinatie met een telwoord.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bus naar het centrum vertrekt <elke> tien minuten.",
                            "target_lang_translations": {
                                "en": "The bus to the center leaves <w>every</w> ten minutes."
                            }
                        },
                        {
                            "text": "We gaan <elke> paar weken bij onze grootouders op bezoek.",
                            "target_lang_translations": {
                                "en": "We visit our grandparents <w>every</w> few weeks."
                            }
                        }
                    ],
                    "synonyms": [
                        "iedere"
                    ],
                    "common_phrases": [

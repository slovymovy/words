    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c0792dd1-1715-30c2-8781-4c95ac7b7bf9",
                    "sense_definition": "Приводить к каким-либо последствиям, быть причиной чего-либо; неизбежно вызывать что-либо.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Любое нарушение закона будет неизбежно <влечь> за собой наказание.",
                            "target_lang_translations": {
                                "en": "Any violation of the law will inevitably <entail> punishment."
                            }
                        },
                        {
                            "text": "Это необдуманное решение может <влечь> за собой серьёзные финансовые потери.",
                            "target_lang_translations": {
                                "en": "This rash decision could <result in> serious financial losses."
                            }
                        }
                    ],
                    "synonyms": [
                        "вызывать"
                    ],
                    "common_phrases": [
                        "влечь за собой"
                    ],
                    "target_lang_definitions": {
                        "en": "To lead to consequences, be the cause of something; to inevitably cause something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "entail",
                                "target_lang_sense_clarification": "involve as a necessary consequence"
                            },
                            {
                                "target_lang_word": "result in",
                                "target_lang_sense_clarification": "cause to happen"
                            },
                            {
                                "target_lang_word": "involve",
                                "target_lang_sense_clarification": "have as a necessary part or result"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d642018-60cc-3099-b1bb-1793b29730ae",
                    "sense_definition": "Вызывать сильный интерес, желание приблизиться или заняться чем-либо; манить к себе.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его всегда <влекло> к морю и дальним странствиям.",
                            "target_lang_translations": {
                                "en": "He was always <drawn> to the sea and distant travels."
                            }
                        },
                        {
                            "text": "Таинственная музыка <влекла> гостей в глубину сада.",
                            "target_lang_translations": {
                                "en": "Mysterious music <lured> the guests into the depths of the garden."
                            }
                        }
                    ],
                    "synonyms": [
                        "манить",
                        "притягивать",
                        "привлекать",
                        "увлекать"
                    ],
                    "target_lang_definitions": {
                        "en": "To arouse strong interest, a desire to approach or engage in something; to beckon."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attract",
                                "target_lang_sense_clarification": "cause to come to a place or participate"
                            },
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "attract or interest someone"
                            },
                            {
                                "target_lang_word": "lure",
                                "target_lang_sense_clarification": "tempt or attract"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9824eb38-9b94-37ef-89c8-8ee3711d9178",
                    "sense_definition": "Перемещать, тащить или тянуть кого-либо или что-либо с усилием.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Уставшие лошади с трудом <влекли> тяжёлую повозку в гору.",
                            "target_lang_translations": {
                                "en": "The tired horses <dragged> the heavy cart up the hill with difficulty."
                            }
                        },
                        {
                            "text": "Бурлаки <влекли> баржу по реке, напевая грустную песню.",
                            "target_lang_translations": {
                                "en": "The burlaks <pulled> the barge along the river, singing a sad song."
                            }
                        }
                    ],
                    "synonyms": [
                        "тащить",
                        "тянуть",
                        "волочить",
                        "влачить"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "в современном языке обычно используются слова «тащить» или «тянуть»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To move, drag, or pull someone or something with effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drag",
                                "target_lang_sense_clarification": "pull along a surface with effort"
                            },
                            {
                                "target_lang_word": "pull",
                                "target_lang_sense_clarification": "exert force to move something"
                            },
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "pull or drag (a vehicle or load)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53fe8447-5011-3c53-a005-232cdba32a46",
                    "sense_definition": "Лечь внутрь чего-либо, поместиться в углубление.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Огромный камень плотно <влёг> в яму посреди дороги.",
                            "target_lang_translations": {
                                "en": "A huge stone <lodged> tightly in the hole in the middle of the road."
                            }
                        },
                        {
                            "text": "Зверь осторожно <влёг> в свою нору, готовясь к зиме.",
                            "target_lang_translations": {
                                "en": "The beast carefully <settled> into its burrow, preparing for winter."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To lie inside something, to fit into a hollow or depression."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lodge",
                                "target_lang_sense_clarification": "become firmly fixed in a place"
                            },
                            {
                                "target_lang_word": "settle",
                                "target_lang_sense_clarification": "become established in a position"
                            },
                            {
                                "target_lang_word": "fit",
                                "target_lang_sense_clarification": "be of the right shape and size"
                            }
                        ]
                    }
                }
            ]
        }
    ]

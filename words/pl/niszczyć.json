    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2f0861f4-436e-3ea3-a4b2-8888761128b1",
                    "sense_definition": "Powodować, że coś przestaje istnieć, traci swój kształt lub staje się niezdatne do użytku; psuć lub dewastować fizycznie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wandalowie często <niszczą> publiczne mienie w parkach.",
                            "target_lang_translations": {
                                "ru": "Вандалы часто <портят> общественное имущество в парках."
                            }
                        },
                        {
                            "text": "Silny wiatr <niszczy> dachy domów w całej okolicy.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер <разрушает> крыши домов во всей округе."
                            }
                        }
                    ],
                    "synonyms": [
                        "psuć",
                        "dewastować",
                        "demolować",
                        "burzyć",
                        "łamać",
                        "destruować"
                    ],
                    "antonyms": [
                        "naprawiać",
                        "budować",
                        "tworzyć"
                    ],
                    "common_phrases": [
                        "niszczyć mienie",
                        "niszczyć przyrodę",
                        "niszczyć dowody"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить к тому, что что-то перестает существовать, теряет форму или становится непригодным для использования; физически портить или опустошать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрушать",
                                "target_lang_sense_clarification": "ломать, превращать в развалины (здания, объекты)"
                            },
                            {
                                "target_lang_word": "уничтожать",
                                "target_lang_sense_clarification": "полностью истреблять, прекращать существование"
                            },
                            {
                                "target_lang_word": "портить",
                                "target_lang_sense_clarification": "повреждать, делать непригодным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ca7959b-55e3-32a1-a05b-87a15417e453",
                    "sense_definition": "Powodować wielkie szkody w czymś niematerialnym, np. w zdrowiu, życiu, relacjach lub planach.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Palenie papierosów powoli <niszczy> twoje zdrowie.",
                            "target_lang_translations": {
                                "ru": "Курение сигарет медленно <разрушает> твое здоровье."
                            }
                        },
                        {
                            "text": "Nie chcę <niszczyć> twoich marzeń, ale musisz być realistą.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <разрушать> твои мечты, но ты должен быть реалистом."
                            }
                        }
                    ],
                    "synonyms": [
                        "unicestwiać",
                        "niwelować",
                        "psuć"
                    ],
                    "antonyms": [
                        "budować",
                        "kreować",
                        "tworzyć"
                    ],
                    "common_phrases": [
                        "niszczyć zdrowie",
                        "niszczyć życie",
                        "niszczyć plany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить большой вред чему-то нематериальному, например, здоровью, жизни, отношениям или планам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрушать",
                                "target_lang_sense_clarification": "планы, мечты, здоровье"
                            },
                            {
                                "target_lang_word": "губить",
                                "target_lang_sense_clarification": "жизнь, здоровье, судьбу"
                            },
                            {
                                "target_lang_word": "портить",
                                "target_lang_sense_clarification": "ухудшать (здоровье, отношения)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e88321e1-2b1d-3a94-ac12-5441d01d7b3f",
                    "sense_definition": "Ulegać uszkodzeniu, zużyciu lub zepsuciu pod wpływem czasu lub czynników zewnętrznych (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tanie buty bardzo szybko się <niszczą>.",
                            "target_lang_translations": {
                                "ru": "Дешевая обувь очень быстро <изнашивается>."
                            }
                        },
                        {
                            "text": "Stare dokumenty <niszczą> się pod wpływem wilgoci.",
                            "target_lang_translations": {
                                "ru": "Старые документы <портятся> под воздействием влаги."
                            }
                        }
                    ],
                    "synonyms": [
                        "niszczeć",
                        "psuć"
                    ],
                    "common_phrases": [
                        "szybko się niszczyć",
                        "ubrania się niszczą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергаться повреждению, износу или порче под воздействием времени или внешних факторов (используется с местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портиться",
                                "target_lang_sense_clarification": "приходить в негодность"
                            },
                            {
                                "target_lang_word": "изнашиваться",
                                "target_lang_sense_clarification": "об одежде, обуви или механизмах"
                            },
                            {
                                "target_lang_word": "разрушаться",
                                "target_lang_sense_clarification": "о материалах или структурах под влиянием среды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bb255f8-1461-3aa8-9d6b-b48b74c1d78b",
                    "sense_definition": "Szkodzić sobie wzajemnie lub doprowadzać się do wyczerpania psychicznego (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciągłymi kłótniami tylko się psychicznie <niszczycie>.",
                            "target_lang_translations": {
                                "ru": "Постоянными ссорами вы только морально себя <уничтожаете>."
                            }
                        },
                        {
                            "text": "Byli małżonkowie <niszczą> się nawzajem w sądzie.",
                            "target_lang_translations": {
                                "ru": "Бывшие супруги <уничтожают> друг друга в суде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niszczyć się psychicznie",
                        "niszczyć się nawzajem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вредить друг другу или доводить себя до психического истощения (используется с местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уничтожать",
                                "target_lang_sense_clarification": "морально давить друг друга или себя"
                            },
                            {
                                "target_lang_word": "изводить",
                                "target_lang_sense_clarification": "мучить, доводить до истощения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
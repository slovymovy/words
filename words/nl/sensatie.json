            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6bcfe4e6-0cfb-3a6c-a19d-63ccf553882b",
                    "sense_definition": "Grote opwinding of opschudding door een verrassende gebeurtenis.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "commotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het nieuws over de ontsnapping zorgde voor grote <w>sensatie</w> in het dorp.",
                            "target_lang_translations": {
                                "ru": "Новости о побеге вызвали настоящую <w>сенсацию</w> в деревне.",
                                "en": "The news about the escape caused a great <w>sensation</w> in the village."
                            }
                        },
                        {
                            "text": "De kranten waren op zoek naar <w>sensatie</w> en overdreven het verhaal enorm.",
                            "target_lang_translations": {
                                "ru": "Газеты гнались за <w>сенсацией</w> и сильно преувеличили эту историю.",
                                "en": "The newspapers were looking for <w>sensation</w> and exaggerated the story enormously."
                            }
                        }
                    ],
                    "synonyms": [
                        "opschudding",
                        "ophef",
                        "beroering"
                    ],
                    "antonyms": [
                        "rust",
                        "kalmte"
                    ],
                    "common_phrases": [
                        "op zoek naar sensatie",
                        "een ware sensatie",
                        "belust op sensatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное волнение или переполох, вызванные неожиданным событием.",
                        "en": "Great excitement or commotion caused by a surprising event."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сенсация",
                                "target_lang_sense_clarification": "событие, вызывающее волнение"
                            },
                            {
                                "target_lang_word": "шумиха",
                                "target_lang_sense_clarification": "ажиотаж, переполох"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "sensation",
                                "target_lang_sense_clarification": "widespread excitement"
                            },
                            {
                                "target_lang_word": "stir",
                                "target_lang_sense_clarification": "commotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f3e00b6-d923-3539-9b5f-663a3063b767",
                    "sense_definition": "Een plotseling en hevig gevoel van opwinding of emotie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was een vreemde <w>sensatie</w> om na al die jaren weer in mijn ouderlijk huis te zijn.",
                            "target_lang_translations": {
                                "ru": "Было странное <w>ощущение</w> снова оказаться в родительском доме спустя столько лет.",
                                "en": "It was a strange <w>sensation</w> to be back in my parental home after all those years."
                            }
                        },
                        {
                            "text": "Ze voelde een lichte <w>sensatie</w> van angst toen ze het donkere bos in liep.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала легкое <w>ощущение</w> страха, входя в темный лес.",
                                "en": "She felt a slight <w>sensation</w> of fear as she walked into the dark forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "gewaarwording",
                        "beleving"
                    ],
                    "common_phrases": [
                        "een vreemde sensatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное и сильное чувство или переживание.",
                        "en": "A sudden and intense feeling of excitement or emotion."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ощущение",
                                "target_lang_sense_clarification": "внутреннее переживание"
                            },
                            {
                                "target_lang_word": "чувство",
                                "target_lang_sense_clarification": "эмоция"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "sensation",
                                "target_lang_sense_clarification": "internal feeling"
                            },
                            {
                                "target_lang_word": "feeling",
                                "target_lang_sense_clarification": "emotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "493d6255-5e25-3753-9b99-c0bafd5721fb",
                    "sense_definition": "Een zintuiglijke waarneming of fysiek gevoel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de verdoving kwam de <w>sensatie</w> in zijn vingertoppen langzaam terug.",
                            "target_lang_translations": {
                                "ru": "После анестезии <w>чувствительность</w> в кончиках его пальцев медленно возвращалась.",
                                "en": "After the anesthesia, the <w>sensation</w> in his fingertips slowly returned."
                            }
                        },
                        {
                            "text": "De <w>sensatie</w> van koud water op een hete dag kan erg verfrissend zijn.",
                            "target_lang_translations": {
                                "ru": "<w>Ощущение</w> холодной воды в жаркий день может быть очень освежающим.",
                                "en": "The <w>sensation</w> of cold water on a hot day can be very refreshing."
                            }
                        }
                    ],
                    "synonyms": [
                        "gevoel",
                        "prikkel"
                    ],
                    "antonyms": [
                        "gevoelloosheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое восприятие или способность чувствовать.",
                        "en": "A sensory perception or physical feeling."

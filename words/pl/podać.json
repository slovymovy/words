    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fefcd6f6-a705-3809-aad7-3198c88fbc3d",
                    "sense_definition": "Wręczyć lub przekazać komuś przedmiot, używając rąk",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transfer_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy możesz mi <w>podać</w> ten talerz leżący na stole?",
                            "target_lang_translations": {
                                "ru": "Можешь <w>подать</w> мне ту тарелку, что на столе?"
                            }
                        },
                        {
                            "text": "Kolega <w>podał</w> mi klucze do samochodu, abym mógł prowadzić.",
                            "target_lang_translations": {
                                "ru": "Коллега <w>подал</w> мне ключи от машины, чтобы я мог вести."
                            }
                        }
                    ],
                    "synonyms": [
                        "wręczyć",
                        "dać",
                        "przekazać"
                    ],
                    "antonyms": [
                        "odebrać",
                        "wziąć"
                    ],
                    "common_phrases": [
                        "podać rękę",
                        "podać sól",
                        "podać coś komuś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вручить или передать кому-то предмет руками"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подать",
                                "target_lang_sense_clarification": "вручить"
                            },
                            {
                                "target_lang_word": "передать",
                                "target_lang_sense_clarification": "из рук в руки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf0c560a-aba4-3759-be21-b7fdd527ea64",
                    "sense_definition": "Zaserwować posiłek; postawić jedzenie lub napoje na stole",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transfer_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner <w>podał</w> nam zupę pomidorową i świeże pieczywo.",
                            "target_lang_translations": {
                                "ru": "Официант <w>подал</w> нам томатный суп и свежий хлеб."
                            }
                        },
                        {
                            "text": "Za chwilę <w>podamy</w> deser oraz kawę dla wszystkich gości.",
                            "target_lang_translations": {
                                "ru": "Через минуту мы <w>подадим</w> десерт и кофе для всех гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaserwować"
                    ],
                    "common_phrases": [
                        "podać do stołu",
                        "podać obiad",
                        "podać kolację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подать еду или напитки на стол"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подать",
                                "target_lang_sense_clarification": "на стол, сервировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50f0a7b1-50fc-3c7b-b6a0-0a9c213d4c78",
                    "sense_definition": "Przekazać informację, ogłosić coś lub udostępnić dane do wiadomości",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transfer_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzecznik prasowy <w>podał</w> oficjalne wyniki wyborów prezydenckich.",
                            "target_lang_translations": {
                                "ru": "Пресс-секретарь <w>объявил</w> официальные результаты президентских выборов."
                            }
                        },
                        {
                            "text": "Musisz <w>podać</w> swój dokładny adres zamieszkania w tym formularzu.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>указать</w> свой точный адрес проживания в этой форме."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogłosić",
                        "oznajmić",
                        "zakomunikować"
                    ],
                    "antonyms": [
                        "zataić",
                        "ukryć"
                    ],
                    "common_phrases": [
                        "podać do wiadomości",
                        "podać przykład",
                        "podać nazwisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщить информацию, объявить что-то или предоставить данные"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообщить",
                                "target_lang_sense_clarification": "информацию"
                            },
                            {
                                "target_lang_word": "назвать",
                                "target_lang_sense_clarification": "имя, адрес"
                            },
                            {
                                "target_lang_word": "объявить",
                                "target_lang_sense_clarification": "результаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0555d2d-4942-36ff-b05e-bc17e9238c67",
                    "sense_definition": "Przekazać piłkę innemu zawodnikowi ze swojej drużyny w trakcie gry sportowej",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napastnik <w>podał</w> piłkę do skrzydłowego, który strzelił gola.",
                            "target_lang_translations": {
                                "ru": "Нападающий <w>передал</w> мяч вингеру, который забил гол."
                            }
                        },
                        {
                            "text": "Nie wiem, komu mam <w>podać</w>, ponieważ wszyscy są kryci przez obrońców.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, кому <w>пасовать</w>, так как все закрыты защитниками."
                            }
                        }
                    ],
                    "synonyms": [
                        "zagrać"
                    ],
                    "common_phrases": [
                        "podać piłkę",
                        "podać do kolegi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать мяч другому игроку своей команды"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передать",
                                "target_lang_sense_clarification": "мяч"
                            },
                            {
                                "target_lang_word": "пасовать",
                                "target_lang_sense_clarification": "делать передачу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3d99039-d927-38d1-bb1d-31621eaeb024",
                    "sense_definition": "Zaaplikować choremu lekarstwo lub inną substancję medyczną",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pielęgniarka szybko <w>podała</w> pacjentowi silny środek przeciwbólowy.",
                            "target_lang_translations": {
                                "ru": "Медсестра быстро <w>дала</w> пациенту сильное обезболивающее."
                            }
                        },
                        {
                            "text": "Lekarz zdecydował, by <w>podać</w> antybiotyk bezpośrednio dożylnie.",
                            "target_lang_translations": {
                                "ru": "Врач решил <w>ввести</w> антибиотик непосредственно внутривенно."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaaplikować"
                    ],
                    "common_phrases": [
                        "podać lek",
                        "podać zastrzyk",
                        "podać kroplówkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать больному лекарство или ввести медицинский препарат"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дать",
                                "target_lang_sense_clarification": "лекарство"
                            },
                            {
                                "target_lang_word": "ввести",
                                "target_lang_sense_clarification": "препарат, инъекцию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "557fabe7-7540-3337-afa8-e8ddb5f844d1",
                    "sense_definition": "Skierować sprawę przeciwko komuś do sądu lub innej instytucji wymiaru sprawiedliwości",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli nie oddasz mi pieniędzy, będę musiał cię <w>podać</w> do sądu.",
                            "target_lang_translations": {
                                "ru": "Если ты не вернешь мне деньги, мне придется <w>подать</w> на тебя в суд."
                            }
                        },
                        {
                            "text": "Sąsiedzi <w>podali</w> go do sądu za ciągłe zakłócanie ciszy nocnej.",
                            "target_lang_translations": {
                                "ru": "Соседи <w>подали</w> на него в суд за постоянное нарушение ночной тишины."
                            }
                        }
                    ],
                    "synonyms": [
                        "pozwać",
                        "zaskarżyć"
                    ],
                    "common_phrases": [
                        "podać do sądu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направить дело против кого-то в суд"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подать",
                                "target_lang_sense_clarification": "в суд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbcb37c4-3935-3155-9d69-8d3a72ae090d",
                    "sense_definition": "Przedstawić się fałszywie jako ktoś inny lub przypisać sobie nieprawdziwą rolę (używane z zaimkiem „się”)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna <w>podał</w> się za pracownika gazowni, aby wejść do mieszkania.",
                            "target_lang_translations": {
                                "ru": "Мужчина <w>выдал</w> себя за сотрудника газовой службы, чтобы войти в квартиру."
                            }
                        },
                        {
                            "text": "Oszust <w>podał</w> się za dalekiego krewnego ofiary, by wyłudzić pieniądze.",
                            "target_lang_translations": {
                                "ru": "Мошенник <w>выдал</w> себя за дальнего родственника жертвы, чтобы выманить деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "udawać"
                    ],
                    "common_phrases": [
                        "podać się za kogoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представиться кем-то другим, приписав себе ложную роль"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдать себя",
                                "target_lang_sense_clarification": "за кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1dc3257-c5d0-39ea-8d81-b97e88d65fdf",
                    "sense_definition": "Złożyć rezygnację z zajmowanego stanowiska lub pełnionej funkcji (używane z zaimkiem „się”)",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Minister <w>podał</w> się do dymisji po ujawnieniu afery korupcyjnej.",
                            "target_lang_translations": {
                                "ru": "Министр <w>подал</w> в отставку после раскрытия коррупционного скандала."
                            }
                        },
                        {
                            "text": "Cały zarząd spółki zapowiedział, że wkrótce <w>poda</w> się do dymisji.",
                            "target_lang_translations": {
                                "ru": "Все правление компании объявило, что вскоре <w>подаст</w> в отставку."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrezygnować",
                        "ustąpić"
                    ],
                    "common_phrases": [
                        "podać się do dymisji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально отказаться от должности"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подать",
                                "target_lang_sense_clarification": "в отставку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a04723b-884a-3b48-a4b7-01ced9eff688",
                    "sense_definition": "Być podobnym do kogoś z rodziny, odziedziczyć po kimś cechy (używane z zaimkiem „się”)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "reflexive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Syn bardzo <w>podał</w> się do ojca, ma ten sam charakter i wygląd.",
                            "target_lang_translations": {
                                "ru": "Сын очень <w>пошел</w> в отца, у него тот же характер и внешность."
                            }
                        },
                        {
                            "text": "Wszyscy w rodzinie mówią, że ona <w>podała</w> się do babci.",
                            "target_lang_translations": {
                                "ru": "Все в семье говорят, что она <w>пошла</w> в бабушку."
                            }
                        }
                    ],
                    "synonyms": [
                        "wdać się",
                        "upodobnić się"
                    ],
                    "common_phrases": [
                        "podać się do ojca",
                        "podać się do matki"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "regionalne (np. Częstochowa, Podhale)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть похожим на родственника, унаследовать черты"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пойти",
                                "target_lang_sense_clarification": "в кого-то (быть похожим)"
                            },
                            {
                                "target_lang_word": "удаться",
                                "target_lang_sense_clarification": "в кого-то"
                            }
                        ]
                    }
                }
            ]
        }

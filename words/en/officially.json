            "senses": [
                {
                    "sense_id": "06fda80d-e398-3392-85f9-00ec85b21618",
                    "sense_definition": "In a formal and public way, or according to official rules and regulations.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new city library was <w>officially</w> opened by the mayor yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера мэр <w>официально</w> открыл новую городскую библиотеку."
                            }
                        },
                        {
                            "text": "Although the couple has separated, they are not <w>officially</w> divorced yet.",
                            "target_lang_translations": {
                                "ru": "Хотя супруги разъехались, они ещё <w>официально</w> не разведены."
                            }

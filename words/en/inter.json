    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f087bd38-ba0b-3240-a58d-950989a68f14",
                    "sense_definition": "To place a dead body in a grave or tomb.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "burial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family gathered quietly to <w>inter</w> their grandfather in the old churchyard.",
                            "target_lang_translations": {
                                "ru": "Семья тихо собралась, чтобы <w>похоронить</w> дедушку на старом церковном кладбище."
                            }
                        },
                        {
                            "text": "It is customary to <w>inter</w> the remains shortly after the funeral service.",
                            "target_lang_translations": {
                                "ru": "Принято <w>предавать земле</w> останки вскоре после поминальной службы."
                            }
                        }
                    ],
                    "synonyms": [
                        "bury",
                        "entomb",
                        "inhume"
                    ],
                    "antonyms": [
                        "dig up",
                        "disinter",
                        "exhume",
                        "unearth"
                    ],
                    "common_phrases": [
                        "inter the body",
                        "inter the remains",
                        "inter in a grave"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать тело умершего в могилу или гробницу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хорони́ть",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "погреба́ть",
                                "target_lang_sense_clarification": "более формальное/книжное"
                            },
                            {
                                "target_lang_word": "предава́ть земле́",
                                "target_lang_sense_clarification": "торжественное выражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82c264e6-3f00-3803-9c34-7a18994b8c71",
                    "sense_definition": "To confine someone, as if in a prison.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captured soldiers were <w>interred</w> in a remote fortress for the duration of the war.",
                            "target_lang_translations": {
                                "ru": "Пленные солдаты были <w>заточены</w> в отдаленной крепости на время войны."
                            }
                        },
                        {
                            "text": "He felt <w>interred</w> by the strict rules of the institution.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя <w>погребённым</w> заживо строгими правилами этого учреждения."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern usage typically prefers 'intern' for confinement."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заключать кого-либо под стражу, лишать свободы (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заточа́ть",
                                "target_lang_sense_clarification": "помещать в заключение"
                            },
                            {
                                "target_lang_word": "заключа́ть в тюрьму́",
                                "target_lang_sense_clarification": "лишать свободы"
                            }
                        ]
                    }
                }
            ]
        },

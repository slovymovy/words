    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8fdd01ec-24cf-368c-8296-4cb86ec4dc94",
                    "sense_definition": "Человек, который легко поддается страху и не способен с ним бороться.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "coward_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты ведешь себя как <w>трус</w>, когда убегаешь от проблем.",
                            "target_lang_translations": {
                                "en": "You are behaving like a <w>coward</w> when you run away from problems."
                            }
                        },
                        {
                            "text": "Этот жалкий <w>трус</w> побоялся сказать правду в лицо.",
                            "target_lang_translations": {
                                "en": "That pathetic <w>coward</w> was afraid to tell the truth to my face."
                            }
                        }
                    ],
                    "synonyms": [
                        "трусишка",
                        "бояка",
                        "боягуз"
                    ],
                    "antonyms": [
                        "храбрец",
                        "смельчак",
                        "герой"
                    ],
                    "common_phrases": [
                        "жалкий трус",
                        "последний трус",
                        "не будь трусом"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who easily yields to fear and is unable to fight it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "coward",
                                "target_lang_sense_clarification": "a person lacking courage"
                            },
                            {
                                "target_lang_word": "chicken",
                                "target_lang_sense_clarification": "informal: a cowardly person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f58cc9e-d80c-3e5e-bb34-2e181cc6d14f",
                    "sense_definition": "Землетрясение (устаревшее значение).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "earthquake_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Древняя летопись описывает страшный <w>трус</w>, разрушивший стены крепости.",
                            "target_lang_translations": {
                                "en": "An ancient chronicle describes a terrible <w>earthquake</w> that destroyed the fortress walls."
                            }
                        },
                        {
                            "text": "Люди молились о спасении от глада, мора и <w>труса</w>.",
                            "target_lang_translations": {
                                "en": "People prayed for salvation from famine, pestilence, and <w>earthquake</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "землетрясение"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An earthquake (archaic term)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earthquake",
                                "target_lang_sense_clarification": "shaking of the ground"
                            },
                            {
                                "target_lang_word": "quake",
                                "target_lang_sense_clarification": "trembling of the earth"
                            }
                        ]
                    }
                },
                {

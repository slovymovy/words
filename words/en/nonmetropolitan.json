{
    "word_family": [
        "metropolis",
        "metropolitan"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5421b11f-d550-3216-804d-7c98b8932878",
                    "sense_definition": "Relating to areas that are not part of a large city or its surrounding suburbs; often rural.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government announced new funding to support healthcare in <w>nonmetropolitan</w> areas.",
                            "target_lang_translations": {
                                "ru": "Правительство объявило о новом финансировании для поддержки здравоохранения в <w>нестоличных</w> районах."
                            }
                        },
                        {
                            "text": "Many people are moving to a <w>nonmetropolitan</w> county to escape the high cost of city living.",
                            "target_lang_translations": {
                                "ru": "Многие люди переезжают в <w>провинциальный</w> округ, чтобы избежать высокой стоимости жизни в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "rural",
                        "provincial",
                        "nonurban"
                    ],
                    "antonyms": [
                        "metropolitan",
                        "urban"
                    ],
                    "common_phrases": [
                        "nonmetropolitan area",
                        "nonmetropolitan county",
                        "nonmetropolitan population"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к территориям, не являющимся частью крупного города или его пригородов; часто сельский или провинциальный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нестоличный",
                                "target_lang_sense_clarification": "относящийся к районам вне главных городов или мегаполисов"
                            },
                            {
                                "target_lang_word": "провинциальный",
                                "target_lang_sense_clarification": "свойственный провинции, удаленный от центра"
                            },
                            {
                                "target_lang_word": "внегородской",
                                "target_lang_sense_clarification": "находящийся за пределами городской черты"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9faa214b-13ca-3440-9746-a3fe40aeb196",
                    "sense_definition": "A person who does not live in a large city or metropolitan area.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The survey highlighted the different political priorities of metropolitans and <w>nonmetropolitans</w>.",
                            "target_lang_translations": {
                                "ru": "Опрос выявил различные политические приоритеты жителей мегаполисов и <w>жителей провинции</w>."
                            }
                        },
                        {
                            "text": "As a <w>nonmetropolitan</w>, he found the constant noise of the city difficult to handle.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>провинциалом</w>, он с трудом переносил постоянный городской шум."
                            }
                        }
                    ],
                    "synonyms": [
                        "country dweller"
                    ],
                    "antonyms": [
                        "metropolitan",
                        "city dweller"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который не живет в крупном городе или мегаполисе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "житель провинции",
                                "target_lang_sense_clarification": "человек, проживающий вне крупных центров"
                            },
                            {
                                "target_lang_word": "провинциал",
                                "target_lang_sense_clarification": "обозначение человека из провинции"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "a6fbfde9-bcac-3346-aed2-c255d89064a9",
                    "sense_definition": "Osoba narodowości czeskiej, obywatel Republiki Czeskiej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "person_nationality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy sąsiad jest <w>Czechem</w> i uczy mnie języka.",
                            "target_lang_translations": {
                                "ru": "Мой новый сосед — <w>чех</w>, и он учит меня языку."
                            }
                        },
                        {
                            "text": "Wszyscy <w>Czesi</w>, których poznałem, byli bardzo przyjaźni.",
                            "target_lang_translations": {
                                "ru": "Все <w>чехи</w>, с которыми я познакомился, были очень дружелюбны."
                            }
                        }
                    ],
                    "synonyms": [
                        "Pepik"
                    ],
                    "common_phrases": [
                        "typowy Czech",
                        "spotkać Czecha"
                    ],
                    "target_lang_definitions": {

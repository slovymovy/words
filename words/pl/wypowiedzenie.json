    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "15d19f3c-65e8-3730-8050-fd0cafd86345",
                    "sense_definition": "Oficjalne pismo lub oświadczenie informujące o rozwiązaniu umowy, najczęściej o pracę lub najmu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "employment_contract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracownik postanowił złożyć <w>wypowiedzenie</w>, ponieważ otrzymał lepszą ofertę pracy.",
                            "target_lang_translations": {
                                "ru": "Сотрудник решил подать <w>заявление об увольнении</w>, так как получил лучшее предложение о работе."
                            }
                        },
                        {
                            "text": "Szef wręczył mu <w>wypowiedzenie</w> umowy z zachowaniem trzymiesięcznego okresu.",
                            "target_lang_translations": {
                                "ru": "Начальник вручил ему <w>уведомление о расторжении</w> договора с соблюдением трехмесячного срока."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymówienie"
                    ],
                    "common_phrases": [
                        "złożyć wypowiedzenie",
                        "okres wypowiedzenia",
                        "wypowiedzenie umowy",
                        "wręczyć wypowiedzenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное письмо или заявление, информирующее о расторжении договора, чаще всего трудового или аренды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расторжение",
                                "target_lang_sense_clarification": "договора"
                            },
                            {
                                "target_lang_word": "заявление об увольнении",
                                "target_lang_sense_clarification": "документ от работника"
                            },
                            {
                                "target_lang_word": "уведомление об увольнении",
                                "target_lang_sense_clarification": "документ от работодателя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4547294-6d12-3e6b-96cf-2e3ab626a4eb",
                    "sense_definition": "Czas, który upływa od momentu złożenia oświadczenia o rozwiązaniu umowy do jej faktycznego wygaśnięcia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "employment_contract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obecnie jestem na <w>wypowiedzeniu</w>, więc mam więcej czasu na szukanie nowej posady.",
                            "target_lang_translations": {
                                "ru": "В данный момент я на <w>отработке</w>, поэтому у меня больше времени на поиск новой должности."
                            }
                        },
                        {
                            "text": "W trakcie <w>wypowiedzenia</w> pracownik może zostać zwolniony z obowiązku świadczenia pracy.",
                            "target_lang_translations": {
                                "ru": "В течение <w>срока уведомления</w> работник может быть освобожден от обязанности выполнять работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "być na wypowiedzeniu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane jako skrót od 'okres wypowiedzenia'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени с момента подачи заявления о расторжении договора до его фактического прекращения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отработка",
                                "target_lang_sense_clarification": "разг. период работы перед увольнением"
                            },
                            {
                                "target_lang_word": "срок уведомления",
                                "target_lang_sense_clarification": "официальный период"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afa8c7e7-89a2-3394-8f33-634ef7e6445d",
                    "sense_definition": "Rzeczownik odczasownikowy od słowa <w>wypowiedzieć</w>, oznaczający czynność oficjalnego ogłoszenia czegoś lub wymówienia słów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Atak nastąpił niespodziewanie, bez wcześniejszego <w>wypowiedzenia</w> wojny przez agresora.",
                            "target_lang_translations": {
                                "ru": "Атака произошла неожиданно, без предварительного <w>объявления</w> войны агрессором."
                            }
                        },
                        {
                            "text": "Wyraźne <w>wypowiedzenie</w> tego trudnego słowa sprawiło uczniowi kłopot.",
                            "target_lang_translations": {
                                "ru": "Четкое <w>произнесение</w> этого трудного слова вызвало у ученика затруднения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wypowiedzenie wojny",
                        "wypowiedzenie posłuszeństwa"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rzeczownik odczasownikowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отглагольное существительное, означающее действие официального объявления чего-либо или произнесения слов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объявление",
                                "target_lang_sense_clarification": "войны, перемирия"
                            },
                            {
                                "target_lang_word": "произнесение",
                                "target_lang_sense_clarification": "слов, звуков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a94aaab-39f0-3db6-ba0e-cace3ca6bba7",
                    "sense_definition": "Uporządkowany ciąg wyrazów przekazujący zrozumiałą informację; w gramatyce jednostka składniowa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każde <w>wypowiedzenie</w> w wypracowaniu powinno być logicznie powiązane z poprzednim.",
                            "target_lang_translations": {
                                "ru": "Каждое <w>высказывание</w> в сочинении должно быть логически связано с предыдущим."
                            }
                        },
                        {
                            "text": "Nauczyciel poprosił nas o wskazanie podmiotu i orzeczenia w tym <w>wypowiedzeniu</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил нас указать подлежащее и сказуемое в этом <w>предложении</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "budowa wypowiedzenia",
                        "wypowiedzenie złożone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упорядоченная последовательность слов, передающая понятную информацию; в грамматике синтаксическая единица."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высказывание",
                                "target_lang_sense_clarification": "лингвистический термин"
                            },
                            {
                                "target_lang_word": "предложение",
                                "target_lang_sense_clarification": "грамматическая единица"
                            }
                        ]
                    }
                }
            ]
        }

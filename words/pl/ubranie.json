            "senses": [
                {
                    "sense_id": "392e4ac1-c433-35f4-a115-e114bb002cc1",
                    "sense_definition": "Wszystko, co służy do okrycia ludzkiego ciała, chroniąc je przed zimnem lub służąc ozdobie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie możesz wyjść na dwór w mróz bez ciepłego <w>ubrania</w>.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь выйти на улицу в мороз без тёплой <w>одежды</w>."
                            }
                        },
                        {
                            "text": "Muszę spakować letnie <w>ubrania</w> na nasz wakacyjny wyjazd.",
                            "target_lang_translations": {
                                "ru": "Мне нужно упаковать летнюю <w>одежду</w> для нашей поездки в отпуск."
                            }
                        }
                    ],
                    "synonyms": [
                        "odzież",
                        "strój",
                        "ubiór",
                        "odzienie",
                        "okrycie",
                        "ciuch"
                    ],
                    "common_phrases": [
                        "ciepłe ubranie",
                        "ubranie robocze",
                        "sklep z ubraniami",
                        "zmiana ubrania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Всё, что надевают на тело, чтобы защитить его от холода или украсить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одежда",
                                "target_lang_sense_clarification": "общее название предметов гардероба"
                            },
                            {
                                "target_lang_word": "вещи",
                                "target_lang_sense_clarification": "разговорное, предметы одежды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01eb8994-00c8-3fdf-8986-988a389f48f5",
                    "sense_definition": "Męski elegancki strój składający się z marynarki i spodni z tego samego materiału.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_suit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek założył swoje najlepsze czarne <w>ubranie</w> na uroczystość.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка надел свой лучший чёрный <w>костюм</w> на торжество."
                            }
                        },
                        {
                            "text": "Pan młody miał na sobie eleganckie, szyte na miarę <w>ubranie</w>.",
                            "target_lang_translations": {
                                "ru": "На женихе был элегантный, сшитый на заказ <w>костюм</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "garnitur"
                    ],
                    "common_phrases": [
                        "ubranie ślubne",
                        "odświętne ubranie",
                        "czarne ubranie"

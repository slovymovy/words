                    "sense_id": "680a8539-03c3-3b10-9bd1-79000895c9f2",
                    "sense_definition": "Een reflexmatige, explosieve uitademing, vaak veroorzaakt door irritatie in de luchtwegen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft al dagen last van een droge <w>hoest</w>.",
                            "target_lang_translations": {
                                "ru": "Он уже несколько дней страдает от сухого <w>кашля</w>."
                            }
                        },
                        {
                            "text": "De dokter luisterde naar mijn longen en vroeg naar de <w>hoest</w>.",
                            "target_lang_translations": {
                                "ru": "Врач послушал мои легкие и спросил про <w>кашель</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kuch"
                    ],
                    "common_phrases": [
                        "een droge hoest",
                        "een zware hoest",
                        "vastzittende hoest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рефлекторный, резкий выдох, часто вызванный раздражением дыхательных путей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кашель",
                                "target_lang_sense_clarification": "симптом"

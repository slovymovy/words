    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a0d9932a-c412-3360-bce1-f0da046f0b6f",
                    "sense_definition": "The state of being protected from danger, risk, or injury.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He managed to reach the border and escape to <w>safety</w>.",
                            "target_lang_translations": {
                                "ru": "Ему удалось добраться до границы и уйти в <w>безопасное место</w>."
                            }
                        },
                        {
                            "text": "For your own <w>safety</w>, please remain seated while the vehicle is in motion.",
                            "target_lang_translations": {
                                "ru": "Ради вашей собственной <w>безопасности</w>, пожалуйста, оставайтесь на местах во время движения транспортного средства."
                            }
                        }
                    ],
                    "synonyms": [
                        "security",
                        "protection"
                    ],
                    "antonyms": [
                        "danger"
                    ],
                    "common_phrases": [
                        "safety first",
                        "health and safety",
                        "safety belt",
                        "in safety",
                        "public safety"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние защищенности от опасности, риска или травм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безопа́сность",
                                "target_lang_sense_clarification": "состояние"
                            },
                            {
                                "target_lang_word": "сохра́нность",
                                "target_lang_sense_clarification": "целостность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15a87314-9e0b-3683-b266-291dc6ce2ae0",
                    "sense_definition": "A mechanism on a weapon or machine designed to prevent accidental operation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be sure that the <w>safety</w> is set before handling the firearm.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что <w>предохранитель</w> включен, прежде чем брать в руки огнестрельное оружие."
                            }
                        },
                        {
                            "text": "He released the <w>safety</w> on the drill before starting the work.",
                            "target_lang_translations": {
                                "ru": "Он снял дрель с <w>предохранителя</w> перед началом работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "lock"
                    ],
                    "common_phrases": [
                        "safety catch",
                        "safety lock",
                        "release the safety"
                    ],
                    "target_lang_definitions": {
                        "ru": "Механизм на оружии или устройстве, предназначенный для предотвращения случайного срабатывания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предохрани́тель",
                                "target_lang_sense_clarification": "механизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79ca76d7-f7f4-39be-91d0-9a2f89f4f9ee",
                    "sense_definition": "A defensive player in American football positioned far behind the line of scrimmage to defend against long passes.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "football_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>safety</w> intercepted the ball near the end zone to save the game.",
                            "target_lang_translations": {
                                "ru": "<w>Сейфти</w> перехватил мяч возле зачетной зоны, чтобы спасти игру."
                            }
                        },
                        {
                            "text": "Our team needs a faster <w>safety</w> to cover their wide receivers.",
                            "target_lang_translations": {
                                "ru": "Нашей команде нужен более быстрый <w>сейфти</w>, чтобы прикрывать их уайд-ресиверов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "free safety",
                        "strong safety"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "American Football"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Игрок защиты в американском футболе, находящийся далеко позади линии схватки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "се́йфти",
                                "target_lang_sense_clarification": "позиция игрока"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e72e3d8-259c-38c0-8584-6423e1a040a8",
                    "sense_definition": "A scoring play in American football worth two points, awarded when a ball carrier is tackled in their own end zone.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "football_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The defense tackled the quarterback for a <w>safety</w> early in the first quarter.",
                            "target_lang_translations": {
                                "ru": "Защита захватила квотербека, заработав <w>сейфти</w> в начале первой четверти."
                            }
                        },
                        {
                            "text": "A <w>safety</w> is a rare way to score points compared to a touchdown.",
                            "target_lang_translations": {
                                "ru": "<w>Сейфти</w> — это редкий способ набора очков по сравнению с тачдауном."
                            }
                        }
                    ],
                    "common_phrases": [
                        "score a safety"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "American Football"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Результативное действие в американском футболе, приносящее два очка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "се́йфти",
                                "target_lang_sense_clarification": "очки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d5ba0198-d139-3cd6-9b8e-7409568d0829",
                    "sense_definition": "To secure a mechanical component or weapon to prevent accidental operation or detachment.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic <w>safetied</w> the bolts with wire to ensure they would not vibrate loose.",
                            "target_lang_translations": {
                                "ru": "Механик <w>законтрил</w> болты проволокой, чтобы они не разболтались от вибрации."
                            }
                        },
                        {
                            "text": "He <w>safetied</w> his weapon immediately after the training exercise was finished.",
                            "target_lang_translations": {
                                "ru": "Он <w>поставил</w> оружие <w>на предохранитель</w> сразу после окончания тренировочного упражнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "secure"
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "86cae014-762a-3082-8476-a86fc0a1ff2e",
                    "sense_definition": "Met veel mensen, veel verkeer of veel activiteiten; niet rustig.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "busy_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is gevaarlijk om deze <w>drukke</w> straat over te steken zonder goed uit te kijken.",
                            "target_lang_translations": {
                                "ru": "Опасно переходить эту <w>оживлённую</w> улицу, не осмотревшись хорошенько."
                            }
                        },
                        {
                            "text": "Op <w>drukke</w> dagen staan er lange rijen voor de kassa van de supermarkt.",
                            "target_lang_translations": {
                                "ru": "В <w>загруженные</w> дни у касс супермаркета стоят длинные очереди."
                            }
                        }
                    ],
                    "synonyms": [
                        "levendig",
                        "vol"
                    ],
                    "antonyms": [
                        "rustig",
                        "stil",
                        "verlaten"
                    ],
                    "common_phrases": [
                        "een drukke straat",
                        "een drukke dag",
                        "drukke spits"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризуется большим количеством людей, движения или активности; неспокойный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оживлённый",
                                "target_lang_sense_clarification": "о месте или улице"
                            },
                            {
                                "target_lang_word": "многолюдный",
                                "target_lang_sense_clarification": "с большим количеством людей"
                            },
                            {
                                "target_lang_word": "суматошный",
                                "target_lang_sense_clarification": "о времени или дне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85688e5e-32fb-3526-9aa9-9bf927090062",
                    "sense_definition": "Veel te doen hebbend; weinig tijd hebbend (van een persoon).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "busy_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan vanavond niet komen sporten, want ik ben erg <w>druk</w> met mijn huiswerk.",
                            "target_lang_translations": {
                                "ru": "Я не могу прийти на тренировку сегодня вечером, так как очень <w>занят</w> домашним заданием."
                            }
                        },
                        {
                            "text": "Mijn vader heeft het <w>druk</w> op zijn werk en komt laat thuis.",
                            "target_lang_translations": {
                                "ru": "У моего отца <w>много дел</w> на работе, и он поздно приходит домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezet"
                    ],
                    "antonyms": [
                        "vrij"
                    ],
                    "common_phrases": [
                        "het druk hebben",
                        "druk zijn met",
                        "zich druk maken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий много дел; не располагающий свободным временем (о человеке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "занятой",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "занят",
                                "target_lang_sense_clarification": "краткая форма, состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0c99c28-3d55-3214-a338-12164d4032da",
                    "sense_definition": "Veel tijd en energie vragend (van een taak, baan of periode).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "busy_task",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij heeft een <w>drukke</w> baan in het ziekenhuis en werkt vaak in het weekend.",
                            "target_lang_translations": {
                                "ru": "У неё <w>напряжённая</w> работа в больнице, и она часто работает по выходным."
                            }
                        },
                        {
                            "text": "We hebben een <w>drukke</w> week achter de rug met veel vergaderingen.",
                            "target_lang_translations": {
                                "ru": "У нас была <w>насыщенная</w> неделя с большим количеством совещаний."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwaar",
                        "intensief"
                    ],
                    "antonyms": [
                        "rustig",
                        "ontspannen"
                    ],
                    "common_phrases": [
                        "een drukke baan",
                        "een drukke agenda",
                        "een drukke periode"
                    ],
                    "target_lang_definitions": {

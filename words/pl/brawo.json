    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "33a171f1-9753-34a7-98ba-cdab1ec1bda6",
                    "sense_definition": "Okrzyk wyrażający entuzjastyczną pochwałę, uznanie lub zachwyt dla czyjegoś osiągnięcia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "approval_applause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Brawo</w>, udało ci się zdać ten trudny egzamin!",
                            "target_lang_translations": {
                                "ru": "<w>Браво</w> [Молодец], тебе удалось сдать этот трудный экзамен!"
                            }
                        },
                        {
                            "text": "Kiedy strzelił decydującego gola, wszyscy krzyknęli: „<w>Brawo</w>!”.",
                            "target_lang_translations": {
                                "ru": "Когда он забил решающий гол, все крикнули: «<w>Браво</w>!»."
                            }
                        }
                    ],
                    "synonyms": [
                        "świetnie",
                        "wspaniale",
                        "pięknie",
                        "wiwat"
                    ],
                    "common_phrases": [
                        "brawo ty",
                        "wielkie brawa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, выражающий восторженную похвалу, признание или восхищение чьим-либо достижением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "браво",
                                "target_lang_sense_clarification": "возглас одобрения"
                            },
                            {
                                "target_lang_word": "молодец",
                                "target_lang_sense_clarification": "похвала человеку"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fcd1296a-5e0b-391b-9c1f-32b2c419c391",
                    "sense_definition": "Oklaski, uderzanie w dłonie jako wyraz uznania i aprobaty; często używane w liczbie mnogiej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "approval_applause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Publiczność nagrodziła artystę gromkimi <w>brawami</w> po zakończeniu spektaklu.",
                            "target_lang_translations": {
                                "ru": "Публика наградила артиста громкими <w>аплодисментами</w> после окончания спектакля."
                            }
                        },
                        {
                            "text": "Wszyscy wstali z miejsc, aby bić <w>brawo</w> zwycięzcy konkursu.",
                            "target_lang_translations": {
                                "ru": "Все встали с мест, чтобы <w>поаплодировать</w> победителю конкурса."
                            }
                        }
                    ],
                    "synonyms": [
                        "oklaski",
                        "aplauz",
                        "owacje",
                        "poklask"
                    ],
                    "common_phrases": [
                        "bić brawo",
                        "burza braw",
                        "gromkie brawa"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zwykle w liczbie mnogiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аплодисменты, хлопанье в ладоши в знак признания и одобрения; часто используется во множественном числе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аплодисменты",
                                "target_lang_sense_clarification": "хлопанье в ладоши"
                            },
                            {
                                "target_lang_word": "овации",
                                "target_lang_sense_clarification": "бурные аплодисменты"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "haast",
        "haasten",
        "haastig",
        "overhaasten",
        "overhaasting"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1cf00d17-3cbf-3698-9cbb-51f5733cf7e7",
                    "sense_definition": "Gedaan of gebeurd met te veel haast, vaak zonder goed na te denken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_too_fast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was zo <w>overhaast</w> dat hij zijn schooltas vergat mee te nemen.",
                            "target_lang_translations": {
                                "ru": "Он так <w>спешил</w>, что забыл взять с собой школьный портфель."
                            }
                        },
                        {
                            "text": "Neem geen <w>overhaaste</w> beslissing waar je later spijt van krijgt.",
                            "target_lang_translations": {
                                "ru": "Не принимай <w>поспешного</w> решения, о котором позже пожалеешь."
                            }
                        }
                    ],
                    "synonyms": [
                        "overijld",
                        "ondoordacht",
                        "haastig"
                    ],
                    "antonyms": [
                        "weloverwogen",
                        "doordacht",
                        "kalm"
                    ],
                    "common_phrases": [
                        "een overhaaste beslissing",
                        "overhaaste conclusies",
                        "overhaaste reactie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный или произошедший слишком быстро, часто без должного размышления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поспешный",
                                "target_lang_sense_clarification": "сделанный в спешке"
                            },
                            {
                                "target_lang_word": "опрометчивый",
                                "target_lang_sense_clarification": "необдуманный, рискованный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "34426df5-5deb-343f-ba40-d0cea0616aec",
                    "sense_definition": "Op een te haastige manier, zonder voldoende voorbereiding of nadenken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_too_fast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vertrok <w>overhaast</w> toen hij het slechte nieuws hoorde.",
                            "target_lang_translations": {
                                "ru": "Он <w>поспешно</w> уехал, когда услышал плохие новости."
                            }
                        },
                        {
                            "text": "We moeten dit project niet <w>overhaast</w> afronden.",
                            "target_lang_translations": {
                                "ru": "Мы не должны завершать этот проект <w>поспешно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "overijld",
                        "haastig",
                        "abrupt"
                    ],
                    "antonyms": [
                        "rustig",
                        "weloverwogen"
                    ],
                    "common_phrases": [
                        "niet overhaast handelen",
                        "iets overhaast doen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слишком быстро, без достаточной подготовки или размышлений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поспешно",
                                "target_lang_sense_clarification": "быстро, торопливо"
                            },
                            {
                                "target_lang_word": "опрометчиво",
                                "target_lang_sense_clarification": "необдуманно"
                            },
                            {
                                "target_lang_word": "впопыхах",
                                "target_lang_sense_clarification": "в суматохе"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e3d63670-b7de-3f88-848b-e6885bdbae3d",
                    "sense_definition": "Voltooid deelwoord van <w>overhaasten</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft zich <w>overhaast</w> om op tijd bij de vergadering te zijn.",
                            "target_lang_translations": {
                                "ru": "Он <w>поторопился</w>, чтобы успеть на собрание вовремя."
                            }
                        },
                        {
                            "text": "Heb je je niet <w>overhaast</w> met die dure aankoop?",
                            "target_lang_translations": {
                                "ru": "Ты не <w>поспешил</w> с этой дорогой покупкой?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich overhaast hebben"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие прошедшего времени от глагола 'overhaasten' (торопиться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поторопился",
                                "target_lang_sense_clarification": "совершил действие слишком быстро"
                            },
                            {
                                "target_lang_word": "поспешил",
                                "target_lang_sense_clarification": "сделал что-то раньше времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1445a58d-4890-384c-9619-9113f60c43fa",
                    "sense_definition": "Enkelvoud tegenwoordige tijd van <w>overhaasten</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>overhaast</w> me liever niet bij dit belangrijke werk.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю не <w>торопиться</w> с этой важной работой."
                            }
                        },
                        {
                            "text": "<w>Overhaast</w> je niet, we hebben nog genoeg tijd.",
                            "target_lang_translations": {
                                "ru": "Не <w>торопись</w>, у нас еще достаточно времени."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich overhaasten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единственное число настоящего времени глагола 'overhaasten' (торопиться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тороплюсь",
                                "target_lang_sense_clarification": "1-е лицо ед. ч."
                            },
                            {
                                "target_lang_word": "торопись",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

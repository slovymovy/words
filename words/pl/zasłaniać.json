                {
                    "sense_id": "b6e9f320-e265-32e9-a8ee-6b403afdb29b",
                    "sense_definition": "Czynić coś niewidocznym lub ograniczać dostęp światła poprzez umieszczenie przed tym przeszkody.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "covering_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wysoki budynek <w>zasłania</w> nam słońce przez większą część dnia.",
                            "target_lang_translations": {
                                "ru": "Высокое здание <w>заслоняет</w> нам солнце большую часть дня."
                            }
                        },
                        {
                            "text": "Chłopiec <w>zasłaniał</w> usta dłonią, żeby nie było widać, że się śmieje.",
                            "target_lang_translations": {
                                "ru": "Мальчик <w>закрывал</w> рот ладонью, чтобы не было видно, что он смеется."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakrywać",
                        "przysłaniać",
                        "ukrywać"
                    ],
                    "antonyms": [
                        "odsłaniać",
                        "odkrywać",
                        "ukazywać"
                    ],
                    "common_phrases": [
                        "zasłaniać okna",
                        "zasłaniać widok",
                        "zasłaniać twarz",
                        "zasłaniać słońce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо невидимым или преграждать доступ света, помещая перед объектом препятствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заслонять",
                                "target_lang_sense_clarification": "преграждать вид или свет"
                            },
                            {
                                "target_lang_word": "закрывать",
                                "target_lang_sense_clarification": "делать невидимым, прикрывать частью тела"
                            },
                            {
                                "target_lang_word": "загораживать",
                                "target_lang_sense_clarification": "создавать преграду для обзора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9d1fba1-1c3e-3612-b14e-87d65a68554b",
                    "sense_definition": "Chronić własną osobę przed uderzeniem, wzrokiem innych lub niebezpieczeństwem, umieszczając coś przed sobą; używane z zaimkiem zwrotnym „się”.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "covering_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna <w>zasłaniał się</w> teczką, biegnąc w ulewnym deszczu.",
                            "target_lang_translations": {
                                "ru": "Мужчина <w>прикрывался</w> портфелем, убегая под проливным дождем."
                            }
                        },
                        {
                            "text": "Podczas walki zawodnik szczelnie <w>zasłaniał się</w> gardą przed ciosami.",
                            "target_lang_translations": {
                                "ru": "Во время боя спортсмен плотно <w>закрывался</w> гардой от ударов."
                            }
                        }
                    ],
                    "synonyms": [
                        "osłaniać się",
                        "chronić się",
                        "kryć się"
                    ],
                    "antonyms": [
                        "odsłaniać się",
                        "wychylać się"
                    ],
                    "common_phrases": [
                        "zasłaniać się rękami",
                        "zasłaniać się przed ciosem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защищать себя от удара, чужих взглядов или опасности, помещая что-либо перед собой (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикрываться",
                                "target_lang_sense_clarification": "использовать предмет для защиты"
                            },
                            {
                                "target_lang_word": "закрываться",
                                "target_lang_sense_clarification": "защищать себя руками или предметом"
                            },
                            {
                                "target_lang_word": "заслоняться",
                                "target_lang_sense_clarification": "ограждать себя от чего-либо"
                            }
                        ]
                    }
                }
            ]

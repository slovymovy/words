    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f23d5def-6c8b-3bca-a2c4-2b735527f29d",
                    "sense_definition": "Przedmiot składający się z laski i rozpinanej czaszy z nieprzemakalnego materiału, służący do ochrony przed deszczem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chyba będzie padać, więc na wszelki wypadek weź <w>parasol</w>.",
                            "target_lang_translations": {
                                "ru": "Кажется, будет дождь, так что на всякий случай возьми <w>зонт</w>."
                            }
                        },
                        {
                            "text": "Kiedy zaczęło mocno kropić, wszyscy na przystanku otworzyli swoje <w>parasole</w>.",
                            "target_lang_translations": {
                                "ru": "Когда начало сильно накрапывать, все на остановке раскрыли свои <w>зонты</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "parasol przeciwdeszczowy",
                        "gw. (Górny Śląsk) paryzol"
                    ],
                    "common_phrases": [
                        "rozłożyć parasol",
                        "złożyć parasol",
                        "iść pod parasolem",
                        "mokry parasol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, состоящий из трости и раскладного купола из непромокаемого материала, служащий для защиты от дождя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зонт",
                                "target_lang_sense_clarification": "стандартное название"
                            },
                            {
                                "target_lang_word": "зонтик",
                                "target_lang_sense_clarification": "разговорное или уменьшительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdb38048-d7a0-3229-883e-17c6681e2454",
                    "sense_definition": "Przedmiot o konstrukcji podobnej do parasola przeciwdeszczowego, zwykle większy, służący do ochrony przed słońcem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słońce świeciło tak mocno, że musieliśmy usiąść pod tym <w>parasolem</w>.",
                            "target_lang_translations": {
                                "ru": "Солнце светило так сильно, что нам пришлось сесть под этим <w>зонтом</w>."
                            }
                        },
                        {
                            "text": "W ogrodzie kawiarni stał duży, biały <w>parasol</w>, dający przyjemny cień.",
                            "target_lang_translations": {
                                "ru": "В саду кафе стоял большой белый <w>зонт</w>, дающий приятную тень."
                            }
                        }
                    ],
                    "synonyms": [
                        "parasol przeciwsłoneczny"
                    ],
                    "common_phrases": [
                        "parasol ogrodowy",
                        "parasol plażowy",
                        "siedzieć pod parasolem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет конструкции, похожей на зонт от дождя, обычно большего размера, служащий для защиты от солнца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зонт",
                                "target_lang_sense_clarification": "пляжный или садовый"
                            },
                            {
                                "target_lang_word": "тент",
                                "target_lang_sense_clarification": "навес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f58a8e0-a435-3c83-ac34-e3f218a5840e",
                    "sense_definition": "Regionalne, potoczne określenie mieszkańca lewego brzegu Wisły (szczególnie Sandomierza), używane przez mieszkańców Tarnobrzega.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miejscowi czasem żartobliwie nazywają przybysza zza rzeki <w>parasolem</w>.",
                            "target_lang_translations": {
                                "ru": "Местные иногда в шутку называют пришельца из-за реки <w>зонтом</w>."
                            }
                        },
                        {
                            "text": "To określenie jest znane tylko lokalnie, nikt inny nie zrozumie, o co chodzi z tym <w>parasolem</w>.",
                            "target_lang_translations": {
                                "ru": "Это название известно только локально, никто другой не поймет, в чем дело с этим <w>зонтом</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Tarnobrzeg"
                        },
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное, разговорное название жителя левого берега Вислы (особенно Сандомира), используемое жителями Тарнобжега."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зонт",
                                "target_lang_sense_clarification": "прозвище"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "получение",
        "получатель",
        "получка"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "594c6380-f8d8-3464-81ea-62939050409c",
                    "sense_definition": "Принимать, брать что-либо, что дают, присылают, вручают или доставляют.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "receive_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я очень люблю <получать> подарки на день рождения.",
                            "target_lang_translations": {
                                "en": "I really love <receiving> birthday presents."
                            }
                        },
                        {
                            "text": "Мы каждую неделю <получаем> письма от бабушки.",
                            "target_lang_translations": {
                                "en": "We <receive> letters from grandmother every week."
                            }
                        }
                    ],
                    "synonyms": [
                        "принимать"
                    ],
                    "antonyms": [
                        "посылать",
                        "передавать",
                        "отправлять"
                    ],
                    "common_phrases": [
                        "получать письмо",
                        "получать подарок",
                        "получать посылку",
                        "получать приглашение"
                    ],
                    "target_lang_definitions": {
                        "en": "To accept or take something that is given, sent, or delivered."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "receive",
                                "target_lang_sense_clarification": "general / formal"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "common / informal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06fc74b2-cc25-39d4-bccd-f305d4eb300a",
                    "sense_definition": "Зарабатывать деньги за выполненную работу или получать регулярные выплаты.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "earn_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат <получает> хорошую зарплату на заводе.",
                            "target_lang_translations": {
                                "en": "My brother <gets> a good salary at the factory."
                            }
                        },
                        {
                            "text": "Студенты в нашем университете <получают> стипендию.",
                            "target_lang_translations": {
                                "en": "Students at our university <receive> a scholarship."
                            }
                        }
                    ],
                    "synonyms": [
                        "зарабатывать"
                    ],
                    "common_phrases": [
                        "получать зарплату",
                        "получать деньги",
                        "получать пенсию",
                        "хорошо получать"
                    ],
                    "target_lang_definitions": {
                        "en": "To earn money for work done or receive regular payments."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "receive",
                                "target_lang_sense_clarification": "salary, pension, scholarship"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "paid, salary"
                            },
                            {
                                "target_lang_word": "earn",
                                "target_lang_sense_clarification": "money for work"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c78d9537-e84d-3f1c-bd0a-b3cf741ee9af",
                    "sense_definition": "Приобретать, добывать что-либо желаемое или необходимое (знания, опыт, ощущения).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "receive_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой школе дети <получают> отличное образование.",
                            "target_lang_translations": {
                                "en": "Children <receive> an excellent education in this school."
                            }
                        },
                        {
                            "text": "От своей работы он <получает> огромное удовольствие.",
                            "target_lang_translations": {
                                "en": "He <derives> huge pleasure from his work."
                            }
                        }
                    ],
                    "common_phrases": [
                        "получать образование",
                        "получать опыт",
                        "получать удовольствие",
                        "получать информацию"
                    ],
                    "target_lang_definitions": {
                        "en": "To acquire or obtain something desired or necessary (knowledge, experience, feelings)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "education, pleasure"
                            },
                            {
                                "target_lang_word": "gain",
                                "target_lang_sense_clarification": "experience, knowledge"
                            },
                            {
                                "target_lang_word": "derive",
                                "target_lang_sense_clarification": "pleasure, benefit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89241e98-4734-30be-b5aa-db577f658830",
                    "sense_definition": "Добывать или производить вещество с помощью обработки или химической реакции.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "produce_technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из нефти на заводе <получают> бензин и керосин.",
                            "target_lang_translations": {
                                "en": "Gasoline and kerosene are <obtained> from oil at the plant."
                            }
                        },
                        {
                            "text": "Ученые <получают> новые данные в ходе эксперимента.",
                            "target_lang_translations": {
                                "en": "Scientists <obtain> new data during the experiment."
                            }
                        }
                    ],
                    "common_phrases": [
                        "получать энергию",
                        "получать вещество",
                        "получать результат"
                    ],
                    "target_lang_definitions": {
                        "en": "To extract or produce a substance through processing or chemical reaction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "obtain",
                                "target_lang_sense_clarification": "scientific / technical"
                            },
                            {
                                "target_lang_word": "derive",
                                "target_lang_sense_clarification": "from a source"
                            },
                            {
                                "target_lang_word": "produce",
                                "target_lang_sense_clarification": "manufacture"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74df06ef-a2e4-3b01-a521-84821cae5da9",
                    "sense_definition": "Подвергаться наказанию, выговору или побоям за какой-либо проступок.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "punishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В детстве я часто <получал> от родителей за плохие оценки.",
                            "target_lang_translations": {
                                "en": "As a child, I often <got it> from my parents for bad grades."
                            }
                        },
                        {
                            "text": "Если ты опоздаешь, то обязательно <получишь> от начальника.",
                            "target_lang_translations": {
                                "en": "If you are late, you will definitely <get it> from the boss."
                            }
                        }
                    ],
                    "common_phrases": [
                        "получать по шее",
                        "получать нагоняй"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be punished, scolded, or beaten for a misdeed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get it",
                                "target_lang_sense_clarification": "idiomatic / colloquial"
                            },
                            {
                                "target_lang_word": "catch it",
                                "target_lang_sense_clarification": "idiomatic"
                            }
                        ]
                    }
                }
            ]

            "senses": [
                {
                    "sense_id": "7cadfc21-f44b-3665-8511-1db7a43922db",
                    "sense_definition": "Dźwięk naśladujący odgłos wydawany podczas ziewania, wyrażający senność lub znudzenie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Maciek ziewnął szeroko i powiedział: <w>aaa</w>, ale jestem zmęczony.",
                            "target_lang_translations": {
                                "ru": "Мацек широко зевнул и сказал: <w>а-а-а</w>, как же я устал."
                            }
                        },
                        {
                            "text": "Wszyscy w klasie zaczęli ziewać: <w>aaa</w>, ta lekcja jest nudna.",
                            "target_lang_translations": {
                                "ru": "Все в классе начали зевать: <w>а-а-а</w>, этот урок скучный."
                            }
                        }
                    ],
                    "synonyms": [
                        "aa"
                    ],
                    "common_phrases": [
                        "głośne ziewnięcie aaa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, имитирующий зевание, выражающий сонливость или скуку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а-а-а",
                                "target_lang_sense_clarification": "звук при зевании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8b3688e-45c2-32cb-bfcd-780bcb3d1358",
                    "sense_definition": "Okrzyk wyrażający nagły strach, ból lub przerażenie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Aaa</w>! Ratunku, w kuchni jest wielki pająk!",
                            "target_lang_translations": {
                                "ru": "<w>А-а-а</w>! Помогите, на кухне огромный паук!"
                            }
                        },
                        {
                            "text": "Dziecko krzyknęło <w>aaa</w>, gdy przewróciło się na chodnik.",
                            "target_lang_translations": {
                                "ru": "Ребенок закричал <w>а-а-а</w>, когда упал на тротуар."
                            }
                        }
                    ],
                    "synonyms": [
                        "aa"
                    ],
                    "common_phrases": [
                        "krzyczeć głośno aaa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, выражающий внезапный страх, боль или ужас."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а-а-а",
                                "target_lang_sense_clarification": "крик страха или боли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5be7700-b1c2-3da8-9e2d-8f3d3055e141",
                    "sense_definition": "Wyraz nagłego zrozumienia sytuacji, przypomnienia sobie o czymś lub zaskoczenia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Aaa</w>, teraz już rozumiem, o co ci chodziło.",
                            "target_lang_translations": {
                                "ru": "<w>А-а</w>, теперь я понимаю, что ты имел в виду."
                            }
                        },
                        {
                            "text": "<w>Aaa</w>, to ty dzwoniłeś do mnie wczoraj wieczorem!",
                            "target_lang_translations": {
                                "ru": "<w>А-а</w>, так это ты звонил мне вчера вечером!"
                            }
                        }
                    ],
                    "synonyms": [
                        "aa"
                    ],
                    "common_phrases": [
                        "aaa rozumiem",
                        "aaa to tak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение внезапного понимания ситуации, воспоминания о чем-либо или удивления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а-а",
                                "target_lang_sense_clarification": "возглас понимания"
                            },
                            {
                                "target_lang_word": "а-а-а",
                                "target_lang_sense_clarification": "протяжный возглас догадки"
                            }

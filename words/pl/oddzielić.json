    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2c9a2426-9742-359d-abb8-00865a9276de",
                    "sense_definition": "Sprawić, że jeden element lub część przestaje być połączona z resztą; ustanowić granicę między rzeczami lub osobami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz dokładnie <oddzielić> żółtko od białka, aby ubić pianę.",
                            "target_lang_translations": {
                                "ru": "Ты должен тщательно <w>отделить</w> желток от белка, чтобы взбить пену."
                            }
                        },
                        {
                            "text": "Wysoki mur miał <oddzielić> strefę przemysłową od osiedla mieszkaniowego.",
                            "target_lang_translations": {
                                "ru": "Высокая стена должна была <w>отделить</w> промышленную зону от жилого массива."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozdzielić",
                        "odłączyć",
                        "odseparować"
                    ],
                    "antonyms": [
                        "połączyć",
                        "złączyć",
                        "scalić"
                    ],
                    "common_phrases": [
                        "oddzielić ziarno od plew",
                        "oddzielić grubą kreską",
                        "oddzielić żółtko od białka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы один элемент или часть перестали быть соединены с остальными; установить границу между вещами или людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отделить",
                                "target_lang_sense_clarification": "отсоединить часть от целого"
                            },
                            {
                                "target_lang_word": "разделить",
                                "target_lang_sense_clarification": "установить границу, разграничить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c7fbac2-2a6d-3305-aab6-8d8a28639236",
                    "sense_definition": "Odłączyć się od grupy lub całości; stać się osobnym bytem (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wycieczki w lesie nie wolno <oddzielić> się od grupy.",
                            "target_lang_translations": {
                                "ru": "Во время похода в лесу нельзя <w>отделяться</w> от группы."
                            }
                        },
                        {
                            "text": "Olej w butelce może <oddzielić> się od wody, jeśli przestaniesz mieszać.",
                            "target_lang_translations": {
                                "ru": "Масло в бутылке может <w>отделиться</w> от воды, если перестанешь мешать."
                            }
                        }
                    ],
                    "synonyms": [
                        "odłączyć się",
                        "odseparować się"
                    ],
                    "antonyms": [
                        "przyłączyć się",
                        "zjednoczyć się"
                    ],
                    "common_phrases": [
                        "oddzielić się od grupy",
                        "oddzielić się od stada",
                        "oddzielić się od tła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсоединиться от группы или целого; стать отдельным объектом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отделиться",
                                "target_lang_sense_clarification": "обособиться, отойти от группы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
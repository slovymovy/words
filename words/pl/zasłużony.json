    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "57d298d7-cd3b-39f3-94b6-35df479fa326",
                    "sense_definition": "Taki, na który ktoś zapracował i który mu się słusznie należy (np. nagroda, kara, odpoczynek).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "deserved_thing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po całym tygodniu ciężkiej pracy udał się na <w>zasłużony</w> odpoczynek.",
                            "target_lang_translations": {
                                "ru": "После целой недели тяжелой работы он отправился на <w>заслуженный</w> отдых."
                            }
                        },
                        {
                            "text": "Drużyna otrzymała <w>zasłużone</w> brawa za swoją wspaniałą grę w finale.",
                            "target_lang_translations": {
                                "ru": "Команда получила <w>заслуженные</w> аплодисменты за свою великолепную игру в финале."
                            }
                        }
                    ],
                    "synonyms": [
                        "należny",
                        "sprawiedliwy"
                    ],
                    "antonyms": [
                        "niezasłużony"
                    ],
                    "common_phrases": [
                        "zasłużony odpoczynek",
                        "zasłużona kara",
                        "zasłużona nagroda",
                        "zasłużony urlop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который кто-то заработал и который полагается ему по праву (например, награда, наказание, отдых)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заслуженный",
                                "target_lang_sense_clarification": "справедливый, заработанный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95d0a48c-b350-3603-ad02-e79310582988",
                    "sense_definition": "Taki, który ma duże osiągnięcia i cieszy się z tego powodu szacunkiem oraz uznaniem.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "meritorious_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek był bardzo <w>zasłużonym</w> lekarzem i pomógł wielu ludziom.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был очень <w>заслуженным</w> врачом и помог многим людям."
                            }
                        },
                        {
                            "text": "Prezydent wręczył medale najbardziej <w>zasłużonym</w> obywatelom naszego kraju.",
                            "target_lang_translations": {
                                "ru": "Президент вручил медали наиболее <w>заслуженным</w> гражданам нашей страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "ceniony",
                        "szanowany",
                        "wybitny"
                    ],
                    "common_phrases": [
                        "zasłużony obywatel",
                        "zasłużony działacz",
                        "zasłużony dla kraju",
                        "zasłużony artysta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большие достижения и пользующийся в связи с этим уважением и признанием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заслуженный",
                                "target_lang_sense_clarification": "почётный, уважаемый (о человеке)"
                            }
                        ]
                    }
                }
            ]
        },
        {

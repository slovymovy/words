                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to bundle",
                                "target_lang_sense_clarification": "to tie or roll up a number of things together"
                            },
                            {
                                "target_lang_word": "to bind",
                                "target_lang_sense_clarification": "to tie or fasten tightly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вязать в пучки",
                                "target_lang_sense_clarification": "собирать в связки"
                            },
                            {
                                "target_lang_word": "связывать",
                                "target_lang_sense_clarification": "соединять вместе"
                            }
                        ]
                    }
                },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f039d7e8-fa3b-3c90-9e7d-b03cc0a392b2",
                    "sense_definition": "Исполнить мелодию голосом, часто тихо, не в полный голос или без слов.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "singing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она любила тихо <напеть> знакомую мелодию, занимаясь домашними делами.",
                            "target_lang_translations": {
                                "en": "She liked to quietly <hum> a familiar melody while doing housework."
                            }
                        },
                        {
                            "text": "Попробуй <напеть> мне этот мотив, и я, возможно, вспомню название песни.",
                            "target_lang_translations": {
                                "en": "Try to <hum> that tune to me, and I might remember the name of the song."
                            }
                        }
                    ],
                    "synonyms": [
                        "намурлыкать",
                        "пропеть"
                    ],
                    "common_phrases": [
                        "напеть мелодию",
                        "напеть мотив",
                        "напеть песню"
                    ],
                    "target_lang_definitions": {
                        "en": "To sing a melody quietly, often without words or in a low voice."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hum",
                                "target_lang_sense_clarification": "sing with closed lips"
                            },
                            {
                                "target_lang_word": "croon",
                                "target_lang_sense_clarification": "sing softly or sentimentally"
                            },
                            {
                                "target_lang_word": "sing softly",
                                "target_lang_sense_clarification": "sing in a low voice"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9a5c304-d514-3972-9ba4-c8209e69f7ba",
                    "sense_definition": "Записать своё пение на звуковой носитель (диск, пластинку, плёнку).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "recording",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Известный артист успел <напеть> новый альбом всего за две недели.",
                            "target_lang_translations": {
                                "en": "The famous artist managed to <record> a new album in just two weeks."
                            }
                        },
                        {
                            "text": "Мы планируем <напеть> несколько песен в профессиональной студии.",
                            "target_lang_translations": {
                                "en": "We plan to <record> several songs in a professional studio."
                            }
                        }
                    ],
                    "synonyms": [
                        "записать"
                    ],
                    "common_phrases": [
                        "напеть пластинку",
                        "напеть диск",
                        "напеть альбом"
                    ],
                    "target_lang_definitions": {
                        "en": "To record one's singing onto an audio medium (disk, record, tape)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "record",
                                "target_lang_sense_clarification": "capture singing on media"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63f17f02-15ee-3a3d-b1ca-e3ba793e3f3b",
                    "sense_definition": "Спеть большое количество песен или музыкальных произведений.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "singing_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За этот долгий вечер мы с друзьями <напели> множество старых романсов.",
                            "target_lang_translations": {
                                "en": "During that long evening, my friends and I <sang> a multitude of old romances."
                            }
                        },
                        {
                            "text": "Певец <напел> столько хитов за свою карьеру, что их трудно сосчитать.",
                            "target_lang_translations": {
                                "en": "The singer <sang> so many hits during his career that they are hard to count."
                            }
                        }
                    ],
                    "synonyms": [
                        "спеть"
                    ],
                    "common_phrases": [
                        "напеть много песен",
                        "напеть кучу всего"
                    ],
                    "target_lang_definitions": {
                        "en": "To sing a large quantity of songs or musical pieces."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sing",
                                "target_lang_sense_clarification": "perform songs"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa638bd9-3a8c-3d69-91e2-ca555d12e3d7",
                    "sense_definition": "Сообщить ложные сведения, наговорить на кого-то, наябедничать или убедить в чём-то ложном.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "deception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кто-то уже успел <напеть> начальнику про моё небольшое опоздание.",
                            "target_lang_translations": {
                                "en": "Someone has already managed to <snitch> to the boss about my slight lateness."
                            }
                        },
                        {
                            "text": "Не верь всему, что могут <напеть> завистники за твоей спиной.",
                            "target_lang_translations": {
                                "en": "Don't believe everything that envious people might <whisper> behind your back."
                            }
                        }
                    ],
                    "synonyms": [
                        "наговори́ть",
                        "наплести"
                    ],
                    "common_phrases": [
                        "напеть в уши",
                        "напеть с три короба"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To report false information, slander someone, or convince someone of something untrue."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fill someone's head with lies",
                                "target_lang_sense_clarification": "tell lies to convince"
                            },
                            {
                                "target_lang_word": "snitch",
                                "target_lang_sense_clarification": "inform on someone"
                            },
                            {
                                "target_lang_word": "whisper",
                                "target_lang_sense_clarification": "spread rumors or lies"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
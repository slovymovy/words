    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "adfac618-5e10-36cb-b432-f97933f12929",
                    "sense_definition": "Привести в действие, заставить работать (механизм, прибор, устройство), обычно подсоединив к источнику энергии.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "activation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, <включи> свет, в комнате стало очень темно.",
                            "target_lang_translations": {
                                "en": "Please <turn on> the light, it has become very dark in the room."
                            }
                        },
                        {
                            "text": "Он вернулся домой и сразу <включил> телевизор, чтобы посмотреть новости.",
                            "target_lang_translations": {
                                "en": "He returned home and immediately <turned on> the TV to watch the news."
                            }
                        }
                    ],
                    "synonyms": [
                        "активи́ровать"
                    ],
                    "antonyms": [
                        "вы́ключить",
                        "отключить"
                    ],
                    "common_phrases": [
                        "включить свет",
                        "включить телевизор",
                        "включить компьютер",
                        "включить музыку"
                    ],
                    "target_lang_definitions": {
                        "en": "To activate or make a mechanism, device, or appliance work, usually by connecting it to a power source."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn on",
                                "target_lang_sense_clarification": "device, light"
                            },
                            {
                                "target_lang_word": "switch on",
                                "target_lang_sense_clarification": "device, light"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "caf42ecc-58ff-3759-8ac1-303c7eb84776",
                    "sense_definition": "Ввести, внести в состав, в число кого-либо или чего-либо; сделать частью группы или списка.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <включить> этот важный пункт в наш договор.",
                            "target_lang_translations": {
                                "en": "We decided to <include> this important clause in our contract."
                            }
                        },
                        {
                            "text": "Тренер планирует <включить> нового игрока в основной состав команды.",
                            "target_lang_translations": {
                                "en": "The coach plans to <include> the new player in the main team roster."
                            }
                        }
                    ],
                    "synonyms": [
                        "добавить"
                    ],
                    "antonyms": [
                        "исключить"
                    ],
                    "common_phrases": [
                        "включить в список",
                        "включить в состав",
                        "включить в программу",
                        "включить в счёт"
                    ],
                    "target_lang_definitions": {
                        "en": "To introduce or insert into a composition, list, or group; to make something a part of a whole."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "include",
                                "target_lang_sense_clarification": "in a list or group"
                            },
                            {
                                "target_lang_word": "incorporate",
                                "target_lang_sense_clarification": "into a larger whole"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
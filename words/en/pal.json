{
    "word_family": [
        "pally"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa709c8e-233f-3c81-99c3-67b38e21da30",
                    "sense_definition": "A close friend or companion.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "friendship_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Little Timmy is out playing with his <w>pals</w> in the park.",
                            "target_lang_translations": {
                                "ru": "Маленький Тимми играет со своими <w>приятелями</w> в парке."
                            }
                        },
                        {
                            "text": "He has been my best <w>pal</w> since we were in kindergarten together.",
                            "target_lang_translations": {
                                "ru": "Он был моим лучшим <w>другом</w> с тех пор, как мы вместе ходили в детский сад."
                            }
                        }
                    ],
                    "synonyms": [
                        "pal around"
                    ],
                    "common_phrases": [
                        "best pal",
                        "old pal",
                        "pen pal"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Близкий друг или товарищ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приятель",
                                "target_lang_sense_clarification": "товарищ, друг (разг.)"
                            },
                            {
                                "target_lang_word": "друг",
                                "target_lang_sense_clarification": "близкий человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1968d0a6-190f-3585-a670-d04e120a8677",
                    "sense_definition": "An informal term of address to a person, often used to show aggression, annoyance, or irony.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't you threaten me, <w>pal</w>, or I will call the police.",
                            "target_lang_translations": {
                                "ru": "Не смей мне угрожать, <w>приятель</w>, или я вызову полицию."
                            }
                        },
                        {
                            "text": "Listen <w>pal</w>, you need to move your car right now.",
                            "target_lang_translations": {
                                "ru": "Послушай, <w>парень</w>, тебе нужно сейчас же убрать машину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "listen pal",
                        "look pal",
                        "hey pal"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное обращение к человеку, часто используемое для выражения агрессии, раздражения или иронии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приятель",
                                "target_lang_sense_clarification": "ироничное или агрессивное обращение"
                            },
                            {
                                "target_lang_word": "парень",
                                "target_lang_sense_clarification": "фамильярное обращение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb4d408b-41ad-3603-a649-d680ec007fc5",
                    "sense_definition": "To spend time with someone as a friend; to associate socially.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "friendship_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent the whole summer <w>palling</w> around with his cousins by the lake.",
                            "target_lang_translations": {
                                "ru": "Он провел все лето, <w>тусуясь</w> со своими двоюродными братьями у озера."
                            }
                        },
                        {
                            "text": "The politician was criticized for <w>palling</w> with known criminals.",
                            "target_lang_translations": {
                                "ru": "Политика критиковали за то, что он <w>водился</w> с известными преступниками."
                            }
                        }
                    ],
                    "synonyms": [
                        "pal around"
                    ],
                    "common_phrases": [
                        "pal around",
                        "pal up"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить время с кем-то по-дружески; общаться в социальной обстановке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тусоваться",
                                "target_lang_sense_clarification": "проводить время вместе (разг.)"
                            },
                            {
                                "target_lang_word": "водиться",
                                "target_lang_sense_clarification": "общаться, иметь дело (часто с неодобрением)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bd9f70b5-1821-31cd-a1f8-ede5e356bfbf",
                    "sense_definition": "Публичное высказывание, речь или доклад перед собранием людей.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В своём <w>выступлении</w> докладчик осветил важные международные вопросы.",
                            "target_lang_translations": {
                                "en": "In his <w>speech</w>, the speaker covered important international issues."
                            }
                        },
                        {
                            "text": "Президент закончил своё <w>выступление</w> под громкие аплодисменты зала.",
                            "target_lang_translations": {
                                "en": "The president finished his <w>speech</w> to the loud applause of the hall."
                            }
                        }
                    ],
                    "synonyms": [
                        "речь",
                        "доклад"
                    ],
                    "common_phrases": [
                        "публичное выступление",
                        "выступление президента",
                        "готовить выступление",
                        "выступление с докладом"
                    ],
                    "target_lang_definitions": {
                        "en": "A public speech, address, or report delivered to an audience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speech",
                                "target_lang_sense_clarification": "A formal address delivered to an audience"
                            },
                            {
                                "target_lang_word": "statement",
                                "target_lang_sense_clarification": "A formal declaration of opinion or position"
                            },
                            {
                                "target_lang_word": "presentation",
                                "target_lang_sense_clarification": "A formal talk giving information or a report"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5eb848bb-8432-3d61-9c61-06ed3ac1e36f",
                    "sense_definition": "Исполнение художественного произведения, сценического или музыкального номера перед публикой.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зрители с восторгом встретили <w>выступление</w> известной рок-группы.",
                            "target_lang_translations": {
                                "en": "The audience enthusiastically welcomed the <w>performance</w> of the famous rock group."
                            }
                        },
                        {
                            "text": "Это было лучшее <w>выступление</w> наших фигуристов на чемпионате.",
                            "target_lang_translations": {
                                "en": "It was the best <w>performance</w> by our figure skaters at the championship."
                            }
                        }
                    ],
                    "synonyms": [
                        "перформанс"
                    ],
                    "common_phrases": [
                        "концертное выступление",
                        "живое выступление",
                        "выступление артиста",
                        "выступление на сцене"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of performing an artistic work, such as a concert or play, before an audience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "performance",
                                "target_lang_sense_clarification": "The execution of an artistic work or entertainment"
                            },
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "A public exhibition or entertainment event"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b7da0f9-07ee-3c3a-923d-8a2535b1b084",
                    "sense_definition": "Начало движения или похода (обычно о войсках).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Генерал отдал приказ о немедленном <w>выступлении</w> полка на позиции.",
                            "target_lang_translations": {
                                "en": "The general gave the order for the immediate <w>departure</w> of the regiment to the positions."
                            }
                        },
                        {
                            "text": "Внезапное <w>выступление</w> армии противника застало нас врасплох.",
                            "target_lang_translations": {
                                "en": "The sudden <w>advance</w> of the enemy army caught us by surprise."
                            }
                        }
                    ],
                    "synonyms": [
                        "выдвижение",
                        "выход",
                        "марш"
                    ],
                    "common_phrases": [
                        "выступление войск",
                        "приказ к выступлению",
                        "время выступления"
                    ],
                    "target_lang_definitions": {
                        "en": "The beginning of a movement or march, especially regarding military troops."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "departure",
                                "target_lang_sense_clarification": "The act of setting out or leaving"
                            },
                            {
                                "target_lang_word": "march",
                                "target_lang_sense_clarification": "The steady advance or movement of troops"
                            },
                            {
                                "target_lang_word": "advance",
                                "target_lang_sense_clarification": "Forward movement, especially of armed forces"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
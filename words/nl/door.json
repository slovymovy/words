                {
                    "sense_id": "0995fe16-21dc-3278-b414-5ad555887224",
                    "sense_definition": "Gedurende de hele periode; van het begin tot het einde (vaak als achterzetsel gebruikt).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "duration_complete",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen hebben de hele dag <w>door</w> buiten gespeeld.",
                            "target_lang_translations": {
                                "en": "The children played outside <w>all</w> day <w>long</w>."
                            }
                        },
                        {
                            "text": "Het hele jaar <w>door</w> kun je hier verse appels kopen.",
                            "target_lang_translations": {
                                "en": "You can buy fresh apples here <w>throughout</w> the year."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de hele dag door",
                        "het jaar door"
                    ],
                    "target_lang_definitions": {

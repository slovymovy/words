                {
                    "sense_id": "eee5311e-5a30-3bfd-8287-c824c715bf26",
                    "sense_definition": "Relating to plants or the nature of plant life.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "plant_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The compost heap was full of decaying <w>vegetal</w> matter.",
                            "target_lang_translations": {
                                "ru": "Компостная куча была полна гниющих <w>растительных</w> остатков."
                            }
                        },
                        {
                            "text": "Scientists studied the <w>vegetal</w> biodiversity found in the tropical rainforest.",
                            "target_lang_translations": {
                                "ru": "Ученые изучали <w>растительное</w> биоразнообразие, обнаруженное в тропических лесах."
                            }
                        }
                    ],
                    "synonyms": [
                        "botanical",
                        "vegetable",
                        "plant"
                    ],
                    "antonyms": [
                        "animal",
                        "mineral"
                    ],
                    "common_phrases": [
                        "vegetal matter",
                        "vegetal life",
                        "vegetal oil"
                    ],
                    "target_lang_definitions": {

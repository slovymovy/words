                            "text": "Can I bum a <w>smoke</w> from you?",
                            "target_lang_translations": {
                                "ru": "Могу я стрельнуть у тебя <w>сигарету</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "cig",
                        "ciggy",
                        "fag"
                    ],
                    "common_phrases": [

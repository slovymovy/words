    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c9916ac9-06e2-392f-9d90-826f7b3caa4f",
                    "sense_definition": "Een felle elektrische ontlading in de lucht die optreedt tijdens onweer, vaak zichtbaar als een lichtflits.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>bliksem</w> sloeg tijdens de storm in op het dak van de boerderij.",
                            "target_lang_translations": {
                                "ru": "Во время шторма <w>молния</w> ударила в крышу фермы."
                            }
                        },
                        {
                            "text": "Na de felle flits van de <w>bliksem</w> volgde direct een harde donderslag.",
                            "target_lang_translations": {
                                "ru": "Сразу за яркой вспышкой <w>молнии</w> последовал сильный удар грома."
                            }
                        }
                    ],
                    "synonyms": [
                        "weerlicht"
                    ],
                    "common_phrases": [
                        "donder en bliksem",
                        "inslag van de bliksem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Яркий электрический разряд в атмосфере, возникающий во время грозы, часто видимый как вспышка света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молния",
                                "target_lang_sense_clarification": "атмосферное явление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "245bfb76-e7d1-373b-84bc-2dbad8dc38fa",
                    "sense_definition": "Een uitroep om ontevredenheid, woede of verbazing uit te drukken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Bliksem</w>, ik ben mijn portemonnee vergeten mee te nemen!",
                            "target_lang_translations": {
                                "ru": "<w>Чёрт</w>, я забыл взять с собой кошелёк!"
                            }
                        },
                        {
                            "text": "Dat is me toch wat, <w>bliksem</w> nog aan toe.",
                            "target_lang_translations": {
                                "ru": "Ну и дела, <w>чёрт возьми</w>."
                            }
                        }
                    ],
                    "common_phrases": [

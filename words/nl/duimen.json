{
    "word_family": [
        "duim",
        "duimstok",
        "duimspijker",
        "duimkruid"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a196e14-e91d-3a61-96b3-87b6752f30ca",
                    "sense_definition": "Meervoud van <w>duim</w>",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mensen hebben twee <w>duimen</w> en acht vingers.",
                            "target_lang_translations": {
                                "ru": "У людей два <w>больших пальца</w> и восемь остальных пальцев."
                            }
                        },
                        {
                            "text": "De handschoenen waren te krap bij de <w>duimen</w>.",
                            "target_lang_translations": {
                                "ru": "Перчатки были слишком тесными в <w>больших пальцах</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова duim (большой палец)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "большие пальцы",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "159dae32-9342-3a19-9508-b0298d13e57b",
                    "sense_definition": "Hopen dat iets goed afloopt of dat iemand succes heeft",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "hope_luck",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal morgen voor je <w>duimen</w> tijdens het examen.",
                            "target_lang_translations": {
                                "ru": "Я буду <w>держать</w> за тебя <w>кулаки</w> завтра во время экзамена."
                            }
                        },
                        {
                            "text": "We <w>duimen</w> allemaal dat het mooi weer blijft.",
                            "target_lang_translations": {
                                "ru": "Мы все <w>держим кулаки</w>, чтобы погода оставалась хорошей."
                            }
                        }
                    ],
                    "synonyms": [
                        "hopen"
                    ],
                    "common_phrases": [
                        "voor iemand duimen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надеяться на хороший исход или удачу для кого-то"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "держать кулаки",
                                "target_lang_sense_clarification": "идиома: желать удачи"
                            },
                            {
                                "target_lang_word": "болеть",
                                "target_lang_sense_clarification": "переживать за кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edbcbf83-a75a-3138-880c-e1beb2983f5f",
                    "sense_definition": "Op de duim zuigen, meestal als gewoonte bij jonge kinderen",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel kleine kinderen <w>duimen</w> als ze moe zijn.",
                            "target_lang_translations": {
                                "ru": "Многие маленькие дети <w>сосут палец</w>, когда устали."
                            }
                        },
                        {
                            "text": "De baby lag rustig te <w>duimen</w> in de wieg.",
                            "target_lang_translations": {
                                "ru": "Младенец лежал в колыбели и спокойно <w>сосал палец</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "duimzuigen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосать большой палец (обычно о детях)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сосать палец",
                                "target_lang_sense_clarification": "привычка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74462e79-ec25-3c10-94e3-3298b8b7979d",
                    "sense_definition": "De duim omhoog houden langs de weg om een lift te vragen",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij stond uren te <w>duimen</w> langs de snelweg voordat er iemand stopte.",
                            "target_lang_translations": {
                                "ru": "Он часами стоял и <w>голосовал</w> у шоссе, прежде чем кто-то остановился."
                            }
                        },
                        {
                            "text": "In mijn studententijd heb ik door heel Europa <w>geduimd</w>.",
                            "target_lang_translations": {
                                "ru": "В студенческие годы я <w>путешествовал автостопом</w> по всей Европе."
                            }
                        }
                    ],
                    "synonyms": [
                        "liften"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поднимать большой палец вверх, чтобы поймать попутную машину"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голосовать",
                                "target_lang_sense_clarification": "на дороге"
                            },
                            {
                                "target_lang_word": "путешествовать автостопом",
                                "target_lang_sense_clarification": "способ передвижения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0d20a6b-14c8-3ea4-ab42-238f8470a546",
                    "sense_definition": "Met de duimen om elkaar heen draaien uit verveling of ongeduld",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zat maar wat te <w>duimen</w> terwijl hij op de uitslag wachtte.",
                            "target_lang_translations": {
                                "ru": "Он просто сидел и <w>бездельничал</w>, ожидая результатов."
                            }
                        },
                        {
                            "text": "In plaats van te werken, zat hij de hele middag te <w>duimen</w>.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы работать, он весь день <w>бил баклуши</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "duimendraaien",
                        "nietsdoen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крутить большими пальцами от скуки или нетерпения; бездельничать"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бездельничать",
                                "target_lang_sense_clarification": "ничего не делать"
                            },
                            {
                                "target_lang_word": "бить баклуши",
                                "target_lang_sense_clarification": "идиома"
                            },
                            {
                                "target_lang_word": "сидеть сложа руки",
                                "target_lang_sense_clarification": "бездействовать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d7630548-8376-3f03-ba41-8e85f0787e62",
                    "sense_definition": "Taki, który odbywa się w pozycji siedzącej lub wymaga długiego przebywania w tej pozycji (często o pracy lub trybie życia).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "lifestyle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarze ostrzegają, że <w>siedzący</w> tryb życia może prowadzić do wielu chorób.",
                            "target_lang_translations": {
                                "ru": "Врачи предупреждают, что <w>сидячий</w> образ жизни может привести ко многим болезням."
                            }
                        },
                        {
                            "text": "Jego nowa praca jest typowo <w>siedząca</w>, więc po godzinach stara się uprawiać sport.",
                            "target_lang_translations": {
                                "ru": "Его новая работа типично <w>сидячая</w>, поэтому после работы он старается заниматься спортом."
                            }
                        }
                    ],
                    "synonyms": [
                        "stacjonarny",
                        "biurowy"
                    ],
                    "antonyms": [
                        "stojący",
                        "fizyczny",
                        "ruchowy",
                        "aktywny"
                    ],
                    "common_phrases": [
                        "siedzący tryb życia",
                        "praca siedząca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который происходит в сидячем положении или требует длительного пребывания в этом положении (часто о работе или образе жизни)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидячий",
                                "target_lang_sense_clarification": "о работе или образе жизни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ad2d448-8dd6-3d17-be27-39505d342014",
                    "sense_definition": "Przeznaczony do tego, aby na nim usiąść; służący do siedzenia (zwykle o miejscach w transporcie lub na widowni).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "places",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W pociągu był duży tłok i nie znaleźliśmy żadnego miejsca <w>siedzącego</w>.",
                            "target_lang_translations": {
                                "ru": "В поезде была большая давка, и мы не нашли ни одного <w>сидячего</w> места."
                            }
                        },
                        {
                            "text": "Sala kinowa posiada trzysta miejsc <w>siedzących</w> dla widzów.",
                            "target_lang_translations": {
                                "ru": "В кинозале есть триста <w>сидячих</w> мест для зрителей."
                            }
                        }
                    ],
                    "antonyms": [
                        "stojący"
                    ],
                    "common_phrases": [
                        "miejsce siedzące",
                        "miejsca siedzące"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для сидения (обычно о местах в транспорте или зрительном зале)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидячий",
                                "target_lang_sense_clarification": "о местах для сидения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bbccebc2-5f88-3b82-a3fb-680c321c171d",
                    "sense_definition": "Imiesłów przymiotnikowy czynny od czasownika <w>siedzieć</w>; określający kogoś lub coś, co w danej chwili znajduje się w pozycji siedzącej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna <w>siedzący</w> na ławce czytał poranną gazetę.",
                            "target_lang_translations": {
                                "ru": "Мужчина, <w>сидящий</w> на скамейке, читал утреннюю газету."
                            }
                        },
                        {
                            "text": "Podeszła do dziewczynki <w>siedzącej</w> samotnie przy stole.",
                            "target_lang_translations": {
                                "ru": "Она подошла к девочке, <w>сидящей</w> в одиночестве за столом."
                            }
                        }
                    ],
                    "antonyms": [
                        "stojący",
                        "leżący",
                        "idący"
                    ],
                    "common_phrases": [
                        "siedzący obok",
                        "siedzący na ławce"
                    ],
                    "traits": [
                        {

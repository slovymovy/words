    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8a78ac77-6315-30cf-971c-c2c4e894446e",
                    "sense_definition": "Wyjmować lub wydobywać coś, co znajduje się wewnątrz czegoś innego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "extraction_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <wyciągnąć> klucze z torebki, żeby otworzyć drzwi.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <достать> ключи из сумочки, чтобы открыть дверь."
                            }
                        },
                        {
                            "text": "Nauczyciel kazał uczniom <wyciągnąć> podręczniki i zeszyty.",
                            "target_lang_translations": {
                                "ru": "Учитель велел ученикам <достать> учебники и тетради."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyjmować"
                    ],
                    "antonyms": [
                        "wkładać",
                        "chować"
                    ],
                    "common_phrases": [
                        "wyciągać z kieszeni",
                        "wyciągać rękę",
                        "wyciągać wnioski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доставать или вынимать что-то, находящееся внутри чего-то другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доставать",
                                "target_lang_sense_clarification": "брать, извлекать откуда-либо"
                            },
                            {
                                "target_lang_word": "вытаскивать",
                                "target_lang_sense_clarification": "извлекать наружу, часто с усилием"
                            },
                            {
                                "target_lang_word": "извлекать",
                                "target_lang_sense_clarification": "более формальный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7dfab57-d4f3-3c89-8a5f-54ca4974e045",
                    "sense_definition": "Prostować kończyny lub wysuwać je w określonym kierunku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "body_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pasażer <wyciąga> nogi, żeby trochę odpocząć w podróży.",
                            "target_lang_translations": {
                                "ru": "Пассажир <вытягивает> ноги, чтобы немного отдохнуть в дороге."
                            }
                        },
                        {
                            "text": "Dziecko z uśmiechem <wyciąga> rączki do swojej mamy.",
                            "target_lang_translations": {
                                "ru": "Ребенок с улыбкой <протягивает> ручки к своей маме."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozkładać",
                        "wyjmować"
                    ],
                    "antonyms": [
                        "kulić",
                        "cofać"
                    ],
                    "common_phrases": [
                        "wyciągać nogi",
                        "wyciągać szyję",
                        "wyciągać ramiona"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпрямлять конечности или выдвигать их в определенном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытягивать",
                                "target_lang_sense_clarification": "выпрямлять ноги, шею"
                            },
                            {
                                "target_lang_word": "протягивать",
                                "target_lang_sense_clarification": "простирать руки к кому-либо"
                            },
                            {
                                "target_lang_word": "тянуть",
                                "target_lang_sense_clarification": "направлять конечности к чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9110b87-7513-3af1-a92f-bb7f9c6d2ffa",
                    "sense_definition": "Kłaść się wygodnie w pozycji wyprostowanej, aby odpocząć (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "body_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ciężkim dniu pracy Marek <wyciągnął> się na kanapie.",
                            "target_lang_translations": {
                                "ru": "После тяжелого рабочего дня Марек <вытянулся> на диване."
                            }
                        },
                        {
                            "text": "Kot leniwie <wyciąga> się na słońcu w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Кот лениво <вытягивается> на солнце в саду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyciągać się na łóżku",
                        "wyciągać się na trawie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удобно ложиться, выпрямившись во весь рост, чтобы отдохнуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытягиваться",
                                "target_lang_sense_clarification": "ложиться, выпрямив тело"
                            },
                            {
                                "target_lang_word": "растягиваться",
                                "target_lang_sense_clarification": "ложиться свободно и небрежно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5617ac1e-5911-3d72-b690-e1507a41269c",
                    "sense_definition": "Skłaniać kogoś do ujawnienia informacji, których nie chce powiedzieć; wydobywać informacje.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "extraction_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policjant próbuje <wyciągnąć> od świadka szczegóły zdarzenia.",
                            "target_lang_translations": {
                                "ru": "Полицейский пытается <вытянуть> из свидетеля подробности происшествия."
                            }
                        },
                        {
                            "text": "Nie uda ci się <wyciągnąć> ze mnie tej tajemnicy.",
                            "target_lang_translations": {
                                "ru": "Тебе не удастся <вытянуть> из меня эту тайну."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyciągać prawdę",
                        "wyciągać informacje",
                        "wyciągać zeznania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-то раскрыть информацию; добывать сведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытягивать",
                                "target_lang_sense_clarification": "добиваться сведений с трудом"
                            },
                            {
                                "target_lang_word": "выуживать",
                                "target_lang_sense_clarification": "хитростью узнавать информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae95b6f8-10f5-3c74-886e-264f2d117693",
                    "sense_definition": "Tracić pierwotny kształt i stawać się dłuższym lub luźniejszym na skutek używania (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "elongation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tani materiał szybko się <wyciąga> w praniu i traci fason.",
                            "target_lang_translations": {
                                "ru": "Дешевый материал быстро <растягивается> при стирке и теряет форму."
                            }
                        },
                        {
                            "text": "Po przejechaniu wielu kilometrów łańcuch rowerowy <wyciągnął> się.",
                            "target_lang_translations": {
                                "ru": "После многих километров езды велосипедная цепь <растянулась>."
                            }
                        }
                    ],
                    "antonyms": [
                        "kurczyć się"
                    ],
                    "common_phrases": [
                        "sweter się wyciąga",
                        "rękawy się wyciągają"
                    ],
                    "target_lang_definitions": {
                        "ru": "Терять первоначальную форму, становясь длиннее или свободнее в результате носки или стирки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растягиваться",
                                "target_lang_sense_clarification": "терять форму, увеличиваться в размере"
                            },
                            {
                                "target_lang_word": "вытягиваться",
                                "target_lang_sense_clarification": "становиться длиннее, деформироваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fe34710-4b6c-3049-b731-cb1266cdd0fd",
                    "sense_definition": "Rozciągać coś, sprawiając, że staje się dłuższe, często niszcząc przy tym kształt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "elongation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wieszaj mokrego swetra w ten sposób, bo go <wyciągniesz>.",
                            "target_lang_translations": {
                                "ru": "Не вешай мокрый свитер так, иначе ты его <растянешь>."
                            }
                        },
                        {
                            "text": "Dziecko nieświadomie <wyciąga> gumkę w spodniach.",
                            "target_lang_translations": {
                                "ru": "Ребенок неосознанно <растягивает> резинку на штанах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyciągać sweter",
                        "wyciągać strunę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растягивать что-то, делая длиннее и часто портя форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растягивать",
                                "target_lang_sense_clarification": "делать шире или длиннее, деформировать"
                            },
                            {
                                "target_lang_word": "вытягивать",
                                "target_lang_sense_clarification": "удлинять натяжением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aada10c2-0497-370b-bca6-b5ceef40fcb3",
                    "sense_definition": "Szybko urosnąć, stać się wysokim (zwykle o dzieciach lub młodzieży; używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój syn niesamowicie się <wyciągnął> przez te wakacje.",
                            "target_lang_translations": {
                                "ru": "Твой сын невероятно <вытянулся> за эти каникулы."
                            }
                        },
                        {
                            "text": "Przez ostatni rok bardzo się <wyciągnęła> i jest teraz wyższa ode mnie.",
                            "target_lang_translations": {
                                "ru": "За последний год она очень <вытянулась> и теперь выше меня."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chłopak się wyciągnął"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро вырасти, стать высоким (обычно о детях или подростках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытягиваться",
                                "target_lang_sense_clarification": "быстро расти в высоту"
                            },
                            {
                                "target_lang_word": "вымахать",
                                "target_lang_sense_clarification": "разг. сильно вырасти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54c890ff-df3d-357c-92ff-37dde5084ad9",
                    "sense_definition": "Osiągać określoną prędkość, wynik lub zarabiać pewną sumę pieniędzy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "achievement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój stary samochód ledwo <wyciąga> sto kilometrów na godzinę.",
                            "target_lang_translations": {
                                "ru": "Моя старая машина едва <выжимает> сто километров в час."
                            }
                        },
                        {
                            "text": "W nowej pracy on <wyciąga> całkiem niezłe pieniądze co miesiąc.",
                            "target_lang_translations": {
                                "ru": "На новой работе он <зарабатывает> весьма неплохие деньги каждый месяц."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyciągać setkę",
                        "wyciągać średnią",
                        "wyciągać pensję"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts regarding speed or money"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Развивать определенную скорость или зарабатывать определенную сумму денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выжимать",
                                "target_lang_sense_clarification": "о скорости: достигать максимума"
                            },
                            {
                                "target_lang_word": "зарабатывать",
                                "target_lang_sense_clarification": "получать доход"
                            },
                            {
                                "target_lang_word": "получать",
                                "target_lang_sense_clarification": "о деньгах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a428faea-fff0-3f7c-bb26-b1e902d19960",
                    "sense_definition": "Stawiać elementy konstrukcyjne budowli, np. mury lub dach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Murarze zaczęli już <wyciągać> ściany parteru.",
                            "target_lang_translations": {
                                "ru": "Каменщики уже начали <возводить> стены первого этажа."
                            }
                        },
                        {
                            "text": "Musimy <wyciągnąć> komin przed nadejściem zimy.",
                            "target_lang_translations": {
                                "ru": "Мы должны <вывести> дымоход до наступления зимы."
                            }
                        }
                    ],
                    "synonyms": [
                        "stawiać"
                    ],
                    "common_phrases": [
                        "wyciągać mury",
                        "wyciągać fundamenty"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "often used in construction jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возводить строительные элементы здания, например, стены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возводить",
                                "target_lang_sense_clarification": "строить стены, здания"
                            },
                            {
                                "target_lang_word": "выводить",
                                "target_lang_sense_clarification": "проф. строить, поднимать стены"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
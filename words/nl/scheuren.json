    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "19ee1bfe-eb73-3832-a05e-551815fe1fc8",
                    "sense_definition": "Iets kapotmaken door er hard aan te trekken zodat het in stukken breekt",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_tearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <scheurde> de brief in kleine stukjes voordat hij hem weggooide.",
                            "target_lang_translations": {
                                "ru": "Он <порвал> письмо на мелкие кусочки, прежде чем выбросить его."
                            }
                        },
                        {
                            "text": "Pas op dat je je nieuwe jas niet <scheurt> aan die scherpe spijker.",
                            "target_lang_translations": {
                                "ru": "Осторожно, не <порви> свою новую куртку об этот острый гвоздь."
                            }
                        }
                    ],
                    "synonyms": [
                        "verscheuren",
                        "stuktrekken"
                    ],
                    "antonyms": [
                        "lijmen",
                        "plakken",
                        "herstellen"
                    ],
                    "common_phrases": [
                        "in stukken scheuren",
                        "kleren scheuren",
                        "papier scheuren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Порвать или разорвать что-либо на части, приложив силу"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рвать",
                                "target_lang_sense_clarification": "повреждать, разделять на части"
                            },
                            {
                                "target_lang_word": "разрывать",
                                "target_lang_sense_clarification": "рвать на куски"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3828a0c9-6aca-3d56-8060-6a1b7e20e321",
                    "sense_definition": "Kapotgaan door spanning of druk waarbij er een barst of opening ontstaat",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_tearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude muur <scheurde> van boven naar beneden door de verzakking.",
                            "target_lang_translations": {
                                "ru": "Старая стена <треснула> сверху донизу из-за проседания грунта."
                            }
                        },
                        {
                            "text": "Mijn broek is <gescheurd> toen ik over het hek klom.",
                            "target_lang_translations": {
                                "ru": "Мои штаны <порвались>, когда я перелезал через забор."
                            }
                        }
                    ],
                    "synonyms": [
                        "barsten",
                        "splijten",
                        "knappen"
                    ],
                    "common_phrases": [
                        "uit zijn voegen scheuren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Порваться или треснуть из-за натяжения или давления"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трескаться",
                                "target_lang_sense_clarification": "о твердых предметах, стенах"
                            },
                            {
                                "target_lang_word": "рваться",
                                "target_lang_sense_clarification": "о ткани, одежде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a6d4834-84e5-377d-bda2-efef53ec0fc2",
                    "sense_definition": "Erg hard en wild rijden met een voertuig",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jongeren <scheurden> met hoge snelheid door de smalle straten.",
                            "target_lang_translations": {
                                "ru": "Молодые люди <неслись> на высокой скорости по узким улицам."
                            }
                        },
                        {
                            "text": "We moeten flink <scheuren> om nog op tijd op het vliegveld te komen.",
                            "target_lang_translations": {
                                "ru": "Нам нужно сильно <гнать>, чтобы успеть в аэропорт вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "racen",
                        "sjezen",
                        "crossen"
                    ],
                    "common_phrases": [
                        "door de bocht scheuren",
                        "over de weg scheuren"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informeel gebruik"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ехать очень быстро и агрессивно на транспортном средстве"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнать",
                                "target_lang_sense_clarification": "быстро ехать (разг.)"
                            },
                            {
                                "target_lang_word": "нестись",
                                "target_lang_sense_clarification": "мчаться с большой скоростью"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "07b292b9-7bd9-3849-8e6c-5a4130b639ad",
                    "sense_definition": "meervoud van <w>scheur</w>",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de aardbeving zaten er grote <scheuren> in de muren van het huis.",
                            "target_lang_translations": {
                                "ru": "После землетрясения в стенах дома появились большие <трещины>."
                            }
                        },
                        {
                            "text": "De wegdek heeft veel <scheuren> en moet dringend gerepareerd worden.",
                            "target_lang_translations": {
                                "ru": "На дорожном покрытии много <трещин>, и его нужно срочно ремонтировать."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова \"трещина\" или \"разрыв\""
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трещины",
                                "target_lang_sense_clarification": "разломы в твердом материале"
                            },
                            {
                                "target_lang_word": "разрывы",
                                "target_lang_sense_clarification": "дыры в ткани или бумаге"
                            }
                        ]
                    }
                }
            ]
        }
    ]

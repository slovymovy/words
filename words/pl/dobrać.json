    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "539bb7f6-9a1d-33c0-ab9a-b5155e8ac17b",
                    "sense_definition": "Wybrać elementy tak, aby pasowały do siebie, tworzyły komplet lub harmonijną całość.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz starannie <dobrać> krawat do tej koszuli, żeby wszystko pasowało.",
                            "target_lang_translations": {
                                "ru": "Ты должен тщательно <подобрать> галстук к этой рубашке, чтобы всё подходило."
                            }
                        },
                        {
                            "text": "Autor potrafił idealnie <dobrać> słowa, aby opisać tę trudną sytuację.",
                            "target_lang_translations": {
                                "ru": "Автор умел идеально <подобрать> слова, чтобы описать эту сложную ситуацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopasować",
                        "dostosować",
                        "skompletować",
                        "wyselekcjonować"
                    ],
                    "common_phrases": [
                        "dobrać słowa",
                        "dobrać kolor",
                        "dobrać parę",
                        "dobrać odpowiednio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбрать элементы так, чтобы они подходили друг другу, составляли комплект или гармоничное целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подобрать",
                                "target_lang_sense_clarification": "найти подходящее, сочетающееся"
                            },
                            {
                                "target_lang_word": "выбрать",
                                "target_lang_sense_clarification": "отобрать из множества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5f773b7-d57f-39dc-832d-bd92f9fa6d51",
                    "sense_definition": "Wziąć dodatkową ilość czegoś (np. jedzenia) lub wyciągnąć dodatkowe elementy (np. karty w grze).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli jesteś głodny, możesz śmiało <dobrać> sobie jeszcze trochę ziemniaków.",
                            "target_lang_translations": {
                                "ru": "Если ты голоден, можешь смело <взять еще> немного картошки."
                            }
                        },
                        {
                            "text": "W tej turze gracz musi <dobrać> dwie karty ze stosu.",
                            "target_lang_translations": {
                                "ru": "В этом ходу игрок должен <добрать> две карты из колоды."
                            }
                        }
                    ],
                    "synonyms": [
                        "dołożyć",
                        "wziąć"
                    ],
                    "antonyms": [
                        "odłożyć"
                    ],
                    "common_phrases": [
                        "dobrać kartę",
                        "dobrać zupy",
                        "dobrać wody"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взять дополнительное количество чего-либо (например, еды) или вытянуть дополнительные элементы (например, карты)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добрать",
                                "target_lang_sense_clarification": "взять недостающее или карты"
                            },
                            {
                                "target_lang_word": "взять добавки",
                                "target_lang_sense_clarification": "о еде"
                            },
                            {
                                "target_lang_word": "доложить",
                                "target_lang_sense_clarification": "положить еще еды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29469564-e8f2-367d-b38d-2b447d3f0b10",
                    "sense_definition": "Uzyskać dostęp do czegoś zamkniętego, ukrytego lub chronionego (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodzieje próbowali <dobrać> się do sejfu ukrytego w ścianie.",
                            "target_lang_translations": {
                                "ru": "Воры пытались <добраться> до сейфа, спрятанного в стене."
                            }
                        },
                        {
                            "text": "Dzieci w końcu <dobrały> się do słodyczy schowanych na górnej półce.",
                            "target_lang_translations": {
                                "ru": "Дети наконец <добрались> до сладостей, спрятанных на верхней полке."
                            }
                        }
                    ],
                    "synonyms": [
                        "włamać się",
                        "dostać się"
                    ],
                    "common_phrases": [
                        "dobrać się do skóry",
                        "dobrać się do zapasów",
                        "dobrać się do miodu"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "z zaimkiem „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить доступ к чему-то закрытому, спрятанному или охраняемому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добраться",
                                "target_lang_sense_clarification": "получить доступ, проникнуть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f550d081-8b40-3191-be0d-c1e6892da051",
                    "sense_definition": "Zacząć zalecać się do kogoś w sposób bezpośredni i natarczywy, dążąc do kontaktu fizycznego (używane z zaimkiem „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na imprezie jakiś nieznajomy próbował <dobrać> się do mojej koleżanki.",
                            "target_lang_translations": {
                                "ru": "На вечеринке какой-то незнакомец пытался <приставать> к моей подруге."
                            }
                        },
                        {
                            "text": "Ona nie pozwoliła, by ktokolwiek <dobrał> się do niej bez jej zgody.",
                            "target_lang_translations": {
                                "ru": "Она не позволила, чтобы кто-либо <добрался> до нее без ее согласия."
                            }
                        }
                    ],
                    "synonyms": [
                        "zalecać się",
                        "przystawiać się"
                    ],
                    "common_phrases": [
                        "dobrać się do kogoś"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "z zaimkiem „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать настойчиво ухаживать или приставать к кому-то, стремясь к физическому контакту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приставать",
                                "target_lang_sense_clarification": "настойчиво ухаживать, лезть"
                            },
                            {
                                "target_lang_word": "добраться",

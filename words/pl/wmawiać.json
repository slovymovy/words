    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ab8728e6-ed03-3af3-80f6-31e43dbd0910",
                    "sense_definition": "Uporczywie przekonywać kogoś o czymś, co nie jest prawdą; próbować sprawić, by ktoś uwierzył w fałszywą informację.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "convincing_false",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie próbuj mi <w>wmawiać</w>, że to nie była twoja wina.",
                            "target_lang_translations": {
                                "ru": "Не пытайся <w>внушить</w> мне, что это была не твоя вина."
                            }
                        },
                        {
                            "text": "Sąsiedzi próbowali <w>wmawiać</w> wszystkim, że widzieli ducha w starym domu.",
                            "target_lang_translations": {
                                "ru": "Соседи пытались <w>уверить</w> всех, что видели призрака в старом доме."
                            }
                        },
                        {
                            "text": "Dlaczego chcesz <w>wmawiać</w> komuś kłamstwa zamiast powiedzieć prawdę?",
                            "target_lang_translations": {
                                "ru": "Почему ты хочешь <w>внушать</w> кому-то ложь вместо того, чтобы сказать правду?"
                            }
                        }
                    ],
                    "synonyms": [
                        "imputować",
                        "insynuować"
                    ],
                    "common_phrases": [
                        "wmawiać komuś kłamstwa",
                        "wmawiać komuś winę",
                        "wmawiać komuś dziecko",
                        "wmawiać komuś chorobę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво убеждать кого-то в чем-то, что не является правдой; пытаться заставить кого-то поверить в ложную информацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внушать",
                                "target_lang_sense_clarification": "заставлять поверить"
                            },
                            {
                                "target_lang_word": "уверять",
                                "target_lang_sense_clarification": "настойчиво утверждать ложное"
                            },
                            {
                                "target_lang_word": "приписывать",
                                "target_lang_sense_clarification": "навязывать (вину, поступок)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9634d46-0bda-3d5c-96ee-801dcf220c91",
                    "sense_definition": "Przekonywać samego siebie o słuszności czegoś, co jest nieprawdą lub jest mało prawdopodobne (używane z zaimkiem zwrotnym „sobie”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "convincing_false",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestań <w>wmawiać</w> sobie, że jesteś do niczego, bo to nieprawda.",
                            "target_lang_translations": {
                                "ru": "Перестань <w>внушать</w> себе, что ты никчемный, потому что это неправда."
                            }
                        },
                        {
                            "text": "Marek ciągle <w>wmawia</w> sobie choroby, mimo że jest całkowicie zdrowy.",
                            "target_lang_translations": {
                                "ru": "Марек постоянно <w>придумывает</w> себе болезни, хотя он совершенно здоров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wmawiać sobie coś",
                        "wmawiać sobie choroby",
                        "wmawiać sobie problemy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убеждать самого себя в правильности чего-то, что является неправдой или маловероятно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внушать себе",
                                "target_lang_sense_clarification": "убеждать себя"
                            },
                            {
                                "target_lang_word": "надумывать",
                                "target_lang_sense_clarification": "воображать несуществующее"
                            },
                            {
                                "target_lang_word": "придумывать",
                                "target_lang_sense_clarification": "выдумывать (например, болезни)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
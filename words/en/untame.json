    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4a6aab2e-0465-3367-9a7e-d12914bbdebd",
                    "sense_definition": "Not tame; remaining in a wild or natural state.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "wild_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The explorers gazed out at the vast and <w>untame</w> wilderness before them.",
                            "target_lang_translations": {
                                "ru": "Исследователи смотрели на огромную и <w>дикую</w> местность, простирающуюся перед ними."
                            }
                        },
                        {
                            "text": "She possessed an <w>untame</w> spirit that refused to follow traditional rules.",
                            "target_lang_translations": {
                                "ru": "Она обладала <w>неукротимым</w> духом, который отказывался следовать традиционным правилам."
                            }
                        }
                    ],
                    "synonyms": [
                        "wild",
                        "feral",
                        "undomesticated"
                    ],
                    "antonyms": [
                        "tame",
                        "domesticated"
                    ],
                    "common_phrases": [
                        "untame spirit",
                        "untame nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не прирученный; остающийся в диком или естественном состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дикий",
                                "target_lang_sense_clarification": "находящийся в первозданном состоянии"
                            },
                            {
                                "target_lang_word": "неукротимый",
                                "target_lang_sense_clarification": "о духе или характере"
                            },
                            {
                                "target_lang_word": "неприрученный",
                                "target_lang_sense_clarification": "не одомашненный"
                            },
                            {
                                "target_lang_word": "необузданный",
                                "target_lang_sense_clarification": "не поддающийся контролю"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "acff8e77-9fe9-315e-a439-2b4dc0e040a0",
                    "sense_definition": "To cause something to become wild again; to reverse the process of taming.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "make_wild",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The project aims to <w>untame</w> the river by removing dams and restoring natural flow.",
                            "target_lang_translations": {
                                "ru": "Проект направлен на то, чтобы <w>вернуть в естественное состояние</w> реку, убрав плотины и восстановив природное течение."
                            }
                        },
                        {
                            "text": "It is difficult to <w>untame</w> an animal that has relied on humans for survival.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>вернуть к дикой жизни</w> животное, которое полагалось на людей ради выживания."
                            }
                        }
                    ],
                    "synonyms": [
                        "rewild"
                    ],
                    "antonyms": [
                        "tame",
                        "domesticate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать снова диким; обратить вспять процесс приручения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вернуть в дикое состояние",
                                "target_lang_sense_clarification": "восстановить природные свойства"
                            },
                            {
                                "target_lang_word": "заставить одичать",
                                "target_lang_sense_clarification": "о животных"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0a5545e2-333e-3e96-8e4a-21c96e4b45d5",
                    "sense_definition": "Taki, za który trzeba zapłacić; nie darmowy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "costing_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten parking w centrum miasta jest <w>płatny</w> w dni robocze.",
                            "target_lang_translations": {
                                "ru": "Эта парковка в центре города <w>платная</w> в рабочие дни."
                            }
                        },
                        {
                            "text": "Czy wstęp do tego muzeum jest <w>płatny</w> dla studentów?",
                            "target_lang_translations": {
                                "ru": "Вход в этот музей <w>платный</w> для студентов?"
                            }
                        }
                    ],
                    "synonyms": [
                        "odpłatny"
                    ],
                    "antonyms": [
                        "bezpłatny",
                        "darmowy",
                        "gratisowy"
                    ],
                    "common_phrases": [
                        "płatny parking",
                        "wstęp płatny",
                        "płatna autostrada",
                        "płatna toaleta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, за который нужно платить; не бесплатный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платный",
                                "target_lang_sense_clarification": "требующий оплаты (о товарах, услугах, местах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60f1e07e-6af7-3dd2-9d23-05d3c7806d31",
                    "sense_definition": "Taki, za którego wykonanie otrzymuje się wynagrodzenie pieniężne.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "remunerated",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przysługuje ci w pełni <w>płatny</w> urlop wypoczynkowy raz w roku.",
                            "target_lang_translations": {
                                "ru": "Тебе полагается полностью <w>оплачиваемый</w> отпуск раз в год."
                            }
                        },
                        {
                            "text": "Znalazłem dorywcze zajęcie <w>płatne</w> za każdą przepracowaną godzinę.",
                            "target_lang_translations": {
                                "ru": "Я нашел подработку, <w>оплачиваемую</w> за каждый отработанный час."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpłatny"
                    ],
                    "antonyms": [
                        "bezpłatny"
                    ],
                    "common_phrases": [
                        "urlop płatny",
                        "staż płatny",
                        "zajęcie płatne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, за выполнение которого получают денежное вознаграждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оплачиваемый",
                                "target_lang_sense_clarification": "за который платят деньги (о работе, отпуске)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2612a859-3b44-30ea-a26d-74fd702798c7",
                    "sense_definition": "Taki, który podlega zapłacie w określonym terminie lub w określony sposób.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "payable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rachunek za prąd jest <w>płatny</w> do dziesiątego dnia każdego miesiąca.",
                            "target_lang_translations": {
                                "ru": "Счет за электричество <w>подлежит оплате</w> до десятого числа каждого месяца."
                            }
                        },
                        {
                            "text": "Ten czek jest <w>płatny</w> na okaziciela w każdym oddziale banku.",
                            "target_lang_translations": {
                                "ru": "Этот чек <w>подлежит оплате</w> на предъявителя в любом отделении банка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "płatny z góry",
                        "płatny przelewem",
                        "płatny przy odbiorze",
                        "czek płatny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подлежащий оплате в определенный срок или определенным способом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подлежащий оплате",
                                "target_lang_sense_clarification": "о финансовых обязательствах"
                            },
                            {
                                "target_lang_word": "к оплате",
                                "target_lang_sense_clarification": "фраза, указывающая на необходимость платежа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "880f85aa-74e5-3768-bfad-6b85405e4f3b",
                    "sense_definition": "Taki, który wykonuje pewne usługi (często nielegalne lub nieetyczne) za pieniądze.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hired_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W filmie sensacyjnym głównym antagonistą był <w>płatny</w> zabójca.",
                            "target_lang_translations": {
                                "ru": "В боевике главным антагонистом был <w>наемный</w> убийца."
                            }
                        },
                        {
                            "text": "To był <w>płatny</w> zdrajca, który sprzedawał informacje wrogom.",
                            "target_lang_translations": {
                                "ru": "Это был <w>продажный</w> предатель, который продавал информацию врагам."
                            }
                        }
                    ],
                    "synonyms": [
                        "najemny"
                    ],
                    "common_phrases": [
                        "płatny zabójca",
                        "płatny morderca",
                        "płatny donosiciel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполняющий определенные услуги (часто незаконные или неэтичные) за деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наемный",
                                "target_lang_sense_clarification": "нанятый для выполнения задачи (часто об убийце)"
                            },
                            {
                                "target_lang_word": "продажный",
                                "target_lang_sense_clarification": "действующий ради денег, беспринципный (о предателе)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
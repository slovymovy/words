    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7413361d-c90b-3fe4-99af-5daaa58b66aa",
                    "sense_definition": "Het officieel melden van inkomsten, goederen of vermogen bij de belastingdienst of de douane.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fiscal_declaration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iedereen moet zijn jaarlijkse <w>aangifte</w> doen voor de eerste dag van mei.",
                            "target_lang_translations": {
                                "en": "Everyone must file their annual <w>tax return</w> before the first day of May."
                            }
                        },
                        {
                            "text": "De douane vroeg bij de grens om een <w>aangifte</w> van de ingevoerde goederen.",
                            "target_lang_translations": {
                                "en": "Customs asked for a <w>declaration</w> of the imported goods at the border."
                            }
                        }
                    ],
                    "synonyms": [
                        "declaratie"
                    ],
                    "common_phrases": [
                        "aangifte doen",
                        "aangifte inkomstenbelasting",
                        "elektronische aangifte"
                    ],
                    "target_lang_definitions": {
                        "en": "The official reporting of income, goods, or assets to the tax authorities or customs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tax return",
                                "target_lang_sense_clarification": "specifically for income tax"
                            },
                            {
                                "target_lang_word": "customs declaration",
                                "target_lang_sense_clarification": "for goods at a border"
                            },
                            {
                                "target_lang_word": "declaration",
                                "target_lang_sense_clarification": "general official statement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf638c6f-a245-3f11-9bb3-cd3353c1ca10",
                    "sense_definition": "Het officieel melden van een strafbaar feit, zoals diefstal of mishandeling, bij de politie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "crime_report",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de inbraak deed de bewoner direct <w>aangifte</w> bij het politiebureau.",
                            "target_lang_translations": {
                                "en": "After the burglary, the resident immediately filed a <w>report</w> at the police station."
                            }
                        },
                        {
                            "text": "De politie neemt de <w>aangifte</w> serieus en start onmiddellijk een onderzoek.",
                            "target_lang_translations": {
                                "en": "The police take the <w>report</w> seriously and are starting an investigation immediately."
                            }
                        }
                    ],
                    "synonyms": [
                        "melding"
                    ],
                    "common_phrases": [
                        "aangifte doen",
                        "aangifte opnemen",
                        "valse aangifte"
                    ],
                    "target_lang_definitions": {
                        "en": "The official reporting of a criminal offense, such as theft or assault, to the police."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "police report",
                                "target_lang_sense_clarification": "official record of a crime"
                            },
                            {
                                "target_lang_word": "complaint",
                                "target_lang_sense_clarification": "formal legal accusation"
                            },
                            {
                                "target_lang_word": "report",
                                "target_lang_sense_clarification": "general term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "347c51d4-10e3-32a4-9ae3-e5d684742007",
                    "sense_definition": "Het formulier of het schriftelijke document waarmee een officiÃ«le melding wordt gedaan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet een kopie van de <w>aangifte</w> naar de verzekering sturen voor de schadevergoeding.",
                            "target_lang_translations": {
                                "en": "You must send a copy of the <w>report</w> to the insurance company for compensation."
                            }
                        },
                        {
                            "text": "Hij was vergeten zijn handtekening onder de <w>aangifte</w> te zetten.",
                            "target_lang_translations": {

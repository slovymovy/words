    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a1ade0c1-ac1e-3b1b-96dc-94af765582d7",
                    "sense_definition": "A motorized vehicle with runners at the front and a continuous track at the rear, designed for traveling over snow.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "snow_vehicle_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We rented a <w>skidoo</w> to explore the snowy trails around the mountain lodge.",
                            "target_lang_translations": {
                                "ru": "Мы арендовали <w>снегоход</w>, чтобы исследовать заснеженные тропы вокруг горного домика."
                            }
                        },
                        {
                            "text": "He parked his <w>skidoo</w> next to the cabin after a long day of riding.",
                            "target_lang_translations": {
                                "ru": "Он припарковал свой <w>снегоход</w> рядом с хижиной после долгого дня езды."
                            }
                        }
                    ],
                    "synonyms": [
                        "snowmobile",
                        "snow machine"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in Canada"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Моторизованное транспортное средство с лыжами спереди и гусеницей сзади, предназначенное для передвижения по снегу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снегоход",
                                "target_lang_sense_clarification": "транспортное средство"
                            },
                            {
                                "target_lang_word": "аэросани",
                                "target_lang_sense_clarification": "устаревший или общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75911c3c-4da1-375c-9005-8385f6bad4de",
                    "sense_definition": "A lamp or light designed to flash on and off automatically to attract attention.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The antique sign featured a <w>skidoo</w> lamp that blinked rhythmically in the darkness.",
                            "target_lang_translations": {
                                "ru": "На старинной вывеске была <w>мигающая</w> лампа, которая ритмично вспыхивала в темноте."
                            }
                        },
                        {
                            "text": "Engineers in the early 20th century developed a <w>skidoo</w> mechanism for eye-catching displays.",
                            "target_lang_translations": {
                                "ru": "Инженеры в начале 20-го века разработали механизм <w>мигания</w> для привлекающих внимание витрин."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "early 20th century usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лампа или фонарь, предназначенные для автоматического включения и выключения (мигания) с целью привлечения внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мигающая лампа",
                                "target_lang_sense_clarification": "устройство освещения"
                            },
                            {
                                "target_lang_word": "лампа-вспышка",
                                "target_lang_sense_clarification": "для вывесок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0282a29a-0d4e-33f5-8b38-8f9d2dbd7318",
                    "sense_definition": "To drive or ride on a snowmobile.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "snow_vehicle_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>skidooed</w> across the frozen lake to reach the fishing spot.",
                            "target_lang_translations": {
                                "ru": "Они <w>поехали на снегоходах</w> через замерзшее озеро, чтобы добраться до места рыбалки."
                            }
                        },
                        {
                            "text": "In the winter, we often <w>skidoo</w> through the forest trails near our home.",
                            "target_lang_translations": {
                                "ru": "Зимой мы часто <w>катаемся на снегоходах</w> по лесным тропам недалеко от нашего дома."

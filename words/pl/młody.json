    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9b75310c-4a07-3468-a3e3-f35b128f5a64",
                    "sense_definition": "Będący we wczesnym okresie życia lub rozwoju; mający mało lat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "age_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>młodszy</w> brat chodzi jeszcze do przedszkola.",
                            "target_lang_translations": {
                                "ru": "Мой <w>младший</w> брат ходит ещё в детский сад."
                            }
                        },
                        {
                            "text": "To bardzo <w>młody</w> i utalentowany lekarz.",
                            "target_lang_translations": {
                                "ru": "Это очень <w>молодой</w> и талантливый врач."
                            }
                        },
                        {
                            "text": "W parku bawiło się wiele <w>młodych</w> osób.",
                            "target_lang_translations": {
                                "ru": "В парке развлекалось много <w>молодых</w> людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "niedojrzały",
                        "młodociany",
                        "nieletni"
                    ],
                    "antonyms": [
                        "stary",
                        "stary"
                    ],
                    "common_phrases": [
                        "młody człowiek",
                        "młode pokolenie",
                        "młody wiek",
                        "za młodu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в раннем периоде жизни или развития; проживший мало лет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодой",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "юный",
                                "target_lang_sense_clarification": "относящийся к юности, очень молодой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "335c0520-475a-37fb-9e41-dfde49c5fccc",
                    "sense_definition": "O roślinach lub produktach spożywczych: taki, który niedawno wyrósł, jest świeży lub pochodzi z tegorocznych zbiorów.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "freshness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na obiad podano schabowego i <w>młode</w> ziemniaki z koperkiem.",
                            "target_lang_translations": {
                                "ru": "На обед подали свиную отбивную и <w>молодой</w> картофель с укропом."
                            }
                        },
                        {
                            "text": "Wiosną na drzewach pojawiają się <w>młode</w> listki.",
                            "target_lang_translations": {
                                "ru": "Весной на деревьях появляются <w>молодые</w> листочки."
                            }
                        }
                    ],
                    "synonyms": [
                        "świeży",
                        "tegoroczny",
                        "wczesny",
                        "nowy"
                    ],
                    "antonyms": [
                        "stary",
                        "zeszłoroczny"
                    ],
                    "common_phrases": [
                        "młode ziemniaki",
                        "młoda kapusta",
                        "młode wino",
                        "młody las"
                    ],
                    "target_lang_definitions": {
                        "ru": "О растениях или продуктах питания: недавно выросший, свежий или из урожая этого года."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодой",
                                "target_lang_sense_clarification": "о картофеле, вине, овощах"
                            },
                            {
                                "target_lang_word": "ранний",
                                "target_lang_sense_clarification": "об урожае"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ba9fe96-23ba-39de-b4c2-7f7342cc34f5",
                    "sense_definition": "Taki, który zajmuje się czymś krótko, ma małe doświadczenie lub krótki staż w danej dziedzinie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako <w>młody</w> pracownik musisz się jeszcze wiele nauczyć od starszych kolegów.",
                            "target_lang_translations": {
                                "ru": "Как <w>молодой</w> сотрудник, ты должен ещё многому научиться у старших коллег."
                            }
                        },
                        {
                            "text": "Zespół składa się głównie z <w>młodych</w> inżynierów pełnych zapału.",
                            "target_lang_translations": {
                                "ru": "Команда состоит в основном из <w>молодых</w> инженеров, полных энтузиазма."
                            }
                        }
                    ],
                    "synonyms": [
                        "niedoświadczony",
                        "nowy",
                        "niedojrzały"
                    ],
                    "antonyms": [
                        "stary",
                        "stary"
                    ],
                    "common_phrases": [
                        "młody stażem",
                        "młody kierowca",
                        "młody talent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто занимается чем-либо недолго, имеет мало опыта или небольшой стаж в данной области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодой",
                                "target_lang_sense_clarification": "неопытный"
                            },
                            {
                                "target_lang_word": "начинающий",
                                "target_lang_sense_clarification": "новичок в деле"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7ce61499-c02d-3f30-be1f-0ca243e1eb3b",
                    "sense_definition": "Mężczyzna w dniu swojego ślubu (skrócona forma od: pan młody).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "wedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Młody</w> czekał na swoją wybrankę przed ołtarzem.",
                            "target_lang_translations": {
                                "ru": "<w>Жених</w> ждал свою избранницу перед алтарём."
                            }
                        },
                        {
                            "text": "Wszyscy goście złożyli życzenia <w>młodemu</w> i pannie młodej.",
                            "target_lang_translations": {
                                "ru": "Все гости поздравили <w>жениха</w> и невесту."
                            }
                        }
                    ],
                    "synonyms": [
                        "pan młody",
                        "nowożeniec",
                        "oblubieniec"
                    ],
                    "common_phrases": [
                        "pan młody",
                        "młoda para",
                        "państwo młodzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина в день своей свадьбы (сокращенная форма от: pan młody)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жених",
                                "target_lang_sense_clarification": "стандартное название"
                            },
                            {
                                "target_lang_word": "молодой",
                                "target_lang_sense_clarification": "разговорное или фольклорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "184f157c-4d5c-3ef5-b9f9-6abb5d1ef1dd",
                    "sense_definition": "Para, która właśnie wzięła ślub (używane w liczbie mnogiej).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "wedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice kupili <w>młodym</w> mieszkanie w prezencie ślubnym.",
                            "target_lang_translations": {
                                "ru": "Родители купили <w>молодым</w> квартиру в качестве свадебного подарка."
                            }
                        },
                        {
                            "text": "Wznoszę toast za zdrowie i szczęście <w>młodych</w>!",
                            "target_lang_translations": {
                                "ru": "Я поднимаю тост за здоровье и счастье <w>молодых</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "nowożeńcy",
                        "młoda para",
                        "państwo młodzi"
                    ],
                    "common_phrases": [
                        "zdrowie młodych",
                        "prezent dla młodych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пара, которая только что поженилась (используется во множественном числе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодожёны",
                                "target_lang_sense_clarification": "официальное"
                            },
                            {
                                "target_lang_word": "молодые",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d317d3d7-badb-340e-993b-5527ab72cfcb",
                    "sense_definition": "Potomek zwierzęcia; zwierzę w wczesnym okresie życia (zazwyczaj w formie nijakiej: to młode).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "offspring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kotka przeniosła swoje <w>młode</w> w bezpieczne miejsce.",
                            "target_lang_translations": {
                                "ru": "Кошка перенесла своих <w>детёнышей</w> в безопасное место."
                            }
                        },
                        {
                            "text": "Niedźwiedzica broniła swoich <w>młodych</w> przed zagrożeniem.",
                            "target_lang_translations": {
                                "ru": "Медведица защищала своих <w>детёнышей</w> от опасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "potomek"
                    ],
                    "common_phrases": [
                        "samica z młodymi",
                        "karmić młode"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потомство животного; животное в раннем периоде жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детёныш",
                                "target_lang_sense_clarification": "одно животное"
                            },
                            {
                                "target_lang_word": "потомство",
                                "target_lang_sense_clarification": "собирательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a62c4af8-903d-3c7c-a852-576494603f92",
                    "sense_definition": "Potoczne określenie chłopca lub młodego mężczyzny, często używane jako bezpośredni zwrot do kogoś młodszego.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słuchaj, <w>młody</w>, musisz mi pomóc z tymi zakupami.",
                            "target_lang_translations": {
                                "ru": "Слушай, <w>малой</w>, ты должен помочь мне с этими покупками."
                            }
                        },
                        {
                            "text": "<w>Młody</w> jeszcze nie wie, jak to się robi w tej firmie.",
                            "target_lang_translations": {
                                "ru": "<w>Молодой</w> ещё не знает, как делаются дела в этой фирме."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ej młody"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często używane protekcjonalnie lub koleżeńsko"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное обозначение мальчика или молодого мужчины, часто используется как прямое обращение к кому-то младшему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодой",
                                "target_lang_sense_clarification": "фамильярное обращение"
                            },
                            {
                                "target_lang_word": "малой",
                                "target_lang_sense_clarification": "слэнг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2847822e-69f1-39c1-bb02-c89c9c94687f",
                    "sense_definition": "Een tang met scherpe snijranden, voornamelijk gebruikt om metaaldraad of spijkers door te knippen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gebruikte een <w>kniptang</w> om het oude hek te repareren.",
                            "target_lang_translations": {
                                "en": "He used <w>wire cutters</w> to repair the old fence.",
                                "ru": "Он использовал <w>кусачки</w>, чтобы починить старый забор."
                            }
                        },
                        {
                            "text": "Heb je een <w>kniptang</w> om dit stuk dikke ijzerdraad door te knippen?",
                            "target_lang_translations": {
                                "en": "Do you have <w>wire cutters</w> to cut through this piece of thick iron wire?",
                                "ru": "У тебя есть <w>кусачки</w>, чтобы перекусить этот кусок толстой проволоки?"
                            }
                        }
                    ],
                    "synonyms": [
                        "draadtang"
                    ],
                    "common_phrases": [
                        "draad doorknippen",
                        "ijzerdraad knippen"
                    ],
                    "target_lang_definitions": {
                        "en": "A pair of pliers with sharp cutting edges, mainly used for cutting metal wire or nails.",
                        "ru": "Щипцы с острыми режущими краями, используемые в основном для перекусывания металлической проволоки или гвоздей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wire cutters",
                                "target_lang_sense_clarification": "tool for cutting wire"
                            },
                            {
                                "target_lang_word": "pincers",
                                "target_lang_sense_clarification": "tool for gripping and cutting"
                            },
                            {
                                "target_lang_word": "nippers",
                                "target_lang_sense_clarification": "small cutting tool"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кусачки",
                                "target_lang_sense_clarification": "инструмент для перекусывания проволоки"
                            },
                            {
                                "target_lang_word": "острогубцы",
                                "target_lang_sense_clarification": "щипцы с острыми губками"
                            },
                            {
                                "target_lang_word": "клещи",
                                "target_lang_sense_clarification": "инструмент для захвата и перекусывания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1142b00e-f3af-3ac4-98bd-74e5f054c044",
                    "sense_definition": "Een tang waarmee een conducteur een gaatje in een vervoersbewijs knipt om het ongeldig te maken voor hergebruik.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De conducteur pakte zijn <w>kniptang</w> om de kaartjes van de passagiers te controleren.",
                            "target_lang_translations": {
                                "en": "The conductor grabbed his <w>ticket punch</w> to check the passengers' tickets.",
                                "ru": "Кондуктор взял свой <w>компостер</w>, чтобы проверить билеты пассажиров."
                            }
                        },
                        {
                            "text": "Vroeger had elke conducteur in de trein een zilveren <w>kniptang</w> bij zich.",
                            "target_lang_translations": {
                                "en": "In the past, every conductor on the train carried a silver <w>ticket punch</w> with him.",
                                "ru": "Раньше у каждого кондуктора в поезде был с собой серебряный <w>компостер</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gaatjestang"
                    ],
                    "common_phrases": [
                        "kaartje knippen"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly replaced by digital scanners"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A tool used by a conductor to punch a hole in a ticket to invalidate it.",
                        "ru": "Инструмент, используемый кондуктором для пробивания отверстия в билете с целью сделать его недействительным."
                    },
                    "translations": {

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a59dc261-f776-3b75-af58-bc0cf086c864",
                    "sense_definition": "Heel erg lijden onder een fysieke of emotionele toestand, of (figuurlijk) sterven door de intensiteit ervan.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "suffering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik dacht dat ik zou <vergaan> van de zenuwen voor het examen.",
                            "target_lang_translations": {
                                "en": "I thought I would <die> of nerves before the exam.",
                                "ru": "Я думал, что <умру> от нервного напряжения перед экзаменом."
                            }
                        },
                        {
                            "text": "De vluchtelingen dreigden te <vergaan> van de kou in de bergen.",
                            "target_lang_translations": {
                                "en": "The refugees were in danger of <perishing> from the cold in the mountains.",
                                "ru": "Беженцам грозила опасность <погибнуть> от холода в горах."
                            }
                        }
                    ],
                    "synonyms": [
                        "creperen",
                        "omkomen",
                        "sterven"
                    ],
                    "common_phrases": [
                        "vergaan van de pijn",
                        "vergaan van de honger",
                        "vergaan van de kou"
                    ],
                    "target_lang_definitions": {
                        "en": "To suffer intensely from a physical or emotional condition, or (figuratively) to die from its intensity.",
                        "ru": "Сильно страдать от физического или эмоционального состояния, или (фигурально) умирать от его интенсивности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "perish",
                                "target_lang_sense_clarification": "to die or suffer destruction (formal)"
                            },
                            {
                                "target_lang_word": "die",
                                "target_lang_sense_clarification": "to cease living or suffer extreme pain/emotion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "умирать",
                                "target_lang_sense_clarification": "фигурально, от сильных переживаний или боли"
                            },
                            {
                                "target_lang_word": "погибать",
                                "target_lang_sense_clarification": "физически, от тяжелых условий"
                            },
                            {
                                "target_lang_word": "изнемогать",
                                "target_lang_sense_clarification": "сильно страдать, терять силы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f0d8d5b-e891-387f-bb18-b59c490e447d",
                    "sense_definition": "Langzaam kapotgaan, uit elkaar vallen of verdwijnen door ouderdom of rotting.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "decay",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude houten schuur is in de loop der jaren volledig <vergaan>.",
                            "target_lang_translations": {
                                "en": "The old wooden barn has completely <decayed> over the years.",
                                "ru": "Старый деревянный сарай с годами полностью <сгнил>."
                            }
                        },
                        {
                            "text": "Alles wat mensen bouwen, zal uiteindelijk weer tot stof <vergaan>.",
                            "target_lang_translations": {
                                "en": "Everything humans build will eventually <turn> to dust again.",
                                "ru": "Всё, что строят люди, в конечном итоге снова <обратится> в прах."
                            }
                        }
                    ],
                    "synonyms": [
                        "verrotten",
                        "wegrotten",
                        "uiteenvallen",
                        "verdwijnen"
                    ],
                    "antonyms": [
                        "ontstaan"
                    ],
                    "common_phrases": [
                        "tot stof vergaan",
                        "vergane glorie"
                    ],
                    "target_lang_definitions": {
                        "en": "To slowly break down, fall apart, or disappear due to old age or rotting.",
                        "ru": "Медленно разрушаться, распадаться или исчезать из-за старости или гниения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decay",
                                "target_lang_sense_clarification": "to decompose or decline in quality"
                            },
                            {
                                "target_lang_word": "rot",
                                "target_lang_sense_clarification": "to decompose biologically"
                            },
                            {
                                "target_lang_word": "perish",
                                "target_lang_sense_clarification": "to decay or disappear (literary)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гнить",
                                "target_lang_sense_clarification": "портиться от гниения"
                            },
                            {
                                "target_lang_word": "истлевать",
                                "target_lang_sense_clarification": "превращаться в прах, разрушаться от времени"
                            },
                            {
                                "target_lang_word": "разрушаться",
                                "target_lang_sense_clarification": "приходить в негодность, распадаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f6116eb-28f5-343c-bb76-4495e2de2f90",
                    "sense_definition": "Het voorbijgaan van een periode of tijdstip.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tijd <vergaat> ontzettend snel als je het naar je zin hebt.",
                            "target_lang_translations": {
                                "en": "Time <passes> incredibly quickly when you are having fun.",
                                "ru": "Время <летит> невероятно быстро, когда хорошо проводишь время."
                            }
                        },
                        {
                            "text": "Vele jaren zijn <vergaan> zonder dat hij iets van zijn familie hoorde.",
                            "target_lang_translations": {
                                "en": "Many years have <passed> without him hearing anything from his family.",
                                "ru": "Много лет <прошло> с тех пор, как он получал известия от семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorbijgaan",
                        "verstrijken",
                        "omvliegen"
                    ],
                    "target_lang_definitions": {
                        "en": "The passing of a period or point in time.",
                        "ru": "То, как складывается судьба или обстоятельства для кого-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "to move past in time"
                            },
                            {
                                "target_lang_word": "go by",
                                "target_lang_sense_clarification": "to elapse"
                            },
                            {
                                "target_lang_word": "elapse",
                                "target_lang_sense_clarification": "to pass (of time)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "статься",
                                "target_lang_sense_clarification": "случиться, произойти с кем-либо"
                            },
                            {
                                "target_lang_word": "складываться",
                                "target_lang_sense_clarification": "об обстоятельствах или судьбе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2b0327e-25ca-396e-873e-0e07681613b8",
                    "sense_definition": "Zinken en daardoor verloren gaan (specifiek gebruikt voor schepen).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_disaster",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het schip is tijdens de zware storm met man en muis <vergaan>.",
                            "target_lang_translations": {
                                "en": "The ship <perished> with all hands during the heavy storm.",
                                "ru": "Корабль <затонул> вместе со всей командой во время сильного шторма."
                            }
                        },
                        {
                            "text": "In de zeventiende eeuw zijn veel handelsschepen op deze route <vergaan>.",
                            "target_lang_translations": {
                                "en": "In the seventeenth century, many merchant ships <perished> on this route.",
                                "ru": "В семнадцатом веке на этом маршруте <погибло> много торговых судов."
                            }
                        }
                    ],
                    "synonyms": [
                        "zinken",
                        "ten onder gaan"
                    ],
                    "common_phrases": [
                        "met man en muis vergaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To sink and thereby be lost (specifically used for ships).",
                        "ru": "Идти ко дну, тонуть (обычно о судах)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "perish",
                                "target_lang_sense_clarification": "to be destroyed or lost, especially at sea"
                            },
                            {
                                "target_lang_word": "sink",
                                "target_lang_sense_clarification": "to go down below the surface of water"
                            },
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "to sink (of a ship)"
                            }
                        ],
                        "ru": [

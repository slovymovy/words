                    "sense_id": "3068af4f-5558-3608-b93e-ade171cd23d9",
                    "sense_definition": "De benadrukte vorm van <w>geen</w>: absoluut niet een, niet de of niet het. Wordt gebruikt om met klem aan te geven dat iets er niet is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "negation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <géén> reisgids en bevat dan ook geen routes.",
                            "target_lang_translations": {
                                "en": "It is <not a> travel guide and therefore contains no routes."
                            }
                        },
                        {
                            "text": "Ik wil absoluut <géén> suiker in mijn koffie hebben.",
                            "target_lang_translations": {
                                "en": "I want absolutely <no> sugar in my coffee."
                            }
                        }
                    ],
                    "common_phrases": [
                        "absoluut géén",
                        "echt géén"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "benadrukte vorm"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The emphasized form of 'geen': absolutely not a, not the, or not it. Used to indicate with emphasis that something is absent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "no",
                                "target_lang_sense_clarification": "emphasized negation"
                            },
                            {
                                "target_lang_word": "not a",
                                "target_lang_sense_clarification": "emphasized negation before singular nouns"
                            },
                            {
                                "target_lang_word": "not any",
                                "target_lang_sense_clarification": "emphasized absence"
                            }
                        ]
                    }
                }
            ]
        },

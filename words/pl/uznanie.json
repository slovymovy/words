                {
                    "sense_id": "17bb8461-12d7-36a7-80fd-44e51ac8a696",
                    "sense_definition": "Szacunek, poważanie i podziw, jakimi darzy się kogoś za jego osiągnięcia, zasługi lub postawę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego ciężka praca i poświęcenie w końcu zdobyły <uznanie> przełożonych.",
                            "target_lang_translations": {
                                "ru": "Его упорный труд и самоотверженность в конце концов завоевали <w>признание</w> начальства."
                            }
                        },
                        {
                            "text": "Młoda pianistka cieszy się coraz większym <uznaniem> w świecie muzyki.",
                            "target_lang_translations": {
                                "ru": "Молодая пианистка пользуется всё большим <w>признанием</w> в музыкальном мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "szacunek",
                        "poważanie",
                        "podziw",
                        "aprobata",
                        "estyma"
                    ],
                    "antonyms": [
                        "lekceważenie",
                        "pogarda",
                        "dezaprobata"
                    ],
                    "common_phrases": [
                        "wyrazy uznania",
                        "zdobyć uznanie",
                        "cieszyć się uznaniem",
                        "słowa uznania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уважение, почтение и восхищение, испытываемые к кому-либо за его достижения, заслуги или поведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признание",
                                "target_lang_sense_clarification": "положительная оценка, общественное уважение"
                            },
                            {
                                "target_lang_word": "уважение",
                                "target_lang_sense_clarification": "почтение, высокая оценка личности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "896dc014-d043-315c-b5ab-c7760c1f6047",
                    "sense_definition": "Swobodna ocena sytuacji i prawo do podjęcia decyzji według własnej woli i wyczucia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wybór odpowiedniego koloru ścian pozostawiam twojemu <uznaniu>.",
                            "target_lang_translations": {
                                "ru": "Выбор подходящего цвета стен я оставляю на твое <w>усмотрение</w>."
                            }
                        },
                        {
                            "text": "Kucharz doprawił zupę solą i pieprzem według własnego <uznania>.",
                            "target_lang_translations": {
                                "ru": "Повар приправил суп солью и перцем по собственному <w>усмотрению</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wola",
                        "osąd",
                        "decyzja",
                        "wybór"
                    ],
                    "common_phrases": [
                        "według uznania",
                        "do uznania",
                        "własne uznanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободная оценка ситуации и право принимать решения по собственной воле и чутью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усмотрение",
                                "target_lang_sense_clarification": "решение по собственной воле, выбор"

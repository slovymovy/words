            "senses": [
                {
                    "sense_id": "b7b645b7-07be-39f1-890c-3cead4a609f3",
                    "sense_definition": "Meervoud van <w>jager</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>jagers</w> trokken vroeg in de ochtend het bos in om wild te zoeken.",
                            "target_lang_translations": {
                                "ru": "Рано утром <w>охотники</w> отправились в лес на поиски дичи."
                            }
                        },
                        {
                            "text": "In de prehistorie leefden de meeste mensen als <w>jagers</w> en verzamelaars.",
                            "target_lang_translations": {
                                "ru": "В доисторические времена большинство людей жили как <w>охотники</w> и собиратели."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jagers en verzamelaars",
                        "groep jagers",
                        "ervaren jagers"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число слова <w>jager</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охотники",
                                "target_lang_sense_clarification": "люди, занимающиеся охотой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f040ec4c-0ac2-31ca-8e5a-bd9aff728a67",
                    "sense_definition": "Een familie van zeevogels (Stercorariidae), ook wel roofmeeuwen genoemd, die bekendstaan om het stelen van voedsel van andere vogels.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "birds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de expeditie naar het noordpoolgebied hebben we diverse soorten <w>jagers</w> waargenomen.",
                            "target_lang_translations": {
                                "ru": "Во время экспедиции в Арктику мы наблюдали различные виды <w>поморников</w>."
                            }
                        },
                        {
                            "text": "Deze <w>jagers</w> vallen vaak andere zeevogels aan om hun prooi af te pakken.",
                            "target_lang_translations": {
                                "ru": "Эти <w>поморники</w> часто нападают на других морских птиц, чтобы отнять у них добычу."
                            }
                        }
                    ],
                    "synonyms": [
                        "roofmeeuwen"
                    ],
                    "common_phrases": [
                        "grote jagers",
                        "kleine jagers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Семейство морских птиц (поморниковые), также называемых хищными чайками, известных тем, что они отнимают пищу у других птиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поморники",
                                "target_lang_sense_clarification": "семейство морских птиц"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "9d77b928-2c06-3af1-89b3-6e3546c96dbc",
                    "sense_definition": "Сленговое выражение для подтверждения получения информации или согласия; сокращение от <w>понятно</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Я задержусь на работе на час. — <w>Пон</w>, тогда ужинайте без меня.",
                            "target_lang_translations": {
                                "en": "— I'll be stuck at work for an hour. — <w>Got it</w>, then have dinner without me."
                            }
                        },
                        {
                            "text": "— Завтра встречаемся в парке в два. — Ок, <w>пон</w>.",
                            "target_lang_translations": {
                                "en": "— We're meeting in the park at two tomorrow. — Ok, <w>understood</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "понятно"
                    ],
                    "common_phrases": [
                        "ну пон",
                        "теперь пон"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "молодежный сленг"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Slang expression to confirm receipt of information or agreement; abbreviation of \"understood\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "got it",
                                "target_lang_sense_clarification": "informal acknowledgment"
                            },
                            {
                                "target_lang_word": "understood",
                                "target_lang_sense_clarification": "general agreement"
                            },
                            {
                                "target_lang_word": "I see",
                                "target_lang_sense_clarification": "acknowledgment of information"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ff39e2b6-08fc-380d-863e-997d40257554",
                    "sense_definition": "Разговорная сокращённая форма слова <w>понятно</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне всё <w>пон</w>, вопросов больше нет.",
                            "target_lang_translations": {
                                "en": "Everything is <w>clear</w> to me, I have no more questions."
                            }
                        },
                        {
                            "text": "Сначала было сложно, но теперь <w>пон</w>, как это работает.",
                            "target_lang_translations": {
                                "en": "It was hard at first, but now it's <w>clear</w> how it works."
                            }
                        }
                    ],
                    "synonyms": [
                        "понятно"
                    ],
                    "common_phrases": [
                        "всё пон"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "сокращение"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial shortened form of the word \"clear\" or \"understood\"."
                    },
                    "translations": {
                        "en": [
                            {

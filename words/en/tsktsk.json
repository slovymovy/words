    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "3c508993-586d-3c0b-a648-5b6db3371fd5",
                    "sense_definition": "Used to express disapproval, annoyance, or mild criticism, often by making a clicking sound with the tongue.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "\"<w>Tsktsk</w>, you really should have been more careful with that glass.\"",
                            "target_lang_translations": {
                                "ru": "\"<w>Ц-ц-ц</w>, тебе действительно следовало быть осторожнее с этим стаканом.\""
                            }
                        },
                        {
                            "text": "\"<w>Tsktsk</w>, look at this mess you have made in the living room.\"",
                            "target_lang_translations": {
                                "ru": "\"<w>Ай-ай-ай</w>, посмотри, какой беспорядок ты устроил в гостиной.\""
                            }
                        }
                    ],
                    "synonyms": [
                        "tut-tut",
                        "tsk-tsk"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "alternative spelling of tsk tsk"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения неодобрения, раздражения или легкой критики, часто сопровождается цоканьем языком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ц-ц-ц",
                                "target_lang_sense_clarification": "звук неодобрения"
                            },
                            {
                                "target_lang_word": "ай-ай-ай",
                                "target_lang_sense_clarification": "выражение укоризны"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a4232163-7bcb-31a9-bf9c-f3508522c83e",
                    "sense_definition": "To make a clicking sound with the tongue to express disapproval or annoyance.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The librarian <w>tsktsked</w> at the noisy students near the entrance.",
                            "target_lang_translations": {
                                "ru": "Библиотекарь <w>зацокала</w> на шумных студентов у входа."
                            }
                        },
                        {
                            "text": "She <w>tsktsked</w> in disappointment when she saw the scratches on her car.",
                            "target_lang_translations": {
                                "ru": "Она разочарованно <w>поцокала</w>, увидев царапины на своей машине."
                            }
                        }
                    ],
                    "synonyms": [
                        "tut-tut",
                        "cluck"
                    ],
                    "common_phrases": [
                        "tsktsk at someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать цокающий звук языком для выражения неодобрения или раздражения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цокать",
                                "target_lang_sense_clarification": "издавать звук языком"
                            },
                            {
                                "target_lang_word": "аяяйкать",
                                "target_lang_sense_clarification": "выражать неодобрение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "5369b718-8dbc-30fa-8fe7-1003aaed16b1",
                    "sense_definition": "A planned action, method, or maneuver calculated to achieve a specific goal or end.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "method_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team discussed a new <w>tactic</w> to improve their sales figures next quarter.",

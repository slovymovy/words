    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "53f06936-f225-33d4-b22c-e4d2a732e5b4",
                    "sense_definition": "Ребёнок или подросток, не имеющий семьи и дома, живущий на улице без присмотра взрослых.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "homeless_child",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После войны на вокзалах часто ночевали голодные <w>беспризорники</w>.",
                            "target_lang_translations": {
                                "en": "After the war, hungry <w>street children</w> often slept at train stations."
                            }
                        },
                        {
                            "text": "Государство создало специальные приюты, чтобы каждый <w>беспризорник</w> мог получить образование.",
                            "target_lang_translations": {
                                "en": "The state created special shelters so that every <w>homeless child</w> could receive an education."
                            }
                        }
                    ],
                    "synonyms": [
                        "бродяжка"
                    ],
                    "common_phrases": [
                        "голодный беспризорник",
                        "бывший беспризорник",
                        "толпа беспризорников",
                        "судьба беспризорника"
                    ],
                    "target_lang_definitions": {
                        "en": "A child or teenager without a family or home, living on the street without adult supervision."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "street child",
                                "target_lang_sense_clarification": "a homeless child living on the streets"
                            },
                            {
                                "target_lang_word": "homeless child",
                                "target_lang_sense_clarification": "a child with no home"
                            },
                            {
                                "target_lang_word": "waif",
                                "target_lang_sense_clarification": "a homeless, neglected, or abandoned person, especially a child"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "073d72ad-d0bd-3ad4-ae0e-e2199f81406c",
                    "sense_definition": "Тот, кто лишён необходимого внимания, заботы или надзора (обычно в переносном смысле).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "neglected_one",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Без строгого начальника наш отдел чувствовал себя <w>беспризорником</w>.",
                            "target_lang_translations": {
                                "en": "Without a strict boss, our department felt like an <w>abandoned child</w>."
                            }
                        },
                        {
                            "text": "Ты выглядишь как <w>беспризорник</w> в этой грязной и рваной одежде.",
                            "target_lang_translations": {
                                "en": "You look like a <w>ragamuffin</w> in those dirty and torn clothes."
                            }
                        }
                    ],
                    "common_phrases": [
                        "чувствовать себя беспризорником",
                        "выглядеть как беспризорник"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who is deprived of necessary attention, care, or supervision (usually figurative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ragamuffin",
                                "target_lang_sense_clarification": "a person, typically a child, in ragged, dirty clothes"
                            },
                            {
                                "target_lang_word": "neglected child",
                                "target_lang_sense_clarification": "someone lacking care or supervision"
                            },
                            {
                                "target_lang_word": "waif",
                                "target_lang_sense_clarification": "someone looking thin and neglected"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
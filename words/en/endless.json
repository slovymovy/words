    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1f91eff8-e92f-3f83-acce-6a7c7df5a156",
                    "sense_definition": "Continuing for a long time and seeming to have no end.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The journey home seemed <w>endless</w> because of the heavy traffic.",
                            "target_lang_translations": {
                                "ru": "Дорога домой казалась <w>бесконечной</w> из-за сильных пробок."
                            }
                        },
                        {
                            "text": "She was tired of the <w>endless</w> arguments with her neighbors.",
                            "target_lang_translations": {
                                "ru": "Она устала от <w>бесконечных</w> споров с соседями."
                            }
                        }
                    ],
                    "synonyms": [
                        "unending",
                        "ceaseless",
                        "neverending",
                        "eternal"
                    ],
                    "antonyms": [
                        "finite",
                        "limited"
                    ],
                    "common_phrases": [
                        "endless hours",
                        "endless stream",
                        "endless wait"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжающийся очень долго и кажущийся нескончаемым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесконечный",
                                "target_lang_sense_clarification": "о времени или длительных процессах"
                            },
                            {
                                "target_lang_word": "нескончаемый",
                                "target_lang_sense_clarification": "о потоке событий, разговорах, ожидании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "307be759-556a-3225-a56a-c9720da86038",
                    "sense_definition": "Extremely large in number or amount; impossible to count or exhaust.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are <w>endless</w> possibilities for what we can cook for dinner.",
                            "target_lang_translations": {
                                "ru": "Существует <w>бесконечное</w> множество вариантов того, что можно приготовить на ужин."
                            }
                        },
                        {
                            "text": "He has <w>endless</w> patience when teaching young children.",
                            "target_lang_translations": {
                                "ru": "У него <w>безграничное</w> терпение, когда он учит маленьких детей."
                            }
                        }
                    ],
                    "synonyms": [
                        "infinite",
                        "unlimited"
                    ],
                    "antonyms": [
                        "limited",
                        "finite"
                    ],
                    "common_phrases": [
                        "endless possibilities",
                        "endless variety",
                        "endless supply"
                    ],
                    "target_lang_definitions": {
                        "ru": "Огромный по количеству или объему; неисчерпаемый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесконечный",
                                "target_lang_sense_clarification": "об общем количестве"
                            },
                            {
                                "target_lang_word": "безграничный",
                                "target_lang_sense_clarification": "о возможностях или терпении"
                            },
                            {
                                "target_lang_word": "неисчерпаемый",
                                "target_lang_sense_clarification": "о запасах или ресурсах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb9b59e0-3b7a-33a5-9800-fc3caf2e7a12",
                    "sense_definition": "Extending indefinitely in space or forming a continuous loop.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The machine is driven by an <w>endless</w> belt connected to the motor.",
                            "target_lang_translations": {
                                "ru": "Машина приводится в движение <w>бесконечным</w> ремнем, соединенным с двигателем."
                            }
                        },
                        {
                            "text": "We looked out at the <w>endless</w> line of trees on the horizon.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели на <w>бесконечную</w> линию деревьев на горизонте."
                            }
                        }
                    ],
                    "synonyms": [
                        "unending"
                    ],
                    "antonyms": [
                        "finite"
                    ],
                    "common_phrases": [
                        "endless belt",
                        "endless loop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простирающийся неопределенно далеко в пространстве или образующий непрерывный цикл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесконечный",
                                "target_lang_sense_clarification": "о механизмах (ремень, лента) или пространстве"
                            },
                            {
                                "target_lang_word": "бескрайний",
                                "target_lang_sense_clarification": "о просторах, горизонте"
                            }
                        ]
                    }
                }
            ]
        }

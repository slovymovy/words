                },
                {
                    "sense_id": "2ef3a6c9-34c4-3225-b566-9007e4286760",
                    "sense_definition": "Al zwemmend een wateroppervlak of gebied volledig doorkruisen of oversteken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "traverse_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dappere ridder <w>doorzwom</w> de brede slotgracht om het kasteel te bereiken.",
                            "target_lang_translations": {
                                "en": "The brave knight <w>swam across</w> the wide moat to reach the castle.",
                                "ru": "Храбрый рыцарь <w>переплыл</w> широкий крепостной ров, чтобы добраться до замка."
                            }
                        },
                        {
                            "text": "Hij heeft in zijn leven al heel wat watertjes <w>doorzwommen</w>.",
                            "target_lang_translations": {
                                "en": "He has <w>swum through</w> quite a few waters in his life.",
                                "ru": "В своей жизни он <w>переплыл</w> уже немало вод."
                            }
                        }
                    ],
                    "synonyms": [
                        "overzwemmen",
                        "doorkruisen"
                    ],
                    "common_phrases": [
                        "vele wateren doorzwommen hebben"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inseparable verb (stress on 'zwem')"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To completely cross or traverse a body of water or area by swimming.",
                        "ru": "Полностью пересечь водную поверхность или область вплавь."
                    },

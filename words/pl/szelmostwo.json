{
    "word_family": [
        "szelma",
        "szelmowski",
        "szelmowsko"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9ead2a4-60af-333b-adb1-bee5d9b508f3",
                    "sense_definition": "Czyn lub zachowanie charakterystyczne dla osoby przebiegłej i nieuczciwej; postępek podstępny, ale często sprytny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prasa brukowa przez wiele tygodni rozpisywała się o jego finansowych <w>szelmostwach</w>.",
                            "target_lang_translations": {
                                "ru": "Желтая пресса неделями расписывала его финансовые <w>махинации</w>."
                            }
                        },
                        {
                            "text": "To było zwykłe <w>szelmostwo</w>, którego nikt się po tak szanowanym obywatelu nie spodziewał.",
                            "target_lang_translations": {
                                "ru": "Это была обыкновенная <w>подлость</w>, которой никто не ожидал от столь уважаемого гражданина."
                            }
                        }
                    ],
                    "synonyms": [
                        "łotrostwo",
                        "niegodziwość",
                        "draństwo",
                        "oszustwo",
                        "sprawka"
                    ],
                    "antonyms": [
                        "dobry uczynek"
                    ],
                    "common_phrases": [
                        "popełnić szelmostwo",
                        "drobne szelmostwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поступок или поведение, характерные для хитрого и нечестного человека; коварное, но часто ловкое действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плутовство",
                                "target_lang_sense_clarification": "хитрая проделка, уловка"
                            },
                            {
                                "target_lang_word": "мошенничество",
                                "target_lang_sense_clarification": "обман, жульничество (особенно в финансовых делах)"
                            },
                            {
                                "target_lang_word": "подлость",
                                "target_lang_sense_clarification": "низкий, бесчестный поступок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6ccf991-2468-31fb-bd8b-d5d09db5feda",
                    "sense_definition": "Cechy charakteru polegające na sprycie, przebiegłości i braku skrupułów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W jego oczach widać było pewne <w>szelmostwo</w> i chęć do płatania figli.",
                            "target_lang_translations": {
                                "ru": "В его глазах читалось некое <w>лукавство</w> и желание проказничать."
                            }
                        },
                        {
                            "text": "Dzięki wrodzonemu <w>szelmostwu</w> udało mu się wielokrotnie uniknąć odpowiedzialności.",
                            "target_lang_translations": {
                                "ru": "Благодаря врожденной <w>пройдошливости</w> ему удавалось многократно избегать ответственности."
                            }
                        }
                    ],
                    "synonyms": [
                        "cwaniactwo",
                        "spryt",
                        "przebiegłość",
                        "łajdactwo"
                    ],
                    "antonyms": [
                        "uczciwość",
                        "prawość",
                        "naiwność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Черты характера, проявляющиеся в хитрости, изворотливости и отсутствии совести."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плутовство",
                                "target_lang_sense_clarification": "склонность к хитрости и обману"
                            },
                            {
                                "target_lang_word": "пройдошливость",
                                "target_lang_sense_clarification": "изворотливость, умение устраивать свои дела"
                            },
                            {
                                "target_lang_word": "лукавство",
                                "target_lang_sense_clarification": "хитрость, коварство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
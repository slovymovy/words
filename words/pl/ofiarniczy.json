            "senses": [
                {
                    "sense_id": "12b5737c-203b-3042-bf00-78e857839308",
                    "sense_definition": "Związany ze składaniem ofiar, dotyczący rytuału ofiarowania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wykład dotyczył <w>ofiarniczego</w> charakteru eucharystii.",
                            "target_lang_translations": {
                                "ru": "Лекция была посвящена <w>жертвенному</w> характеру евхаристии."
                            }
                        },
                        {
                            "text": "Archeolodzy odnaleźli kamienny nóż <w>ofiarniczy</w> w ruinach starożytnej świątyni.",
                            "target_lang_translations": {
                                "ru": "Археологи нашли каменный <w>жертвенный</w> нож в руинах древнего храма."
                            }
                        }
                    ],
                    "synonyms": [
                        "rytualny",
                        "ofiarny"
                    ],
                    "common_phrases": [
                        "nóż ofiarniczy",
                        "stos ofiarniczy",
                        "czynność ofiarnicza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с принесением жертв, касающийся ритуала жертвоприношения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жертвенный",
                                "target_lang_sense_clarification": "относящийся к жертвоприношению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "648b8800-7e9f-3de1-966b-f28337b54ea0",
                    "sense_definition": "Należący do ofiarnika, czyli osoby składającej ofiarę w obrzędach religijnych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bogato zdobiona szata <w>ofiarnicza</w> była przechowywana w świątelnym skarbcu.",
                            "target_lang_translations": {
                                "ru": "Богато украшенное <w>жреческое</w> облачение хранилось в сокровищнице храма."
                            }
                        },
                        {
                            "text": "Jego ród od wielu pokoleń pełnił zaszczytne funkcje <w>ofiarnicze</w>.",
                            "target_lang_translations": {
                                "ru": "Его род на протяжении многих поколений выполнял почетные <w>жреческие</w> функции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szata ofiarnicza",
                        "strój ofiarniczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий жертвователю (жрецу), то есть лицу, совершающему жертвоприношение в религиозных обрядах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жреческий",
                                "target_lang_sense_clarification": "относящийся к жрецу или совершителю обряда"
                            },
                            {
                                "target_lang_word": "ритуальный",
                                "target_lang_sense_clarification": "связанный с выполнением обряда"
                            }
                        ]
                    }
                }
            ]
        }
    ]

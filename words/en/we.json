{
    "word_family": [],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "d4f8415c-0c11-3e24-b209-8dfb03102ded",
                    "sense_definition": "Used to refer to the speaker and one or more other people considered together.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_group_standard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>We</w> are here to help you with your project.",
                            "target_lang_translations": {
                                "ru": "<w>Мы</w> здесь, чтобы помочь вам с вашим проектом."
                            }
                        },
                        {
                            "text": "<w>We</w> need to have a serious conversation about the future.",
                            "target_lang_translations": {
                                "ru": "<w>Мы</w> должны серьезно поговорить о будущем."
                            }
                        }
                    ],
                    "antonyms": [
                        "they"
                    ],
                    "common_phrases": [
                        "here we go",
                        "shall we",
                        "off we go",
                        "there we are"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения говорящего вместе с одним или несколькими другими людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мы",
                                "target_lang_sense_clarification": "местоимение 1-го лица мн. ч."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "525cfe1a-7b83-3352-bf84-d2ce17848658",
                    "sense_definition": "Used to refer to a group, organization, team, or nation that the speaker belongs to or identifies with.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_group_standard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>We</w> lost the football match against the rival school.",
                            "target_lang_translations": {
                                "ru": "<w>Мы</w> проиграли футбольный матч школе-сопернику."
                            }
                        },
                        {
                            "text": "In English, <w>we</w> do not usually assign gender to nouns.",
                            "target_lang_translations": {
                                "ru": "В английском языке <w>мы</w> обычно не присваиваем род существительным."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения группы, организации, команды или нации, к которой принадлежит говорящий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мы",
                                "target_lang_sense_clarification": "коллективное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb33a1b4-2c92-3af7-8422-45527bd465b6",
                    "sense_definition": "Used to refer to people in general; humanity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_group_generic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>We</w> live and learn from our mistakes.",
                            "target_lang_translations": {
                                "ru": "<w>Мы</w> живем и учимся на своих ошибках."
                            }
                        },
                        {
                            "text": "As <w>we</w> age, our health becomes a greater priority.",
                            "target_lang_translations": {
                                "ru": "По мере того как <w>мы</w> стареем, наше здоровье становится более важным приоритетом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "as we know",
                        "we live and learn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения людей вообще; человечество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мы",
                                "target_lang_sense_clarification": "обобщающее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bddf93e-767b-3f85-be9e-645184818ca9",
                    "sense_definition": "Used by a speaker or writer to refer to themselves and the audience together, or to speak with the authority of an organization.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pronoun_group_formal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To solve this equation, <w>we</w> first calculate the value of x.",
                            "target_lang_translations": {
                                "ru": "Чтобы решить это уравнение, <w>мы</w> сначала вычисляем значение x."
                            }
                        },
                        {
                            "text": "As the government, <w>we</w> have a duty to ensure public safety.",
                            "target_lang_translations": {
                                "ru": "Как правительство, <w>мы</w> обязаны обеспечить общественную безопасность."
                            }
                        }
                    ],
                    "common_phrases": [
                        "we will see",
                        "we can conclude"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "editorial or authorial we"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется говорящим или писателем для обозначения себя вместе с аудиторией или при выступлении от лица организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мы",
                                "target_lang_sense_clarification": "авторское или официальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67485585-5e9f-34cd-86a8-5f23f3e16f35",
                    "sense_definition": "Used to address a single person, especially a patient or child, to sound gentle, encouraging, or patronizing.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pronoun_group_special",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "How are <w>we</w> feeling this morning, Mrs. Miggins?",
                            "target_lang_translations": {
                                "ru": "Как <w>мы</w> себя чувствуем сегодня утром, миссис Миггинс?"
                            }
                        },
                        {
                            "text": "Are <w>we</w> ready to go to bed now, sweetie?",
                            "target_lang_translations": {
                                "ru": "<w>Мы</w> готовы ложиться спать, милая?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used by nurses, doctors, or parents"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется при обращении к одному человеку (особенно пациенту или ребенку) для выражения мягкости, ободрения или покровительственного тона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мы",
                                "target_lang_sense_clarification": "сочувственное или покровительственное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e6890d5-4dcc-3106-b9c1-af4a0a15a6ea",
                    "sense_definition": "Used by a sovereign (monarch) to refer to themselves.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "pronoun_group_special",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>We</w> are Queen of all these Isles.",
                            "target_lang_translations": {
                                "ru": "<w>Мы</w> — Королева всех этих островов."
                            }
                        },
                        {
                            "text": "<w>We</w> grant this charter for the benefit of our subjects.",
                            "target_lang_translations": {
                                "ru": "<w>Мы</w> даруем эту хартию на благо наших подданных."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "royal we"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "mostly historical or ceremonial"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется монархом для обозначения самого себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мы",
                                "target_lang_sense_clarification": "монаршее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5746986-ac18-36bd-bdad-8476e3d6afba",
                    "sense_definition": "Used as the object of a verb or preposition, replacing 'us'.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "pronoun_group_dialect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was a generous man and always bought <w>we</w> drinks after work.",
                            "target_lang_translations": {
                                "ru": "Он был щедрым человеком и всегда покупал <w>нам</w> выпивку после работы."
                            }
                        },
                        {
                            "text": "What have you done for <w>we</w> lately?",
                            "target_lang_translations": {
                                "ru": "Что ты сделал для <w>нас</w> в последнее время?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "regional usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в диалектах как дополнение глагола или предлога, заменяя 'us' (нас/нам)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нас",
                                "target_lang_sense_clarification": "винительный падеж"
                            },
                            {
                                "target_lang_word": "нам",
                                "target_lang_sense_clarification": "дательный падеж"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "9fab4c9d-3b27-3171-b641-fb0acf39d79d",
                    "sense_definition": "Used before a noun to show that the speaker belongs to that specific category of people.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "determiner_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>We</w> Canadians like to think of ourselves as different.",
                            "target_lang_translations": {
                                "ru": "<w>Мы</w>, канадцы, любим считать себя особенными."
                            }
                        },
                        {
                            "text": "<w>We</w> the undersigned wish to express our disapproval.",
                            "target_lang_translations": {
                                "ru": "<w>Мы</w>, нижеподписавшиеся, хотим выразить свое неодобрение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "we the people"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется перед существительным, чтобы показать принадлежность говорящего к определенной категории людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мы",
                                "target_lang_sense_clarification": "определительное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9bbed660-142e-35f4-969d-5713fed96fba",
                    "sense_definition": "Initialism of Western Europe.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name_group",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The study compares economic growth in the US and <w>WE</w>.",
                            "target_lang_translations": {
                                "ru": "Исследование сравнивает экономический рост в США и <w>Западной Европе</w>."
                            }
                        },
                        {
                            "text": "Travel restrictions apply to several <w>WE</w> countries.",
                            "target_lang_translations": {
                                "ru": "Ограничения на поездки распространяются на несколько стран <w>Западной Европы</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от Western Europe (Западная Европа)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Западная Европа",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ffad709a-217e-35d1-8611-cd8677e8bf94",
                    "sense_definition": "Initialism of war establishment.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The regiment was brought up to full <w>WE</w> before deployment.",
                            "target_lang_translations": {
                                "ru": "Полк был доукомплектован до полного <w>штата военного времени</w> перед отправкой."
                            }
                        },
                        {
                            "text": "Changes to the <w>WE</w> were implemented last month.",
                            "target_lang_translations": {
                                "ru": "Изменения в <w>штате военного времени</w> были внедрены в прошлом месяце."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "military initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от War Establishment (штат военного времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штат военного времени",
                                "target_lang_sense_clarification": "военный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f97c81f1-5382-328e-b55d-b560021242cc",
                    "sense_definition": "A fixed amount of money paid to a worker, usually calculated on a monthly or annual basis.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "money_payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was happy to accept the job because of the competitive <w>salary</w>.",
                            "target_lang_translations": {
                                "ru": "Она была рада принять предложение о работе из-за конкурентной <w>зарплаты</w>."
                            }
                        },
                        {
                            "text": "The manager receives his monthly <w>salary</w> directly into his bank account.",
                            "target_lang_translations": {
                                "ru": "Менеджер получает свою ежемесячную <w>зарплату</w> прямо на банковский счет."
                            }
                        }
                    ],
                    "synonyms": [
                        "pay",
                        "earnings",
                        "income"
                    ],
                    "antonyms": [
                        "wage"
                    ],
                    "common_phrases": [
                        "annual salary",
                        "monthly salary",
                        "basic salary",
                        "competitive salary",
                        "earn a salary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фиксированная сумма денег, выплачиваемая работнику, обычно рассчитываемая на ежемесячной или ежегодной основе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарплата",
                                "target_lang_sense_clarification": "самое распространенное, общеупотребительное слово"
                            },
                            {
                                "target_lang_word": "оклад",
                                "target_lang_sense_clarification": "фиксированная часть зарплаты без учета премий и надбавок"
                            },
                            {
                                "target_lang_word": "заработная плата",
                                "target_lang_sense_clarification": "официальный, формальный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ed8e74aa-92fe-3aae-a146-e4167358d407",
                    "sense_definition": "To pay an employee on the basis of a fixed period rather than hourly.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "money_payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company decided to <w>salary</w> the new staff members to ensure their financial security.",
                            "target_lang_translations": {
                                "ru": "Компания решила <w>назначить оклад</w> новым сотрудникам, чтобы обеспечить их финансовую безопасность."
                            }
                        },
                        {
                            "text": "It is uncommon to <w>salary</w> temporary workers in this industry.",
                            "target_lang_translations": {
                                "ru": "В этой отрасли не принято <w>платить оклад</w> временным работникам."
                            }
                        }
                    ],
                    "synonyms": [
                        "pay"
                    ],
                    "target_lang_definitions": {
                        "ru": "Платить сотруднику на основе фиксированного периода, а не почасово."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платить оклад",
                                "target_lang_sense_clarification": "выплачивать фиксированное вознаграждение"
                            },
                            {
                                "target_lang_word": "назначить оклад",
                                "target_lang_sense_clarification": "перевести на фиксированную оплату труда"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6de84f96-de3a-3da3-bd06-18c345c5ff91",
                    "sense_definition": "Saline; pertaining to or containing salt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient text described the <w>salary</w> taste of the sea water.",
                            "target_lang_translations": {
                                "ru": "Древний текст описывал <w>солёный</w> вкус морской воды."
                            }
                        },
                        {
                            "text": "He noted the <w>salary</w> quality of the soil near the coast.",
                            "target_lang_translations": {
                                "ru": "Он отметил <w>солевое</w> качество почвы у побережья."
                            }
                        }
                    ],
                    "synonyms": [
                        "saline",
                        "salty"
                    ],

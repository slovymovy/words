            "senses": [
                {
                    "sense_id": "508b9415-ebb5-31d9-b569-4a72ec925c11",
                    "sense_definition": "In a polite, proper, and serious way, often referring to clothing or social behavior.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was dressed too <w>formally</w> for the casual dinner party.",
                            "target_lang_translations": {
                                "ru": "Он был одет слишком <w>официально</w> для неофициального ужина."
                            }
                        },
                        {
                            "text": "You should speak <w>formally</w> when addressing the committee members.",
                            "target_lang_translations": {
                                "ru": "Вам следует говорить <w>официально</w>, обращаясь к членам комитета."
                            }
                        }
                    ],
                    "antonyms": [
                        "informally"
                    ],
                    "common_phrases": [
                        "dressed formally",
                        "speak formally",
                        "act formally"
                    ],
                    "target_lang_definitions": {
                        "ru": "В вежливой, официальной манере, часто относится к одежде или социальному поведению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "официально",
                                "target_lang_sense_clarification": "соблюдая этикет или дресс-код"
                            }

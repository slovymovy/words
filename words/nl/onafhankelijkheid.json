            "senses": [
                {
                    "sense_id": "7df3d106-7ea2-3a21-b60a-92c14de88eb6",
                    "sense_definition": "De toestand waarin een land of staat volledig soeverein is en niet door een ander land wordt bestuurd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Soms wenst een staat <w>onafhankelijkheid</w> te verkrijgen door middel van een onafhankelijkheidsverklaring.",
                            "target_lang_translations": {
                                "en": "Sometimes a state wishes to obtain <w>independence</w> by means of a declaration of independence.",
                                "ru": "Иногда государство желает обрести <w>независимость</w> посредством декларации независимости."
                            }
                        },
                        {
                            "text": "De kolonie vocht jarenlang voor haar <w>onafhankelijkheid</w> van het moederland.",
                            "target_lang_translations": {
                                "en": "The colony fought for years for its <w>independence</w> from the motherland.",
                                "ru": "Колония годами боролась за свою <w>независимость</w> от метрополии."
                            }
                        }
                    ],
                    "synonyms": [
                        "soevereiniteit",
                        "zelfstandigheid"
                    ],
                    "antonyms": [
                        "afhankelijkheid"
                    ],
                    "common_phrases": [
                        "onafhankelijkheid uitroepen",
                        "strijd voor onafhankelijkheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The state in which a country or state is fully sovereign and not governed by another country.",
                        "ru": "Состояние, при котором страна или государство полностью суверенны и не управляются другой страной."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "independence",
                                "target_lang_sense_clarification": "political freedom"
                            },
                            {
                                "target_lang_word": "sovereignty",
                                "target_lang_sense_clarification": "supreme authority"
                            },
                            {
                                "target_lang_word": "autonomy",
                                "target_lang_sense_clarification": "self-government"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "независимость",
                                "target_lang_sense_clarification": "политическая свобода"
                            },
                            {
                                "target_lang_word": "суверенитет",
                                "target_lang_sense_clarification": "верховная власть государства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "421ed16a-7763-3b19-a1e9-eba7b0c603f3",
                    "sense_definition": "De situatie waarin iemand of iets niet afhankelijk is van de steun, invloed of hulp van anderen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij hecht veel waarde aan haar financiële <w>onafhankelijkheid</w> en spaart elke maand.",
                            "target_lang_translations": {
                                "en": "She attaches great value to her financial <w>independence</w> and saves every month.",
                                "ru": "Она придает большое значение своей финансовой <w>независимости</w> и откладывает деньги каждый месяц."
                            }
                        },
                        {
                            "text": "De <w>onafhankelijkheid</w> van de journalistiek is belangrijk voor objectief nieuws.",
                            "target_lang_translations": {
                                "en": "The <w>independence</w> of journalism is important for objective news.",
                                "ru": "<w>Независимость</w> журналистики важна для объективных новостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "zelfstandigheid",
                        "vrijheid"
                    ],
                    "antonyms": [
                        "afhankelijkheid"
                    ],
                    "common_phrases": [
                        "financiële onafhankelijkheid",
                        "persoonlijke onafhankelijkheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The situation in which someone or something is not dependent on the support, influence, or help of others.",
                        "ru": "Ситуация, в которой кто-либо или что-либо не зависит от поддержки, влияния или помощи других."
                    },
                    "translations": {

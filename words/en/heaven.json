{
    "word_family": [
        "heavenly",
        "heavenward",
        "heavenliness"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "03ee5ae7-9c5a-331c-ad2c-69be46c625d0",
                    "sense_definition": "The abode of God, the angels, and the spirits of the righteous after death; the state of being in the presence of God.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "religious_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She believes that her grandmother is watching over her from <w>heaven</w>.",
                            "target_lang_translations": {
                                "ru": "Она верит, что бабушка наблюдает за ней с <w>небес</w>."
                            }
                        },
                        {
                            "text": "Many religions teach that good people go to <w>heaven</w> when they die.",
                            "target_lang_translations": {
                                "ru": "Многие религии учат, что хорошие люди попадают в <w>рай</w> после смерти."
                            }
                        }
                    ],
                    "synonyms": [
                        "paradise",
                        "kingdom come",
                        "pearly gates"
                    ],
                    "antonyms": [
                        "hell"
                    ],
                    "common_phrases": [
                        "go to heaven",
                        "heaven and hell"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often capitalized as Heaven"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обитель Бога, ангелов и душ праведников после смерти; состояние пребывания в присутствии Бога."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рай",
                                "target_lang_sense_clarification": "место вечного блаженства"
                            },
                            {
                                "target_lang_word": "небеса",
                                "target_lang_sense_clarification": "обитель Бога"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5edc234-9ef2-3c4d-a4d1-f7e57181055d",
                    "sense_definition": "A state of supreme happiness, delight, or ecstasy; a place or experience that brings great joy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lying on the beach with a cold drink was absolute <w>heaven</w>.",
                            "target_lang_translations": {
                                "ru": "Лежать на пляже с холодным напитком было настоящим <w>блаженством</w>."
                            }
                        },
                        {
                            "text": "It was <w>heaven</w> on earth to finally relax after such a long week.",
                            "target_lang_translations": {
                                "ru": "Наконец-то расслабиться после такой длинной недели было <w>раем</w> на земле."
                            }
                        }
                    ],
                    "synonyms": [
                        "paradise",
                        "delight",
                        "dream"
                    ],
                    "antonyms": [
                        "hell",
                        "horror",
                        "nightmare"
                    ],
                    "common_phrases": [
                        "heaven on earth",
                        "in seventh heaven",
                        "match made in heaven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние высшего счастья, восторга или экстаза; место или опыт, приносящие огромную радость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блаженство",
                                "target_lang_sense_clarification": "высшее наслаждение"
                            },
                            {
                                "target_lang_word": "рай",
                                "target_lang_sense_clarification": "перен. счастье, восторг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f4332b3-ff5d-3885-84c5-7adb2c0f087a",
                    "sense_definition": "God, fate, or providence; often used in exclamations to express surprise, emphasis, or a hope.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "personification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Good <w>heavens</w>, I did not expect to see you here so early!",
                            "target_lang_translations": {
                                "ru": "Боже <w>правый</w>, я не ожидал увидеть вас здесь так рано!"
                            }
                        },
                        {
                            "text": "<w>Heaven</w> forbid that something bad should happen to them on the trip.",
                            "target_lang_translations": {
                                "ru": "<w>Боже</w> упаси, чтобы с ними случилось что-то плохое в поездке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "thank heaven",
                        "good heavens",
                        "heaven forbid",
                        "heaven knows",
                        "heaven help us"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "capitalized as Heaven"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бог, судьба или провидение; часто используется в восклицаниях для выражения удивления, акцента или надежды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "небеса",
                                "target_lang_sense_clarification": "высшие силы"
                            },
                            {
                                "target_lang_word": "Бог",
                                "target_lang_sense_clarification": "в устойчивых выражениях"
                            },
                            {
                                "target_lang_word": "провидение",
                                "target_lang_sense_clarification": "судьба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60a09e24-661e-3677-9ca8-5986ea28953a",
                    "sense_definition": "The sky, the firmament, or the expanse of space surrounding the earth.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>heavens</w> opened and a heavy rain began to fall upon the city.",
                            "target_lang_translations": {
                                "ru": "<w>Небеса</w> разверзлись, и на город обрушился сильный ливень."
                            }
                        },
                        {
                            "text": "Astronomers spend their nights observing the stars and planets in the <w>heavens</w>.",
                            "target_lang_translations": {
                                "ru": "Астрономы проводят ночи, наблюдая за звездами и планетами в <w>небесах</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sky",
                        "firmament",
                        "welkin"
                    ],
                    "common_phrases": [
                        "open the heavens",
                        "move heaven and earth"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or poetic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небо, небосвод или космическое пространство, окружающее землю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "небеса",
                                "target_lang_sense_clarification": "небосвод"
                            },
                            {
                                "target_lang_word": "небо",
                                "target_lang_sense_clarification": "атмосфера, космос"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cffaee2d-0e9d-3140-bed2-8db6241e32c5",
                    "sense_definition": "A female given name.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anthroponym",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My friend <w>Heaven</w> is coming to the party tonight.",
                            "target_lang_translations": {
                                "ru": "Моя подруга <w>Хэвен</w> придет сегодня на вечеринку."
                            }
                        },
                        {
                            "text": "They decided to name their newborn daughter <w>Heaven</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать свою новорожденную дочь <w>Хэвен</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское личное имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хэвен",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0e3b194-ae2c-3172-b2cf-88b0f8176948",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "anthroponym",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Heaven</w> signed the legal documents yesterday.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Хэвен</w> подписал юридические документы вчера."
                            }
                        },
                        {
                            "text": "We visited the <w>Heaven</w> family at their estate.",
                            "target_lang_translations": {
                                "ru": "Мы навестили семью <w>Хэвен</w> в их поместье."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хэвен",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
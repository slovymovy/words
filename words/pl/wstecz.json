                {
                    "sense_id": "0e9baa3f-1eee-3f5b-9a0f-922608697e01",
                    "sense_definition": "Określenie wskazujące na odcinek czasu, który upłynął od jakiegoś wydarzenia do chwili obecnej; temu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_ago",
                    "name_type": "no",
                    "examples": [

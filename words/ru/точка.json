                    "frequency": "Middle",
                    "semantic_group_id": "state_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вода достигла <w>точки</w> кипения и начала превращаться в пар.",
                            "target_lang_translations": {
                                "en": "The water reached the boiling <w>point</w> and started turning into steam."
                            }

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "17e7fdff-d82b-3b07-a250-8c7a080a6a2a",
                    "sense_definition": "Available as another possibility or different from the one first considered.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "choice_possibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to take an <w>alternative</w> route to avoid the heavy traffic.",
                            "target_lang_translations": {
                                "ru": "Мы решили выбрать <w>альтернативный</w> маршрут, чтобы избежать пробок."
                            }
                        },
                        {
                            "text": "Solar power is becoming a popular <w>alternative</w> energy source for many homes.",
                            "target_lang_translations": {
                                "ru": "Солнечная энергия становится популярным <w>альтернативным</w> источником энергии для многих домов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "alternative energy",
                        "alternative route",
                        "alternative solution",
                        "alternative method"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доступный как другой вариант или отличный от того, что рассматривался вначале"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "альтернати́вный",
                                "target_lang_sense_clarification": "иной, запасной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e8bc617-9302-34aa-aa16-3fe903554969",
                    "sense_definition": "Not traditional, outside the mainstream, or belonging to a counterculture.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "non_traditional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many people are turning to <w>alternative</w> medicine to treat chronic illnesses.",
                            "target_lang_translations": {
                                "ru": "Многие люди обращаются к <w>альтернативной</w> медицине для лечения хронических заболеваний."
                            }
                        },
                        {
                            "text": "The radio station plays mostly <w>alternative</w> rock from the nineties.",
                            "target_lang_translations": {
                                "ru": "Эта радиостанция играет в основном <w>альтернативный</w> рок девяностых."
                            }
                        }
                    ],
                    "common_phrases": [
                        "alternative medicine",
                        "alternative lifestyle",
                        "alternative rock",
                        "alternative media"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нетрадиционный, выходящий за рамки мейнстрима или принадлежащий к контркультуре"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "альтернати́вный",
                                "target_lang_sense_clarification": "нетрадиционный"
                            },
                            {
                                "target_lang_word": "нетрадицио́нный",
                                "target_lang_sense_clarification": "о медицине, методах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "094bffe4-dacd-342b-a583-c07d0070672e",
                    "sense_definition": "Presenting two or more mutually exclusive choices in grammar.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_choice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The word \"or\" is an example of an <w>alternative</w> conjunction.",
                            "target_lang_translations": {
                                "ru": "Слово \"или\" — это пример <w>разделительного</w> союза."
                            }
                        },
                        {
                            "text": "In this sentence, the <w>alternative</w> structure forces the reader to choose one meaning.",
                            "target_lang_translations": {
                                "ru": "В этом предложении <w>альтернативная</w> структура заставляет читателя выбрать одно значение."
                            }
                        }
                    ],
                    "synonyms": [
                        "disjunctive"
                    ],
                    "common_phrases": [
                        "alternative conjunction"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "grammar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлагающий два или более взаимоисключающих варианта (в грамматике)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздели́тельный",

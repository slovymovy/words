            "pos": "verb",
            "senses": [
                {
                    "sense_id": "783733d6-da9d-3a06-a499-525b9e068793",
                    "sense_definition": "Przyjmować płyn do ust i połykać go; spożywać napoje.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano <piję> mocną kawę z mlekiem i cukrem.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <пью> крепкий кофе с молоком и сахаром."
                            }
                        },
                        {
                            "text": "Lekarz zalecił mi, abym <pił> dużo wody mineralnej.",
                            "target_lang_translations": {
                                "ru": "Врач посоветовал мне <пить> много минеральной воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "sączyć",
                        "łykać"
                    ],
                    "common_phrases": [
                        "pić wodę",
                        "pić kawę",
                        "pić herbatę",
                        "pić duszkiem",
                        "pić przez słomkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать жидкость в рот и глотать её; употреблять напитки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пить",
                                "target_lang_sense_clarification": "глотать жидкость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "737632bd-5b44-348e-b730-8da7d42857b9",
                    "sense_definition": "Spożywać napoje alkoholowe, często w nadmiarze lub nałogowo.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na weselu goście <pili> szampana za zdrowie młodej pary.",
                            "target_lang_translations": {
                                "ru": "На свадьбе гости <пили> шампанское за здоровье молодой пары."
                            }
                        },
                        {
                            "text": "Mój sąsiad niestety za dużo <pije> i ma przez to problemy.",
                            "target_lang_translations": {
                                "ru": "Мой сосед, к сожалению, слишком много <пьет>, и из-за этого у него проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "chlać",
                        "tankować",
                        "zaglądać do kieliszka",
                        "drinkować"
                    ],
                    "common_phrases": [
                        "pić alkohol",
                        "pić na umór",
                        "pić do lustra",
                        "pić za zdrowie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Употреблять алкогольные напитки, часто в избытке или как вредная привычка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пить",
                                "target_lang_sense_clarification": "употреблять алкоголь"
                            },
                            {
                                "target_lang_word": "выпивать",
                                "target_lang_sense_clarification": "употреблять спиртное"
                            },
                            {
                                "target_lang_word": "пьянствовать",
                                "target_lang_sense_clarification": "злоупотреблять алкоголем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60d73a2e-a449-3f33-a018-6dfb0eb39b40",
                    "sense_definition": "O obuwiu lub odzieży: być zbyt ciasnym, powodować ból przez ucisk.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę zmienić te buty, bo strasznie mnie <piją> w pięty.",
                            "target_lang_translations": {
                                "ru": "Мне нужно сменить эти туфли, потому что они ужасно <жмут> мне пятки."
                            }
                        },
                        {
                            "text": "Kupiłem nowe spodnie, ale niestety <piją> mnie w pasie.",
                            "target_lang_translations": {
                                "ru": "Я купил новые брюки, но, к сожалению, они <жмут> мне в поясе."
                            }
                        }
                    ],
                    "synonyms": [
                        "uwierać",
                        "gnieść",
                        "uciskać"
                    ],
                    "common_phrases": [

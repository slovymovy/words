    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e827938b-63eb-3f1c-9edb-5dfdb8a34755",
                    "sense_definition": "Kobieta wykonująca zawód artystyczny, która gra rolę w teatrze, filmie lub telewizji.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "theater_film",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta młoda <aktorka> zagrała główną rolę w nowym serialu telewizyjnym.",
                            "target_lang_translations": {
                                "ru": "Эта молодая <актриса> сыграла главную роль в новом телесериале."
                            }
                        },
                        {
                            "text": "Moja siostra marzy o tym, by zostać sławną <aktorką> filmową.",
                            "target_lang_translations": {
                                "ru": "Моя сестра мечтает стать известной <актрисой> кино."
                            }
                        }
                    ],
                    "synonyms": [
                        "artystka"
                    ],
                    "antonyms": [
                        "widz"
                    ],
                    "common_phrases": [
                        "aktorka filmowa",
                        "aktorka teatralna",
                        "znana aktorka",
                        "aktorka drugoplanowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина творческой профессии, исполняющая роли в театре, кино или на телевидении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "актриса",
                                "target_lang_sense_clarification": "профессия"
                            },
                            {
                                "target_lang_word": "артистка",
                                "target_lang_sense_clarification": "исполнительница ролей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d4885ea-3e2d-312d-b20b-536cf64e5df9",
                    "sense_definition": "Kobieta, która zachowuje się nienaturalnie, udając kogoś innego lub nieszczerze okazując uczucia.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wierz w jej łzy, to po prostu niezła <aktorka>.",
                            "target_lang_translations": {
                                "ru": "Не верь её слезам, она просто неплохая <актриса>."
                            }
                        },
                        {
                            "text": "Zachowuje się jak <aktorka>, próbując wymusić na nas zmianę decyzji.",
                            "target_lang_translations": {
                                "ru": "Она ведет себя как <актриса>, пытаясь заставить нас изменить решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "symulantka",
                        "udawaczka",
                        "hipokrytka"
                    ],
                    "common_phrases": [
                        "niezła aktorka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w przenośni"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, которая ведет себя неестественно, притворяясь кем-то другим или неискренне выражая чувства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "актриса",
                                "target_lang_sense_clarification": "притворщица (перен.)"
                            },
                            {
                                "target_lang_word": "притворщица",
                                "target_lang_sense_clarification": "синоним"
                            },
                            {
                                "target_lang_word": "симулянтка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6887c4bf-1214-3b4f-b338-ce5d899c824b",
                    "sense_definition": "Uczestniczka biorąca czynny udział w jakimś wydarzeniu lub sytuacji (użycie metaforyczne).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "participant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Była jedną z głównych <aktorek> wydarzeń politycznych tamtego roku.",
                            "target_lang_translations": {
                                "ru": "Она была одной из главных <участниц> политических событий того года."
                            }
                        },
                        {
                            "text": "Jako ważna <aktorka> sceny gospodarczej miała wpływ na rynek.",
                            "target_lang_translations": {
                                "ru": "Как важное <действующее лицо> экономической сцены, она имела влияние на рынок."
                            }
                        }
                    ],
                    "synonyms": [
                        "uczestniczka"
                    ],
                    "antonyms": [
                        "obserwatorka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "użycie książkowe lub publicystyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Участница, принимающая активное участие в каком-либо событии или ситуации (метафорическое значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участница",
                                "target_lang_sense_clarification": "событий"
                            },
                            {
                                "target_lang_word": "действующее лицо",
                                "target_lang_sense_clarification": "фигура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20db5525-4bb9-3261-a4cd-fbfb23101fc3",
                    "sense_definition": "Kobieta wnosząca powództwo do sądu (znaczenie historyczne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych dokumentach sądowych zapisano ją jako <aktorkę> sporu.",
                            "target_lang_translations": {
                                "ru": "В старых судебных документах она была записана как <истица> в споре."
                            }
                        },
                        {
                            "text": "Ówczesna <aktorka> musiała przedstawić dowody przed trybunałem.",
                            "target_lang_translations": {
                                "ru": "Тогдашняя <истица> должна была представить доказательства перед судом."
                            }
                        }
                    ],
                    "synonyms": [
                        "powódka"
                    ],
                    "antonyms": [
                        "pozwana"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dawne terminologia prawnicza"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, подающая иск в суд (историческое значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "истица",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

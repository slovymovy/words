            "senses": [
                {
                    "sense_id": "fa109eb9-182e-3cf9-b19f-a159ab2513f1",
                    "sense_definition": "Osoba, która kosi zboże lub trawę, zazwyczaj ręcznie za pomocą kosy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczony <kośnik> oparł się o drzewo, by odpocząć po ciężkiej pracy.",
                            "target_lang_translations": {
                                "ru": "Уставший <косарь> прислонился к дереву, чтобы отдохнуть после тяжелой работы."
                            }
                        },
                        {
                            "text": "Na polu pracowali <kośnicy>, rytmicznie machając ostrymi kosami.",
                            "target_lang_translations": {
                                "ru": "На поле работали <косари>, ритмично взмахивая острыми косами."
                            }
                        }
                    ],
                    "synonyms": [
                        "kosiarz"
                    ],
                    "common_phrases": [
                        "wprawny kośnik",
                        "praca kośnika"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie częściej używa się słowa kosiarz"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "występuje w gwarach i dialektach"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который косит зерновые или траву, обычно вручную с помощью косы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "косарь",
                                "target_lang_sense_clarification": "человек, работающий косой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72c5936e-6142-32ad-8add-c1a95d06adb7",
                    "sense_definition": "Hoacyn; gatunek tropikalnego ptaka z Ameryki Południowej, wyróżniający się czubem na głowie i specyficznym zapachem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Kośnik> jest znany z nietypowego dla ptaków układu trawiennego.",
                            "target_lang_translations": {
                                "ru": "<Гоацин> известен нетипичной для птиц пищеварительной системой."
                            }
                        },
                        {
                            "text": "W gęstwinie lasu deszczowego dostrzegliśmy barwnego <kośnika>.",
                            "target_lang_translations": {
                                "ru": "В чаще дождевого леса мы заметили красочного <гоацина>."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoacyn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гоацин; вид тропической птицы из Южной Америки, отличающийся хохолком на голове и специфическим запахом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гоацин",
                                "target_lang_sense_clarification": "вид птицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "216490ea-3bcf-358a-9422-802ade39e8b8",
                    "sense_definition": "Ozdobna wstążka wplatana w warkocz, będąca elementem stroju ludowego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda dziewczyna wplotła w warkocz błękitny <kośnik>.",
                            "target_lang_translations": {
                                "ru": "Молодая девушка вплела в косу голубой <косник>."
                            }
                        },
                        {
                            "text": "Jej świąteczny strój zdobił jedwabny <kośnik>.",
                            "target_lang_translations": {
                                "ru": "Её праздничный наряд украшал шелковый <косник>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kolorowy kośnik"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy dawnych strojów"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "termin etnograficzny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративная лента, вплетаемая в косу, являющаяся элементом народного костюма."
                    },

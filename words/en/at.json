                },
                {
                    "sense_id": "1ad8adc7-5b80-38f7-a83a-13cc0e61ef81",
                    "sense_definition": "Used to indicate an area of skill, ability, or activity.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "skill_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is very good <w>at</w> playing the guitar and singing.",
                            "target_lang_translations": {
                                "ru": "Он очень хорош <w>в</w> игре на гитаре и пении."
                            }
                        },
                        {
                            "text": "I have always been terrible <w>at</w> remembering people's names.",
                            "target_lang_translations": {
                                "ru": "Я всегда был ужасен <w>в</w> запоминании имен людей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "good at",
                        "bad at",
                        "excellent at",
                        "terrible at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания области навыков, способностей или деятельности."

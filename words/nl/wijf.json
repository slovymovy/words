    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "da6fda88-00e6-3ffa-b311-40236efb63ae",
                    "sense_definition": "Een benaming voor een vrouw die vaak als onbeleefd, minachtend of zeer informeel wordt ervaren.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat stomme <wijf> heeft weer niet naar ons geluisterd.",
                            "target_lang_translations": {
                                "en": "That stupid <w>bitch</w> didn't listen to us again.",
                                "ru": "Эта тупая <баба> опять нас не послушала."
                            }
                        },
                        {
                            "text": "Hij keek zijn ogen uit naar dat lekkere <wijf> aan de bar.",
                            "target_lang_translations": {
                                "en": "He couldn't take his eyes off that hot <w>chick</w> at the bar.",
                                "ru": "Он во все глаза смотрел на ту классную <тёлку> у бара."
                            }
                        }
                    ],
                    "synonyms": [
                        "trut",
                        "feeks",
                        "teef"
                    ],
                    "antonyms": [
                        "kerel",
                        "man"
                    ],
                    "common_phrases": [
                        "lekker-wijf",
                        "stom-wijf",
                        "oud-wijf"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "vaak pejoratief"
                        },
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A term for a woman often perceived as rude, derogatory, or very informal.",
                        "ru": "Название женщины, которое часто воспринимается как грубое, пренебрежительное или очень неформальное."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bitch",
                                "target_lang_sense_clarification": "derogatory, rude"
                            },
                            {
                                "target_lang_word": "chick",
                                "target_lang_sense_clarification": "informal, often referring to attractiveness"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "баба",
                                "target_lang_sense_clarification": "грубое или просторечное название женщины"
                            },
                            {
                                "target_lang_word": "тёлка",
                                "target_lang_sense_clarification": "сленг, часто о привлекательной женщине"
                            },
                            {
                                "target_lang_word": "стерва",
                                "target_lang_sense_clarification": "ругательное, о неприятной женщине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c2e6742-6acf-3a90-a31f-e1e2cd702d3d",
                    "sense_definition": "De vrouw met wie een man getrouwd is (in verouderd taalgebruik).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude verhalen leest men vaak over een visser en zijn <wijf>.",
                            "target_lang_translations": {
                                "en": "In old stories, one often reads about a fisherman and his <w>wife</w>.",
                                "ru": "В старых сказках часто читают о рыбаке и его <жене>."
                            }
                        },
                        {
                            "text": "Hij nam haar tot zijn <wijf> en zij leefden nog lang en gelukkig.",
                            "target_lang_translations": {
                                "en": "He took her as his <w>wife</w> and they lived happily ever after.",
                                "ru": "Он взял её в <жёны>, и жили они долго и счастливо."
                            }
                        }
                    ],
                    "synonyms": [
                        "echtgenote",
                        "gade"
                    ],
                    "antonyms": [
                        "echtgenoot",
                        "man"
                    ],
                    "common_phrases": [
                        "tot-wijf-nemen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The woman a man is married to (in archaic usage).",
                        "ru": "Женщина, с которой мужчина состоит в браке (в устаревшем употреблении)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wife",
                                "target_lang_sense_clarification": "archaic or dialectal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "жена",
                                "target_lang_sense_clarification": "устаревшее словоупотребление в значении \"супруга\""
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "24ec227b-bc0d-3f8e-86b0-088b96aba7f4",
                    "sense_definition": "To, co znajduje się wewnątrz jakiegoś obiektu, naczynia lub przestrzeni.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Celnicy dokładnie sprawdzili <zawartość> walizki podróżnego.",
                            "target_lang_translations": {
                                "ru": "Таможенники тщательно проверили <w>содержимое</w> чемодана путешественника."
                            }
                        },
                        {
                            "text": "Wylała całą <zawartość> butelki do zlewu, ponieważ napój był zepsuty.",
                            "target_lang_translations": {
                                "ru": "Она вылила всё <w>содержимое</w> бутылки в раковину, так как напиток был испорчен."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zawartość pudełka",
                        "zawartość przesyłki",
                        "zawartość kieszeni",
                        "zawartość żołądka",
                        "cenna zawartość"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что находится внутри какого-либо объекта, сосуда или пространства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "содержимое",
                                "target_lang_sense_clarification": "то, что находится внутри чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b535b7a-563c-34c3-a8a3-3259a5bf3201",
                    "sense_definition": "Ilość lub udział określonego składnika w danej substancji lub produkcie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "proportion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten produkt spożywczy ma bardzo wysoką <zawartość> cukru.",
                            "target_lang_translations": {
                                "ru": "В этом продукте питания очень высокое <w>содержание</w> сахара."
                            }
                        },
                        {
                            "text": "Policja zmierzyła kierowcy <zawartość> alkoholu we krwi.",
                            "target_lang_translations": {
                                "ru": "Полиция измерила у водителя <w>содержание</w> алкоголя в крови."
                            }
                        }
                    ],
                    "synonyms": [
                        "ilość",
                        "stężenie",
                        "procent",
                        "odsetek",
                        "promil"
                    ],
                    "common_phrases": [
                        "zawartość alkoholu",
                        "zawartość tłuszczu",
                        "zawartość błonnika",
                        "niska zawartość",
                        "wysoka zawartość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество или доля определенного компонента в данном веществе или продукте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "содержание",
                                "target_lang_sense_clarification": "наличие или количество вещества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba92f28d-a298-3aae-8505-a0c76e751efc",
                    "sense_definition": "Treść, temat lub informacje zawarte w tekście, dokumencie, dziele kultury lub mediach cyfrowych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel ocenił <zawartość> merytoryczną mojej pracy dyplomowej.",
                            "target_lang_translations": {
                                "ru": "Учитель оценил смысловое <w>содержание</w> моей дипломной работы."
                            }
                        },
                        {
                            "text": "Rodzice powinni kontrolować <zawartość>, jaką ich dzieci oglądają w internecie.",
                            "target_lang_translations": {
                                "ru": "Родители должны контролировать <w>контент</w>, который их дети смотрят в интернете."
                            }
                        }
                    ],
                    "synonyms": [
                        "treść",
                        "tematyka",
                        "problematyka",
                        "fabuła"
                    ],
                    "common_phrases": [
                        "zawartość strony",
                        "zawartość pliku",
                        "zawartość merytoryczna",
                        "unikalna zawartość",
                        "dostęp do zawartości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суть, тема или информация, содержащаяся в тексте, документе, произведении культуры или цифровых медиа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "содержание",
                                "target_lang_sense_clarification": "суть текста, книги или документа"
                            },
                            {
                                "target_lang_word": "контент",
                                "target_lang_sense_clarification": "информация в интернете или медиа"
                            }
                        ]
                    }
                }
            ]
        }

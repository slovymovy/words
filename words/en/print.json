                        "photographic print"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображение или рисунок, воспроизведенный во множестве копий (например, литография или фотография)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "репродукция",
                                "target_lang_sense_clarification": "копия картины"
                            },
                            {
                                "target_lang_word": "эстамп",
                                "target_lang_sense_clarification": "гравюра"
                            },
                            {
                                "target_lang_word": "отпечаток",
                                "target_lang_sense_clarification": "фотография"
                            }
                        ]
                    }

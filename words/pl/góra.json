                },
                {
                    "sense_id": "aad3596a-db81-3e51-8b5d-6c43a3d1d9f7",
                    "sense_definition": "Wyższa kondygnacja budynku, piętro lub strych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci bawią się na <w>górze</w> w swoim pokoju.",
                            "target_lang_translations": {
                                "ru": "Дети играют <w>наверху</w> в своей комнате."
                            }
                        },
                        {
                            "text": "Zaniosłem czyste pranie na <w>górę</w>, do sypialni.",
                            "target_lang_translations": {
                                "ru": "Я отнес чистое белье <w>наверх</w>, в спальню."
                            }
                        }
                    ],
                    "antonyms": [
                        "dół"
                    ],
                    "common_phrases": [

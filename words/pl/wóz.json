                            },
                            {
                                "target_lang_word": "машина",
                                "target_lang_sense_clarification": "разговорное название автомобиля"
                            },
                            {
                                "target_lang_word": "авто",
                                "target_lang_sense_clarification": "сокращение от автомобиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58a0cbc8-c961-331b-afd8-03f57e086c5a",
                    "sense_definition": "Ilość materiału lub towaru, która mieści się na wozie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz przywiózł z lasu cały <w>wóz</w> drewna na opał.",
                            "target_lang_translations": {
                                "ru": "Хозяин привез из леса целый <w>воз</w> дров на растопку."
                            }
                        },
                        {
                            "text": "Z pola zwieziono ostatni <w>wóz</w> tegorocznego zboża.",
                            "target_lang_translations": {
                                "ru": "С поля вывезли последний <w>воз</w> урожая этого года."
                            }
                        }
                    ],
                    "common_phrases": [

                            }
                        },
                        {
                            "text": "Lekarz poprosił pacjenta, aby stanął na <w>wadze</w> bez butów.",
                            "target_lang_translations": {
                                "ru": "Врач попросил пациента встать на <w>весы</w> без обуви."
                            }
                        }
                    ],
                    "common_phrases": [
                        "waga łazienkowa",
                        "waga kuchenna",
                        "waga szalkowa",
                        "stanąć na wadze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибор для измерения массы (веса) тел."

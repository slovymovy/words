    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e34c1377-246e-30b6-bc4d-4cce820c84ff",
                    "sense_definition": "Działanie mające na celu przeciwstawienie się jakiejś sile, władzy, przemocy lub woli innej osoby; aktywny sprzeciw.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "resistance_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze stawiali zacięty <w>opór</w> atakującym wojskom nieprzyjaciela.",
                            "target_lang_translations": {
                                "ru": "Солдаты оказывали ожесточенное <w>сопротивление</w> атакующим войскам противника."
                            }
                        },
                        {
                            "text": "Wprowadzenie nowych przepisów napotkało silny <w>opór</w> ze strony społeczeństwa.",
                            "target_lang_translations": {
                                "ru": "Введение новых правил встретило сильное <w>сопротивление</w> со стороны общества."
                            }
                        }
                    ],
                    "synonyms": [
                        "zanokcica",
                        "zanokcica"
                    ],
                    "antonyms": [
                        "uległość",
                        "poddanie się"
                    ],
                    "common_phrases": [
                        "stawiać opór",
                        "ruch oporu",
                        "linia najmniejszego oporu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, направленное на противодействие какой-либо силе, власти или насилию; активное противостояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопротивление",
                                "target_lang_sense_clarification": "противодействие силе или власти"
                            },
                            {
                                "target_lang_word": "отпор",
                                "target_lang_sense_clarification": "активное отражение нападения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc535367-7648-33d2-8658-7554da4276fb",
                    "sense_definition": "Siła fizyczna, która przeciwdziała ruchowi ciała lub przepływowi prądu elektrycznego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "resistance_physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód zużywa więcej paliwa, gdy musi pokonać duży <w>opór</w> powietrza.",
                            "target_lang_translations": {
                                "ru": "Автомобиль расходует больше топлива, когда ему приходится преодолевать большое <w>сопротивление</w> воздуха."
                            }
                        },
                        {
                            "text": "Miedź jest materiałem, który stawia bardzo mały <w>opór</w> elektryczny.",
                            "target_lang_translations": {
                                "ru": "Медь — это материал, который оказывает очень малое электрическое <w>сопротивление</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "opór powietrza",
                        "opór elektryczny",
                        "opór materii"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая сила, препятствующая движению тела или протеканию электрического тока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопротивление",
                                "target_lang_sense_clarification": "физическая величина или сила"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f64b9164-f146-392d-9ceb-e61b5e3df7ca",
                    "sense_definition": "Wewnętrzne poczucie niechęci, wahania lub wątpliwości przed wykonaniem jakiejś czynności (często używane w liczbie mnogiej).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "resistance_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miał duże <w>opory</w> przed wystąpieniem publicznym przed tak dużą widownią.",
                            "target_lang_translations": {
                                "ru": "У него были большие <w>сомнения</w> перед публичным выступлением перед такой большой аудиторией."
                            }
                        },
                        {
                            "text": "Przyjęła tę propozycję pracy bez żadnych <w>oporów</w>.",
                            "target_lang_translations": {
                                "ru": "Она приняла это предложение о работе без каких-либо <w>колебаний</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zanokcica",
                        "zanokcica"
                    ],
                    "antonyms": [
                        "chęć",
                        "entuzjazm"
                    ],
                    "common_phrases": [
                        "mieć opory",
                        "bez oporów",
                        "przełamać opory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутреннее чувство нежелания, нерешительности или сомнения перед выполнением какого-либо действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колебания",
                                "target_lang_sense_clarification": "нерешительность (обычно во мн. ч.)"
                            },
                            {
                                "target_lang_word": "сомнения",
                                "target_lang_sense_clarification": "неуверенность"
                            },
                            {
                                "target_lang_word": "предубеждение",
                                "target_lang_sense_clarification": "предвзятое отношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91948a8d-9b3f-33d9-bcc0-7d36a0a716a8",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>opora</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W konstrukcji mostu brakowało solidnych <w>opór</w>, co groziło zawaleniem.",
                            "target_lang_translations": {

                },
                {
                    "sense_id": "16399f06-da63-37c8-8729-9eff76133882",
                    "sense_definition": "Slangowe, często obraźliwe określenie osoby chorej na AIDS.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "disease_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Środowisko odrzuciło go, nazywając go pogardliwie <adidasem>.",
                            "target_lang_translations": {
                                "ru": "Окружение отвергло его, презрительно называя <w>спидозником</w>."
                            }

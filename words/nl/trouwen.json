{
    "word_family": [
        "trouw",
        "trouwerij",
        "trouwring",
        "trouwjurk",
        "ondertrouw",
        "huwelijkstrouw"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "13366062-90e6-39cd-bd1f-24ac39ecf4a2",
                    "sense_definition": "In het huwelijk treden; een wettelijke of ceremoniële verbintenis aangaan met een partner.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "marriage_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil je met me <trouwen>?",
                            "target_lang_translations": {
                                "en": "Will you <marry> me?",
                                "ru": "Ты <выйдешь> за меня <замуж>?"
                            }
                        },
                        {
                            "text": "Op 3 juli ga ik <trouwen> met mijn vriendin.",
                            "target_lang_translations": {
                                "en": "On July 3rd I am going to <marry> my girlfriend.",
                                "ru": "3 июля я <женюсь> на своей девушке."
                            }
                        },
                        {
                            "text": "Mijn ouders zijn al dertig jaar <getrouwd>.",
                            "target_lang_translations": {
                                "en": "My parents have been <married> for thirty years.",
                                "ru": "Мои родители <женаты> уже тридцать лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "huwen"
                    ],
                    "antonyms": [
                        "scheiden"
                    ],
                    "common_phrases": [
                        "met iemand trouwen",
                        "gaan trouwen",
                        "voor de wet trouwen"
                    ],
                    "target_lang_definitions": {
                        "en": "To enter into marriage; to make a legal or ceremonial commitment to a partner.",
                        "ru": "Вступать в брак; заключать законный или церемониальный союз с партнером."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "marry",
                                "target_lang_sense_clarification": "to take as a spouse or become spouses"
                            },
                            {
                                "target_lang_word": "wed",
                                "target_lang_sense_clarification": "formal or literary term"
                            },
                            {
                                "target_lang_word": "get married",
                                "target_lang_sense_clarification": "common phrase for the action"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "жениться",
                                "target_lang_sense_clarification": "о мужчине или паре"
                            },
                            {
                                "target_lang_word": "выходить замуж",
                                "target_lang_sense_clarification": "о женщине"
                            },
                            {
                                "target_lang_word": "пожениться",
                                "target_lang_sense_clarification": "о паре (совершенный вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65973367-43dd-356a-86fa-5c949af77bac",
                    "sense_definition": "De huwelijksplechtigheid voltrekken; twee personen officieel in de echt verbinden.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "marriage_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is de dominee die ons <getrouwd> heeft.",
                            "target_lang_translations": {
                                "en": "That is the minister who <married> us.",
                                "ru": "Это пастор, который нас <обвенчал>."
                            }
                        },
                        {
                            "text": "De burgemeester mag mensen in het stadhuis <trouwen>.",
                            "target_lang_translations": {
                                "en": "The mayor is allowed to <marry> people in the city hall.",
                                "ru": "Мэр может <сочетать> людей <браком> в ратуше."
                            }
                        }
                    ],
                    "synonyms": [
                        "in de echt verbinden"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform a marriage ceremony; to officially join two people in wedlock.",
                        "ru": "Проводить брачную церемонию; официально соединять двух людей узами брака."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "marry",
                                "target_lang_sense_clarification": "to officiate the ceremony for a couple"
                            },
                            {
                                "target_lang_word": "wed",
                                "target_lang_sense_clarification": "to join in matrimony"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "венчать",
                                "target_lang_sense_clarification": "совершать церковный обряд"
                            },
                            {
                                "target_lang_word": "сочетать браком",
                                "target_lang_sense_clarification": "официально регистрировать союз"
                            },
                            {
                                "target_lang_word": "женить",
                                "target_lang_sense_clarification": "соединять браком (разг.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
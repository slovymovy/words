    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2388a9ab-43f3-3bc8-847e-41bef03b14bd",
                    "sense_definition": "Wycinać podłużne wgłębienia, rowki lub otwory w twardym materiale, a także wymywać grunt przez płynącą wodę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Artysta precyzyjnie <żłobił> dłutem skomplikowane ornamenty w dębowej desce.",
                            "target_lang_translations": {
                                "ru": "Художник точно <выдалбливал> долотом сложные орнаменты в дубовой доске."
                            }
                        },
                        {
                            "text": "Górska rzeka przez tysiące lat <żłobiła> głęboki wąwóz w skałach wapiennych.",
                            "target_lang_translations": {
                                "ru": "Горная река тысячелетиями <протачивала> глубокое ущелье в известняковых скалах."
                            }
                        }
                    ],
                    "synonyms": [
                        "drążyć",
                        "ryć",
                        "wycinać",
                        "rzeźbić"
                    ],
                    "antonyms": [
                        "wygładzać",
                        "wyrównywać",
                        "zasypywać"
                    ],
                    "common_phrases": [
                        "żłobić koryto",
                        "żłobić rowki",
                        "woda żłobi kamień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать углубления, канавки или отверстия в твердом материале, а также размывать грунт (о воде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдалбливать",
                                "target_lang_sense_clarification": "делать углубления инструментом"
                            },
                            {
                                "target_lang_word": "размывать",
                                "target_lang_sense_clarification": "о действии воды на грунт"
                            },
                            {
                                "target_lang_word": "протачивать",
                                "target_lang_sense_clarification": "о воде, пробивающей путь в камне"
                            },
                            {
                                "target_lang_word": "бороздить",
                                "target_lang_sense_clarification": "оставлять борозды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f0df0d7-27d9-39b4-83e0-898a46f4d849",
                    "sense_definition": "Powodować powstawanie widocznych śladów starzenia się na twarzy lub trwałych zmian w psychice.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "figurative_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieloletnie troski i ciężka praca <żłobiły> głębokie zmarszczki na czole starszej kobiety.",
                            "target_lang_translations": {
                                "ru": "Многолетние заботы и тяжелый труд <бороздили> глубокие морщины на лбу пожилой женщины."
                            }
                        },
                        {
                            "text": "Traumatyczne wydarzenia z dzieciństwa <żłobiły> w jego umyśle trwały lęk przed ciemnością.",
                            "target_lang_translations": {
                                "ru": "Травматические события детства <оставили> в его уме стойкий страх темноты."
                            }
                        }
                    ],
                    "synonyms": [
                        "naznaczać",
                        "ryć",
                        "odciskać"
                    ],
                    "antonyms": [
                        "wygładzać"
                    ],
                    "common_phrases": [
                        "czas żłobi zmarszczki",
                        "cierpienie żłobi twarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставлять глубокие следы (о времени, переживаниях), вызывая морщины или душевные изменения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бороздить",
                                "target_lang_sense_clarification": "о морщинах"
                            },
                            {
                                "target_lang_word": "прорезать",
                                "target_lang_sense_clarification": "о появлении глубоких линий"
                            },
                            {
                                "target_lang_word": "оставлять след",
                                "target_lang_sense_clarification": "в переносном смысле"
                            }
                        ]
                    }
                }
            ]
        }
    ]

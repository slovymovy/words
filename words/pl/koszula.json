    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9ccce274-e62b-3fc8-8236-85fa7ad68710",
                    "sense_definition": "Lekkie ubranie z rękawami i kołnierzykiem, okrywające tułów, zwykle zapinane z przodu na guziki.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biała <w>koszula</w> zawsze dodaje mężczyźnie elegancji.",
                            "target_lang_translations": {
                                "ru": "Белая <w>рубашка</w> всегда придает мужчине элегантности."
                            }
                        },
                        {
                            "text": "Muszę wyprasować moją ulubioną <w>koszulę</w> przed wyjściem do pracy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно погладить мою любимую <w>рубашку</w> перед уходом на работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "biała koszula",
                        "koszula w kratę",
                        "koszula flanelowa",
                        "prasować koszulę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкая одежда с рукавами и воротником, закрывающая туловище, обычно застегивающаяся спереди на пуговицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рубашка",
                                "target_lang_sense_clarification": "предмет одежды"
                            },
                            {
                                "target_lang_word": "сорочка",
                                "target_lang_sense_clarification": "мужская рубашка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d93b4961-4a01-31c7-a87a-cbb7ed131dcc",
                    "sense_definition": "Luźny ubiór noszony bezpośrednio na ciele, służący do spania lub jako bielizna (często jako skrót od: koszula nocna).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia zawsze śpi w ciepłej, flanelowej <w>koszuli</w>.",
                            "target_lang_translations": {
                                "ru": "Бабушка всегда спит в теплой фланелевой <w>рубашке</w>."
                            }
                        },
                        {
                            "text": "Pacjent otrzymał czystą <w>koszulę</w> szpitalną przed zabiegiem.",
                            "target_lang_translations": {
                                "ru": "Пациент получил чистую больничную <w>рубашку</w> перед процедурой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "koszula nocna",
                        "koszula szpitalna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободная одежда, надеваемая непосредственно на тело, предназначенная для сна или используемая как нижнее белье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ночная рубашка",
                                "target_lang_sense_clarification": "одежда для сна"
                            },
                            {
                                "target_lang_word": "сорочка",
                                "target_lang_sense_clarification": "нижнее белье"
                            },
                            {
                                "target_lang_word": "рубашка",
                                "target_lang_sense_clarification": "больничная или ночная"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
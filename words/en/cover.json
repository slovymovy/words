{
    "word_family": [
        "coverage",
        "covering",
        "uncover",
        "undercover",
        "bedcover",
        "hardcover",
        "softcover",
        "discover",
        "recovery"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f3f4acd8-95dc-3b85-9d88-c7823eff734e",
                    "sense_definition": "To place something over or upon an object, usually to hide, protect, or keep it warm.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>cover</w> the baby with a warm blanket.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>укрой</w> ребенка теплым одеялом."
                            }
                        },
                        {
                            "text": "She <w>covered</w> her eyes with her hands.",
                            "target_lang_translations": {
                                "ru": "Она <w>закрыла</w> глаза руками."
                            }
                        }
                    ],
                    "antonyms": [
                        "uncover"
                    ],
                    "common_phrases": [
                        "cover with",
                        "cover up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить что-либо сверху или на предмет, обычно чтобы спрятать, защитить или сохранить тепло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накрывать",
                                "target_lang_sense_clarification": "класть что-то сверху"
                            },
                            {
                                "target_lang_word": "укрывать",
                                "target_lang_sense_clarification": "для тепла или защиты"
                            },
                            {
                                "target_lang_word": "закрывать",
                                "target_lang_sense_clarification": "заслонять (например, глаза)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "590044cc-feb4-36bd-9f61-d5270b1411ec",
                    "sense_definition": "To lie over or spread across the surface of something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Snow <w>covered</w> the fields for weeks.",
                            "target_lang_translations": {
                                "ru": "Снег неделями <w>покрывал</w> поля."
                            }
                        },
                        {
                            "text": "Dust <w>covered</w> the furniture in the old house.",
                            "target_lang_translations": {
                                "ru": "Пыль <w>покрыла</w> мебель в старом доме."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лежать поверх или распространяться по поверхности чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "быть расположенным поверх чего-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a8e04fe-6fe0-30c3-bf7a-dba847bf9378",
                    "sense_definition": "To deal with, discuss, or include a specific subject or field of information.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The history course <w>covers</w> the nineteenth century.",
                            "target_lang_translations": {
                                "ru": "Курс истории <w>охватывает</w> девятнадцатый век."
                            }
                        },
                        {
                            "text": "Does this book <w>cover</w> all the basic rules of grammar?",
                            "target_lang_translations": {
                                "ru": "Эта книга <w>охватывает</w> все основные грамматические правила?"
                            }
                        }
                    ],
                    "synonyms": [
                        "impregnate"
                    ],
                    "common_phrases": [
                        "cover a topic",
                        "cover everything"
                    ],
                    "target_lang_definitions": {
                        "ru": "Затрагивать, обсуждать или включать в себя определенную тему или область информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охватывать",
                                "target_lang_sense_clarification": "включать в себя тему"
                            },
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "калькированное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ff21ebd-cf29-3148-b1ab-4e0f74b0faad",
                    "sense_definition": "To travel a specific distance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>covered</w> ten miles in just two hours.",
                            "target_lang_translations": {
                                "ru": "Мы <w>прошли</w> десять миль всего за два часа."
                            }
                        },
                        {
                            "text": "The hikers <w>covered</w> a lot of ground before sunset.",
                            "target_lang_translations": {
                                "ru": "Туристы <w>прошли</w> большое расстояние до заката."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cover a distance",
                        "cover ground"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преодолеть определенное расстояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пройти",
                                "target_lang_sense_clarification": "пешком"
                            },
                            {
                                "target_lang_word": "проехать",
                                "target_lang_sense_clarification": "на транспорте"
                            },
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "книжное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "520be367-b6a6-3e51-9373-6a4cee8edb1b",
                    "sense_definition": "To provide enough money to pay for something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten dollars should <w>cover</w> the cost of lunch.",
                            "target_lang_translations": {
                                "ru": "Десять долларов должны <w>покрыть</w> стоимость обеда."
                            }
                        },
                        {
                            "text": "The grant will <w>cover</w> my tuition fees.",
                            "target_lang_translations": {
                                "ru": "Грант <w>покроет</w> мою плату за обучение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cover the cost",
                        "cover the bill",
                        "cover expenses"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставить достаточно денег для оплаты чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "возмещать расходы"
                            },
                            {
                                "target_lang_word": "оплачивать",
                                "target_lang_sense_clarification": "платить за что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab2cbebc-8221-340c-a553-414d0580add7",
                    "sense_definition": "To record or perform a new version of a song that was originally recorded by another artist.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band <w>covered</w> a famous Beatles song.",
                            "target_lang_translations": {
                                "ru": "Группа <w>перепела</w> известную песню Битлз."
                            }
                        },
                        {
                            "text": "Many artists have <w>covered</w> this classic track.",
                            "target_lang_translations": {
                                "ru": "Многие артисты <w>сделали кавер</w> на этот классический трек."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Записать или исполнить новую версию песни, изначально записанной другим исполнителем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перепевать",
                                "target_lang_sense_clarification": "исполнять чужую песню"
                            },
                            {
                                "target_lang_word": "делать кавер",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c343c30-9c1e-3e40-83b4-7d484479d73d",
                    "sense_definition": "To protect or guard someone, often by shooting at the enemy to keep them away.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Cover</w> me while I move to the next building.",
                            "target_lang_translations": {
                                "ru": "<w>Прикрой</w> меня, пока я перебираюсь к следующему зданию."
                            }
                        },
                        {
                            "text": "The soldiers <w>covered</w> their retreat with heavy fire.",
                            "target_lang_translations": {
                                "ru": "Солдаты <w>прикрывали</w> свое отступление плотным огнем."
                            }
                        }
                    ],
                    "synonyms": [
                        "impregnate"
                    ],
                    "common_phrases": [
                        "cover me"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защищать или охранять кого-либо, часто стреляя по врагу, чтобы не дать ему приблизиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикрывать",
                                "target_lang_sense_clarification": "защищать огнем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3198a46-6801-3096-acb4-a5ebe863f4f1",
                    "sense_definition": "To do someone else's work or duties temporarily while they are away.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "substitution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>cover</w> for me at work tomorrow?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>подменить</w> меня завтра на работе?"
                            }
                        },
                        {
                            "text": "I need someone to <w>cover</w> my shift on Friday.",
                            "target_lang_translations": {
                                "ru": "Мне нужно, чтобы кто-то <w>подменил</w> меня на смене в пятницу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cover for someone",
                        "cover a shift"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временно выполнять чью-то работу или обязанности во время отсутствия этого человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подменять",
                                "target_lang_sense_clarification": "заменять кого-то на время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "641675f2-17b0-3e58-a55b-f278f675845a",
                    "sense_definition": "To provide insurance protection for a person or item.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Does this policy <w>cover</w> theft and fire damage?",
                            "target_lang_translations": {
                                "ru": "Этот полис <w>покрывает</w> кражу и ущерб от пожара?"
                            }
                        },
                        {
                            "text": "You are not <w>covered</w> for driving abroad.",
                            "target_lang_translations": {
                                "ru": "Вы не <w>застрахованы</w> для вождения за границей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечивать страховую защиту человека или предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "о страховке"
                            },
                            {
                                "target_lang_word": "страховать",
                                "target_lang_sense_clarification": "защищать от рисков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c6bd83c-49b8-39c2-ad97-280a99e35e49",
                    "sense_definition": "To copulate with (used of male animals, especially horses or dogs).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "zoology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The breeder arranged for the stallion to <w>cover</w> the mare.",
                            "target_lang_translations": {
                                "ru": "Заводчик договорился, чтобы жеребец <w>покрыл</w> кобылу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical or agricultural context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупляться (о самцах животных, особенно лошадях или собаках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "о животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e5b0a31-e376-3bd4-a6d2-34b731045aad",
                    "sense_definition": "To arrange plates, cutlery, and glasses on a table for a meal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "dining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The servants were asked to <w>cover</w> the table for twelve guests.",
                            "target_lang_translations": {
                                "ru": "Слуг попросили <w>накрыть</w> на стол для двенадцати гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "lay the table",
                        "set the table"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Расставлять тарелки, приборы и бокалы на столе для еды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накрывать",
                                "target_lang_sense_clarification": "сервировать стол"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "22a5fe47-8523-35f6-ae13-c137a2528b0d",
                    "sense_definition": "The outer part of a book or magazine that protects and binds the pages.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "book_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her picture appeared on the <w>cover</w> of the magazine.",
                            "target_lang_translations": {
                                "ru": "Её фотография появилась на <w>обложке</w> журнала."
                            }
                        },
                        {
                            "text": "You shouldn't judge a book by its <w>cover</w>.",
                            "target_lang_translations": {
                                "ru": "Не стоит судить о книге по её <w>обложке</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "front cover",
                        "back cover",
                        "hard cover"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешняя часть книги или журнала, которая защищает и скрепляет страницы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обложка",
                                "target_lang_sense_clarification": "книги или журнала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8df5cb9-2238-32bc-af9f-d9d64468cb7a",
                    "sense_definition": "A thing put over something else to close, protect, or hide it.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Where is the <w>cover</w> for this pot?",
                            "target_lang_translations": {
                                "ru": "Где <w>крышка</w> от этой кастрюли?"
                            }
                        },
                        {
                            "text": "He bought a waterproof <w>cover</w> for his car.",
                            "target_lang_translations": {
                                "ru": "Он купил водонепроницаемый <w>чехол</w> для своей машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "impregnate"
                    ],
                    "common_phrases": [
                        "protective cover"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, помещаемый поверх чего-либо, чтобы закрыть, защитить или спрятать это."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крышка",
                                "target_lang_sense_clarification": "для посуды, ящика"
                            },
                            {
                                "target_lang_word": "чехол",
                                "target_lang_sense_clarification": "для техники, машины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4095fed0-a960-3640-976f-ece249daee0e",
                    "sense_definition": "Shelter or protection from weather, attack, or danger.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We ran for <w>cover</w> when the rain started.",
                            "target_lang_translations": {
                                "ru": "Мы побежали в <w>укрытие</w>, когда начался дождь."
                            }
                        },
                        {
                            "text": "The soldiers took <w>cover</w> behind a wall.",
                            "target_lang_translations": {
                                "ru": "Солдаты ушли в <w>укрытие</w> за стеной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "take cover",
                        "under cover"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убежище или защита от непогоды, нападения или опасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрытие",
                                "target_lang_sense_clarification": "убежище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0d08fbe-948c-379b-9e58-c3eadd6394f7",
                    "sense_definition": "A new recording or performance of a song by an artist other than the original creator.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Their latest hit is actually a <w>cover</w> of an 80s song.",
                            "target_lang_translations": {
                                "ru": "Их последний хит на самом деле — <w>кавер</w> на песню 80-х."
                            }
                        },
                        {
                            "text": "The band played a few <w>covers</w> to warm up the crowd.",
                            "target_lang_translations": {
                                "ru": "Группа сыграла несколько <w>каверов</w>, чтобы разогреть толпу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cover version",
                        "cover song"
                    ],
                    "target_lang_definitions": {
                        "ru": "Новая запись или исполнение песни артистом, отличным от оригинального создателя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кавер",
                                "target_lang_sense_clarification": "музыкальная версия"
                            },
                            {
                                "target_lang_word": "кавер-версия",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8391e927-dd53-3ae1-b322-878f2fd9d292",
                    "sense_definition": "Bedclothes, such as sheets and blankets, used to keep a person warm in bed.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "bedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was cold, so I pulled the <w>covers</w> up to my chin.",
                            "target_lang_translations": {
                                "ru": "Было холодно, поэтому я натянул <w>одеяло</w> до подбородка."
                            }
                        },
                        {
                            "text": "She crawled under the <w>covers</w> and fell asleep.",
                            "target_lang_translations": {
                                "ru": "Она залезла под <w>одеяло</w> и заснула."
                            }
                        }
                    ],
                    "common_phrases": [
                        "under the covers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постельные принадлежности, такие как простыни и одеяла, используемые для сохранения тепла в постели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одеяло",
                                "target_lang_sense_clarification": "для укрывания"
                            },
                            {
                                "target_lang_word": "покрывало",
                                "target_lang_sense_clarification": "верхнее покрытие кровати"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbb5da14-c7b5-3ec4-ac32-7655862a5b22",
                    "sense_definition": "Financial protection provided by an insurance company against specific risks.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "financial",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7bcd72df-07de-395e-bba7-98ee4c0d6533",
                    "sense_definition": "Zorganizowane, przestępcze działanie mające na celu pozbawienie kogoś życia lub zdrowia, często z przyczyn politycznych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attack_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj w centrum miasta miał miejsce nieudany <w>zamach</w> na życie premiera.",
                            "target_lang_translations": {
                                "ru": "Вчера в центре города произошло неудачное <w>покушение</w> на жизнь премьер-министра."
                            }
                        },
                        {
                            "text": "Policja zatrzymała grupę, która planowała krwawy <w>zamach</w> terrorystyczny.",
                            "target_lang_translations": {
                                "ru": "Полиция задержала группу, которая планировала кровавый <w>теракт</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "atak",
                        "napad"
                    ],
                    "common_phrases": [
                        "zamach terrorystyczny",
                        "dokonać zamachu",
                        "zamach bombowy",
                        "ofiara zamachu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованное преступное действие, направленное на лишение кого-либо жизни или здоровья, часто по политическим мотивам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покушение",
                                "target_lang_sense_clarification": "на жизнь"
                            },
                            {
                                "target_lang_word": "теракт",
                                "target_lang_sense_clarification": "террористический акт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d368f23-13f9-3196-bb22-0dcfe52e575a",
                    "sense_definition": "Działanie wymierzone przeciwko czemuś, mające na celu obalenie, zniszczenie lub ograniczenie czegoś (np. praw, wolności, władzy).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attack_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wojskowy <w>zamach</w> stanu doprowadził do gwałtownej zmiany rządu w tym kraju.",
                            "target_lang_translations": {
                                "ru": "Военный <w>переворот</w> привел к резкой смене правительства в этой стране."
                            }
                        },
                        {
                            "text": "Opozycja uznała nową ustawę za jawny <w>zamach</w> na wolność słowa.",
                            "target_lang_translations": {
                                "ru": "Оппозиция сочла новый закон явным <w>посягательством</w> на свободу слова."
                            }
                        }
                    ],
                    "synonyms": [
                        "atak"
                    ],
                    "common_phrases": [
                        "zamach stanu",
                        "zamach na wolność",
                        "zamach na demokrację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, направленное против чего-либо с целью свержения, уничтожения или ограничения (например, прав, свободы, власти)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переворот",
                                "target_lang_sense_clarification": "государственный"
                            },
                            {
                                "target_lang_word": "посягательство",
                                "target_lang_sense_clarification": "на права или свободу"
                            },
                            {
                                "target_lang_word": "покушение",
                                "target_lang_sense_clarification": "попытка уничтожить или ограничить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "868cbd51-12ad-35bf-a78a-cc01830e4a5e",
                    "sense_definition": "Energiczny ruch ręką, nogą lub trzymanym przedmiotem w tył, wykonywany w celu nabrania siły przed uderzeniem lub rzutem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tenisista wziął duży <w>zamach</w> rakietą i uderzył piłkę z całej siły.",
                            "target_lang_translations": {
                                "ru": "Теннисист сделал широкий <w>замах</w> ракеткой и ударил по мячу изо всех сил."
                            }
                        },
                        {
                            "text": "Drwal zrobił potężny <w>zamach</w> siekierą, aby rozłupać twarde drewno.",
                            "target_lang_translations": {
                                "ru": "Лесоруб сделал мощный <w>замах</w> топором, чтобы расколоть твердое дерево."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wziąć zamach",
                        "brać zamach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Энергичное движение рукой, ногой или предметом назад для набора силы перед ударом или броском."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замах",
                                "target_lang_sense_clarification": "движение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

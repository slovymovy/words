                    "sense_id": "d5787a65-efce-3f0d-8438-cdeca845de95",
                    "sense_definition": "A bearded iris plant.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gardener planted several <w>beardeds</w> along the path.",
                            "target_lang_translations": {
                                "ru": "Садовник посадил несколько <w>бородатых ирисов</w> вдоль дорожки."
                            }

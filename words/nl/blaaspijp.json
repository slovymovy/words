{
    "word_family": [
        "blazen",
        "pijp",
        "glasblazer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6469e82b-b4df-32e2-a683-75bded253d73",
                    "sense_definition": "Een wapen dat bestaat uit een lange buis waarmee men pijltjes wegschiet door er krachtig in te blazen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jager gebruikte een <w>blaaspijp</w> om geruisloos te kunnen jagen.",
                            "target_lang_translations": {
                                "ru": "Охотник использовал <w>духовую трубку</w> для бесшумной охоты."
                            }
                        },
                        {
                            "text": "Met een <w>blaaspijp</w> kun je kleine pijltjes over een grote afstand schieten.",
                            "target_lang_translations": {
                                "ru": "Из <w>духовой трубки</w> можно стрелять маленькими дротиками на большое расстояние."
                            }
                        }
                    ],
                    "synonyms": [
                        "blaasroer"
                    ],
                    "common_phrases": [
                        "met een blaaspijp schieten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оружие, состоящее из длинной трубки, через которую выдувают стрелки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "духовая трубка",
                                "target_lang_sense_clarification": "оружие"
                            },
                            {
                                "target_lang_word": "духовое ружье",
                                "target_lang_sense_clarification": "оружие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2746bc79-5cb7-3b8b-a521-2bdfa6049294",
                    "sense_definition": "Een holle buis die gebruikt wordt om lucht doorheen te blazen, bijvoorbeeld bij het glasblazen of om een vuur aan te wakkeren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De glasblazer draaide de <w>blaaspijp</w> rond om de vaas te vormen.",
                            "target_lang_translations": {
                                "ru": "Стеклодув вращал <w>стеклодувную трубку</w>, чтобы придать форму вазе."
                            }
                        },
                        {
                            "text": "Gebruik een <w>blaaspijp</w> om voorzichtig zuurstof bij de vlam te krijgen.",
                            "target_lang_translations": {
                                "ru": "Используйте <w>паяльную трубку</w>, чтобы осторожно подать кислород к пламени."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de blaaspijp hanteren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полая трубка, используемая для выдувания воздуха, например, при стеклодувных работах или для раздувания огня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стеклодувная трубка",
                                "target_lang_sense_clarification": "инструмент стеклодува"
                            },
                            {
                                "target_lang_word": "паяльная трубка",
                                "target_lang_sense_clarification": "инструмент для пайки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "a5544f2c-8848-3336-9042-f57781b5b108",
                    "sense_definition": "Uroczyste, zorganizowane przejście dużej grupy ludzi, zazwyczaj ulicami miasta, mające na celu uczczenie jakiegoś wydarzenia, święta lub wyrażenie wspólnych poglądów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "event_procession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy mieszkańcy wyszli na ulice, aby oglądać barwny <w>pochód</w> artystów.",
                            "target_lang_translations": {
                                "ru": "Все жители вышли на улицы, чтобы посмотреть на красочное <w>шествие</w> артистов."
                            }
                        },
                        {
                            "text": "Na czele grupy szedł orkiestrant, który prowadził cały <w>pochód</w> przez rynek.",
                            "target_lang_translations": {
                                "ru": "Во главе группы шел музыкант, который вел все <w>шествие</w> через рыночную площадь."
                            }
                        },
                        {
                            "text": "Uroczysty <w>pochód</w> pierwszomajowy przyciągnął tłumy widzów.",
                            "target_lang_translations": {
                                "ru": "Торжественная первомайская <w>демонстрация</w> привлекла толпы зрителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "parada",
                        "manifestacja",
                        "defilada",
                        "marsz"
                    ],
                    "common_phrases": [
                        "pochód pierwszomajowy",
                        "czoło pochodu",
                        "zamykać pochód",
                        "iść w pochodzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественное, организованное прохождение большой группы людей, обычно по улицам города, с целью празднования события или выражения взглядов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шествие",
                                "target_lang_sense_clarification": "организованное движение группы людей"
                            },
                            {
                                "target_lang_word": "демонстрация",
                                "target_lang_sense_clarification": "публичное массовое шествие"
                            },
                            {
                                "target_lang_word": "парад",
                                "target_lang_sense_clarification": "торжественный смотр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40687aa6-871e-3a41-85ce-f7f5831bea4d",
                    "sense_definition": "Zorganizowane przemieszczanie się wojsk lub innych grup zbrojnych, często w kontekście działań wojennych lub strategicznych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szybki <w>pochód</w> armii zaskoczył przeciwnika i zmusił go do odwrotu.",
                            "target_lang_translations": {
                                "ru": "Быстрый <w>марш</w> армии застал противника врасплох и заставил его отступить."
                            }
                        },
                        {
                            "text": "Historycy opisują ten <w>pochód</w> jako jeden z najtrudniejszych manewrów w tamtej wojnie.",
                            "target_lang_translations": {
                                "ru": "Историки описывают этот <w>поход</w> как один из самых сложных маневров в той войне."
                            }
                        }
                    ],

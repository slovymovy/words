                {
                    "sense_id": "e97dc457-8dbe-369a-a99e-96e38676fbcb",
                    "sense_definition": "Niet vaak; bijna nooit.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het komt in dit land <w>weinig</w> voor dat het zo hard vriest.",
                            "target_lang_translations": {
                                "en": "It <w>rarely</w> happens in this country that it freezes this hard.",
                                "ru": "В этой стране <w>редко</w> случается, чтобы мороз был таким сильным."
                            }
                        },
                        {
                            "text": "Ze gaat nog maar <w>weinig</w> op bezoek bij haar oude buren.",
                            "target_lang_translations": {
                                "en": "She visits her old neighbors <w>seldom</w> now.",
                                "ru": "Она теперь <w>редко</w> заходит в гости к своим старым соседям."
                            }

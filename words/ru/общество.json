                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_organization",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "Акционерное <w>общество</w> объявило о выплате дивидендов.",
                            "target_lang_translations": {

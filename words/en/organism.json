                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "органическое строение",
                                "target_lang_sense_clarification": "структура"
                            },
                            {
                                "target_lang_word": "организм",
                                "target_lang_sense_clarification": "внутреннее устройство"
                            }
                        ]
                    }
                }
            ]
        }

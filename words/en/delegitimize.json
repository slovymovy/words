{
    "word_family": [
        "legitimate",
        "legitimacy",
        "illegitimate",
        "delegitimization",
        "legitimization"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17abdca8-d26c-3402-85e5-73c5ab544824",
                    "sense_definition": "To make something appear invalid, unacceptable, or without authority.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The opposition party tried to <w>delegitimize</w> the election results by claiming there was fraud.",
                            "target_lang_translations": {
                                "ru": "Оппозиционная партия пыталась <w>делегитимизировать</w> результаты выборов, заявляя о фальсификациях."
                            }
                        },
                        {
                            "text": "Constant scandals can <w>delegitimize</w> a leader's authority in the eyes of the public.",
                            "target_lang_translations": {
                                "ru": "Постоянные скандалы могут <w>лишить легитимности</w> власть лидера в глазах общественности."
                            }
                        }
                    ],
                    "synonyms": [
                        "discredit",
                        "invalidate",
                        "undermine"
                    ],
                    "antonyms": [
                        "legitimize",
                        "validate",
                        "endorse"
                    ],
                    "common_phrases": [
                        "delegitimize the government",
                        "delegitimize the process",
                        "attempt to delegitimize"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать что-либо недействительным, неприемлемым или лишенным авторитета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делегитимизировать",
                                "target_lang_sense_clarification": "лишать доверия или признания"
                            },
                            {
                                "target_lang_word": "лишать легитимности",
                                "target_lang_sense_clarification": "делать необоснованным в глазах общества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "032b520a-e1bd-311e-a25b-9068728eabae",
                    "sense_definition": "To make something illegal that was previously legal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new regulations aim to <w>delegitimize</w> the trade of endangered species products.",
                            "target_lang_translations": {
                                "ru": "Новые правила направлены на то, чтобы <w>лишить законного статуса</w> торговлю продуктами из исчезающих видов."
                            }
                        },
                        {
                            "text": "Activists fought to <w>delegitimize</w> the use of harmful chemicals in local agriculture.",
                            "target_lang_translations": {
                                "ru": "Активисты боролись за то, чтобы <w>объявить незаконным</w> использование вредных химикатов в местном сельском хозяйстве."
                            }
                        }
                    ],
                    "synonyms": [
                        "criminalize",
                        "outlaw",
                        "ban"
                    ],
                    "antonyms": [
                        "legalize",
                        "decriminalize"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать незаконным то, что ранее было законным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лишать законного статуса",
                                "target_lang_sense_clarification": "отменять правовую основу"
                            },
                            {
                                "target_lang_word": "объявлять незаконным",
                                "target_lang_sense_clarification": "налагать запрет"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
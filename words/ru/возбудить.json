{
    "word_family": [
        "возбуждение",
        "возбудитель",
        "возбудимость",
        "возбуждённый",
        "возбуждающий"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d0a7825e-be84-3dad-a7d4-18cc00b872c3",
                    "sense_definition": "Вызвать, пробудить какое-либо чувство, желание, мысль или состояние.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "feelings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его рассказ о путешествиях <возбудил> у нас живой интерес к этой стране.",
                            "target_lang_translations": {
                                "en": "His story about his travels <aroused> a lively interest in this country among us."
                            }
                        },
                        {
                            "text": "Запах свежей выпечки <возбудил> у гостей сильный аппетит.",
                            "target_lang_translations": {
                                "en": "The smell of fresh pastries <stimulated> a strong appetite in the guests."
                            }
                        }
                    ],
                    "synonyms": [
                        "вызвать",
                        "пробудить",
                        "породить"
                    ],
                    "antonyms": [
                        "подавить",
                        "усыпить",
                        "заглушить"
                    ],
                    "common_phrases": [
                        "возбудить интерес",
                        "возбудить аппетит",
                        "возбудить подозрение",
                        "возбудить любопытство"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause or awaken a feeling, desire, or interest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arouse",
                                "target_lang_sense_clarification": "interest, appetite, or feelings"
                            },
                            {
                                "target_lang_word": "excite",
                                "target_lang_sense_clarification": "interest or curiosity"
                            },
                            {
                                "target_lang_word": "stimulate",
                                "target_lang_sense_clarification": "activity or appetite"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ceab13aa-2a2a-39b6-99ad-46b5f17aa378",
                    "sense_definition": "Начать официальное производство по какому-либо делу, предложить что-либо на рассмотрение в официальном порядке.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прокурор принял решение <возбудить> уголовное дело против мошенников.",
                            "target_lang_translations": {
                                "en": "The prosecutor decided to <initiate> a criminal case against the fraudsters."
                            }
                        },
                        {
                            "text": "Адвокат планирует <возбудить> ходатайство о пересмотре решения суда.",
                            "target_lang_translations": {
                                "en": "The lawyer plans to <file> a motion to review the court's decision."
                            }
                        }
                    ],
                    "synonyms": [
                        "начать",
                        "инициировать",
                        "завести"
                    ],
                    "antonyms": [
                        "прекратить",
                        "закрыть"
                    ],
                    "common_phrases": [
                        "возбудить уголовное дело",
                        "возбудить ходатайство",
                        "возбудить иск"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "официальное, юридическое"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To start a legal case or official process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "initiate",
                                "target_lang_sense_clarification": "proceedings or a case"
                            },
                            {
                                "target_lang_word": "bring",
                                "target_lang_sense_clarification": "a charge or suit"
                            },
                            {
                                "target_lang_word": "file",
                                "target_lang_sense_clarification": "a motion or petition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0824d4bd-2385-3c60-9380-8dfc646cb1cc",
                    "sense_definition": "Привести в состояние нервного подъёма, сильной взволнованности или чувственного (сексуального) влечения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Громкая музыка и яркий свет могут сильно <возбудить> нервную систему ребёнка.",
                            "target_lang_translations": {
                                "en": "Loud music and bright light can strongly <excite> a child's nervous system."
                            }
                        },
                        {
                            "text": "Её неожиданное прикосновение мгновенно <возбудило> его.",
                            "target_lang_translations": {
                                "en": "Her unexpected touch instantly <turned> him <on>."
                            }
                        }
                    ],
                    "synonyms": [
                        "взволновать",
                        "раззадорить"
                    ],
                    "antonyms": [
                        "успокоить"
                    ],
                    "common_phrases": [
                        "сильно возбудить",
                        "возбудить нервы",
                        "возбудить воображение"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause sexual arousal or nervous excitement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn on",
                                "target_lang_sense_clarification": "sexually (informal)"
                            },
                            {
                                "target_lang_word": "arouse",
                                "target_lang_sense_clarification": "sexually or emotionally"
                            },
                            {
                                "target_lang_word": "excite",
                                "target_lang_sense_clarification": "the nervous system or senses"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54b7ea34-842c-3d0b-a2fa-8d3f27e674ac",
                    "sense_definition": "Настроить кого-либо враждебно по отношению к кому-то или чему-то.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Своими неосторожными словами он <возбудил> против себя весь коллектив.",
                            "target_lang_translations": {
                                "en": "With his careless words, he <turned> the whole team <against> himself."
                            }
                        },
                        {
                            "text": "Оратор пытался <возбудить> толпу и призвать людей к беспорядкам.",
                            "target_lang_translations": {
                                "en": "The speaker tried to <incite> the crowd and call people to riot."
                            }
                        }
                    ],
                    "synonyms": [
                        "настроить",
                        "восстановить",
                        "подстрекнуть"
                    ],
                    "antonyms": [
                        "примирить"
                    ],
                    "common_phrases": [
                        "возбудить ненависть",
                        "возбудить вражду",
                        "возбудить против себя"
                    ],
                    "target_lang_definitions": {
                        "en": "To make someone hostile or angry towards someone else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn against",
                                "target_lang_sense_clarification": "make hostile towards"
                            },
                            {
                                "target_lang_word": "incite",
                                "target_lang_sense_clarification": "a crowd or group to action"
                            },
                            {
                                "target_lang_word": "set against",
                                "target_lang_sense_clarification": "cause conflict between"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f552af9a-b560-3ffc-995a-754fc929adfc",
                    "sense_definition": "Создать или вызвать физическое явление (электрический ток, магнитное поле, колебания и т.п.).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чтобы <возбудить> электрический ток в цепи, необходим источник энергии.",
                            "target_lang_translations": {
                                "en": "To <induce> an electric current in a circuit, an energy source is needed."
                            }
                        },
                        {
                            "text": "Генератор должен <возбудить> магнитное поле для начала полноценной работы.",
                            "target_lang_translations": {
                                "en": "The generator must <excite> a magnetic field to begin full operation."
                            }
                        }
                    ],
                    "synonyms": [
                        "генерировать",
                        "создать"
                    ],
                    "antonyms": [
                        "погасить"
                    ],
                    "common_phrases": [
                        "возбудить ток",
                        "возбудить колебания",
                        "возбудить поле"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "физика, техника"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To produce a physical effect like an electric current or magnetic field."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "excite",
                                "target_lang_sense_clarification": "a field or atom"
                            },
                            {
                                "target_lang_word": "induce",
                                "target_lang_sense_clarification": "a current"
                            },
                            {
                                "target_lang_word": "generate",
                                "target_lang_sense_clarification": "a field or oscillation"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
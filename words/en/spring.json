{
    "word_family": [
        "springy",
        "springtime",
        "springboard",
        "springiness",
        "offspring",
        "wellspring",
        "handspring",
        "mainspring"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "48574dce-6450-3342-9cea-bb922ba78de3",
                    "sense_definition": "The season of the year between winter and summer, when the weather becomes warmer and plants begin to grow.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "season_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Flowers start to bloom in the <w>spring</w>.",
                            "target_lang_translations": {
                                "ru": "Цветы начинают цвести <w>весной</w>."
                            }
                        },
                        {
                            "text": "We are planning to visit Japan next <w>spring</w>.",
                            "target_lang_translations": {
                                "ru": "Мы планируем посетить Японию следующей <w>весной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "springtime"
                    ],
                    "antonyms": [
                        "autumn"
                    ],
                    "common_phrases": [
                        "early spring",
                        "late spring",
                        "spring break",
                        "spring cleaning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время года между зимой и летом, когда погода становится теплее и начинают расти растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "весна",
                                "target_lang_sense_clarification": "время года"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b4a8ab8-2538-34e5-8320-467f3142d2a5",
                    "sense_definition": "A twisted piece of metal or other flexible material that returns to its original shape after being pressed or pulled.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "device_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old mattress has a broken <w>spring</w> that makes it uncomfortable.",
                            "target_lang_translations": {
                                "ru": "В старом матрасе сломана <w>пружина</w>, из-за чего на нем неудобно спать."
                            }
                        },
                        {
                            "text": "A pen usually contains a small <w>spring</w> inside to help the tip move in and out.",
                            "target_lang_translations": {
                                "ru": "В ручке обычно есть маленькая <w>пружина</w>, помогающая стержню выдвигаться и убираться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "coil spring",
                        "box spring"
                    ],
                    "target_lang_definitions": {
                        "ru": "Витая деталь из металла или другого гибкого материала, которая возвращается в исходную форму после сжатия или растяжения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пружина",
                                "target_lang_sense_clarification": "деталь"
                            },
                            {
                                "target_lang_word": "рессора",
                                "target_lang_sense_clarification": "деталь подвески"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40da6177-ef70-3ed1-a4c8-0a7d5bca3f58",
                    "sense_definition": "A place where water flows naturally from the ground.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "water_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They filled their bottles with fresh water from the mountain <w>spring</w>.",
                            "target_lang_translations": {
                                "ru": "Они наполнили бутылки свежей водой из горного <w>родника</w>."
                            }
                        },
                        {
                            "text": "The town is famous for its natural hot <w>springs</w>.",
                            "target_lang_translations": {
                                "ru": "Город славится своими природными горячими <w>источниками</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "fount",
                        "source"
                    ],
                    "common_phrases": [
                        "hot spring",
                        "mineral spring",
                        "spring water"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где вода естественным образом выходит из-под земли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родник",
                                "target_lang_sense_clarification": "водный источник"
                            },
                            {
                                "target_lang_word": "источник",
                                "target_lang_sense_clarification": "водный источник"
                            },
                            {
                                "target_lang_word": "ключ",
                                "target_lang_sense_clarification": "бьющий из земли источник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e991475a-0830-342c-adc6-88e814ed439a",
                    "sense_definition": "A sudden jump or leap upwards or forwards.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "jump_action_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With a sudden <w>spring</w>, the cat landed on the table.",
                            "target_lang_translations": {
                                "ru": "Одним внезапным <w>прыжком</w> кошка приземлилась на стол."
                            }
                        },
                        {
                            "text": "He cleared the fence with a single powerful <w>spring</w>.",
                            "target_lang_translations": {
                                "ru": "Он перемахнул через забор одним мощным <w>прыжком</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "jump",
                        "leap",
                        "bound"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапный прыжок вверх или вперед."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прыжок",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88f5b094-c524-3fe8-824e-d1474f820031",
                    "sense_definition": "The ability of an object to return to its original shape; resilience or bounce.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "elasticity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The elastic band has lost its <w>spring</w> and will not stretch anymore.",
                            "target_lang_translations": {
                                "ru": "Резинка потеряла свою <w>упругость</w> и больше не растягивается."
                            }
                        },
                        {
                            "text": "There is a <w>spring</w> in his step that shows he is happy today.",
                            "target_lang_translations": {
                                "ru": "В его походке чувствуется <w>пружинистость</w>, что говорит о том, что он сегодня счастлив."
                            }
                        }
                    ],
                    "synonyms": [
                        "bounce",
                        "resilience",
                        "elasticity",
                        "springiness"
                    ],
                    "common_phrases": [
                        "spring in one's step"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность предмета возвращаться в исходную форму; упругость или пружинистость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упругость",
                                "target_lang_sense_clarification": "свойство материала"
                            },
                            {
                                "target_lang_word": "пружинистость",
                                "target_lang_sense_clarification": "свойство движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b96e07ad-76f4-3baa-9df4-54ae53d23f16",
                    "sense_definition": "The place or source from which something originates.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "origin_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Honesty is the <w>spring</w> of all true friendship.",
                            "target_lang_translations": {
                                "ru": "Честность — это <w>источник</w> любой настоящей дружбы."
                            }
                        },
                        {
                            "text": "They tried to discover the secret <w>springs</w> of human behavior.",
                            "target_lang_translations": {
                                "ru": "Они пытались открыть тайные <w>пружины</w> человеческого поведения."
                            }
                        }
                    ],
                    "synonyms": [
                        "source",
                        "fount",
                        "beginning"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "somewhat literary in modern usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или источник, откуда что-то происходит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "источник",
                                "target_lang_sense_clarification": "начало чего-либо"
                            },
                            {
                                "target_lang_word": "пружина",
                                "target_lang_sense_clarification": "перен. скрытая причина"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "221b09d2-6241-320b-9504-50803a5735dd",
                    "sense_definition": "To move or jump suddenly and quickly in a particular direction.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "jump_action_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cat is ready to <w>spring</w> on the mouse.",
                            "target_lang_translations": {
                                "ru": "Кошка готова <w>броситься</w> на мышь."
                            }
                        },
                        {
                            "text": "He <w>sprang</w> out of bed when the alarm went off.",
                            "target_lang_translations": {
                                "ru": "Он <w>выскочил</w> из кровати, когда прозвенел будильник."
                            }
                        }
                    ],
                    "synonyms": [
                        "jump",
                        "leap",
                        "bound"
                    ],
                    "common_phrases": [
                        "spring into action",
                        "spring to life",
                        "spring to one's feet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно и быстро двигаться или прыгать в определенном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прыгать",
                                "target_lang_sense_clarification": "совершать прыжок"
                            },
                            {
                                "target_lang_word": "бросаться",
                                "target_lang_sense_clarification": "резко устремляться"
                            },
                            {
                                "target_lang_word": "вскакивать",
                                "target_lang_sense_clarification": "быстро вставать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36ace185-dc0d-38ac-8d21-2e30380b945a",
                    "sense_definition": "To appear or come into existence suddenly; to originate from a source.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "origin_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "New businesses <w>sprang</w> up all over the city.",
                            "target_lang_translations": {
                                "ru": "Новые предприятия <w>возникали</w> по всему городу."
                            }
                        },
                        {
                            "text": "Where did this idea <w>spring</w> from?",
                            "target_lang_translations": {
                                "ru": "Откуда <w>возникла</w> эта идея?"
                            }
                        }
                    ],
                    "synonyms": [
                        "arise",
                        "form",
                        "come into being"
                    ],
                    "common_phrases": [
                        "spring up",
                        "spring from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно появляться или возникать; происходить из какого-либо источника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возникать",
                                "target_lang_sense_clarification": "появляться"
                            },
                            {
                                "target_lang_word": "происходить",
                                "target_lang_sense_clarification": "брать начало"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "daefda0a-8ae7-340d-ae24-0cae75f6e6e0",
                    "sense_definition": "To make something happen suddenly, or to present something unexpectedly.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "trigger_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to <w>spring</w> a surprise party on her.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>устроить</w> для нее вечеринку-сюрприз."
                            }
                        },
                        {
                            "text": "The police were waiting to <w>spring</w> the trap.",
                            "target_lang_translations": {
                                "ru": "Полиция ждала, чтобы <w>захлопнуть</w> ловушку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spring a surprise",
                        "spring a trap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы что-то произошло внезапно, или неожиданно что-то представить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устроить",
                                "target_lang_sense_clarification": "сюрприз и т.п."
                            },
                            {
                                "target_lang_word": "захлопнуть",
                                "target_lang_sense_clarification": "ловушку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6f24475-32a1-3a63-b2c2-dcca77190cb9",
                    "sense_definition": "To develop a leak or crack suddenly (usually of a boat or container).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "leak_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boat <w>sprang</w> a leak during the storm.",
                            "target_lang_translations": {
                                "ru": "Лодка <w>дала течь</w> во время шторма."
                            }
                        },
                        {
                            "text": "We need to fix the pipe before it <w>springs</w> another leak.",
                            "target_lang_translations": {
                                "ru": "Нам нужно починить трубу, пока она снова не <w>дала течь</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spring a leak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно дать течь или треснуть (обычно о лодке или емкости)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дать течь",
                                "target_lang_sense_clarification": "о судне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd326add-81c7-3d57-8474-4dfc07e9ff97",
                    "sense_definition": "To help someone escape from prison.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "prison_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gang made a plan to <w>spring</w> their leader from jail.",
                            "target_lang_translations": {
                                "ru": "Банда разработала план, как <w>вытащить</w> своего лидера из тюрьмы."
                            }
                        },
                        {
                            "text": "He hoped his lawyer could <w>spring</w> him on bail.",
                            "target_lang_translations": {
                                "ru": "Он надеялся, что адвокат сможет <w>освободить</w> его под залог."
                            }
                        }
                    ],
                    "synonyms": [
                        "free",
                        "release",
                        "jailbreak"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помочь кому-то сбежать из тюрьмы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытащить",
                                "target_lang_sense_clarification": "разг. освободить"
                            },
                            {
                                "target_lang_word": "организовать побег",
                                "target_lang_sense_clarification": "помочь сбежать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c5aa0bf1-4242-38cc-b12c-2cc693f9a9db",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Spring</w> is waiting in the lobby.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Спринг</w> ждет в вестибюле."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Спринг",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c613d01-9819-3cf5-9441-84ec38e2cc1a",
                    "sense_definition": "A place name, such as a town in Texas.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place_group",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They moved to <w>Spring</w>, Texas, last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году они переехали в <w>Спринг</w>, штат Техас."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Географическое название, например, город в Техасе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Спринг",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
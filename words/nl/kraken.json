    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "eca5a35d-ce45-3d6a-9ebe-8a4167bb4420",
                    "sense_definition": "Een scherp, knappend of piepend geluid maken, vaak door druk, beweging of ouderdom.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude houten vloer <w>kraakt</w> als je eroverheen loopt.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный пол <w>скрипит</w>, когда по нему ходишь."
                            }
                        },
                        {
                            "text": "Die deur <w>kraakt</w> verschrikkelijk en moet gesmeerd worden.",
                            "target_lang_translations": {
                                "ru": "Эта дверь ужасно <w>скрипит</w>, её нужно смазать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kraken en piepen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать резкий, трескучий или скрипучий звук, часто из-за давления, движения или старости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрипеть",
                                "target_lang_sense_clarification": "издавать резкий звук при трении (пол, дверь)"
                            },
                            {
                                "target_lang_word": "трещать",
                                "target_lang_sense_clarification": "издавать сухой, ломающийся звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd3f31a8-6b54-3a00-a8b9-e05260ff5c2a",
                    "sense_definition": "Iets hards met kracht breken of openmaken, vaak om bij de inhoud te komen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "break_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met deze tang kun je makkelijk harde noten <w>kraken</w>.",
                            "target_lang_translations": {
                                "ru": "Этими щипцами можно легко <w>колоть</w> твердые орехи."
                            }
                        },
                        {
                            "text": "Hij probeerde de kluis te <w>kraken</w> om bij het geld te komen.",
                            "target_lang_translations": {
                                "ru": "Он пытался <w>взломать</w> сейф, чтобы добраться до денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "krieken",
                        "inbreken"
                    ],
                    "common_phrases": [
                        "noten kraken",
                        "harde noten kraken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ломать или вскрывать что-то твердое с силой, часто чтобы добраться до содержимого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колоть",
                                "target_lang_sense_clarification": "разбивать твердую оболочку (орехи)"
                            },
                            {
                                "target_lang_word": "взламывать",
                                "target_lang_sense_clarification": "вскрывать запертое (сейф)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "949bd0ed-7708-31e4-a678-b502eabf0e95",
                    "sense_definition": "Een moeilijke code, raadsel of beveiliging oplossen of ontcijferen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "solve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het team van wiskundigen wist de geheime code te <w>kraken</w>.",
                            "target_lang_translations": {
                                "ru": "Команде математиков удалось <w>взломать</w> секретный код."
                            }
                        },
                        {
                            "text": "Na uren puzzelen heb ik het raadsel eindelijk <w>gekraakt</w>.",
                            "target_lang_translations": {
                                "ru": "После часов раздумий я наконец <w>разгадал</w> эту загадку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een code kraken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решить или расшифровать сложный код, загадку или систему безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взломать",
                                "target_lang_sense_clarification": "расшифровать код"
                            },
                            {
                                "target_lang_word": "разгадать",
                                "target_lang_sense_clarification": "найти решение загадки"
                            }
                        ]

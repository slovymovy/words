                            {
                                "target_lang_word": "refute",
                                "target_lang_sense_clarification": "an argument or accusation"
                            },
                            {
                                "target_lang_word": "parry",
                                "target_lang_sense_clarification": "verbal attacks or questions"
                            },
                            {
                                "target_lang_word": "counter",
                                "target_lang_sense_clarification": "an opposing point"
                            }
                        ]

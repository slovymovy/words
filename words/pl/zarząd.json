            "senses": [
                {
                    "sense_id": "7af14078-5924-3cf2-9b70-00be15cfef80",
                    "sense_definition": "Zespół osób uprawnionych do kierowania działalnością przedsiębiorstwa, organizacji lub instytucji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy <w>zarząd</w> firmy postanowił zmienić strategię marketingową.",
                            "target_lang_translations": {
                                "ru": "Новое <w>правление</w> фирмы решило изменить маркетинговую стратегию."
                            }
                        },
                        {
                            "text": "Mój ojciec został powołany na członka <w>zarządu</w> dużej spółki.",
                            "target_lang_translations": {
                                "ru": "Мой отец был назначен членом <w>правления</w> крупной компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierownictwo",
                        "dyrekcja",
                        "szefostwo"
                    ],
                    "common_phrases": [
                        "członek zarządu",
                        "prezes zarządu",
                        "posiedzenie zarządu",
                        "zarząd spółki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа лиц, уполномоченных руководить деятельностью предприятия, организации или учреждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правление",
                                "target_lang_sense_clarification": "выборный орган управления"
                            },
                            {
                                "target_lang_word": "руководство",
                                "target_lang_sense_clarification": "начальство, совокупность руководителей"
                            },
                            {
                                "target_lang_word": "дирекция",
                                "target_lang_sense_clarification": "руководящий орган"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e474d0d8-87c1-3bc6-8f16-1caa6884bac1",
                    "sense_definition": "Instytucja lub jednostka organizacyjna zajmująca się administrowaniem określonym obszarem, np. drogami lub transportem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę złożyć wniosek w miejskim <w>zarządzie</w> dróg i transportu.",

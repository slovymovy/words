                    "target_lang_definitions": {
                        "en": "To settle on or choose something after consideration"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "settle on",
                                "target_lang_sense_clarification": "decide on"
                            },
                            {
                                "target_lang_word": "choose",
                                "target_lang_sense_clarification": "select"
                            }
                        ]
                    }
                }
            ]
        }
    ]

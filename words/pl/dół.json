{
    "word_family": [
        "dołek",
        "dolny",
        "dołować",
        "zdołowany",
        "podole"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c86ab4ad-8baa-3b96-9c94-9e32da7477fa",
                    "sense_definition": "Najniższa część jakiegoś przedmiotu, obiektu, obszaru lub konstrukcji; przeciwieństwo góry.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_bottom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkam na <dole>, a moi rodzice na piętrze.",
                            "target_lang_translations": {
                                "ru": "Я живу <внизу>, а мои родители на этаже."
                            }
                        },
                        {
                            "text": "Proszę podpisać się u <dołu> tego dokumentu.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подпишитесь <внизу> этого документа."
                            }
                        }
                    ],
                    "antonyms": [
                        "góra"
                    ],
                    "common_phrases": [
                        "na dole",
                        "u dołu",
                        "z góry na dół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самая низкая часть предмета, объекта, территории или конструкции; противоположность верху."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "низ",
                                "target_lang_sense_clarification": "нижняя часть чего-либо"
                            },
                            {
                                "target_lang_word": "нижняя часть",
                                "target_lang_sense_clarification": "общая ссылка на расположение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "760b7f88-a219-3ec0-b452-7a0d5dd6dbb0",
                    "sense_definition": "Wykopane w ziemi zagłębienie, pusta przestrzeń w gruncie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "hole_ground",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy wykopali głęboki <dół> pod fundamenty nowego domu.",
                            "target_lang_translations": {
                                "ru": "Рабочие вырыли глубокую <яму> под фундамент нового дома."
                            }
                        },
                        {
                            "text": "Uważaj, żeby nie wpaść w ten <dół> na środku drogi.",
                            "target_lang_translations": {
                                "ru": "Осторожно, чтобы не упасть в эту <яму> посреди дороги."
                            }
                        }
                    ],
                    "synonyms": [
                        "dołek",
                        "ziemianka"
                    ],
                    "antonyms": [
                        "pagórek"
                    ],
                    "common_phrases": [
                        "kopać dół",
                        "zasypać dół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выкопанное в земле углубление, пустое пространство в грунте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яма",
                                "target_lang_sense_clarification": "углубление в земле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9f354d1-c4fb-3ada-9bb2-f0f0f00c371c",
                    "sense_definition": "Stan przygnębienia, smutku lub braku energii; potoczne określenie gorszego samopoczucia psychicznego.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam dzisiaj strasznego <doła> i nic mi się nie chce robić.",
                            "target_lang_translations": {
                                "ru": "У меня сегодня жуткая <хандра>, и ничего не хочется делать."
                            }
                        },
                        {
                            "text": "Po obejrzeniu tego smutnego filmu wszyscy złapali <doła>.",
                            "target_lang_translations": {
                                "ru": "После просмотра этого грустного фильма на всех напала <тоска>."
                            }
                        }
                    ],
                    "synonyms": [
                        "chandra",
                        "depresja",
                        "splin"
                    ],
                    "common_phrases": [
                        "mieć doła",
                        "złapać doła"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used frequently in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние подавленности, грусти или отсутствия энергии; разговорное обозначение плохого психологического самочувствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хандра",
                                "target_lang_sense_clarification": "разговорное, уныние"
                            },
                            {
                                "target_lang_word": "депрессия",
                                "target_lang_sense_clarification": "психологическое состояние"
                            },
                            {
                                "target_lang_word": "тоска",
                                "target_lang_sense_clarification": "глубокая грусть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae7adaca-57d6-3242-8321-19bad7fb3465",
                    "sense_definition": "Naturalne zagłębienie w budowie ciała człowieka lub zwierzęcia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zbadał <dół> pachowy pacjenta, szukając powiększonych węzłów chłonnych.",
                            "target_lang_translations": {
                                "ru": "Врач осмотрел подмышечную <впадину> пациента в поисках увеличенных лимфоузлов."
                            }
                        },
                        {
                            "text": "Ból promieniował w okolice <dołu> nadobojczykowego.",
                            "target_lang_translations": {
                                "ru": "Боль отдавала в область надключичной <ямки>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dół pachowy",
                        "dół podkolanowy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "medical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественное углубление в строении тела человека или животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "впадина",
                                "target_lang_sense_clarification": "анатомическое углубление"
                            },
                            {
                                "target_lang_word": "ямка",
                                "target_lang_sense_clarification": "небольшое углубление в теле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78c83980-07c2-3398-97c5-3b511d8cfdb2",
                    "sense_definition": "Ludzie zajmujący najniższą pozycję w hierarchii społecznej lub w strukturze organizacji (używane zwykle w liczbie mnogiej).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "social_hierarchy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Decyzje zarządu rzadko są konsultowane z partyjnymi <dołami>.",
                            "target_lang_translations": {
                                "ru": "Решения правления редко обсуждаются с партийными <низами>."
                            }
                        },
                        {
                            "text": "Niezadowolenie społeczne zaczęło się szerzyć przez <doły> społeczne.",
                            "target_lang_translations": {
                                "ru": "Социальное недовольство начало распространяться через социальные <низы>."
                            }
                        }
                    ],
                    "antonyms": [
                        "góra"
                    ],
                    "common_phrases": [
                        "doły społeczne",
                        "doły partyjne"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "sociological or political context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, занимающие самое низкое положение в социальной иерархии или структуре организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "низы",
                                "target_lang_sense_clarification": "низшие слои общества или организации"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
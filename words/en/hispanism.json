            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2009efa9-dc54-390e-b7ce-c9c28f6a0df7",
                    "sense_definition": "The study of the Spanish language or culture.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor dedicated his career to <w>Hispanism</w> and the literature of the Golden Age.",
                            "target_lang_translations": {
                                "ru": "Профессор посвятил свою карьеру <w>испанистике</w> и литературе Золотого века."
                            }
                        },
                        {
                            "text": "Modern <w>Hispanism</w> often includes the study of both Peninsular and Latin American societies.",
                            "target_lang_translations": {
                                "ru": "Современная <w>испанистика</w> часто включает изучение как пиренейского, так и латиноамериканского общества."
                            }
                        }
                    ],
                    "common_phrases": [
                        "academic Hispanism",
                        "field of Hispanism"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изучение испанского языка, литературы и культуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испанистика",
                                "target_lang_sense_clarification": "научная дисциплина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1939469-21c2-3b83-9cd9-6229fc1da653",
                    "sense_definition": "A Spanish word, phrase, or idiom used in another language.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The use of the word 'patio' in English is a well-known <w>Hispanism</w>.",
                            "target_lang_translations": {
                                "ru": "Употребление слова «патио» в английском языке — это известный <w>испанизм</w>."
                            }
                        },
                        {
                            "text": "The translator identified a <w>Hispanism</w> in the text that sounded unnatural in the target language.",
                            "target_lang_translations": {
                                "ru": "Переводчик обнаружил в тексте <w>испанизм</w>, который звучал неестественно на целевом языке."
                            }
                        }
                    ],
                    "synonyms": [
                        "Hispanicism",
                        "Castilianism"
                    ],
                    "common_phrases": [
                        "lexical Hispanism",
                        "borrowed Hispanism"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испанское слово или оборот речи, заимствованные другим языком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испанизм",
                                "target_lang_sense_clarification": "лингвистическое заимствование"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "powierzać",
        "powierzenie",
        "powiernik",
        "powiernica",
        "powiernictwo"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ca430a28-de10-3067-81cd-dbb8f2c5ad9c",
                    "sense_definition": "Przekazać komuś odpowiedzialność za kogoś lub coś, wyznaczyć zadanie do wykonania lub oddać coś pod czyjąś opiekę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor postanowił <w>powierzyć</w> mi kierowanie nowym projektem.",
                            "target_lang_translations": {
                                "ru": "Директор решил <w>поручить</w> мне руководство новым проектом."
                            }
                        },
                        {
                            "text": "Muszę wyjechać na kilka dni, więc chcę ci <w>powierzyć</w> opiekę nad moim psem.",
                            "target_lang_translations": {
                                "ru": "Мне нужно уехать на несколько дней, поэтому я хочу <w>доверить</w> тебе присмотр за моей собакой."
                            }
                        }
                    ],
                    "synonyms": [
                        "poruczyć"
                    ],
                    "antonyms": [
                        "odebrać"
                    ],
                    "common_phrases": [
                        "powierzyć zadanie",
                        "powierzyć opiekę",
                        "powierzyć stanowisko",
                        "powierzyć misję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать кому-либо ответственность за кого-то или что-то, поручить выполнение задачи или отдать под чью-либо опеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поручить",
                                "target_lang_sense_clarification": "дать задание или возложить обязанность"
                            },
                            {
                                "target_lang_word": "доверить",
                                "target_lang_sense_clarification": "передать под ответственность, полагаясь на честность"
                            },
                            {
                                "target_lang_word": "вверить",
                                "target_lang_sense_clarification": "передать на попечение (более официально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "977e5734-0097-3005-bfd3-6baa1de7cbd7",
                    "sense_definition": "Wyjawić komuś tajemnicę lub poufną informację, wierząc w jego dyskrecję.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bała się <w>powierzyć</w> komukolwiek swój największy sekret.",
                            "target_lang_translations": {
                                "ru": "Она боялась <w>доверить</w> кому-либо свой самый большой секрет."
                            }
                        },
                        {
                            "text": "Jeśli chcesz mi <w>powierzyć</w> tę tajemnicę, obiecuję, że nikomu jej nie zdradzę.",
                            "target_lang_translations": {
                                "ru": "Если ты хочешь <w>доверить</w> мне эту тайну, обещаю, что никому её не выдам."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwierzyć"
                    ],
                    "antonyms": [
                        "zataić"
                    ],
                    "common_phrases": [
                        "powierzyć sekret",
                        "powierzyć tajemnicę",
                        "powierzyć troski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскрыть кому-либо тайну или конфиденциальную информацию, полагаясь на его порядочность и молчание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доверить",
                                "target_lang_sense_clarification": "сообщить что-то сокровенное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6de478de-03f2-34a0-b171-0fcc88abfad0",
                    "sense_definition": "Z pełnym zaufaniem oddać coś, siebie lub swój los pod czyjąś władzę lub opiekę (często w kontekście podniosłym lub religijnym).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trudnych chwilach człowiek często pragnie <w>powierzyć</w> swój los Bogu.",
                            "target_lang_translations": {
                                "ru": "В трудные минуты человек часто хочет <w>вверить</w> свою судьбу Богу."
                            }
                        },
                        {
                            "text": "Postanowili <w>powierzyć</w> przyszłość narodu w ręce młodych liderów.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>вверить</w> будущее народа в руки молодых лидеров."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawierzyć"
                    ],
                    "common_phrases": [
                        "powierzyć los",
                        "powierzyć życie",
                        "powierzyć się Bogu"
                    ],
                    "target_lang_definitions": {
                        "ru": "С полным доверием отдать что-то, себя или свою судьбу под чью-либо власть или опеку (часто в возвышенном или религиозном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вверить",
                                "target_lang_sense_clarification": "торжественно передать (судьбу, душу)"
                            },
                            {
                                "target_lang_word": "вручить",
                                "target_lang_sense_clarification": "передать в чьи-либо руки (судьбу, жизнь)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
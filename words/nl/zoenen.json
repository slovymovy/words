    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ad4c3969-9f34-357c-906f-9c778be63be2",
                    "sense_definition": "Iemand of iets met de lippen aanraken als uiting van liefde, vriendschap of als groet.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>zoenen</w> elkaar altijd drie keer op de wang bij een begroeting.",
                            "target_lang_translations": {
                                "en": "They always <w>kiss</w> each other three times on the cheek when greeting."
                            }
                        },
                        {
                            "text": "Hij <w>zoende</w> haar zachtjes op haar voorhoofd voordat hij vertrok.",
                            "target_lang_translations": {
                                "en": "He <w>kissed</w> her gently on her forehead before he left."
                            }
                        }
                    ],
                    "synonyms": [
                        "kussen"
                    ],
                    "common_phrases": [
                        "met elkaar zoenen",
                        "iemand wakker zoenen",
                        "staan te zoenen"
                    ],
                    "target_lang_definitions": {
                        "en": "To touch someone or something with the lips as an expression of love, friendship, or as a greeting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kiss",
                                "target_lang_sense_clarification": "to touch with the lips"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb9d71af-0614-3397-a97c-5a43d3430a5a",
                    "sense_definition": "Een ruzie of geschil bijleggen; vrede sluiten.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De strijdende partijen <w>zoenden</w> hun conflict na jaren van oorlog.",
                            "target_lang_translations": {
                                "en": "The warring parties <w>settled</w> their conflict after years of war."
                            }
                        },
                        {
                            "text": "In oude teksten leest men hoe families hun vetes <w>zoenden</w>.",
                            "target_lang_translations": {
                                "en": "In old texts one reads how families <w>settled</w> their feuds."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This usage is obsolete in modern Dutch; 'verzoenen' is used instead."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To settle a quarrel or dispute; to make peace."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "settle",
                                "target_lang_sense_clarification": "to resolve a dispute"
                            },
                            {
                                "target_lang_word": "reconcile",
                                "target_lang_sense_clarification": "to restore friendly relations"
                            }
                        ]
                    }
                }
            ]
        },
        {

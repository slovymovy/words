                },
                {
                    "sense_id": "3e79b0a4-0106-37fa-b9ff-fdd23886ebe8",
                    "sense_definition": "Nazwisko żeńskie używane w Polsce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "surname_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pani <Sęk> jest naszą nową nauczycielką matematyki.",
                            "target_lang_translations": {
                                "ru": "Госпожа <Сенк> — наша новая учительница математики."
                            }
                        },
                        {
                            "text": "Rozmawiałem wczoraj z Anną <Sęk> o projekcie.",
                            "target_lang_translations": {
                                "ru": "Вчера я разговаривал с Анной <Сенк> о проекте."
                            }
                        }
                    ],

                },
                {
                    "sense_id": "2c10fb26-c62b-3bb6-ad3c-93ecb514bc8f",
                    "sense_definition": "A lazy person or someone who moves very slowly.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hurry up, you slow <w>poke</w>, or we will miss the bus!",
                            "target_lang_translations": {
                                "ru": "Поторапливайся, ты, <w>копуша</w>, иначе мы опоздаем на автобус!"
                            }
                        },
                        {
                            "text": "He is such an old <w>poke</w> that it takes him all day to finish a simple task.",
                            "target_lang_translations": {
                                "ru": "Он такой старый <w>тормоз</w>, что у него уходит весь день на простую задачу."
                            }

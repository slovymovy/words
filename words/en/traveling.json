                            {
                                "target_lang_word": "передвижной",
                                "target_lang_sense_clarification": "о выставках, цирках"
                            },
                            {
                                "target_lang_word": "разъездной",
                                "target_lang_sense_clarification": "связанный с частыми поездками по работе"
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8555d9b3-935b-3878-9030-d39d65c8be50",
                    "sense_definition": "Возмутительный поступок, недопустимое поведение или состояние беспорядка.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "outrage_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы не будем терпеть это <w>безобразие</w> на рабочем месте.",
                            "target_lang_translations": {
                                "en": "We will not tolerate this <w>outrage</w> in the workplace."
                            }
                        },
                        {
                            "text": "В комнате творилось настоящее <w>безобразие</w>, вещи валялись повсюду.",
                            "target_lang_translations": {
                                "en": "There was a real <w>mess</w> in the room, things were scattered everywhere."
                            }
                        }
                    ],
                    "synonyms": [
                        "бесчинство",
                        "свинство",
                        "непотребство"
                    ],
                    "common_phrases": [
                        "творить безобразие",
                        "прекратить безобразие",
                        "сплошное безобразие"
                    ],
                    "target_lang_definitions": {
                        "en": "An outrageous act, unacceptable behavior, or a state of disorder."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "outrage",
                                "target_lang_sense_clarification": "an act that causes strong resentment or anger"
                            },
                            {
                                "target_lang_word": "disgrace",
                                "target_lang_sense_clarification": "shameful behavior or a shameful situation"
                            },
                            {
                                "target_lang_word": "mess",
                                "target_lang_sense_clarification": "a state of disorder or untidiness (colloquial)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b18c0639-49c0-3ecb-9428-1570210bff91",
                    "sense_definition": "Отталкивающая, крайне некрасивая внешность или физический недостаток.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она считала свой шрам ужасным и пыталась скрыть это <w>безобразие</w>.",
                            "target_lang_translations": {
                                "en": "She considered her scar terrible and tried to hide this <w>deformity</w>."
                            }
                        },
                        {
                            "text": "Истинное <w>безобразие</w> человека кроется не в лице, а в его злых мыслях.",
                            "target_lang_translations": {
                                "en": "True human <w>ugliness</w> lies not in the face, but in evil thoughts."
                            }
                        }
                    ],
                    "synonyms": [
                        "уродство",
                        "уродливость",
                        "безобразность"
                    ],
                    "antonyms": [
                        "красота"
                    ],
                    "common_phrases": [
                        "физическое безобразие",
                        "скрывать безобразие"
                    ],
                    "target_lang_definitions": {
                        "en": "Repulsive, extremely ugly appearance or physical defect."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ugliness",
                                "target_lang_sense_clarification": "the quality of being unpleasant to look at"
                            },
                            {
                                "target_lang_word": "deformity",
                                "target_lang_sense_clarification": "a misshapen feature or physical defect"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "21facc42-ac2f-362d-94f1-932c495b805d",
                    "sense_definition": "Возглас, который используется для выражения сильного возмущения или недовольства.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "outrage_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Безобразие</w>! Вы наступили мне на ногу и даже не извинились.",
                            "target_lang_translations": {
                                "en": "<w>Outrageous</w>! You stepped on my foot and didn't even apologize."
                            }
                        },
                        {
                            "text": "Какое <w>безобразие</w>, что магазин закрылся раньше времени!",
                            "target_lang_translations": {
                                "en": "What a <w>disgrace</w> that the shop closed early!"
                            }
                        }
                    ],
                    "synonyms": [
                        "свинство"
                    ],
                    "common_phrases": [
                        "какое безобразие",
                        "просто безобразие"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in spoken language to express anger"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation used to express strong indignation or dissatisfaction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "It's a disgrace!",
                                "target_lang_sense_clarification": "expression of strong disapproval"
                            },
                            {
                                "target_lang_word": "Outrageous!",
                                "target_lang_sense_clarification": "expression of shock or anger"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ee83feb6-1afd-3ca1-8257-b23f3568b16e",
                    "sense_definition": "Сотрудник, который ведет делопроизводство, отвечает на звонки и помогает руководителю в организации работы.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой личный <w>секретарь</w> напомнит вам о времени нашей встречи.",
                            "target_lang_translations": {
                                "en": "My personal <w>secretary</w> will remind you of the time of our meeting."
                            }
                        },
                        {
                            "text": "<w>Секретарь</w> директора попросила посетителей подождать в приемной несколько минут.",
                            "target_lang_translations": {
                                "en": "The director's <w>secretary</w> asked the visitors to wait in the reception area for a few minutes."
                            }
                        }
                    ],
                    "synonyms": [
                        "помощник",
                        "ассистент"
                    ],
                    "common_phrases": [
                        "личный секретарь",
                        "секретарь директора",
                        "работать секретарем"
                    ],
                    "target_lang_definitions": {
                        "en": "An employee who handles correspondence, answers calls, and assists a manager in organizing work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "secretary",
                                "target_lang_sense_clarification": "office employee"
                            },
                            {
                                "target_lang_word": "assistant",
                                "target_lang_sense_clarification": "helper"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "353f13f7-d289-3e02-b9f2-acfa29d8a099",
                    "sense_definition": "Руководящая должность в политических партиях, общественных или международных организациях.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Генеральный <w>секретарь</w> ООН призвал страны к мирному решению конфликта.",
                            "target_lang_translations": {
                                "en": "The UN <w>Secretary</w>-General called on countries to resolve the conflict peacefully."
                            }
                        },
                        {
                            "text": "На съезде партии был избран новый первый <w>секретарь</w> комитета.",
                            "target_lang_translations": {
                                "en": "A new first <w>secretary</w> of the committee was elected at the party congress."
                            }
                        }
                    ],
                    "common_phrases": [
                        "генеральный секретарь",
                        "государственный секретарь",
                        "пресс-секретарь"
                    ],
                    "target_lang_definitions": {
                        "en": "A leadership position in political parties, public, or international organizations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "secretary",
                                "target_lang_sense_clarification": "official title"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43499564-8cb7-378b-9a85-99d0c39e19f6",
                    "sense_definition": "Человек, который ведет протокол и записывает ход собрания или судебного заседания.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Секретарь</w> собрания тщательно записывал все предложения участников в протокол.",
                            "target_lang_translations": {
                                "en": "The <w>secretary</w> of the meeting carefully recorded all the participants' proposals in the minutes."
                            }
                        },
                        {
                            "text": "Судья попросил, чтобы <w>секретарь</w> зачитал показания главного свидетеля.",
                            "target_lang_translations": {
                                "en": "The judge asked the <w>clerk</w> to read out the main witness's testimony."
                            }
                        }
                    ],
                    "common_phrases": [
                        "секретарь собрания",
                        "секретарь суда",
                        "вести протокол"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who keeps minutes and records the proceedings of a meeting or court session."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "secretary",
                                "target_lang_sense_clarification": "record keeper"
                            },
                            {
                                "target_lang_word": "clerk",
                                "target_lang_sense_clarification": "court official"
                            }
                        ]
                    }
                }
            ]
        }
    ]

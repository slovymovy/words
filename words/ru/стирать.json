    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "13166bbf-d69c-3dfc-8acc-b885d41bed2d",
                    "sense_definition": "Мыть одежду, бельё или другие изделия из ткани с помощью воды и моющих средств.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама учит меня <w>стирать</w> свои вещи самостоятельно.",
                            "target_lang_translations": {
                                "en": "Mom is teaching me to <w>wash</w> my things by myself."
                            }
                        },
                        {
                            "text": "Я предпочитаю <w>стирать</w> в машинке, чтобы экономить время.",
                            "target_lang_translations": {
                                "en": "I prefer to <w>wash</w> clothes in the machine to save time."
                            }
                        }
                    ],
                    "antonyms": [
                        "пачкать",
                        "грязнить"
                    ],
                    "common_phrases": [
                        "стирать бельё",
                        "стирать одежду",
                        "стирать в машинке",
                        "стирать вручную"
                    ],
                    "target_lang_definitions": {
                        "en": "To clean clothes or linen using water and detergent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wash",
                                "target_lang_sense_clarification": "general term for cleaning with water"
                            },
                            {
                                "target_lang_word": "launder",
                                "target_lang_sense_clarification": "specifically for washing clothes/linen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6b88a6f-e272-3256-bb12-3bc417492cc0",
                    "sense_definition": "Удалять написанное или нарисованное с какой-либо поверхности при помощи трения.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ученик вышел к доске и начал <w>стирать</w> старые записи.",
                            "target_lang_translations": {
                                "en": "The student went to the board and started to <w>erase</w> the old notes."
                            }
                        },
                        {
                            "text": "Не нужно <w>стирать</w> ластиком правильный ответ в тетради.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>erase</w> the correct answer in the notebook with an eraser."
                            }
                        }
                    ],
                    "synonyms": [
                        "удалять",
                        "вытирать"
                    ],
                    "antonyms": [
                        "писать",
                        "рисовать"
                    ],
                    "common_phrases": [
                        "стирать с доски",
                        "стирать ластиком",
                        "стирать написанное"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove something written or drawn from a surface by rubbing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "erase",
                                "target_lang_sense_clarification": "remove marks using an eraser or friction"
                            },
                            {
                                "target_lang_word": "wipe off",
                                "target_lang_sense_clarification": "remove from a surface like a board"
                            },
                            {
                                "target_lang_word": "rub out",
                                "target_lang_sense_clarification": "remove pencil marks"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e5b909e-5a83-3076-8dab-470562b45085",
                    "sense_definition": "Уничтожать или удалять информацию, цифровые данные или воспоминания.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компьютерный вирус может безвозвратно <w>стирать</w> важные файлы.",
                            "target_lang_translations": {
                                "en": "A computer virus can permanently <w>erase</w> important files."
                            }
                        },
                        {
                            "text": "Время постепенно <w>стирает</w> из памяти неприятные моменты прошлого.",
                            "target_lang_translations": {
                                "en": "Time gradually <w>erases</w> unpleasant moments of the past from memory."
                            }
                        }
                    ],
                    "synonyms": [
                        "удалять",
                        "уничтожать"
                    ],
                    "antonyms": [
                        "сохранять",
                        "записывать",
                        "запоминать"
                    ],
                    "common_phrases": [
                        "стирать память",
                        "стирать файлы",
                        "стирать границы",
                        "стирать следы"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To destroy or remove information, digital data, or memories."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "erase",
                                "target_lang_sense_clarification": "remove data or memories"
                            },
                            {
                                "target_lang_word": "wipe",
                                "target_lang_sense_clarification": "completely remove data from a device"
                            },
                            {
                                "target_lang_word": "delete",
                                "target_lang_sense_clarification": "remove files or records"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b59fb299-7039-3c38-8bbc-f0e610b0d658",
                    "sense_definition": "Повреждать кожу до ран или мозолей в результате длительного трения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новые туфли могут сильно <w>стирать</w> ноги в первый же день.",
                            "target_lang_translations": {
                                "en": "New shoes can badly <w>rub</w> your feet on the very first day."
                            }
                        },
                        {
                            "text": "Турист боялся <w>стирать</w> ступни во время долгого похода по горам.",
                            "target_lang_translations": {
                                "en": "The tourist was afraid of <w>chafing</w> his feet during the long hike in the mountains."
                            }
                        }
                    ],
                    "synonyms": [
                        "натирать"
                    ],
                    "common_phrases": [
                        "стирать ноги",
                        "стирать в кровь",
                        "стирать пятки"
                    ],
                    "target_lang_definitions": {
                        "en": "To damage the skin causing wounds or calluses due to prolonged friction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rub",
                                "target_lang_sense_clarification": "cause friction against skin"
                            },
                            {
                                "target_lang_word": "chafe",
                                "target_lang_sense_clarification": "make sore by rubbing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aaec1023-9216-334e-bb02-e5d3447c3550",
                    "sense_definition": "Превращать что-либо в мелкие частицы или порошок посредством сильного трения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тяжёлые жернова продолжают медленно <w>стирать</w> твёрдое зерно в муку.",
                            "target_lang_translations": {
                                "en": "Heavy millstones continue to slowly <w>grind</w> hard grain into flour."
                            }
                        },
                        {
                            "text": "Он был в ярости и грозился <w>стирать</w> в порошок всех своих врагов.",
                            "target_lang_translations": {
                                "en": "He was furious and threatened to <w>grind</w> all his enemies into dust."
                            }
                        }
                    ],
                    "synonyms": [
                        "измельчать",
                        "перетирать"
                    ],
                    "common_phrases": [
                        "стирать в порошок",
                        "стирать в пыль"
                    ],
                    "target_lang_definitions": {
                        "en": "To turn something into small particles or powder through strong friction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grind",
                                "target_lang_sense_clarification": "reduce to powder by friction"
                            },
                            {
                                "target_lang_word": "pulverize",
                                "target_lang_sense_clarification": "crush completely into dust"
                            }
                        ]
                    }
                }
            ]
        }
    ]

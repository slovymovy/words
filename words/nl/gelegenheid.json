    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b9046c5d-2446-38e9-b6fe-a761191d11c4",
                    "sense_definition": "Een gunstig moment of een mogelijkheid om iets te doen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "opportunity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb nog niet de <w>gelegenheid</w> gehad naar je rapport te kijken.",
                            "target_lang_translations": {
                                "ru": "У меня еще не было <w>возможности</w> взглянуть на твой отчет.",
                                "en": "I haven't had the <w>opportunity</w> to look at your report yet."
                            }
                        },
                        {
                            "text": "Hij maakte van de <w>gelegenheid</w> gebruik om zich aan iedereen voor te stellen.",
                            "target_lang_translations": {
                                "ru": "Он воспользовался <w>случаем</w>, чтобы представиться всем.",
                                "en": "He took the <w>opportunity</w> to introduce himself to everyone."
                            }
                        },
                        {
                            "text": "In het schema is er <w>gelegenheid</w> om een kop koffie te drinken.",
                            "target_lang_translations": {
                                "ru": "В расписании есть <w>возможность</w> выпить чашечку кофе.",
                                "en": "In the schedule there is an <w>opportunity</w> to drink a cup of coffee."
                            }
                        }
                    ],
                    "synonyms": [
                        "kans",
                        "mogelijkheid"
                    ],
                    "common_phrases": [
                        "gebruikmaken van de gelegenheid",
                        "ter gelegenheid van",
                        "bij gelegenheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удобный момент или возможность что-то сделать.",
                        "en": "A favorable moment or a possibility to do something."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возможность",
                                "target_lang_sense_clarification": "шанс, удобный момент"
                            },
                            {
                                "target_lang_word": "случай",
                                "target_lang_sense_clarification": "стечение обстоятельств, повод"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "opportunity",
                                "target_lang_sense_clarification": "favorable chance"
                            },
                            {
                                "target_lang_word": "occasion",
                                "target_lang_sense_clarification": "particular time or event"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c769f8a7-0a17-3123-8710-92fafbaaf381",
                    "sense_definition": "Een openbare plaats voor vermaak, eten of drinken, zoals een café of restaurant.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "venue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vaders restaurant is een populaire <w>gelegenheid</w> in de stad.",
                            "target_lang_translations": {
                                "ru": "Ресторан моего отца — популярное <w>заведение</w> в городе.",
                                "en": "My father's restaurant is a popular <w>establishment</w> in the city."
                            }
                        },
                        {
                            "text": "We zoeken een geschikte <w>gelegenheid</w> om onze bruiloft te vieren.",
                            "target_lang_translations": {
                                "ru": "Мы ищем подходящее <w>заведение</w>, чтобы отпраздновать нашу свадьбу.",
                                "en": "We are looking for a suitable <w>venue</w> to celebrate our wedding."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaak",
                        "etablissement",
                        "uitgaansgelegenheid"
                    ],
                    "common_phrases": [
                        "populaire gelegenheid",
                        "gezellige gelegenheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общественное место для развлечений, еды или напитков, например, кафе или ресторан.",
                        "en": "A public place for entertainment, eating, or drinking, such as a cafe or restaurant."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заведение",
                                "target_lang_sense_clarification": "кафе, ресторан, клуб"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "venue",
                                "target_lang_sense_clarification": "location for events"
                            },
                            {
                                "target_lang_word": "establishment",
                                "target_lang_sense_clarification": "business, place"
                            }
                        ]
                    }
                }
            ]

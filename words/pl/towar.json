{
    "word_family": [
        "towarowy",
        "towarowiec",
        "towaroznawstwo",
        "towarzystwo"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc322538-d0e6-3b91-bb4f-aa4e469d3d83",
                    "sense_definition": "Przedmiot przeznaczony na sprzedaż, wymianę handlową lub do dalszego przerobu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "commerce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sklep spożywczy zamówił świeży <w>towar</w> prosto od rolnika.",
                            "target_lang_translations": {
                                "ru": "Продуктовый магазин заказал свежий <w>товар</w> прямо от фермера."
                            }
                        },
                        {
                            "text": "Klient ma prawo zwrócić wadliwy <w>towar</w> w ciągu dwóch tygodni.",
                            "target_lang_translations": {
                                "ru": "Клиент имеет право вернуть бракованный <w>товар</w> в течение двух недель."
                            }
                        }
                    ],
                    "synonyms": [
                        "produkt",
                        "wyrób"
                    ],
                    "common_phrases": [
                        "dostawa towaru",
                        "zwrot towaru",
                        "towar luksusowy",
                        "jakość towaru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, предназначенный для продажи, торгового обмена или дальнейшей переработки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "товар",
                                "target_lang_sense_clarification": "предмет торговли"
                            },
                            {
                                "target_lang_word": "продукция",
                                "target_lang_sense_clarification": "совокупность продуктов производства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2278f93c-ce3c-3b49-baed-2563df0d31ba",
                    "sense_definition": "Narkotyki lub inne nielegalne substancje odurzające.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "slang_substances",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja zatrzymała przemytników, którzy przewozili nielegalny <w>towar</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция задержала контрабандистов, которые перевозили нелегальный <w>товар</w>."
                            }
                        },
                        {
                            "text": "W filmie gangsterzy ukryli <w>towar</w> w starym magazynie.",
                            "target_lang_translations": {
                                "ru": "В фильме гангстеры спрятали <w>товар</w> на старом складе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czysty towar",
                        "przemycać towar",
                        "brać towar"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "używane w kontekście przestępczym lub potocznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наркотики или другие нелегальные одурманивающие вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "товар",
                                "target_lang_sense_clarification": "сленг, наркотики"
                            },
                            {
                                "target_lang_word": "дурь",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ede32da9-234d-3fc0-b9a9-ea7011884e54",
                    "sense_definition": "Atrakcyjna fizycznie kobieta lub dziewczyna (rzadziej mężczyzna), określenie często uprzedmiotawiające.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopaki w klubie szeptali, że ta nowa dziewczyna to niezły <w>towar</w>.",
                            "target_lang_translations": {
                                "ru": "Парни в клубе шептались, что эта новенькая — классная <w>штучка</w>."
                            }
                        },
                        {
                            "text": "Nie powinieneś mówić o niej jak o <w>towarze</w>, to brak szacunku.",
                            "target_lang_translations": {
                                "ru": "Ты не должен говорить о ней как о <w>товаре</w>, это неуважение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niezły towar",
                        "gorący towar"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "często obraźliwe lub uprzedmiotawiające"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически привлекательная женщина или девушка (реже мужчина), часто с оттенком объективации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штучка",
                                "target_lang_sense_clarification": "сленг, о привлекательной девушке"
                            },
                            {
                                "target_lang_word": "цыпочка",
                                "target_lang_sense_clarification": "сленг, фамильярно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56535f67-9bdf-3ab9-87cb-7f5976803806",
                    "sense_definition": "Pociąg przeznaczony do przewozu ładunków, a nie pasażerów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musieliśmy czekać na przejeździe kolejowym, aż przejedzie długi <w>towar</w>.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось ждать на железнодорожном переезде, пока проедет длинный <w>товарняк</w>."
                            }
                        },
                        {
                            "text": "Ten <w>towar</w> transportuje węgiel ze śląskich kopalni na północ kraju.",
                            "target_lang_translations": {
                                "ru": "Этот <w>товарняк</w> везёт уголь из силезских шахт на север страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "pociąg towarowy",
                        "towarowiec"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "żargon kolejowy lub mowa potoczna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поезд, предназначенный для перевозки грузов, а не пассажиров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "товарняк",
                                "target_lang_sense_clarification": "разг. товарный поезд"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
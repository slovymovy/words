                            {
                                "target_lang_word": "sunken",
                                "target_lang_sense_clarification": "having gone to the bottom (e.g., a ship)"
                            },
                            {
                                "target_lang_word": "submerged",
                                "target_lang_sense_clarification": "completely under water"
                            }
                        ]
                    }
                }
            ]
        },

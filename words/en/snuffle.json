{
    "word_family": [
        "snuffler",
        "snuffy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8508acd7-403e-3d65-88a4-08036610c3c1",
                    "sense_definition": "To breathe in noisily through the nose, typically to smell or investigate something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_smell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hunting dog began to <w>snuffle</w> around the base of the old oak tree.",
                            "target_lang_translations": {
                                "ru": "Охотничья собака начала <w>вынюхивать</w> вокруг основания старого дуба."
                            }
                        },
                        {
                            "text": "We heard the hedgehog <w>snuffle</w> through the dry leaves in the garden.",
                            "target_lang_translations": {
                                "ru": "Мы слышали, как ёж <w>сопит</w> в сухих листьях в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "sniff"
                    ],
                    "common_phrases": [
                        "snuffle around",
                        "snuffle at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шумно втягивать воздух носом, обычно чтобы принюхаться или что-то исследовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопеть",
                                "target_lang_sense_clarification": "издавать шум носом при дыхании или поиске"
                            },
                            {
                                "target_lang_word": "вынюхивать",
                                "target_lang_sense_clarification": "искать по запаху"
                            },
                            {
                                "target_lang_word": "обнюхивать",
                                "target_lang_sense_clarification": "исследовать запах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f78a6d84-24e7-308c-8c65-5fa85841b441",
                    "sense_definition": "To make a wet, nasal sound while breathing or speaking, often due to a cold or crying.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The child continued to <w>snuffle</w> into his pillow long after the argument ended.",
                            "target_lang_translations": {
                                "ru": "Ребёнок продолжал <w>шмыгать носом</w> в подушку ещё долго после того, как ссора закончилась."
                            }
                        },
                        {
                            "text": "She tried to <w>snuffle</w> a reply, but her nose was too blocked to speak clearly.",
                            "target_lang_translations": {
                                "ru": "Она попыталась <w>просопеть</w> ответ, но её нос был слишком заложен, чтобы говорить чётко."
                            }
                        }
                    ],
                    "synonyms": [
                        "sniffle",
                        "whine"
                    ],
                    "common_phrases": [
                        "snuffle and cry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать влажные носовые звуки при дыхании или разговоре, часто из-за простуды или плача."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шмыгать носом",
                                "target_lang_sense_clarification": "втягивать воздух носом при плаче или насморке"
                            },
                            {
                                "target_lang_word": "сопеть",
                                "target_lang_sense_clarification": "тяжело дышать из-за заложенности носа"
                            },
                            {
                                "target_lang_word": "гундосить",
                                "target_lang_sense_clarification": "говорить в нос"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "30ee82b6-aac5-3ac8-bc2b-63e813e01512",
                    "sense_definition": "A noisy intake of breath through the nose.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The horse gave a loud <w>snuffle</w> and shook its mane.",
                            "target_lang_translations": {
                                "ru": "Лошадь издала громкое <w>фырканье</w> и тряхнула гривой."
                            }
                        },
                        {
                            "text": "With a quiet <w>snuffle</w>, the puppy settled down to sleep.",
                            "target_lang_translations": {
                                "ru": "С тихим <w>сопением</w> щенок улёгся спать."
                            }
                        }
                    ],
                    "synonyms": [
                        "sniff",
                        "snort"
                    ],
                    "common_phrases": [
                        "give a snuffle",
                        "loud snuffle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шумный вдох носом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопение",
                                "target_lang_sense_clarification": "шумное дыхание"
                            },
                            {
                                "target_lang_word": "фырканье",
                                "target_lang_sense_clarification": "резкий звук носом"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "zang",
        "zanger",
        "zangeres",
        "gezang",
        "voorzanger"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "afe7160b-e46a-3b15-be2b-f04d4812a917",
                    "sense_definition": "Met de stem een melodie laten horen, meestal door woorden op verschillende toonhoogtes uit te spreken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "music_production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <zingen> iedere zaterdag in een koor.",
                            "target_lang_translations": {
                                "en": "They <sing> in a choir every Saturday."
                            }
                        },
                        {
                            "text": "Op verjaardagen <zingen> we altijd een lied voor de jarige.",
                            "target_lang_translations": {
                                "en": "On birthdays we always <sing> a song for the birthday person."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vals zingen",
                        "een liedje zingen",
                        "in een koor zingen",
                        "zuiver zingen"
                    ],
                    "target_lang_definitions": {
                        "en": "To produce a melody with the voice, usually by pronouncing words at different pitches."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sing",
                                "target_lang_sense_clarification": "produce musical sounds with the voice"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87224034-e57a-3c77-9d2e-4376eda73137",
                    "sense_definition": "Geheime informatie doorgeven aan de politie of autoriteiten, vaak over medeplichtigen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "communication_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De crimineel begon te <zingen> toen hem strafvermindering werd beloofd.",
                            "target_lang_translations": {
                                "en": "The criminal started to <sing> when he was promised a reduced sentence."
                            }
                        },
                        {
                            "text": "Als hij gaat <zingen>, komt de hele organisatie in de problemen.",
                            "target_lang_translations": {
                                "en": "If he starts to <sing>, the whole organization will be in trouble."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorslaan",
                        "klikken",
                        "verraden"
                    ],
                    "antonyms": [
                        "zwijgen"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Bargoens"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To pass secret information to the police or authorities, often about accomplices."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sing",
                                "target_lang_sense_clarification": "confess or betray secrets (slang)"
                            },
                            {
                                "target_lang_word": "talk",
                                "target_lang_sense_clarification": "reveal information"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0760958d-d840-38fc-bfb2-2a53f1149211",
                    "sense_definition": "Een zoemend of fluitend geluid maken, specifiek gezegd van water dat bijna het kookpunt bereikt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sound_inanimate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water <zingt> al, het zal spoedig koken.",
                            "target_lang_translations": {
                                "en": "The water is already <singing>; it will boil soon."
                            }
                        },
                        {
                            "text": "Zet het vuur lager zodra de ketel begint te <zingen>.",
                            "target_lang_translations": {
                                "en": "Turn down the heat as soon as the kettle starts to <sing>."
                            }
                        }
                    ],
                    "synonyms": [
                        "suizen",
                        "zoemen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a buzzing or whistling sound, specifically said of water reaching the boiling point."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sing",
                                "target_lang_sense_clarification": "make a high-pitched sound (e.g. a kettle)"
                            },
                            {
                                "target_lang_word": "whistle",
                                "target_lang_sense_clarification": "emit a high-pitched sound"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
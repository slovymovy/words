    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "738c89b5-3dcc-37fa-bb2b-c45dc6b07260",
                    "sense_definition": "De bevoegdheid of macht om beslissingen te nemen en gehoorzaamheid af te dwingen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "power_right",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar had moeite om zijn <w>gezag</w> in de drukke klas te handhaven.",
                            "target_lang_translations": {
                                "en": "The teacher had trouble maintaining his <w>authority</w> in the busy class."
                            }
                        },
                        {
                            "text": "Hij heeft niet voldoende <w>gezag</w> om dat voorstel in zijn eentje goed te keuren.",
                            "target_lang_translations": {
                                "en": "He does not have sufficient <w>authority</w> to approve that proposal by himself."
                            }
                        }
                    ],
                    "synonyms": [
                        "autoriteit"
                    ],
                    "common_phrases": [
                        "gezag hebben",
                        "gezag uitstralen",
                        "ouderlijk gezag"
                    ],
                    "target_lang_definitions": {
                        "en": "The power or right to make decisions and enforce obedience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "authority",
                                "target_lang_sense_clarification": "power to command"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f30b2750-35f4-3acd-9016-74654d56bd11",
                    "sense_definition": "De overheid of de personen en instanties die de wettelijke macht bezitten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "government_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de rellen greep het <w>gezag</w> in om de openbare orde te herstellen.",
                            "target_lang_translations": {
                                "en": "During the riots, the <w>authorities</w> intervened to restore public order."
                            }
                        },
                        {
                            "text": "Burgers moeten de regels van het bevoegd <w>gezag</w> respecteren en opvolgen.",
                            "target_lang_translations": {
                                "en": "Citizens must respect and follow the rules of the competent <w>authority</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het bevoegd gezag",
                        "openbaar gezag",
                        "ondermijning van het gezag"
                    ],
                    "target_lang_definitions": {
                        "en": "The government or the persons and bodies that possess legal power."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "authorities",
                                "target_lang_sense_clarification": "government or police"
                            },
                            {
                                "target_lang_word": "authority",
                                "target_lang_sense_clarification": "official power"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d05591b-9a66-3a87-854b-95cee3f8626a",
                    "sense_definition": "Het aanzien of respect dat iemand krijgt door kennis, ervaring of persoonlijkheid.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "prestige_respect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door zijn jarenlange ervaring spreekt de arts met veel <w>gezag</w> over dit onderwerp.",
                            "target_lang_translations": {
                                "en": "Due to his years of experience, the doctor speaks with great <w>authority</w> on this subject."
                            }
                        },
                        {
                            "text": "Het <w>gezag</w> van de trainer is bepalend voor het succes van het hele team.",
                            "target_lang_translations": {
                                "en": "The <w>authority</w> of the trainer is decisive for the success of the whole team."
                            }
                        }
                    ],
                    "synonyms": [
                        "prestige",
                        "autoriteit"
                    ],
                    "common_phrases": [
                        "met gezag spreken",
                        "natuurlijk gezag"
                    ],
                    "target_lang_definitions": {
                        "en": "The standing or respect someone receives through knowledge, experience, or personality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "authority",
                                "target_lang_sense_clarification": "personal influence or expertise"
                            },
                            {
                                "target_lang_word": "prestige",
                                "target_lang_sense_clarification": "reputation"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
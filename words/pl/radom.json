                    "sense_id": "0a8628b2-9c4f-3a2a-90b5-ad5a18e3ea6a",
                    "sense_definition": "miasto na prawach powiatu w centralno-wschodniej Polsce, w województwie mazowieckim, nad rzeką Mleczną",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "city_radom",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W <w>Radomiu</w> co dwa lata odbywają się międzynarodowe pokazy lotnicze.",
                            "target_lang_translations": {
                                "ru": "В <w>Радоме</w> каждые два года проходят международные авиашоу."
                            }
                        },
                        {
                            "text": "Festiwal Gombrowiczowski wyprowadza <w>Radom</w> z prowincji w świat wielkiej literatury i teatru.",
                            "target_lang_translations": {
                                "ru": "Фестиваль Гомбровича выводит <w>Радом</w> из провинции в мир большой литературы и театра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Radom Główny",
                        "województwo radomskie"
                    ],
                    "target_lang_definitions": {
                        "ru": "город на правах повята в центрально-восточной Польше, в Мазовецком воеводстве, на реке Млечна"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Радом",
                                "target_lang_sense_clarification": "город в Польше"
                            }
                        ]
                    }
                }
            ]
        },

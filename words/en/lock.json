    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a66ab89f-3efe-3601-89dd-f7cc6e6315b3",
                    "sense_definition": "To fasten something, such as a door or container, with a key or electronic device so that it cannot be opened.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "fastening_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please remember to <w>lock</w> the front door when you leave the house.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не забудь <w>запереть</w> входную дверь, когда будешь уходить из дома."
                            }
                        },
                        {
                            "text": "I always <w>lock</w> my bicycle to the fence to keep it safe.",
                            "target_lang_translations": {
                                "ru": "Я всегда <w>пристёгиваю</w> свой велосипед к забору, чтобы его не украли."
                            }
                        }
                    ],
                    "antonyms": [
                        "unlock"
                    ],
                    "common_phrases": [
                        "lock the door",
                        "lock up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запирать что-либо, например дверь или контейнер, на ключ или с помощью электронного устройства, чтобы это нельзя было открыть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запирать",
                                "target_lang_sense_clarification": "на ключ"
                            },
                            {
                                "target_lang_word": "замыкать",
                                "target_lang_sense_clarification": "закрывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09db264f-0cd9-31b6-bebd-706bbcbb9477",
                    "sense_definition": "To become fixed in one position and unable to move.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "immobility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you brake too hard on the ice, the wheels might <w>lock</w> and cause a skid.",
                            "target_lang_translations": {
                                "ru": "Если слишком резко затормозить на льду, колёса могут <w>заблокироваться</w> и вызвать занос."
                            }
                        },
                        {
                            "text": "The mechanism tends to <w>lock</w> in place if it is not oiled regularly.",
                            "target_lang_translations": {
                                "ru": "Механизм имеет тенденцию <w>заклинивать</w>, если его регулярно не смазывать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lock in place"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зафиксироваться в одном положении и потерять способность двигаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заклинивать",
                                "target_lang_sense_clarification": "застревать"
                            },
                            {
                                "target_lang_word": "блокироваться",
                                "target_lang_sense_clarification": "о колесах, механизмах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4fca9183-c974-36b9-b5d6-a1a75555a4f4",
                    "sense_definition": "To join parts together firmly; to intertwine or clasp together.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intertwine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The protesters decided to <w>lock</w> arms to form a human chain.",
                            "target_lang_translations": {
                                "ru": "Протестующие решили <w>сцепить</w> руки, чтобы образовать живую цепь."
                            }
                        },
                        {
                            "text": "The two pieces of the puzzle <w>lock</w> together perfectly.",
                            "target_lang_translations": {
                                "ru": "Два кусочка пазла идеально <w>соединяются</w> друг с другом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lock arms",
                        "lock horns"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крепко соединять части; сплетать или сцеплять вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сцеплять",
                                "target_lang_sense_clarification": "руки, детали"
                            },
                            {
                                "target_lang_word": "соединяться",
                                "target_lang_sense_clarification": "подходить друг к другу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "865e33d4-1df6-30fb-b85f-8005c954c3d3",
                    "sense_definition": "To restrict access to a digital file, account, or thread, preventing changes or new posts.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_restriction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The administrator had to <w>lock</w> the forum thread because the discussion became too heated.",
                            "target_lang_translations": {
                                "ru": "Администратору пришлось <w>заблокировать</w> тему на форуме, так как дискуссия стала слишком жаркой."
                            }
                        },
                        {
                            "text": "After three incorrect password attempts, the system will <w>lock</w> your account.",
                            "target_lang_translations": {
                                "ru": "После трёх неверных попыток ввода пароля система <w>заблокирует</w> вашу учётную запись."
                            }
                        }
                    ],
                    "antonyms": [
                        "unlock"
                    ],
                    "common_phrases": [
                        "lock the account",
                        "lock the screen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограничивать доступ к цифровому файлу, учетной записи или ветке форума, предотвращая изменения или новые сообщения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блокировать",
                                "target_lang_sense_clarification": "доступ, аккаунт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00dac8e2-447e-38b3-bfa0-422715939e0d",
                    "sense_definition": "To move a boat through a gated section of a canal to change water levels.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "canal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It took us an hour to <w>lock</w> the boat down to the lower river level.",
                            "target_lang_translations": {
                                "ru": "У нас ушёл час, чтобы <w>прошлюзовать</w> лодку до нижнего уровня реки."
                            }
                        },
                        {
                            "text": "The crew prepared to <w>lock</w> through the narrow passage.",
                            "target_lang_translations": {
                                "ru": "Экипаж приготовился <w>шлюзоваться</w> через узкий проход."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать лодку через шлюзованный участок канала для изменения уровня воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлюзовать",
                                "target_lang_sense_clarification": "проводить через шлюз"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6b930dba-877e-39ae-bf52-550c2e5d9b93",
                    "sense_definition": "A device used to keep a door, window, or container fastened, usually operated by a key.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "fastening_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She put the key in the <w>lock</w> and turned it to open the door.",
                            "target_lang_translations": {
                                "ru": "Она вставила ключ в <w>замок</w> и повернула его, чтобы открыть дверь."
                            }
                        },
                        {
                            "text": "The old <w>lock</w> was rusty and very difficult to open.",
                            "target_lang_translations": {
                                "ru": "Старый <w>замок</w> заржавел, и его было очень трудно открыть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "door lock",
                        "safety lock",
                        "pick a lock"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство, используемое для запирания двери, окна или контейнера, обычно открываемое ключом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замок",
                                "target_lang_sense_clarification": "дверной, навесной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52a25a0d-fbb4-3309-b6ab-6ff8ee3856dc",
                    "sense_definition": "A small bunch or portion of hair that grows together.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She kept a <w>lock</w> of her baby's hair in a special box.",
                            "target_lang_translations": {
                                "ru": "Она хранила <w>локон</w> волос своего ребёнка в специальной шкатулке."
                            }
                        },
                        {
                            "text": "A single <w>lock</w> of dark hair fell across his forehead.",
                            "target_lang_translations": {
                                "ru": "Одинокая <w>прядь</w> тёмных волос упала ему на лоб."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lock of hair",
                        "golden locks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой пучок или прядь волос, растущих вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "локон",
                                "target_lang_sense_clarification": "завитой"
                            },
                            {
                                "target_lang_word": "прядь",
                                "target_lang_sense_clarification": "пучок волос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76595cc8-2f56-3e15-a62c-f641a8d92c02",
                    "sense_definition": "A section of a canal closed off by gates, used to raise or lower boats to different water levels.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "canal_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boat waited in the <w>lock</w> while the water level rose.",
                            "target_lang_translations": {
                                "ru": "Лодка ждала в <w>шлюзе</w>, пока поднимался уровень воды."
                            }
                        },
                        {
                            "text": "We traveled through several <w>lock</w> gates during our river cruise.",
                            "target_lang_translations": {
                                "ru": "Во время нашего речного круиза мы прошли через несколько <w>шлюзовых</w> ворот."
                            }
                        }
                    ],
                    "common_phrases": [
                        "canal lock"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок канала, огороженный воротами, используемый для подъема или опускания лодок на разные уровни воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлюз",
                                "target_lang_sense_clarification": "гидротехническое сооружение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7800a967-1140-3e47-bb49-465d07b42131",
                    "sense_definition": "A situation where success or control is certain and guaranteed.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With such a high score, the team has a <w>lock</w> on the championship.",
                            "target_lang_translations": {
                                "ru": "С таким счетом победа в чемпионате для команды — <w>верное дело</w>."
                            }
                        },
                        {
                            "text": "He thinks he is a <w>lock</w> for the promotion, but nothing is decided yet.",
                            "target_lang_translations": {
                                "ru": "Он думает, что повышение ему <w>гарантировано</w>, но ещё ничего не решено."
                            }
                        }
                    ],
                    "common_phrases": [
                        "a total lock",
                        "have a lock on"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe a sure thing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, когда успех или контроль несомненны и гарантированы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гарантия",
                                "target_lang_sense_clarification": "успеха"
                            },
                            {
                                "target_lang_word": "верное дело",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a5a9e44-97f0-31ff-8209-171a8e7e6409",
                    "sense_definition": "A wrestling hold in which a part of the opponent's body is gripped tightly.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "wrestling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wrestler trapped his opponent in a painful <w>lock</w>.",
                            "target_lang_translations": {
                                "ru": "Борец захватил противника в болезненный <w>захват</w>."
                            }

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9008fc85-f237-389d-b396-6eab5f609603",
                    "sense_definition": "Fijn poeder van vuil of aarde dat op de grond, meubels of in de lucht zweeft.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er ligt een dikke laag <w>stof</w> op de oude boeken.",
                            "target_lang_translations": {
                                "en": "There is a thick layer of <w>dust</w> on the old books.",
                                "ru": "На старых книгах лежит толстый слой <w>пыли</w>."
                            }
                        },
                        {
                            "text": "Als je niet schoonmaakt, verzamelt zich overal <w>stof</w>.",
                            "target_lang_translations": {
                                "en": "If you don't clean, <w>dust</w> collects everywhere.",
                                "ru": "Если не убираться, повсюду скапливается <w>пыль</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "laag stof",
                        "stof afnemen",
                        "onder het stof"
                    ],
                    "target_lang_definitions": {
                        "en": "Fine powder of dirt or earth that floats on the ground, furniture, or in the air.",
                        "ru": "Мелкий порошок из грязи или земли, который оседает на полу, мебели или витает в воздухе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dust",
                                "target_lang_sense_clarification": "fine powder"
                            }
                        ],
                        "ru": [

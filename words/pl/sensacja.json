    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "30db0b72-7324-3a8f-9f50-d6eb32e444a1",
                    "sense_definition": "Wydarzenie lub wiadomość, która wywołuje żywe, powszechne zainteresowanie i poruszenie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "public_interest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiadomość o nagłym ślubie znanej aktorki była wielką <w>sensacją</w> w całym kraju.",
                            "target_lang_translations": {
                                "ru": "Новость о внезапной свадьбе известной актрисы стала большой <w>сенсацией</w> во всей стране."
                            }
                        },
                        {
                            "text": "Niektórzy dziennikarze gonią za tanią <w>sensacją</w>, ignorując rzetelne fakty.",
                            "target_lang_translations": {
                                "ru": "Некоторые журналисты гонятся за дешевой <w>сенсацией</w>, игнорируя достоверные факты."
                            }
                        }
                    ],
                    "synonyms": [
                        "rewelacja",
                        "hit",
                        "bomba"
                    ],
                    "common_phrases": [
                        "tania sensacja",
                        "szukać sensacji",
                        "gonić za sensacją",
                        "nie lada sensacja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Событие или новость, вызывающие живой, всеобщий интерес и волнение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сенсация",
                                "target_lang_sense_clarification": "событие или новость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69b5df1b-88c5-3570-b5a5-50dcd3d3c59f",
                    "sense_definition": "Silne wrażenie i stan ogólnego poruszenia spowodowany czymś zaskakującym lub niezwykłym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "public_interest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej niespodziewane pojawienie się na scenie wywołało ogromną <w>sensację</w> wśród publiczności.",
                            "target_lang_translations": {
                                "ru": "Её неожиданное появление на сцене произвело огромную <w>сенсацию</w> среди публики."
                            }
                        },
                        {
                            "text": "Wyniki wyborów wzbudziły niekłamaną <w>sensację</w> i stały się głównym tematem rozmów.",
                            "target_lang_translations": {
                                "ru": "Результаты выборов вызвали неподдельную <w>сенсацию</w> и стали главной темой разговоров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wzbudzić sensację",
                        "wywołać sensację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное впечатление и состояние всеобщего возбуждения, вызванное чем-то неожиданным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фурор",
                                "target_lang_sense_clarification": "сильное впечатление"
                            },
                            {
                                "target_lang_word": "сенсация",
                                "target_lang_sense_clarification": "ажиотаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5291ebc-01c3-30e3-8363-6a4e99640719",
                    "sense_definition": "Odbierane odczucie fizyczne lub doznanie płynące z organizmu, często nieprzyjemne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po zjedzeniu nieświeżej ryby turysta miał silne <w>sensacje</w> żołądkowe.",
                            "target_lang_translations": {
                                "ru": "После употребления несвежей рыбы у туриста возникли сильные желудочные <w>расстройства</w>."
                            }
                        },
                        {
                            "text": "Pacjent opisywał lekarzowi dziwne <w>sensacje</w> bólowe w okolicach klatki piersiowej.",
                            "target_lang_translations": {
                                "ru": "Пациент описывал врачу странные болевые <w>ощущения</w> в области грудной клетки."
                            }
                        }
                    ],
                    "synonyms": [
                        "doznanie",
                        "odczucie"
                    ],
                    "common_phrases": [
                        "sensacje żołądkowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое ощущение или чувство, исходящее от организма, часто неприятное."
                    },
                    "translations": {
                        "ru": [
                            {

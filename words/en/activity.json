            "pos": "noun",
            "senses": [
                {
                    "sense_id": "88cfa8ff-dd6a-3b7f-ba4b-a806955ffdba",
                    "sense_definition": "Something done for enjoyment, exercise, or as part of a specific purpose, often involving movement.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Swimming is a healthy physical <w>activity</w> that strengthens the whole body.",
                            "target_lang_translations": {
                                "ru": "Плавание — это полезная физическая <w>активность</w>, которая укрепляет всё тело."
                            }
                        },
                        {
                            "text": "The school offers a wide range of after-school <w>activities</w> for students.",
                            "target_lang_translations": {
                                "ru": "Школа предлагает широкий выбор внеклассных <w>занятий</w> для учеников."
                            }
                        }
                    ],
                    "synonyms": [
                        "pursuit"
                    ],
                    "common_phrases": [
                        "physical activity",
                        "leisure activity",
                        "outdoor activity",
                        "social activity"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что делается ради удовольствия, упражнения или с определенной целью, часто предполагающее движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "занятие",
                                "target_lang_sense_clarification": "времяпрепровождение, увлечение или урок"
                            },
                            {
                                "target_lang_word": "активность",
                                "target_lang_sense_clarification": "физическая нагрузка или подвижность"
                            },
                            {
                                "target_lang_word": "деятельность",
                                "target_lang_sense_clarification": "активные действия или работа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d46a7f0b-dffc-3b48-a783-38a25881d816",
                    "sense_definition": "The state or situation in which a lot of things are happening or people are moving around.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city center was full of noise and <w>activity</w> during the festival.",
                            "target_lang_translations": {
                                "ru": "Во время фестиваля центр города был полон шума и <w>оживления</w>."
                            }
                        },
                        {
                            "text": "There was a sudden burst of <w>activity</w> when the manager arrived.",
                            "target_lang_translations": {
                                "ru": "Когда приехал менеджер, произошел внезапный всплеск <w>активности</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "rest",
                        "passivity"
                    ],
                    "common_phrases": [
                        "buzz with activity",
                        "flurry of activity",
                        "sign of activity",
                        "burst of activity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, при котором происходит много событий или наблюдается оживленное движение людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оживление",
                                "target_lang_sense_clarification": "суета, движение, бурная жизнь"
                            },
                            {
                                "target_lang_word": "активность",
                                "target_lang_sense_clarification": "энергичные действия или интенсивность событий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f88e3a7-3129-39d7-ab2d-4131f364a5a9",
                    "sense_definition": "A specific action, task, or function that is performed to achieve a result.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Police are investigating suspicious <w>activity</w> reported in the neighborhood.",
                            "target_lang_translations": {
                                "ru": "Полиция расследует подозрительную <w>деятельность</w>, о которой сообщили в районе."
                            }
                        },
                        {
                            "text": "The report details the economic <w>activity</w> of the region over the last year.",
                            "target_lang_translations": {
                                "ru": "В отчете подробно описывается экономическая <w>деятельность</w> региона за последний год."
                            }
                        }
                    ],
                    "synonyms": [
                        "undertaking",
                        "endeavor",
                        "venture",
                        "enterprise"
                    ],
                    "common_phrases": [
                        "criminal activity",
                        "economic activity",
                        "business activity",
                        "illegal activity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное действие, задача или функция, выполняемая для достижения результата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "деятельность",
                                "target_lang_sense_clarification": "работа, функционирование (экономическая, преступная)"
                            },
                            {
                                "target_lang_word": "действия",
                                "target_lang_sense_clarification": "поступки или операции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d081127-0004-3e7f-88fd-e8b37600e383",
                    "sense_definition": "The rate at which the atoms of a radioactive substance break down.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "scientific_property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists measured the <w>activity</w> of the sample using a sensitive device.",
                            "target_lang_translations": {
                                "ru": "Ученые измерили <w>активность</w> образца с помощью чувствительного прибора."
                            }
                        },
                        {
                            "text": "The high level of <w>activity</w> near the reactor core required special shielding.",
                            "target_lang_translations": {
                                "ru": "Высокий уровень <w>активности</w> рядом с активной зоной реактора требовал специальной защиты."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "physics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скорость распада атомов радиоактивного вещества."

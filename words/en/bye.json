{
    "word_family": [
        "goodbye",
        "bye-bye"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "6ce7d368-e61c-333b-a950-182ff23c505d",
                    "sense_definition": "A common shortening of \"goodbye\", used to express farewell when parting from someone or ending a conversation.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "farewell_expressions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have to go home now, so **bye**!",
                            "target_lang_translations": {
                                "ru": "Мне пора домой, так что <w>пока</w>!"
                            }
                        },
                        {
                            "text": "She waved and said **bye** to her friends before getting on the bus.",
                            "target_lang_translations": {
                                "ru": "Она помахала и сказала <w>пока</w> друзьям, прежде чем сесть в автобус."
                            }
                        }
                    ],
                    "synonyms": [
                        "goodbye",
                        "farewell",
                        "so long",
                        "see you"
                    ],
                    "antonyms": [
                        "hello",
                        "hi"
                    ],
                    "common_phrases": [
                        "bye for now",
                        "okay bye",
                        "bye-bye"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенное сокращение от \"goodbye\", используемое для выражения прощания при расставании с кем-либо или окончании разговора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока",
                                "target_lang_sense_clarification": "самое распространенное неформальное прощание"
                            },
                            {
                                "target_lang_word": "счастливо",
                                "target_lang_sense_clarification": "дружеское пожелание при прощании"
                            },
                            {
                                "target_lang_word": "до свидания",
                                "target_lang_sense_clarification": "нейтральное, вежливое прощание"
                            },
                            {
                                "target_lang_word": "бай-бай",
                                "target_lang_sense_clarification": "разговорное, заимствованное из английского"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41cf5fec-3142-3d5b-b6b2-45ea01043894",
                    "sense_definition": "An exclamation used to dismiss someone or to express disbelief at something considered ridiculous, annoying, or irrelevant.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dismissal_expressions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You think I'm going to believe that excuse? **Bye**.",
                            "target_lang_translations": {
                                "ru": "Думаешь, я поверю в эту отговорку? <w>Пока</w>."
                            }
                        },
                        {
                            "text": "He tried to argue with me about the rules, but I was just like, girl **bye**.",
                            "target_lang_translations": {
                                "ru": "Он пытался спорить со мной о правилах, но я просто такая: подруга, <w>ой, всё</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "get out of here",
                        "yeah right",
                        "nonsense",
                        "please"
                    ],
                    "common_phrases": [
                        "girl bye",
                        "boy bye"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Used to dismiss someone or something."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, используемое, чтобы отмахнуться от кого-то или выразить недоверие к чему-то нелепому или раздражающему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока",
                                "target_lang_sense_clarification": "используется саркастически для прекращения разговора"
                            },
                            {
                                "target_lang_word": "ой, всё",
                                "target_lang_sense_clarification": "сленговое выражение для прекращения неприятного разговора"
                            },
                            {
                                "target_lang_word": "свободен",
                                "target_lang_sense_clarification": "грубая форма отсылания кого-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6b996e7a-a149-3805-8d4e-212d00b550ee",
                    "sense_definition": "The status of a player or team in a tournament who advances to the next round without playing because they have no opponent paired with them.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_terminology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The top-seeded team received a **bye** in the first round of the playoffs.",
                            "target_lang_translations": {
                                "ru": "Команда с наивысшим рейтингом получила <w>проход без игры</w> в первом раунде плей-офф."
                            }
                        },
                        {
                            "text": "Because there was an odd number of competitors, one person got a **bye**.",
                            "target_lang_translations": {
                                "ru": "Поскольку было нечетное количество участников, один человек получил <w>свободный проход</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pass",
                        "free pass"
                    ],
                    "common_phrases": [
                        "first-round bye",
                        "earn a bye"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положение игрока или команды в турнире, когда они проходят в следующий круг без игры из-за отсутствия соперника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проход без игры",
                                "target_lang_sense_clarification": "спортивный термин"
                            },
                            {
                                "target_lang_word": "свободный тур",
                                "target_lang_sense_clarification": "ситуация, когда команда не играет в текущем раунде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a53bb62-e546-3062-8868-77caea1e6ca3",
                    "sense_definition": "A run scored in cricket that is not credited to the batsman because the ball passed the striker without touching the bat or body.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sports_terminology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wicketkeeper missed the ball, allowing the batting team to score two **byes**.",
                            "target_lang_translations": {
                                "ru": "Уикет-кипер пропустил мяч, позволив отбивающей команде заработать два <w>бая</w>."
                            }
                        },
                        {
                            "text": "Extras in the final score included four **byes** and two wides.",
                            "target_lang_translations": {
                                "ru": "Дополнительные очки в итоговом счете включали четыре <w>бая</w> и два уайда."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очко в крикете, засчитанное команде, когда мяч пролетает мимо бэтсмена, не коснувшись биты или тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бай",
                                "target_lang_sense_clarification": "термин в крикете"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3992b689-f645-360c-ac3b-3aa58a3e6bd4",
                    "sense_definition": "Describing something that is out of the way, remote, or secondary; often used in compounds.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They traveled down a quiet **bye** road to avoid the main highway.",
                            "target_lang_translations": {
                                "ru": "Они поехали по тихой <w>объездной</w> дороге, чтобы избежать главного шоссе."
                            }
                        },
                        {
                            "text": "It was a **bye** issue that did not attract much attention from the public.",
                            "target_lang_translations": {
                                "ru": "Это был <w>второстепенный</w> вопрос, который не привлек особого внимания общественности."
                            }
                        }
                    ],
                    "synonyms": [
                        "secondary",
                        "incidental",
                        "remote"
                    ],
                    "antonyms": [
                        "main",
                        "primary"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Mostly found in older texts or specific compounds like 'bye-law' or 'bye-election'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывает что-то, находящееся в стороне, отдаленное или второстепенное; часто используется в сложных словах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второстепенный",
                                "target_lang_sense_clarification": "не основной"
                            },
                            {
                                "target_lang_word": "боковой",
                                "target_lang_sense_clarification": "находящийся в стороне"
                            },
                            {
                                "target_lang_word": "обходной",
                                "target_lang_sense_clarification": "о пути или дороге"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "b547f818-0f9d-3add-a881-74e56ed488c3",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We have a meeting scheduled with Mr. **Bye** tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "У нас запланирована встреча с мистером <w>Баем</w> завтра утром."
                            }
                        },
                        {
                            "text": "The **Bye** family has lived in this village for over a century.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Бай</w> живет в этой деревне уже более века."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бай",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "dd9f148d-420b-3542-880a-0a5d60541c99",
                    "sense_definition": "Een mannelijke voornaam.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "name_sense",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn grootvader heette <w>Johannes</w>, maar iedereen noemde hem Jan.",
                            "target_lang_translations": {
                                "ru": "Моего дедушку звали <w>Йоханнес</w>, но все называли его Ян."
                            }
                        },
                        {
                            "text": "<w>Johannes</w> is een klassieke naam die nog steeds veel voorkomt.",
                            "target_lang_translations": {
                                "ru": "<w>Йоханнес</w> — это классическое имя, которое до сих пор часто встречается."
                            }
                        }
                    ],
                    "synonyms": [
                        "Jan",
                        "Johannus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Йоханнес",
                                "target_lang_sense_clarification": "нидерландское имя"
                            },
                            {
                                "target_lang_word": "Иван",
                                "target_lang_sense_clarification": "русский эквивалент имени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c4e9b9e-2cdf-313d-9277-c7e69138a09c",
                    "sense_definition": "Een profeet uit de Bijbel die Jezus doopte in de rivier de Jordaan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biblical_person",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "Op het schilderij staat <w>Johannes</w> afgebeeld in kleding van kameelhaar.",
                            "target_lang_translations": {
                                "ru": "На картине <w>Иоанн</w> изображен в одежде из верблюжьей шерсти."
                            }
                        },
                        {
                            "text": "De feestdag van <w>Johannes</w> de Doper wordt in juni gevierd.",
                            "target_lang_translations": {
                                "ru": "Праздник <w>Иоанна</w> Крестителя отмечается в июне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Johannes de Doper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пророк из Библии, крестивший Иисуса в реке Иордан."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Иоанн",
                                "target_lang_sense_clarification": "Иоанн Креститель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2f13c0f-4944-365d-98ef-75ad8c00844e",
                    "sense_definition": "Een van de twaalf apostelen van Jezus, traditioneel beschouwd als de schrijver van het vierde evangelie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biblical_person",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "Tijdens het Laatste Avondmaal zat <w>Johannes</w> naast Jezus.",
                            "target_lang_translations": {
                                "ru": "Во время Тайной вечери <w>Иоанн</w> сидел рядом с Иисусом."
                            }
                        },
                        {
                            "text": "De apostel <w>Johannes</w> speelt een belangrijke rol in het Nieuwe Testament.",
                            "target_lang_translations": {
                                "ru": "Апостол <w>Иоанн</w> играет важную роль в Новом Завете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Johannes de Apostel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из двенадцати апостолов Иисуса, традиционно считающийся автором четвертого Евангелия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Иоанн",
                                "target_lang_sense_clarification": "Иоанн Богослов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ea7d24f-94f2-304b-bc4a-2f4a77fe195a",
                    "sense_definition": "Een van de vier evangeliën in het Nieuwe Testament van de Bijbel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "bible_book",
                    "name_type": "document_or_program_name",
                    "examples": [
                        {
                            "text": "De dominee las zondag een hoofdstuk voor uit <w>Johannes</w>.",
                            "target_lang_translations": {
                                "ru": "В воскресенье пастор читал главу из <w>Иоанна</w>."
                            }
                        },
                        {
                            "text": "De tekst in <w>Johannes</w> verschilt in stijl van de andere drie evangeliën.",
                            "target_lang_translations": {
                                "ru": "Текст у <w>Иоанна</w> отличается по стилю от трех других Евангелий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Evangelie naar Johannes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одно из четырех Евангелий в Новом Завете Библии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Иоанн",
                                "target_lang_sense_clarification": "Евангелие от Иоанна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97be36b3-3031-3b54-a9bb-7d0589459052",
                    "sense_definition": "Het woord dat in het Nederlandse telefoonalfabet wordt gebruikt om de letter J aan te duiden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "Mijn postcode eindigt met de J van <w>Johannes</w>.",
                            "target_lang_translations": {
                                "ru": "Мой почтовый индекс заканчивается на J, как <w>Йоханнес</w>."
                            }
                        },
                        {
                            "text": "Om de naam duidelijk te spellen, zei hij: <w>Johannes</w>, Anton, Nico.",
                            "target_lang_translations": {
                                "ru": "Чтобы четко продиктовать имя, он сказал: <w>Йоханнес</w>, Антон, Нико."
                            }
                        }
                    ],
                    "synonyms": [
                        "Juliett"
                    ],
                    "common_phrases": [
                        "J van Johannes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в нидерландском телефонном алфавите для обозначения буквы J."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Йоханнес",
                                "target_lang_sense_clarification": "в телефонном алфавите"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
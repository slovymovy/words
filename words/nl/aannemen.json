            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b4b1c277-fd91-3a12-9476-d9d200a44f40",
                    "sense_definition": "Iets in ontvangst nemen of accepteren wat aangeboden wordt, zoals een geschenk of een uitnodiging",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "accept_receive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <nam> het geschenk dankbaar <aan>.",
                            "target_lang_translations": {
                                "ru": "Она с благодарностью <приняла> подарок.",
                                "en": "She gratefully <accepted> the gift."
                            }
                        },
                        {
                            "text": "Ik <neem> je uitnodiging graag <aan>.",
                            "target_lang_translations": {
                                "ru": "Я с удовольствием <принимаю> твое приглашение.",
                                "en": "I gladly <accept> your invitation."
                            }
                        }
                    ],
                    "synonyms": [
                        "accepteren",
                        "aanvaarden"
                    ],
                    "common_phrases": [
                        "een pakketje aannemen",
                        "de telefoon aannemen",
                        "een uitnodiging aannemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать или получать то, что предлагают, например, подарок или приглашение",
                        "en": "To accept or receive something that is offered, such as a gift or an invitation"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "получать что-либо предложенное"
                            },
                            {
                                "target_lang_word": "брать",
                                "target_lang_sense_clarification": "физически брать в руки или отвечать на звонок"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "accept",
                                "target_lang_sense_clarification": "something offered"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "a call or package"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d3217b9-b93f-375b-89e5-2fcf99d3516a",
                    "sense_definition": "Geloven of veronderstellen dat iets waar is zonder zekerheid",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "assume",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <nemen> <aan> dat je de waarheid spreekt.",
                            "target_lang_translations": {
                                "ru": "Мы <предполагаем>, что ты говоришь правду.",
                                "en": "We <assume> that you are speaking the truth."
                            }
                        },
                        {
                            "text": "Hij <nam> zonder meer <aan> dat het verhaal klopte.",
                            "target_lang_translations": {
                                "ru": "Он безоговорочно <поверил>, что история правдива.",
                                "en": "He simply <assumed> that the story was correct."
                            }
                        }
                    ],
                    "synonyms": [
                        "veronderstellen",
                        "assumeren"

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одежда",
                                "target_lang_sense_clarification": "вещи"
                            },
                            {
                                "target_lang_word": "шмотки",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "56d73b10-5c4f-3f2f-b326-deacd2d85e6f",
                    "sense_definition": "To supply or equip someone with a set of clothes, tools, or equipment.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "equip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team was <w>kitted</w> out in brand new uniforms for the season.",
                            "target_lang_translations": {
                                "ru": "Команда была <w>экипирована</w> в совершенно новую форму на сезон."
                            }
                        },
                        {
                            "text": "They <w>kitted</w> themselves up with hiking gear before climbing the mountain.",
                            "target_lang_translations": {
                                "ru": "Они <w>снарядились</w> походным инвентарем перед восхождением на гору."
                            }
                        }
                    ],
                    "synonyms": [

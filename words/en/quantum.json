                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "количество",
                                "target_lang_sense_clarification": "величина, число"
                            },
                            {
                                "target_lang_word": "объем",
                                "target_lang_sense_clarification": "мера, степень"
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "330a2235-7251-339e-8591-54eee938df0a",
                    "sense_definition": "The state or quality of being connected or linked together.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "state_of_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new high-speed train will improve <w>connectivity</w> between the major cities.",
                            "target_lang_translations": {
                                "ru": "Новый скоростной поезд улучшит <w>сообщение</w> между крупными городами."
                            }
                        },
                        {
                            "text": "Global <w>connectivity</w> allows businesses to operate across different time zones.",
                            "target_lang_translations": {
                                "ru": "Глобальная <w>взаимосвязь</w> позволяет бизнесу работать в разных часовых поясах."
                            }
                        }
                    ],
                    "synonyms": [
                        "connectedness",
                        "linkage"
                    ],
                    "antonyms": [
                        "isolation",
                        "disconnection"
                    ],
                    "common_phrases": [
                        "improve connectivity",
                        "regional connectivity",
                        "global connectivity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние или свойство быть связанным или соединенным воедино."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связность",
                                "target_lang_sense_clarification": "общая характеристика целостности системы или сети"
                            },
                            {
                                "target_lang_word": "сообщение",
                                "target_lang_sense_clarification": "транспортная связь между пунктами"
                            },
                            {
                                "target_lang_word": "взаимосвязь",
                                "target_lang_sense_clarification": "взаимная зависимость или контакт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe80364a-d198-362f-82b9-650c416b6c04",
                    "sense_definition": "The ability of computers, devices, or systems to connect to and communicate with a network.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "network_ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please check your internet <w>connectivity</w> before starting the video call.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте <w>подключение</w> к интернету перед началом видеозвонка."
                            }
                        },
                        {
                            "text": "The remote village suffers from poor <w>connectivity</w> and slow download speeds.",
                            "target_lang_translations": {
                                "ru": "Удаленная деревня страдает от плохой <w>связи</w> и низкой скорости загрузки."
                            }
                        }
                    ],
                    "synonyms": [
                        "network access",
                        "signal"
                    ],
                    "common_phrases": [
                        "internet connectivity",
                        "wireless connectivity",
                        "connectivity issues",
                        "loss of connectivity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность компьютеров или устройств подключаться к сети и обмениваться данными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "наличие сигнала или качество соединения"
                            },
                            {
                                "target_lang_word": "подключение",
                                "target_lang_sense_clarification": "доступ к сети или интернету"
                            },
                            {
                                "target_lang_word": "соединение",
                                "target_lang_sense_clarification": "технический контакт с сетью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1697d93e-4328-39e0-9d8d-3b11e2ea8c77",
                    "sense_definition": "A measure of the number of ways that points are connected to each other in a graph.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In graph theory, high <w>connectivity</w> implies that the network is robust against the removal of edges.",
                            "target_lang_translations": {
                                "ru": "В теории графов высокая <w>связность</w> означает, что сеть устойчива к удалению ребер."
                            }
                        },
                        {
                            "text": "The student calculated the vertex <w>connectivity</w> of the complex diagram.",
                            "target_lang_translations": {
                                "ru": "Студент вычислил вершинную <w>связность</w> сложной диаграммы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "graph connectivity",
                        "vertex connectivity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мера того, насколько сильно связаны вершины в графе (математический термин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связность",
                                "target_lang_sense_clarification": "термин в теории графов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

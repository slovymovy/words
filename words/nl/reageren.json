{
    "word_family": [
        "reactie",
        "reactief",
        "reagens",
        "reactor"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "817b6269-9736-39b2-a478-57b201773c77",
                    "sense_definition": "Iets doen of zeggen als antwoord op een gebeurtenis, vraag of opmerking.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_response",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>reageerde</w> helemaal niet op mijn berichtje.",
                            "target_lang_translations": {
                                "ru": "Он совсем не <w>отреагировал</w> на моё сообщение.",
                                "en": "He didn't <w>respond</w> to my message at all."
                            }
                        },
                        {
                            "text": "De politie moest snel <w>reageren</w> op de noodoproep.",
                            "target_lang_translations": {
                                "ru": "Полиция должна была быстро <w>отреагировать</w> на экстренный вызов.",
                                "en": "The police had to <w>respond</w> quickly to the emergency call."
                            }
                        },
                        {
                            "text": "Mijn ouders <w>reageerden</w> heel blij op het goede nieuws.",
                            "target_lang_translations": {
                                "ru": "Мои родители очень радостно <w>отреагировали</w> на хорошие новости.",
                                "en": "My parents <w>reacted</w> very happily to the good news."
                            }
                        }
                    ],
                    "synonyms": [
                        "antwoorden",
                        "responderen",
                        "ingaan"
                    ],
                    "antonyms": [
                        "negeren"
                    ],
                    "common_phrases": [
                        "reageren op",
                        "snel reageren",
                        "emotioneel reageren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать или говорить что-то в ответ на событие, вопрос или замечание.",
                        "en": "To do or say something in response to an event, question, or remark."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реагировать",
                                "target_lang_sense_clarification": "проявлять реакцию на что-либо"
                            },
                            {
                                "target_lang_word": "отвечать",
                                "target_lang_sense_clarification": "давать ответ"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "react",
                                "target_lang_sense_clarification": "to act in response"
                            },
                            {
                                "target_lang_word": "respond",
                                "target_lang_sense_clarification": "to answer or reply"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1df21b36-decc-3143-9f16-e179ae87ff1a",
                    "sense_definition": "Een chemische verandering ondergaan door contact met een andere stof.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "chemical_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sommige metalen <w>reageren</w> heftig als ze in contact komen met water.",
                            "target_lang_translations": {
                                "ru": "Некоторые металлы бурно <w>реагируют</w> при контакте с водой.",
                                "en": "Some metals <w>react</w> violently when they come into contact with water."
                            }
                        },
                        {
                            "text": "Deze twee stoffen <w>reageren</w> met elkaar en vormen een nieuw gas.",
                            "target_lang_translations": {
                                "ru": "Эти два вещества <w>реагируют</w> друг с другом и образуют новый газ.",
                                "en": "These two substances <w>react</w> with each other and form a new gas."
                            }
                        }
                    ],
                    "common_phrases": [
                        "reageren met"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергаться химическому изменению при контакте с другим веществом.",
                        "en": "To undergo a chemical change through contact with another substance."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реагировать",
                                "target_lang_sense_clarification": "химически"
                            },
                            {
                                "target_lang_word": "вступать в реакцию",
                                "target_lang_sense_clarification": "химический процесс"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "react",
                                "target_lang_sense_clarification": "chemically"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
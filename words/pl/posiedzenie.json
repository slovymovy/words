                },
                {
                    "sense_id": "8c999346-583d-3c55-81dd-c3a37cf2b2e7",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>posiedzieć</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

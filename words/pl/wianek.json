    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "84495ba7-3899-3418-83ba-6036e92f9786",
                    "sense_definition": "Mały wieniec z kwiatów, ziół lub liści, pleciony w kształcie obręczy i noszony na głowie jako ozdoba.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "head_ornament",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda dziewczyna uplotła piękny <w>wianek</w> z polnych kwiatów i założyła go na włosy.",
                            "target_lang_translations": {
                                "ru": "Молодая девушка сплела красивый <w>венок</w> из полевых цветов и надела его на волосы."
                            }
                        },
                        {
                            "text": "Zgodnie z tradycją w noc świętojańską panny puszczają <w>wianki</w> na rzekę.",
                            "target_lang_translations": {
                                "ru": "Согласно традиции, в ночь на Ивана Купалу девушки пускают <w>венки</w> по реке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pleść wianek",
                        "puszczać wianki",
                        "wianek z kwiatów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький венок из цветов, трав или листьев, сплетенный в форме кольца, который носят на голове как украшение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "венок",
                                "target_lang_sense_clarification": "украшение из цветов на голову"
                            },
                            {
                                "target_lang_word": "веночек",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "809cf46e-5007-3016-9ba5-a598e72a0914",
                    "sense_definition": "Metaforyczne lub dawne określenie na dziewictwo i niewinność kobiety.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "virginity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starych pieśniach ludowych często pojawia się motyw dziewczyny, która straciła swój <w>wianek</w>.",
                            "target_lang_translations": {
                                "ru": "В старинных народных песнях часто встречается мотив девушки, которая потеряла свой <w>венок</w>."
                            }
                        },
                        {
                            "text": "Dla wielu kultur <w>wianek</w> był symbolem czystości panny młodej w dniu ślubu.",
                            "target_lang_translations": {
                                "ru": "Для многих культур <w>венок</w> был символом чистоты невесты в день свадьбы."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziewictwo",
                        "cnota"
                    ],
                    "common_phrases": [
                        "stracić wianek"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly found in literature or folklore"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Метафорическое или устаревшее обозначение девственности и невинности женщины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девственность",
                                "target_lang_sense_clarification": "значение символа"
                            },
                            {
                                "target_lang_word": "невинность",
                                "target_lang_sense_clarification": "синоним девственности"
                            },
                            {
                                "target_lang_word": "венок",
                                "target_lang_sense_clarification": "символ девственности в фольклоре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "581b5881-3b92-333e-9360-aafd355787d2",
                    "sense_definition": "Ozdobna rózga lub gałązka pełniąca funkcję rekwizytu w dawnym obrzędzie weselnym.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "folklore_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas tradycyjnego wesela starosta trzymał w ręku ozdobny <w>wianek</w>.",
                            "target_lang_translations": {
                                "ru": "Во время традиционной свадьбы староста держал в руке украшенный <w>венок</w>."
                            }
                        },
                        {
                            "text": "Etnografowie opisują <w>wianek</w> jako ważny element dawnych rytuałów przejścia.",
                            "target_lang_translations": {
                                "ru": "Этнографы описывают <w>венок</w> как важный элемент древних ритуалов перехода."
                            }
                        }
                    ],
                    "synonyms": [
                        "rózga weselna"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical folklore context"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "specific to certain folk traditions"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшенная розга или ветка, служащая реквизитом в старинном свадебном обряде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свадебный венок",
                                "target_lang_sense_clarification": "ритуальный предмет"
                            },
                            {
                                "target_lang_word": "ритуальный венок",
                                "target_lang_sense_clarification": "обрядовый реквизит"
                            }
                        ]
                    }
                }
            ]
        }
    ]

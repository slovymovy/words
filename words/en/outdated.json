{
    "word_family": [
        "date",
        "dated",
        "outdate",
        "update"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "056332a2-f24d-3d28-8372-c42e0fd5b428",
                    "sense_definition": "No longer considered useful, interesting, or modern; old-fashioned.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory was closed down because of its <w>outdated</w> machinery.",
                            "target_lang_translations": {
                                "ru": "Завод закрыли из-за его <w>устаревшего</w> оборудования."
                            }
                        },
                        {
                            "text": "Many people feel that these laws are <w>outdated</w> and need to be changed.",
                            "target_lang_translations": {
                                "ru": "Многие считают эти законы <w>устаревшими</w> и требующими изменений."
                            }
                        }
                    ],
                    "antonyms": [
                        "modern",
                        "trendy",
                        "in fashion",
                        "in style"
                    ],
                    "common_phrases": [
                        "outdated equipment",
                        "outdated system",
                        "outdated methods",
                        "outdated ideas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Больше не считающийся полезным или современным; старомодный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устаревший",
                                "target_lang_sense_clarification": "вышедший из употребления"
                            },
                            {
                                "target_lang_word": "старомодный",
                                "target_lang_sense_clarification": "не соответствующий современной моде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "994b37de-baad-32df-8f11-cf6849179faf",
                    "sense_definition": "Not the most recent version; containing information that is no longer correct.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please update your browser, as you are using an <w>outdated</w> version.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, обновите ваш браузер, так как вы используете <w>устаревшую</w> версию."
                            }
                        },
                        {
                            "text": "The report contained <w>outdated</w> information that misled the investors.",
                            "target_lang_translations": {
                                "ru": "В отчете содержалась <w>устаревшая</w> информация, которая ввела инвесторов в заблуждение."
                            }
                        }
                    ],
                    "synonyms": [
                        "superseded"
                    ],
                    "antonyms": [
                        "current",
                        "up to date"
                    ],
                    "common_phrases": [
                        "outdated version",
                        "outdated information",
                        "outdated software",
                        "outdated data"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не самая последняя версия; содержащий информацию, которая больше не является верной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устаревший",
                                "target_lang_sense_clarification": "о версии или программном обеспечении"
                            },
                            {
                                "target_lang_word": "неактуальный",
                                "target_lang_sense_clarification": "об информации или данных"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "oversight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please report any accidents to your <w>supervisor</w> immediately."
                        },
                        {
                            "text": "My direct <w>supervisor</w> is responsible for assigning daily tasks to the team."
                        }
                    ],
                    "synonyms": [
                        "manager",
                        "overseer",
                        "boss",
                        "foreman",
                        "chief"
                    ],
                    "antonyms": [

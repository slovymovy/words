            "pos": "name",
            "senses": [
                {
                    "sense_id": "51f496c5-333d-3c01-b219-b5a30d85d7b7",
                    "sense_definition": "A country located in West Asia on the southern end of the Arabian Peninsula.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "yemen_country",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The Republic of <w>Yemen</w> has a rich history that dates back thousands of years.",
                            "target_lang_translations": {
                                "ru": "Республика <w>Йемен</w> имеет богатую историю, уходящую корнями в тысячелетия."
                            }
                        },
                        {
                            "text": "Many ancient trade routes passed through <w>Yemen</w> on their way to the Mediterranean.",
                            "target_lang_translations": {
                                "ru": "Многие древние торговые пути проходили через <w>Йемен</w> по дороге к Средиземному морю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Republic of Yemen",
                        "history of Yemen",
                        "capital of Yemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страна, расположенная в Западной Азии на южной оконечности Аравийского полуострова."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Йемен",
                                "target_lang_sense_clarification": "страна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7573c30-1c89-35eb-b84d-39b1f049dac0",
                    "sense_definition": "A historical region in the southern Arabian Peninsula, historically known as Arabia Felix.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "yemen_region",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Ancient geographers referred to the fertile lands of <w>Yemen</w> as Arabia Felix.",
                            "target_lang_translations": {
                                "ru": "Древние географы называли плодородные земли <w>Йемена</w> Счастливой Аравией."
                            }
                        },
                        {
                            "text": "The kingdoms of old <w>Yemen</w> grew wealthy from the trade in frankincense and myrrh.",
                            "target_lang_translations": {
                                "ru": "Королевства древнего <w>Йемена</w> разбогатели на торговле ладаном и миррой."
                            }
                        }
                    ],
                    "synonyms": [
                        "Arabia Felix"
                    ],
                    "common_phrases": [
                        "ancient Yemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исторический регион на юге Аравийского полуострова, исторически известный как Счастливая Аравия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Йемен",
                                "target_lang_sense_clarification": "историческая область"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ba7a02ad-1282-35be-b96d-d538f2705db1",
                    "sense_definition": "Сравнительная степень прилагательного <w>важный</w>; обладающий большим значением, весом или влиянием по сравнению с чем-либо другим.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "comp_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Семья для него сейчас <w>важнее</w> карьеры.",
                            "target_lang_translations": {
                                "en": "Family is <w>more important</w> to him now than his career."
                            }
                        },
                        {
                            "text": "Нет ничего <w>важнее</w>, чем здоровье близких людей.",
                            "target_lang_translations": {
                                "en": "There is nothing <w>more important</w> than the health of loved ones."
                            }
                        }
                    ],
                    "synonyms": [
                        "более важный",
                        "значительнее"
                    ],
                    "antonyms": [
                        "менее важный",
                        "второстепеннее"
                    ],
                    "common_phrases": [
                        "нет ничего важнее",
                        "что важнее",
                        "намного важнее"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "comparative degree"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Comparative degree of the adjective 'important'; having greater significance, weight, or influence compared to something else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "more important",
                                "target_lang_sense_clarification": "general comparative significance"
                            },
                            {
                                "target_lang_word": "of greater importance",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1f4c1648-3c01-34a6-9204-e639deb91c7b",
                    "sense_definition": "Сравнительная степень наречия <w>важно</w>; с большей значительностью или многозначительностью (о манере поведения).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "comp_adv_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник посмотрел на нас ещё <w>важнее</w> и продолжил речь.",
                            "target_lang_translations": {
                                "en": "The boss looked at us even <w>more pompously</w> and continued his speech."
                            }
                        },
                        {
                            "text": "Он стал ходить <w>важнее</w>, подняв голову высоко вверх.",
                            "target_lang_translations": {
                                "en": "He began to walk <w>more self-importantly</w>, holding his head high."
                            }
                        }
                    ],
                    "synonyms": [
                        "более важно",
                        "надменнее"
                    ],
                    "antonyms": [
                        "менее важно",
                        "скромнее"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "comparative degree"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Comparative degree of the adverb 'importantly'; with greater significance or self-importance (regarding manner of behavior)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "more pompously",
                                "target_lang_sense_clarification": "referring to arrogant behavior"
                            },
                            {
                                "target_lang_word": "more self-importantly",
                                "target_lang_sense_clarification": "referring to manner or attitude"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abb28e31-219e-3993-bbdb-a9cc75a14ec7",
                    "sense_definition": "Употребляется в значении сказуемого: более существенно, более значимо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "predicative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Важнее</w> всего то, что мы успели закончить проект вовремя.",
                            "target_lang_translations": {
                                "en": "What is <w>more important</w> than anything is that we managed to finish the project on time."
                            }
                        },
                        {
                            "text": "Мне <w>важнее</w> знать правду, чем слышать приятную ложь.",
                            "target_lang_translations": {
                                "en": "It is <w>more important</w> for me to know the truth than to hear a pleasant lie."
                            }
                        }
                    ],
                    "synonyms": [
                        "существеннее",
                        "значимее"
                    ],
                    "antonyms": [
                        "менее важно",
                        "безразличнее"
                    ],
                    "common_phrases": [
                        "важнее всего",
                        "гораздо важнее"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "comparative degree"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used as a predicate: more essential, more significant."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "more important",
                                "target_lang_sense_clarification": "predicative use"
                            },
                            {
                                "target_lang_word": "matters more",
                                "target_lang_sense_clarification": "verb phrase equivalent"
                            }
                        ]
                    }
                }
            ]
        }

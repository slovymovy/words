                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обучать",
                                "target_lang_sense_clarification": "давать знания, учить чему-либо"
                            },
                            {
                                "target_lang_word": "воспитывать",
                                "target_lang_sense_clarification": "формировать личность, прививать навыки поведения"
                            },
                            {
                                "target_lang_word": "просвещать",
                                "target_lang_sense_clarification": "распространять знания среди широкой аудитории"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c59bf926-6ab9-36cf-9c67-df8092657e3e",
                    "sense_definition": "Запас ресурсов или средств, сохраняемый для использования в будущем при необходимости.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Государство обязано иметь золотой <резерв> для поддержания экономики.",
                            "target_lang_translations": {
                                "en": "The state is obliged to have a gold <reserve> to support the economy."
                            }
                        },
                        {
                            "text": "Мы оставили небольшой денежный <резерв> на случай непредвиденных расходов.",
                            "target_lang_translations": {
                                "en": "We left a small cash <reserve> in case of unforeseen expenses."
                            }
                        }
                    ],
                    "synonyms": [
                        "запас"
                    ],
                    "common_phrases": [
                        "золотой резерв",
                        "валютный резерв",
                        "государственный резерв",
                        "кадровый резерв"
                    ],
                    "target_lang_definitions": {
                        "en": "A supply of resources or funds kept for future use when needed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reserve",
                                "target_lang_sense_clarification": "funds or resources"
                            },
                            {
                                "target_lang_word": "stockpile",
                                "target_lang_sense_clarification": "accumulated supply"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c1374bd-5f7e-35ee-b540-224a26c47b7b",
                    "sense_definition": "Часть войск или военнообязанных граждан, предназначенная для использования в особых случаях или при мобилизации.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир решил не вводить <резерв> в бой до решающего момента.",
                            "target_lang_translations": {
                                "en": "The commander decided not to send the <reserve> into battle until the decisive moment."
                            }
                        },
                        {
                            "text": "После окончания службы офицер был зачислен в <резерв>.",
                            "target_lang_translations": {
                                "en": "After finishing his service, the officer was transferred to the <reserve>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "находиться в резерве",
                        "стратегический резерв",
                        "трудовые резервы",
                        "вывести в резерв"
                    ],
                    "target_lang_definitions": {
                        "en": "A body of troops or conscripts kept back for special use or mobilization."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reserve",
                                "target_lang_sense_clarification": "military force"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df87387d-88e0-3fa9-b61a-1793b6673157",
                    "sense_definition": "Скрытые, ещё не использованные возможности или силы (переносное значение).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спортсмен исчерпал все физические <резервы> своего организма на финише.",
                            "target_lang_translations": {
                                "en": "The athlete exhausted all the physical <reserves> of his body at the finish line."
                            }
                        },
                        {
                            "text": "Нам нужно найти внутренние <резервы> для повышения эффективности работы.",
                            "target_lang_translations": {
                                "en": "We need to find internal <reserves> to increase work efficiency."
                            }
                        }
                    ],
                    "synonyms": [
                        "запас",
                        "запас"
                    ],
                    "common_phrases": [
                        "скрытые резервы",
                        "внутренние резервы",
                        "поиск резервов",
                        "исчерпать резервы"
                    ],
                    "target_lang_definitions": {
                        "en": "Hidden, unused capabilities, strengths, or energy (figurative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reserve",
                                "target_lang_sense_clarification": "hidden potential or energy"
                            },
                            {
                                "target_lang_word": "hidden resource",
                                "target_lang_sense_clarification": "figurative"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            {
                                "target_lang_word": "назад",
                                "target_lang_sense_clarification": "направление движения"
                            },
                            {
                                "target_lang_word": "задним ходом",
                                "target_lang_sense_clarification": "способ движения транспорта"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "backwards",
                                "target_lang_sense_clarification": "direction of movement"
                            },
                            {
                                "target_lang_word": "back",
                                "target_lang_sense_clarification": "movement away from front"
                            },
                            {
                                "target_lang_word": "backward",
                                "target_lang_sense_clarification": "direction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c0d64a8-8ad6-386b-befb-d61135df717d",
                    "sense_definition": "In een slechtere toestand komen; in kwaliteit of gezondheid verminderen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "deterioration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gezondheid van de patiënt ging na de operatie snel <w>achteruit</w>.",
                            "target_lang_translations": {
                                "ru": "Здоровье пациента после операции быстро пошло <w>на спад</w>.",
                                "en": "The patient's health went <w>downhill</w> rapidly after the surgery."
                            }
                        },
                        {
                            "text": "Sinds de economische crisis gaat het bedrijf hard <w>achteruit</w>.",
                            "target_lang_translations": {
                                "ru": "С момента экономического кризиса дела компании идут <w>к худшему</w>.",
                                "en": "Since the economic crisis, the company has been going <w>downhill</w> fast."
                            }
                        }
                    ],
                    "antonyms": [
                        "vooruit"
                    ],
                    "common_phrases": [
                        "snel achteruit",
                        "hard achteruit",
                        "erop achteruit gaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить в худшее состояние; снижение качества или здоровья.",
                        "en": "To come into a worse state; to decline in quality or health."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на спад",
                                "target_lang_sense_clarification": "об ухудшении состояния"
                            },
                            {
                                "target_lang_word": "к худшему",
                                "target_lang_sense_clarification": "негативное изменение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "downhill",

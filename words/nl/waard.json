                {
                    "sense_id": "45efa71f-2c7b-3950-9533-b989e15c47b1",
                    "sense_definition": "Een bepaalde financiële waarde hebbend; in geld uit te drukken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "value_financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat oude horloge is veel geld <w>waard</w>.",
                            "target_lang_translations": {
                                "ru": "Эти старые часы <w>стоят</w> больших денег.",
                                "en": "That old watch is <w>worth</w> a lot of money."
                            }
                        },
                        {
                            "text": "Ik vraag me af hoeveel dit schilderij tegenwoordig <w>waard</w> is.",
                            "target_lang_translations": {
                                "ru": "Интересно, сколько сейчас <w>стоит</w> эта картина.",
                                "en": "I wonder how much this painting is <w>worth</w> nowadays."
                            }
                        }
                    ],
                    "common_phrases": [
                        "geld waard zijn",
                        "niets waard",
                        "goud waard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий определенную финансовую стоимость; оцениваемый в деньгах.",
                        "en": "Having a specific financial value; expressible in money."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоящий",
                                "target_lang_sense_clarification": "имеющий цену"
                            },
                            {

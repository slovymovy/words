                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "upside down",
                                "target_lang_sense_clarification": "top turned to the bottom"
                            },
                            {
                                "target_lang_word": "inverted",
                                "target_lang_sense_clarification": "technically or optically reversed"
                            },
                            {
                                "target_lang_word": "reversed",
                                "target_lang_sense_clarification": "general physical reversal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "перевёрнутый",
                                "target_lang_sense_clarification": "вверх дном или задом наперёд"
                            }
                        ]
                    }
                }
            ]
        },
        {

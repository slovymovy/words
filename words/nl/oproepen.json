{
    "word_family": [
        "oproep",
        "oproeping"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "594c440d-64c9-3880-87ed-fec4dea0a866",
                    "sense_definition": "Dringend vragen of aanmoedigen om iets te doen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De burgemeester <w>roept op</w> tot kalmte na de onrustige nacht in de stad.",
                            "target_lang_translations": {
                                "ru": "Мэр <w>призывает</w> к спокойствию после беспокойной ночи в городе.",
                                "en": "The mayor <w>calls for</w> calm after the restless night in the city."
                            }
                        },
                        {
                            "text": "Wij <w>roepen</w> iedereen <w>op</w> om te gaan stemmen bij de verkiezingen.",
                            "target_lang_translations": {
                                "ru": "Мы <w>призываем</w> всех пойти голосовать на выборах.",
                                "en": "We <w>call on</w> everyone to vote in the elections."
                            }
                        }
                    ],
                    "synonyms": [
                        "aansporen",
                        "verzoeken",
                        "aanmanen"
                    ],
                    "antonyms": [
                        "afraden",
                        "ontmoedigen"
                    ],
                    "common_phrases": [
                        "oproepen tot kalmte",
                        "oproepen tot actie",
                        "iemand oproepen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво просить или побуждать сделать что-либо.",
                        "en": "To urgently ask or encourage someone to do something."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "призывать",
                                "target_lang_sense_clarification": "побуждать к действию"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "call on",
                                "target_lang_sense_clarification": "someone to do something"
                            },
                            {
                                "target_lang_word": "call for",
                                "target_lang_sense_clarification": "an action or result"
                            },
                            {
                                "target_lang_word": "urge",
                                "target_lang_sense_clarification": "strongly encourage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc34e2ae-50fa-3816-b47a-866cc427be58",
                    "sense_definition": "Officieel bevelen om ergens te verschijnen, bijvoorbeeld voor de rechtbank of militaire dienst.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter kan getuigen <w>oproepen</w> om een verklaring af te leggen.",
                            "target_lang_translations": {
                                "ru": "Судья может <w>вызвать</w> свидетелей для дачи показаний.",
                                "en": "The judge can <w>summon</w> witnesses to make a statement."
                            }
                        },
                        {
                            "text": "De regering <w>riep</w> alle weerbare mannen <w>op</w> voor de verdediging van het land.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>призвало</w> всех боеспособных мужчин для защиты страны.",
                                "en": "The government <w>called up</w> all able-bodied men for the defense of the country."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontbieden",
                        "sommeren"
                    ],
                    "common_phrases": [
                        "getuigen oproepen",
                        "dienstplichtigen oproepen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально приказывать явиться куда-либо, например в суд или на военную службу.",
                        "en": "To officially order someone to appear, for example in court or for military service."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "в суд, к должностному лицу"
                            },
                            {
                                "target_lang_word": "призывать",
                                "target_lang_sense_clarification": "на военную службу"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "summon",
                                "target_lang_sense_clarification": "to court or official meeting"
                            },
                            {
                                "target_lang_word": "call up",
                                "target_lang_sense_clarification": "for military service"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64554509-65f8-3a61-aa1f-edc067f4e3df",
                    "sense_definition": "Beelden, herinneringen of gevoelens in de geest naar boven halen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die oude foto's <w>roepen</w> mooie herinneringen <w>op</w> aan mijn jeugd.",
                            "target_lang_translations": {
                                "ru": "Эти старые фотографии <w>пробуждают</w> приятные воспоминания о моей юности.",
                                "en": "Those old photos <w>bring back</w> beautiful memories of my childhood."
                            }
                        },
                        {
                            "text": "De geur van vers brood <w>roept</w> direct een gevoel van thuis <w>op</w>.",
                            "target_lang_translations": {
                                "ru": "Запах свежего хлеба сразу <w>вызывает</w> чувство дома.",
                                "en": "The smell of fresh bread immediately <w>evokes</w> a feeling of home."
                            }
                        }
                    ],
                    "synonyms": [
                        "opwekken",
                        "evoceren"
                    ],
                    "antonyms": [
                        "onderdrukken"
                    ],
                    "common_phrases": [
                        "herinneringen oproepen",
                        "een beeld oproepen",
                        "gevoelens oproepen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать в сознании образы, воспоминания или чувства.",
                        "en": "To bring images, memories, or feelings to mind."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "чувства, эмоции"
                            },
                            {
                                "target_lang_word": "пробуждать",
                                "target_lang_sense_clarification": "воспоминания"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "evoke",
                                "target_lang_sense_clarification": "feelings or memories"
                            },
                            {
                                "target_lang_word": "conjure up",
                                "target_lang_sense_clarification": "images or atmosphere"
                            },
                            {
                                "target_lang_word": "call up",
                                "target_lang_sense_clarification": "memories or images"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ad2f43cb-74c2-323f-9ef6-1070491e081a",
                    "sense_definition": "Meervoud van <w>oproep</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik had drie gemiste <w>oproepen</w> op mijn telefoon toen ik wakker werd.",
                            "target_lang_translations": {
                                "ru": "У меня было три пропущенных <w>вызова</w> на телефоне, когда я проснулся.",
                                "en": "I had three missed <w>calls</w> on my phone when I woke up."
                            }
                        },
                        {
                            "text": "De vele <w>oproepen</w> tot vrede werden helaas genegeerd door de strijdende partijen.",
                            "target_lang_translations": {
                                "ru": "Многие <w>призывы</w> к миру были, к сожалению, проигнорированы воюющими сторонами.",
                                "en": "The many <w>calls</w> for peace were unfortunately ignored by the warring parties."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова oproep (вызов, призыв).",
                        "en": "Plural of 'oproep' (call, appeal)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызовы",
                                "target_lang_sense_clarification": "телефонные звонки"
                            },
                            {
                                "target_lang_word": "призывы",
                                "target_lang_sense_clarification": "обращения, лозунги"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "calls",
                                "target_lang_sense_clarification": "phone calls or general requests"
                            },
                            {
                                "target_lang_word": "appeals",
                                "target_lang_sense_clarification": "urgent requests"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
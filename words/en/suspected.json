                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола suspect."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подозревать",
                                "target_lang_sense_clarification": "считать виновным или нечестным"
                            },
                            {
                                "target_lang_word": "предполагать",
                                "target_lang_sense_clarification": "допускать возможность чего-либо"
                            }

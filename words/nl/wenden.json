                    ],
                    "target_lang_definitions": {
                        "en": "To turn a sailboat through the wind to sail on the other tack.",
                        "ru": "Совершать поворот на парусном судне, пересекая линию ветра носом (оверштаг)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tack",
                                "target_lang_sense_clarification": "nautical maneuver"
                            },
                            {
                                "target_lang_word": "go about",
                                "target_lang_sense_clarification": "nautical: change course"
                            },
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "general change of direction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "делать поворот оверштаг",
                                "target_lang_sense_clarification": "морской термин"
                            },
                            {
                                "target_lang_word": "менять галс",
                                "target_lang_sense_clarification": "менять курс относительно ветра"
                            }
                        ]
                    }
                }

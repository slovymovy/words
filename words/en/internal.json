    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "dda64223-5e16-3d80-8a9e-2c34dff44786",
                    "sense_definition": "Situated on the inside or within something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_inside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>internal</w> structure of the building was damaged by the fire.",
                            "target_lang_translations": {
                                "ru": "<w>Внутренняя</w> структура здания была повреждена пожаром."
                            }
                        },
                        {
                            "text": "We need to examine the <w>internal</w> parts of the machine to find the problem.",
                            "target_lang_translations": {
                                "ru": "Нам нужно осмотреть <w>внутренние</w> части машины, чтобы найти проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "interior",
                        "inner"
                    ],
                    "antonyms": [
                        "external",
                        "exterior"
                    ],
                    "common_phrases": [
                        "internal structure",
                        "internal mechanism",
                        "internal walls"
                    ],
                    "target_lang_definitions": {

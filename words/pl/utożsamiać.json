{
    "word_family": [
        "tożsamość",
        "tożsamy",
        "utożsamienie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0eeadef4-5593-30d0-aed6-fa72c0b4d5d5",
                    "sense_definition": "Odczuwać silną więź z jakąś osobą, grupą społeczną lub ideą; uważać się za członka jakiejś społeczności lub zwolennika danych poglądów (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "identyfikacja_osobista",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu czytelników <utożsamia się> z głównym bohaterem tej powieści.",
                            "target_lang_translations": {
                                "ru": "Многие читатели <отождествляют себя> с главным героем этого романа."
                            }
                        },
                        {
                            "text": "Nie <utożsamiam się> z decyzjami politycznymi, które podejmuje obecny rząd.",
                            "target_lang_translations": {
                                "ru": "Я не <отождествляю себя> с политическими решениями, которые принимает нынешнее правительство."
                            }
                        }
                    ],
                    "synonyms": [
                        "identyfikować się",
                        "solidaryzować się",
                        "czuć więź"
                    ],
                    "antonyms": [
                        "dystansować się",
                        "odcinać się"
                    ],
                    "common_phrases": [
                        "utożsamiać się z grupą",
                        "utożsamiać się z poglądami",
                        "w pełni się utożsamiać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствовать сильную связь с кем-либо, группой или идеей; считать себя членом сообщества или сторонником взглядов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отождествлять",
                                "target_lang_sense_clarification": "себя"
                            },
                            {
                                "target_lang_word": "ассоциировать",
                                "target_lang_sense_clarification": "себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20df8243-8b0a-30c1-aa79-aeaeaafb1dde",
                    "sense_definition": "Uznawać dwie różne osoby, rzeczy lub zjawiska za to samo; stawiać znak równości między czymś a czymś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "porównywanie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ludzie często błędnie <utożsamiają> bogactwo z prawdziwym szczęściem.",
                            "target_lang_translations": {
                                "ru": "Люди часто ошибочно <отождествляют> богатство с истинным счастьем."
                            }
                        },
                        {
                            "text": "Nie należy <utożsamiać> milczenia ze zgodą w każdej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Не следует <отождествлять> молчание с согласием в любой ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrównywać",
                        "identyfikować",
                        "łączyć"
                    ],
                    "antonyms": [
                        "odróżniać",
                        "rozróżniać",
                        "przeciwstawiać"
                    ],
                    "common_phrases": [
                        "utożsamiać kogoś z czymś",
                        "błędnie utożsamiać",
                        "utożsamiać skutek z przyczyną"
                    ],
                    "target_lang_definitions": {
                        "ru": "Признавать разные лица, вещи или явления одним и тем же; ставить знак равенства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отождествлять",
                                "target_lang_sense_clarification": "признавать тождественным"
                            },
                            {
                                "target_lang_word": "ассоциировать",
                                "target_lang_sense_clarification": "связывать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
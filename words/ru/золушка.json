                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "fictional_name",
                    "examples": [
                        {
                            "text": "Каждая девочка в детстве мечтает быть похожей на <w>Золушку</w>."
                        },
                        {
                            "text": "Мы всей семьёй смотрели старый добрый фильм про <w>Золушку</w>."
                        }
                    ],
                    "common_phrases": [
                        "сказка про Золушку",
                        "туфелька Золушки",
                        "бедная Золушка"

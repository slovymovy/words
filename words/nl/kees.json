    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e1578d35-ce76-3e28-9460-e479bc474573",
                    "sense_definition": "Een veelvoorkomende, traditionele Nederlandse jongensnaam.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "name_1",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn buurman heet <w>Kees</w> en hij is erg aardig.",
                            "target_lang_translations": {
                                "ru": "Моего соседа зовут <w>Кес</w>, и он очень милый."
                            }
                        },
                        {
                            "text": "Klaar is <w>Kees</w>, we kunnen naar huis.",
                            "target_lang_translations": {
                                "ru": "<w>Дело в шляпе</w>, мы можем идти домой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Klaar is Kees",
                        "Jan, Piet en Kees"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенное традиционное голландское мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кес",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f21a0564-7684-3168-ab9f-af0079afcbbc",
                    "sense_definition": "Een hondenras met een dikke vacht, opstaande oren en een gekrulde staart; verkorting van keeshond.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dog",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>kees</w> blafte luid toen de postbode aan de deur kwam.",
                            "target_lang_translations": {
                                "ru": "<w>Кесхонд</w> громко залаял, когда почтальон подошел к двери."
                            }
                        },
                        {
                            "text": "Zij heeft al jaren een trouwe <w>kees</w> als huisdier.",
                            "target_lang_translations": {
                                "ru": "У нее уже много лет живет верный <w>кесхонд</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "keeshond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Порода собак с густой шерстью, стоячими ушами и закрученным хвостом; сокращение от кесхонд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кесхонд",
                                "target_lang_sense_clarification": "порода собак"
                            },
                            {
                                "target_lang_word": "шпиц",
                                "target_lang_sense_clarification": "разновидность собаки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0123b9b4-19af-3dcf-a695-28e7922a5d8a",
                    "sense_definition": "Een historische scheldnaam of geuzennaam voor een patriot in de 18e eeuw.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de geschiedenisboeken wordt de strijd tussen de prinsgezinden en elke <w>kees</w> beschreven.",
                            "target_lang_translations": {
                                "ru": "В учебниках истории описывается борьба между оранжистами и каждым <w>кесом</w>."
                            }
                        },
                        {
                            "text": "Hij werd door zijn tegenstanders spottend een <w>kees</w> genoemd.",
                            "target_lang_translations": {
                                "ru": "Противники насмешливо называли его <w>кесом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "patriot"
                    ],
                    "antonyms": [
                        "prinsgezinde"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Историческое прозвище нидерландских патриотов в XVIII веке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кес",
                                "target_lang_sense_clarification": "историческое прозвище"
                            },
                            {
                                "target_lang_word": "патриот",
                                "target_lang_sense_clarification": "историческое значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "42633e27-2f32-390d-b262-53e32ecdb07c",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>kezen</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kees</w> graag een potje kaarten met mijn familie.",
                            "target_lang_translations": {
                                "ru": "Я люблю <w>играть в кезен</w> в карты с семьей."
                            }
                        },
                        {
                            "text": "Elke vrijdagavond <w>kees</w> ik met mijn vrienden in het café.",
                            "target_lang_translations": {
                                "ru": "Каждый вечер пятницы я <w>играю в кезен</w> с друзьями в кафе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени от глагола kezen (играть в кезен)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "играю в кезен",
                                "target_lang_sense_clarification": "настольная игра"
                            }
                        ]
                    }
                }
            ]
        }
    ]

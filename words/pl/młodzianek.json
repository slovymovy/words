    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2af7f2bc-2d21-308e-8997-f1f23131d079",
                    "sense_definition": "Młody chłopiec lub mężczyzna, często postrzegany jako niedoświadczony; zdrobnienie od słowa <w>młodzian</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "young_male",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nieśmiały <w>młodzianek</w> nie wiedział, jak zachować się w towarzystwie.",
                            "target_lang_translations": {
                                "ru": "Этот застенчивый <w>юнец</w> не знал, как вести себя в обществе."
                            }
                        },
                        {
                            "text": "Na przyjęciu pojawił się urodziwy <w>młodzianek</w>, który od razu wzbudził zainteresowanie.",
                            "target_lang_translations": {
                                "ru": "На приеме появился красивый <w>юноша</w>, который сразу вызвал интерес."
                            }
                        }
                    ],
                    "synonyms": [
                        "niewiniątko",
                        "niewiniątko"
                    ],
                    "antonyms": [
                        "starzec"
                    ],
                    "common_phrases": [
                        "urodziwy młodzianek",
                        "niedoświadczony młodzianek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодой мальчик или мужчина, часто воспринимаемый как неопытный; уменьшительная форма от слова młodzian."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юнец",
                                "target_lang_sense_clarification": "неопытный молодой человек"
                            },
                            {
                                "target_lang_word": "юноша",
                                "target_lang_sense_clarification": "молодой человек"
                            },
                            {
                                "target_lang_word": "паренёк",
                                "target_lang_sense_clarification": "разговорное, уменьшительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a9bad50-4cf6-3b26-9e22-7eb1e12ed9c5",
                    "sense_definition": "W tradycji biblijnej: jedno z niemowląt płci męskiej zamordowanych w Betlejem na rozkaz Heroda.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biblical_figure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W kalendarzu liturgicznym obchodzimy święto upamiętniające biblijnych <w>młodzianków</w>.",
                            "target_lang_translations": {
                                "ru": "В литургическом календаре мы отмечаем праздник в память о библейских <w>младенцах</w>."
                            }
                        },
                        {
                            "text": "Malarz przedstawił na obrazie rzeź niewinnych <w>młodzianków</w> w Judei.",
                            "target_lang_translations": {
                                "ru": "Художник изобразил на картине избиение невинных <w>младенцев</w> в Иудее."
                            }
                        }
                    ],
                    "synonyms": [
                        "niewiniątko"
                    ],
                    "common_phrases": [
                        "Święci Młodziankowie"
                    ],
                    "target_lang_definitions": {
                        "ru": "В библейской традиции: один из младенцев мужского пола, убитых в Вифлееме по приказу Ирода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "младенец",
                                "target_lang_sense_clarification": "библейский контекст (Вифлеемские младенцы)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c7e9edd-169a-3328-9fde-c8e568e02cb3",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>młodzianka</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tamtych czasach wiele <w>młodzianek</w> marzyło o szybkim zamążpójściu.",
                            "target_lang_translations": {
                                "ru": "В те времена многие <w>девицы</w> мечтали о скором замужестве."
                            }
                        },
                        {
                            "text": "Głosy roześmianych <w>młodzianek</w> niosły się echem po szkolnym korytarzu.",
                            "target_lang_translations": {
                                "ru": "Голоса смеющихся <w>девушек</w> эхом разносились по школьному коридору."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "dopełniacz liczby mnogiej od: młodzianka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного młodzianka (девушка, девица)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девиц",
                                "target_lang_sense_clarification": "форма родительного падежа мн. ч."
                            },
                            {
                                "target_lang_word": "девушек",
                                "target_lang_sense_clarification": "форма родительного падежа мн. ч."
                            }
                        ]
                    }
                }
            ]
        }
    ]

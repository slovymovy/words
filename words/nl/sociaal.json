            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "40702746-1967-3231-836f-e03aeaa7a176",
                    "sense_definition": "Gevoel hebbend voor de nood van anderen; vriendelijk en behulpzaam in de omgang",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn nieuwe buurvrouw is heel <w>sociaal</w> en maakt makkelijk contact.",
                            "target_lang_translations": {
                                "ru": "Моя новая соседка очень <w>общительная</w> и легко идет на контакт."
                            }
                        },
                        {
                            "text": "Het is niet erg <w>sociaal</w> om de hele avond op je telefoon te kijken.",
                            "target_lang_translations": {
                                "ru": "Смотреть весь вечер в телефон — это не очень <w>общительно</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "asociaal",
                        "onsociaal",
                        "antisociaal"
                    ],
                    "common_phrases": [
                        "sociaal vaardig",
                        "sociaal gedrag",
                        "sociaal contact"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чуткий к нуждам других; дружелюбный и отзывчивый в общении"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общительный",
                                "target_lang_sense_clarification": "легко вступающий в контакт"
                            },
                            {
                                "target_lang_word": "отзывчивый",
                                "target_lang_sense_clarification": "готовый помочь другим"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38679b99-01f5-3cb8-b547-2649e9249c95",
                    "sense_definition": "Betrekking hebbend op de menselijke samenleving",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>sociale</w> media hebben een grote invloed op de jeugd.",
                            "target_lang_translations": {
                                "ru": "<w>Социальные</w> сети оказывают большое влияние на молодежь."
                            }
                        },
                        {
                            "text": "Hij heeft een grote <w>sociale</w> achterstand door zijn moeilijke jeugd.",
                            "target_lang_translations": {
                                "ru": "У него большое <w>социальное</w> отставание из-за трудного детства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sociale media",
                        "sociale zekerheid",
                        "sociale huurwoning",
                        "sociale wetenschappen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к жизни общества и взаимодействию людей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "социальный",
                                "target_lang_sense_clarification": "связанный с обществом"
                            },
                            {
                                "target_lang_word": "общественный",
                                "target_lang_sense_clarification": "относящийся к обществу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e637997b-4d85-3368-8a69-b828a4880cfa",
                    "sense_definition": "Geneigd om in groepen te leven",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is een erg <w>sociaal</w> dier dat niet alleen kan zijn.",
                            "target_lang_translations": {
                                "ru": "Это очень <w>социальное</w> животное, которое не может быть одно."
                            }
                        },
                        {
                            "text": "Wolven zijn <w>sociale</w> wezens die samenwerken tijdens de jacht.",
                            "target_lang_translations": {
                                "ru": "Волки — <w>социальные</w> существа, которые сотрудничают во время охоты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sociaal dier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живущий группами или сообществами (о животных)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "социальный",
                                "target_lang_sense_clarification": "биологический термин"
                            },
                            {
                                "target_lang_word": "стайный",
                                "target_lang_sense_clarification": "живущий в стае"
                            }

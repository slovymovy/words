{
    "word_family": [
        "excess",
        "excessive",
        "excessively",
        "exceedingly"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2f34441a-70d8-3860-83ea-2d6e545218f3",
                    "sense_definition": "To go beyond a specific limit, rule, or restriction.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quantitative_comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drivers who <w>exceed</w> the speed limit will be fined heavily.",
                            "target_lang_translations": {
                                "ru": "Водители, которые <w>превышают</w> скорость, будут оштрафованы на крупную сумму."
                            }
                        },
                        {
                            "text": "The weight of your luggage must not <w>exceed</w> twenty kilograms.",
                            "target_lang_translations": {
                                "ru": "Вес вашего багажа не должен <w>превышать</w> двадцати килограммов."
                            }
                        }
                    ],
                    "synonyms": [
                        "overstep",
                        "surpass",
                        "pass",
                        "go beyond"
                    ],
                    "antonyms": [
                        "to fall short"
                    ],
                    "common_phrases": [
                        "exceed the limit",
                        "exceed the speed limit",
                        "exceed authority"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выходить за пределы установленной нормы, правила или ограничения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "превыша́ть",
                                "target_lang_sense_clarification": "лимит, скорость, полномочия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d819c52-6353-3236-aee1-04add6725442",
                    "sense_definition": "To be greater in number, quantity, or size than something else.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quantitative_comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The final cost of the construction project will likely <w>exceed</w> the initial budget.",
                            "target_lang_translations": {
                                "ru": "Окончательная стоимость строительного проекта, вероятно, <w>превысит</w> первоначальный бюджет."
                            }
                        },
                        {
                            "text": "Sales figures for this quarter <w>exceed</w> those of the previous year.",
                            "target_lang_translations": {
                                "ru": "Показатели продаж за этот квартал <w>превышают</w> показатели прошлого года."
                            }
                        }
                    ],
                    "synonyms": [
                        "surpass",
                        "top",
                        "outstrip",
                        "outnumber"
                    ],
                    "antonyms": [
                        "to fall below"
                    ],
                    "common_phrases": [
                        "exceed the cost",
                        "exceed the value",
                        "exceed the number"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть больше по количеству, размеру или степени, чем что-то другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "превыша́ть",
                                "target_lang_sense_clarification": "количество, сумму"
                            },
                            {
                                "target_lang_word": "превосходи́ть",
                                "target_lang_sense_clarification": "по численности, объему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b477cd9a-8256-341e-be3f-42ca2e59c0b5",
                    "sense_definition": "To be better or greater than a standard or expectation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "qualitative_comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The quality of her essay has <w>exceeded</w> my expectations.",
                            "target_lang_translations": {
                                "ru": "Качество её эссе <w>превзошло</w> мои ожидания."
                            }
                        },
                        {
                            "text": "The hotel service <w>exceeded</w> the standards of a five-star resort.",
                            "target_lang_translations": {
                                "ru": "Обслуживание в отеле <w>превзошло</w> стандарты пятизвёздочного курорта."
                            }
                        }
                    ],
                    "synonyms": [
                        "surpass",
                        "excel",
                        "outdo",
                        "outshine"
                    ],
                    "antonyms": [
                        "to fall short",
                        "to be inferior"
                    ],
                    "common_phrases": [
                        "exceed expectations",
                        "exceed requirements",
                        "exceed hopes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть лучше или выше определенного стандарта или ожидания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "превосходи́ть",
                                "target_lang_sense_clarification": "ожидания, качество"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "32861213-b58c-38ba-a34f-32cdaa40334a",
                    "sense_definition": "Kledingstukken of accessoires om het lichaam doen om ze te dragen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is erg koud buiten, dus je moet zeker een warme jas <w>aandoen</w>.",
                            "target_lang_translations": {
                                "ru": "На улице очень холодно, поэтому тебе обязательно нужно <w>надеть</w> теплую куртку.",
                                "en": "It is very cold outside, so you should definitely <w>put on</w> a warm coat."
                            }
                        },
                        {
                            "text": "Ik ga even snel andere schoenen <w>aandoen</w> voordat we naar het feest vertrekken.",
                            "target_lang_translations": {
                                "ru": "Я быстро <w>надену</w> другую обувь, прежде чем мы отправимся на вечеринку.",
                                "en": "I'm going to quickly <w>put on</w> other shoes before we leave for the party."
                            }
                        }
                    ],
                    "synonyms": [
                        "aantrekken"
                    ],
                    "antonyms": [
                        "uitdoen",
                        "uittrekken"
                    ],
                    "common_phrases": [
                        "een jas aandoen",
                        "schoenen aandoen",
                        "iets warms aandoen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надевать одежду или аксессуары на тело для ношения.",
                        "en": "To put clothes or accessories on the body to wear them."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надевать",
                                "target_lang_sense_clarification": "одежда"
                            },
                            {
                                "target_lang_word": "обувать",
                                "target_lang_sense_clarification": "обувь"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "put on",
                                "target_lang_sense_clarification": "clothing, shoes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc093300-53e6-3c11-af95-36bf277360ea",
                    "sense_definition": "Een elektrisch apparaat of lichtbron in werking stellen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je alsjeblieft het licht <w>aandoen</w>, want het wordt hier al donker.",
                            "target_lang_translations": {
                                "ru": "Можешь, пожалуйста, <w>включить</w> свет, а то здесь уже темнеет.",
                                "en": "Could you please <w>turn on</w> the light, because it's getting dark here."
                            }
                        },
                        {
                            "text": "Zullen we de verwarming <w>aandoen</w> of pakken we liever een warme deken?",
                            "target_lang_translations": {
                                "ru": "Мы <w>включим</w> отопление или лучше возьмем теплое одеяло?",
                                "en": "Shall we <w>turn on</w> the heating or would we rather grab a warm blanket?"
                            }
                        }
                    ],
                    "synonyms": [
                        "aanzetten",
                        "aanzetten"
                    ],
                    "antonyms": [
                        "uitdoen",
                        "uitzetten"
                    ],
                    "common_phrases": [

                {
                    "sense_id": "706f04b3-cfa9-342a-8495-ac9c8eca2f60",
                    "sense_definition": "Geweld gebruiken tegen iemand of iets; het initiatief nemen in een gevecht.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de vroege ochtend besloot het leger de vijandelijke stad <w>aan te vallen</w>.",
                            "target_lang_translations": {
                                "en": "In the early morning, the army decided to <w>attack</w> the enemy city.",
                                "ru": "Рано утром армия решила <w>атаковать</w> вражеский город."
                            }
                        },
                        {
                            "text": "De wilde hond wilde de fietser <w>aanvallen</w>, maar werd tegengehouden.",
                            "target_lang_translations": {
                                "en": "The wild dog wanted to <w>attack</w> the cyclist but was held back.",
                                "ru": "Дикая собака хотела <w>напасть</w> на велосипедиста, но её удержали."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestormen",
                        "belagen",
                        "bestoken"
                    ],
                    "antonyms": [
                        "verdedigen",
                        "beschermen"
                    ],
                    "common_phrases": [
                        "de vijand aanvallen",
                        "van achteren aanvallen",
                        "onverwachts aanvallen"
                    ],
                    "target_lang_definitions": {
                        "en": "To use violence against someone or something; to take the initiative in a fight.",
                        "ru": "Применять насилие против кого-либо или чего-либо; проявлять инициативу в бою."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attack",
                                "target_lang_sense_clarification": "to use physical force"
                            },
                            {
                                "target_lang_word": "assault",
                                "target_lang_sense_clarification": "to attack violently"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нападать",
                                "target_lang_sense_clarification": "применять силу"
                            },
                            {
                                "target_lang_word": "атаковать",
                                "target_lang_sense_clarification": "совершать атаку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50faf93e-90a3-37ee-862b-61c89ff4824d",
                    "sense_definition": "Iemand fel bekritiseren of beschuldigen met woorden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verbal_attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oppositie bleef de minister <w>aanvallen</w> op zijn nieuwe beleid.",
                            "target_lang_translations": {
                                "en": "The opposition kept <w>attacking</w> the minister on his new policy.",
                                "ru": "Оппозиция продолжала <w>нападать</w> на министра из-за его новой политики."
                            }
                        },
                        {
                            "text": "Je hoeft me niet meteen persoonlijk <w>aan te vallen</w> als ik een fout maak.",
                            "target_lang_translations": {
                                "en": "You don't have to <w>attack</w> me personally right away if I make a mistake.",
                                "ru": "Не обязательно сразу лично <w>нападать</w> на меня, если я совершу ошибку."
                            }
                        }
                    ],
                    "synonyms": [
                        "bekritiseren"
                    ],
                    "antonyms": [
                        "prijzen",
                        "verdedigen"
                    ],
                    "common_phrases": [

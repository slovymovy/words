            "senses": [
                {
                    "sense_id": "5aa95e34-0f3c-3be3-889b-56df83179c48",
                    "sense_definition": "An activity that one enjoys doing in one's spare time.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I like to collect stamps from different countries as a <w>hobby</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нравится собирать марки из разных стран в качестве <w>хобби</w>."
                            }
                        },
                        {
                            "text": "She decided to take up a new <w>hobby</w> like painting or gardening.",
                            "target_lang_translations": {
                                "ru": "Она решила найти себе новое <w>увлечение</w>, например, живопись или садоводство."
                            }
                        }
                    ],
                    "synonyms": [
                        "pastime",
                        "avocation",
                        "pursuit",
                        "amusement",
                        "diversion"
                    ],
                    "antonyms": [
                        "work"
                    ],
                    "common_phrases": [
                        "take up a hobby",
                        "favorite hobby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность или занятие, которым человек занимается в свободное время ради удовольствия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хо́бби",
                                "target_lang_sense_clarification": "любимое занятие"
                            },
                            {
                                "target_lang_word": "увлече́ние",
                                "target_lang_sense_clarification": "занятие для души"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "790a830a-0282-3a34-a5fd-266d7ca52230",
                    "sense_definition": "A favorite topic or subject that one frequently talks about.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "topic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is talking about politics again, which is his pet <w>hobby</w>.",
                            "target_lang_translations": {
                                "ru": "Он снова говорит о политике, это его любимый <w>конёк</w>."
                            }
                        },
                        {
                            "text": "Don't get him started on his favorite <w>hobby</w>, or he will talk for hours.",
                            "target_lang_translations": {
                                "ru": "Не заводи с ним речь о его любимом <w>коньке</w>, иначе он будет говорить часами."
                            }
                        }
                    ],
                    "synonyms": [

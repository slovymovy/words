                {
                    "sense_id": "bc409c55-925d-3b01-9cad-159548c8b9e2",
                    "sense_definition": "Okres życia pomiędzy dzieciństwem a dorosłością.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia bardzo lubiła opowiadać historie z czasów swojej <w>młodości</w>.",
                            "target_lang_translations": {
                                "ru": "Бабушка очень любила рассказывать истории времен своей <w>молодости</w>."
                            }
                        },
                        {
                            "text": "Moja <w>młodość</w> przypadła na trudne czasy, ale wspominam ją z uśmiechem.",
                            "target_lang_translations": {
                                "ru": "Моя <w>молодость</w> пришлась на трудные времена, но я вспоминаю её с улыбкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "adolescencja"
                    ],
                    "antonyms": [
                        "starość"
                    ],
                    "common_phrases": [
                        "lata młodości",
                        "druga młodość",
                        "kwiat młodości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период жизни между детством и зрелостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодость",
                                "target_lang_sense_clarification": "период жизни"
                            },
                            {
                                "target_lang_word": "юность",
                                "target_lang_sense_clarification": "ранняя молодость, отрочество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e49ba2ab-b5ae-3ffb-be21-8e2e860e82db",
                    "sense_definition": "Cecha tego, co młode; cecha tego, kto jest młody.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo podeszłego wieku, w jego oczach wciąż widać było <w>młodość</w> i energię.",
                            "target_lang_translations": {
                                "ru": "Несмотря на преклонный возраст, в его глазах всё ещё были видны <w>молодость</w> и энергия."
                            }
                        },
                        {
                            "text": "Wielu ludzi marzy o tym, aby zachować <w>młodość</w> na zawsze.",
                            "target_lang_translations": {
                                "ru": "Многие люди мечтают о том, чтобы сохранить <w>молодость</w> навсегда."
                            }
                        }
                    ],
                    "antonyms": [
                        "starość"
                    ],
                    "common_phrases": [
                        "wieczna młodość",
                        "eliksir młodości",
                        "zachować młodość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство того, кто молод; внешние признаки молодого возраста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодость",
                                "target_lang_sense_clarification": "свойство, качество"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "bevelen",
        "bevelhebber",
        "bevelvoering"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0df24953-0d82-3446-8225-c79a6d2e1842",
                    "sense_definition": "Een dwingende opdracht die zonder tegenspraak moet worden uitgevoerd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaat voerde het <w>bevel</w> van zijn meerdere onmiddellijk uit.",
                            "target_lang_translations": {
                                "en": "The soldier immediately carried out the <w>order</w> of his superior.",
                                "ru": "Солдат немедленно выполнил <w>приказ</w> своего начальника."
                            }
                        },
                        {
                            "text": "Op <w>bevel</w> van de politie moest iedereen het gebouw verlaten.",
                            "target_lang_translations": {
                                "en": "By <w>order</w> of the police, everyone had to leave the building.",
                                "ru": "По <w>приказу</w> полиции все должны были покинуть здание."
                            }
                        }
                    ],
                    "synonyms": [
                        "order",
                        "commando",
                        "gebod",
                        "instructie"
                    ],
                    "antonyms": [
                        "verzoek"
                    ],
                    "common_phrases": [
                        "een bevel geven",
                        "een bevel opvolgen",
                        "op bevel van"
                    ],
                    "target_lang_definitions": {
                        "en": "A mandatory instruction that must be executed without contradiction.",
                        "ru": "Обязательное для исполнения распоряжение, которое не подлежит обсуждению."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "order",
                                "target_lang_sense_clarification": "an authoritative instruction"
                            },
                            {
                                "target_lang_word": "command",
                                "target_lang_sense_clarification": "a specific directive"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приказ",
                                "target_lang_sense_clarification": "официальное или военное указание"
                            },
                            {
                                "target_lang_word": "распоряжение",
                                "target_lang_sense_clarification": "указание к действию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5179c89-1e2e-3ce2-ad0c-5f2b9607fe8a",
                    "sense_definition": "De macht en bevoegdheid om leiding te geven aan een groep, vaak in militaire zin.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De generaal heeft het <w>bevel</w> over alle troepen in deze regio.",
                            "target_lang_translations": {
                                "en": "The general has <w>command</w> over all troops in this region.",
                                "ru": "Генерал осуществляет <w>командование</w> всеми войсками в этом регионе."
                            }
                        },
                        {
                            "text": "Tijdens de oefening stond de eenheid onder <w>bevel</w> van een ervaren officier.",
                            "target_lang_translations": {
                                "en": "During the exercise, the unit was under the <w>command</w> of an experienced officer.",
                                "ru": "Во время учений подразделение находилось под <w>командованием</w> опытного офицера."
                            }
                        }
                    ],
                    "synonyms": [
                        "commando"
                    ],
                    "common_phrases": [
                        "het bevel voeren",
                        "onder bevel staan",
                        "opperbevel"
                    ],
                    "target_lang_definitions": {
                        "en": "The power and authority to lead a group, often in a military context.",
                        "ru": "Власть и полномочия руководить группой, часто в военном смысле."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "command",
                                "target_lang_sense_clarification": "authority or control over a group"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "командование",
                                "target_lang_sense_clarification": "руководство войсками или группой"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "f605b7da-8a8f-3bff-b388-5b5bf4aeeb79",
                    "sense_definition": "A greeting said when meeting someone or acknowledging someone’s arrival or presence.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hello</w> there, it is very nice to meet you.",
                            "target_lang_translations": {
                                "ru": "<w>Привет</w>, очень приятно познакомиться."
                            }
                        },
                        {
                            "text": "Please say <w>hello</w> to your parents for me.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, передавай <w>привет</w> родителям от меня."
                            }
                        }
                    ],
                    "synonyms": [
                        "hi",
                        "hey",
                        "greetings",
                        "good morning",
                        "good day"
                    ],
                    "antonyms": [
                        "goodbye",
                        "bye"
                    ],
                    "common_phrases": [
                        "hello there",
                        "say hello"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приветствие, которое произносят при встрече с кем-либо или для того, чтобы отметить чье-то прибытие или присутствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здра́вствуйте",
                                "target_lang_sense_clarification": "формальное или нейтральное"
                            },
                            {
                                "target_lang_word": "приве́т",
                                "target_lang_sense_clarification": "неформальное"
                            },
                            {
                                "target_lang_word": "здра́вствуй",
                                "target_lang_sense_clarification": "нейтральное, к одному человеку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "731eccb9-9ed3-3b3b-b795-652352d1cb9e",
                    "sense_definition": "A greeting used when answering the telephone.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hello</w>? I can hear you now.",
                            "target_lang_translations": {
                                "ru": "<w>Алло</w>? Теперь я вас слышу."
                            }
                        },
                        {
                            "text": "<w>Hello</w>, this is the front desk speaking.",
                            "target_lang_translations": {
                                "ru": "<w>Здравствуйте</w>, это администратор."
                            }
                        }
                    ],
                    "synonyms": [
                        "hi"
                    ],
                    "antonyms": [
                        "bye",
                        "goodbye"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приветствие, используемое при ответе на телефонный звонок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "алло́",
                                "target_lang_sense_clarification": "стандартное телефонное приветствие"
                            },
                            {
                                "target_lang_word": "слу́шаю",
                                "target_lang_sense_clarification": "вежливый ответ на звонок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6c740d4-4bb0-3add-9a18-62132f558481",
                    "sense_definition": "A call for response if it is not clear if anyone is present or listening.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "checking_presence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hello</w>? Is anyone inside the house?",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>? Есть кто-нибудь в доме?"
                            }
                        },
                        {
                            "text": "I shouted <w>hello</w> into the dark cave but heard no answer.",
                            "target_lang_translations": {
                                "ru": "Я крикнул «<w>ау</w>» в темную пещеру, но не услышал ответа."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, требующий ответа, если неясно, присутствует ли кто-нибудь или слушает ли кто-нибудь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ау́",
                                "target_lang_sense_clarification": "оклик в лесу или пустом помещении"
                            },
                            {
                                "target_lang_word": "е́сть кто́-нибудь?",
                                "target_lang_sense_clarification": "фраза для проверки присутствия"
                            },
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "разговорный оклик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdf1741a-c792-3a37-b9d9-e7cdc69e2349",
                    "sense_definition": "Used sarcastically to imply that the person addressed has done something foolish or missed something obvious.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sarcastic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You cannot start the car without the key, <w>hello</w>?",
                            "target_lang_translations": {
                                "ru": "Нельзя завести машину без ключа, <w>алло</w>?"
                            }
                        },
                        {
                            "text": "We have been waiting for you for an hour, <w>hello</w>?",
                            "target_lang_translations": {
                                "ru": "Мы ждем тебя уже целый час, <w>ау</w>?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "sarcastic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется саркастически, чтобы намекнуть, что собеседник сделал глупость или не замечает очевидного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "алло́",
                                "target_lang_sense_clarification": "сленг: призыв очнуться/подумать"
                            },
                            {
                                "target_lang_word": "ау́",
                                "target_lang_sense_clarification": "ироничный призыв к вниманию"
                            },
                            {
                                "target_lang_word": "здра́сте",
                                "target_lang_sense_clarification": "ироничное удивление глупости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56a347cd-fd1b-37fb-92c0-0da3a5e2e646",
                    "sense_definition": "An expression of puzzlement or discovery.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surprise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hello</w>! What is going on here?",
                            "target_lang_translations": {
                                "ru": "<w>Здрасьте</w>! Что здесь происходит?"
                            }
                        },
                        {
                            "text": "I opened the box and, <w>hello</w>, the missing ring was right there.",
                            "target_lang_translations": {
                                "ru": "Я открыл коробку и, <w>здрасьте</w>, пропавшее кольцо было прямо там."
                            }
                        }
                    ],
                    "synonyms": [
                        "wow"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "expression of surprise"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение недоумения или неожиданного открытия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здра́сте",
                                "target_lang_sense_clarification": "выражение удивления"
                            },
                            {
                                "target_lang_word": "вот это да",
                                "target_lang_sense_clarification": "восклицание удивления"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0d0985fa-fb66-35aa-b70a-936e29c303b6",
                    "sense_definition": "An utterance of \"hello\" or a similar greeting.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "greeting_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She gave me a big <w>hello</w> when I arrived at the party.",
                            "target_lang_translations": {
                                "ru": "Она передала мне большой <w>привет</w>, когда я пришел на вечеринку."
                            }
                        },
                        {
                            "text": "We exchanged quick <w>hellos</w> before rushing to our meetings.",
                            "target_lang_translations": {
                                "ru": "Мы обменялись быстрыми <w>приветствиями</w>, прежде чем побежать на встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "greeting"
                    ],
                    "common_phrases": [
                        "big hello",
                        "quick hello"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово \"привет\" или подобное приветствие как существительное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приве́т",
                                "target_lang_sense_clarification": "существительное"
                            },
                            {
                                "target_lang_word": "приве́тствие",
                                "target_lang_sense_clarification": "формальное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "31ef55b0-6f87-3a8e-b645-5b55b7da4580",
                    "sense_definition": "To greet someone by saying \"hello\".",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "greeting_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>helloed</w> to my daughter from across the street.",
                            "target_lang_translations": {
                                "ru": "Он <w>поздоровался</w> с моей дочерью с другой стороны улицы."
                            }
                        },
                        {
                            "text": "I spent the whole morning <w>helloing</w> new guests at the hotel.",
                            "target_lang_translations": {
                                "ru": "Я провел все утро, <w>приветствуя</w> новых гостей в отеле."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "rare usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Здороваться с кем-либо, говоря \"привет\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здоро́ваться",
                                "target_lang_sense_clarification": "глагол"
                            },
                            {
                                "target_lang_word": "приве́тствовать",
                                "target_lang_sense_clarification": "глагол"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
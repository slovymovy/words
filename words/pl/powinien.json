{
    "word_family": [
        "powinność",
        "winien",
        "wina",
        "obwiniać",
        "uniewinniać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c23363bd-5797-3c6b-b1e6-5a15b7039340",
                    "sense_definition": "Wyraża obowiązek, przymus moralny lub konieczność wykonania jakiejś czynności wynikającą z zasad, prawa lub norm społecznych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy kierowca <w>powinien</w> przestrzegać przepisów drogowych dla bezpieczeństwa innych.",
                            "target_lang_translations": {
                                "ru": "Каждый водитель <w>должен</w> соблюдать правила дорожного движения ради безопасности других."
                            }
                        },
                        {
                            "text": "Nie <w>powinieneś</w> rozmawiać w bibliotece, ponieważ przeszkadzasz innym czytelnikom.",
                            "target_lang_translations": {
                                "ru": "Тебе не <w>следует</w> разговаривать в библиотеке, так как ты мешаешь другим читателям."
                            }
                        }
                    ],
                    "synonyms": [
                        "musieć",
                        "należy"
                    ],
                    "antonyms": [
                        "móc"
                    ],
                    "common_phrases": [
                        "każdy powinien",
                        "nie powinienem",
                        "powinienem był"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает обязанность, моральный долг или необходимость выполнения действия, вытекающую из правил или социальных норм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "должен",
                                "target_lang_sense_clarification": "обязанность"
                            },
                            {
                                "target_lang_word": "следует",
                                "target_lang_sense_clarification": "предписание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "146eef2e-7a25-3a1e-9888-8d9895390567",
                    "sense_definition": "Wyraża radę, sugestię lub opinię, że zrobienie czegoś jest właściwe, korzystne lub pożądane w danej sytuacji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "advice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli źle się czujesz, <w>powinieneś</w> pójść do lekarza i odpocząć.",
                            "target_lang_translations": {
                                "ru": "Если ты плохо себя чувствуешь, тебе <w>следует</w> пойти к врачу и отдохнуть."
                            }
                        },
                        {
                            "text": "<w>Powinnaś</w> spróbować tego ciasta, bo jest naprawdę pyszne.",
                            "target_lang_translations": {
                                "ru": "Тебе <w>стоит</w> попробовать этот пирог, потому что он действительно вкусный."
                            }
                        }
                    ],
                    "synonyms": [
                        "warto"
                    ],
                    "common_phrases": [
                        "powinieneś spróbować",
                        "powinien wiedzieć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает совет, предложение или мнение о том, что действие является правильным, полезным или желательным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следует",
                                "target_lang_sense_clarification": "совет"
                            },
                            {
                                "target_lang_word": "стоит",
                                "target_lang_sense_clarification": "рекомендация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb1f6c99-b0c6-3249-b706-0f4328c435f5",
                    "sense_definition": "Wyraża przypuszczenie lub przewidywanie, że coś jest bardzo prawdopodobne lub oczekiwane przez mówiącego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "probability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta zupa <w>powinna</w> być już gotowa, gotuje się od godziny.",
                            "target_lang_translations": {
                                "ru": "Этот суп уже <w>должен</w> быть готов, он варится целый час."
                            }
                        },
                        {
                            "text": "Mój pociąg <w>powinien</w> przyjechać na stację za pięć minut.",
                            "target_lang_translations": {
                                "ru": "Мой поезд <w>должен</w> прибыть на станцию через пять минут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "to powinno wystarczyć",
                        "nie powinno być"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает предположение или ожидание, что событие весьма вероятно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "должен",
                                "target_lang_sense_clarification": "вероятность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
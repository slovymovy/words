    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cf3b8a14-0760-3e3d-ae72-13c2b2e4b054",
                    "sense_definition": "Wychodzić z jakiegoś miejsca, wyjeżdżać skądś lub odchodzić od kogoś (często na stałe lub na dłuższy czas).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "leave_abandon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy natychmiast <opuszczać> ten budynek z powodu alarmu pożarowego.",
                            "target_lang_translations": {
                                "ru": "Мы должны немедленно <покинуть> это здание из-за пожарной тревоги."
                            }
                        },
                        {
                            "text": "Bardzo trudno było mu <opuszczać> rodzinny dom i wyjeżdżać na studia.",
                            "target_lang_translations": {
                                "ru": "Ему было очень трудно <покидать> родной дом и уезжать на учебу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zostawiać",
                        "porzucać",
                        "wyjeżdżać"
                    ],
                    "antonyms": [
                        "podnosić",
                        "unosić"
                    ],
                    "common_phrases": [
                        "opuszczać dom",
                        "opuszczać kraj",
                        "opuszczać pokój",
                        "opuszczać rodzinę",
                        "opuszczać strefę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уходить из какого-либо места, уезжать откуда-то или уходить от кого-то (часто навсегда или на длительное время)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покидать",
                                "target_lang_sense_clarification": "уходить, уезжать из места или от человека"
                            },
                            {
                                "target_lang_word": "оставлять",
                                "target_lang_sense_clarification": "уходить от кого-то, бросать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75986ffc-f4c7-3e8d-b08e-ba02f617f91e",
                    "sense_definition": "Przesuwać coś w dół; sprawiać, że coś znajduje się na niższym poziomie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "move_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy słońce świeci zbyt mocno, warto <opuszczać> rolety w oknach.",
                            "target_lang_translations": {
                                "ru": "Когда солнце светит слишком ярко, стоит <опускать> жалюзи на окнах."
                            }
                        },
                        {
                            "text": "Na znak żałoby nakazano <opuszczać> flagi do połowy masztu.",
                            "target_lang_translations": {
                                "ru": "В знак траура было приказано <приспустить> флаги до середины мачты."
                            }
                        }
                    ],
                    "synonyms": [
                        "zniżać",
                        "spuszczać"
                    ],
                    "antonyms": [
                        "podnosić",
                        "unosić"
                    ],
                    "common_phrases": [
                        "opuszczać rolety",
                        "opuszczać głowę",
                        "opuszczać wzrok",
                        "opuszczać ręce",
                        "opuszczać szybę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать что-то вниз; делать так, чтобы что-то находилось на более низком уровне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опускать",
                                "target_lang_sense_clarification": "перемещать вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "397fa40f-90ac-364a-a675-ab1f804fd5b9",
                    "sense_definition": "Nie brać w czymś udziału; rezygnować z obecności na jakimś wydarzeniu lub z wykonania jakiejś czynności.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "skip_miss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinieneś tak często <opuszczać> lekcji matematyki, jeśli chcesz zdać egzamin.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует так часто <пропускать> уроки математики, если хочешь сдать экзамен."
                            }
                        },
                        {
                            "text": "W tej grze planszowej musisz <opuszczać> jedną kolejkę za karę.",
                            "target_lang_translations": {
                                "ru": "В этой настольной игре ты должен <пропускать> один ход в качестве наказания."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomijać",
                        "omijać"
                    ],
                    "common_phrases": [
                        "opuszczać lekcje",
                        "opuszczać zajęcia",
                        "opuszczać kolejkę",
                        "opuszczać treningi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не принимать в чем-то участия; отказываться от присутствия на каком-то событии или пропускать свою очередь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропускать",
                                "target_lang_sense_clarification": "не посещать занятия, пропускать ход"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c8ff9fb-9010-389f-84bf-66af108482f5",
                    "sense_definition": "Zaniedbywać swoje obowiązki, naukę lub wygląd; osiągać gorsze wyniki niż wcześniej (używane z zaimkiem „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "neglect_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zaczął <opuszczać> się w nauce, odkąd spędza tyle czasu przed komputerem.",
                            "target_lang_translations": {
                                "ru": "Он начал <скатываться> в учебе, с тех пор как проводит столько времени за компьютером."
                            }
                        },
                        {
                            "text": "Nie wolno ci <opuszczać> się w treningach, jeśli chcesz wygrać te zawody.",
                            "target_lang_translations": {
                                "ru": "Тебе нельзя <расслабляться> на тренировках, если хочешь выиграть эти соревнования."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaniedbywać się",
                        "rozleniwiać się"
                    ],
                    "antonyms": [
                        "podnosić"
                    ],
                    "common_phrases": [
                        "opuszczać się w nauce",
                        "opuszczać się w pracy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wymaga użycia zaimka zwrotnego 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пренебрегать обязанностями, учебой или своим внешним видом; добиваться худших результатов, чем раньше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запускать",
                                "target_lang_sense_clarification": "пренебрегать делами или учебой"
                            },
                            {
                                "target_lang_word": "опускаться",
                                "target_lang_sense_clarification": "переставать следить за собой, деградировать"
                            },
                            {
                                "target_lang_word": "расслабляться",
                                "target_lang_sense_clarification": "снижать требования к себе, лениться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
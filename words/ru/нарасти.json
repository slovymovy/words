    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "839d9191-9ce3-3857-99e4-c721967ea699",
                    "sense_definition": "Увеличиться в размерах, объёме, силе или интенсивности.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "growth_increase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Шум в зале успел <w>нарасти</w> до пугающего уровня.",
                            "target_lang_translations": {
                                "en": "The noise in the hall managed to <w>grow</w> to a frightening level."
                            }
                        },
                        {
                            "text": "К концу дня напряжение может снова <w>нарасти</w>.",
                            "target_lang_translations": {
                                "en": "By the end of the day, the tension might <w>mount</w> again."
                            }
                        }
                    ],
                    "synonyms": [
                        "увеличиться",
                        "усилиться",
                        "возрасти"
                    ],
                    "antonyms": [
                        "уменьшиться",
                        "снизиться"
                    ],
                    "common_phrases": [
                        "быстро нарасти",
                        "заметно нарасти",
                        "напряжение наросло"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase in size, volume, strength, or intensity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grow",
                                "target_lang_sense_clarification": "general increase in size or intensity"
                            },
                            {
                                "target_lang_word": "mount",
                                "target_lang_sense_clarification": "to increase gradually, especially tension or pressure"
                            },
                            {
                                "target_lang_word": "increase",
                                "target_lang_sense_clarification": "to become greater in amount or degree"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcc81f3b-6868-30d8-9c40-77ae59490dfb",
                    "sense_definition": "Образоваться, появиться на поверхности чего-либо (о слое льда, грязи, мха и т. п.).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "growth_increase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На крыльях самолёта <w>нарос</w> толстый слой льда.",
                            "target_lang_translations": {
                                "en": "A thick layer of ice <w>built up</w> on the airplane's wings."
                            }
                        },
                        {
                            "text": "За много лет на камне <w>нарос</w> густой мох.",
                            "target_lang_translations": {
                                "en": "Over many years, thick moss <w>grew</w> on the stone."
                            }
                        }
                    ],
                    "synonyms": [
                        "наслоиться",
                        "образоваться"
                    ],
                    "common_phrases": [
                        "лёд нарос",
                        "слой нарос",
                        "грязь наросла"
                    ],
                    "target_lang_definitions": {
                        "en": "To form or appear on a surface (referring to a layer of ice, dirt, moss, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "build up",
                                "target_lang_sense_clarification": "to accumulate on a surface over time"
                            },
                            {
                                "target_lang_word": "accumulate",
                                "target_lang_sense_clarification": "to gather or collect"
                            },
                            {
                                "target_lang_word": "grow",
                                "target_lang_sense_clarification": "biological growth on a surface (e.g., moss)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c56a0db8-2b9a-3bdd-8270-80998d0f2128",
                    "sense_definition": "Накопиться в значительном количестве (обычно о денежных суммах, долгах или процентах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "growth_increase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За время просрочки на счёте <w>наросли</w> большие пени.",
                            "target_lang_translations": {
                                "en": "Large penalties <w>accrued</w> on the account during the overdue period."
                            }
                        },
                        {
                            "text": "У компании <w>наросли</w> долги перед поставщиками.",
                            "target_lang_translations": {
                                "en": "The company's debts to suppliers <w>piled up</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "накопиться",
                        "набежать"
                    ],
                    "common_phrases": [
                        "проценты наросли",
                        "долги наросли",
                        "пени наросли"
                    ],
                    "target_lang_definitions": {
                        "en": "To accumulate in significant quantity (usually regarding monetary sums, debts, or interest)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accrue",
                                "target_lang_sense_clarification": "financial accumulation like interest or penalties"
                            },
                            {
                                "target_lang_word": "accumulate",
                                "target_lang_sense_clarification": "to gather together or acquire an increasing number"
                            },
                            {
                                "target_lang_word": "pile up",
                                "target_lang_sense_clarification": "to accumulate rapidly or in large amounts (often negative)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3d6359c-3d35-328a-b37b-f1ad2c4851ab",
                    "sense_definition": "Форма повелительного наклонения глагола <w>нарастить</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Нарасти</w> мне ресницы, чтобы они выглядели длиннее.",
                            "target_lang_translations": {
                                "en": "<w>Extend</w> my eyelashes so they look longer."
                            }
                        },
                        {
                            "text": "Пожалуйста, <w>нарасти</w> темп работы, иначе мы не успеем.",
                            "target_lang_translations": {
                                "en": "Please <w>increase</w> the work pace, otherwise we won't make it in time."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative mood"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative form of the verb 'нарастить' (to make something grow, extend, or increase)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "extend",
                                "target_lang_sense_clarification": "to make longer (e.g., hair, lashes)"
                            },
                            {
                                "target_lang_word": "increase",
                                "target_lang_sense_clarification": "to make greater (e.g., pace, volume)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "6b2257f7-5249-36c6-9eb5-31182c6f2ffc",
                    "sense_definition": "Given my knowledge about someone or something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Knowing> you, you have probably already finished the work.",
                            "target_lang_translations": {
                                "ru": "<Зная> тебя, ты, вероятно, уже закончил работу."
                            }
                        },
                        {
                            "text": "<Knowing> how much she likes chocolate, I bought her a box.",
                            "target_lang_translations": {
                                "ru": "<Зная>, как сильно она любит шоколад, я купил ей коробку."
                            }
                        }
                    ],
                    "common_phrases": [

                {
                    "sense_id": "23ae1677-4a15-3e98-beb1-3a423d302383",
                    "sense_definition": "Een persoon die anderen de weg wijst of rondleidt en daarbij uitleg geeft over bezienswaardigheden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "person_guide",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>gids</w> vertelde ons alles over de geschiedenis van het oude kasteel.",
                            "target_lang_translations": {
                                "en": "The <w>guide</w> told us everything about the history of the old castle.",
                                "ru": "<w>Гид</w> рассказал нам всё об истории старого замка."
                            }
                        },
                        {
                            "text": "We hebben een lokale <w>gids</w> ingehuurd om ons de stad te laten zien.",
                            "target_lang_translations": {
                                "en": "We hired a local <w>guide</w> to show us the city.",
                                "ru": "Мы наняли местного <w>гида</w>, чтобы он показал нам город."
                            }
                        }
                    ],
                    "synonyms": [
                        "rondleider",
                        "begeleider"
                    ],
                    "common_phrases": [
                        "ervaren gids",
                        "lokale gids",
                        "met een gids"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who shows others the way or leads them around, giving explanations about sights.",
                        "ru": "Человек, который показывает дорогу или проводит экскурсии, давая пояснения о достопримечательностях."
                    },
                    "translations": {

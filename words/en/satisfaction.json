    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "615924bd-4126-3510-9556-341a30f22ea9",
                    "sense_definition": "The pleasant feeling obtained when a need or desire is fulfilled or when something is done well.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She looked at her finished painting with a smile of <w>satisfaction</w>.",
                            "target_lang_translations": {
                                "ru": "Она посмотрела на свою законченную картину с улыбкой <w>удовлетворения</w>."
                            }
                        },
                        {
                            "text": "He gets a lot of <w>satisfaction</w> from helping people in his community.",
                            "target_lang_translations": {
                                "ru": "Он получает огромное <w>удовлетворение</w> от помощи людям в своем сообществе."
                            }
                        }
                    ],
                    "synonyms": [
                        "pleasure",
                        "contentment",
                        "happiness",
                        "delight",
                        "gratification"
                    ],
                    "antonyms": [
                        "dissatisfaction",
                        "displeasure",
                        "unhappiness",
                        "frustration"
                    ],
                    "common_phrases": [
                        "with satisfaction",
                        "get satisfaction",
                        "deep satisfaction",
                        "sense of satisfaction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приятное чувство, возникающее, когда потребность или желание исполнены, или когда что-то сделано хорошо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удовлетворе́ние",
                                "target_lang_sense_clarification": "чувство"
                            },
                            {
                                "target_lang_word": "удово́льствие",
                                "target_lang_sense_clarification": "приятное ощущение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a2f63c7-7b5b-31a0-b13e-d104cc92fd6b",
                    "sense_definition": "The act of fulfilling a need, desire, or expectation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company prioritizes customer <w>satisfaction</w> to ensure loyalty.",
                            "target_lang_translations": {
                                "ru": "Компания ставит во главу угла <w>удовлетворённость</w> клиентов, чтобы обеспечить их лояльность."
                            }
                        },
                        {
                            "text": "Job <w>satisfaction</w> is often more important to employees than a high salary.",
                            "target_lang_translations": {
                                "ru": "<w>Удовлетворённость</w> работой часто важнее для сотрудников, чем высокая зарплата."
                            }
                        }
                    ],
                    "synonyms": [
                        "fulfillment"
                    ],
                    "common_phrases": [
                        "customer satisfaction",
                        "job satisfaction",
                        "satisfaction of needs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по исполнению потребности, желания или ожидания"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удовлетворе́ние",
                                "target_lang_sense_clarification": "процесс исполнения"
                            },
                            {
                                "target_lang_word": "удовлетворённость",
                                "target_lang_sense_clarification": "состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70bff076-83ba-3c86-81dd-8118a07497aa",
                    "sense_definition": "A specific thing, person, or event that provides gratification or pleasure.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a great <w>satisfaction</w> to know that the project was finally complete.",
                            "target_lang_translations": {
                                "ru": "Было огромным <w>удовольствием</w> знать, что проект наконец завершен."
                            }
                        },
                        {
                            "text": "Her children's success was a huge <w>satisfaction</w> to her.",
                            "target_lang_translations": {
                                "ru": "Успех её детей был для неё огромной <w>радостью</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "joy",
                        "comfort"
                    ],
                    "antonyms": [
                        "disappointment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная вещь, человек или событие, доставляющие радость или удовольствие"
                    },
                    "translations": {
                        "ru": [
                            {

{
    "word_family": [],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "20626ae9-7377-31b6-a3f4-5e65fd11d48b",
                    "sense_definition": "Dawna, długa i ozdobna suknia lub szata wierzchnia, noszona zazwyczaj przez kobiety.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Królowa włożyła na bal swą najpiękniejszą, jedwabną <w>robę</w>.",
                            "target_lang_translations": {
                                "ru": "Королева надела на бал свое самое красивое шелковое <w>платье</w>."
                            }
                        },
                        {
                            "text": "W skrzyni znaleziono starą, zakurzoną <w>robę</w> obszytą złotą nicią.",
                            "target_lang_translations": {
                                "ru": "В сундуке нашли старое, пыльное <w>платье</w>, обшитое золотой нитью."
                            }
                        }
                    ],
                    "synonyms": [
                        "odzienie",
                        "ubiór",
                        "odzież",
                        "ubranie"
                    ],
                    "common_phrases": [
                        "jedwabna roba",
                        "bogata roba",
                        "długa roba"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo używane w kontekście historycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинное длинное нарядное платье или верхняя одежда, которую обычно носили женщины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платье",
                                "target_lang_sense_clarification": "женская одежда"
                            },
                            {
                                "target_lang_word": "одеяние",
                                "target_lang_sense_clarification": "торжественная или старинная одежда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "109fe275-ce61-3de0-ac1d-2c7549af8227",
                    "sense_definition": "Regionalne lub potoczne określenie dorosłej kobiety, często używane w odniesieniu do żony.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "woman_dialect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiad narzekał, że jego <w>roba</w> znowu wydała pieniądze na głupstwa.",
                            "target_lang_translations": {
                                "ru": "Сосед жаловался, что его <w>баба</w> снова потратила деньги на глупости."
                            }
                        },
                        {
                            "text": "Jakaś nieznajoma <w>roba</w> pytała o drogę do dworca.",
                            "target_lang_translations": {
                                "ru": "Какая-то незнакомая <w>женщина</w> спрашивала дорогу на вокзал."
                            }
                        }
                    ],
                    "synonyms": [
                        "kobieta",
                        "żona"
                    ],
                    "common_phrases": [
                        "moja roba",
                        "stara roba"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "występuje np. w gwarze poznańskiej"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное или разговорное название взрослой женщины, часто используемое по отношению к жене."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баба",
                                "target_lang_sense_clarification": "просторечное название женщины или жены"
                            },
                            {
                                "target_lang_word": "женщина",
                                "target_lang_sense_clarification": "взрослый человек женского пола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f21ed2dc-71ad-316b-a912-6db266a1f4a7",
                    "sense_definition": "Kobieta pozbawiona wolności osobistej, będąca czyjąś własnością; niewolnica.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slave_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biedna <w>roba</w> musiała pracować w polu od świtu do zmierzchu.",
                            "target_lang_translations": {
                                "ru": "Бедная <w>рабыня</w> должна была работать в поле от рассвета до заката."
                            }
                        },
                        {
                            "text": "Władca podarował swojemu rycerzowi ziemię oraz jedną <w>robę</w> do pomocy.",
                            "target_lang_translations": {
                                "ru": "Правитель подарил своему рыцарю землю и одну <w>рабыню</w> в помощь."
                            }
                        }
                    ],
                    "synonyms": [
                        "niewolnica"
                    ],
                    "common_phrases": [
                        "wierna roba",
                        "praca roby"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "termin historyczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, лишенная личной свободы и являющаяся чьей-либо собственностью; рабыня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рабыня",
                                "target_lang_sense_clarification": "женщина-невольница"
                            },
                            {
                                "target_lang_word": "невольница",
                                "target_lang_sense_clarification": "женщина, лишенная свободы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53861746-5857-3647-aa2b-9e70596b23f5",
                    "sense_definition": "Samica świni domowej lub dzika.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_dialect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W chlewiku leżała wielka <w>roba</w> karmiąca swoje młode.",
                            "target_lang_translations": {
                                "ru": "В хлеву лежала огромная <w>свиноматка</w>, кормящая своих детенышей."
                            }
                        },
                        {
                            "text": "Gospodarz dokupił nową <w>robę</w>, aby powiększyć hodowlę.",
                            "target_lang_translations": {
                                "ru": "Хозяин докупил новую <w>свиноматку</w>, чтобы расширить хозяйство."
                            }
                        }
                    ],
                    "synonyms": [
                        "locha",
                        "maciora"
                    ],
                    "common_phrases": [
                        "tłusta roba",
                        "dzika roba"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Самка домашней свиньи или дикого кабана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свиноматка",
                                "target_lang_sense_clarification": "самка свиньи, используемая для разведения"
                            },
                            {
                                "target_lang_word": "свинья",
                                "target_lang_sense_clarification": "домашнее животное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

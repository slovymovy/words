                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "secretly",
                                "target_lang_sense_clarification": "regarding inner thoughts or feelings"
                            },
                            {
                                "target_lang_word": "deep down",
                                "target_lang_sense_clarification": "inwardly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "втайне",
                                "target_lang_sense_clarification": "внутренне, не подавая вида"
                            },
                            {
                                "target_lang_word": "про себя",
                                "target_lang_sense_clarification": "в мыслях, не вслух"
                            },
                            {
                                "target_lang_word": "в глубине души",
                                "target_lang_sense_clarification": "глубоко внутри, скрывая от посторонних"
                            }
                        ]

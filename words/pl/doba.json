                        "era",
                        "okres",
                        "czasy"
                    ],
                    "common_phrases": [
                        "w dobie",
                        "w obecnej dobie",
                        "duch doby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длительный период в истории или жизни, характеризующийся определенными явлениями."
                    },
                    "translations": {
                        "ru": [
                            {

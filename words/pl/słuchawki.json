                {
                    "sense_id": "c5b6430b-57e9-37f0-9775-6faca281a81e",
                    "sense_definition": "Urządzenie elektroniczne zakładane na uszy lub wkładane do uszu, służące do indywidualnego słuchania dźwięku (np. muzyki, rozmów).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "audio_devices",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem nowe <w>słuchawki</w>, żeby słuchać muzyki w drodze do pracy.",
                            "target_lang_translations": {
                                "ru": "Я купил новые <w>наушники</w>, чтобы слушать музыку по дороге на работу."
                            }
                        },
                        {
                            "text": "Proszę założyć <w>słuchawki</w>, aby nie przeszkadzać innym domownikom.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, наденьте <w>наушники</w>, чтобы не мешать другим домашним."
                            }
                        }
                    ],
                    "common_phrases": [
                        "słuchawki bezprzewodowe",
                        "słuchawki douszne",
                        "słuchawki nauszne",
                        "założyć słuchawki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Электронное устройство, надеваемое на уши или вставляемое в уши для индивидуального прослушивания звука (например, музыки, разговоров)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наушники",
                                "target_lang_sense_clarification": "устройство для прослушивания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a55a0b4-4b24-3b37-ac17-4e2b268abde3",
                    "sense_definition": "Forma mianownika, biernika i wołacza liczby mnogiej od: <w>słuchawka</w> (np. telefoniczna, prysznicowa, lekarska).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie <w>słuchawki</w> telefoniczne w biurze zostały wymienione na nowe.",
                            "target_lang_translations": {
                                "ru": "Все телефонные <w>трубки</w> в офисе были заменены на новые."
                            }
                        },
                        {
                            "text": "W sklepie oglądaliśmy różne <w>słuchawki</w> prysznicowe do naszej łazienki.",
                            "target_lang_translations": {
                                "ru": "В магазине мы смотрели разные душевые <w>лейки</w> для нашей ванной."
                            }

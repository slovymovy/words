                    "frequency": "High",
                    "semantic_group_id": "adj_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two countries signed an <w>international</w> trade agreement.",
                            "target_lang_translations": {
                                "ru": "Две страны подписали <w>международное</w> торговое соглашение."
                            }
                        },
                        {
                            "text": "She works for a large <w>international</w> company with offices in Europe and Asia.",
                            "target_lang_translations": {
                                "ru": "Она работает в крупной <w>международной</w> компании с офисами в Европе и Азии."
                            }
                        }
                    ],
                    "synonyms": [
                        "multinational",
                        "intergovernmental"
                    ],
                    "antonyms": [
                        "domestic",
                        "national",
                        "local"
                    ],
                    "common_phrases": [
                        "international trade",
                        "international relations",
                        "international airport",
                        "international law"
                    ],
                    "target_lang_definitions": {

                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное ласковое обращение к другу или знакомому (региональное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дорогуша",

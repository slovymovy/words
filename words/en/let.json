{
    "word_family": [
        "sublet",
        "outlet",
        "inlet",
        "letdown",
        "let-out"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "950573bd-3abb-3a93-9469-8755e8df45d4",
                    "sense_definition": "To allow or permit someone to do something or something to happen; not to prevent.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>let</w> me help you with those heavy bags.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>позволь</w> мне помочь тебе с этими тяжелыми сумками."
                            }
                        },
                        {
                            "text": "Her parents would not <w>let</w> her go to the party late at night.",
                            "target_lang_translations": {
                                "ru": "Её родители не <w>пускали</w> её на вечеринку поздно вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "allow",
                        "permit"
                    ],
                    "antonyms": [
                        "forbid",
                        "prohibit",
                        "prevent"
                    ],
                    "common_phrases": [
                        "let go",
                        "let in",
                        "let out",
                        "let fall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрешать или позволять кому-либо делать что-либо или чему-либо происходить; не препятствовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позволять",
                                "target_lang_sense_clarification": "давать разрешение"
                            },
                            {
                                "target_lang_word": "пускать",
                                "target_lang_sense_clarification": "давать возможность идти или входить"
                            },
                            {
                                "target_lang_word": "разрешать",
                                "target_lang_sense_clarification": "давать согласие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91d018a5-b41d-336f-af6d-bbe16c4988a4",
                    "sense_definition": "Used to form the imperative for suggestions, commands, or requests involving the speaker and others.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Let</w>'s go to the cinema this weekend.",
                            "target_lang_translations": {
                                "ru": "<w>Давай</w> пойдем в кино в эти выходные."
                            }
                        },
                        {
                            "text": "<w>Let</w> us pray for peace and understanding.",
                            "target_lang_translations": {
                                "ru": "<w>Давайте</w> помолимся за мир и взаимопонимание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "let us",
                        "let's go",
                        "let's see",
                        "let me see"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для формирования повелительного наклонения при предложениях, командах или просьбах, вовлекающих говорящего и других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давай",
                                "target_lang_sense_clarification": "побуждение к совместному действию (неформально)"
                            },
                            {
                                "target_lang_word": "давайте",
                                "target_lang_sense_clarification": "побуждение к совместному действию (формально)"
                            },
                            {
                                "target_lang_word": "пусть",
                                "target_lang_sense_clarification": "пожелание или приказ третьему лицу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90a6703c-9f77-3ea2-ba0e-517377bbb685",
                    "sense_definition": "To cause someone to know or have something; to enable.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>let</w> me know as soon as you arrive at the station.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>дай</w> мне знать, как только приедешь на вокзал."
                            }
                        },
                        {
                            "text": "Could you <w>let</w> us have the report by tomorrow morning?",
                            "target_lang_translations": {
                                "ru": "Не могли бы вы <w>предоставить</w> нам отчет к завтрашнему утру?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "let know",
                        "let have"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать кому-либо знать о чем-либо или иметь что-либо; обеспечивать возможность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давать знать",
                                "target_lang_sense_clarification": "сообщать информацию"
                            },
                            {
                                "target_lang_word": "сообщать",
                                "target_lang_sense_clarification": "информировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e7f45c3-7482-396e-99d5-c53b91d4cf0c",
                    "sense_definition": "To leave someone or something undisturbed; to refrain from interfering.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you <w>let</w> the dough be for an hour, it will rise properly.",
                            "target_lang_translations": {
                                "ru": "Если <w>оставить</w> тесто в покое на час, оно поднимется как надо."
                            }
                        },
                        {
                            "text": "He just wants to be <w>let</w> alone to finish his work.",
                            "target_lang_translations": {
                                "ru": "Он просто хочет, чтобы его <w>оставили</w> в покое, чтобы закончить работу."
                            }
                        }
                    ],
                    "antonyms": [
                        "disturb",
                        "bother"
                    ],
                    "common_phrases": [
                        "let be",
                        "let alone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставлять кого-либо или что-либо в покое; воздерживаться от вмешательства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставлять",
                                "target_lang_sense_clarification": "не трогать, не мешать"
                            },
                            {
                                "target_lang_word": "не трогать",
                                "target_lang_sense_clarification": "не беспокоить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0dfe41c8-432d-3406-a2da-e3d1038b9251",
                    "sense_definition": "To allow the possession and use of a property (like a house or room) in exchange for rent.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "renting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to <w>let</w> their apartment to tourists while they traveled.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>сдать</w> свою квартиру туристам на время путешествия."
                            }
                        },
                        {
                            "text": "There are many rooms to <w>let</w> in this part of the city.",
                            "target_lang_translations": {
                                "ru": "В этой части города много комнат, которые <w>сдаются</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "allow",
                        "permit"
                    ],
                    "common_phrases": [
                        "to let"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрешать владение и использование недвижимости (например, дома или комнаты) в обмен на арендную плату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "предоставлять в пользование за плату"
                            },
                            {
                                "target_lang_word": "сдавать в аренду",
                                "target_lang_sense_clarification": "официальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08baf601-de9a-39a2-a2e1-27af96673742",
                    "sense_definition": "To hinder, obstruct, or prevent someone from doing something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hindrance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy storm did <w>let</w> the travelers from reaching their destination on time.",
                            "target_lang_translations": {
                                "ru": "Сильный шторм <w>помешал</w> путешественникам добраться до места назначения вовремя."
                            }
                        },
                        {
                            "text": "He vowed that nothing should <w>let</w> him from completing his quest.",
                            "target_lang_translations": {
                                "ru": "Он поклялся, что ничто не <w>остановит</w> его от завершения поисков."
                            }
                        }
                    ],
                    "synonyms": [
                        "allow",
                        "permit"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мешать, препятствовать или не давать кому-либо что-либо сделать (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "препятствовать",
                                "target_lang_sense_clarification": "создавать преграды"
                            },
                            {
                                "target_lang_word": "мешать",
                                "target_lang_sense_clarification": "быть помехой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b82d5056-16e6-3bfb-9ece-a4353952f70d",
                    "sense_definition": "A service in tennis or similar games that must be replayed, usually because the ball touched the net but landed in the correct area.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The umpire called a <w>let</w> after the ball clipped the top of the net.",
                            "target_lang_translations": {
                                "ru": "Судья объявил <w>переподачу</w> после того, как мяч задел верхний край сетки."
                            }
                        },
                        {
                            "text": "It was a clear <w>let</w>, so the player had to serve again.",
                            "target_lang_translations": {
                                "ru": "Это была явная <w>переподача</w>, поэтому игроку пришлось подавать снова."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подача в теннисе или подобных играх, которую необходимо переиграть, обычно потому, что мяч коснулся сетки, но приземлился в правильной зоне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переподача",
                                "target_lang_sense_clarification": "спортивный термин"
                            },
                            {
                                "target_lang_word": "касание сетки",
                                "target_lang_sense_clarification": "причина переподачи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6baf07e-5f1b-3377-bfa4-9a6b82b4f5f4",
                    "sense_definition": "The act of renting out a property, or the property itself that is rented.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "renting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We were lucky to find such a cheap <w>let</w> in the city center.",
                            "target_lang_translations": {
                                "ru": "Нам повезло найти такой дешевый вариант <w>аренды</w> в центре города."
                            }
                        },
                        {
                            "text": "The agent described it as a very good <w>let</w> due to its location.",
                            "target_lang_translations": {
                                "ru": "Агент описал это как очень хороший вариант для <w>сдачи</w> благодаря расположению."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт сдачи недвижимости в аренду или сама недвижимость, которая сдается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдача внаём",
                                "target_lang_sense_clarification": "процесс сдачи"
                            },
                            {
                                "target_lang_word": "аренда",
                                "target_lang_sense_clarification": "наем жилья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "954e573b-f71d-37aa-8b7d-0eca17bbeec1",
                    "sense_definition": "An obstacle, hindrance, or impediment.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hindrance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They were able to pass through the border without <w>let</w> or hindrance.",
                            "target_lang_translations": {
                                "ru": "Они смогли пересечь границу без <w>препятствий</w> и помех."
                            }
                        },
                        {
                            "text": "Despite every <w>let</w> placed in his path, he eventually succeeded.",
                            "target_lang_translations": {
                                "ru": "Несмотря на каждое <w>препятствие</w> на его пути, он в конце концов добился успеха."
                            }
                        }
                    ],
                    "common_phrases": [
                        "without let or hindrance"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly used in legal or formal phrases"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Препятствие, помеха или затруднение (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "препятствие",
                                "target_lang_sense_clarification": "преграда"
                            },
                            {
                                "target_lang_word": "помеха",
                                "target_lang_sense_clarification": "то, что мешает"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "48f8da3e-a545-397a-8bf4-6b13e8eeb81a",
                    "sense_definition": "De verantwoordelijkheid voor een fout of een vervelende gebeurtenis.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "blame_responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is niet mijn <w>schuld</w> dat we de trein hebben gemist.",
                            "target_lang_translations": {
                                "en": "It is not my <w>fault</w> that we missed the train.",
                                "ru": "Это не моя <w>вина</w>, что мы опоздали на поезд."
                            }
                        },
                        {
                            "text": "Hij gaf zijn broer de <w>schuld</w> van het ongeluk met de auto.",
                            "target_lang_translations": {
                                "en": "He put the <w>blame</w> on his brother for the car accident.",
                                "ru": "Он возложил <w>вину</w> за автомобильную аварию на своего брата."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de schuld geven",
                        "de schuld krijgen",
                        "eigen schuld"
                    ],
                    "target_lang_definitions": {
                        "en": "The responsibility for a mistake or an unpleasant event.",
                        "ru": "Ответственность за ошибку или неприятное событие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fault",
                                "target_lang_sense_clarification": "responsibility for a mistake"
                            },
                            {
                                "target_lang_word": "blame",
                                "target_lang_sense_clarification": "responsibility for a bad event"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вина",
                                "target_lang_sense_clarification": "ответственность за проступок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d67ee002-3d83-3460-b3b8-69577e723eff",
                    "sense_definition": "Een geldbedrag dat men verplicht is aan iemand te betalen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "financial_debt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij werkt hard om zijn <w>schuld</w> bij de bank af te lossen.",
                            "target_lang_translations": {
                                "en": "He works hard to pay off his <w>debt</w> to the bank.",
                                "ru": "Он усердно работает, чтобы погасить свой <w>долг</w> перед банком."
                            }
                        },
                        {
                            "text": "Door onverwachte kosten kwam het gezin diep in de <w>schulden</w>.",
                            "target_lang_translations": {
                                "en": "Due to unexpected costs, the family got deep into <w>debt</w>.",
                                "ru": "Из-за непредвиденных расходов семья глубоко влезла в <w>долги</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "vordering"
                    ],
                    "common_phrases": [
                        "in de schulden zitten",
                        "schuld aflossen",
                        "schulden maken"
                    ],
                    "target_lang_definitions": {
                        "en": "An amount of money that one is obliged to pay to someone.",
                        "ru": "Денежная сумма, которую необходимо выплатить кому-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "debt",
                                "target_lang_sense_clarification": "money owed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "долг",
                                "target_lang_sense_clarification": "денежное обязательство"
                            },
                            {
                                "target_lang_word": "задолженность",
                                "target_lang_sense_clarification": "финансовая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "229d0fa8-c935-37a7-9d62-9ef91d95159a",
                    "sense_definition": "Het feit dat men strafbaar is of het besef iets verkeerds te hebben gedaan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "guilt_culpability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte heeft zijn <w>schuld</w> voor de rechter bekend.",
                            "target_lang_translations": {
                                "en": "The suspect admitted his <w>guilt</w> before the judge.",
                                "ru": "Подозреваемый признал свою <w>вину</w> перед судьей."
                            }
                        },
                        {
                            "text": "Er was onvoldoende bewijs voor zijn <w>schuld</w> aan de misdaad.",
                            "target_lang_translations": {
                                "en": "There was insufficient evidence for his <w>guilt</w> in the crime.",
                                "ru": "Было недостаточно доказательств его <w>виновности</w> в преступлении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "schuld bekennen",
                        "schuld bewijzen"
                    ],
                    "target_lang_definitions": {
                        "en": "The fact of being punishable or the realization of having done something wrong.",
                        "ru": "Факт нарушения закона или осознание совершения чего-то неправильного."
                    },
                    "translations": {

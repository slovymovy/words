    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "76753604-3381-3dd2-97eb-790152f3fa64",
                    "sense_definition": "Pójście w jakieś miejsce w celu rozeznania się w sytuacji lub zdobycia informacji, często używane w kontekście codziennym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gathering_information",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść na <w>zwiady</w>, żeby sprawdzić, czy sklep jest już otwarty.",
                            "target_lang_translations": {
                                "ru": "Мне нужно сходить на <w>разведку</w>, чтобы проверить, открыт ли уже магазин."
                            }
                        },
                        {
                            "text": "Nasz kot wyszedł na nocne <w>zwiady</w> po okolicy.",
                            "target_lang_translations": {
                                "ru": "Наш кот вышел на ночную <w>разведку</w> по окрестностям."
                            }
                        }
                    ],
                    "synonyms": [
                        "rekonesans"
                    ],
                    "common_phrases": [
                        "iść na zwiady",
                        "wysłać na zwiady",
                        "nocne zwiady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поход в какое-либо место с целью выяснения ситуации или получения информации, часто используется в повседневном контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разведка",
                                "target_lang_sense_clarification": "выяснение обстановки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f37d279d-f3c4-34f3-aa47-0fbd6b363722",
                    "sense_definition": "Działania wojskowe mające na celu zdobycie informacji o liczebności i położeniu wojsk nieprzyjaciela oraz o terenie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "gathering_information",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dowódca wysłał patrol na <w>zwiad</w> przed planowanym atakiem.",
                            "target_lang_translations": {
                                "ru": "Командир отправил патруль в <w>разведку</w> перед планируемой атакой."
                            }
                        },
                        {
                            "text": "Dzięki skutecznemu <w>zwiadowi</w> uniknęliśmy zasadzki wroga.",
                            "target_lang_translations": {
                                "ru": "Благодаря эффективной <w>разведке</w> мы избежали вражеской засады."
                            }
                        }
                    ],
                    "synonyms": [
                        "rekonesans",
                        "rozpoznanie wojskowe",
                        "rozpoznanie terenu"
                    ],
                    "common_phrases": [
                        "zwiad lotniczy",
                        "zwiad taktyczny",
                        "zwiad terenu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Военные действия, направленные на получение информации о численности и расположении войск противника, а также о местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разведка",
                                "target_lang_sense_clarification": "военная операция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "444434ee-810e-3575-a9a4-b0089c3c3f76",
                    "sense_definition": "Pododdział żołnierzy wyznaczony do prowadzenia działań rozpoznawczych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gathering_information",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz <w>zwiad</w> powrócił do bazy z cennymi informacjami.",
                            "target_lang_translations": {
                                "ru": "Наша <w>разведка</w> вернулась на базу с ценной информацией."
                            }
                        },
                        {
                            "text": "W dawnych czasach lekki <w>zwiad</w> poruszał się konno.",
                            "target_lang_translations": {
                                "ru": "В прежние времена легкая <w>разведка</w> передвигалась верхом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zwiad konny",
                        "dowódca zwiadu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подразделение солдат, назначенное для ведения разведывательных действий."
                    },
                    "translations": {

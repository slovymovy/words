                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arm",
                                "target_lang_sense_clarification": "figurative extension of land"
                            },
                            {
                                "target_lang_word": "spur",
                                "target_lang_sense_clarification": "projection of land or forest"
                            }
                        ]
                    }
                }
            ]
        }

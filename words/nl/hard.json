                        ]
                    }
                },
                {
                    "sense_id": "58226524-5f13-3063-96c8-61339d6ce15c",
                    "sense_definition": "Moeilijk, zwaar of streng; zonder medelijden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "difficulty_severity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was een <w>harde</w> strijd om de finale te bereiken.",
                            "target_lang_translations": {
                                "ru": "Это была <w>тяжёлая</w> борьба за выход в финал."
                            }
                        },
                        {
                            "text": "De rechter gaf de crimineel een <w>harde</w> straf.",
                            "target_lang_translations": {
                                "ru": "Судья вынес преступнику <w>суровый</w> приговор."
                            }
                        }
                    ],
                    "antonyms": [
                        "zacht",
                        "zwak",
                        "week"
                    ],
                    "common_phrases": [
                        "harde tijden",
                        "harde werker",
                        "harde feiten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудный, тяжёлый или суровый; безжалостный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тяжёлый",
                                "target_lang_sense_clarification": "трудный"
                            },
                            {
                                "target_lang_word": "суровый",
                                "target_lang_sense_clarification": "строгий"
                            },
                            {
                                "target_lang_word": "жестокий",
                                "target_lang_sense_clarification": "безжалостный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de40b3ce-0705-30c7-b274-2f577255e21d",
                    "sense_definition": "Met veel kalk (van water).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "water_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze regio komt er <w>hard</w> water uit de kraan.",
                            "target_lang_translations": {
                                "ru": "В этом регионе из крана течёт <w>жёсткая</w> вода."
                            }
                        },
                        {
                            "text": "<w>Hard</w> water kan slecht zijn voor je wasmachine.",
                            "target_lang_translations": {
                                "ru": "<w>Жёсткая</w> вода может быть вредна для вашей стиральной машины."
                            }
                        }
                    ],
                    "antonyms": [
                        "zacht"
                    ],
                    "common_phrases": [
                        "hard water"
                    ],
                    "target_lang_definitions": {
                        "ru": "С высоким содержанием извести (о воде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жёсткий",
                                "target_lang_sense_clarification": "о воде"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "szambowy",
        "szambiarz",
        "szambowóz",
        "szambonurek"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c3dd0bb-b4d4-3cce-9e9e-8f62fc3278e9",
                    "sense_definition": "Podziemny, bezodpływowy zbiornik na nieczystości płynne odprowadzane z domowej instalacji kanalizacyjnej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właściciel domu musi regularnie dbać o to, aby opróżniać <w>szambo</w>.",
                            "target_lang_translations": {
                                "ru": "Владелец дома должен регулярно следить за тем, чтобы опорожнять <w>выгребную яму</w>."
                            }
                        },
                        {
                            "text": "Na naszej działce nie ma kanalizacji, więc wykopaliśmy szczelne <w>szambo</w>.",
                            "target_lang_translations": {
                                "ru": "На нашем участке нет канализации, поэтому мы выкопали герметичный <w>септик</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kloaka",
                        "gw. (Śląsk Cieszyński) zengruba"
                    ],
                    "common_phrases": [
                        "wywóz szamba",
                        "pełne szambo",
                        "opróżniać szambo",
                        "szczelne szambo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подземный бессточный резервуар для жидких нечистот, отводимых из домашней канализации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выгребная яма",
                                "target_lang_sense_clarification": "традиционный резервуар для нечистот"
                            },
                            {
                                "target_lang_word": "септик",
                                "target_lang_sense_clarification": "современная система очистки, часто используется как синоним"
                            },
                            {
                                "target_lang_word": "выгреб",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27ac759f-6c3c-3ddd-a218-d76bb7676612",
                    "sense_definition": "Samochód ciężarowy wyposażony w pompę i cysternę, służący do wywozu nieczystości płynnych.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rano pod nasz blok podjechało <w>szambo</w>, co wywołało spory hałas.",
                            "target_lang_translations": {
                                "ru": "Утром к нашему дому подъехала <w>ассенизаторская машина</w>, что наделало много шума."
                            }
                        },
                        {
                            "text": "Kierowca prowadzący <w>szambo</w> miał problem z wjazdem na wąską posesję.",
                            "target_lang_translations": {
                                "ru": "У водителя <w>ассенизатора</w> возникли проблемы с въездом на узкий участок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szambo przyjechało",
                        "zamówić szambo"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie o szambowozie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грузовой автомобиль, оснащенный насосом и цистерной, служащий для вывоза жидких бытовых отходов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ассенизаторская машина",
                                "target_lang_sense_clarification": "транспортное средство"
                            },
                            {
                                "target_lang_word": "ассенизатор",
                                "target_lang_sense_clarification": "разговорное название машины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d82b148a-83d3-3353-a36c-d5c05198998c",
                    "sense_definition": "Sytuacja bardzo nieprzyjemna, kłopotliwa, pełna niejasności lub moralnie naganna.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ujawnieniu tajnych dokumentów w rządzie zrobiło się niezłe <w>szambo</w>.",
                            "target_lang_translations": {
                                "ru": "После обнародования секретных документов в правительстве поднялась настоящая <w>грязь</w>."
                            }
                        },
                        {
                            "text": "Nie chcę się mieszać w to towarzyskie <w>szambo</w> i plotki.",
                            "target_lang_translations": {
                                "ru": "Я не хочу вмешиваться в эту светскую <w>помойку</w> и сплетни."
                            }
                        }
                    ],
                    "synonyms": [
                        "bagno"
                    ],
                    "common_phrases": [
                        "niezłe szambo",
                        "zrobić szambo",
                        "medialne szambo"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень неприятная, скандальная или морально грязная ситуация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грязь",
                                "target_lang_sense_clarification": "переносное значение: скандалы, интриги"
                            },
                            {
                                "target_lang_word": "помойка",
                                "target_lang_sense_clarification": "переносное значение: неприятная среда или ситуация"
                            },
                            {
                                "target_lang_word": "дерьмо",
                                "target_lang_sense_clarification": "грубое, но точное соответствие по смыслу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2de2226-c610-350a-8d49-aa4ac485643c",
                    "sense_definition": "Grupa osób postrzegana jako zdemoralizowana, nieuczciwa lub podejrzana.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trzymaj się z daleka od tych oszustów, to jest zwykłe <w>szambo</w>.",
                            "target_lang_translations": {
                                "ru": "Держись подальше от этих мошенников, это просто <w>сброд</w>."
                            }
                        },
                        {
                            "text": "Całe to polityczne <w>szambo</w> dba tylko o własne interesy.",
                            "target_lang_translations": {
                                "ru": "Вся эта политическая <w>гниль</w> заботится только о собственных интересах."
                            }
                        }
                    ],
                    "synonyms": [
                        "bagno"
                    ],
                    "common_phrases": [
                        "moralne szambo"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "wydźwięk pejoratywny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, воспринимаемая как деморализованная, нечестная или подозрительная."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сброд",
                                "target_lang_sense_clarification": "о людях"
                            },
                            {
                                "target_lang_word": "гадюшник",
                                "target_lang_sense_clarification": "о коллективе или месте"
                            },
                            {
                                "target_lang_word": "гниль",
                                "target_lang_sense_clarification": "о моральном разложении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "autonoom",
        "autonomist",
        "autonomisme"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6dcf15df-ea2a-3264-9ad0-b67230b4523c",
                    "sense_definition": "Het recht of de bevoegdheid van een land, regio of organisatie om zichzelf te besturen en eigen wetten te maken.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vlaanderen wil steeds meer <w>autonomie</w> op het gebied van cultuur en onderwijs.",
                            "target_lang_translations": {
                                "en": "Flanders wants increasingly more <w>autonomy</w> in the areas of culture and education."
                            }
                        },
                        {
                            "text": "De regio strijdt al jaren voor politieke <w>autonomie</w> binnen het koninkrijk.",
                            "target_lang_translations": {
                                "en": "The region has been fighting for political <w>autonomy</w> within the kingdom for years."
                            }
                        }
                    ],
                    "synonyms": [
                        "zelfbestuur",
                        "zelfstandigheid"
                    ],
                    "antonyms": [
                        "heteronomie"
                    ],
                    "common_phrases": [
                        "politieke autonomie",
                        "lokale autonomie",
                        "meer autonomie"
                    ],
                    "target_lang_definitions": {
                        "en": "The right or authority of a country, region, or organization to govern itself and make its own laws."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "autonomy",
                                "target_lang_sense_clarification": "political/administrative"
                            },
                            {
                                "target_lang_word": "self-government",
                                "target_lang_sense_clarification": "political structure"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f5222f4-c16a-35de-91d0-ceffde52317d",
                    "sense_definition": "De vrijheid en het vermogen van een individu om eigen beslissingen te nemen en onafhankelijk te handelen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Patiënten eisen steeds meer <w>autonomie</w> op bij het bepalen van hun medische behandeling.",
                            "target_lang_translations": {
                                "en": "Patients are demanding more <w>autonomy</w> in determining their medical treatment."
                            }
                        },
                        {
                            "text": "Het behouden van persoonlijke <w>autonomie</w> is essentieel voor het welzijn van ouderen.",
                            "target_lang_translations": {
                                "en": "Maintaining personal <w>autonomy</w> is essential for the well-being of the elderly."
                            }
                        }
                    ],
                    "synonyms": [
                        "zelfbeschikking",
                        "onafhankelijkheid"
                    ],
                    "antonyms": [
                        "heteronomie"
                    ],
                    "common_phrases": [
                        "persoonlijke autonomie",
                        "autonomie van de patiënt",
                        "lichamelijke autonomie"
                    ],
                    "target_lang_definitions": {
                        "en": "The freedom and ability of an individual to make their own decisions and act independently."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "autonomy",
                                "target_lang_sense_clarification": "personal freedom"
                            },
                            {
                                "target_lang_word": "self-determination",
                                "target_lang_sense_clarification": "making own choices"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0527266-cde0-3b46-914b-0fd035fad7fe",
                    "sense_definition": "De staat van onafhankelijk zijn van externe bronnen of toevoer, vaak met betrekking tot energie of grondstoffen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>autonomie</w> van dit bedrijf ging zover dat ze niet meer waren aangesloten op het stroomnet.",
                            "target_lang_translations": {
                                "en": "The <w>self-sufficiency</w> of this company went so far that they were no longer connected to the power grid."
                            }
                        },
                        {
                            "text": "Voor een grotere strategische <w>autonomie</w> investeert het land in eigen energiebronnen.",
                            "target_lang_translations": {
                                "en": "For greater strategic <w>autonomy</w>, the country is investing in its own energy sources."
                            }
                        }
                    ],
                    "synonyms": [
                        "zelfvoorzienendheid"
                    ],
                    "common_phrases": [
                        "strategische autonomie",
                        "energetische autonomie"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of being independent from external sources or supply, often regarding energy or raw materials."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "self-sufficiency",
                                "target_lang_sense_clarification": "independence from external supply"
                            },
                            {
                                "target_lang_word": "autonomy",
                                "target_lang_sense_clarification": "strategic independence"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c566b632-f3d9-3a0b-98e7-a8caff19decf",
                    "sense_definition": "A short form of the male given names <w>Steven</w> or <w>Stephen</w>, or a formal name in its own right.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "male_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My brother <w>Steve</w> is coming to visit us this weekend.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>Стив</w> приедет навестить нас в эти выходные."
                            }
                        },
                        {
                            "text": "Please tell <w>Steve</w> that the meeting starts at nine o'clock.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, передай <w>Стиву</w>, что собрание начинается в девять часов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма мужского имени Стивен или Стивен, либо самостоятельное имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Стив",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db13a914-dd70-3c2e-98b6-4776316ffd87",
                    "sense_definition": "A short form of the female given name <w>Stephanie</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "female_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Although her name is Stephanie, she prefers to be called <w>Steve</w> by her friends.",
                            "target_lang_translations": {
                                "ru": "Хотя её зовут Стефани, она предпочитает, чтобы друзья называли её <w>Стив</w>."
                            }
                        },
                        {
                            "text": "We were surprised when <w>Steve</w> walked in, as we expected a man, not a woman.",
                            "target_lang_translations": {
                                "ru": "Мы удивились, когда вошла <w>Стив</w>, так как ожидали мужчину, а не женщину."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма женского имени Стефани."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Стив",
                                "target_lang_sense_clarification": "женское имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "39bec328-fafc-3ac0-957c-f7f898fc428c",
                    "sense_definition": "To pack or stow items tightly, especially cargo in a ship's hold.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crew worked hard to <w>steve</w> the cotton bales into the lower deck.",
                            "target_lang_translations": {
                                "ru": "Команда усердно работала, чтобы <w>уложить</w> кипы хлопка на нижнюю палубу."
                            }
                        },
                        {
                            "text": "It takes skill to <w>steve</w> the cargo so that it does not shift during the voyage.",
                            "target_lang_translations": {
                                "ru": "Требуется мастерство, чтобы <w>уложить</w> груз так, чтобы он не сместился во время плавания."
                            }
                        }
                    ],
                    "synonyms": [
                        "stow",
                        "pack",
                        "load"
                    ],
                    "antonyms": [
                        "unload"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Плотно укладывать или размещать предметы, особенно груз в трюме корабля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укладывать",
                                "target_lang_sense_clarification": "груз в трюм"
                            },
                            {
                                "target_lang_word": "грузить",
                                "target_lang_sense_clarification": "заполнять трюм"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f7db5781-da0d-3a32-8e75-5f67613fe14b",
                    "sense_definition": "An atmospheric optical phenomenon appearing as a purple ribbon of light in the sky, distinct from an aurora.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Photographers captured images of <w>Steve</w> appearing alongside the green aurora borealis.",
                            "target_lang_translations": {
                                "ru": "Фотографы запечатлели изображения <w>Стива</w>, появившегося рядом с зеленым северным сиянием."
                            }
                        },
                        {
                            "text": "Scientists are studying the atmospheric conditions that create the phenomenon known as <w>Steve</w>.",
                            "target_lang_translations": {
                                "ru": "Ученые изучают атмосферные условия, создающие явление, известное как <w>Стив</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "scientific"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Атмосферное оптическое явление в виде фиолетовой ленты света на небе, отличное от полярного сияния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Стив",
                                "target_lang_sense_clarification": "атмосферное явление"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "зевать",
        "прозевать",
        "зевота",
        "зевок",
        "ротозей"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5c8a93b4-1ebd-3e6f-bb43-58c979515579",
                    "sense_definition": "Проявить невнимательность, рассеянность; отвлечься от дела и упустить что-то важное.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "inattention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вратарь <w>зазевался</w> и пропустил лёгкий мяч в свои ворота.",
                            "target_lang_translations": {
                                "en": "The goalkeeper <w>was caught napping</w> and let an easy ball into his net."
                            }
                        },
                        {
                            "text": "На вокзале нельзя <w>зазеваться</w>, иначе можно остаться без багажа.",
                            "target_lang_translations": {
                                "en": "You cannot <w>get distracted</w> at the station, otherwise you might be left without luggage."
                            }
                        }
                    ],
                    "synonyms": [
                        "отвлечься",
                        "проворонить"
                    ],
                    "antonyms": [
                        "сосредоточиться",
                        "бдеть"
                    ],
                    "common_phrases": [
                        "нельзя зазеваться",
                        "чуть не зазевался"
                    ],
                    "target_lang_definitions": {
                        "en": "To become inattentive or distracted, often resulting in a mistake or missed opportunity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get distracted",
                                "target_lang_sense_clarification": "to lose attention"
                            },
                            {
                                "target_lang_word": "be caught napping",
                                "target_lang_sense_clarification": "idiom: to be unprepared due to inattention"
                            },
                            {
                                "target_lang_word": "lose focus",
                                "target_lang_sense_clarification": "to stop paying attention"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbcff8fc-b80d-3f94-ae14-9b3b165a1254",
                    "sense_definition": "Увлечённо засмотреться на кого-либо или что-либо, забыв об окружающем.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gazing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы <w>зазевались</w> на красивые витрины и отстали от группы.",
                            "target_lang_translations": {
                                "en": "The tourists <w>gawked</w> at the beautiful shop windows and fell behind the group."
                            }
                        },
                        {
                            "text": "Водитель <w>зазевался</w> на рекламный щит и чуть не создал аварию.",
                            "target_lang_translations": {
                                "en": "The driver <w>stared</w> at a billboard and almost caused an accident."
                            }
                        }
                    ],
                    "synonyms": [
                        "засмотреться",
                        "заглядеться"
                    ],
                    "common_phrases": [
                        "зазеваться по сторонам",
                        "зазеваться на витрины"
                    ],
                    "target_lang_definitions": {
                        "en": "To stare at something with absorption, forgetting one's surroundings."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gawk",
                                "target_lang_sense_clarification": "to stare openly and often stupidly"
                            },
                            {
                                "target_lang_word": "gape",
                                "target_lang_sense_clarification": "to look with amazement, often with open mouth"
                            },
                            {
                                "target_lang_word": "stare",
                                "target_lang_sense_clarification": "to look fixedly"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
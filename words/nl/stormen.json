            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c7bbab21-12d4-3265-bb63-60e8956a7685",
                    "sense_definition": "Bijzonder sterk en hevig waaien.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>stormde</w> geweldig die nacht en er verging een aantal schepen.",
                            "target_lang_translations": {
                                "ru": "В ту ночь сильно <w>штормило</w>, и несколько кораблей затонуло.",
                                "en": "It <w>stormed</w> terribly that night and a number of ships were lost."
                            }
                        },
                        {
                            "text": "Als het buiten <w>stormt</w>, blijven de meeste mensen liever binnen.",
                            "target_lang_translations": {
                                "ru": "Когда на улице <w>штормит</w>, большинство людей предпочитают оставаться дома.",
                                "en": "When it <w>storms</w> outside, most people prefer to stay indoors."
                            }
                        }
                    ],
                    "synonyms": [
                        "spoken"
                    ],
                    "common_phrases": [
                        "het stormt",
                        "hard stormen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дуть очень сильно и порывисто; бушевать (о погоде).",
                        "en": "To blow very strongly and violently; to be stormy."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штормить",
                                "target_lang_sense_clarification": "безличный глагол, о погоде"
                            },
                            {
                                "target_lang_word": "бушевать",
                                "target_lang_sense_clarification": "о стихии, ветре"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "storm",
                                "target_lang_sense_clarification": "impersonal verb, regarding weather"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b80712e-b8a4-3be4-8c8e-95b5499f5405",
                    "sense_definition": "Zich met grote snelheid, wildheid of emotie voortbewegen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bel ging en de kinderen <w>stormden</w> direct naar buiten.",
                            "target_lang_translations": {
                                "ru": "Прозвенел звонок, и дети сразу <w>ринулись</w> на улицу.",
                                "en": "The bell rang and the children immediately <w>rushed</w> outside."
                            }
                        },
                        {
                            "text": "Hij <w>stormde</w> woedend de vergaderzaal uit zonder iets te zeggen.",
                            "target_lang_translations": {
                                "ru": "Он в ярости <w>вылетел</w> из конференц-зала, не сказав ни слова.",
                                "en": "He <w>stormed</w> furiously out of the meeting room without saying a word."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naar binnen stormen",
                        "naar buiten stormen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться с большой скоростью, яростью или на эмоциях.",
                        "en": "To move with great speed, wildness, or emotion."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нестись",
                                "target_lang_sense_clarification": "быстро двигаться"

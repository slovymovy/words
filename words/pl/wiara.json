                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Верность данному слову, преданность или постоянство в чувствах (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верность",
                                "target_lang_sense_clarification": "преданность"
                            },
                            {
                                "target_lang_word": "честность",
                                "target_lang_sense_clarification": "добросовестность"
                            }
                        ]

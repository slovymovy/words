                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "татский язык",
                                "target_lang_sense_clarification": "язык татов"
                            },
                            {
                                "target_lang_word": "татский",
                                "target_lang_sense_clarification": "прилагательное или субстантивированное существительное"
                            }

{
    "word_family": [
        "frictional",
        "frictionless",
        "antifriction"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dfe6f639-6405-3be8-9a5d-5996145793fb",
                    "sense_definition": "Disagreement or unfriendliness caused by people having different opinions or interests.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "conflict_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is often <w>friction</w> between the two departments regarding the budget allocation.",
                            "target_lang_translations": {
                                "ru": "Между двумя отделами часто возникают <w>разногласия</w> по поводу распределения бюджета."
                            }
                        },
                        {
                            "text": "Living in such a small apartment caused significant <w>friction</w> between the roommates.",
                            "target_lang_translations": {
                                "ru": "Проживание в такой маленькой квартире вызывало значительные <w>трения</w> между соседями."
                            }
                        }
                    ],
                    "synonyms": [
                        "conflict",
                        "discord",
                        "tension",
                        "disagreement",
                        "strife"
                    ],
                    "antonyms": [
                        "harmony",
                        "accord",
                        "agreement"
                    ],
                    "common_phrases": [
                        "cause friction",
                        "friction between",
                        "source of friction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разногласия или неприязнь, вызванные различием во мнениях или интересах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разногласия",
                                "target_lang_sense_clarification": "отсутствие согласия, споры"
                            },
                            {
                                "target_lang_word": "трения",
                                "target_lang_sense_clarification": "конфликты, напряженность в отношениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73871f3d-8a24-3199-b231-1e902a27a279",
                    "sense_definition": "A physical force that resists the movement of one surface sliding over or through another.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oil is used in the engine to reduce <w>friction</w> between the moving parts.",
                            "target_lang_translations": {
                                "ru": "Масло используется в двигателе для уменьшения <w>трения</w> между движущимися частями."
                            }
                        },
                        {
                            "text": "Without <w>friction</w>, the tires would just spin on the ice without moving the car forward.",
                            "target_lang_translations": {
                                "ru": "Без <w>трения</w> шины просто крутились бы на льду, не двигая машину вперед."
                            }
                        }
                    ],
                    "synonyms": [
                        "resistance",
                        "drag"
                    ],
                    "common_phrases": [
                        "reduce friction",
                        "kinetic friction",
                        "static friction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая сила, препятствующая движению одной поверхности относительно другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трение",
                                "target_lang_sense_clarification": "физическое явление"
                            },
                            {
                                "target_lang_word": "сила трения",
                                "target_lang_sense_clarification": "физическая величина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f4c7229-1fd5-31a1-a39b-bc48c0aaa115",
                    "sense_definition": "The action of rubbing one object or surface against another.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He warmed his cold hands by the <w>friction</w> of rubbing them together vigorously.",
                            "target_lang_translations": {
                                "ru": "Он согрел замерзшие руки <w>трением</w>, энергично потирая их друг о друга."
                            }
                        },
                        {
                            "text": "The constant <w>friction</w> of the rope against the sharp rock caused it to fray and break.",
                            "target_lang_translations": {
                                "ru": "Постоянное <w>трение</w> веревки об острый камень привело к тому, что она перетерлась и порвалась."
                            }
                        }
                    ],
                    "synonyms": [
                        "rubbing",
                        "abrasion"
                    ],
                    "common_phrases": [
                        "heat of friction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по трению одного предмета или поверхности о другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трение",
                                "target_lang_sense_clarification": "механическое действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71dbc22e-652e-3739-ad93-1fd3424e24ba",
                    "sense_definition": "A massage technique involving rubbing the body to improve blood circulation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The therapist applied gentle <w>friction</w> to the patient's leg to help restore warmth.",
                            "target_lang_translations": {
                                "ru": "Терапевт применил легкое <w>растирание</w> ноги пациента, чтобы помочь восстановить тепло."
                            }
                        },
                        {
                            "text": "Daily <w>frictions</w> on the skin were prescribed to improve the blood flow in the affected area.",
                            "target_lang_translations": {
                                "ru": "Ежедневные <w>растирания</w> кожи были предписаны для улучшения кровотока в пораженной области."
                            }
                        }
                    ],
                    "synonyms": [
                        "massage",
                        "rubbing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Техника массажа, включающая растирание тела для улучшения кровообращения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растирание",
                                "target_lang_sense_clarification": "прием массажа"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "947b36f9-9c69-3de7-8057-0bda4595d25a",
                    "sense_definition": "Gemakkelijk te beïnvloeden of aan te tasten, bijvoorbeeld door een ziekte of emotie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "susceptible",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kleine kinderen zijn vaak <w>vatbaarder</w> voor griep dan volwassenen.",
                            "target_lang_translations": {
                                "ru": "Маленькие дети часто более <w>восприимчивы</w> к гриппу, чем взрослые."
                            }
                        },
                        {
                            "text": "Door vermoeidheid was hij tijdelijk meer <w>vatbaar</w> voor negatieve gedachten.",
                            "target_lang_translations": {
                                "ru": "Из-за усталости он был временно более <w>подвержен</w> негативным мыслям."
                            }
                        }
                    ],
                    "synonyms": [
                        "gevoelig",
                        "ontvankelijk"
                    ],
                    "antonyms": [
                        "onvatbaar",
                        "onvatbaar"
                    ],
                    "common_phrases": [
                        "vatbaar zijn voor",
                        "vatbaar voor ziektes",
                        "vatbaar voor kritiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легко поддающийся влиянию или воздействию, например, болезни или эмоциям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восприимчивый",
                                "target_lang_sense_clarification": "к болезням, инфекциям или внешнему влиянию"
                            },
                            {
                                "target_lang_word": "подверженный",
                                "target_lang_sense_clarification": "склонный к чему-то негативному, например, стрессу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c49cbf0-a0fc-3553-baa4-908ff61fb5cf",
                    "sense_definition": "Geschikt om een bepaalde handeling of werking te ondergaan; waar iets op toegepast kan worden.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "subject_to",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn eerste ontwerp was zeker nog voor verbetering <w>vatbaar</w>.",
                            "target_lang_translations": {
                                "ru": "Его первый проект, безусловно, еще <w>поддавался</w> улучшению."
                            }
                        },
                        {
                            "text": "Tegen deze beslissing van de rechter is geen beroep <w>vatbaar</w>.",
                            "target_lang_translations": {
                                "ru": "Это решение судьи не <w>подлежит</w> обжалованию."
                            }
                        }
                    ],
                    "synonyms": [
                        "geschikt"
                    ],
                    "antonyms": [
                        "onvatbaar"
                    ],
                    "common_phrases": [

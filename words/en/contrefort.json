    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "28f219e0-0b82-3de0-93a2-366489e6af0c",
                    "sense_definition": "A buttress or masonry structure built against a wall to strengthen or support it.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "structural_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineer examined the <w>contrefort</w> supporting the ancient cathedral wall.",
                            "target_lang_translations": {
                                "ru": "Инженер осмотрел <w>контрфорс</w>, поддерживающий стену древнего собора."
                            }
                        },
                        {
                            "text": "A heavy masonry <w>contrefort</w> was added to stabilize the structure.",
                            "target_lang_translations": {
                                "ru": "Для стабилизации сооружения был добавлен массивный каменный <w>контрфорс</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "counterfort"
                    ],
                    "common_phrases": [
                        "masonry contrefort",
                        "wall contrefort"
                    ],
                    "target_lang_definitions": {
                        "ru": "Контрфорс или каменная конструкция, возведенная у стены для ее укрепления или поддержки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контрфорс",
                                "target_lang_sense_clarification": "архитектурный элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a82a7a27-24b5-350a-b7c7-6037a4d8cd62",
                    "sense_definition": "A spur or minor ridge projecting from a main mountain range.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geographical_feature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They climbed the steep <w>contrefort</w> to reach the summit.",
                            "target_lang_translations": {
                                "ru": "Они поднялись по крутому <w>контрфорсу</w>, чтобы добраться до вершины."
                            }
                        },
                        {
                            "text": "The path winds along a rocky <w>contrefort</w> of the mountain range.",
                            "target_lang_translations": {
                                "ru": "Тропа вьется вдоль скалистого <w>отрога</w> горного хребта."
                            }
                        }
                    ],
                    "synonyms": [
                        "counterfort"
                    ],
                    "common_phrases": [
                        "mountain contrefort",
                        "rocky contrefort"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрог или небольшой хребет, отходящий от основного горного массива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрог",
                                "target_lang_sense_clarification": "горный выступ"
                            },
                            {
                                "target_lang_word": "контрфорс",
                                "target_lang_sense_clarification": "геологический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

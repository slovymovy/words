    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d15d750b-d3b6-38bf-90be-09657b0aa57d",
                    "sense_definition": "In a genuine, honest, and earnest way.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>sincerely</w> hope that you will feel better soon.",
                            "target_lang_translations": {
                                "ru": "Я <w>искренне</w> надеюсь, что вы скоро поправитесь."
                            }
                        },
                        {
                            "text": "She <w>sincerely</w> believed that she was making the right choice.",
                            "target_lang_translations": {
                                "ru": "Она <w>искренне</w> верила, что делает правильный выбор."
                            }
                        }
                    ],
                    "synonyms": [
                        "honestly",
                        "genuinely",
                        "truthfully",
                        "earnestly"
                    ],
                    "common_phrases": [
                        "sincerely hope",
                        "sincerely appreciate",
                        "sincerely apologize",
                        "sincerely believe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искренним, честным и серьезным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искренне",
                                "target_lang_sense_clarification": "честно, от всего сердца"
                            },
                            {
                                "target_lang_word": "от души",
                                "target_lang_sense_clarification": "с глубоким чувством"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3c33a4b-87cc-3bfb-ad15-6eda138ae2c3",
                    "sense_definition": "A conventional formula used to end a formal letter or email, typically when the recipient is addressed by name.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please let me know if you have any questions.\n<w>Sincerely</w>,\nJohn Smith",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, дайте знать, если у вас возникнут вопросы.\n<w>С уважением</w>,\nДжон Смит"
                            }
                        },
                        {
                            "text": "I look forward to hearing from you.\n<w>Sincerely</w>,\nJane Doe",
                            "target_lang_translations": {
                                "ru": "С нетерпением жду вашего ответа.\n<w>С уважением</w>,\nДжейн Доу"
                            }
                        }
                    ],
                    "synonyms": [
                        "yours sincerely",
                        "sincerely yours",
                        "yours truly",
                        "best regards",
                        "regards"
                    ],
                    "common_phrases": [
                        "yours sincerely",
                        "sincerely yours"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стандартная формула вежливости для завершения официального письма или электронного сообщения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "С уважением",
                                "target_lang_sense_clarification": "стандартная подпись в деловой переписке"
                            },
                            {
                                "target_lang_word": "Искренне Ваш",
                                "target_lang_sense_clarification": "более формальный или устаревший вариант"
                            }
                        ]
                    }
                }
            ]
        }
    ]

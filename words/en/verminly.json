    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9ace2aeb-d77c-3aac-b495-9dee604aa8bc",
                    "sense_definition": "Resembling or characteristic of vermin.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The abandoned house was infested with <w>verminly</w> creatures hiding in the walls.",
                            "target_lang_translations": {
                                "ru": "Заброшенный дом кишел <w>паразитическими</w> тварями, прячущимися в стенах."
                            }
                        },
                        {
                            "text": "He described the corrupt officials as a <w>verminly</w> generation destroying the city.",
                            "target_lang_translations": {
                                "ru": "Он описал коррумпированных чиновников как <w>гнусное</w> поколение, разрушающее город."
                            }
                        }
                    ],
                    "synonyms": [
                        "verminous"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий паразитов или вредителей; свойственный им."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паразитический",
                                "target_lang_sense_clarification": "относящийся к паразитам"
                            },
                            {
                                "target_lang_word": "мерзкий",
                                "target_lang_sense_clarification": "вызывающий отвращение, гнусный"
                            },
                            {
                                "target_lang_word": "вредительский",
                                "target_lang_sense_clarification": "свойственный вредителям"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "acfee539-4675-3d8c-9a5d-4aa12920c563",
                    "sense_definition": "In a manner resembling vermin; like a pest.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The spy crept <w>verminly</w> through the sewers to escape the guards.",
                            "target_lang_translations": {
                                "ru": "Шпион <w>по-крысиному</w> пробирался по канализации, чтобы скрыться от стражи."
                            }
                        },
                        {
                            "text": "They acted <w>verminly</w> by stealing food from the weakest members of the group.",
                            "target_lang_translations": {
                                "ru": "Они поступили <w>гнусно</w>, украв еду у самых слабых членов группы."
                            }
                        }
                    ],
                    "synonyms": [
                        "verminously"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подобно паразиту или вредителю; гнусно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнусно",
                                "target_lang_sense_clarification": "подло, отвратительно"
                            },
                            {
                                "target_lang_word": "по-крысиному",
                                "target_lang_sense_clarification": "скрытно и подло"
                            },
                            {
                                "target_lang_word": "как паразит",
                                "target_lang_sense_clarification": "за счет других"
                            }
                        ]
                    }
                }
            ]
        }
    ]

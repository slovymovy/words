    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "332a4f99-5bb0-37e5-9bc5-9de728aa1e7d",
                    "sense_definition": "Kopiować dane, pliki lub programy z internetu na własny komputer, telefon lub inne urządzenie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę znaleźć darmowe wi-fi, aby <w>pobierać</w> te duże pliki.",
                            "target_lang_translations": {
                                "ru": "Мне нужно найти бесплатный Wi-Fi, чтобы <w>скачать</w> эти большие файлы."
                            }
                        },
                        {
                            "text": "Uczniowie mogą <w>pobierać</w> podręczniki w formacie cyfrowym ze strony biblioteki.",
                            "target_lang_translations": {
                                "ru": "Ученики могут <w>скачивать</w> учебники в цифровом формате с сайта библиотеки."
                            }
                        }
                    ],
                    "synonyms": [
                        "ściągać"
                    ],
                    "antonyms": [
                        "wysyłać",
                        "udostępniać"
                    ],
                    "common_phrases": [
                        "pobierać pliki",
                        "pobierać aplikację",
                        "pobierać dane",
                        "pobierać z internetu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Копировать данные, файлы или программы из интернета на собственный компьютер, телефон или другое устройство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скачивать",
                                "target_lang_sense_clarification": "файлы, программы"
                            },
                            {
                                "target_lang_word": "загружать",
                                "target_lang_sense_clarification": "данные из интернета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f0fe1f6-02e0-32df-8f44-5829363513f2",
                    "sense_definition": "Zawierać związek małżeński (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "relationships",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi znajomi planują <w>pobierać się</w> w przyszłym roku w lipcu.",
                            "target_lang_translations": {
                                "ru": "Мои знакомые планируют <w>пожениться</w> в июле следующего года."
                            }
                        },
                        {
                            "text": "W dzisiejszych czasach ludzie decydują się <w>pobierać</w> znacznie później niż dawniej.",
                            "target_lang_translations": {
                                "ru": "В наше время люди решают <w>жениться</w> гораздо позже, чем раньше."
                            }
                        }
                    ],
                    "synonyms": [
                        "żenić się"
                    ],
                    "antonyms": [
                        "rozwodzić się"
                    ],
                    "common_phrases": [
                        "pobierać się z miłości",
                        "pobierać się w kościele"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вступать в брак (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жениться",
                                "target_lang_sense_clarification": "вступать в брак"
                            },
                            {
                                "target_lang_word": "пожениться",
                                "target_lang_sense_clarification": "создать семью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "197523b6-bf3a-3dd1-af65-651e970da8d3",
                    "sense_definition": "Przyjmować lub ściągać należne pieniądze, takie jak opłaty, podatki, pensję lub zasiłki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bank będzie automatycznie <w>pobierać</w> opłatę za prowadzenie twojego konta.",
                            "target_lang_translations": {
                                "ru": "Банк будет автоматически <w>взимать</w> плату за обслуживание твоего счета."
                            }
                        },
                        {
                            "text": "Mój dziadek co miesiąc chodzi na pocztę, aby <w>pobierać</w> emeryturę.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка каждый месяц ходит на почту, чтобы <w>получать</w> пенсию."
                            }
                        }
                    ],
                    "synonyms": [
                        "otrzymywać",
                        "inkasować"
                    ],
                    "antonyms": [
                        "płacić",
                        "wpłacać"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "93b04679-1369-3607-a2e7-58b7af5fa2a5",
                    "sense_definition": "Potrawa z grubego plastra mięsa (zazwyczaj wołowego) krojonego w poprzek włókien, smażona lub grillowana.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na obiad zamówiłem soczysty <w>stek</w> z pieczonymi ziemniakami.",
                            "target_lang_translations": {
                                "ru": "На обед я заказал сочный <w>стейк</w> с печёным картофелем."
                            }
                        },
                        {
                            "text": "Najbardziej lubię średnio wysmażony <w>stek</w> wołowy.",
                            "target_lang_translations": {
                                "ru": "Больше всего я люблю говяжий <w>стейк</w> средней прожарки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stek wołowy",
                        "krwisty stek",
                        "średnio wysmażony stek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Блюдо из толстого куска мяса (обычно говядины), нарезанного поперек волокон, жареное или приготовленное на гриле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стейк",
                                "target_lang_sense_clarification": "кулинарное блюдо"
                            },
                            {
                                "target_lang_word": "бифштекс",
                                "target_lang_sense_clarification": "блюдо из говядины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47049869-a7fd-33b2-884c-8acc09ee821c",
                    "sense_definition": "Duże nagromadzenie słów lub określeń, zazwyczaj o negatywnym wydźwięku, takich jak kłamstwa czy wyzwiska.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To, co powiedziałeś, to zwykły <w>stek</w> bzdur i pomówień.",
                            "target_lang_translations": {
                                "ru": "То, что ты сказал, — это просто <w>нагромождение</w> вздора и клеветы."
                            }
                        },
                        {
                            "text": "Zatrzymany mężczyzna niespodziewanie obrzucił ich <w>stekiem</w> wyzwisk.",
                            "target_lang_translations": {
                                "ru": "Задержанный мужчина неожиданно обрушил на них <w>поток</w> оскорблений."
                            }
                        }
                    ],
                    "synonyms": [
                        "nagromadzenie",
                        "zbiorowisko"
                    ],
                    "common_phrases": [
                        "stek bzdur",
                        "stek kłamstw",
                        "stek wyzwisk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое скопление слов или выражений, обычно негативных, таких как ложь или оскорбления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нагромождение",
                                "target_lang_sense_clarification": "скопление (лжи, вздора)"
                            },
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "обилие (ругательств)"
                            },
                            {
                                "target_lang_word": "куча",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7e797af-9792-35e4-a876-5550b17b5c70",
                    "sense_definition": "Końcowa, rozszerzona część jelita u niektórych kręgowców, będąca wspólnym ujściem układów wydalniczego i rozrodczego.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "U ptaków <w>stek</w> jest miejscem, gdzie łączą się drogi moczowe i płciowe.",
                            "target_lang_translations": {
                                "ru": "У птиц <w>клоака</w> является местом, где соединяются мочевые и половые пути."
                            }
                        },
                        {
                            "text": "Gady wydalają produkty przemiany materii przez <w>stek</w>.",
                            "target_lang_translations": {
                                "ru": "Пресмыкающиеся выделяют продукты обмена веществ через <w>клоаку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kloaka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конечная расширенная часть кишки у некоторых позвоночных, являющаяся общим выходом для выделительной и половой систем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клоака",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                }
            ]
        }

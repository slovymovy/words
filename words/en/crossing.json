                },
                {
                    "sense_id": "73f20a04-7d01-3f9e-985b-21d0e8aaceb6",
                    "sense_definition": "The central area in a cruciform church where the nave, chancel, and transepts intersect.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The large dome was constructed directly above the <w>crossing</w> of the cathedral.",
                            "target_lang_translations": {
                                "ru": "Большой купол был возведен прямо над <w>средокрестием</w> собора."
                            }

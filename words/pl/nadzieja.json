                },
                {
                    "sense_id": "17bbc71c-ebb7-360e-aae1-e7270624e21f",
                    "sense_definition": "Opinia, sąd lub zdanie na jakiś temat.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "opinion_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Według mojej <w>nadziei</w>, sprawa ta zakończy się pomyślnie dla wszystkich.",
                            "target_lang_translations": {
                                "ru": "По моему <w>мнению</w>, это дело закончится благополучно для всех."
                            }
                        },
                        {
                            "text": "Wyraził swoją <w>nadzieję</w> w tej kwestii, nie zważając na sprzeciw innych.",
                            "target_lang_translations": {
                                "ru": "Он выразил свое <w>мнение</w> по этому вопросу, не обращая внимания на возражения других."
                            }
                        }
                    ],
                    "synonyms": [
                        "opinia",
                        "sąd",
                        "zdanie"
                    ],
                    "traits": [

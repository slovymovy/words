                            },
                            {
                                "target_lang_word": "вкладчик",
                                "target_lang_sense_clarification": "деньги"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "investor",
                                "target_lang_sense_clarification": "financial"
                            },
                            {
                                "target_lang_word": "financier",
                                "target_lang_sense_clarification": "funding projects"
                            }
                        ]
                    }

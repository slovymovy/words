    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "5ab558af-c272-3fb0-9713-1500b2fb1a0d",
                    "sense_definition": "Gebruikt als voorlopig onderwerp om aan te geven dat iets of iemand bestaat of aanwezig is (vaak in combinatie met 'zijn').",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Er</w> is geen melk meer in de koelkast.",
                            "target_lang_translations": {
                                "en": "<w>There</w> is no milk left in the fridge.",
                                "ru": "В холодильнике больше <w>нет</w> молока."
                            }
                        },
                        {
                            "text": "Is <w>er</w> nog iemand in de vergaderzaal?",
                            "target_lang_translations": {
                                "en": "Is <w>there</w> still someone in the meeting room?",
                                "ru": "<w>Есть</w> ли еще кто-нибудь в конференц-зале?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "er is",
                        "er zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Used as a provisional subject to indicate that something or someone exists or is present (often in combination with 'to be').",
                        "ru": "Используется как формальное подлежащее для указания на существование или присутствие кого-либо или чего-либо (часто в сочетании с 'zijn')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "there",
                                "target_lang_sense_clarification": "existential subject (there is/are)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "указание на наличие"
                            },
                            {
                                "target_lang_word": "там",
                                "target_lang_sense_clarification": "указание на место или наличие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "588f6b09-ec10-39bd-a295-acce39d7c11d",
                    "sense_definition": "Verwijst naar een hoeveelheid of aantal van iets dat eerder genoemd is; betekent 'van die' of 'van dat'.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broer heeft drie kinderen en ik heb <w>er</w> twee.",
                            "target_lang_translations": {
                                "en": "My brother has three children and I have two <w>of them</w>.",
                                "ru": "У моего брата трое детей, а у меня <w>их</w> двое."
                            }
                        },
                        {
                            "text": "Hoeveel appels wil je hebben? Ik wil <w>er</w> graag vijf.",
                            "target_lang_translations": {
                                "en": "How many apples do you want? I would like five <w>of them</w>.",
                                "ru": "Сколько яблок ты хочешь? Я бы хотел <w>их</w> пять."
                            }
                        }
                    ],
                    "synonyms": [
                        "ervan"
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to a quantity or number of something previously mentioned; translates to 'of them' or 'of those'.",
                        "ru": "Указывает на количество ранее упомянутых предметов или лиц; соответствует значениям 'из них' или 'их'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of them",
                                "target_lang_sense_clarification": "referring to a quantity"
                            },
                            {
                                "target_lang_word": "ones",
                                "target_lang_sense_clarification": "referring to a count"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "их",
                                "target_lang_sense_clarification": "родительный падеж, часть целого"
                            },
                            {
                                "target_lang_word": "из них",
                                "target_lang_sense_clarification": "выделение из группы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71499819-4dfb-322b-91a1-7d809ba0d853",
                    "sense_definition": "Verwijst naar dingen, dieren of zaken in combinatie met een voorzetsel; vervangt 'het' of 'ze'.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <w>er</w> gisteren lang met hem over gepraat.",
                            "target_lang_translations": {
                                "en": "I talked to him about <w>it</w> for a long time yesterday.",
                                "ru": "Я вчера долго говорил с ним об <w>этом</w>."
                            }
                        },
                        {
                            "text": "Je kunt <w>er</w> de hoge bergen boven zien uitsteken.",
                            "target_lang_translations": {
                                "en": "You can see the high mountains sticking out above <w>it</w>.",
                                "ru": "Можно увидеть, как над <w>ними</w> возвышаются высокие горы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to things, animals, or matters in combination with a preposition; replaces 'it' or 'them'.",
                        "ru": "Ссылается на предметы, животных или явления в сочетании с предлогом; заменяет местоимения 'это', 'о нем', 'о ней' и т.д."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "it",
                                "target_lang_sense_clarification": "object of a preposition"
                            },
                            {
                                "target_lang_word": "them",
                                "target_lang_sense_clarification": "object of a preposition (plural)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "этом",
                                "target_lang_sense_clarification": "местоимение в предложном падеже"
                            },
                            {
                                "target_lang_word": "это",
                                "target_lang_sense_clarification": "указательное местоимение"
                            },
                            {
                                "target_lang_word": "них",
                                "target_lang_sense_clarification": "множественное число"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6891e5d4-286a-34e1-91a7-e16e752d3784",
                    "sense_definition": "Gebruikt als onderwerp in passieve zinnen om een handeling te beschrijven zonder specifieke dader.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Er</w> wordt hard gewerkt aan de nieuwe weg.",
                            "target_lang_translations": {
                                "en": "<w>There</w> is hard work being done on the new road.",
                                "ru": "<w>Там</w> усердно работают над новой дорогой."
                            }
                        },
                        {
                            "text": "Tijdens het feest werd <w>er</w> veel gelachen en gedanst.",
                            "target_lang_translations": {
                                "en": "During the party <w>there</w> was a lot of laughing and dancing.",
                                "ru": "Во время вечеринки <w>там</w> много смеялись и танцевали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "er wordt",
                        "er werd"
                    ],
                    "target_lang_definitions": {
                        "en": "Used as a subject in passive sentences to describe an action without a specific doer.",
                        "ru": "Используется как подлежащее в пассивных предложениях для описания действия без указания конкретного исполнителя."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "there",
                                "target_lang_sense_clarification": "dummy subject in passive constructions"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "там",
                                "target_lang_sense_clarification": "формальное подлежащее"
                            },
                            {
                                "target_lang_word": "–",
                                "target_lang_sense_clarification": "часто не переводится"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "377103bd-f1d9-3b62-b26d-7042b18ef9f0",
                    "sense_definition": "Mecz, partia gry lub etap zawodów sportowych mający na celu wyłonienie zwycięzcy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "game_match",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Finałowa <w>rozgrywka</w> przyciągnęła przed telewizory miliony widzów.",
                            "target_lang_translations": {
                                "ru": "Финальный <w>поединок</w> собрал у экранов телевизоров миллионы зрителей."
                            }
                        },
                        {
                            "text": "To była bardzo zacięta <w>rozgrywka</w>, która zakończyła się remisem.",
                            "target_lang_translations": {
                                "ru": "Это была очень напряженная <w>игра</w>, которая закончилась вничью."
                            }
                        }
                    ],
                    "synonyms": [
                        "mecz"
                    ],
                    "common_phrases": [
                        "finałowa rozgrywka",
                        "zacięta rozgrywka",
                        "rozgrywka ligowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Матч, партия или этап спортивных соревнований, целью которого является выявление победителя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "матч",
                                "target_lang_sense_clarification": "спортивное состязание"
                            },
                            {
                                "target_lang_word": "партия",
                                "target_lang_sense_clarification": "часть игры (шахматы, теннис)"
                            },
                            {
                                "target_lang_word": "поединок",
                                "target_lang_sense_clarification": "соревнование двух сторон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8a51aa4-2cee-300e-9beb-e2ae11e4bd95",
                    "sense_definition": "Sposób prowadzenia gry, jej przebieg i mechanika, zwłaszcza w kontekście gier komputerowych lub planszowych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gameplay",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta gra oferuje wiele godzin wciągającej <w>rozgrywki</w>.",
                            "target_lang_translations": {
                                "ru": "Эта игра предлагает много часов увлекательного <w>игрового процесса</w>."
                            }
                        },
                        {
                            "text": "Tryb wieloosobowy urozmaica <w>rozgrywkę</w> i pozwala rywalizować z innymi.",
                            "target_lang_translations": {
                                "ru": "Многопользовательский режим разнообразит <w>геймплей</w> и позволяет соревноваться с другими."
                            }
                        }
                    ],
                    "synonyms": [
                        "gra"
                    ],
                    "common_phrases": [
                        "tryb rozgrywki",
                        "mechanika rozgrywki",
                        "płynna rozgrywka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ход игры, её механика и процесс, особенно в контексте компьютерных или настольных игр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игровой процесс",
                                "target_lang_sense_clarification": "механика игры"
                            },
                            {
                                "target_lang_word": "геймплей",
                                "target_lang_sense_clarification": "компьютерные игры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e17e4382-a609-38a6-a850-03cf2c80b4f1",
                    "sense_definition": "Rywalizacja, spór lub walka między przeciwstawnymi stronami o wpływy, władzę lub realizację własnych celów.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W firmie trwa cicha <w>rozgrywka</w> o stanowisko dyrektora.",
                            "target_lang_translations": {
                                "ru": "В компании идет тихая <w>борьба</w> за должность директора."
                            }
                        },
                        {
                            "text": "To tylko kolejna polityczna <w>rozgrywka</w> przed zbliżającymi się wyborami.",
                            "target_lang_translations": {
                                "ru": "Это всего лишь очередная политическая <w>игра</w> перед предстоящими выборами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rozgrywka polityczna",
                        "zakulisowa rozgrywka",
                        "ostra rozgrywka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соперничество, спор или борьба между сторонами за влияние, власть или достижение своих целей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "борьба",
                                "target_lang_sense_clarification": "противостояние"
                            },
                            {
                                "target_lang_word": "игра",
                                "target_lang_sense_clarification": "политическая или закулисная"
                            },
                            {
                                "target_lang_word": "интрига",
                                "target_lang_sense_clarification": "скрытые действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab5e006c-0356-3f7a-a820-4bd0fe6fd144",
                    "sense_definition": "W grach karcianych (np. w brydżu): faza gry polegająca na zagrywaniu kart, następująca po licytacji.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Błąd w <w>rozgrywce</w> kosztował nas utratę ważnych punktów.",
                            "target_lang_translations": {
                                "ru": "Ошибка в <w>розыгрыше</w> стоила нам потери важных очков."
                            }
                        },
                        {
                            "text": "Doświadczony brydżysta potrafi zaplanować całą <w>rozgrywkę</w> już po wyjściu pierwszą kartą.",
                            "target_lang_translations": {
                                "ru": "Опытный бриджист может спланировать весь <w>розыгрыш</w> уже после первого хода."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plan rozgrywki",
                        "błąd w rozgrywce"
                    ],
                    "target_lang_definitions": {
                        "ru": "В карточных играх (например, в бридже): фаза игры, следующая за торговлей, когда игроки выкладывают карты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "розыгрыш",
                                "target_lang_sense_clarification": "разыгрывание карт"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have a lot of <w>sympathy</w> for her situation because I went through the same thing last year."
                        },
                        {
                            "text": "He felt no <w>sympathy</w> for the criminal who had hurt so many innocent people."
                        }
                    ],
                    "synonyms": [
                        "pity",
                        "compassion",
                        "empathy",
                        "commiseration"
                    ],
                    "antonyms": [
                        "apathy"
                    ],
                    "common_phrases": [

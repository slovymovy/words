{
    "word_family": [
        "grouping",
        "subgroup",
        "supergroup",
        "regroup",
        "intergroup"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e4f6319c-88f6-3623-bb4a-05b0d696c52e",
                    "sense_definition": "A number of people or things that are located, gathered, or classed together.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>group</w> of students stood waiting for the bus.",
                            "target_lang_translations": {
                                "ru": "<w>Группа</w> студентов стояла в ожидании автобуса."
                            }
                        },
                        {
                            "text": "We need to work in a <w>group</w> to finish the project.",
                            "target_lang_translations": {
                                "ru": "Нам нужно работать в <w>группе</w>, чтобы закончить проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "collection",
                        "set"
                    ],
                    "antonyms": [
                        "individual"
                    ],
                    "common_phrases": [
                        "group of people",
                        "small group",
                        "large group",
                        "age group"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество людей или вещей, которые находятся, собраны или классифицированы вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гру́ппа",
                                "target_lang_sense_clarification": "совокупность людей или предметов"
                            },
                            {
                                "target_lang_word": "коллекти́в",
                                "target_lang_sense_clarification": "людей, объединенных общей деятельностью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e5af99b-cd1c-3829-a70d-61c3bb15087c",
                    "sense_definition": "A number of musicians who perform music together.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother plays the drums in a local rock <w>group</w>.",
                            "target_lang_translations": {
                                "ru": "Мой брат играет на барабанах в местной рок-<w>группе</w>."
                            }
                        },
                        {
                            "text": "The <w>group</w> released their first album last year.",
                            "target_lang_translations": {
                                "ru": "<w>Группа</w> выпустила свой первый альбом в прошлом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "band",
                        "ensemble"
                    ],
                    "common_phrases": [
                        "rock group",
                        "pop group",
                        "musical group"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несколько музыкантов, которые исполняют музыку вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гру́ппа",
                                "target_lang_sense_clarification": "музыкальная"
                            },
                            {
                                "target_lang_word": "анса́мбль",
                                "target_lang_sense_clarification": "музыкальный коллектив"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ab5f564-ba81-34c8-96e5-78fe7ec38fd1",
                    "sense_definition": "A set of teams playing each other in the same division of a competition.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our team was eliminated in the <w>group</w> stage of the tournament.",
                            "target_lang_translations": {
                                "ru": "Наша команда выбыла на <w>групповом</w> этапе турнира."
                            }
                        },
                        {
                            "text": "Brazil finished at the top of their <w>group</w>.",
                            "target_lang_translations": {
                                "ru": "Бразилия финишировала первой в своей <w>группе</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "group stage",
                        "win the group"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор команд, играющих друг с другом в одном дивизионе соревнования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гру́ппа",
                                "target_lang_sense_clarification": "турнирная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3d222e4-1b82-3d79-9b29-6be2bbb60360",
                    "sense_definition": "A commercial organization consisting of several companies under common ownership.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She works for a large insurance <w>group</w> in the city.",
                            "target_lang_translations": {
                                "ru": "Она работает в крупной страховой <w>группе</w> в городе."
                            }
                        },
                        {
                            "text": "The banking <w>group</w> announced record profits this quarter.",
                            "target_lang_translations": {
                                "ru": "Банковская <w>группа</w> объявила о рекордной прибыли в этом квартале."
                            }
                        }
                    ],
                    "common_phrases": [
                        "banking group",
                        "media group"
                    ],
                    "target_lang_definitions": {
                        "ru": "Коммерческая организация, состоящая из нескольких компаний под общим владением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гру́ппа",
                                "target_lang_sense_clarification": "компаний"
                            },
                            {
                                "target_lang_word": "конце́рн",
                                "target_lang_sense_clarification": "объединение предприятий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6756e3d-79ac-30c7-8156-ab8fe3831fff",
                    "sense_definition": "A number of users with the same rights regarding file access and system execution.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The administrator added the new user to the admin <w>group</w>.",
                            "target_lang_translations": {
                                "ru": "Администратор добавил нового пользователя в <w>группу</w> администраторов."
                            }
                        },
                        {
                            "text": "File permissions are set for the owner and the <w>group</w>.",
                            "target_lang_translations": {
                                "ru": "Права доступа к файлу установлены для владельца и <w>группы</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Несколько пользователей с одинаковыми правами доступа к файлам и выполнению системных действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гру́ппа",
                                "target_lang_sense_clarification": "пользователей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ff5fde6-d626-31d5-8ac3-5dfcf3eac978",
                    "sense_definition": "A set with a specific binary operation that satisfies certain algebraic conditions.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In algebra, a <w>group</w> must have an identity element.",
                            "target_lang_translations": {
                                "ru": "В алгебре <w>группа</w> должна иметь нейтральный элемент."
                            }
                        },
                        {
                            "text": "The set of integers forms a <w>group</w> under addition.",
                            "target_lang_translations": {
                                "ru": "Множество целых чисел образует <w>группу</w> по сложению."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множество с определенной бинарной операцией, удовлетворяющее определенным алгебраическим условиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гру́ппа",
                                "target_lang_sense_clarification": "алгебраическая"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0964a274-6027-3ea8-92ee-1846c4256083",
                    "sense_definition": "To put people or things into a group or category.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>group</w> these books by genre on the shelves.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>сгруппируйте</w> эти книги по жанрам на полках."
                            }
                        },
                        {
                            "text": "The teacher <w>grouped</w> the students into pairs for the exercise.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>сгруппировал</w> учеников по парам для выполнения упражнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "categorize",
                        "classify",
                        "collect",
                        "gather"
                    ],
                    "common_phrases": [
                        "group together",
                        "group by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединять людей или вещи в группу или категорию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "группирова́ть",
                                "target_lang_sense_clarification": "объединять"
                            },
                            {
                                "target_lang_word": "классифици́ровать",
                                "target_lang_sense_clarification": "распределять по категориям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b8410f7-8997-3507-b7ed-07a97fdbfe3f",
                    "sense_definition": "To come together to form a group.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The frightened sheep <w>grouped</w> together in the corner of the field.",
                            "target_lang_translations": {
                                "ru": "Испуганные овцы <w>сгруппировались</w> в углу поля."
                            }
                        },
                        {
                            "text": "Reporters <w>grouped</w> around the celebrity as she left the building.",
                            "target_lang_translations": {
                                "ru": "Репортеры <w>сгруппировались</w> вокруг знаменитости, когда она выходила из здания."
                            }
                        }
                    ],
                    "synonyms": [
                        "assemble"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собираться вместе, чтобы образовать группу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "группирова́ться",
                                "target_lang_sense_clarification": "собираться в группу"
                            },
                            {
                                "target_lang_word": "собира́ться",
                                "target_lang_sense_clarification": "вместе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
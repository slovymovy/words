            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "c1eee277-c971-3c1c-a830-bc6c0739defd",
                    "sense_definition": "Słowo łączące części wypowiedzi, z których druga wyjaśnia, uściśla lub podsumowuje pierwszą; wprowadza wniosek logiczny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "explanation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest wegetarianinem, <w>czyli</w> nie je mięsa.",
                            "target_lang_translations": {
                                "ru": "Мой брат вегетарианец, <w>то есть</w> он не ест мяса."
                            }
                        },
                        {
                            "text": "Sklep jest zamknięty, <w>czyli</w> musimy zrobić zakupy gdzie indziej.",
                            "target_lang_translations": {
                                "ru": "Магазин закрыт, <w>значит</w>, нам придется делать покупки в другом месте."
                            }
                        }
                    ],
                    "synonyms": [
                        "to znaczy",
                        "to jest",
                        "innymi słowy",
                        "inaczej mówiąc",
                        "a więc",
                        "więc",
                        "toteż",
                        "mianowicie",
                        "inaczej",
                        "jednym słowem"
                    ],
                    "common_phrases": [
                        "czyli że",
                        "czyli jak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, связывающий части высказывания, где вторая часть поясняет, уточняет или подытоживает первую; также вводит логический вывод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "то есть",
                                "target_lang_sense_clarification": "пояснение или уточнение"
                            },
                            {
                                "target_lang_word": "значит",
                                "target_lang_sense_clarification": "логический вывод"
                            },
                            {
                                "target_lang_word": "иными словами",
                                "target_lang_sense_clarification": "перефразирование"
                            },
                            {
                                "target_lang_word": "следовательно",
                                "target_lang_sense_clarification": "формальный вывод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00a653d0-fad5-3c99-8cea-4898dd58de6e",
                    "sense_definition": "Dawne słowo wprowadzające zdanie warunkowe.",

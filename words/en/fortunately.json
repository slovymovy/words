    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a1bc5d3d-96c3-386d-afbd-1ebeb4605eac",
                    "sense_definition": "Used to indicate that a particular event or fact is good, lucky, or desirable.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My mom has <w>fortunately</w> agreed to help us with the science project for school.",
                            "target_lang_translations": {
                                "ru": "Моя мама, <w>к счастью</w>, согласилась помочь нам со школьным научным проектом."
                            }
                        },
                        {
                            "text": "<w>Fortunately</w> for Sam, his pet hamster was found alive and well.",
                            "target_lang_translations": {
                                "ru": "<w>К счастью</w> для Сэма, его домашнего хомячка нашли живым и здоровым."
                            }
                        }
                    ],
                    "synonyms": [
                        "luckily",
                        "thankfully",
                        "happily",
                        "mercifully"
                    ],
                    "antonyms": [
                        "unfortunately"
                    ],
                    "common_phrases": [
                        "fortunately for us",
                        "fortunately enough",
                        "very fortunately"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на то, что определенное событие или факт является хорошим, удачным или желательным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к счастью",
                                "target_lang_sense_clarification": "вводное слово, выражающее радость по поводу чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8dd3bdb6-1961-36e1-8ccd-2f65bf4f93ac",
                    "sense_definition": "In a manner that produces a good result; by good luck.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The complex situation resolved <w>fortunately</w> without any further conflict.",
                            "target_lang_translations": {
                                "ru": "Сложная ситуация разрешилась <w>благополучно</w> без дальнейших конфликтов."
                            }
                        },
                        {
                            "text": "Everything turned out <w>fortunately</w> in the end despite our initial worries.",
                            "target_lang_translations": {
                                "ru": "В конце концов, несмотря на наши первоначальные опасения, всё сложилось <w>удачно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "luckily",
                        "auspiciously"
                    ],
                    "antonyms": [
                        "unfortunately"
                    ],
                    "common_phrases": [
                        "ended fortunately",
                        "turned out fortunately"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, который приводит к хорошему результату; по счастливой случайности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удачно",
                                "target_lang_sense_clarification": "успешно, с хорошим исходом"
                            },
                            {
                                "target_lang_word": "благополучно",
                                "target_lang_sense_clarification": "без бед и несчастий, с хорошим концом"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
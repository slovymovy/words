            "senses": [
                {
                    "sense_id": "b2267f52-9434-3b0b-a716-eab89bc3cac2",
                    "sense_definition": "Чувство долга, ответственности, способность правильно оценивать окружающую действительность и контролировать своё поведение.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель похвалил ученика за проявленную <сознательность> и честное признание своей вины.",
                            "target_lang_translations": {
                                "en": "The teacher praised the student for the <w>conscientiousness</w> shown and the honest admission of guilt."
                            }
                        },
                        {
                            "text": "Высокая гражданская <сознательность> побуждает людей активно участвовать в жизни своего района.",
                            "target_lang_translations": {
                                "en": "High civic <w>consciousness</w> encourages people to actively participate in the life of their neighborhood."
                            }
                        }
                    ],
                    "synonyms": [
                        "ответственность",
                        "добросовестность"
                    ],
                    "antonyms": [
                        "безответственность",
                        "несознательность",
                        "халатность"
                    ],
                    "common_phrases": [
                        "гражданская сознательность",
                        "проявить сознательность",
                        "высокая сознательность"
                    ],
                    "target_lang_definitions": {
                        "en": "A sense of duty, responsibility, and the ability to correctly evaluate reality and control one's behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conscientiousness",
                                "target_lang_sense_clarification": "trait of being careful, diligent, and guided by one's conscience"
                            },
                            {
                                "target_lang_word": "consciousness",
                                "target_lang_sense_clarification": "social or political awareness (e.g., civic consciousness)"
                            },
                            {
                                "target_lang_word": "sense of responsibility",
                                "target_lang_sense_clarification": "feeling of duty or obligation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2530cfa-ae44-3802-9511-4a441345b08d",
                    "sense_definition": "Намеренность, обдуманность и осмысленность совершаемых действий.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "deliberateness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В каждом его движении чувствовалась полная <сознательность> и уверенность в правильности выбора.",
                            "target_lang_translations": {
                                "en": "In his every movement, one could feel complete <w>deliberateness</w> and confidence in the correctness of the choice."
                            }
                        },
                        {
                            "text": "Суд принял во внимание <сознательность> действий обвиняемого при вынесении приговора.",
                            "target_lang_translations": {
                                "en": "The court took into account the <w>deliberateness</w> of the accused's actions when passing the sentence."
                            }
                        }
                    ],
                    "synonyms": [
                        "намеренность",
                        "обдуманность",
                        "осмысленность"
                    ],
                    "antonyms": [
                        "спонтанность",
                        "неосознанность",
                        "случайность"
                    ],
                    "common_phrases": [
                        "сознательность выбора",
                        "сознательность действий"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of being intentional, deliberate, and fully aware in one's actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deliberateness",
                                "target_lang_sense_clarification": "quality of being done intentionally and carefully"
                            },
                            {
                                "target_lang_word": "conscious nature",
                                "target_lang_sense_clarification": "aspect of being done with full awareness"
                            }
                        ]
                    }
                }
            ]
        }
    ]

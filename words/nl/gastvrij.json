                        ],
                        "ru": [
                            {
                                "target_lang_word": "гостеприимный",
                                "target_lang_sense_clarification": "готовый с радостью принять и угостить гостей"
                            },
                            {
                                "target_lang_word": "радушный",
                                "target_lang_sense_clarification": "сердечный и открытый в общении"
                            }

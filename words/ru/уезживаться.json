    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6d561266-855d-310f-a093-bbe935d00992",
                    "sense_definition": "Становиться ровным, плотным и гладким в результате частой езды по поверхности (о дороге, снеге).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Грунтовая дорога за лето <уезживается> машинами до твердости асфальта.",
                            "target_lang_translations": {
                                "en": "Over the summer, the dirt road <w>gets packed down</w> by cars to the hardness of asphalt."
                            }
                        },
                        {
                            "text": "Зимой лыжня в парке быстро <уезживается> многочисленными спортсменами.",
                            "target_lang_translations": {
                                "en": "In winter, the ski track in the park quickly <w>gets packed down</w> by numerous athletes."
                            }
                        }
                    ],
                    "synonyms": [
                        "укатываться",
                        "утаптываться"
                    ],
                    "antonyms": [
                        "разбиваться",
                        "рыхлиться"
                    ],
                    "common_phrases": [
                        "дорога уезживается",
                        "снег уезживается"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become smooth, dense, and even as a result of frequent driving or riding over the surface (about a road or snow)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get packed down",
                                "target_lang_sense_clarification": "about a road or snow surface"
                            },
                            {
                                "target_lang_word": "become hard-beaten",
                                "target_lang_sense_clarification": "about a path or track"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcc6bd8d-3c6d-320f-aa1d-957f79729555",
                    "sense_definition": "Приходить в состояние сильной усталости или изнеможения от длительной езды (о животных или людях).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К концу рабочего дня старая лошадь совсем <уезживалась> и еле передвигала ноги.",
                            "target_lang_translations": {
                                "en": "By the end of the workday, the old horse <w>got completely worn out</w> and could barely move its legs."
                            }
                        },
                        {
                            "text": "Водитель такси сильно <уезживался> за смену в городских пробках.",
                            "target_lang_translations": {
                                "en": "The taxi driver <w>got terribly exhausted</w> during his shift in city traffic jams."
                            }
                        }
                    ],
                    "synonyms": [
                        "утомляться",
                        "уставать",
                        "выматываться"
                    ],
                    "antonyms": [
                        "отдыхать"
                    ],
                    "common_phrases": [
                        "лошадь уезживается",
                        "сильно уезживаться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To reach a state of severe fatigue or exhaustion from long driving or riding (about animals or people)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get worn out",
                                "target_lang_sense_clarification": "from driving or riding"
                            },
                            {
                                "target_lang_word": "get exhausted",
                                "target_lang_sense_clarification": "from driving or riding"
                            }
                        ]
                    }
                }
            ]
        }
    ]

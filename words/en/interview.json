{
    "word_family": [
        "interviewer",
        "interviewee"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0217e42c-d5fa-3fbe-99d1-d6081c6f613f",
                    "sense_definition": "A formal meeting where someone is asked questions to determine if they are suitable for a job or course.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "job_assessment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a new suit to her job <w>interview</w>.",
                            "target_lang_translations": {
                                "ru": "Она надела новый костюм на <w>собеседование</w>."
                            }
                        },
                        {
                            "text": "The <w>interview</w> lasted for about an hour.",
                            "target_lang_translations": {
                                "ru": "<w>Собеседование</w> длилось около часа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "job interview",
                        "interview for a job",
                        "conduct an interview"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная встреча, на которой кандидату задают вопросы, чтобы определить, подходит ли он для работы или учебного курса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собесе́дование",
                                "target_lang_sense_clarification": "при приеме на работу или поступлении"
                            },
                            {
                                "target_lang_word": "интервью́",
                                "target_lang_sense_clarification": "калька с английского, часто используется в бизнесе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b16968bd-c9b9-3277-9294-2ece64a2cad4",
                    "sense_definition": "A conversation in which a journalist asks a person questions to record their answers for a news story or broadcast.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "media_conversation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actor gave a candid <w>interview</w> about his recent film.",
                            "target_lang_translations": {
                                "ru": "Актер дал откровенное <w>интервью</w> о своем недавнем фильме."
                            }
                        },
                        {
                            "text": "I read an interesting <w>interview</w> in the newspaper this morning.",
                            "target_lang_translations": {
                                "ru": "Я прочитал интересное <w>интервью</w> в газете сегодня утром."
                            }
                        }
                    ],
                    "common_phrases": [
                        "exclusive interview",
                        "give an interview",
                        "television interview"
                    ],
                    "target_lang_definitions": {
                        "ru": "Беседа, в ходе которой журналист задает вопросы человеку для записи ответов для новостного сюжета или передачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интервью́",
                                "target_lang_sense_clarification": "беседа с журналистом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28e864a4-d2f0-3217-b74e-0ab576526af7",
                    "sense_definition": "A formal questioning of a suspect or witness by the police.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "police_questioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The suspect remained silent during the police <w>interview</w>.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый хранил молчание во время полицейского <w>допроса</w>."
                            }
                        },
                        {
                            "text": "Detectives recorded the <w>interview</w> for evidence.",
                            "target_lang_translations": {
                                "ru": "Детективы записали <w>допрос</w> в качестве улики."
                            }
                        }
                    ],
                    "common_phrases": [
                        "police interview",
                        "interview room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный опрос подозреваемого или свидетеля полицией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "допро́с",
                                "target_lang_sense_clarification": "строгий, официальный (обычно подозреваемого)"
                            },
                            {
                                "target_lang_word": "опро́с",
                                "target_lang_sense_clarification": "сбор сведений (обычно свидетелей)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee5b125c-3164-3026-9cf0-7018971f30d0",
                    "sense_definition": "A formal face-to-face meeting between important people, such as monarchs or diplomats.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "formal_meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ambassador requested a private <w>interview</w> with the queen.",
                            "target_lang_translations": {
                                "ru": "Посол попросил о частной <w>аудиенции</w> у королевы."
                            }
                        },
                        {
                            "text": "Historical records describe the famous <w>interview</w> between the two kings.",
                            "target_lang_translations": {
                                "ru": "Исторические записи описывают знаменитую <w>встречу</w> двух королей."
                            }
                        }
                    ],
                    "synonyms": [
                        "audience"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "modern usage typically prefers 'meeting' or 'audience'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная личная встреча между важными персонами, например, монархами или дипломатами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аудие́нция",
                                "target_lang_sense_clarification": "официальный прием у высокопоставленного лица"
                            },
                            {
                                "target_lang_word": "встре́ча",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1c1801c5-fe71-3236-975a-a392a21da117",
                    "sense_definition": "To ask a person questions in a formal meeting for a job, news story, or investigation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_questioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>interviewed</w> three candidates for the manager position.",
                            "target_lang_translations": {
                                "ru": "Они <w>провели собеседование</w> с тремя кандидатами на должность менеджера."
                            }
                        },
                        {
                            "text": "A famous journalist <w>interviewed</w> the president yesterday.",
                            "target_lang_translations": {
                                "ru": "Известный журналист <w>взял интервью</w> у президента вчера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "interview a candidate",
                        "interview a suspect",
                        "interview live"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задавать вопросы человеку на официальной встрече для приема на работу, новостного сюжета или расследования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проводи́ть собесе́дование",
                                "target_lang_sense_clarification": "с кандидатом на работу"
                            },
                            {
                                "target_lang_word": "брать интервью́",
                                "target_lang_sense_clarification": "у знаменитости или для СМИ"
                            },
                            {
                                "target_lang_word": "интервьюи́ровать",
                                "target_lang_sense_clarification": "процесс опроса"
                            },
                            {
                                "target_lang_word": "опра́шивать",
                                "target_lang_sense_clarification": "собирать сведения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb8b5405-05b8-335f-9bd9-62c045194242",
                    "sense_definition": "To attend a formal meeting to be assessed for a job or position.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_attending",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>interviewed</w> for a job at the bank last week.",
                            "target_lang_translations": {
                                "ru": "На прошлой неделе он <w>проходил собеседование</w> на работу в банке."
                            }
                        },
                        {
                            "text": "She is currently <w>interviewing</w> with several tech companies.",
                            "target_lang_translations": {
                                "ru": "В данный момент она <w>проходит собеседования</w> в нескольких технологических компаниях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "interview for",
                        "interview with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Присутствовать на официальной встрече для оценки пригодности к работе или должности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проходи́ть собесе́дование",
                                "target_lang_sense_clarification": "быть в роли кандидата"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
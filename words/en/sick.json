{
    "word_family": [
        "sickness",
        "sicken",
        "sickly",
        "sickening",
        "sickbed"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8e251008-13d1-3f9e-8b45-3bf774361b9c",
                    "sense_definition": "In poor health; suffering from a disease or illness.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She felt too <w>sick</w> to go to work today.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала себя слишком <w>больной</w>, чтобы идти сегодня на работу."
                            }
                        },
                        {
                            "text": "The nurse worked hard to care for the <w>sick</w> patients.",
                            "target_lang_translations": {
                                "ru": "Медсестра усердно работала, ухаживая за <w>больными</w> пациентами."
                            }
                        }
                    ],
                    "synonyms": [
                        "ill",
                        "unwell",
                        "poorly",
                        "unhealthy"
                    ],
                    "antonyms": [
                        "healthy",
                        "well",
                        "fit"
                    ],
                    "common_phrases": [
                        "get sick",
                        "fall sick",
                        "call in sick",
                        "care for the sick"
                    ],
                    "target_lang_definitions": {
                        "ru": "В плохом состоянии здоровья; страдающий от болезни или недуга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "больной",
                                "target_lang_sense_clarification": "страдающий болезнью"
                            },
                            {
                                "target_lang_word": "нездоровый",
                                "target_lang_sense_clarification": "о состоянии организма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3d64b10-92a6-3879-a142-377b11e1f02e",
                    "sense_definition": "Having a strong urge to vomit.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I feel <w>sick</w> after eating that old seafood.",
                            "target_lang_translations": {
                                "ru": "Меня <w>тошнит</w> после того, как я съел те старые морепродукты."
                            }
                        },
                        {
                            "text": "The winding road made the child feel <w>sick</w> to his stomach.",
                            "target_lang_translations": {
                                "ru": "Из-за извилистой дороги ребенка <w>затошнило</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nauseated",
                        "queasy",
                        "sick to one's stomach"
                    ],
                    "antonyms": [
                        "well"
                    ],
                    "common_phrases": [
                        "feel sick",
                        "be sick",
                        "sick to my stomach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий сильный позыв к рвоте; чувствующий тошноту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тошнить",
                                "target_lang_sense_clarification": "безличный глагол (меня тошнит)"
                            },
                            {
                                "target_lang_word": "дурно",
                                "target_lang_sense_clarification": "наречие (мне дурно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6769055-6d1c-33c6-9bf1-a57757025b7f",
                    "sense_definition": "Thoroughly bored with or annoyed by something or someone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>sick</w> of hearing the same excuses every day.",
                            "target_lang_translations": {
                                "ru": "Мне <w>надоело</w> слышать одни и те же оправдания каждый день."
                            }
                        },
                        {
                            "text": "She is <w>sick</w> and tired of the constant rain.",
                            "target_lang_translations": {
                                "ru": "Ей до смерти <w>надоел</w> постоянный дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "fed up",
                        "tired"
                    ],
                    "common_phrases": [
                        "sick of",
                        "sick and tired",
                        "make me sick"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно уставший от чего-либо или кого-либо; испытывающий раздражение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надоесть",
                                "target_lang_sense_clarification": "глагол (мне надоело)"
                            },
                            {
                                "target_lang_word": "достать",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94c74826-e70c-373d-8ec8-6f9c82ae6d5e",
                    "sense_definition": "Mentally unstable, deeply disturbed, or morbid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Only a <w>sick</w> person would hurt a defenseless animal.",
                            "target_lang_translations": {
                                "ru": "Только <w>больной</w> человек мог бы обидеть беззащитное животное."
                            }
                        },
                        {
                            "text": "He has a <w>sick</w> obsession with violent movies.",
                            "target_lang_translations": {
                                "ru": "У него <w>больная</w> одержимость жестокими фильмами."
                            }
                        }
                    ],
                    "synonyms": [
                        "disturbed",
                        "twisted",
                        "warped"
                    ],
                    "common_phrases": [
                        "sick mind",
                        "sick person"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психически неуравновешенный, глубоко расстроенный или ненормальный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "больной",
                                "target_lang_sense_clarification": "психически"
                            },
                            {
                                "target_lang_word": "ненормальный",
                                "target_lang_sense_clarification": "о психике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4c0ae61-2f74-39d9-aca0-3d3edcda77c6",
                    "sense_definition": "Cruel, offensive, or shocking in a way that causes disgust.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That was a really <w>sick</w> joke to make at a funeral.",
                            "target_lang_translations": {
                                "ru": "Это была действительно <w>мерзкая</w> шутка для похорон."
                            }
                        },
                        {
                            "text": "I find his sense of humor quite <w>sick</w> and offensive.",
                            "target_lang_translations": {
                                "ru": "Я нахожу его чувство юмора довольно <w>извращённым</w> и оскорбительным."
                            }
                        }
                    ],
                    "synonyms": [
                        "twisted",
                        "morbid"
                    ],
                    "common_phrases": [
                        "sick joke",
                        "sick humor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жестокий, оскорбительный или шокирующий, вызывающий отвращение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извращённый",
                                "target_lang_sense_clarification": "о юморе или поступках"
                            },
                            {
                                "target_lang_word": "мерзкий",
                                "target_lang_sense_clarification": "отвратительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7ed8905-c0f5-306c-8853-372639a05040",
                    "sense_definition": "Very good, impressive, or excellent.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "slang_positive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your new skateboard tricks are absolutely <w>sick</w>!",
                            "target_lang_translations": {
                                "ru": "Твои новые трюки на скейтборде просто <w>чумовые</w>!"
                            }
                        },
                        {
                            "text": "We went to a <w>sick</w> party last night with great music.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы были на <w>крутой</w> вечеринке с отличной музыкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "awesome",
                        "cool",
                        "wicked",
                        "rad",
                        "dope"
                    ],
                    "antonyms": [
                        "uncool",
                        "crap",
                        "naff"
                    ],
                    "common_phrases": [
                        "that is sick",
                        "sick skills"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used by younger speakers"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень хороший, впечатляющий или отличный (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крутой",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "чумовой",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "34a1b468-3d12-3910-84c8-37c576fcad5d",
                    "sense_definition": "Partially digested food that has been vomited.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cleaner had to mop up a pile of <w>sick</w>.",
                            "target_lang_translations": {
                                "ru": "Уборщику пришлось вытирать кучу <w>рвоты</w>."
                            }
                        },
                        {
                            "text": "The smell of <w>sick</w> in the room was overwhelming.",
                            "target_lang_translations": {
                                "ru": "Запах <w>рвоты</w> в комнате был невыносимым."
                            }
                        }
                    ],
                    "synonyms": [
                        "vomit"
                    ],
                    "common_phrases": [

{
    "word_family": [
        "łomot",
        "łomotnąć",
        "załomotać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "187ade49-8c99-38ca-89a6-d7cf21d79b86",
                    "sense_definition": "Uderzać w coś wielokrotnie i z dużą siłą, powodując głośny, głuchy hałas.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ktoś zaczął głośno <w>łomotać</w> do drzwi w środku nocy, budząc wszystkich domowników.",
                            "target_lang_translations": {
                                "ru": "Кто-то начал громко <w>колотить</w> в дверь посреди ночи, разбудив всех домочадцев."
                            }
                        },
                        {
                            "text": "Silny wiatr sprawiał, że okiennice zaczęły <w>łomotać</w> o ściany budynku.",
                            "target_lang_translations": {
                                "ru": "Из-за сильного ветра ставни начали <w>стучать</w> о стены здания."
                            }
                        }
                    ],
                    "synonyms": [
                        "walić",
                        "tłuc",
                        "grzmocić",
                        "gruchotać"
                    ],
                    "common_phrases": [
                        "łomotać do drzwi",
                        "łomotać w szybę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бить во что-то многократно и с большой силой, производя громкий, глухой шум."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колотить",
                                "target_lang_sense_clarification": "сильно стучать"
                            },
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "производить шум ударами"
                            },
                            {
                                "target_lang_word": "грохотать",
                                "target_lang_sense_clarification": "издавать громкий шум при ударах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f2c1c3a-5455-3180-9077-fd48fd7b39e8",
                    "sense_definition": "O sercu: bić bardzo mocno i szybko, zazwyczaj z powodu silnych emocji lub wysiłku fizycznego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "heart_beat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Serce <w>łomotało</w> mi w piersi ze strachu, gdy usłyszałem dziwne kroki na schodach.",
                            "target_lang_translations": {
                                "ru": "Сердце <w>колотилось</w> у меня в груди от страха, когда я услышал странные шаги на лестнице."
                            }
                        },
                        {
                            "text": "Po przebiegnięciu maratonu czułem, jak krew <w>łomocze</w> mi w skroniach.",
                            "target_lang_translations": {
                                "ru": "После марафона я чувствовал, как кровь <w>стучит</w> у меня в висках."
                            }
                        }
                    ],
                    "synonyms": [
                        "walić",
                        "tłuc"
                    ],
                    "common_phrases": [
                        "serce łomocze"
                    ],
                    "target_lang_definitions": {
                        "ru": "О сердце: биться очень сильно и быстро, обычно из-за сильных эмоций или физического напряжения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колотиться",
                                "target_lang_sense_clarification": "о сердце: сильно биться"
                            },
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "о пульсе или сердце"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da0da293-54de-37a5-8869-85078ac1f583",
                    "sense_definition": "Bić kogoś mocno i gwałtownie; zadawać komuś uderzenia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych czasach zdarzało się, że rozbójnicy mogli kogoś <w>łomotać</w> kijami na drodze.",
                            "target_lang_translations": {
                                "ru": "В былые времена случалось, что разбойники могли <w>дубасить</w> кого-то палками на дороге."
                            }
                        },
                        {
                            "text": "Nie wolno nikogo <w>łomotać</w>, przemoc fizyczna jest karalna.",
                            "target_lang_translations": {
                                "ru": "Нельзя никого <w>колотить</w>, физическое насилие наказуемо."
                            }
                        }
                    ],
                    "synonyms": [
                        "grzmocić",
                        "tłuc",
                        "walić"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бить кого-то сильно и яростно; наносить удары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дубасить",
                                "target_lang_sense_clarification": "разг. сильно бить"
                            },
                            {
                                "target_lang_word": "колотить",
                                "target_lang_sense_clarification": "наносить удары"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
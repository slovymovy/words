    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "659bf6eb-f7f7-3eff-ab06-83113e288a32",
                    "sense_definition": "Present participle and gerund of <w>expect</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are <w>expecting</w> a lot of rain this weekend.",
                            "target_lang_translations": {
                                "ru": "Мы <w>ожидаем</w> много дождей в эти выходные."
                            }
                        },
                        {
                            "text": "She is <w>expecting</w> a phone call from her manager.",
                            "target_lang_translations": {
                                "ru": "Она <w>ждет</w> телефонного звонка от своего менеджера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "expecting company",
                        "expecting trouble",
                        "expecting a call"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма причастия настоящего времени и герундия от глагола \"expect\" (ожидать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ожидать",
                                "target_lang_sense_clarification": "рассчитывать на то, что что-то произойдет"
                            },
                            {
                                "target_lang_word": "ждать",
                                "target_lang_sense_clarification": "находиться в состоянии ожидания"
                            }

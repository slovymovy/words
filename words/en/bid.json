{
    "word_family": [
        "bidder",
        "bidding",
        "outbid",
        "underbid",
        "overbid",
        "rebid"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c23234f6-4883-39a0-82ba-b4745815ce12",
                    "sense_definition": "To offer to pay a particular amount of money for something, especially at an auction.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "auction_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>bid</w> £2000 for the antique Persian carpet.",
                            "target_lang_translations": {
                                "ru": "Она <w>предложила</w> 2000 фунтов за старинный персидский ковер."
                            }
                        },
                        {
                            "text": "Several international companies <w>bid</w> for the lucrative construction contract.",
                            "target_lang_translations": {
                                "ru": "Несколько международных компаний <w>подали заявки</w> на выгодный контракт на строительство."
                            }
                        }
                    ],
                    "synonyms": [
                        "bit",
                        "bit"
                    ],
                    "common_phrases": [
                        "bid for",
                        "bid on",
                        "bid against"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлагать определенную сумму денег за что-либо, особенно на аукционе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предлагать цену",
                                "target_lang_sense_clarification": "на аукционе"
                            },
                            {
                                "target_lang_word": "делать ставку",
                                "target_lang_sense_clarification": "на аукционе"
                            },
                            {
                                "target_lang_word": "подавать заявку",
                                "target_lang_sense_clarification": "на участие в тендере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b71f43e-e78b-3bd1-84b9-1d9445ca1dcf",
                    "sense_definition": "To make a serious attempt to achieve or obtain something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "attempt_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was <w>bidding</w> for the chance to coach the national team.",
                            "target_lang_translations": {
                                "ru": "Он <w>боролся</w> за шанс тренировать национальную сборную."
                            }
                        },
                        {
                            "text": "The senator is <w>bidding</w> to retain his seat in the upcoming election.",
                            "target_lang_translations": {
                                "ru": "Сенатор <w>пытается</w> сохранить свое место на предстоящих выборах."
                            }
                        }
                    ],
                    "synonyms": [
                        "bit",
                        "bit"
                    ],
                    "common_phrases": [
                        "bid for power",
                        "bid to win"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предпринимать серьезную попытку достичь или получить что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бороться",
                                "target_lang_sense_clarification": "за что-либо"
                            },
                            {
                                "target_lang_word": "претендовать",
                                "target_lang_sense_clarification": "на что-либо"
                            },
                            {
                                "target_lang_word": "пытаться",
                                "target_lang_sense_clarification": "достичь цели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da2827ad-1260-38cd-bb4b-099912b7c728",
                    "sense_definition": "To say a greeting or farewell to someone.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>bid</w> our guests a fond farewell at the door.",
                            "target_lang_translations": {
                                "ru": "Мы тепло <w>попрощались</w> с гостями у двери."
                            }
                        },
                        {
                            "text": "The retiring captain <w>bade</w> farewell to his crew for the last time.",
                            "target_lang_translations": {
                                "ru": "Уходящий в отставку капитан в последний раз <w>попрощался</w> со своим экипажем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bid farewell",
                        "bid goodnight",
                        "bid welcome"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Произносить приветствие или прощание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "доброй ночи, удачи"
                            },
                            {
                                "target_lang_word": "прощаться",
                                "target_lang_sense_clarification": "говорить прощай"
                            },
                            {
                                "target_lang_word": "приветствовать",
                                "target_lang_sense_clarification": "говорить привет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb108d6f-697c-377b-b160-07432ee15f44",
                    "sense_definition": "To announce how many tricks a player proposes to win in a card game.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My partner <w>bid</w> three hearts, so I knew he had a strong hand.",
                            "target_lang_translations": {
                                "ru": "Мой партнер <w>объявил</w> три черви, поэтому я знал, что у него сильная рука."
                            }
                        },
                        {
                            "text": "You should not <w>bid</w> if you do not have enough high cards.",
                            "target_lang_translations": {
                                "ru": "Вам не следует <w>делать заявку</w>, если у вас недостаточно старших карт."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Объявлять количество взяток, которое игрок намерен выиграть в карточной игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объявлять",
                                "target_lang_sense_clarification": "в карточной игре"
                            },
                            {
                                "target_lang_word": "делать заявку",
                                "target_lang_sense_clarification": "в бридже или преферансе"
                            },
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "масть или игру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25308897-b97a-3255-8f5b-a00f411b9f08",
                    "sense_definition": "To order or command someone to do something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king <w>bade</w> the knight to rise and speak.",
                            "target_lang_translations": {
                                "ru": "Король <w>велел</w> рыцарю встать и говорить."
                            }
                        },
                        {
                            "text": "She did exactly as she was <w>bidden</w> by her employer.",
                            "target_lang_translations": {
                                "ru": "Она сделала именно так, как ей было <w>велено</w> работодателем."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приказывать или велеть кому-либо сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велеть",
                                "target_lang_sense_clarification": "устаревшее или литературное"
                            },
                            {
                                "target_lang_word": "приказывать",
                                "target_lang_sense_clarification": "отдавать распоряжение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "edc82750-cbdc-3db8-9856-55a603e939b1",
                    "sense_definition": "An offer to pay a particular price for something, especially at an auction.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "auction_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company submitted a <w>bid</w> for the new transport contract.",
                            "target_lang_translations": {
                                "ru": "Компания подала <w>заявку</w> на новый транспортный контракт."
                            }
                        },
                        {
                            "text": "His opening <w>bid</w> was far lower than the item's value.",
                            "target_lang_translations": {
                                "ru": "Его начальная <w>ставка</w> была намного ниже стоимости предмета."
                            }
                        }
                    ],
                    "synonyms": [
                        "bit",
                        "bit"
                    ],
                    "common_phrases": [
                        "make a bid",
                        "accept a bid",
                        "winning bid",
                        "highest bid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предложение определенной цены за что-либо, особенно на аукционе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставка",
                                "target_lang_sense_clarification": "на аукционе"
                            },
                            {
                                "target_lang_word": "заявка",
                                "target_lang_sense_clarification": "на торгах или тендере"
                            },
                            {
                                "target_lang_word": "предложение цены",
                                "target_lang_sense_clarification": "коммерческое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d8fce2e-139d-35ff-96a2-136c472dda9a",
                    "sense_definition": "An attempt or effort to achieve a goal.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "attempt_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <w>bid</w> for the presidency gained support rapidly.",
                            "target_lang_translations": {
                                "ru": "Ее <w>заявка</w> на пост президента быстро набирала поддержку."
                            }
                        },
                        {
                            "text": "The team made a desperate <w>bid</w> for victory in the final minutes.",
                            "target_lang_translations": {
                                "ru": "Команда предприняла отчаянную <w>попытку</w> победить на последних минутах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bid for power",
                        "bid for freedom",
                        "takeover bid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Попытка или усилие для достижения цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попытка",
                                "target_lang_sense_clarification": "добиться чего-либо"
                            },
                            {
                                "target_lang_word": "заявка",
                                "target_lang_sense_clarification": "претензия на успех"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3f41a8a-28a6-3833-a9cd-425e0fa023aa",
                    "sense_definition": "A period of time spent in prison.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "prison_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He did a five-year <w>bid</w> for robbery before being released.",
                            "target_lang_translations": {
                                "ru": "Он отсидел пятилетний <w>срок</w> за ограбление перед освобождением."
                            }
                        },
                        {
                            "text": "They talked about life inside during his last <w>bid</w>.",
                            "target_lang_translations": {
                                "ru": "Они говорили о жизни внутри во время его последнего <w>срока</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bit"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени, проведенный в тюрьме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срок",
                                "target_lang_sense_clarification": "тюремное заключение"
                            },
                            {
                                "target_lang_word": "отсидка",
                                "target_lang_sense_clarification": "жаргон"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "505cdbb8-71ab-3f39-97f4-b9a08ddf394c",
                    "sense_definition": "Twice a day; used in medical prescriptions.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "medical_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor changed the prescription from once daily to <w>bid</w>.",
                            "target_lang_translations": {
                                "ru": "Врач изменил рецепт с одного раза в день на <w>дважды в день</w>."
                            }
                        },
                        {
                            "text": "Patients often find a <w>bid</w> dosing schedule harder to follow than a daily one.",
                            "target_lang_translations": {
                                "ru": "Пациенты часто находят график приема <w>дважды в день</w> более сложным для соблюдения, чем ежедневный."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "medical technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Два раза в день (используется в медицинских рецептах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дважды в день",
                                "target_lang_sense_clarification": "медицинское предписание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
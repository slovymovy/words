    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b1a89a03-a1b0-3450-bec1-5ab5c3a5cb03",
                    "sense_definition": "Действие по значению глагола <w>благословлять</w>; крестное знамение или слова молитвы с просьбой о покровительстве Бога.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Священник прочитал молитву и дал прихожанам своё <w>благословение</w>.",
                            "target_lang_translations": {
                                "en": "The priest read a prayer and gave the parishioners his <w>blessing</w>."
                            }
                        },
                        {
                            "text": "Перед долгой дорогой сын попросил у матери <w>благословение</w>.",
                            "target_lang_translations": {
                                "en": "Before the long journey, the son asked his mother for her <w>blessing</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "проклятие"
                    ],
                    "common_phrases": [
                        "дать благословение",
                        "получить благословение",
                        "просить благословения",
                        "божье благословение"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of asking for God's protection or help, or the words used to do this."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blessing",
                                "target_lang_sense_clarification": "divine protection or prayer"
                            },
                            {
                                "target_lang_word": "benediction",
                                "target_lang_sense_clarification": "formal prayer asking for God's help"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d192e56-ace5-36de-b3e9-2bd1c515730c",
                    "sense_definition": "Разрешение, согласие или одобрение на совершение какого-либо действия.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник дал <w>благословение</w> на запуск нового сложного проекта.",
                            "target_lang_translations": {
                                "en": "The boss gave his <w>blessing</w> for the launch of the new complex project."
                            }
                        },
                        {
                            "text": "Мы не можем начать строительство без официального <w>благословения</w> властей.",
                            "target_lang_translations": {
                                "en": "We cannot start construction without the official <w>blessing</w> of the authorities."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрешение",
                        "одобрение",
                        "согласие"
                    ],
                    "common_phrases": [
                        "родительское благословение",
                        "дать благословение"
                    ],
                    "target_lang_definitions": {
                        "en": "Permission, agreement, or approval to perform an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blessing",
                                "target_lang_sense_clarification": "approval or permission"
                            },
                            {
                                "target_lang_word": "approval",
                                "target_lang_sense_clarification": "formal consent"
                            }
                        ]
                    }
                }
            ]

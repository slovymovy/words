{
    "word_family": [
        "fabricate",
        "fabrication",
        "prefabricate",
        "prefabrication",
        "fabricator"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c38d50b4-c91e-3cf2-b7cc-e980b64bbe48",
                    "sense_definition": "A material made of fibers, such as cotton, wool, or silk, used for making clothes and other items.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material_textile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought some blue cotton <w>fabric</w> to make a summer dress.",
                            "target_lang_translations": {
                                "ru": "Она купила синюю хлопковую <w>ткань</w>, чтобы сшить летнее платье."
                            }
                        },
                        {
                            "text": "This synthetic <w>fabric</w> is very durable and easy to wash.",
                            "target_lang_translations": {
                                "ru": "Эта синтетическая <w>ткань</w> очень прочная и легко стирается."
                            }
                        }
                    ],
                    "synonyms": [
                        "cloth",
                        "textile"
                    ],
                    "common_phrases": [
                        "cotton fabric",
                        "synthetic fabric",
                        "woven fabric",
                        "soft fabric"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материал, изготовленный из волокон (хлопка, шерсти или шелка), используемый для пошива одежды и других изделий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ткань",
                                "target_lang_sense_clarification": "текстильный материал"
                            },
                            {
                                "target_lang_word": "материал",
                                "target_lang_sense_clarification": "разг. ткань для шитья"
                            },
                            {
                                "target_lang_word": "материя",
                                "target_lang_sense_clarification": "разг. ткань"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40836b23-552a-3a4c-8f3c-0d4cc05fb3cf",
                    "sense_definition": "The basic structure or framework that holds something together, often used metaphorically regarding society or organizations.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "structure_framework",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trust and cooperation are essential parts of the social <w>fabric</w>.",
                            "target_lang_translations": {
                                "ru": "Доверие и сотрудничество — неотъемлемые части социальной <w>структуры</w>."
                            }
                        },
                        {
                            "text": "The war severely damaged the moral <w>fabric</w> of the nation.",
                            "target_lang_translations": {
                                "ru": "Война нанесла серьезный ущерб моральным <w>устоям</w> нации."
                            }
                        }
                    ],
                    "synonyms": [
                        "cloth",
                        "textile"
                    ],
                    "common_phrases": [
                        "social fabric",
                        "fabric of society",
                        "moral fabric",
                        "fabric of life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Базовая структура или основа, скрепляющая что-либо (часто используется метафорически в отношении общества или организаций)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "структура",
                                "target_lang_sense_clarification": "внутреннее устройство"
                            },
                            {
                                "target_lang_word": "основа",
                                "target_lang_sense_clarification": "фундамент, база"
                            },
                            {
                                "target_lang_word": "устройство",
                                "target_lang_sense_clarification": "организация, порядок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "984e38c2-c5a4-324e-bf93-4971955ac21e",
                    "sense_definition": "The physical walls, roof, and floors of a building or structure, especially regarding its maintenance or history.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "structure_framework",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>fabric</w> of the ancient church requires urgent repair work.",
                            "target_lang_translations": {
                                "ru": "<w>Стены</w> древней церкви требуют срочного ремонта."
                            }
                        },
                        {
                            "text": "Architects worked carefully to preserve the original <w>fabric</w> of the historic building.",
                            "target_lang_translations": {
                                "ru": "Архитекторы тщательно работали над сохранением оригинальной <w>конструкции</w> исторического здания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "building fabric",
                        "historic fabric"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физические стены, крыша и перекрытия здания, особенно в контексте его сохранения или истории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конструкция",
                                "target_lang_sense_clarification": "строение здания"
                            },
                            {
                                "target_lang_word": "остов",
                                "target_lang_sense_clarification": "каркас сооружения"
                            },
                            {
                                "target_lang_word": "стены",
                                "target_lang_sense_clarification": "физическая оболочка здания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
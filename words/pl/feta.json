                    "sense_id": "2b073a46-52f2-3fe8-8192-b04ebec848dc",
                    "sense_definition": "Rodzaj greckiego sera (zwykle owczego) o słonym smaku.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lubię grecką sałatkę z <w>fetą</w> i czarnymi oliwkami.",
                            "target_lang_translations": {
                                "ru": "Я люблю греческий салат с <w>фетой</w> и черными маслинами."
                            }
                        },
                        {
                            "text": "Do przygotowania tego dania potrzebna jest oryginalna <w>feta</w> z mleka owczego.",
                            "target_lang_translations": {
                                "ru": "Для приготовления этого блюда нужна настоящая <w>фета</w> из овечьего молока."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sałatka z fetą",
                        "ser feta",
                        "kostki fety"
                    ],
                    "target_lang_definitions": {

                                            {
                                                "example_id": "b1bf9404-ab03-43a4-8335-57e694d34ecc",
                                                "sense_id": "c18b3c7a-eaa4-3da6-89d0-a7c595eba47d",
                                                "text": "Es war eine sehr klare Botschaft, die da aus Rom kam."
                                            },

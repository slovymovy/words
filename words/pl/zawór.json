            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c6318b09-03f5-38b2-8cb9-cb8c4d52e1e4",
                    "sense_definition": "Urządzenie mechaniczne służące do zamykania, otwierania lub regulowania przepływu płynów lub gazów w przewodach.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mechanics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hydraulik musiał wymienić uszkodzony <w>zawór</w> pod zlewem w kuchni.",
                            "target_lang_translations": {
                                "ru": "Сантехнику пришлось заменить поврежденный <w>вентиль</w> под раковиной на кухне."
                            }
                        },
                        {
                            "text": "Pamiętaj, aby zakręcić główny <w>zawór</w> wody przed dłuższym wyjazdem.",
                            "target_lang_translations": {
                                "ru": "Не забудь перекрыть главный <w>вентиль</w> воды перед длительным отъездом."
                            }
                        }
                    ],
                    "synonyms": [
                        "kurek",
                        "wentyl"
                    ],
                    "common_phrases": [
                        "zawór bezpieczeństwa",
                        "zawór zwrotny",
                        "główny zawór",
                        "zakręcić zawór"
                    ],
                    "target_lang_definitions": {
                        "ru": "Механическое устройство, служащее для закрытия, открытия или регулирования потока жидкостей или газов в трубопроводах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клапан",
                                "target_lang_sense_clarification": "механизм для регулирования потока"
                            },
                            {
                                "target_lang_word": "вентиль",
                                "target_lang_sense_clarification": "запорное устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7dc2e15-c216-3a59-829a-5689f92f53d7",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>zawora</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starej bramie brakowało solidnych <w>zawór</w>, więc łatwo było ją otworzyć.",
                            "target_lang_translations": {
                                "ru": "В старых воротах не хватало надежных <w>засовов</w>, поэтому их было легко открыть."
                            }
                        },
                        {
                            "text": "Konstrukcja tych drewnianych <w>zawór</w> była bardzo przestarzała i wymagała naprawy.",
                            "target_lang_translations": {
                                "ru": "Конструкция этих деревянных <w>засовов</w> была очень устаревшей и требовала ремонта."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного <w>zawora</w> (засов, запор)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засов",

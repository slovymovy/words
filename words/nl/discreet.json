            "senses": [
                {
                    "sense_id": "06dd3d57-e0ba-3343-b1fe-9d4ed6da723f",
                    "sense_definition": "Zonder de aandacht te trekken; onopvallend en tactvol.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ober gaf ons op <w>discrete</w> wijze de rekening zonder ons gesprek te storen.",
                            "target_lang_translations": {
                                "ru": "Официант <w>тактично</w> передал нам счет, не мешая нашему разговору."
                            }
                        },
                        {
                            "text": "Zij is altijd heel <w>discreet</w> als het gaat om privékwesties van haar vrienden.",
                            "target_lang_translations": {
                                "ru": "Она всегда очень <w>сдержанна</w>, когда речь идет о личных делах ее друзей."
                            }
                        }
                    ],
                    "synonyms": [
                        "onopvallend"
                    ],
                    "antonyms": [
                        "indiscreet"
                    ],
                    "common_phrases": [
                        "op discrete wijze",
                        "heel discreet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не привлекающий внимания; незаметный и тактичный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдержанный",
                                "target_lang_sense_clarification": "умеющий вести себя осторожно, не болтливый"
                            },
                            {
                                "target_lang_word": "тактичный",
                                "target_lang_sense_clarification": "обладающий чувством меры, не навязчивый"
                            },
                            {
                                "target_lang_word": "деликатный",
                                "target_lang_sense_clarification": "вежливый, мягкий в обращении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6156dd05-013b-33df-857b-a3dd94eb54e9",
                    "sense_definition": "Los van elkaar staand; niet aaneensluitend (vooral gebruikt in de wiskunde en wetenschap).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de <w>discrete</w> wiskunde werken we met gehele getallen in plaats van continue lijnen.",
                            "target_lang_translations": {
                                "ru": "В <w>дискретной</w> математике мы работаем с целыми числами вместо непрерывных линий."
                            }
                        },
                        {
                            "text": "Het onderzoek analyseert <w>discrete</w> variabelen die niet met elkaar verbonden zijn.",
                            "target_lang_translations": {
                                "ru": "В исследовании анализируются <w>дискретные</w> переменные, которые не связаны друг с другом."
                            }
                        }
                    ],
                    "antonyms": [
                        "continu"
                    ],
                    "common_phrases": [
                        "discrete wiskunde",
                        "discrete variabelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий из отдельных частей; не непрерывный (используется преимущественно в математике и науке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дискретный",
                                "target_lang_sense_clarification": "прерывистый, раздельный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

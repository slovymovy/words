    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f8ec7eb7-327e-3031-adfa-d05c841c2344",
                    "sense_definition": "Существовать, иметься в наличии, присутствовать где-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем городе <есть> большой и красивый парк.",
                            "target_lang_translations": {
                                "en": "In our city <there is> a big and beautiful park."
                            }
                        },
                        {
                            "text": "Скажи честно, <есть> ли у нас шанс победить?",
                            "target_lang_translations": {
                                "en": "Tell me honestly, <is there> a chance for us to win?"
                            }
                        }
                    ],
                    "synonyms": [
                        "употреблять",
                        "питаться"
                    ],
                    "antonyms": [
                        "нет"
                    ],
                    "common_phrases": [
                        "есть ли",
                        "то, что есть"
                    ],
                    "target_lang_definitions": {
                        "en": "To exist, to be available, to be present somewhere."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "there is",
                                "target_lang_sense_clarification": "existence"
                            },
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "exist"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e932b63-535b-3a50-91fc-db084cc677f4",
                    "sense_definition": "Используется в конструкции «у кого-то есть» для обозначения владения чем-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня <есть> интересная идея для нового проекта.",
                            "target_lang_translations": {
                                "en": "I <have> an interesting idea for a new project."
                            }
                        },
                        {
                            "text": "У него <есть> всё необходимое для счастливой жизни.",
                            "target_lang_translations": {
                                "en": "He <has> everything necessary for a happy life."
                            }
                        }
                    ],
                    "antonyms": [
                        "нет"
                    ],
                    "common_phrases": [
                        "у меня есть",
                        "у тебя есть"
                    ],
                    "target_lang_definitions": {
                        "en": "Used in the construction \"someone has\" to denote possession."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "possession"
                            },
                            {
                                "target_lang_word": "have got",
                                "target_lang_sense_clarification": "possession"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d19de39e-b470-3d84-99fc-75c85440f684",
                    "sense_definition": "Принимать пищу, глотать и пережёвывать еду.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "eating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я очень хочу <есть>, давай пойдём в кафе.",
                            "target_lang_translations": {
                                "en": "I really want to <eat>, let's go to a cafe."
                            }
                        },
                        {
                            "text": "По утрам он обычно <ест> овсяную кашу с фруктами.",
                            "target_lang_translations": {
                                "en": "In the mornings he usually <eats> oatmeal with fruit."
                            }
                        }
                    ],
                    "synonyms": [
                        "питаться",
                        "употреблять"
                    ],
                    "antonyms": [
                        "голодать"
                    ],
                    "common_phrases": [
                        "хотеть есть",
                        "есть суп",
                        "много есть"
                    ],
                    "target_lang_definitions": {
                        "en": "To consume food, swallow and chew it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eat",
                                "target_lang_sense_clarification": "consume food"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c3455a5-e985-311b-805e-245d34173dc5",
                    "sense_definition": "Являться, представлять собой (связка в составном сказуемом, часто опускается в настоящем времени, но используется для акцента или в определениях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Совесть <есть> внутренний судья каждого человека.",
                            "target_lang_translations": {
                                "en": "Conscience <is> the inner judge of every person."
                            }
                        },
                        {
                            "text": "Мы должны принимать жизнь такой, какая она <есть>.",
                            "target_lang_translations": {
                                "en": "We must accept life as it <is>."
                            }
                        }
                    ],
                    "synonyms": [
                        "е"
                    ],
                    "common_phrases": [
                        "то есть",
                        "как есть",
                        "кто есть кто"
                    ],
                    "target_lang_definitions": {
                        "en": "To be, to constitute (copula in a compound predicate, often omitted in the present tense but used for emphasis or in definitions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "is",
                                "target_lang_sense_clarification": "copula"
                            },
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "copula"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "769a8636-6479-3f84-bd9e-a8ebb8e22171",
                    "sense_definition": "Вызывать физическое раздражение, жжение или резь (обычно о дыме, мыле или луке, раздражающих глаза).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "irritation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Густой едкий дым от костра <ел> глаза и мешал смотреть.",
                            "target_lang_translations": {
                                "en": "Thick acrid smoke from the bonfire <stung> my eyes and made it hard to see."
                            }
                        },
                        {
                            "text": "Когда режешь лук, он часто <ест> глаза до слёз.",
                            "target_lang_translations": {
                                "en": "When you cut onions, it often <stings> your eyes to tears."
                            }
                        }
                    ],
                    "common_phrases": [
                        "есть глаза"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause physical irritation, burning, or stinging (usually about smoke, soap, or onions irritating the eyes)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sting",
                                "target_lang_sense_clarification": "eyes"
                            },
                            {
                                "target_lang_word": "burn",
                                "target_lang_sense_clarification": "irritate"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53d5c932-ca65-3600-825f-e469bd8d6b35",
                    "sense_definition": "Кусать или жалить (о насекомых).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "eating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вечером у реки нас нещадно <ели> комары и мошки.",
                            "target_lang_translations": {
                                "en": "In the evening by the river, mosquitoes and midges <ate> us alive."
                            }
                        },
                        {
                            "text": "Наденьте плотную одежду, чтобы вас не <ели> насекомые в лесу.",
                            "target_lang_translations": {
                                "en": "Wear thick clothes so insects don't <bite> you in the forest."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To bite or sting (about insects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bite",
                                "target_lang_sense_clarification": "insects"
                            },
                            {
                                "target_lang_word": "eat",
                                "target_lang_sense_clarification": "colloquial, insects"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1e57ec3-6056-3bfe-8409-bcd9204c94bd",
                    "sense_definition": "Разрушать химически, вызывать коррозию (о ржавчине, кислоте и т.п.).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Морская соль медленно <ест> металл корпуса старого корабля.",
                            "target_lang_translations": {
                                "en": "Sea salt slowly <eats away> the metal of the old ship's hull."
                            }
                        },
                        {
                            "text": "Эта сильная кислота <ест> даже стекло и некоторые виды пластика.",
                            "target_lang_translations": {
                                "en": "This strong acid <eats> even glass and some types of plastic."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ржавчина ест"
                    ],
                    "target_lang_definitions": {
                        "en": "To destroy chemically, to cause corrosion (about rust, acid, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eat away",
                                "target_lang_sense_clarification": "corrode"
                            },
                            {
                                "target_lang_word": "corrode",
                                "target_lang_sense_clarification": "chemical destruction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc560221-9bff-3534-a998-4c3bbf7a1fb0",
                    "sense_definition": "Изводить кого-либо упрёками, критикой или мучить морально.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "irritation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она целыми днями <ест> мужа за то, что он мало зарабатывает.",
                            "target_lang_translations": {
                                "en": "She <nags> her husband all day because he earns little."
                            }
                        },
                        {
                            "text": "Тоска <ела> его сердце, когда он вспоминал о потерянном доме.",
                            "target_lang_translations": {
                                "en": "Anguish <gnawed at> his heart when he remembered his lost home."
                            }
                        }
                    ],
                    "common_phrases": [
                        "есть поедом"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To harass someone with reproaches, criticism, or to torment morally."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nag",
                                "target_lang_sense_clarification": "harass"
                            },
                            {
                                "target_lang_word": "gnaw",
                                "target_lang_sense_clarification": "torment"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d8f9a2db-4153-3d00-9dc8-22bffb8d7505",
                    "sense_definition": "Военный ответ командиру, означающий подтверждение получения приказа и готовность его выполнить.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "military_command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Прекратить огонь! — <Есть> прекратить огонь, товарищ командир!",
                            "target_lang_translations": {
                                "en": "— Cease fire! — <Yes, sir>, cease fire, Comrade Commander!"
                            }
                        },
                        {
                            "text": "Получив приказ выступать, солдат громко ответил: «<Есть>!» и побежал к машине.",
                            "target_lang_translations": {
                                "en": "Having received the order to move out, the soldier answered loudly: '<Yes, sir>!' and ran to the vehicle."
                            }
                        }
                    ],
                    "synonyms": [
                        "так точно"
                    ],
                    "antonyms": [
                        "никак нет"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "military"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Military response to a commander, signifying confirmation of an order and readiness to execute it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Yes, sir!",
                                "target_lang_sense_clarification": "military"
                            },
                            {
                                "target_lang_word": "Aye, aye!",
                                "target_lang_sense_clarification": "naval/military"
                            }
                        ]
                    }
                }
            ]
        },
        {

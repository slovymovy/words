                {
                    "sense_id": "489104eb-5515-3651-93ac-b28c17f43836",
                    "sense_definition": "Dać coś komuś bezpłatnie, przekazać jako prezent.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chcę ci <w>podarować</w> tę książkę na pamiątkę.",
                            "target_lang_translations": {
                                "ru": "Я хочу <w>подарить</w> тебе эту книгу на память."
                            }
                        },
                        {
                            "text": "Rodzice <w>podarowali</w> nam piękny zestaw mebli do nowego mieszkania.",
                            "target_lang_translations": {
                                "ru": "Родители <w>подарили</w> нам красивый набор мебели для новой квартиры."
                            }
                        }
                    ],
                    "synonyms": [
                        "darować"
                    ],
                    "common_phrases": [
                        "podarować prezent",
                        "podarować kwiaty",
                        "podarować komuś coś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать что-то кому-то бесплатно, передать в качестве подарка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подарить",
                                "target_lang_sense_clarification": "вручить как подарок"
                            },
                            {
                                "target_lang_word": "преподнести",
                                "target_lang_sense_clarification": "торжественно подарить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3601406-bd55-3b73-b58b-ee4d3803f992",
                    "sense_definition": "Zrezygnować z robienia czegoś, co uważa się za zbędne lub niewłaściwe; odpuścić coś sobie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "refraining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powinieneś <w>podarować</w> sobie te złośliwe uwagi.",
                            "target_lang_translations": {
                                "ru": "Тебе стоит <w>оставить</w> при себе эти язвительные замечания."
                            }
                        },
                        {
                            "text": "Z powodu zmęczenia postanowiłem <w>podarować</w> sobie dzisiejszy trening.",
                            "target_lang_translations": {
                                "ru": "Из-за усталости я решил <w>пропустить</w> сегодняшнюю тренировку."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpuścić",
                        "darować"
                    ],
                    "common_phrases": [
                        "podarować sobie coś",
                        "podarować sobie komentarze",
                        "podarować sobie resztę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used with the reflexive pronoun 'sobie'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказаться от выполнения чего-либо, что считается лишним или неуместным; перестать делать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропустить",
                                "target_lang_sense_clarification": "о занятии или событии"
                            },
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "перестать делать или говорить (обычно с 'при себе')"
                            },
                            {
                                "target_lang_word": "отказаться",
                                "target_lang_sense_clarification": "решить не делать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b2b78e4-89a9-3d3f-8fb4-a63f59744d94",
                    "sense_definition": "Okazać łaskę rezygnując z kary lub pretensji; puścić coś w niepamięć.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "forgiving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Król zdecydował się <w>podarować</w> więźniowi życie.",
                            "target_lang_translations": {
                                "ru": "Король решил <w>даровать</w> узнику жизнь."
                            }
                        },
                        {
                            "text": "Musisz mu to <w>podarować</w>, jeśli chcecie się pogodzić.",
                            "target_lang_translations": {
                                "ru": "Ты должен ему это <w>простить</w>, если вы хотите помириться."
                            }
                        }
                    ],
                    "synonyms": [
                        "przebaczyć",
                        "wybaczyć",
                        "darować"
                    ],
                    "common_phrases": [
                        "podarować życie",
                        "podarować winy",
                        "podarować karę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявить милость, отказавшись от наказания или претензий; простить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простить",

                },
                {
                    "sense_id": "fd0b7825-3ad7-34d3-8fbc-f711c9924f47",
                    "sense_definition": "Gemakkelijk te verteren (van voedsel).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "voedsel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik neem als lunch graag een <w>lichte</w> salade.",
                            "target_lang_translations": {
                                "ru": "На обед я предпочитаю <w>лёгкий</w> салат."
                            }
                        },
                        {
                            "text": "Deze maaltijd is <w>licht</w> en valt niet zwaar op de maag.",
                            "target_lang_translations": {
                                "ru": "Эта еда <w>лёгкая</w> и не вызывает тяжести в желудке."
                            }
                        }
                    ],
                    "antonyms": [
                        "zwaar"
                    ],
                    "common_phrases": [
                        "lichte maaltijd",
                        "licht verteerbaar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легко усваиваемый (о еде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лёгкий",
                                "target_lang_sense_clarification": "о пище"
                            },
                            {
                                "target_lang_word": "диетический",
                                "target_lang_sense_clarification": "полезный для здоровья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "474b25a8-c0da-377b-9fcd-86ff42c040ed",
                    "sense_definition": "In geringe mate; niet ernstig of hevig.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intensiteit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij had last van een <w>lichte</w> hoofdpijn na het werk.",

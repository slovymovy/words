{
    "word_family": [
        "drzwiczki",
        "drzwiowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "daba7f51-e624-34aa-80bf-7369f27925e7",
                    "sense_definition": "Ruchoma przegroda zamykająca otwór w ścianie, umożliwiająca wchodzenie do pomieszczeń i wychodzenie z nich.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę, zamknij za sobą <w>drzwi</w>, gdy będziesz wychodzić.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, закрой за собой <w>дверь</w>, когда будешь уходить."
                            }
                        },
                        {
                            "text": "Stare, drewniane <w>drzwi</w> skrzypią przy każdym otwarciu.",
                            "target_lang_translations": {
                                "ru": "Старая деревянная <w>дверь</w> скрипит при каждом открывании."
                            }
                        }
                    ],
                    "synonyms": [
                        "wejście",
                        "wrota"
                    ],
                    "common_phrases": [
                        "otworzyć drzwi",
                        "zamknąć drzwi",
                        "pukać do drzwi",
                        "stać w drzwiach"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rzeczownik występujący tylko w liczbie mnogiej (plurale tantum)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижная перегородка, закрывающая проём в стене, позволяющая входить в помещения и выходить из них."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дверь",
                                "target_lang_sense_clarification": "входная или межкомнатная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0571ae0-3583-3823-be6c-92c0a836e7e5",
                    "sense_definition": "Element zamykający otwór w meblach, pojazdach lub urządzeniach, działający podobnie jak drzwi do pomieszczeń.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znowu nie domknąłeś <w>drzwi</w> od szafy w sypialni.",
                            "target_lang_translations": {
                                "ru": "Ты снова не закрыл <w>дверцу</w> шкафа в спальне."
                            }
                        },
                        {
                            "text": "Pasażer otworzył tylne <w>drzwi</w> samochodu i szybko wysiadł.",
                            "target_lang_translations": {
                                "ru": "Пассажир открыл заднюю <w>дверь</w> автомобиля и быстро вышел."
                            }
                        }
                    ],
                    "synonyms": [
                        "drzwiczki"
                    ],
                    "common_phrases": [
                        "drzwi od szafy",
                        "drzwi samochodowe",
                        "tylne drzwi"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rzeczownik występujący tylko w liczbie mnogiej (plurale tantum)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент, закрывающий проём в мебели, транспортных средствах или устройствах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дверца",
                                "target_lang_sense_clarification": "шкафа, тумбочки"
                            },
                            {
                                "target_lang_word": "дверь",
                                "target_lang_sense_clarification": "автомобиля, вагона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35edebb5-bfa0-340c-ac62-cb16d25a0863",
                    "sense_definition": "Sposób na osiągnięcie czegoś lub dostęp do nowych możliwości (znaczenie przenośne).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dobra znajomość języków obcych otwiera <w>drzwi</w> do międzynarodowej kariery.",
                            "target_lang_translations": {
                                "ru": "Хорошее знание иностранных языков открывает <w>двери</w> к международной карьере."
                            }
                        },
                        {
                            "text": "To stypendium było dla niej <w>drzwiami</w> do wielkiego sukcesu.",
                            "target_lang_translations": {
                                "ru": "Эта стипендия стала для неё <w>дверью</w> к большому успеху."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drzwi do kariery",
                        "otwarte drzwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ достижения чего-либо или доступ к новым возможностям (переносное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дверь",
                                "target_lang_sense_clarification": "путь к возможностям"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
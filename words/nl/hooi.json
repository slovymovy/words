                {
                    "sense_id": "e1d1687f-656a-360e-b771-d238363387bd",
                    "sense_definition": "Gedroogd gras, voornamelijk gebruikt als voedsel voor vee.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hay_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer brengt het <w>hooi</w> naar de schuur voor de winter.",
                            "target_lang_translations": {
                                "ru": "Фермер относит <w>сено</w> в сарай на зиму."
                            }
                        },
                        {
                            "text": "Paarden eten graag vers <w>hooi</w> als ze op stal staan.",
                            "target_lang_translations": {
                                "ru": "Лошади любят есть свежее <w>сено</w>, когда стоят в конюшне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "te veel hooi op je vork nemen",
                        "een speld in een hooiberg zoeken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сушеная трава, используемая в основном как корм для скота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сено",
                                "target_lang_sense_clarification": "сухая трава"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b8c50fa-d16e-3430-8225-4796e37d64b0",
                    "sense_definition": "Gras dat op het land staat en bestemd is om gemaaid en gedroogd te worden.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "hay_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>hooi</w> staat hoog en moet bijna gemaaid worden.",
                            "target_lang_translations": {
                                "ru": "<w>Трава</w> стоит высоко, и ее скоро нужно косить."
                            }
                        },
                        {
                            "text": "We wachten op droog weer voordat we het <w>hooi</w> van het land halen.",
                            "target_lang_translations": {
                                "ru": "Мы ждем сухой погоды, прежде чем убрать <w>сено</w> с поля."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Трава на поле, предназначенная для скашивания и сушки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трава на сено",
                                "target_lang_sense_clarification": "растущая трава"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b0330c1f-1e52-3e80-8756-5262352b31ab",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>hooien</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>hooi</w> meestal in de zomer als het warm en droog is.",
                            "target_lang_translations": {
                                "ru": "Я обычно <w>заготавливаю сено</w> летом, когда тепло и сухо."
                            }
                        },
                        {
                            "text": "<w>Hooi</w> jij dit jaar ook bij de buren op het land?",
                            "target_lang_translations": {
                                "ru": "Ты тоже <w>заготавливаешь сено</w> в этом году на поле у соседей?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола \"hooien\" (заготавливать сено)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заготавливаю сено",
                                "target_lang_sense_clarification": "форма глагола"
                            }
                        ]
                    }
                }
            ]
        }
    ]

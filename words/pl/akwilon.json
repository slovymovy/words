            "senses": [
                {
                    "sense_id": "4671a7d8-4c6f-3e82-8568-a4d9bc2a0c2b",
                    "sense_definition": "Gwałtowny, zimny wiatr wiejący z północy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "north_wind_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podróżnicy skulili się, gdy smagał ich lodowaty <w>akwilon</w>.",
                            "target_lang_translations": {
                                "ru": "Путешественники съежились, когда их хлестал ледяной <w>аквилон</w>."
                            }
                        },
                        {
                            "text": "Mroźny <w>akwilon</w> zerwał ostatnie liście z drzew w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Морозный <w>аквилон</w> сорвал последние листья с деревьев в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "bora"
                    ],
                    "common_phrases": [
                        "mroźny akwilon",
                        "dmący akwilon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный, холодный ветер, дующий с севера (часто употребляется в поэтическом контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аквилон",
                                "target_lang_sense_clarification": "поэтическое название ветра"
                            },
                            {
                                "target_lang_word": "северный ветер",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d617d3f6-c2cc-3a75-a8c8-90deedc2799e",
                    "sense_definition": "Rzymski bóg wiatru północnego, utożsamiany z greckim Boreaszem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "north_wind_concept",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Starożytni poeci opisywali, jak <w>Akwilon</w> dmie z północy.",
                            "target_lang_translations": {
                                "ru": "Древние поэты описывали, как <w>Аквилон</w> дует с севера."
                            }
                        },
                        {
                            "text": "W mitologii rzymskiej <w>Akwilon</w> był uosobieniem zimnego wiatru.",
                            "target_lang_translations": {
                                "ru": "В римской мифологии <w>Аквилон</w> был олицетворением холодного ветра."
                            }
                        }
                    ],
                    "synonyms": [
                        "Boreasz"
                    ],
                    "common_phrases": [
                        "gniewny Akwilon",
                        "potężny Akwilon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Римский бог северного ветра, отождествляемый с греческим Бореем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Аквилон",
                                "target_lang_sense_clarification": "божество"
                            }
                        ]
                    }
                }
            ]
        }
    ]

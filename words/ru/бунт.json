            "senses": [
                {
                    "sense_id": "9a299554-6d2b-364c-a9a6-666c06546467",
                    "sense_definition": "Стихийное восстание, массовое выступление против власти или установленного порядка.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "uprising",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Власти смогли быстро подавить <w>бунт</w> в столице.",
                            "target_lang_translations": {
                                "en": "The authorities were able to quickly suppress the <w>riot</w> in the capital."
                            }
                        },
                        {
                            "text": "На корабле начался <w>бунт</w> из-за нехватки пресной воды.",
                            "target_lang_translations": {
                                "en": "A <w>mutiny</w> broke out on the ship due to a lack of fresh water."
                            }
                        },
                        {
                            "text": "Московское войско принуждено было укрощать вспыхнувший <w>бунт</w>.",
                            "target_lang_translations": {
                                "en": "The Moscow army was forced to subdue the <w>rebellion</w> that had broken out."
                            }
                        }
                    ],
                    "synonyms": [
                        "мятеж",
                        "восстание",
                        "беспоря́дки"
                    ],
                    "antonyms": [
                        "покорность",
                        "повиновение"
                    ],
                    "common_phrases": [
                        "подавить бунт",
                        "поднять бунт",
                        "голодный бунт",
                        "бессмысленный и беспощадный"
                    ],
                    "target_lang_definitions": {
                        "en": "A spontaneous uprising, riot, or mass protest against authority or the established order."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "riot",
                                "target_lang_sense_clarification": "violent public disturbance"
                            },
                            {
                                "target_lang_word": "mutiny",
                                "target_lang_sense_clarification": "rebellion by soldiers or sailors"
                            },
                            {
                                "target_lang_word": "rebellion",
                                "target_lang_sense_clarification": "resistance to established government"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25be30d2-8fc9-30d2-8c50-a073a073f71c",
                    "sense_definition": "Активное выражение несогласия, неповиновения или протеста против чьей-либо воли.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disobedience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Подростковый <w>бунт</w> часто выражается в отказе следовать правилам родителей.",
                            "target_lang_translations": {
                                "en": "Teenage <w>rebellion</w> is often expressed by a refusal to follow parents' rules."
                            }
                        },
                        {
                            "text": "Это был тихий <w>бунт</w> против несправедливого решения начальника.",
                            "target_lang_translations": {
                                "en": "It was a quiet <w>rebellion</w> against the boss's unfair decision."
                            }
                        }
                    ],
                    "synonyms": [
                        "неповиновение",
                        "несогласие",
                        "непокорность",
                        "возмущение"
                    ],
                    "antonyms": [
                        "согласие"
                    ],
                    "common_phrases": [
                        "подростковый бунт",
                        "устроить бунт"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An active expression of disagreement, disobedience, or protest against someone's will."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rebellion",
                                "target_lang_sense_clarification": "defiance of authority or control"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ad535c6-f05d-361c-9ed7-4053801f477d",
                    "sense_definition": "Связка, пачка или кипа товара (обычно льна, пеньки, табака или бумаги).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "bundle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие аккуратно складывали товар в большие <w>бунты</w>.",
                            "target_lang_translations": {
                                "en": "The workers carefully stacked the goods into large <w>bales</w>."
                            }
                        },
                        {
                            "text": "На складе лежали <w>бунты</w> с табаком и пенькой.",
                            "target_lang_translations": {
                                "en": "<w>Bales</w> of tobacco and hemp lay in the warehouse."
                            }
                        }
                    ],
                    "synonyms": [
                        "свя́зка",
                        "па́чка",
                        "ки́па"
                    ],

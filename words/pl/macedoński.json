    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "917fe6d5-1169-3bd6-987a-c585059bbd1b",
                    "sense_definition": "odnoszący się do państwa Macedonii Północnej lub narodu macedońskiego",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "geo_ethnic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd <w>macedoński</w> podpisał nową umowę handlową z sąsiadami.",
                            "target_lang_translations": {
                                "ru": "<w>Македонское</w> правительство подписало новое торговое соглашение с соседями."
                            }
                        },
                        {
                            "text": "Mój kolega ma <w>macedońskie</w> obywatelstwo i często tam podróżuje.",
                            "target_lang_translations": {
                                "ru": "У моего коллеги <w>македонское</w> гражданство, и он часто туда ездит."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naród macedoński",
                        "granica macedońska",
                        "kultura macedońska"
                    ],
                    "target_lang_definitions": {
                        "ru": "относящийся к государству Северная Македония или македонскому народу"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "македонский",
                                "target_lang_sense_clarification": "о государстве или народе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6a8c70c-ce1c-35d2-b460-dfcdf956e8ef",
                    "sense_definition": "odnoszący się do historycznej krainy Macedonii (np. w Grecji) lub starożytnego państwa Aleksandra Wielkiego",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geo_ethnic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aleksander <w>Macedoński</w> podbił ogromne terytoria w starożytności.",
                            "target_lang_translations": {
                                "ru": "Александр <w>Македонский</w> завоевал огромные территории в древности."
                            }
                        },
                        {
                            "text": "Tradycyjna sałatka <w>macedońska</w> składa się z wielu różnych owoców.",
                            "target_lang_translations": {
                                "ru": "Традиционный <w>македонский</w> салат состоит из множества разных фруктов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "falanga macedońska",
                        "król macedoński"
                    ],
                    "target_lang_definitions": {
                        "ru": "относящийся к исторической области Македония или древнему государству Александра Великого"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "македонский",
                                "target_lang_sense_clarification": "исторический"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9ad85d34-3f76-3888-ab92-f905c84c6e92",
                    "sense_definition": "język południowosłowiański, język urzędowy Macedonii Północnej",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "language",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Współczesny <w>macedoński</w> jest bardzo podobny do języka bułgarskiego.",
                            "target_lang_translations": {
                                "ru": "Современный <w>македонский</w> очень похож на болгарский язык."
                            }
                        },
                        {
                            "text": "Czy wiesz, że <w>macedoński</w> zapisuje się cyrylicą?",
                            "target_lang_translations": {
                                "ru": "Ты знаешь, что <w>македонский</w> записывается кириллицей?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "język macedoński",
                        "literatura macedońska"
                    ],
                    "target_lang_definitions": {
                        "ru": "южнославянский язык, официальный язык Северной Македонии"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "македонский язык",
                                "target_lang_sense_clarification": "язык"
                            },
                            {
                                "target_lang_word": "македонский",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4a429ba-f133-3173-8419-2a1cec85ddac",
                    "sense_definition": "wymarły język używany przez starożytnych mieszkańców Macedonii",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "language",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Naukowcy wciąż spierają się o to, jak brzmiał starożytny <w>macedoński</w>.",
                            "target_lang_translations": {
                                "ru": "Ученые до сих пор спорят о том, как звучал древний <w>македонский</w>."
                            }
                        },
                        {
                            "text": "Niewiele tekstów przetrwało, w których zapisano <w>macedoński</w> z czasów antycznych.",
                            "target_lang_translations": {
                                "ru": "Сохранилось немного текстов, в которых записан <w>македонский</w> античных времен."
                            }
                        }
                    ],
                    "synonyms": [
                        "staromacedoński"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "dotyczy języka historycznego"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "вымерший язык, который использовали древние жители Македонии"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "древнемакедонский язык",
                                "target_lang_sense_clarification": "термин"
                            },
                            {
                                "target_lang_word": "македонский",
                                "target_lang_sense_clarification": "в историческом контексте"
                            }
                        ]
                    }
                }
            ]
        }
    ]

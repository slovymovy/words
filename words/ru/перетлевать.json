    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0dde44a5-525a-3b80-aa98-796e890d33dd",
                    "sense_definition": "Разрушаться, распадаться от гниения (обычно о многих предметах или полностью).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "decay",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Осенние листья медленно <w>перетлевали</w> под снегом, удобряя лесную почву.",
                            "target_lang_translations": {
                                "en": "Autumn leaves were slowly <w>rotting</w> under the snow, fertilizing the forest soil."
                            }
                        },
                        {
                            "text": "Старые пни в лесу постепенно <w>перетлевали</w> и превращались в мягкую труху.",
                            "target_lang_translations": {
                                "en": "Old stumps in the forest were gradually <w>mouldering</w> and turning into soft dust."
                            }
                        }
                    ],
                    "synonyms": [
                        "сгнивать",
                        "разлагаться",
                        "истлевать"
                    ],
                    "target_lang_definitions": {
                        "en": "To decompose or disintegrate due to rotting (usually completely or regarding many objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rot away",
                                "target_lang_sense_clarification": "to decompose completely"
                            },
                            {
                                "target_lang_word": "moulder",
                                "target_lang_sense_clarification": "to slowly decay into dust"
                            },
                            {
                                "target_lang_word": "decay",
                                "target_lang_sense_clarification": "general biological decomposition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49c107d6-9674-3793-a8ab-87780098ee55",
                    "sense_definition": "Сгорать без пламени, превращаясь в золу или уголь (обычно о многих предметах).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "burn",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Угли в костре уже <w>перетлевали</w>, и воздух вокруг становился холодным.",
                            "target_lang_translations": {
                                "en": "The coals in the campfire were already <w>burning out</w>, and the air around was becoming cold."
                            }
                        },
                        {
                            "text": "Дрова в печи <w>перетлевали</w>, оставляя после себя лишь горстку серого пепла.",
                            "target_lang_translations": {
                                "en": "The firewood in the stove was <w>burning out</w>, leaving behind only a handful of gray ash."
                            }
                        }
                    ],
                    "synonyms": [
                        "перегорать",
                        "истлевать"
                    ],
                    "target_lang_definitions": {
                        "en": "To burn without a flame, turning into ash or charcoal (usually completely)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "burn out",
                                "target_lang_sense_clarification": "to finish burning and turn to ash"
                            },
                            {
                                "target_lang_word": "smoulder away",
                                "target_lang_sense_clarification": "to burn slowly without flame until consumed"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
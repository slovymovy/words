    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bb795c8f-3740-352a-9e50-cf2cc95553dd",
                    "sense_definition": "Met een smaak die het midden houdt tussen bitter en zoet.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "smaak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De donkere chocolade had een aangename <w>bitterzoete</w> nasmaak.",
                            "target_lang_translations": {
                                "ru": "У темного шоколада было приятное <w>горько-сладкое</w> послевкусие."
                            }
                        },
                        {
                            "text": "Wij dronken een cocktail met een frisse en <w>bitterzoete</w> smaak.",
                            "target_lang_translations": {
                                "ru": "Мы пили коктейль со свежим <w>горько-сладким</w> вкусом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bitterzoete smaak",
                        "bitterzoete chocolade",
                        "bitterzoete vrucht"
                    ],
                    "target_lang_definitions": {
                        "ru": "Со вкусом, сочетающим в себе горечь и сладость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горько-сладкий",
                                "target_lang_sense_clarification": "о вкусе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96c4aa84-dfc7-3480-b559-fe68cbd78699",
                    "sense_definition": "Gekenmerkt door een mengeling van vreugde en verdriet; tegelijkertijd fijn en pijnlijk.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het afscheid van zijn collega's was een <w>bitterzoet</w> moment voor hem.",
                            "target_lang_translations": {
                                "ru": "Прощание с коллегами стало для него <w>горько-сладким</w> моментом."
                            }
                        },
                        {
                            "text": "Ze dacht met een <w>bitterzoete</w> glimlach terug aan haar studententijd.",
                            "target_lang_translations": {
                                "ru": "Она вспоминала свои студенческие годы с <w>горько-сладкой</w> улыбкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bitterzoete herinnering",
                        "bitterzoet gevoel",
                        "bitterzoet afscheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся смесью радости и грусти; одновременно приятный и болезненный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горько-сладкий",
                                "target_lang_sense_clarification": "об эмоциях или воспоминаниях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e5b6713f-3bae-372c-9d53-869d306d4bb2",
                    "sense_definition": "Een vaste plant uit de nachtschadefamilie met paarse bloemen en rode bessen (Solanum dulcamara).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Langs de waterkant vonden we bloeiend <w>bitterzoet</w> tussen het riet.",
                            "target_lang_translations": {
                                "ru": "У кромки воды, среди камышей, мы нашли цветущий <w>паслён сладко-горький</w>."
                            }
                        },
                        {
                            "text": "Je moet oppassen met de bessen van het <w>bitterzoet</w>, want ze zijn giftig.",
                            "target_lang_translations": {
                                "ru": "Нужно быть осторожным с ягодами <w>паслёна сладко-горького</w>, так как они ядовиты."
                            }
                        }
                    ],
                    "synonyms": [
                        "alfrank",
                        "dolkruid",
                        "elfrank"
                    ],
                    "target_lang_definitions": {
                        "ru": "Многолетнее растение семейства пасленовых с фиолетовыми цветами и красными ягодами (Solanum dulcamara)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паслён сладко-горький",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
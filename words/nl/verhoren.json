    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "305e4f09-d4ef-39bc-adb9-e8b6e66616ad",
                    "sense_definition": "Iemand indringend vragen stellen om de waarheid te achterhalen, vaak uitgevoerd door politie of justitie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "interrogation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie moest de verdachte urenlang <w>verhoren</w> voordat hij bekende.",
                            "target_lang_translations": {
                                "en": "The police had to <w>interrogate</w> the suspect for hours before he confessed."
                            }
                        },
                        {
                            "text": "Zij werd als getuige <w>verhoord</w> over wat ze die avond had gezien.",
                            "target_lang_translations": {
                                "en": "She was <w>questioned</w> as a witness about what she had seen that evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "ondervragen"
                    ],
                    "common_phrases": [
                        "een verdachte verhoren",
                        "getuigen verhoren"
                    ],
                    "target_lang_definitions": {
                        "en": "To question someone intensely to uncover the truth, often performed by police or justice officials."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "interrogate",
                                "target_lang_sense_clarification": "to question formally and systematically"
                            },
                            {
                                "target_lang_word": "question",
                                "target_lang_sense_clarification": "to ask questions to a suspect or witness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4067ac5e-6407-3e5c-b6f0-c7e640e6e437",
                    "sense_definition": "Luisteren naar een gebed, wens of verzoek en dit in vervulling laten gaan.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "granting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gelovigen hoopten dat God hun gebeden zou <w>verhoren</w>.",
                            "target_lang_translations": {
                                "en": "The believers hoped that God would <w>answer</w> their prayers."
                            }
                        },
                        {
                            "text": "Mijn wens om een wereldreis te maken werd eindelijk <w>verhoord</w>.",
                            "target_lang_translations": {
                                "en": "My wish to travel the world was finally <w>granted</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een gebed verhoren",
                        "een wens verhoren"
                    ],
                    "target_lang_definitions": {
                        "en": "To listen to a prayer, wish, or request and cause it to be fulfilled."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "answer",
                                "target_lang_sense_clarification": "regarding prayers"
                            },
                            {
                                "target_lang_word": "grant",
                                "target_lang_sense_clarification": "regarding wishes or requests"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "28aea430-d621-309c-a937-edf04faef940",
                    "sense_definition": "Meervoud van <w>verhoor</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>verhoren</w> van de getuigen namen de hele middag in beslag.",
                            "target_lang_translations": {
                                "en": "The witness <w>hearings</w> took up the entire afternoon."
                            }
                        },
                        {
                            "text": "Er staan deze week nog drie <w>verhoren</w> gepland in deze zaak.",
                            "target_lang_translations": {
                                "en": "Three more <w>interrogations</w> are scheduled for this case this week."
                            }
                        }

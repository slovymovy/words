                    "sense_id": "d6ab42a2-f200-3c21-9565-a2b810cbd32e",
                    "sense_definition": "Приблизить что-либо вплотную к чему-либо, прижать к поверхности.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама осторожно <приложила> ладонь ко лбу ребёнка, чтобы проверить температуру.",
                            "target_lang_translations": {
                                "en": "Mom gently <put> her palm to the child's forehead to check the temperature."
                            }
                        },
                        {
                            "text": "Врач сказал немедленно <приложить> лёд к ушибу, чтобы снять отёк.",
                            "target_lang_translations": {
                                "en": "The doctor said to immediately <apply> ice to the bruise to relieve the swelling."
                            }
                        }
                    ],
                    "synonyms": [
                        "прижать",
                        "прислонить"
                    ],
                    "antonyms": [
                        "отодвинуть",
                        "убрать"
                    ],
                    "common_phrases": [
                        "приложить руку к сердцу",
                        "приложить палец к губам",
                        "приложить лёд",
                        "приложить к уху"
                    ],
                    "target_lang_definitions": {
                        "en": "To place something close against a surface or object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "apply",
                                "target_lang_sense_clarification": "e.g. ice, medication, or an object to a surface"
                            },
                            {
                                "target_lang_word": "put",
                                "target_lang_sense_clarification": "general placement against something"
                            },
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "to hold against something with slight pressure"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8acbb015-1579-3aa0-b84a-dcc8cd5a4c47",
                    "sense_definition": "Добавить или присоединить что-либо к чему-либо (часто о документах, файлах или дополнениях).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attach_add",
                    "name_type": "no",
                    "examples": [

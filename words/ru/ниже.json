        "занижать",
        "унижение",
        "снижение",
        "понижение",
        "занижение"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "41adea97-4769-3eeb-99c3-39bd42b0b44b",
                    "sense_definition": "Сравнительная степень прилагательного <w>низкий</w> и наречия <w>низко</w>; обозначает нахождение на меньшей высоте, меньший уровень или степень проявления чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_lower_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солнце опускалось всё <w>ниже</w> к горизонту.",
                            "target_lang_translations": {
                                "en": "The sun was sinking ever <w>lower</w> towards the horizon."
                            }
                        },
                        {
                            "text": "Эта полка висит <w>ниже</w>, чем остальные.",
                            "target_lang_translations": {
                                "en": "This shelf hangs <w>lower</w> than the others."
                            }
                        },
                        {
                            "text": "Голову нужно наклонить <w>ниже</w>, чтобы пройти в дверь.",
                            "target_lang_translations": {
                                "en": "You need to bend your head <w>lower</w> to go through the door."
                            }
                        }
                    ],
                    "antonyms": [
                        "выше"
                    ],
                    "common_phrases": [
                        "гораздо ниже",
                        "ещё ниже",
                        "ниже ростом",
                        "опустить ниже"
                    ],
                    "target_lang_definitions": {
                        "en": "Comparative degree of the adjective 'low' and adverb 'low'; denotes being at a lesser height, lower level, or degree."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lower",
                                "target_lang_sense_clarification": "comparative degree"
                            },
                            {
                                "target_lang_word": "further down",
                                "target_lang_sense_clarification": "spatial position"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45e82ab0-fbd2-37f7-80a9-7c1ec045b3df",
                    "sense_definition": "Наречие, указывающее на место далее в тексте, книге или документе.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_text_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Подробный список использованной литературы приведён <w>ниже</w>.",
                            "target_lang_translations": {
                                "en": "A detailed list of references is provided <w>below</w>."
                            }
                        },
                        {
                            "text": "Как будет показано <w>ниже</w>, этот метод имеет свои недостатки.",
                            "target_lang_translations": {
                                "en": "As will be shown <w>below</w>, this method has its drawbacks."
                            }
                        }
                    ],
                    "synonyms": [
                        "далее"
                    ],
                    "antonyms": [
                        "выше"
                    ],
                    "common_phrases": [
                        "см. ниже",
                        "смотри ниже",
                        "указано ниже",
                        "приведено ниже"
                    ],
                    "target_lang_definitions": {
                        "en": "Adverb indicating a place further on in a text, book, or document."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "below",
                                "target_lang_sense_clarification": "reference in text"
                            },
                            {
                                "target_lang_word": "further down",
                                "target_lang_sense_clarification": "later in text"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "4a7742ad-2d6c-3f30-a96b-822d4ff9852d",
                    "sense_definition": "Предлог, указывающий на нахождение в более низком положении по отношению к чему-либо или на меньшую величину (употребляется с родительным падежом).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_lower_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой температура часто опускается <w>ниже</w> нуля.",
                            "target_lang_translations": {
                                "en": "In winter, the temperature often drops <w>below</w> zero."
                            }
                        },
                        {
                            "text": "Самолет летел <w>ниже</w> облаков.",
                            "target_lang_translations": {
                                "en": "The plane was flying <w>below</w> the clouds."
                            }
                        },
                        {
                            "text": "Результаты теста оказались <w>ниже</w> среднего уровня.",
                            "target_lang_translations": {
                                "en": "The test results turned out to be <w>below</w> average."
                            }
                        }
                    ],
                    "antonyms": [
                        "выше"
                    ],
                    "common_phrases": [
                        "ниже нуля",
                        "ниже среднего",
                        "ниже уровня моря",
                        "ниже достоинства"
                    ],
                    "target_lang_definitions": {
                        "en": "Preposition indicating a location in a lower position relative to something or a lesser magnitude."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "below",
                                "target_lang_sense_clarification": "lower than"
                            },
                            {
                                "target_lang_word": "under",
                                "target_lang_sense_clarification": "beneath"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "2dbfc230-0d8a-30c1-8255-40064792d70c",
                    "sense_definition": "Устаревший союз, используемый для усиления отрицания; соответствует значениям «даже», «и даже», «ни даже».",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_archaic_conjunction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он не смел возразить, <w>ниже</w> поднять глаза на своего начальника.",
                            "target_lang_translations": {
                                "en": "He did not dare to object, <w>nor</w> raise his eyes to his boss."
                            }
                        },
                        {
                            "text": "Никто не знал ответа, <w>ниже</w> сам учитель.",
                            "target_lang_translations": {
                                "en": "No one knew the answer, <w>not even</w> the teacher himself."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Встречается преимущественно в литературе XVIII–XIX веков."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Archaic conjunction used to emphasize negation; corresponds to 'nor', 'not even'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nor",
                                "target_lang_sense_clarification": "archaic/literary"
                            },
                            {
                                "target_lang_word": "not even",
                                "target_lang_sense_clarification": "emphatic negation"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
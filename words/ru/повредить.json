            "senses": [
                {
                    "sense_id": "aee3a3b3-e8c6-3255-a086-0932fd3fc3d4",
                    "sense_definition": "Нарушить целостность, испортить что-либо или нанести физическую травму части тела.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Во время футбольного матча игрок упал и <повредил> колено.",
                            "target_lang_translations": {
                                "en": "During the football match, the player fell and <w>injured</w> his knee."
                            }
                        },
                        {
                            "text": "Сильный ураган <повредил> крышу старого дома, и она начала протекать.",
                            "target_lang_translations": {
                                "en": "A strong hurricane <w>damaged</w> the roof of the old house, and it started to leak."
                            }
                        }
                    ],
                    "synonyms": [
                        "испортить",
                        "сломать",
                        "травмировать",
                        "поранить",
                        "разбить"
                    ],
                    "antonyms": [
                        "починить",
                        "исправить",
                        "вылечить",
                        "восстановить"
                    ],
                    "common_phrases": [
                        "повредить ногу",
                        "повредить спину",
                        "повредить машину",
                        "серьёзно повредить",
                        "случайно повредить"
                    ],
                    "target_lang_definitions": {
                        "en": "To physically break, spoil something, or inflict injury on a body part."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "damage",
                                "target_lang_sense_clarification": "to harm or spoil the condition of an object"
                            },
                            {
                                "target_lang_word": "injure",
                                "target_lang_sense_clarification": "to cause physical trauma to a body part"
                            },
                            {
                                "target_lang_word": "hurt",
                                "target_lang_sense_clarification": "to cause physical pain or injury"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a5909a7-a9ba-3752-a282-78f62db07013",
                    "sense_definition": "Причинить вред, ущерб или отрицательно повлиять на что-либо (здоровье, репутацию, дело).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот громкий скандал может серьёзно <повредить> его репутации.",
                            "target_lang_translations": {
                                "en": "This loud scandal could seriously <w>damage</w> his reputation."
                            }
                        },
                        {
                            "text": "Немного отдыха и свежего воздуха тебе сейчас точно не <повредит>.",
                            "target_lang_translations": {
                                "en": "A little rest and fresh air definitely won't <w>hurt</w> you right now."
                            }
                        }
                    ],
                    "synonyms": [
                        "навредить",
                        "нанести ущерб"
                    ],
                    "antonyms": [
                        "помочь",
                        "поспособствовать"
                    ],
                    "common_phrases": [
                        "повредить здоровью",
                        "повредить репутации",
                        "повредить карьере",
                        "это не повредит"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause harm or have a negative effect on abstract things like health, reputation, or business."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "harm",
                                "target_lang_sense_clarification": "to cause general negative effects or injury"
                            },
                            {
                                "target_lang_word": "damage",
                                "target_lang_sense_clarification": "to have a detrimental effect on reputation or interests"
                            },
                            {
                                "target_lang_word": "hurt",
                                "target_lang_sense_clarification": "to have a negative impact on a situation or person"
                            }
                        ]
                    }
                }
            ]
        }
    ]

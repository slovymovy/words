                    "common_phrases": [
                        "Old Sac"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город Сакраменто, штат Калифорния, США (разговорное сокращение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сак",
                                "target_lang_sense_clarification": "сокращение от Сакраменто"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac9d5de9-7ea1-3919-a111-41149d8b0392",
                    "sense_definition": "Acronym of Strategic Air Command, a former major command of the United States Air Force.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "org_name_sac",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "During the Cold War, <w>SAC</w> was responsible for two-thirds of the American nuclear triad.",
                            "target_lang_translations": {
                                "ru": "Во время холодной войны <w>САК</w> отвечало за две трети американской ядерной триады."
                            }
                        },
                        {
                            "text": "The museum features several bombers that were once flown by pilots in <w>SAC</w>.",
                            "target_lang_translations": {
                                "ru": "В музее представлено несколько бомбардировщиков, на которых когда-то летали пилоты <w>САК</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical military organization"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стратегическое авиационное командование ВВС США."
                    },
                    "translations": {

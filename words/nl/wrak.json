{
    "word_family": [
        "wrakhout",
        "wrakstuk"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e6e1183b-d6cc-3359-9bd2-67ca7d925455",
                    "sense_definition": "Het overblijfsel van een verongelukt of vernield vaartuig, voertuig of vliegtuig.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "broken_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is ten strengste verboden om het <w>wrak</w> naast het koraalrif te betreden.",
                            "target_lang_translations": {
                                "en": "It is strictly forbidden to enter the <w>wreck</w> next to the coral reef.",
                                "ru": "Строго запрещено проникать в <w>обломки</w> рядом с коралловым рифом."
                            }
                        },
                        {
                            "text": "Duikers hebben goud gevonden in het oude <w>wrak</w> op de zeebodem.",
                            "target_lang_translations": {
                                "en": "Divers found gold in the old <w>wreck</w> on the seabed.",
                                "ru": "Дайверы нашли золото в старом <w>затонувшем судне</w> на морском дне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het wrak lichten",
                        "een gezonken wrak"
                    ],
                    "target_lang_definitions": {
                        "en": "The remains of a crashed or destroyed vessel, vehicle, or aircraft.",
                        "ru": "Остатки потерпевшего крушение или разрушенного судна, транспортного средства или самолета."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wreck",
                                "target_lang_sense_clarification": "remains of a vehicle or ship"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обломки",
                                "target_lang_sense_clarification": "остатки разрушенного объекта"
                            },
                            {
                                "target_lang_word": "остов",
                                "target_lang_sense_clarification": "каркас или корпус судна/машины"
                            },
                            {
                                "target_lang_word": "затонувшее судно",
                                "target_lang_sense_clarification": "конкретно для кораблей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a728df77-6f3e-3f00-8b42-cfc879a4997e",
                    "sense_definition": "Iemand die lichamelijk of geestelijk volledig uitgeput of gebroken is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "broken_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Henk was een <w>wrak</w> na het overlijden van zijn vrouw.",
                            "target_lang_translations": {
                                "en": "Henk was a <w>wreck</w> after his wife's death.",
                                "ru": "Хенк был <w>развалиной</w> после смерти жены."
                            }
                        },
                        {
                            "text": "Door de jarenlange stress is hij veranderd in een lichamelijk <w>wrak</w>.",
                            "target_lang_translations": {
                                "en": "Due to years of stress, he has turned into a physical <w>wreck</w>.",
                                "ru": "Из-за многолетнего стресса он превратился в физическую <w>развалину</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een menselijk wrak",
                        "een emotioneel wrak"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who is physically or mentally completely exhausted or broken.",
                        "ru": "Человек, который физически или морально полностью истощен или сломлен."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wreck",
                                "target_lang_sense_clarification": "person in a bad state"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "развалина",
                                "target_lang_sense_clarification": "о человеке с плохим здоровьем или в депрессии"
                            },
                            {
                                "target_lang_word": "разбитый человек",
                                "target_lang_sense_clarification": "о моральном состоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f63c741-769e-367d-8afc-1c675de56262",
                    "sense_definition": "Een voertuig dat in zeer slechte staat verkeert, maar vaak nog wel functioneert.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "broken_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij rijdt rond in een roestig <w>wrak</w> dat bijna uit elkaar valt.",
                            "target_lang_translations": {
                                "en": "He drives around in a rusty <w>wreck</w> that is nearly falling apart.",
                                "ru": "Он разъезжает на ржавой <w>развалюхе</w>, которая почти разваливается на части."
                            }
                        },
                        {
                            "text": "Wat een <w>wrak</w> is die auto van jou, hij start bijna nooit.",
                            "target_lang_translations": {
                                "en": "What a <w>wreck</w> that car of yours is; it almost never starts.",
                                "ru": "Ну и <w>корыто</w> твоя машина, она почти никогда не заводится."
                            }
                        }
                    ],
                    "synonyms": [
                        "wreekte"
                    ],
                    "common_phrases": [
                        "een oud wrak",
                        "rijdend wrak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used jokingly or disparagingly"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A vehicle that is in very poor condition, but often still functions.",
                        "ru": "Транспортное средство в очень плохом состоянии, но часто все еще функционирующее."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wreck",
                                "target_lang_sense_clarification": "dilapidated vehicle"
                            },
                            {
                                "target_lang_word": "clunker",
                                "target_lang_sense_clarification": "old, run-down car"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "развалюха",
                                "target_lang_sense_clarification": "разговорное, о старой машине"
                            },
                            {
                                "target_lang_word": "драндулет",
                                "target_lang_sense_clarification": "шутливое/пренебрежительное"
                            },
                            {
                                "target_lang_word": "корыто",
                                "target_lang_sense_clarification": "слэнг, о плохой машине"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c50216ba-4efa-3663-bc22-c69776b9c0d4",
                    "sense_definition": "In slechte staat verkerend, bouwvallig of niet stevig.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "broken_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We durfden niet over de <w>wrakke</w> brug te lopen omdat hij kraakte.",
                            "target_lang_translations": {
                                "en": "We didn't dare walk over the <w>rickety</w> bridge because it creaked.",
                                "ru": "Мы не осмелились идти по <w>ветхому</w> мосту, потому что он скрипел."
                            }
                        },
                        {
                            "text": "De oude stoel is zo <w>wrak</w> dat hij instort als je erop gaat zitten.",
                            "target_lang_translations": {
                                "en": "The old chair is so <w>rickety</w> that it collapses if you sit on it.",
                                "ru": "Старый стул настолько <w>хлипкий</w>, что развалится, если на него сесть."
                            }
                        }
                    ],
                    "synonyms": [
                        "wreekte"
                    ],
                    "common_phrases": [
                        "een wrakke stoel",
                        "wrakke gezondheid"
                    ],
                    "target_lang_definitions": {
                        "en": "In poor condition, dilapidated, or not sturdy.",
                        "ru": "Находящийся в плохом состоянии, ветхий или неустойчивый."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rickety",
                                "target_lang_sense_clarification": "unstable or wobbly"
                            },
                            {
                                "target_lang_word": "dilapidated",
                                "target_lang_sense_clarification": "in a state of disrepair"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ветхий",
                                "target_lang_sense_clarification": "обветшалый, старый"
                            },
                            {
                                "target_lang_word": "шаткий",
                                "target_lang_sense_clarification": "неустойчивый"
                            },
                            {
                                "target_lang_word": "хлипкий",
                                "target_lang_sense_clarification": "ненадежный, слабый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b57864df-75ec-3e68-b36c-abdba55c392d",
                    "sense_definition": "Enkelvoud verleden tijd van <w>wreken</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De held <w>wrak</w> de dood van zijn vader in een eerlijk gevecht.",
                            "target_lang_translations": {
                                "en": "The hero <w>avenged</w> his father's death in a fair fight.",
                                "ru": "Герой <w>отомстил</w> за смерть отца в честном бою."
                            }
                        },
                        {
                            "text": "Zij <w>wrak</w> het onrecht dat haar familie was aangedaan.",
                            "target_lang_translations": {
                                "en": "She <w>avenged</w> the injustice done to her family.",
                                "ru": "Она <w>отомстила</w> за несправедливость, причиненную ее семье."
                            }
                        }
                    ],
                    "synonyms": [
                        "wreekte"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Singular past tense of <w>to avenge</w>.",
                        "ru": "Прошедшее время единственного числа от глагола 'wreken' (мстить)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "avenged",
                                "target_lang_sense_clarification": "past action"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отомстил",
                                "target_lang_sense_clarification": "мужской род, прошедшее время"
                            },
                            {
                                "target_lang_word": "отомстила",
                                "target_lang_sense_clarification": "женский род, прошедшее время"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
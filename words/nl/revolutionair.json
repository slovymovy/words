{
    "word_family": [
        "revolutie",
        "revolutioneren"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "128e15f3-e8ba-347f-aee2-9a44d5c4c8ca",
                    "sense_definition": "Totaal anders dan wat er eerder was en daardoor vernieuwend.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "innovation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wetenschapper kwam met een <w>revolutionair</w> idee om energie op te slaan.",
                            "target_lang_translations": {
                                "ru": "Ученый предложил <w>революционную</w> идею хранения энергии."
                            }
                        },
                        {
                            "text": "Deze <w>revolutionaire</w> uitvinding zal de medische wereld voorgoed veranderen.",
                            "target_lang_translations": {
                                "ru": "Это <w>революционное</w> изобретение навсегда изменит мир медицины."
                            }
                        }
                    ],
                    "synonyms": [
                        "vernieuwend",
                        "baanbrekend",
                        "radicaal"
                    ],
                    "common_phrases": [
                        "revolutionair idee",
                        "revolutionaire uitvinding",
                        "revolutionaire verandering",
                        "revolutionair ontwerp"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершенно отличный от того, что было раньше, и поэтому новаторский."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "революционный",
                                "target_lang_sense_clarification": "кардинально новый, прорывной"
                            },
                            {
                                "target_lang_word": "новаторский",
                                "target_lang_sense_clarification": "вносящий новые идеи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b0297d2-7c19-31f3-9faa-fe539128520f",
                    "sense_definition": "Gericht op of kenmerkend voor een plotselinge, radicale verandering van de bestaande politieke of sociale orde.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "political_revolution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>revolutionaire</w> beweging eiste het onmiddellijke aftreden van de regering.",
                            "target_lang_translations": {
                                "ru": "<w>Революционное</w> движение потребовало немедленной отставки правительства."
                            }
                        },
                        {
                            "text": "In die tijd heerste er een <w>revolutionaire</w> sfeer in de straten van de hoofdstad.",
                            "target_lang_translations": {
                                "ru": "В то время на улицах столицы царила <w>революционная</w> атмосфера."
                            }
                        }
                    ],
                    "synonyms": [
                        "oproerig",
                        "opstandig"
                    ],
                    "antonyms": [
                        "antirevolutionair",
                        "contrarevolutionair"
                    ],
                    "common_phrases": [
                        "revolutionaire beweging",
                        "revolutionaire sfeer",
                        "revolutionaire geest",
                        "revolutionaire strijd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направленный на или характерный для внезапного, радикального изменения существующего политического или социального строя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "революционный",
                                "target_lang_sense_clarification": "относящийся к политической революции"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5b795b19-3ae4-30f9-b134-9cefcf4eadeb",
                    "sense_definition": "Iemand die strijdt voor een radicale verandering van de politieke of maatschappelijke orde.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "political_revolution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>revolutionairen</w> kwamen in het geheim bijeen om hun plannen te bespreken.",
                            "target_lang_translations": {
                                "ru": "<w>Революционеры</w> тайно встретились, чтобы обсудить свои планы."
                            }
                        },
                        {
                            "text": "Als jonge <w>revolutionair</w> droomde hij van een eerlijkere samenleving voor iedereen.",
                            "target_lang_translations": {
                                "ru": "Будучи молодым <w>революционером</w>, он мечтал о более справедливом обществе для всех."
                            }
                        }
                    ],
                    "synonyms": [
                        "opstandeling",
                        "rebel"
                    ],
                    "common_phrases": [
                        "jonge revolutionair",
                        "bekende revolutionair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто борется за радикальное изменение политического или общественного строя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "революционер",
                                "target_lang_sense_clarification": "участник революции"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
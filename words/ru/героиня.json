    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "15f356f8-49ed-3f9a-be24-93d3b566a620",
                    "sense_definition": "Женщина, которая совершила подвиг или проявила исключительную смелость, доблесть и самоотверженность.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "heroic_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Жанна д’Арк вошла в историю как национальная <w>героиня</w> Франции.",
                            "target_lang_translations": {
                                "en": "Joan of Arc went down in history as a national <w>heroine</w> of France."
                            }
                        },
                        {
                            "text": "Эта смелая женщина — настоящая <w>героиня</w>, которая спасла детей из огня.",
                            "target_lang_translations": {
                                "en": "This brave woman is a real <w>heroine</w> who saved children from the fire."
                            }
                        }
                    ],
                    "antonyms": [
                        "трусиха"
                    ],
                    "common_phrases": [
                        "национальная героиня",
                        "настоящая героиня",
                        "героиня труда"
                    ],
                    "target_lang_definitions": {
                        "en": "A woman who has performed a heroic deed or displayed exceptional courage, valor, and selflessness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heroine",
                                "target_lang_sense_clarification": "female hero"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fbb1a22-84aa-39c9-9701-9bf78f653869",
                    "sense_definition": "Главное действующее лицо (женского пола) в литературном произведении, фильме или спектакле.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Главная <w>героиня</w> романа преодолевает множество препятствий на пути к счастью.",
                            "target_lang_translations": {
                                "en": "The main <w>heroine</w> of the novel overcomes many obstacles on the path to happiness."
                            }
                        },
                        {
                            "text": "Мне очень понравилась <w>героиня</w> этого фильма своим умом и добротой.",
                            "target_lang_translations": {
                                "en": "I really liked the <w>heroine</w> of this film for her intelligence and kindness."
                            }
                        }
                    ],
                    "synonyms": [
                        "персонаж"
                    ],
                    "common_phrases": [
                        "главная героиня",
                        "героиня романа",
                        "любимая героиня",
                        "героиня фильма"
                    ],
                    "target_lang_definitions": {
                        "en": "The main female character in a literary work, film, or play."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heroine",
                                "target_lang_sense_clarification": "female lead character"
                            }
                        ]
                    }
                }
            ]
        }
    ]

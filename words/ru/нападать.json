    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "672cfc8e-862b-3050-b6fe-44619f26d192",
                    "sense_definition": "Бросаться на кого-либо или что-либо с целью нанести вред, начать драку или войну.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "aggression_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хищник предпочитает <нападать> на свою жертву из засады.",
                            "target_lang_translations": {
                                "en": "The predator prefers to <attack> its prey from an ambush."
                            }
                        },
                        {
                            "text": "Вражеская армия планировала <нападать> на город ранним утром.",
                            "target_lang_translations": {
                                "en": "The enemy army planned to <attack> the city early in the morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "атаковать",
                        "штурмовать",
                        "набрасываться"
                    ],
                    "antonyms": [

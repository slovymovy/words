{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "d63bc3dc-8976-3dd5-ad18-aff0b1d879a3",
                    "sense_definition": "Before the specified duration ends.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please return the completed form <w>within</w> two days.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, верните заполненную форму <w>в течение</w> двух дней."
                            }
                        },
                        {
                            "text": "The ambulance arrived <w>within</w> minutes of the call.",
                            "target_lang_translations": {
                                "ru": "Скорая помощь прибыла <w>в течение</w> нескольких минут после вызова."
                            }
                        }
                    ],
                    "common_phrases": [
                        "within a week",
                        "within days",
                        "within hours",
                        "within a year"
                    ],
                    "target_lang_definitions": {
                        "ru": "До окончания указанного периода времени; не позднее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в течение",
                                "target_lang_sense_clarification": "продолжительность времени"
                            },
                            {
                                "target_lang_word": "не позднее",
                                "target_lang_sense_clarification": "крайний срок"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "за период времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89e7dacb-ca76-3867-a816-d8d11ecb2095",
                    "sense_definition": "In the inner part, spatially; physically inside.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_inside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many people stayed <w>within</w> their homes during the storm.",
                            "target_lang_translations": {
                                "ru": "Многие люди оставались <w>внутри</w> своих домов во время шторма."
                            }
                        },
                        {
                            "text": "There is a beautiful garden located <w>within</w> the castle walls.",
                            "target_lang_translations": {
                                "ru": "Прекрасный сад расположен <w>внутри</w> крепостных стен."
                            }
                        }
                    ],
                    "antonyms": [
                        "outside"
                    ],
                    "common_phrases": [
                        "within the walls",
                        "within the city",
                        "from within"
                    ],
                    "target_lang_definitions": {
                        "ru": "Во внутренней части чего-либо; физически внутри."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутри",
                                "target_lang_sense_clarification": "предлог места"
                            },
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "местонахождение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47a53b48-bc13-3efe-8a76-9348124f987f",
                    "sense_definition": "In the scope or range of.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to find a solution that is <w>within</w> our budget.",
                            "target_lang_translations": {
                                "ru": "Нам нужно найти решение <w>в рамках</w> нашего бюджета."
                            }
                        },
                        {
                            "text": "The finish line was finally <w>within</w> sight.",
                            "target_lang_translations": {
                                "ru": "Финишная черта была наконец <w>в пределах</w> видимости."
                            }
                        }
                    ],
                    "antonyms": [
                        "outside"
                    ],
                    "common_phrases": [
                        "within reach",
                        "within reason",
                        "within sight",
                        "within power"
                    ],
                    "target_lang_definitions": {
                        "ru": "В сфере действия, в диапазоне или в границах чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в пределах",
                                "target_lang_sense_clarification": "расстояние, видимость, досягаемость"
                            },
                            {
                                "target_lang_word": "в рамках",
                                "target_lang_sense_clarification": "бюджет, правила, ограничения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7d9fd1b6-9e71-38f4-ae20-19311c12f513",
                    "sense_definition": "In or into the interior; inside.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_inside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The box was plain on the outside but lined with velvet <w>within</w>.",
                            "target_lang_translations": {
                                "ru": "Коробка была простой снаружи, но обшитой бархатом <w>внутри</w>."
                            }
                        },
                        {
                            "text": "She felt a sudden peace from <w>within</w>.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала внезапное умиротворение, исходящее <w>изнутри</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "outside"
                    ],
                    "common_phrases": [
                        "from within",
                        "look within"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутри или внутрь; во внутренней части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутри",
                                "target_lang_sense_clarification": "наречие места"
                            },
                            {
                                "target_lang_word": "изнутри",
                                "target_lang_sense_clarification": "направление из центра"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "aeb41d32-774c-34d5-92f4-41b69d063fc0",
                    "sense_definition": "In the context of which the present document or ruling is made.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge dismissed the <w>within</w> appeal due to lack of evidence.",
                            "target_lang_translations": {
                                "ru": "Судья отклонил <w>данную</w> апелляцию из-за недостатка улик."
                            }
                        },
                        {
                            "text": "Please refer to the terms specified in the <w>within</w> agreement.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, обратитесь к условиям, указанным в <w>настоящем</w> соглашении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the within appeal",
                        "the within document"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к данному документу или содержащийся в нём (юридический термин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "данный",
                                "target_lang_sense_clarification": "упомянутый"
                            },
                            {
                                "target_lang_word": "настоящий",
                                "target_lang_sense_clarification": "текущий документ"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
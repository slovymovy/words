                },
                {
                    "sense_id": "7f00751d-85a1-33cb-8a1d-f560d61e4428",
                    "sense_definition": "De elfde zoon van aartsvader Jakob in de Bijbel, die onderkoning van Egypte werd.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "biblical_figures",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "<w>Jozef</w> werd door zijn jaloerse broers verkocht.",
                            "target_lang_translations": {

{
    "word_family": [
        "allot",
        "allotment",
        "lottery"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f9c593bc-641e-3a0b-8cdf-44a93f2c7ff2",
                    "sense_definition": "A large number, amount, or quantity of people or things.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a <w>lot</w> of friends in the city.",
                            "target_lang_translations": {
                                "ru": "У неё <w>много</w> друзей в городе."
                            }
                        },
                        {
                            "text": "There is a <w>lot</w> of work to be done today.",
                            "target_lang_translations": {
                                "ru": "Сегодня нужно сделать <w>кучу</w> работы."
                            }
                        },
                        {
                            "text": "Thanks a <w>lot</w> for your help.",
                            "target_lang_translations": {
                                "ru": "<w>Большое</w> спасибо за помощь."
                            }
                        }
                    ],
                    "synonyms": [
                        "load",
                        "heap",
                        "pile",
                        "mass",
                        "ton",
                        "abundance",
                        "great deal",
                        "mountain",
                        "slew"
                    ],
                    "antonyms": [
                        "modicum"
                    ],
                    "common_phrases": [
                        "a lot of",
                        "lots of",
                        "a whole lot",
                        "thanks a lot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество людей или вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "много",
                                "target_lang_sense_clarification": "наречие, обозначающее большое количество"
                            },
                            {
                                "target_lang_word": "куча",
                                "target_lang_sense_clarification": "разговорное: большое количество чего-либо"
                            },
                            {
                                "target_lang_word": "масса",
                                "target_lang_sense_clarification": "множество, большое количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e5256e7-69ba-3a58-b533-f227bb61063b",
                    "sense_definition": "A distinct piece of land, often used for a specific purpose like building or parking.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "land_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We parked the car in the empty <w>lot</w>.",
                            "target_lang_translations": {
                                "ru": "Мы припарковали машину на пустой <w>площадке</w>."
                            }
                        },
                        {
                            "text": "They bought a <w>lot</w> to build a new house.",
                            "target_lang_translations": {
                                "ru": "Они купили <w>участок</w>, чтобы построить новый дом."
                            }
                        }
                    ],
                    "synonyms": [
                        "plot",
                        "parcel"
                    ],
                    "common_phrases": [
                        "parking lot",
                        "vacant lot",
                        "building lot"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный участок земли, часто используемый для строительства или парковки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "кусок земли"
                            },
                            {
                                "target_lang_word": "площадка",
                                "target_lang_sense_clarification": "ровное место, специально оборудованное для чего-либо (например, парковки)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dee97d59-6988-3f52-952d-0533a5ec76be",
                    "sense_definition": "The whole quantity or number of things or people; everything.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am tired of the <w>lot</w> of you.",
                            "target_lang_translations": {
                                "ru": "Я устал от <w>всех</w> вас."
                            }
                        },
                        {
                            "text": "He was hungry and ate the <w>lot</w>.",
                            "target_lang_translations": {
                                "ru": "Он был голоден и съел <w>всё</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "collection",
                        "group",
                        "set"
                    ],
                    "common_phrases": [
                        "the lot",
                        "the whole lot"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вся совокупность вещей или людей; всё вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всё",
                                "target_lang_sense_clarification": "полный объем или количество"
                            },
                            {
                                "target_lang_word": "все",
                                "target_lang_sense_clarification": "о людях: все присутствующие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c57698fa-08c0-3dd3-9d7c-5b907a96650d",
                    "sense_definition": "A specific group of people, often considered together.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "quantity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You <w>lot</w> need to hurry up.",
                            "target_lang_translations": {
                                "ru": "Эй вы, <w>народ</w>, вам нужно поторопиться."
                            }
                        },
                        {
                            "text": "Stay away from that bad <w>lot</w>.",
                            "target_lang_translations": {
                                "ru": "Держись подальше от этой дурной <w>компании</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "crowd",
                        "gang",
                        "group"
                    ],
                    "common_phrases": [
                        "you lot",
                        "bad lot"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенная группа людей, часто рассматриваемая как единое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компания",
                                "target_lang_sense_clarification": "группа людей, проводящих время вместе"
                            },
                            {
                                "target_lang_word": "группа",
                                "target_lang_sense_clarification": "совокупность людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afb5cfb0-a8ef-388e-899e-197337e856a9",
                    "sense_definition": "A person's luck, situation, or destiny in life.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "fate_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was his <w>lot</w> to become a soldier.",
                            "target_lang_translations": {
                                "ru": "Ему выпала <w>участь</w> стать солдатом."
                            }
                        },
                        {
                            "text": "She seemed content with her <w>lot</w> in life.",
                            "target_lang_translations": {
                                "ru": "Она казалась довольной своей <w>судьбой</w> в жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "fate",
                        "destiny",
                        "fortune",
                        "portion"
                    ],
                    "common_phrases": [
                        "lot in life",
                        "cast in one's lot"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Судьба, положение или участь человека в жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участь",
                                "target_lang_sense_clarification": "доля, судьба, часто с оттенком неизбежности"
                            },
                            {
                                "target_lang_word": "судьба",
                                "target_lang_sense_clarification": "складывающийся ход жизни"
                            },
                            {
                                "target_lang_word": "доля",
                                "target_lang_sense_clarification": "жизненный удел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acafaa2c-a7f5-3d60-bc06-10aa93ba260c",
                    "sense_definition": "A specific item or set of items offered for sale, especially at an auction.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "auction_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The next <w>lot</w> is a rare painting.",
                            "target_lang_translations": {
                                "ru": "Следующий <w>лот</w> — редкая картина."
                            }
                        },
                        {
                            "text": "He bid on a <w>lot</w> of antique furniture.",
                            "target_lang_translations": {
                                "ru": "Он сделал ставку на <w>лот</w> старинной мебели."
                            }
                        }
                    ],
                    "synonyms": [
                        "batch",
                        "group",
                        "collection"
                    ],
                    "common_phrases": [
                        "auction lot",
                        "job lot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный предмет или набор предметов, выставленных на продажу, особенно на аукционе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лот",
                                "target_lang_sense_clarification": "единица купли-продажи на аукционе"
                            },
                            {
                                "target_lang_word": "партия",
                                "target_lang_sense_clarification": "группа товаров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d19406b5-258d-37b6-9bba-7bab1b9493bf",
                    "sense_definition": "An object used to make a decision by chance, such as a straw or slip of paper.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fate_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to draw <w>lots</w> to see who would go first.",
                            "target_lang_translations": {
                                "ru": "Они решили тянуть <w>жребий</w>, чтобы узнать, кто пойдет первым."
                            }
                        },
                        {
                            "text": "The soldiers cast <w>lots</w> for the prize.",
                            "target_lang_translations": {
                                "ru": "Солдаты бросали <w>жребий</w> за приз."
                            }
                        }
                    ],
                    "synonyms": [
                        "chance"
                    ],
                    "common_phrases": [
                        "cast lots",
                        "draw lots"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, используемый для принятия решения наугад, например, соломинка или листок бумаги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жребий",
                                "target_lang_sense_clarification": "условный предмет, вынимаемый наудачу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [

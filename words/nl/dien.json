                    "sense_definition": "Vervoeging van <w>dienen</w>: eerste persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>dien</w> mijn ontslag in omdat ik een nieuwe baan heb gevonden.",
                            "target_lang_translations": {
                                "en": "I am <w>submitting</w> my resignation because I have found a new job.",
                                "ru": "Я <w>подаю</w> заявление об увольнении, так как нашел новую работу."
                            }
                        },
                        {
                            "text": "Waarmee kan ik u <w>dien</w>?",
                            "target_lang_translations": {
                                "en": "How may I <w>serve</w> you?",
                                "ru": "Чем я могу вам <w>служить</w>?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "waarmee kan ik u dienen",
                        "een klacht indienen",
                        "het eten opdienen"

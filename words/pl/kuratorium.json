{
    "word_family": [
        "kurator",
        "kuratela",
        "kuratorski"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec4cf30b-9601-37f8-ab11-2fa9fc97e6df",
                    "sense_definition": "Instytucja państwowa sprawująca nadzór pedagogiczny nad szkołami i placówkami oświatowymi w danym województwie",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice złożyli oficjalną skargę do <w>kuratorium</w> na dyrektora szkoły.",
                            "target_lang_translations": {
                                "ru": "Родители подали официальную жалобу в <w>управление образования</w> на директора школы."
                            }
                        },
                        {
                            "text": "Miejscowe <w>kuratorium</w> oświaty ogłosiło wyniki konkursu przedmiotowego dla uczniów.",
                            "target_lang_translations": {
                                "ru": "Местное <w>управление образования</w> объявило результаты предметной олимпиады для учеников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kuratorium oświaty",
                        "pracownik kuratorium",
                        "kontrola z kuratorium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государственное учреждение, осуществляющее педагогический надзор за школами и образовательными учреждениями в данном воеводстве"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управление образования",
                                "target_lang_sense_clarification": "административный орган"
                            },
                            {
                                "target_lang_word": "кураториум",
                                "target_lang_sense_clarification": "в системе образования Польши"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c261805-0128-351e-a737-2530bb088e72",
                    "sense_definition": "Urząd lub obowiązki kuratora, zwłaszcza w kontekście sprawowania opieki prawnej",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd powierzył mu <w>kuratorium</w> nad majątkiem osoby ubezwłasnowolnionej.",
                            "target_lang_translations": {
                                "ru": "Суд поручил ему <w>попечительство</w> над имуществом недееспособного лица."
                            }
                        },
                        {
                            "text": "Sprawowanie <w>kuratorium</w> wymaga dużej odpowiedzialności oraz wiedzy prawniczej.",
                            "target_lang_translations": {
                                "ru": "Осуществление <w>попечительства</w> требует большой ответственности и юридических знаний."
                            }
                        }
                    ],
                    "synonyms": [
                        "kuratorstwo"
                    ],
                    "common_phrases": [
                        "objąć kuratorium",
                        "sprawować kuratorium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Должность или обязанности куратора, особенно в контексте юридической опеки"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попечительство",
                                "target_lang_sense_clarification": "надзор над имуществом или лицом"
                            },
                            {
                                "target_lang_word": "кураторство",
                                "target_lang_sense_clarification": "обязанности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb8f09c8-37c5-3766-baa1-75f1937d255e",
                    "sense_definition": "Zespół kuratorów zarządzający fundacją, uczelnią lub inną instytucją",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Kuratorium</w> fundacji zebrało się, aby omówić budżet na przyszły rok.",
                            "target_lang_translations": {
                                "ru": "<w>Попечительский совет</w> фонда собрался, чтобы обсудить бюджет на следующий год."
                            }
                        },
                        {
                            "text": "Ostateczna decyzja w tej sprawie należy do <w>kuratorium</w> uniwersytetu.",
                            "target_lang_translations": {
                                "ru": "Окончательное решение по этому вопросу принадлежит <w>попечительскому совету</w> университета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "członek kuratorium",
                        "posiedzenie kuratorium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совет кураторов, управляющий фондом, университетом или другим учреждением"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попечительский совет",
                                "target_lang_sense_clarification": "орган управления организацией"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
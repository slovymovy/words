{
    "word_family": [
        "onto",
        "onward",
        "onwards",
        "ongoing",
        "online",
        "onset"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "7028463d-386e-3312-963c-c8f22023aa69",
                    "sense_definition": "Positioned at the upper surface of something, touching it from above.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a large book <w>on</w> the desk.",
                            "target_lang_translations": {
                                "ru": "<w>На</w> столе лежит большая книга."
                            }
                        },
                        {
                            "text": "Please do not sit <w>on</w> the floor.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не сиди <w>на</w> полу."
                            }
                        }
                    ],
                    "synonyms": [
                        "over"
                    ],
                    "common_phrases": [
                        "on the table",
                        "on the floor",
                        "on top",
                        "on the ground"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный на верхней поверхности чего-либо, касаясь её сверху."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "указывает на нахождение предмета на поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f05d0340-888d-3df4-a297-8752ec34ca73",
                    "sense_definition": "Used to indicate a specific day or date.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_day",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will see you <w>on</w> Monday morning.",
                            "target_lang_translations": {
                                "ru": "Увидимся <w>в</w> понедельник утром."
                            }
                        },
                        {
                            "text": "Her birthday is <w>on</w> the fourth of July.",
                            "target_lang_translations": {
                                "ru": "Её день рождения приходится <w>на</w> четвёртое июля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on Monday",
                        "on the weekend",
                        "on time",
                        "on a daily basis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания конкретного дня или даты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "с днями недели (в понедельник)"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "с датами или событиями (на этой неделе, на выходных)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d2b0aea-2237-39de-b401-27f0f645f358",
                    "sense_definition": "Using a machine, device, or medium; transmitted by means of.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "medium",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We watched a very interesting documentary <w>on</w> television.",
                            "target_lang_translations": {
                                "ru": "Мы посмотрели очень интересный документальный фильм <w>по</w> телевизору."
                            }
                        },
                        {
                            "text": "She is currently talking <w>on</w> the phone.",
                            "target_lang_translations": {
                                "ru": "Она сейчас говорит <w>по</w> телефону."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on TV",
                        "on the radio",
                        "on the phone",
                        "on the internet",
                        "on screen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используя машину, устройство или средство связи; передаваемый посредством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "указывает на средство связи (по телефону, по телевизору)"
                            },
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "в среде (в интернете)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57537337-f417-39f2-b801-7b4c4d7de831",
                    "sense_definition": "Aboard a public vehicle or a mode of transport.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There were too many people <w>on</w> the bus.",
                            "target_lang_translations": {
                                "ru": "<w>В</w> автобусе было слишком много людей."
                            }
                        },
                        {
                            "text": "We travelled <w>on</w> a train across the country.",
                            "target_lang_translations": {
                                "ru": "Мы путешествовали по стране <w>на</w> поезде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on a bus",
                        "on a plane",
                        "on board",
                        "on a bike"
                    ],
                    "target_lang_definitions": {
                        "ru": "На борту общественного транспорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "нахождение внутри транспорта (в автобусе)"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "перемещение с помощью транспорта (на поезде)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "708aa2bc-73d2-3eec-95af-829460b965ae",
                    "sense_definition": "Attached to, covering, or resting against an outer surface.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_attached",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She hung a beautiful painting <w>on</w> the wall.",
                            "target_lang_translations": {
                                "ru": "Она повесила красивую картину <w>на</w> стену."
                            }
                        },
                        {
                            "text": "He wears a gold ring <w>on</w> his finger.",
                            "target_lang_translations": {
                                "ru": "Он носит золотое кольцо <w>на</w> пальце."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the wall",
                        "on the side",
                        "on the ceiling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикреплённый к, покрывающий или опирающийся на внешнюю поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "указывает на прикрепление к поверхности (на стене, на пальце)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e69065b2-c27d-31ff-96ba-7e1e0dc7c180",
                    "sense_definition": "Concerning; dealing with the subject of.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "topic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am reading a new book <w>on</w> European history.",
                            "target_lang_translations": {
                                "ru": "Я читаю новую книгу <w>по</w> истории Европы."
                            }
                        },
                        {
                            "text": "They finally reached an agreement <w>on</w> the price.",
                            "target_lang_translations": {
                                "ru": "Они наконец достигли соглашения <w>о</w> цене."
                            }
                        }
                    ],
                    "synonyms": [
                        "about",
                        "apropos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Касательно; имеющий отношение к теме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "указывает на область знаний (книга по истории)"
                            },
                            {
                                "target_lang_word": "о",
                                "target_lang_sense_clarification": "о предмете обсуждения (соглашение о цене)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d31557e6-a79d-3d9b-94a1-525157b25a12",
                    "sense_definition": "Engaged in or occupied with a specific activity or state.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager is away <w>on</w> business this week.",
                            "target_lang_translations": {
                                "ru": "Менеджер на этой неделе в отъезде <w>по</w> делам."
                            }
                        },
                        {
                            "text": "We are going <w>on</w> holiday to Spain next month.",
                            "target_lang_translations": {
                                "ru": "В следующем месяце мы едем <w>в</w> отпуск в Испанию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on holiday",
                        "on vacation",
                        "on sale",
                        "on strike",
                        "on fire"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вовлечённый в определённую деятельность или состояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "нахождение в состоянии (в отпуске, в командировке)"
                            },
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "цель деятельности (по делам)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f87a0bfc-54dc-3b17-8762-c81ebc2b385e",
                    "sense_definition": "Serving as a member of a group or organization.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "membership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has served <w>on</w> the committee for three years.",
                            "target_lang_translations": {
                                "ru": "Она проработала <w>в</w> комитете три года."
                            }
                        },
                        {
                            "text": "Who is currently <w>on</w> the board of directors?",
                            "target_lang_translations": {
                                "ru": "Кто сейчас входит <w>в</w> совет директоров?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the committee",
                        "on the jury",
                        "on the team",
                        "on the board"
                    ],
                    "target_lang_definitions": {
                        "ru": "Являющийся членом группы или организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "членство в составе (в комитете, в совете)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56971b34-397b-3749-8b43-1e2801946b13",
                    "sense_definition": "Supported by or sustained by means of.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sustenance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This toy car runs <w>on</w> batteries.",
                            "target_lang_translations": {
                                "ru": "Эта игрушечная машинка работает <w>на</w> батарейках."
                            }
                        },
                        {
                            "text": "It is difficult to live <w>on</w> such a small salary.",
                            "target_lang_translations": {
                                "ru": "Трудно жить <w>на</w> такую маленькую зарплату."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддерживаемый или работающий за счёт чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "источник энергии или средств (на батарейках, на зарплату)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "116c2996-d617-3807-a4d7-a9b5d5da8144",
                    "sense_definition": "Indicating dependence, reliance, or a basis for something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "dependence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can always depend <w>on</w> me for help.",
                            "target_lang_translations": {
                                "ru": "Ты всегда можешь рассчитывать <w>на</w> мою помощь."
                            }
                        },
                        {
                            "text": "We made the decision based <w>on</w> the available facts.",
                            "target_lang_translations": {
                                "ru": "Мы приняли решение, основываясь <w>на</w> имеющихся фактах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "depend on",
                        "rely on",
                        "count on",
                        "based on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на зависимость, опору или основание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "опора или основание (полагаться на, основано на)"
                            },
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "зависимость (зависеть от)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b66abd6b-90a9-31ef-b148-1aa5310fc272",
                    "sense_definition": "In the possession of; carried by a person.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not have any cash <w>on</w> me right now.",
                            "target_lang_translations": {
                                "ru": "У меня сейчас нет <w>с</w> собой наличных."
                            }
                        },
                        {
                            "text": "The police found a weapon <w>on</w> the suspect.",
                            "target_lang_translations": {
                                "ru": "Полиция обнаружила оружие <w>при</w> подозреваемом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Во владении; имеющийся при себе у человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "наличие при себе (с собой)"
                            },
                            {
                                "target_lang_word": "при",
                                "target_lang_sense_clarification": "обнаружение у кого-либо (при нём)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8dd5cb84-6b6e-32bc-817f-06da44bf942f",
                    "sense_definition": "Paid for by.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Put your wallet away because the drinks are <w>on</w> me.",
                            "target_lang_translations": {
                                "ru": "Убери кошелёк, потому что напитки <w>за</w> мой <w>счёт</w>."
                            }
                        },
                        {
                            "text": "The restaurant owner said the dessert was <w>on</w> the house.",
                            "target_lang_translations": {
                                "ru": "Владелец ресторана сказал, что десерт <w>за счёт</w> заведения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the house",
                        "on me"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оплачиваемый кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за счёт",
                                "target_lang_sense_clarification": "источник оплаты (за счёт заведения)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdb56cfd-f857-3deb-9add-53c6bdafb886",
                    "sense_definition": "Indicating the target or object of an action or feeling.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "target",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The army launched a surprise attack <w>on</w> the capital.",
                            "target_lang_translations": {
                                "ru": "Армия начала внезапное нападение <w>на</w> столицу."
                            }
                        },
                        {
                            "text": "Don't be so hard <w>on</w> yourself when you make a mistake.",
                            "target_lang_translations": {
                                "ru": "Не будь так строг <w>к</w> себе, когда совершаешь ошибку."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на цель или объект действия или чувства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "направленность действия (нападение на)"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "отношение к кому-либо (строг к)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb733689-e54f-3dbb-a733-55e8b2ebfb70",
                    "sense_definition": "Immediately after; at the moment of.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>On</w> arriving at the station, he called his wife.",
                            "target_lang_translations": {
                                "ru": "<w>По</w> прибытии на станцию он позвонил жене."
                            }
                        },
                        {
                            "text": "<w>On</w> further reflection, I think you are right.",
                            "target_lang_translations": {
                                "ru": "<w>По</w> зрелом размышлении, я думаю, вы правы."
                            }
                        }
                    ],
                    "synonyms": [
                        "after"
                    ],
                    "common_phrases": [
                        "on arrival"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сразу после; в момент совершения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "после совершения действия (по прибытии)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c622563e-aab3-3428-98eb-d2ca3f996a6d",
                    "sense_definition": "Into a position of covering, being fitted, or being worn.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "covering_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is cold outside, so put your coat <w>on</w>.",
                            "target_lang_translations": {
                                "ru": "На улице холодно, так что <w>надень</w> пальто."
                            }
                        },
                        {
                            "text": "She tried the shoes <w>on</w> to see if they fit.",
                            "target_lang_translations": {
                                "ru": "Она <w>примерила</w> туфли, чтобы проверить, подходят ли они."
                            }
                        }
                    ],
                    "antonyms": [
                        "off"
                    ],
                    "common_phrases": [
                        "put on",
                        "try on"
                    ],
                    "target_lang_definitions": {
                        "ru": "В положение, когда что-то надето или покрывает."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "надевать на себя (часто часть глагола)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f72c6e4-42c8-3d6c-9091-7470afd8ab57",
                    "sense_definition": "To a state of functioning or operation.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "operating_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please turn the light <w>on</w> so we can see.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>включи</w> свет, чтобы мы могли видеть."
                            }
                        },
                        {
                            "text": "He switched the computer <w>on</w> to check his email.",
                            "target_lang_translations": {
                                "ru": "Он <w>включил</w> компьютер, чтобы проверить почту."
                            }
                        }
                    ],
                    "antonyms": [
                        "off"
                    ],
                    "common_phrases": [
                        "turn on",
                        "switch on"
                    ],
                    "target_lang_definitions": {
                        "ru": "В состояние функционирования или работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включить",
                                "target_lang_sense_clarification": "привести в действие (глагол)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18bbdeb9-e9f7-3507-bf44-a6fe89ef285f",
                    "sense_definition": "Continuing an action; moving forward or ahead.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "continuation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We must walk <w>on</w> until we find the path.",
                            "target_lang_translations": {

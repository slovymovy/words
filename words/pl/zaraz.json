    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4e3405b9-179f-335a-b360-7aaa88c1b0dc",
                    "sense_definition": "W krótkim czasie od chwili mówienia; wkrótce.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poczekaj jeszcze chwilę, <w>zaraz</w> przyjdę ci pomóc.",
                            "target_lang_translations": {
                                "ru": "Подожди ещё минуту, я <w>сейчас</w> приду тебе помочь."
                            }
                        },
                        {
                            "text": "<w>Zaraz</w> będzie padać, więc weź parasol.",
                            "target_lang_translations": {
                                "ru": "<w>Скоро</w> пойдёт дождь, так что возьми зонт."
                            }
                        }
                    ],
                    "synonyms": [
                        "wkrótce",
                        "niedługo",
                        "za moment",
                        "niebawem",
                        "w niedługim czasie"
                    ],
                    "antonyms": [
                        "potem",
                        "później"
                    ],
                    "common_phrases": [
                        "zaraz wracam",
                        "zaraz będzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Через короткое время; скоро, сейчас."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сейчас",
                                "target_lang_sense_clarification": "в скором времени"
                            },
                            {
                                "target_lang_word": "скоро",
                                "target_lang_sense_clarification": "в ближайшем будущем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c18034e2-4bbd-3cc7-a2eb-f13fbb3f8a2c",
                    "sense_definition": "W bezpośrednim sąsiedztwie; bardzo blisko (zwykle w odniesieniu do miejsca).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "space_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sklep spożywczy jest <w>zaraz</w> obok tego budynku.",
                            "target_lang_translations": {
                                "ru": "Продуктовый магазин находится <w>прямо</w> рядом с этим зданием."
                            }
                        },
                        {
                            "text": "Szkoła znajduje się <w>zaraz</w> za rogiem.",
                            "target_lang_translations": {
                                "ru": "Школа находится <w>сразу</w> за углом."
                            }
                        }
                    ],
                    "synonyms": [
                        "tuż",
                        "blisko",
                        "w pobliżu"
                    ],
                    "antonyms": [
                        "daleko",
                        "w oddali"
                    ],
                    "common_phrases": [
                        "zaraz obok",
                        "zaraz za",
                        "zaraz przy"
                    ],
                    "target_lang_definitions": {
                        "ru": "В непосредственной близости; совсем рядом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сразу",
                                "target_lang_sense_clarification": "непосредственно за чем-то"
                            },
                            {
                                "target_lang_word": "прямо",
                                "target_lang_sense_clarification": "совсем рядом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb1ef321-bd5a-3d9b-8462-2f20c5bf899c",
                    "sense_definition": "W tym samym momencie, bez żadnego odstępu czasu; natychmiast.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_immediate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz to zrobić <w>zaraz</w>, nie ma czasu do stracenia.",
                            "target_lang_translations": {
                                "ru": "Ты должен сделать это <w>немедленно</w>, нельзя терять времени."
                            }
                        },
                        {
                            "text": "Odpowiedział na moje pytanie <w>zaraz</w> po jego usłyszeniu.",
                            "target_lang_translations": {
                                "ru": "Он ответил на мой вопрос <w>сразу</w> после того, как услышал его."
                            }
                        }
                    ],
                    "synonyms": [
                        "natychmiast",
                        "od razu",
                        "niezwłocznie",
                        "z miejsca"
                    ],
                    "common_phrases": [
                        "zaraz po"
                    ],
                    "target_lang_definitions": {
                        "ru": "В тот же момент, без промедления; немедленно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сразу",
                                "target_lang_sense_clarification": "без задержки"
                            },
                            {
                                "target_lang_word": "немедленно",
                                "target_lang_sense_clarification": "безотлагательно"
                            },
                            {
                                "target_lang_word": "тут же",
                                "target_lang_sense_clarification": "мгновенно"
                            }
                        ]
                    }
                }
            ]
        },
        {

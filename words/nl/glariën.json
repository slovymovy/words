    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e5b93445-86ba-3e42-a7af-ea63a8315e60",
                    "sense_definition": "Met glimmende ogen of een wazige blik staren, vaak als gevolg van ziekte of hevige emoties.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zieke man lag urenlang stil naar het plafond te <w>glariën</w>.",
                            "target_lang_translations": {
                                "en": "The sick man lay quietly <w>staring glassily</w> at the ceiling for hours.",
                                "ru": "Больной мужчина часами лежал тихо, <глядя остекленевшим взглядом> в потолок."
                            }
                        },
                        {
                            "text": "Hij zat met een verwilderde blik voor zich uit te <w>glariën</w>.",
                            "target_lang_translations": {
                                "en": "He sat <w>gazing vacantly</w> ahead with a wild look in his eyes.",
                                "ru": "Он сидел, <уставившись> перед собой диким взглядом."
                            }
                        }
                    ],
                    "synonyms": [
                        "glaren"
                    ],
                    "target_lang_definitions": {
                        "en": "To stare with shiny eyes or a hazy gaze, often as a result of illness or intense emotions.",
                        "ru": "Смотреть блестящими глазами или мутным взглядом, часто вследствие болезни или сильных эмоций."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stare glassily",
                                "target_lang_sense_clarification": "with a fixed, shiny gaze"
                            },
                            {
                                "target_lang_word": "gaze vacantly",
                                "target_lang_sense_clarification": "without focus or expression"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "смотреть остекленевшим взглядом",
                                "target_lang_sense_clarification": "смотреть неподвижно, часто при болезни"
                            },
                            {
                                "target_lang_word": "уставиться",
                                "target_lang_sense_clarification": "смотреть пристально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfa54a3a-e561-3171-844f-f4db817cb675",
                    "sense_definition": "Licht uitstralen of weerspiegelen, vaak op een flikkerende manier.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op een heldere winteravond <w>glariën</w> de sterren aan de hemel.",
                            "target_lang_translations": {
                                "en": "On a clear winter evening, the stars <w>shimmer</w> in the sky.",
                                "ru": "Ясным зимним вечером звезды <мерцают> в небе."
                            }
                        },
                        {
                            "text": "De kostbare diamanten <w>glariën</w> in het felle lamplicht.",
                            "target_lang_translations": {
                                "en": "The precious diamonds <w>glitter</w> in the bright lamplight.",
                                "ru": "Драгоценные бриллианты <сверкают> в ярком свете лампы."
                            }
                        }
                    ],
                    "synonyms": [
                        "flonkeren",
                        "glinsteren",
                        "schitteren",
                        "glanzen"
                    ],
                    "target_lang_definitions": {
                        "en": "To emit or reflect light, often in a flickering way.",
                        "ru": "Излучать или отражать свет, часто мерцая."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "glitter",
                                "target_lang_sense_clarification": "shine with bright points of light"
                            },
                            {
                                "target_lang_word": "gleam",
                                "target_lang_sense_clarification": "shine brightly"
                            },
                            {
                                "target_lang_word": "shimmer",
                                "target_lang_sense_clarification": "shine with a wavering light"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мерцать",
                                "target_lang_sense_clarification": "светить неровным светом"
                            },
                            {
                                "target_lang_word": "сверкать",
                                "target_lang_sense_clarification": "ярко блестеть"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
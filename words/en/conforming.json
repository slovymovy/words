                    "synonyms": [
                        "compliant",
                        "obedient",
                        "submissive"
                    ],
                    "antonyms": [
                        "nonconforming",
                        "rebellious",
                        "deviant"
                    ],
                    "common_phrases": [
                        "conforming loan",
                        "conforming product"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий установленным правилам, стандартам или образцам; послушный."
                    },
                    "translations": {
                        "ru": [
                            {

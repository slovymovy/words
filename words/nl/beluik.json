{
    "word_family": [
        "luik",
        "beloken"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4b74cd05-9ab2-3fae-9831-785c0387fff2",
                    "sense_definition": "Een groep kleine arbeiderswoningen gebouwd rond een binnenplaats of steegje, vaak niet zichtbaar vanaf de straat.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "housing_complex",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De studenten wonen tegenwoordig in een mooi gerenoveerd <w>beluik</w> in het centrum.",
                            "target_lang_translations": {
                                "ru": "Студенты в наши дни живут в красиво отреставрированном <w>жилом дворике</w> в центре."
                            }
                        },
                        {
                            "text": "Vroeger woonden arme gezinnen dicht op elkaar in een donker en vochtig <w>beluik</w>.",
                            "target_lang_translations": {
                                "ru": "Раньше бедные семьи жили в тесноте в темном и сыром <w>тупике</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "hofje"
                    ],
                    "common_phrases": [
                        "een Gents beluik",
                        "wonen in een beluik"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België (Vlaanderen)"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "verwijst vaak naar 19e-eeuwse arbeidershuisvesting"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа небольших рабочих домиков, построенных вокруг внутреннего двора или переулка, часто скрытая от улицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жилой дворик",
                                "target_lang_sense_clarification": "комплекс жилых домов вокруг двора"
                            },
                            {
                                "target_lang_word": "тупик",
                                "target_lang_sense_clarification": "застроенный переулок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abb603a0-6e47-3d19-aea0-46583dab4040",
                    "sense_definition": "Een omsloten terrein of afgesloten ruimte, vaak behorend bij een groter gebouw.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "enclosed_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De monniken wandelden elke ochtend in stilte binnen het <w>beluik</w> van de abdij.",
                            "target_lang_translations": {
                                "ru": "Каждое утро монахи молча прогуливались по <w>внутреннему двору</w> аббатства."
                            }
                        },
                        {
                            "text": "Het feest vond plaats in het veilige <w>beluik</w> van de oude stadsmuren.",
                            "target_lang_translations": {
                                "ru": "Праздник проходил под надежной защитой <w>ограды</w> старых городских стен."
                            }
                        }
                    ],
                    "synonyms": [
                        "hofje"
                    ],
                    "common_phrases": [
                        "binnen het beluik"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Огороженная территория или замкнутое пространство, часто принадлежащее большому зданию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутренний двор",
                                "target_lang_sense_clarification": "при здании или монастыре"
                            },
                            {
                                "target_lang_word": "ограда",
                                "target_lang_sense_clarification": "огороженное пространство (устар.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>stijf</w> de kraag van mijn overhemd zodat hij netjes blijft staan.",
                            "target_lang_translations": {
                                "en": "I <w>starch</w> the collar of my shirt so it stays neat.",

            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4db9c06a-5234-37e1-8bae-1aa3beca0ab0",
                    "sense_definition": "Ощущая, выражая или причиняя боль, физические или душевные страдания",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pain_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он очень <w>болезненно</w> реагировал на любые замечания в свой адрес.",
                            "target_lang_translations": {
                                "en": "He reacted very <w>sensitively</w> to any remarks directed at him."
                            }
                        },
                        {
                            "text": "Сердце <w>болезненно</w> сжалось от плохих новостей.",
                            "target_lang_translations": {
                                "en": "The heart clenched <w>painfully</w> at the bad news."
                            }
                        }
                    ],
                    "synonyms": [
                        "больно",
                        "мучительно"
                    ],
                    "antonyms": [
                        "безболезненно"
                    ],
                    "common_phrases": [
                        "болезненно реагировать",
                        "болезненно воспринимать",
                        "болезненно сжаться",
                        "болезненно ударить"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that feels, expresses, or causes physical or mental pain; with acute sensitivity"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "painfully",
                                "target_lang_sense_clarification": "causing or expressing pain"
                            },
                            {
                                "target_lang_word": "sensitively",
                                "target_lang_sense_clarification": "reacting with distress or irritation"
                            },
                            {
                                "target_lang_word": "acutely",
                                "target_lang_sense_clarification": "intensely felt"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfd4dcdc-0ce4-3376-a9d9-d7c41bc5709d",
                    "sense_definition": "Чрезмерно, в очень сильной степени (обычно о качествах или состояниях)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pain_intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Девушка была <w>болезненно</w> худой и бледной.",
                            "target_lang_translations": {
                                "en": "The girl was <w>painfully</w> thin and pale."
                            }
                        },
                        {
                            "text": "Этот художник был <w>болезненно</w> самолюбив и не терпел конкуренции.",
                            "target_lang_translations": {
                                "en": "This artist was <w>morbidly</w> vain and did not tolerate competition."
                            }
                        }
                    ],
                    "common_phrases": [
                        "болезненно худой",
                        "болезненно самолюбивый",
                        "болезненно застенчивый"
                    ],
                    "target_lang_definitions": {
                        "en": "To an excessive, unhealthy, or abnormal degree"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "painfully",
                                "target_lang_sense_clarification": "used as an intensifier for negative qualities (e.g., shy, thin)"
                            },
                            {
                                "target_lang_word": "morbidly",
                                "target_lang_sense_clarification": "suggesting an unhealthy mental state or condition"
                            },
                            {
                                "target_lang_word": "sickly",
                                "target_lang_sense_clarification": "suggesting illness"
                            }
                        ]
                    }
                }
            ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "91a52da0-2851-35c7-8af1-5c3a1ecc1e83",
                    "sense_definition": "Zichtbaar maken, laten zien of afbeelden (vaak van gegevens, beelden of geluid).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De grafiek <w>geeft</w> de resultaten van het onderzoek duidelijk <w>weer</w>.",
                            "target_lang_translations": {
                                "en": "The graph clearly <w>displays</w> the results of the research.",
                                "ru": "График наглядно <w>отображает</w> результаты исследования."
                            }
                        },
                        {
                            "text": "Op het scherm wordt de huidige temperatuur <w>weergegeven</w>.",
                            "target_lang_translations": {
                                "en": "The current temperature is <w>displayed</w> on the screen.",
                                "ru": "На экране <w>отображается</w> текущая температура."
                            }
                        }
                    ],
                    "synonyms": [
                        "tonen",
                        "laten zien",
                        "afbeelden",
                        "vertonen"
                    ],
                    "antonyms": [
                        "verbergen"
                    ],
                    "common_phrases": [
                        "grafisch weergeven",
                        "schematisch weergeven",
                        "correct weergeven",
                        "visueel weergeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To make visible, show, or depict (often regarding data, images, or sound).",
                        "ru": "Делать видимым, показывать или изображать (часто о данных, изображениях или звуке)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "display",
                                "target_lang_sense_clarification": "to show information or images on a screen or visual medium"
                            },
                            {
                                "target_lang_word": "represent",
                                "target_lang_sense_clarification": "to depict data or facts, e.g., in a graph"
                            },
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "general term for making something visible"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отображать",
                                "target_lang_sense_clarification": "показывать информацию или изображения на экране или другом носителе"
                            },
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "общий термин для демонстрации чего-либо"
                            },
                            {
                                "target_lang_word": "представлять",
                                "target_lang_sense_clarification": "изображать данные или факты, например, в виде графика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58b54e15-5a52-300b-9a05-da61d1ed3f66",
                    "sense_definition": "Onder woorden brengen, beschrijven of op artistieke wijze uitbeelden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schrijver weet de sfeer van de stad perfect <w>weer</w> te <w>geven</w>.",
                            "target_lang_translations": {
                                "en": "The writer knows how to perfectly <w>capture</w> the atmosphere of the city.",
                                "ru": "Писатель умеет идеально <w>передать</w> атмосферу города."
                            }
                        },
                        {
                            "text": "Het is lastig om mijn gevoelens in woorden <w>weer</w> te <w>geven</w>.",
                            "target_lang_translations": {
                                "en": "It is difficult to <w>express</w> my feelings in words.",
                                "ru": "Трудно <w>выразить</w> мои чувства словами."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitdrukken",
                        "vertolken",
                        "beschrijven",
                        "schetsen"
                    ],
                    "common_phrases": [
                        "de werkelijkheid weergeven",
                        "de sfeer weergeven",
                        "een beeld weergeven",
                        "goed weergeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To express in words, describe, or depict in an artistic way.",
                        "ru": "Выражать словами, описывать или художественно изображать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "express",
                                "target_lang_sense_clarification": "to put thoughts or feelings into words"
                            },
                            {
                                "target_lang_word": "capture",
                                "target_lang_sense_clarification": "to successfully represent an atmosphere or mood"
                            },
                            {
                                "target_lang_word": "portray",
                                "target_lang_sense_clarification": "to describe or depict a character or scene"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "успешно воспроизводить атмосферу, настроение или суть"
                            },
                            {
                                "target_lang_word": "выражать",
                                "target_lang_sense_clarification": "облекать мысли или чувства в слова"
                            },
                            {
                                "target_lang_word": "изображать",
                                "target_lang_sense_clarification": "описывать или показывать художественным способом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e37ff1ce-7e7b-3102-b10c-78c6bd5df844",
                    "sense_definition": "Iets teruggeven aan de eigenaar.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lang aandringen <w>gaf</w> hij het geleende boek eindelijk <w>weer</w>.",
                            "target_lang_translations": {
                                "en": "After much insistence, he finally <w>returned</w> the borrowed book.",
                                "ru": "После долгих уговоров он наконец <w>вернул</w> одолженную книгу."
                            }
                        },
                        {
                            "text": "De aarde <w>geeft</w> het ontvangen water weer <w>weer</w> aan de atmosfeer.",
                            "target_lang_translations": {
                                "en": "The earth <w>returns</w> the received water to the atmosphere again.",
                                "ru": "Земля снова <w>возвращает</w> полученную воду в атмосферу."
                            }
                        }
                    ],
                    "synonyms": [
                        "teruggeven"
                    ],
                    "antonyms": [
                        "houden",
                        "nemen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To give something back to the owner.",
                        "ru": "Возвращать что-либо владельцу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "to give back an item to its owner"
                            },
                            {
                                "target_lang_word": "give back",
                                "target_lang_sense_clarification": "phrasal verb meaning to return something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "возвращать",
                                "target_lang_sense_clarification": "отдавать предмет обратно владельцу"
                            }
                        ]
                    }
                }
            ]
        }

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задняя часть",
                                "target_lang_sense_clarification": "общая часть"
                            },
                            {
                                "target_lang_word": "зад",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "тыл",
                                "target_lang_sense_clarification": "обратная сторона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "476b8096-0618-30e7-93f0-519d5b5b6e5a",
                    "sense_definition": "Część ciała znajdująca się po stronie pleców lub grzbietu.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_back",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Związała długie włosy z <tyłu> głowy w kucyk.",

                    "sense_definition": "Voltooid deelwoord van <w>verhitten</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De melk wordt kort <w>verhit</w> om bacteriën te doden.",
                            "target_lang_translations": {
                                "ru": "Молоко кратковременно <w>нагревают</w>, чтобы убить бактерии."
                            }
                        },
                        {
                            "text": "Heb je de pan al genoeg <w>verhit</w> voor het bakken?",
                            "target_lang_translations": {

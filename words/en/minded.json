    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "149e4b27-f41c-3ccb-b874-8cb8a8a0a735",
                    "sense_definition": "Simple past and past participle of <w>mind</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>minded</w> the children while their parents were at the movies.",
                            "target_lang_translations": {
                                "ru": "Она <w>присматривала</w> за детьми, пока их родители были в кино."
                            }
                        },
                        {
                            "text": "He never <w>minded</w> the cold weather during his morning runs.",
                            "target_lang_translations": {
                                "ru": "Он никогда не <w>возражал</w> против холодной погоды во время своих утренних пробежек."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "of the verb mind"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола mind."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возражал",
                                "target_lang_sense_clarification": "быть против чего-либо"
                            },
                            {
                                "target_lang_word": "присматривал",
                                "target_lang_sense_clarification": "заботиться или следить за кем-то"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e95153b5-1396-3973-9ac4-194e89cc2408",
                    "sense_definition": "Having a specific type of character, attitude, or way of thinking.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are looking for like-<w>minded</w> people to join our book club.",
                            "target_lang_translations": {
                                "ru": "Мы ищем <w>единомышленников</w>, чтобы они присоединились к нашему книжному клубу."
                            }
                        },
                        {
                            "text": "She is a very career-<w>minded</w> individual who works long hours.",
                            "target_lang_translations": {
                                "ru": "Она личность, очень <w>ориентированная</w> на карьеру, которая работает много часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "oriented",
                        "natured"
                    ],
                    "common_phrases": [
                        "open-minded",
                        "like-minded",
                        "absent-minded",
                        "fair-minded"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий определенный склад ума, отношение или образ мыслей (часто используется в сложных словах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настроенный",
                                "target_lang_sense_clarification": "имеющий определенную направленность интересов"
                            },
                            {
                                "target_lang_word": "мыслящий",
                                "target_lang_sense_clarification": "обладающий определенным образом мыслей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53faaa48-114c-39b4-ba85-63737a507a9f",
                    "sense_definition": "Inclined or disposed to do a particular thing.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>minded</w> to accept your offer after reviewing the details.",
                            "target_lang_translations": {
                                "ru": "Я <w>склонен</w> принять ваше предложение после изучения деталей."
                            }
                        },
                        {
                            "text": "If you are so <w>minded</w>, you can help us with the preparations.",
                            "target_lang_translations": {
                                "ru": "Если вы так <w>настроены</w>, вы можете помочь нам с приготовлениями."
                            }
                        }
                    ],
                    "synonyms": [
                        "inclined",
                        "disposed",
                        "willing"
                    ],
                    "antonyms": [
                        "disinclined",
                        "unwilling"
                    ],
                    "common_phrases": [
                        "minded to do",
                        "if so minded"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный или расположенный сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склонен",
                                "target_lang_sense_clarification": "имеющий желание или предрасположенность"
                            },
                            {
                                "target_lang_word": "намерен",
                                "target_lang_sense_clarification": "имеющий намерение совершить действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
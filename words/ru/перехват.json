    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e83afafe-3f42-3c3b-9d29-10a3475542fa",
                    "sense_definition": "Действие по глаголу перехватить; захват, остановка или прерывание движения кого-либо или чего-либо (мяча, самолёта, сообщения).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Защитник совершил удачный <w>перехват</w> мяча и начал контратаку.",
                            "target_lang_translations": {
                                "en": "The defender made a successful <w>interception</w> of the ball and started a counterattack."
                            }
                        },
                        {
                            "text": "Спецслужбы организовали <w>перехват</w> телефонных разговоров преступников.",
                            "target_lang_translations": {
                                "en": "The secret services organized the <w>interception</w> of the criminals' phone calls."
                            }
                        }
                    ],
                    "synonyms": [
                        "захват",
                        "остановка"
                    ],
                    "antonyms": [
                        "пропуск",
                        "потеря"
                    ],
                    "common_phrases": [
                        "перехват мяча",
                        "перехват сообщений",
                        "боевой перехват",
                        "станция перехвата"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of stopping, catching, or interrupting someone or something (such as a ball, aircraft, or message) while it is moving from one place to another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "interception",
                                "target_lang_sense_clarification": "general act of intercepting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8bf6d58-7418-3fa1-9a0a-8f4934fc3d0f",
                    "sense_definition": "Присвоение контроля, управления или инициативы в каком-либо процессе.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хакеры попытались осуществить <w>перехват</w> управления сервером.",
                            "target_lang_translations": {
                                "en": "Hackers attempted to execute a <w>hijacking</w> of the server control."
                            }
                        },
                        {
                            "text": "В ходе дискуссии произошёл <w>перехват</w> инициативы моим оппонентом.",
                            "target_lang_translations": {
                                "en": "During the discussion, there was a <w>seizing</w> of the initiative by my opponent."
                            }
                        }
                    ],
                    "synonyms": [
                        "захват",
                        "овладение"
                    ],
                    "antonyms": [
                        "уступка",
                        "сдача"
                    ],
                    "common_phrases": [
                        "перехват управления",
                        "перехват власти",
                        "перехват инициативы"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of taking over control, management, or the leading position in a process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "takeover",
                                "target_lang_sense_clarification": "of control or management"
                            },
                            {
                                "target_lang_word": "seizing",
                                "target_lang_sense_clarification": "of initiative or power"
                            },
                            {
                                "target_lang_word": "hijacking",
                                "target_lang_sense_clarification": "of a digital session or signal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c57be7a-756f-3ae7-a087-439f12bc1927",
                    "sense_definition": "Изменение положения рук при удержании предмета, спортивного снаряда или руля.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гимнаст выполнил сложный <w>перехват</w> на перекладине.",
                            "target_lang_translations": {
                                "en": "The gymnast performed a difficult <w>regrip</w> on the horizontal bar."
                            }
                        },
                        {
                            "text": "Водитель сделал быстрый <w>перехват</w> руля на крутом повороте.",
                            "target_lang_translations": {

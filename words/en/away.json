    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "da8d6b0f-f070-39c3-ad9d-0ac36118c4ca",
                    "sense_definition": "Moving from a specific place or person; leaving.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not go <w>away</w> without saying goodbye.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не уходи <w>прочь</w>, не попрощавшись."
                            }
                        },
                        {
                            "text": "He walked <w>away</w> from the noisy crowd.",
                            "target_lang_translations": {
                                "ru": "Он ушел <w>прочь</w> от шумной толпы."
                            }
                        }
                    ],
                    "synonyms": [
                        "off"
                    ],
                    "common_phrases": [
                        "go away",
                        "walk away",
                        "run away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаясь от определенного места или человека; удаляясь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прочь",
                                "target_lang_sense_clarification": "наречие, указывающее на удаление от места или объекта"
                            },
                            {
                                "target_lang_word": "вон",
                                "target_lang_sense_clarification": "разговорное, часто используется в командах или эмоциональной речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5dbef3c0-313d-38e1-a464-d309db40a35c",
                    "sense_definition": "At a specified distance in space or time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother lives far <w>away</w> from here.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка живет <w>далеко</w> отсюда."
                            }
                        },
                        {
                            "text": "Christmas is only two weeks <w>away</w>.",
                            "target_lang_translations": {
                                "ru": "Рождество всего в двух неделях <w>от нас</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "far away",
                        "miles away"
                    ],
                    "target_lang_definitions": {
                        "ru": "На определенном расстоянии в пространстве или времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "далеко",
                                "target_lang_sense_clarification": "на большом расстоянии"
                            },
                            {
                                "target_lang_word": "вдали",
                                "target_lang_sense_clarification": "на расстоянии, вдалеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5744f5d-20b3-38d5-9472-34722a0ae3d6",
                    "sense_definition": "To one side; in a different direction.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She looked <w>away</w> when the scary part of the movie started.",
                            "target_lang_translations": {
                                "ru": "Она посмотрела <w>в сторону</w>, когда началась страшная часть фильма."
                            }
                        },
                        {
                            "text": "I tried to talk to him, but he turned <w>away</w>.",
                            "target_lang_translations": {
                                "ru": "Я пытался заговорить с ним, но он <w>отвернулся</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "at bay",
                        "off"
                    ],
                    "common_phrases": [
                        "turn away",
                        "look away"
                    ],
                    "target_lang_definitions": {
                        "ru": "В сторону; в другом направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в сторону",
                                "target_lang_sense_clarification": "направление взгляда или движения вбок"
                            },
                            {
                                "target_lang_word": "прочь",
                                "target_lang_sense_clarification": "отворачиваясь от чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f20243cb-f935-320b-8a49-c8afcdc8c08b",
                    "sense_definition": "Into the appropriate place for storage or safekeeping.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "storage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please put your toys <w>away</w> before dinner.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, убери свои игрушки <w>на место</w> перед ужином."
                            }
                        },
                        {
                            "text": "She packed <w>away</w> her winter clothes for the summer.",
                            "target_lang_translations": {
                                "ru": "Она убрала <w>на хранение</w> свою зимнюю одежду на лето."
                            }
                        }
                    ],
                    "common_phrases": [
                        "put away",
                        "pack away"
                    ],
                    "target_lang_definitions": {
                        "ru": "В подходящее место для хранения или сохранности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на место",

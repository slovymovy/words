                },
                {
                    "sense_id": "cc22a290-2e72-3c32-9ce9-131a06a3ef50",
                    "sense_definition": "Een mate van durf die als onbeleefd, respectloos of ongepast wordt ervaren.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "brutaliteit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vergeef mij mijn <w>stoutmoedigheid</w>, maar ik moet u tegenspreken.",
                            "target_lang_translations": {
                                "en": "Forgive my <w>boldness</w>, but I must contradict you.",
                                "ru": "Простите мою <w>смелость</w>, но я должен вам возразить."
                            }
                        },
                        {
                            "text": "De koning was verbaasd over de <w>stoutmoedigheid</w> van de jonge bediende.",
                            "target_lang_translations": {
                                "en": "The king was amazed at the <w>audacity</w> of the young servant.",
                                "ru": "Король был удивлен <w>дерзостью</w> молодого слуги."
                            }
                        }
                    ],
                    "synonyms": [
                        "brutaliteit",
                        "driestheid",
                        "vrijmoedigheid"
                    ],
                    "target_lang_definitions": {

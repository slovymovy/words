                    "sense_id": "3b553aa7-0dbb-3eed-98a7-2943bb7fafd5",
                    "sense_definition": "De kracht of geloofwaardigheid aan iets ontnemen, zoals een argument, beschuldiging of gerucht, door te bewijzen dat het onjuist is.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "refute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie kon het valse alibi van de verdachte snel <w>ontzenuwen</w>.",
                            "target_lang_translations": {
                                "en": "The police were able to quickly <w>refute</w> the suspect's false alibi.",
                                "ru": "Полиция смогла быстро <w>опровергнуть</w> ложное алиби подозреваемого."
                            }
                        },
                        {
                            "text": "Hij probeerde de hardnekkige geruchten over zijn vertrek te <w>ontzenuwen</w>.",
                            "target_lang_translations": {
                                "en": "He tried to <w>dispel</w> the persistent rumors about his departure.",
                                "ru": "Он пытался <w>развеять</w> упорные слухи о своем уходе."
                            }
                        }
                    ],
                    "synonyms": [
                        "weerleggen",
                        "ontkrachten"
                    ],
                    "antonyms": [
                        "bevestigen",
                        "onderbouwen",
                        "staven"
                    ],
                    "common_phrases": [
                        "een gerucht ontzenuwen",
                        "een argument ontzenuwen",
                        "kritiek ontzenuwen"
                    ],
                    "target_lang_definitions": {
                        "en": "To take away the force or credibility of something, such as an argument, accusation, or rumor, by proving it is incorrect.",
                        "ru": "Лишить силы или правдоподобия что-либо (например, аргумент, обвинение или слух), доказав его ложность."
                    },

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f312579e-3f62-3314-9e9c-4a53f230ff07",
                    "sense_definition": "Mający nierówną powierzchnię, pełną drobnych wypukłości; niegładki w dotyku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zbyt <w>chropowata</w> powierzchnia stołu nie pozwalała nam grać w cymbergaja.",
                            "target_lang_translations": {
                                "ru": "Слишком <w>шероховатая</w> поверхность стола не позволяла нам играть в настольный хоккей."
                            }
                        },
                        {
                            "text": "Ściana w korytarzu jest <w>chropowata</w> i bardzo nieprzyjemna w dotyku.",
                            "target_lang_translations": {
                                "ru": "Стена в коридоре <w>шершавая</w> и очень неприятная на ощупь."
                            }
                        }
                    ],
                    "synonyms": [
                        "szorstki",
                        "chropawy",
                        "niegładki",
                        "nierówny"
                    ],
                    "antonyms": [
                        "gładki",
                        "niechropowaty"
                    ],
                    "common_phrases": [
                        "chropowata powierzchnia",
                        "chropowata ściana",
                        "chropowaty tynk",
                        "chropowata skóra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий неровную поверхность, полную мелких бугорков; негладкий на ощупь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шероховатый",
                                "target_lang_sense_clarification": "о поверхности"
                            },
                            {
                                "target_lang_word": "шершавый",
                                "target_lang_sense_clarification": "неприятный на ощупь"
                            },
                            {
                                "target_lang_word": "неровный",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a3be20f-a73f-35b2-a984-002983082872",
                    "sense_definition": "O głosie lub dźwięku: brzmiący nieczysto, szorstko, pozbawiony dźwięczności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>chropowaty</w> głos zdradzał wielkie zmęczenie po długiej podróży.",
                            "target_lang_translations": {
                                "ru": "Его <w>хриплый</w> голос выдавал сильную усталость после долгого путешествия."
                            }
                        },
                        {
                            "text": "Piosenkarz jest znany ze swojego niskiego, nieco <w>chropowatego</w> brzmienia.",
                            "target_lang_translations": {
                                "ru": "Певец известен своим низким, слегка <w>хрипловатым</w> звучанием."
                            }
                        }
                    ],
                    "synonyms": [
                        "chrapliwy",
                        "szorstki"
                    ],
                    "antonyms": [
                        "gładki"
                    ],
                    "common_phrases": [
                        "chropowaty głos",
                        "chropowaty dźwięk",
                        "chropowaty bas"
                    ],
                    "target_lang_definitions": {
                        "ru": "О голосе или звуке: звучащий нечисто, грубо, лишенный звонкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хриплый",
                                "target_lang_sense_clarification": "о голосе"
                            },
                            {
                                "target_lang_word": "сиплый",
                                "target_lang_sense_clarification": "о дыхании или голосе"
                            },
                            {
                                "target_lang_word": "грубый",
                                "target_lang_sense_clarification": "о тембре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "783301d7-2b90-3784-9f07-c2c584dee03e",
                    "sense_definition": "Niedopracowany, pozbawiony płynności lub ogłady (np. o stylu, manierach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pierwszy rozdział tej książki wydał mi się nieco <w>chropowaty</w> stylistycznie.",
                            "target_lang_translations": {
                                "ru": "Первая глава этой книги показалась мне несколько <w>шероховатой</w> по стилю."
                            }
                        },
                        {
                            "text": "Mimo że jego maniery były <w>chropowate</w>, wszyscy go bardzo lubili.",
                            "target_lang_translations": {
                                "ru": "Хотя его манеры были <w>грубоватыми</w>, все его очень любили."
                            }
                        }
                    ],
                    "synonyms": [
                        "nierówny",
                        "szorstki"
                    ],
                    "antonyms": [
                        "gładki"
                    ],
                    "common_phrases": [
                        "chropowaty styl"
                    ],
                    "target_lang_definitions": {
                        "ru": "Недоработанный, лишенный плавности или изящества (например, о стиле, манерах)."
                    },
                    "translations": {

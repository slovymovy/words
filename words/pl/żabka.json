{
    "word_family": [
        "żaba",
        "żabi"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1d79e8d7-37ee-3d4a-99fe-f91643e1ae73",
                    "sense_definition": "Zdrobnienie od <w>żaba</w>; mała lub młoda żaba.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W małym stawie pływała zielona <żabka>.",
                            "target_lang_translations": {
                                "ru": "В маленьком пруду плавал зелёный <лягушонок>."
                            }
                        },
                        {
                            "text": "Dzieci z zainteresowaniem obserwowały małą <żabkę> na liściu.",
                            "target_lang_translations": {
                                "ru": "Дети с интересом наблюдали за маленьким <лягушонком> на листе."
                            }
                        }
                    ],
                    "synonyms": [
                        "żabcia",
                        "żabusia"
                    ],
                    "common_phrases": [
                        "zielona żabka",
                        "mała żabka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова <w>żaba</w>; маленькая или молодая лягушка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лягушонок",
                                "target_lang_sense_clarification": "детеныш лягушки"
                            },
                            {
                                "target_lang_word": "лягушечка",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b617b10-8b5a-3451-adb4-8caabc1291d2",
                    "sense_definition": "Styl pływacki, w którym ruchy rąk i nóg przypominają pływanie żaby.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na basenie najchętniej pływam <żabką>.",
                            "target_lang_translations": {
                                "ru": "В бассейне я охотнее всего плаваю <брассом>."
                            }
                        },
                        {
                            "text": "Instruktor pokazał nam, jak prawidłowo pływać <żabką>.",
                            "target_lang_translations": {
                                "ru": "Инструктор показал нам, как правильно плавать <брассом>."
                            }
                        }
                    ],
                    "synonyms": [
                        "styl klasyczny",
                        "żaba"
                    ],
                    "common_phrases": [
                        "pływać żabką",
                        "styl żabka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стиль плавания, при котором движения рук и ног напоминают плавание лягушки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брасс",
                                "target_lang_sense_clarification": "стиль плавания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d33c210-ad80-3e3b-a1f7-4ca29eb038d0",
                    "sense_definition": "Mały przedmiot z mechanizmem sprężynowym, służący do przypinania czegoś, np. firanek do karnisza lub prania do sznurka.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić nowe <żabki>, żeby powiesić zasłony w salonie.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новые <зажимы>, чтобы повесить шторы в гостиной."
                            }
                        },
                        {
                            "text": "Ta <żabka> jest zepsuta i nie trzyma prania na sznurku.",
                            "target_lang_translations": {
                                "ru": "Эта <прищепка> сломана и не держит бельё на верёвке."
                            }
                        }
                    ],
                    "synonyms": [
                        "klamerka",
                        "spinacz"
                    ],
                    "common_phrases": [
                        "żabki do firan",
                        "żabki do zasłon",
                        "żabki do prania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький предмет с пружинным механизмом, служащий для прикрепления чего-либо, например, занавесок к карнизу или белья к веревке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зажим",
                                "target_lang_sense_clarification": "для штор"
                            },
                            {
                                "target_lang_word": "прищепка",
                                "target_lang_sense_clarification": "для белья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa10ab0f-1346-36d8-99e0-1bc1235e6ba9",
                    "sense_definition": "Pieszczotliwe określenie dziewczyny, kobiety lub dziecka.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cześć, moja mała <żabko>, jak minął ci dzień?",
                            "target_lang_translations": {
                                "ru": "Привет, мой маленький <лягушонок>, как прошёл твой день?"
                            }
                        },
                        {
                            "text": "Mąż często mówi do swojej żony: „moja <żabko>”.",
                            "target_lang_translations": {
                                "ru": "Муж часто говорит своей жене: «мой <лягушонок>»."
                            }
                        }
                    ],
                    "synonyms": [
                        "żabcia",
                        "żabusia"
                    ],
                    "common_phrases": [
                        "moja żabka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое обращение к девушке, женщине или ребенку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лягушонок",
                                "target_lang_sense_clarification": "ласковое прозвище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be814dcd-e9d1-3020-afc5-c5e4af2ccbe9",
                    "sense_definition": "Ćwiczenie gimnastyczne polegające na skakaniu w przysiadzie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel wychowania fizycznego kazał nam zrobić dziesięć <żabek>.",
                            "target_lang_translations": {
                                "ru": "Учитель физкультуры велел нам сделать десять <лягушек>."
                            }
                        },
                        {
                            "text": "Po serii <żabek> bardzo bolą mnie mięśnie nóg.",
                            "target_lang_translations": {
                                "ru": "После серии <лягушек> у меня очень болят мышцы ног."
                            }
                        }
                    ],
                    "synonyms": [
                        "skok szpagatowy"
                    ],
                    "common_phrases": [
                        "robić żabki",
                        "skakać żabką"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гимнастическое упражнение, заключающееся в прыжках из положения присев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лягушка",
                                "target_lang_sense_clarification": "упражнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e18fe8c-66af-3794-8151-148c6376da6b",
                    "sense_definition": "Rodzaj klucza nastawnego do rur, używanego w hydraulice.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hydraulik poprosił o podanie <żabki>, aby odkręcić starą rurę.",
                            "target_lang_translations": {
                                "ru": "Сантехник попросил подать <газовый ключ>, чтобы открутить старую трубу."
                            }
                        },
                        {
                            "text": "Bez solidnej <żabki> nie naprawisz tej awarii w łazience.",
                            "target_lang_translations": {
                                "ru": "Без надежного <газового ключа> ты не устранишь эту аварию в ванной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "klucz żabka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность разводного ключа для труб, используемого в сантехнике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газовый ключ",
                                "target_lang_sense_clarification": "инструмент"
                            },
                            {
                                "target_lang_word": "разводной ключ",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "286c3483-f55f-3414-b1a6-f2c3ae07f983",
                    "sense_definition": "Dolna część smyczka instrumentów smyczkowych, służąca do napinania włosia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Skrzypek trzymał smyczek tuż przy hebanowej <żabce>.",
                            "target_lang_translations": {
                                "ru": "Скрипач держал смычок как раз у эбеновой <колодки>."
                            }
                        },
                        {
                            "text": "Ta <żabka> jest ozdobiona masą perłową i wygląda pięknie.",
                            "target_lang_translations": {
                                "ru": "Эта <колодка> украшена перламутром и выглядит прекрасно."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижняя часть смычка струнных инструментов, служащая для натяжения волоса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колодка",
                                "target_lang_sense_clarification": "часть смычка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df4d1d07-6dd9-382e-91b0-1a6c11c789c2",
                    "sense_definition": "Element dekoracyjny w architekturze gotyckiej w kształcie zwiniętego liścia lub pączka, umieszczany na krawędziach dachów i wież.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krawędzie dachu katedry były gęsto ozdobione kamiennymi <żabkami>.",
                            "target_lang_translations": {
                                "ru": "Края крыши собора были густо украшены каменными <краббами>."
                            }
                        },
                        {
                            "text": "Podczas renowacji kościoła odtworzono brakujące <żabki> na wieży.",
                            "target_lang_translations": {
                                "ru": "Во время реставрации церкви восстановили недостающие <краббы> на башне."
                            }
                        }
                    ],
                    "synonyms": [
                        "czołganka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративный элемент в готической архитектуре в форме свернутого листа или бутона, размещаемый на краях крыш и башен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крабб",
                                "target_lang_sense_clarification": "архитектурный элемент"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4c114361-85ef-3504-a184-038014049fd4",
                    "sense_definition": "Nazwisko polskie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Żabka> jest naszym nowym sąsiadem z naprzeciwka.",
                            "target_lang_translations": {
                                "ru": "Господин <Жабка> — наш новый сосед напротив."
                            }
                        },
                        {
                            "text": "Nie znam nikogo, kto nosiłby nazwisko <Żabka>.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого, кто носил бы фамилию <Жабка>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Жабка",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "170e3814-6ecb-3cb9-b78a-7a3ae969bd15",
                    "sense_definition": "Een vliegend insect met gele en zwarte strepen dat nectar verzamelt en honing maakt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "dier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zit een <w>bij</w> op de bloem in de tuin.",
                            "target_lang_translations": {
                                "en": "There is a <w>bee</w> on the flower in the garden.",
                                "ru": "На цветке в саду сидит <w>пчела</w>."
                            }
                        },
                        {
                            "text": "Blijf rustig staan, anders steekt de <w>bij</w> je misschien.",
                            "target_lang_translations": {
                                "en": "Stand still, otherwise the <w>bee</w> might sting you.",
                                "ru": "Стой спокойно, иначе <w>пчела</w> может тебя ужалить."

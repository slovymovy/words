    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b42122b1-55fd-3e92-8c8c-0d559f4239dd",
                    "sense_definition": "Главный, наиболее важный, имеющий решающее значение для чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот <ключевой> момент определил исход всей игры.",
                            "target_lang_translations": {
                                "en": "This <key> moment determined the outcome of the entire game."
                            }
                        },
                        {
                            "text": "Образование играет <ключевую> роль в развитии современного общества.",
                            "target_lang_translations": {
                                "en": "Education plays a <key> role in the development of modern society."
                            }
                        },
                        {
                            "text": "На собрании мы обсудили <ключевые> вопросы безопасности.",
                            "target_lang_translations": {
                                "en": "At the meeting, we discussed <key> security issues."
                            }
                        }
                    ],
                    "synonyms": [
                        "основной",
                        "главный"
                    ],
                    "antonyms": [
                        "второстепенный",
                        "неважный"
                    ],
                    "common_phrases": [
                        "ключевой момент",
                        "ключевая роль",
                        "ключевое слово",
                        "ключевая фигура",
                        "ключевой вопрос"
                    ],
                    "target_lang_definitions": {
                        "en": "Main, most important, having decisive significance for something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "key",
                                "target_lang_sense_clarification": "most important, central"
                            },
                            {
                                "target_lang_word": "crucial",
                                "target_lang_sense_clarification": "decisive, critical"
                            },
                            {
                                "target_lang_word": "pivotal",
                                "target_lang_sense_clarification": "of central importance, turning point"
                            },
                            {
                                "target_lang_word": "major",
                                "target_lang_sense_clarification": "significant, serious"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5f3885d-133e-37f8-9150-83562432f10e",
                    "sense_definition": "Относящийся к природному источнику воды (роднику); чистый и холодный, как вода из родника.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В жаркий летний день приятно выпить холодной <ключевой> воды.",
                            "target_lang_translations": {
                                "en": "On a hot summer day, it is pleasant to drink cold <spring> water."
                            }
                        },
                        {
                            "text": "Мы нашли в лесу чистый источник с прозрачной <ключевой> водой.",
                            "target_lang_translations": {
                                "en": "We found a clean source in the forest with clear <spring> water."
                            }
                        }
                    ],
                    "synonyms": [
                        "родниковый"
                    ],
                    "antonyms": [
                        "сточный",
                        "грязный"
                    ],
                    "common_phrases": [
                        "ключевая вода",
                        "ключевой источник"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a natural spring of water; clean and cold."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spring",
                                "target_lang_sense_clarification": "relating to a natural source of water"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73fcd2f7-4c92-3735-b47e-6fb2390aafe4",
                    "sense_definition": "Относящийся к инструменту для отпирания замков (используется почти исключительно в сочетании со словом «скважина»).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Любопытный ребенок посмотрел через <ключевую> скважину в закрытую комнату.",
                            "target_lang_translations": {
                                "en": "The curious child looked through the <key>hole into the locked room."
                            }
                        },
                        {
                            "text": "Из <ключевой> скважины дул холодный сквозняк.",
                            "target_lang_translations": {
                                "en": "A cold draft blew from the <key>hole."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ключевая скважина"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a tool for unlocking locks (used almost exclusively in the phrase 'keyhole')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "key",
                                "target_lang_sense_clarification": "relating to a locking mechanism"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
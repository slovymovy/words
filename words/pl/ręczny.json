            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "99e80dd9-d1f7-3f82-9541-10932476fa20",
                    "sense_definition": "Uruchamiany, sterowany lub obsługiwany za pomocą rąk, a nie automatycznie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mechanisms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten samochód posiada <w>ręczną</w> skrzynię biegów, którą bardzo lubię.",
                            "target_lang_translations": {
                                "ru": "У этого автомобиля <w>ручная</w> коробка передач, которая мне очень нравится."
                            }
                        },
                        {
                            "text": "Zaciągnij hamulec <w>ręczny</w>, zanim wysiądziesz z auta.",
                            "target_lang_translations": {
                                "ru": "Затяни <w>ручной</w> тормоз, прежде чем выйти из машины."
                            }
                        }
                    ],
                    "antonyms": [
                        "automatyczny",
                        "samoczynny"
                    ],
                    "common_phrases": [
                        "hamulec ręczny",
                        "skrzynia ręczna",
                        "sterowanie ręczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводимый в действие, управляемый или обслуживаемый руками, а не автоматически."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ручной",
                                "target_lang_sense_clarification": "о механизме, управлении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94d2e4ff-a290-371d-a761-1a0a8c9063d1",
                    "sense_definition": "Wykonany pracą rąk, bez użycia maszyn przemysłowych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na targu można kupić piękny wyrób <w>ręczny</w> z gliny.",
                            "target_lang_translations": {
                                "ru": "На рынке можно купить красивое изделие <w>ручной</w> работы из глины."
                            }
                        },
                        {
                            "text": "To bardzo precyzyjna robota <w>ręczna</w>, która wymagała wielu godzin pracy.",
                            "target_lang_translations": {
                                "ru": "Это очень точная <w>ручная</w> работа, которая потребовала многих часов труда."
                            }
                        }
                    ],
                    "antonyms": [
                        "maszynowy",
                        "fabryczny",
                        "seryjny"
                    ],
                    "common_phrases": [
                        "robota ręczna",
                        "wyrób ręczny",
                        "praca ręczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный руками, без использования промышленных машин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ручной",
                                "target_lang_sense_clarification": "сделанный руками"
                            },
                            {
                                "target_lang_word": "ручной работы",
                                "target_lang_sense_clarification": "фраза: сделанный вручную"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcbab673-c3a5-3402-bf35-5eba5d7871a6",
                    "sense_definition": "Przystosowany do noszenia przy sobie w rękach (zwykle o bagażu).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pasażerowie mogą zabrać na pokład samolotu jeden bagaż <w>ręczny</w>.",
                            "target_lang_translations": {
                                "ru": "Пассажиры могут взять на борт самолета одно место <w>ручной</w> клади."
                            }
                        },
                        {
                            "text": "Spakowałem najpotrzebniejsze dokumenty w mały plecak <w>ręczny</w>.",
                            "target_lang_translations": {
                                "ru": "Я упаковал самые необходимые документы в маленький рюкзак для <w>ручной</w> клади."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bagaż ręczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приспособленный для ношения с собой в руках (обычно о багаже)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ручной",
                                "target_lang_sense_clarification": "о багаже, ручная кладь"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cb991ea4-73b2-3323-b587-bbb534b832e1",
                    "sense_definition": "Potoczne określenie hamulca pomocniczego w samochodzie, który zaciąga się ręką.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "automotive_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zapomniałem zwolnić <w>ręczny</w> i samochód nie chciał ruszyć.",
                            "target_lang_translations": {
                                "ru": "Я забыл отпустить <w>ручник</w>, и машина не хотела трогаться."
                            }
                        },
                        {
                            "text": "Zaciągnij <w>ręczny</w>, bo parkujemy na stromej górce.",
                            "target_lang_translations": {
                                "ru": "Затяни <w>ручник</w>, потому что мы паркуемся на крутой горке."
                            }
                        }
                    ],
                    "synonyms": [

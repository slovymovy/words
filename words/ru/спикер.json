            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ad221df1-e7fe-3d07-a735-f09a84c64e71",
                    "sense_definition": "Председатель парламента или его палаты.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый <w>спикер</w> парламента открыл первое заседание.",
                            "target_lang_translations": {
                                "en": "The new <w>speaker</w> of the parliament opened the first session."
                            }
                        },
                        {
                            "text": "Депутаты долго обсуждали кандидатуру будущего <w>спикера</w>.",
                            "target_lang_translations": {
                                "en": "The deputies discussed the future <w>speaker</w>'s candidacy for a long time."
                            }
                        }
                    ],
                    "synonyms": [
                        "репродуктор"
                    ],
                    "common_phrases": [
                        "спикер парламента",
                        "спикер Госдумы",
                        "избрание спикера",
                        "вице-спикер"
                    ],
                    "target_lang_definitions": {
                        "en": "The presiding officer of a parliament or one of its chambers."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speaker",
                                "target_lang_sense_clarification": "parliamentary official"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3db43bf4-9470-38ba-afa2-6c36c8fa92ef",
                    "sense_definition": "Человек, который выступает с речью, докладом или презентацией на конференции или другом мероприятии.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "public_speaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На конференцию пригласили известного <w>спикера</w> из Москвы.",
                            "target_lang_translations": {
                                "en": "A famous <w>speaker</w> from Moscow was invited to the conference."
                            }
                        },
                        {
                            "text": "Каждый <w>спикер</w> имеет только двадцать минут для выступления.",
                            "target_lang_translations": {
                                "en": "Each <w>speaker</w> has only twenty minutes for their presentation."
                            }
                        }
                    ],
                    "synonyms": [
                        "репродуктор",
                        "громкоговоритель"
                    ],
                    "antonyms": [
                        "слушатель"
                    ],
                    "common_phrases": [
                        "приглашённый спикер",
                        "список спикеров",
                        "выступление спикера",
                        "стать спикером"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who delivers a speech, report, or presentation at a conference or event."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speaker",
                                "target_lang_sense_clarification": "person giving a speech"
                            },
                            {
                                "target_lang_word": "presenter",
                                "target_lang_sense_clarification": "person giving a presentation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98def77f-b299-31da-8c15-5008c1829b6a",
                    "sense_definition": "Официальный представитель организации или ведомства, уполномоченный общаться с прессой.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "public_speaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Официальный <w>спикер</w> министерства ответил на вопросы журналистов.",
                            "target_lang_translations": {
                                "en": "The ministry's official <w>spokesperson</w> answered the journalists' questions."
                            }
                        },
                        {
                            "text": "Компания наняла опытного <w>спикера</w> для общения со СМИ.",
                            "target_lang_translations": {
                                "en": "The company hired an experienced <w>spokesperson</w> to communicate with the media."
                            }
                        }
                    ],
                    "synonyms": [
                        "репродуктор"
                    ],
                    "common_phrases": [
                        "спикер ведомства",
                        "официальный спикер",
                        "спикер компании"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "neologism"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An official representative of an organization or department authorized to speak to the press."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spokesperson",
                                "target_lang_sense_clarification": "official representative"
                            },
                            {
                                "target_lang_word": "speaker",
                                "target_lang_sense_clarification": "representative (less common)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df5097bd-d3a0-37d2-b796-25c8af19ea49",
                    "sense_definition": "Небольшой динамик внутри компьютера или другого электронного устройства для подачи звуковых сигналов.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При включении компьютера системный <w>спикер</w> издал короткий писк.",
                            "target_lang_translations": {
                                "en": "When the computer was turned on, the system <w>speaker</w> emitted a short beep."
                            }
                        },
                        {
                            "text": "Если <w>спикер</w> молчит, возможно, проблема в материнской плате.",
                            "target_lang_translations": {
                                "en": "If the <w>speaker</w> is silent, the problem might be with the motherboard."
                            }
                        }
                    ],
                    "synonyms": [
                        "громкоговоритель",
                        "динамик"
                    ],
                    "common_phrases": [
                        "системный спикер",
                        "сигнал спикера",
                        "подключить спикер"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A small internal loudspeaker in a computer or electronic device used for sound signals."
                    },
                    "translations": {

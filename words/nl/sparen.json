    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb545c90-a3a9-398f-8393-10ca56b6b9d0",
                    "sense_definition": "Geld opzijzetten om het te bewaren voor later gebruik, in plaats van het direct uit te geven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben hard aan het <w>sparen</w> voor een nieuwe fiets.",
                            "target_lang_translations": {
                                "en": "I am <w>saving</w> hard for a new bike.",
                                "ru": "Я усердно <w>коплю</w> на новый велосипед."
                            }
                        },
                        {
                            "text": "Wij <w>sparen</w> elke maand een vast bedrag voor onze vakantie.",
                            "target_lang_translations": {
                                "en": "We <w>save</w> a fixed amount every month for our vacation.",
                                "ru": "Мы каждый месяц <w>откладываем</w> фиксированную сумму на отпуск."
                            }
                        }
                    ],
                    "synonyms": [
                        "opsparen",
                        "potten"
                    ],
                    "antonyms": [
                        "uitgeven",
                        "spenderen"
                    ],
                    "common_phrases": [
                        "geld sparen",
                        "sparen voor later",
                        "flink sparen"
                    ],
                    "target_lang_definitions": {
                        "en": "To put money aside to keep it for later use, instead of spending it immediately.",
                        "ru": "Откладывать деньги для использования в будущем, вместо того чтобы тратить их сразу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "save",
                                "target_lang_sense_clarification": "to accumulate money for future use"
                            },
                            {
                                "target_lang_word": "put aside",
                                "target_lang_sense_clarification": "to keep money for later use"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "копить",
                                "target_lang_sense_clarification": "накапливать деньги на что-либо"
                            },
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "сберегать часть дохода"
                            },
                            {
                                "target_lang_word": "экономить",
                                "target_lang_sense_clarification": "тратить меньше денег"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "280fc568-c3ee-3a2c-a1d5-c69688fb07e5",
                    "sense_definition": "Voorwerpen van een bepaalde soort bijeenbrengen als hobby of om een doel te bereiken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn grootvader <w>spaart</w> al jaren bijzondere postzegels.",
                            "target_lang_translations": {
                                "en": "My grandfather has been <w>collecting</w> special stamps for years.",
                                "ru": "Мой дедушка уже много лет <w>коллекционирует</w> особые почтовые марки."
                            }
                        },
                        {
                            "text": "Bij de supermarkt kun je punten <w>sparen</w> voor gratis producten.",
                            "target_lang_translations": {
                                "en": "At the supermarket you can <w>collect</w> points for free products.",
                                "ru": "В супермаркете можно <w>копить</w> баллы для получения бесплатных продуктов."
                            }
                        }
                    ],
                    "synonyms": [
                        "verzamelen",
                        "collectioneren"
                    ],
                    "common_phrases": [
                        "punten sparen",
                        "zegels sparen",
                        "fanatiek sparen"
                    ],
                    "target_lang_definitions": {
                        "en": "To gather objects of a particular kind as a hobby or to achieve a goal.",
                        "ru": "Собирать предметы определенного типа в качестве хобби или для достижения цели."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "collect",
                                "target_lang_sense_clarification": "to gather items as a hobby"
                            },
                            {
                                "target_lang_word": "save",
                                "target_lang_sense_clarification": "to accumulate points or stamps for a reward"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "коллекционировать",
                                "target_lang_sense_clarification": "собирать как хобби"
                            },
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "накапливать предметы или баллы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "667ddd05-e008-3ca5-acdd-9ae4cf8949b7",
                    "sense_definition": "Iemand of iets ontzien door geen schade toe te brengen, niet te straffen of met rust te laten.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mercy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten kregen de opdracht om de burgers te <w>sparen</w>.",
                            "target_lang_translations": {
                                "en": "The soldiers were ordered to <w>spare</w> the civilians.",
                                "ru": "Солдаты получили приказ <w>щадить</w> мирных жителей."
                            }
                        },
                        {
                            "text": "Gelukkig is ons huis tijdens de zware storm <w>gespaard</w> gebleven.",
                            "target_lang_translations": {
                                "en": "Fortunately, our house was <w>spared</w> during the severe storm.",
                                "ru": "К счастью, наш дом во время сильного шторма <w>уцелел</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontzien",
                        "behoeden",
                        "vrijwaren"
                    ],
                    "antonyms": [
                        "straffen",
                        "vernietigen"
                    ],
                    "common_phrases": [
                        "kosten noch moeite sparen",
                        "iemand sparen",
                        "zijn krachten sparen"
                    ],
                    "target_lang_definitions": {
                        "en": "To refrain from harming, punishing, or destroying someone or something.",
                        "ru": "Щадить кого-то или что-то, не причиняя вреда, не наказывая или оставляя в покое."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spare",
                                "target_lang_sense_clarification": "to refrain from harming or destroying"
                            },
                            {
                                "target_lang_word": "save",
                                "target_lang_sense_clarification": "to rescue or keep safe from harm"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "щадить",
                                "target_lang_sense_clarification": "проявлять милосердие, не причинять вреда"
                            },
                            {
                                "target_lang_word": "беречь",
                                "target_lang_sense_clarification": "охранять, не расходовать напрасно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

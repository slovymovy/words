            "senses": [
                {
                    "sense_id": "c578eab1-7adf-34b1-b51f-fce305a96dd4",
                    "sense_definition": "Een ziekte of medisch probleem, vaak specifiek voor een bepaald lichaamsdeel of orgaan.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medische_conditie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude vrouw had een ernstige <w>aandoening</w> aan haar longen.",
                            "target_lang_translations": {
                                "ru": "У пожилой женщины было серьезное <w>заболевание</w> легких."
                            }
                        },
                        {
                            "text": "Toen mijn <w>aandoening</w> erger werd, heeft ze voor me gezorgd.",
                            "target_lang_translations": {
                                "ru": "Когда мое <w>заболевание</w> обострилось, она ухаживала за мной."
                            }
                        }
                    ],
                    "synonyms": [
                        "stoornis"
                    ],
                    "common_phrases": [
                        "chronische aandoening",
                        "ernstige aandoening",
                        "aandoening aan de luchtwegen",
                        "aangeboren aandoening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Болезнь или медицинская проблема, часто связанная с определенной частью тела или органом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заболевание",
                                "target_lang_sense_clarification": "медицинское состояние"
                            },
                            {
                                "target_lang_word": "недуг",
                                "target_lang_sense_clarification": "болезнь (часто хроническая или продолжительная)"
                            },
                            {
                                "target_lang_word": "расстройство",
                                "target_lang_sense_clarification": "нарушение функций организма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6680626a-15e7-38d8-957f-65240d35cfa2",
                    "sense_definition": "Een gevoel van ontroering of sterke emotie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jonge vrouw moest huilen van <w>aandoening</w>.",

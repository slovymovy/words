                {
                    "sense_id": "bb430f79-588b-3834-9f67-ccbd2bf48db3",
                    "sense_definition": "Having a sharp, pungent taste; not sweet.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "taste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The coffee tasted <w>bitter</w> without any sugar.",
                            "target_lang_translations": {
                                "ru": "Кофе был <w>горьким</w> на вкус без сахара."
                            }
                        },
                        {
                            "text": "Dark chocolate often has a <w>bitter</w> flavor that some people love.",
                            "target_lang_translations": {
                                "ru": "Тёмный шоколад часто имеет <w>горький</w> привкус, который некоторым нравится."
                            }

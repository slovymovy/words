            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "69691488-e4aa-3a91-9812-ff77df058608",
                    "sense_definition": "Based on or influenced by personal feelings, tastes, or opinions rather than on facts or proof.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge's decision seemed <w>subjective</w> rather than based on the hard evidence."
                        },
                        {
                            "text": "Art is highly <w>subjective</w>, so what one person loves, another might hate."
                        }
                    ],
                    "synonyms": [
                        "personal",
                        "biased",
                        "individual"
                    ],
                    "antonyms": [
                        "objective"
                    ],
                    "common_phrases": [

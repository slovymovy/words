{
    "word_family": [
        "интерес",
        "интересный",
        "интересно",
        "заинтересовать",
        "заинтересоваться",
        "поинтересоваться",
        "заинтересованный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b68cc3b0-3c46-3265-82c1-b5994358b95b",
                    "sense_definition": "Испытывать внимание, любопытство или желание узнать больше о ком-либо или чём-либо; заниматься чем-либо как увлечением.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг серьёзно <интересуется> современной историей.",
                            "target_lang_translations": {
                                "en": "My friend is seriously <interested> in modern history."
                            }
                        },
                        {
                            "text": "В детстве я совсем не <интересовался> спортом.",
                            "target_lang_translations": {
                                "en": "In childhood, I was not <interested> in sports at all."
                            }
                        }
                    ],
                    "synonyms": [
                        "увлекаться"
                    ],
                    "antonyms": [
                        "игнорировать"
                    ],
                    "common_phrases": [
                        "интересоваться музыкой",
                        "интересоваться политикой",
                        "интересоваться мнением",
                        "живо интересоваться"
                    ],
                    "target_lang_definitions": {
                        "en": "To give attention to something or someone; to have a feeling of wanting to know or learn more about something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be interested",
                                "target_lang_sense_clarification": "in a subject or activity"
                            },
                            {
                                "target_lang_word": "take an interest",
                                "target_lang_sense_clarification": "start to learn about something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5e7879f-1f57-3741-b9e1-ef03282e2f76",
                    "sense_definition": "Задавать вопросы, чтобы узнать информацию; наводить справки о чём-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Покупатель <интересовался>, есть ли у нас свежий хлеб.",
                            "target_lang_translations": {
                                "en": "The customer <inquired> if we had fresh bread."
                            }
                        },
                        {
                            "text": "Вы не <интересовались> ценой билетов на самолёт?",
                            "target_lang_translations": {
                                "en": "Have you not <inquired> about the price of plane tickets?"
                            }
                        }
                    ],
                    "synonyms": [
                        "спрашивать",
                        "узнавать",
                        "осведомляться"
                    ],
                    "common_phrases": [
                        "интересоваться здоровьем",
                        "интересоваться ценой",
                        "интересоваться результатами"
                    ],
                    "target_lang_definitions": {
                        "en": "To ask questions in order to obtain information about someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inquire",
                                "target_lang_sense_clarification": "formally ask for information"
                            },
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "request information"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "275548e8-bee2-33b3-9102-fecab71bac7d",
                    "sense_definition": "Чувствовать личное влечение или симпатию к кому-либо (обычно в романтическом смысле).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он давно <интересуется> этой девушкой, но боится подойти.",
                            "target_lang_translations": {
                                "en": "He has been <interested> in this girl for a long time but is afraid to approach her."
                            }
                        },
                        {
                            "text": "Она начала <интересоваться> им после их встречи в парке.",
                            "target_lang_translations": {
                                "en": "She started to <be interested> in him after their meeting in the park."
                            }
                        }
                    ],
                    "synonyms": [
                        "симпатизировать"
                    ],
                    "common_phrases": [
                        "интересоваться девушками",
                        "интересоваться парнями"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To feel a romantic attraction or sympathy towards someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be interested",
                                "target_lang_sense_clarification": "romantically"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b0c6b33-4a55-353f-adaf-22a2924cca7d",
                    "sense_definition": "Иметь желание или стремление к чему-либо (устаревшее значение).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец рассердился и больше не <интересовался> знать о судьбе сына.",
                            "target_lang_translations": {
                                "en": "The father got angry and no longer <cared> to know about his son's fate."
                            }
                        },
                        {
                            "text": "Купец <интересовался> лишь собственной прибылью.",
                            "target_lang_translations": {
                                "en": "The merchant <cared> only about his own profit."
                            }
                        }
                    ],
                    "synonyms": [
                        "желать",
                        "стремиться"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To care about or have a desire for something (archaic or literary usage)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "care",
                                "target_lang_sense_clarification": "have a desire or concern"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
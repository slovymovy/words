                    "frequency": "Middle",
                    "semantic_group_id": "interpret",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She could <w>read</w> his mind just by looking at his face.",
                            "target_lang_translations": {
                                "ru": "Она могла <w>читать</w> его мысли, просто глядя на его лицо."
                            }
                        },
                        {
                            "text": "It is important to <w>read</w> the situation before you decide to act.",
                            "target_lang_translations": {
                                "ru": "Важно <w>понять</w> ситуацию, прежде чем решать действовать."
                            }
                        }
                    ],
                    "synonyms": [
                        "interpret",
                        "understand"
                    ],
                    "common_phrases": [
                        "read someone's mind",
                        "read between the lines",
                        "read the room",
                        "read a situation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Угадывать или понимать чьи-то мысли, чувства или намерения по поведению или внешнему виду."

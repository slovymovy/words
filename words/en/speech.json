    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0bf13836-dd85-3821-adc8-9e104d30ec7d",
                    "sense_definition": "A formal session of speaking, especially a long oral message given publicly by one person.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "public_address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The president gave a powerful <w>speech</w> on television last night.",
                            "target_lang_translations": {
                                "ru": "Президент вчера вечером выступил с сильной <w>речью</w> по телевидению."
                            }
                        },
                        {
                            "text": "I have to make a <w>speech</w> at my sister's wedding.",
                            "target_lang_translations": {
                                "ru": "Мне нужно произнести <w>речь</w> на свадьбе моей сестры."
                            }
                        }
                    ],
                    "synonyms": [
                        "address",
                        "oration"
                    ],
                    "common_phrases": [
                        "give a speech",
                        "make a speech",
                        "acceptance speech",
                        "keynote speech"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичное выступление, официальное устное сообщение, сделанное перед аудиторией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "речь",
                                "target_lang_sense_clarification": "публичное выступление"
                            },
                            {
                                "target_lang_word": "выступление",
                                "target_lang_sense_clarification": "доклад перед публикой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbacfd17-f57a-3048-a8c0-e55a612c4140",
                    "sense_definition": "The ability to speak; the faculty of uttering words or articulate sounds to communicate.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "faculty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the accident, he temporarily lost his <w>speech</w>.",
                            "target_lang_translations": {
                                "ru": "После аварии он временно потерял <w>речь</w>."
                            }
                        },
                        {
                            "text": "Freedom of <w>speech</w> is a fundamental right in many countries.",
                            "target_lang_translations": {
                                "ru": "Свобода <w>слова</w> — это фундаментальное право во многих странах."
                            }

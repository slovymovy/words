{
    "word_family": [
        "natively",
        "nativeness",
        "nativity"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9ec2e37b-73a0-3317-a80c-11e84c3e2d23",
                    "sense_definition": "Connected with the place where you were born or have lived since birth.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "origin_birth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "English is not my <w>native</w> language.",
                            "target_lang_translations": {
                                "ru": "Английский — не мой <w>родной</w> язык."
                            }
                        },
                        {
                            "text": "She returned to her <w>native</w> country after living abroad for twenty years.",
                            "target_lang_translations": {
                                "ru": "Она вернулась в свою <w>родную</w> страну после двадцати лет жизни за границей."
                            }
                        }
                    ],
                    "antonyms": [
                        "foreign"
                    ],
                    "common_phrases": [
                        "native language",
                        "native speaker",
                        "native land",
                        "native country"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с местом, где человек родился или жил с рождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родной",
                                "target_lang_sense_clarification": "о языке, стране или земле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "420565f5-fcc0-374f-a39c-1293e5ddb21e",
                    "sense_definition": "Existing naturally in a particular region or environment; not brought from somewhere else.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "indigenous_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The kangaroo is <w>native</w> to Australia.",
                            "target_lang_translations": {
                                "ru": "Кенгуру <w>родом</w> из Австралии."
                            }
                        },
                        {
                            "text": "We planted a garden full of flowers <w>native</w> to this region.",
                            "target_lang_translations": {
                                "ru": "Мы посадили сад, полный цветов, <w>местных</w> для этого региона."
                            }
                        }
                    ],
                    "synonyms": [
                        "indigenous",
                        "endemic"
                    ],
                    "antonyms": [
                        "foreign",
                        "naturalized"
                    ],
                    "common_phrases": [
                        "native species",
                        "native plants",
                        "native to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественно обитающий или произрастающий в определенном регионе; не завезенный извне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "местный",
                                "target_lang_sense_clarification": "о растениях или животных, свойственных данной местности"
                            },
                            {
                                "target_lang_word": "эндемичный",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffcd24b2-1bb5-3a21-afe7-7a1768a17dfd",
                    "sense_definition": "Relating to the people who originally lived in a place, especially before other people arrived and settled there.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "indigenous_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>native</w> population has maintained its traditions for centuries.",
                            "target_lang_translations": {
                                "ru": "<w>Коренное</w> население веками сохраняло свои традиции."
                            }
                        },
                        {
                            "text": "They are studying the history of <w>Native</w> Americans in this area.",
                            "target_lang_translations": {
                                "ru": "Они изучают историю <w>коренных</w> американцев в этом районе."
                            }
                        }
                    ],
                    "synonyms": [
                        "indigenous",
                        "aboriginal"
                    ],
                    "common_phrases": [
                        "Native American",
                        "native inhabitants",
                        "native traditions"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к людям, которые изначально жили в определенном месте, особенно до прибытия поселенцев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коренной",
                                "target_lang_sense_clarification": "относящийся к исконному населению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d12c4660-43b4-38ba-b6ad-afedaa705cd2",
                    "sense_definition": "Possessed by nature or from birth; not learned or acquired.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "innate_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He handled the difficult situation with his <w>native</w> wit.",
                            "target_lang_translations": {
                                "ru": "Он справился с трудной ситуацией благодаря своему <w>природному</w> остроумию."
                            }
                        },
                        {
                            "text": "Her <w>native</w> intelligence allowed her to solve the puzzle quickly.",
                            "target_lang_translations": {
                                "ru": "Её <w>врождённый</w> интеллект позволил ей быстро решить головоломку."
                            }
                        }
                    ],
                    "synonyms": [
                        "innate",
                        "inborn"
                    ],
                    "common_phrases": [
                        "native wit",
                        "native intelligence",
                        "native ability"
                    ],
                    "target_lang_definitions": {
                        "ru": "Данный от природы или от рождения; не приобретенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "природный",
                                "target_lang_sense_clarification": "о способностях или качествах"
                            },
                            {
                                "target_lang_word": "врождённый",
                                "target_lang_sense_clarification": "присущий с рождения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a4b858c-fbbc-3cc0-bf7e-9d5ccdc94219",
                    "sense_definition": "Designed specifically for a particular computer system, architecture, or software environment.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The application has <w>native</w> support for the new operating system.",
                            "target_lang_translations": {
                                "ru": "Приложение имеет <w>нативную</w> поддержку новой операционной системы."
                            }
                        },
                        {
                            "text": "A <w>native</w> app usually runs faster than a web-based one.",
                            "target_lang_translations": {
                                "ru": "<w>Нативное</w> приложение обычно работает быстрее, чем веб-приложение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "native app",
                        "native support",
                        "cloud native"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разработанный специально для конкретной компьютерной системы или программной среды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нативный",
                                "target_lang_sense_clarification": "профессиональный термин в IT"
                            },
                            {
                                "target_lang_word": "встроенный",
                                "target_lang_sense_clarification": "имеющийся изначально в системе"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1d5af48b-b749-3bed-b205-a67143cc4d5d",
                    "sense_definition": "A person born in a specified place or associated with a place by birth.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "origin_birth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a <w>native</w> of California.",
                            "target_lang_translations": {
                                "ru": "Он <w>уроженец</w> Калифорнии."
                            }
                        },
                        {
                            "text": "The <w>natives</w> of the small town are very friendly to tourists.",
                            "target_lang_translations": {
                                "ru": "<w>Местные жители</w> маленького городка очень дружелюбны к туристам."
                            }
                        }
                    ],
                    "antonyms": [
                        "foreign"
                    ],
                    "common_phrases": [
                        "native of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, родившийся в определенном месте или связанный с ним по рождению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уроженец",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "уроженка",
                                "target_lang_sense_clarification": "женский род"
                            },
                            {
                                "target_lang_word": "местный житель",
                                "target_lang_sense_clarification": "житель данной местности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "535b273f-db3d-391d-aad9-40bd31b3bd0f",
                    "sense_definition": "A member of the original inhabitants of a place, especially indigenous peoples.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "indigenous_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The explorers relied on the <w>natives</w> to guide them through the jungle.",
                            "target_lang_translations": {
                                "ru": "Исследователи полагались на <w>туземцев</w>, чтобы те провели их через джунгли."
                            }
                        },
                        {
                            "text": "Many <w>Natives</w> are working to preserve their ancestral languages.",
                            "target_lang_translations": {
                                "ru": "Многие <w>коренные жители</w> работают над сохранением языков своих предков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go native"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель коренного населения местности, особенно аборигенных народов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коренной житель",
                                "target_lang_sense_clarification": "нейтральное обозначение"
                            },
                            {
                                "target_lang_word": "абориген",
                                "target_lang_sense_clarification": "этнографический термин"
                            },
                            {
                                "target_lang_word": "туземец",
                                "target_lang_sense_clarification": "устаревшее, иногда с оттенком пренебрежения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
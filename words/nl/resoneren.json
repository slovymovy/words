    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "697d095b-6777-37c3-98ee-fc2d555ea716",
                    "sense_definition": "Een gevoel van herkenning, instemming of emotie oproepen bij iemand; aansluiten bij iemands belevingswereld.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn inspirerende toespraak zal zeker <w>resoneren</w> bij het jonge publiek.",
                            "target_lang_translations": {
                                "en": "His inspiring speech will certainly <w>resonate</w> with the young audience."
                            }
                        },
                        {
                            "text": "De protesten in het buitenland <w>resoneren</w> met de gevoelens van mensen hier.",
                            "target_lang_translations": {
                                "en": "The protests abroad <w>resonate</w> with the feelings of people here."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanslaan",
                        "raken"
                    ],
                    "antonyms": [
                        "botsen"
                    ],
                    "common_phrases": [
                        "resoneren bij",
                        "resoneren met"
                    ],
                    "target_lang_definitions": {
                        "en": "To evoke a feeling of recognition, agreement, or emotion; to connect with someone's experience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resonate",
                                "target_lang_sense_clarification": "emotionally or figuratively"
                            },
                            {
                                "target_lang_word": "strike a chord",
                                "target_lang_sense_clarification": "idiomatic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "587195a1-69bc-3b9d-be6a-303a8f7bc6e1",
                    "sense_definition": "Meetrillen of geluid versterken door het overnemen van trillingen van dezelfde frequentie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_vibration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ruiten van het oude huis begonnen te <w>resoneren</w> door het zware vrachtverkeer.",
                            "target_lang_translations": {
                                "en": "The windows of the old house started to <w>resonate</w> due to the heavy truck traffic."
                            }
                        },
                        {
                            "text": "Als je de juiste toonhoogte zingt, kan het glas gaan <w>resoneren</w>.",
                            "target_lang_translations": {
                                "en": "If you sing the right pitch, the glass can start to <w>resonate</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "meetrillen",
                        "meeklinken"
                    ],
                    "antonyms": [
                        "dempen"
                    ],
                    "common_phrases": [
                        "gaan resoneren",
                        "sterk resoneren"
                    ],
                    "target_lang_definitions": {
                        "en": "To vibrate sympathetically or amplify sound by absorbing vibrations of the same frequency."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resonate",
                                "target_lang_sense_clarification": "physics/acoustics"
                            },
                            {
                                "target_lang_word": "vibrate",
                                "target_lang_sense_clarification": "sympathetically"
                            }
                        ]
                    }
                }
            ]
        }
    ]

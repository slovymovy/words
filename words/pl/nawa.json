{
    "word_family": [
        "nawowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "23575416-14e2-3ac3-93de-e42fce3e0755",
                    "sense_definition": "Główna część wnętrza kościoła przeznaczona dla wiernych, znajdująca się zazwyczaj między wejściem a prezbiterium.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "church_architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciotka Lucyna siada przeważnie w <w>nawie</w> bocznej podczas mszy.",
                            "target_lang_translations": {
                                "ru": "Тетя Люцина обычно садится в боковом <w>нефе</w> во время мессы."
                            }
                        },
                        {
                            "text": "Wysoka <w>nawa</w> główna gotyckiej katedry robiła na turystach ogromne wrażenie.",
                            "target_lang_translations": {
                                "ru": "Высокий главный <w>неф</w> готического собора производил на туристов огромное впечатление."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nawa główna",
                        "nawa boczna",
                        "nawa poprzeczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главная часть внутреннего пространства церкви, предназначенная для верующих, обычно расположенная между входом и пресвитерием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неф",
                                "target_lang_sense_clarification": "архитектура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8d4f983-2305-3313-9c44-6209f3678b9f",
                    "sense_definition": "Dawne lub poetyckie określenie dużego statku lub okrętu.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "ship_metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zapieniła się woda pod dziobem <w>nawy</w> kupieckiej zmierzającej do portu.",
                            "target_lang_translations": {
                                "ru": "Вода запенилась под носом купеческого <w>корабля</w>, направляющегося в порт."
                            }
                        },
                        {
                            "text": "Na horyzoncie pojawiła się potężna <w>nawa</w> z rozwiniętymi żaglami.",
                            "target_lang_translations": {
                                "ru": "На горизонте появился мощный <w>корабль</w> с распущенными парусами."
                            }
                        }
                    ],
                    "synonyms": [
                        "statek"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane w dawnym języku lub poezji"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее или поэтическое название большого корабля или судна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корабль",
                                "target_lang_sense_clarification": "поэтическое/устаревшее"
                            },
                            {
                                "target_lang_word": "судно",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ab90f9d-9120-3975-82bb-707eff239f1f",
                    "sense_definition": "Podniosłe, metaforyczne określenie zorganizowanej społeczności, państwa lub Kościoła jako instytucji.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "ship_metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie straszne są <w>Nawie</w> Piotrowej żadne historyczne burze i przeciwności.",
                            "target_lang_translations": {
                                "ru": "<w>Ладье</w> Петровой не страшны никакие исторические бури и невзгоды."
                            }
                        },
                        {
                            "text": "Politycy powinni odpowiedzialnie kierować <w>nawą</w> państwową w czasach kryzysu.",
                            "target_lang_translations": {
                                "ru": "Политики должны ответственно управлять государственным <w>кораблем</w> во времена кризиса."
                            }
                        }
                    ],
                    "synonyms": [
                        "państwo",
                        "kraj"
                    ],
                    "common_phrases": [
                        "nawa państwowa",
                        "Nawa Piotrowa"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "styl podniosły, retoryczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвышенное, метафорическое обозначение организованного сообщества, государства или Церкви как института."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корабль",
                                "target_lang_sense_clarification": "метафора (государства)"
                            },
                            {
                                "target_lang_word": "ладья",
                                "target_lang_sense_clarification": "метафора (церкви)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "bc48a1be-0c54-3293-b4b3-ecfce5269e86",
                    "sense_definition": "Used to indicate the source, origin, or place where someone or something comes.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "origin_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Paul is <w>from</w> New Zealand."
                        },
                        {
                            "text": "I received a letter <w>from</w> my brother yesterday."
                        }
                    ],
                    "synonyms": [
                        "out of"
                    ],
                    "common_phrases": [
                        "come from",
                        "hear from",
                        "from abroad"
                    ]
                },
                {
                    "sense_id": "aae5b7fe-a294-30ff-90da-aa9df9eac66c",
                    "sense_definition": "Used to indicate the starting point of a movement, time period, or range.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "origin_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The store is open <w>from</w> nine to five."
                        },
                        {
                            "text": "He walked <w>from</w> the station to his house."
                        }
                    ],
                    "antonyms": [
                        "to"
                    ],
                    "common_phrases": [
                        "from time to time",
                        "from now on",
                        "from start to finish"
                    ]
                },
                {
                    "sense_id": "976c64cc-9c71-37cb-9c24-5d20614478f6",
                    "sense_definition": "Used to show the material or substance used to create something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This juice is made <w>from</w> fresh oranges."
                        },
                        {
                            "text": "Paper is produced <w>from</w> wood."
                        }
                    ],
                    "common_phrases": [
                        "made from"
                    ]
                },
                {
                    "sense_id": "478e64c0-43aa-3a4a-a936-82db334d3965",
                    "sense_definition": "Used to indicate a cause or reason for something happening.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Too many people die <w>from</w> heart disease."
                        },
                        {
                            "text": "He is suffering <w>from</w> a bad headache."
                        }
                    ],
                    "common_phrases": [
                        "suffer from",
                        "die from",
                        "exhausted from"
                    ]
                },
                {
                    "sense_id": "ff804556-3e1f-37f2-9e3f-aa2b01cd7d4a",
                    "sense_definition": "Used to indicate removal, separation, or protection.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please remove the cake <w>from</w> the oven."
                        },
                        {
                            "text": "Sunscreen protects your skin <w>from</w> the sun."
                        }
                    ],
                    "common_phrases": [
                        "remove from",
                        "protect from",
                        "stop from"
                    ]
                },
                {
                    "sense_id": "1796415a-42cb-3dab-86d8-3d63578b32e8",
                    "sense_definition": "Used to indicate the position or point of view of an observer.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vantage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is hard to tell <w>from</w> here."
                        },
                        {
                            "text": "Try to see the problem <w>from</w> his point of view."
                        }
                    ],
                    "common_phrases": [
                        "from my perspective",
                        "from here",
                        "from where I stand"
                    ]
                },
                {
                    "sense_id": "ad6bdc83-c681-3b51-b4dc-2b8bae1571b7",
                    "sense_definition": "Used to show a difference or distinction between two things.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your opinion differs <w>from</w> mine."
                        },
                        {
                            "text": "He knows right <w>from</w> wrong."
                        }
                    ],
                    "common_phrases": [
                        "different from",
                        "know right from wrong"
                    ]
                },
                {
                    "sense_id": "df980046-a921-363f-b3d0-85e5a4afbbdb",
                    "sense_definition": "Used to indicate that a number or amount is being subtracted.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twenty <w>from</w> thirty leaves ten."
                        },
                        {
                            "text": "Subtract five <w>from</w> the total."
                        }
                    ],
                    "synonyms": [
                        "take away"
                    ],
                    "common_phrases": [
                        "take from"
                    ]
                }
            ]
        }
    ]
}
{
    "word_family": [
        "flauwvallen",
        "verflauwen",
        "flauwerik",
        "flauwigheid",
        "flauwtjes"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6178d481-f73a-37d6-b669-aa5d660a210f",
                    "sense_definition": "Zonder veel smaak, meestal door een gebrek aan zout of kruiden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "taste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soep smaakt een beetje <w>flauw</w>, dus ik doe er wat extra zout bij.",
                            "target_lang_translations": {
                                "en": "The soup tastes a bit <w>bland</w>, so I'm adding some extra salt.",
                                "ru": "Суп на вкус немного <w>пресный</w>, поэтому я добавлю ещё соли."
                            }
                        },
                        {
                            "text": "Zonder kruiden vind ik gekookte aardappelen vaak nogal <w>flauw</w>.",
                            "target_lang_translations": {
                                "en": "Without spices, I often find boiled potatoes rather <w>tasteless</w>.",
                                "ru": "Без специй варёный картофель часто кажется мне довольно <w>безвкусным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "smakeloos"
                    ],
                    "antonyms": [
                        "pittig"
                    ],
                    "common_phrases": [
                        "een flauwe smaak",
                        "flauw eten"
                    ],
                    "target_lang_definitions": {
                        "en": "Lacking strong flavor, usually due to a lack of salt or spices.",
                        "ru": "Лишённый выраженного вкуса, обычно из-за недостатка соли или специй."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bland",
                                "target_lang_sense_clarification": "lacking flavor or zest"
                            },
                            {
                                "target_lang_word": "tasteless",
                                "target_lang_sense_clarification": "completely without taste"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пресный",
                                "target_lang_sense_clarification": "безвкусный, недосоленный (о еде)"
                            },
                            {
                                "target_lang_word": "безвкусный",
                                "target_lang_sense_clarification": "не имеющий вкуса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a425b45-4ff0-352b-b857-08d72dae8e61",
                    "sense_definition": "Niet grappig, kinderachtig of onnozel (vaak gezegd van grappen of gedrag).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn collega maakt de hele dag door <w>flauwe</w> grappen waar niemand om lacht.",
                            "target_lang_translations": {
                                "en": "My colleague makes <w>lame</w> jokes all day long that no one laughs at.",
                                "ru": "Мой коллега целый день отпускает <w>плоские</w> шутки, над которыми никто не смеётся."
                            }
                        },
                        {
                            "text": "Doe niet zo <w>flauw</w> en geef mijn telefoon gewoon terug.",
                            "target_lang_translations": {
                                "en": "Don't be so <w>childish</w> and just give my phone back.",
                                "ru": "Не веди себя так <w>по-детски</w> и просто верни мой телефон."
                            }
                        }
                    ],
                    "synonyms": [
                        "kinderachtig",
                        "onnozel"
                    ],
                    "common_phrases": [
                        "doe niet zo flauw",
                        "een flauwe grap",
                        "flauwe kul"
                    ],
                    "target_lang_definitions": {
                        "en": "Not funny, childish, or silly; often used to describe bad jokes or annoying behavior.",
                        "ru": "Несмешной, детский или глупый; часто используется для описания плохих шуток или раздражающего поведения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lame",
                                "target_lang_sense_clarification": "unconvincing, uncool, or unfunny"
                            },
                            {
                                "target_lang_word": "corny",
                                "target_lang_sense_clarification": "trite, hackneyed, or sentimentally old-fashioned (of jokes)"
                            },
                            {
                                "target_lang_word": "childish",
                                "target_lang_sense_clarification": "immature or silly (of behavior)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "плоский",
                                "target_lang_sense_clarification": "банальный и несмешной (о шутках)"
                            },
                            {
                                "target_lang_word": "глупый",
                                "target_lang_sense_clarification": "нелепый, бессмысленный"
                            },
                            {
                                "target_lang_word": "ребяческий",
                                "target_lang_sense_clarification": "детский, несерьёзный (о поведении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45e332a4-2eec-3cff-ae1d-4de09cc09cbc",
                    "sense_definition": "Nauwelijks waarneembaar, vaag of heel gering.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vague",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb werkelijk geen <w>flauw</w> idee waar we de auto hebben geparkeerd.",
                            "target_lang_translations": {
                                "en": "I really haven't the <w>faintest</w> idea where we parked the car.",
                                "ru": "Я действительно не имею ни <w>малейшего</w> понятия, где мы припарковали машину."
                            }
                        },
                        {
                            "text": "In de verte zagen we slechts het <w>flauwe</w> licht van een lantaarnpaal.",
                            "target_lang_translations": {
                                "en": "In the distance, we only saw the <w>faint</w> light of a lamppost.",
                                "ru": "Вдали мы видели лишь <w>тусклый</w> свет фонарного столба."
                            }
                        }
                    ],
                    "synonyms": [
                        "vaag",
                        "gering"
                    ],
                    "common_phrases": [
                        "geen flauw idee",
                        "een flauw vermoeden",
                        "een flauw licht"
                    ],
                    "target_lang_definitions": {
                        "en": "Barely perceptible, vague, or very slight.",
                        "ru": "Едва заметный, смутный или очень незначительный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "faint",
                                "target_lang_sense_clarification": "lacking brightness, volume, or strength"
                            },
                            {
                                "target_lang_word": "vague",
                                "target_lang_sense_clarification": "indefinite or unclear"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "смутный",
                                "target_lang_sense_clarification": "неясный, неопределённый (о догадках, воспоминаниях)"
                            },
                            {
                                "target_lang_word": "слабый",
                                "target_lang_sense_clarification": "едва различимый (о свете, звуке)"
                            },
                            {
                                "target_lang_word": "малейший",
                                "target_lang_sense_clarification": "ничтожный (в устойчивых выражениях, например, 'ни малейшего понятия')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41a9da5d-d3af-3383-ac8b-084df7b587d9",
                    "sense_definition": "Zich slap en duizelig voelen, met de neiging het bewustzijn te verliezen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de hitte en het gebrek aan frisse lucht voelde ze zich <w>flauw</w> worden.",
                            "target_lang_translations": {
                                "en": "Due to the heat and lack of fresh air, she felt herself becoming <w>faint</w>.",
                                "ru": "Из-за жары и нехватки свежего воздуха она почувствовала, что ей становится <w>дурно</w>."
                            }
                        },
                        {
                            "text": "Hij was helemaal <w>flauw</w> van de honger omdat hij niet had ontbeten.",
                            "target_lang_translations": {
                                "en": "He was completely <w>faint</w> with hunger because he hadn't eaten breakfast.",
                                "ru": "Он совсем <w>ослабел</w> от голода, так как не позавтракал."
                            }
                        }
                    ],
                    "synonyms": [
                        "duizelig",
                        "slap"
                    ],
                    "common_phrases": [
                        "flauw van de honger",
                        "zich flauw voelen"
                    ],
                    "target_lang_definitions": {
                        "en": "Feeling weak, dizzy, and close to losing consciousness.",
                        "ru": "Чувствующий слабость, головокружение и близость к потере сознания."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "faint",
                                "target_lang_sense_clarification": "feeling weak and dizzy"
                            },
                            {
                                "target_lang_word": "light-headed",
                                "target_lang_sense_clarification": "dizzy and slightly faint"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дурно",
                                "target_lang_sense_clarification": "состояние дурноты, предобморочное состояние"
                            },
                            {
                                "target_lang_word": "слабый",
                                "target_lang_sense_clarification": "физически обессиленный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
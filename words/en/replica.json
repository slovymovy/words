    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d9072b1b-304a-35a7-8d67-ae57077e155b",
                    "sense_definition": "An exact copy or reproduction of an object, especially one made by the original artist or manufacturer.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "copy_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum displayed an exact <w>replica</w> of the ancient crown because the original was too fragile.",
                            "target_lang_translations": {
                                "ru": "В музее была выставлена точная <w>копия</w> древней короны, так как оригинал был слишком хрупким."
                            }
                        },
                        {
                            "text": "Although the painting looks authentic, it is actually a high-quality <w>replica</w> created for the exhibition.",
                            "target_lang_translations": {
                                "ru": "Хотя картина выглядит подлинной, на самом деле это высококачественная <w>репродукция</w>, созданная для выставки."
                            }
                        }
                    ],
                    "synonyms": [
                        "copy",
                        "reproduction",
                        "duplicate",
                        "facsimile"
                    ],
                    "antonyms": [
                        "original",
                        "prototype"
                    ],
                    "common_phrases": [
                        "exact replica",
                        "authentic replica",
                        "full-scale replica"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точная копия или воспроизведение объекта, часто выполненное самим автором оригинала или производителем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точная копия",
                                "target_lang_sense_clarification": "полное соответствие оригиналу"
                            },
                            {
                                "target_lang_word": "реплика",
                                "target_lang_sense_clarification": "авторская или музейная копия"
                            },
                            {
                                "target_lang_word": "репродукция",
                                "target_lang_sense_clarification": "копия картины или изображения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33356d6f-2aa2-3842-8326-cc942190e68a",
                    "sense_definition": "A detailed copy of an object, usually made at a smaller scale than the original version.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "copy_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has a detailed <w>replica</w> of a vintage sports car sitting on his office desk.",
                            "target_lang_translations": {
                                "ru": "У него на рабочем столе стоит детальная <w>модель</w> винтажного спортивного автомобиля."
                            }
                        },
                        {
                            "text": "The gift shop sells a small <w>replica</w> of the famous monument for tourists to take home.",
                            "target_lang_translations": {
                                "ru": "В сувенирном магазине продается уменьшенная <w>копия</w> знаменитого памятника, которую туристы могут увезти домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "model",
                        "miniature"
                    ],
                    "common_phrases": [
                        "scale replica",
                        "miniature replica",
                        "die-cast replica"
                    ],
                    "target_lang_definitions": {
                        "ru": "Детальная копия объекта, обычно выполненная в уменьшенном масштабе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модель",
                                "target_lang_sense_clarification": "уменьшенная копия техники"
                            },
                            {
                                "target_lang_word": "макет",
                                "target_lang_sense_clarification": "копия здания или сооружения"
                            },
                            {
                                "target_lang_word": "миниатюрная копия",
                                "target_lang_sense_clarification": "маленькая копия предмета"
                            }
                        ]
                    }
                }
            ]
        }
    ]

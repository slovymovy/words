            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2510b650-2eba-3a4a-866a-0db5276d1123",
                    "sense_definition": "Siłowe lub zbrojne starcie pomiędzy wrogimi stronami, mające na celu pokonanie przeciwnika.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "combat_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze wyruszyli na zaciętą <walkę> z nadchodzącym wrogiem.",
                            "target_lang_translations": {
                                "ru": "Солдаты отправились на ожесточенный <w>бой</w> с наступающим врагом."
                            }
                        },
                        {
                            "text": "W tym regionie toczy się krwawa <walka> zbrojna o niepodległość.",
                            "target_lang_translations": {
                                "ru": "В этом регионе идет кровавая вооруженная <w>борьба</w> за независимость."
                            }
                        }
                    ],
                    "synonyms": [
                        "bój",
                        "starcie",
                        "potyczka",
                        "wojna",
                        "konflikt"
                    ],
                    "common_phrases": [
                        "walka zbrojna",
                        "walka wręcz",
                        "pole walki",
                        "walka na śmierć i życie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вооруженное столкновение или физическое противостояние враждующих сторон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бой",
                                "target_lang_sense_clarification": "сражение, битва"
                            },
                            {
                                "target_lang_word": "борьба",
                                "target_lang_sense_clarification": "противостояние, сопротивление"
                            },
                            {
                                "target_lang_word": "сражение",
                                "target_lang_sense_clarification": "крупное боевое столкновение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d11b6e2-e73c-347d-a372-f7e263141e3a",
                    "sense_definition": "Działanie mające na celu pokonanie trudności, chorób, nałogów lub przeciwności losu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "struggle_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego długa <walka> z chorobą zakończyła się pełnym wyzdrowieniem.",
                            "target_lang_translations": {
                                "ru": "Его долгая <w>борьба</w> с болезнью закончилась полным выздоровлением."
                            }
                        },
                        {
                            "text": "To jest ciągła <walka> z czasem, aby zdążyć z projektem na termin.",
                            "target_lang_translations": {
                                "ru": "Это постоянная <w>борьба</w> со временем, чтобы успеть с проектом в срок."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmaganie"
                    ],
                    "common_phrases": [
                        "walka z czasem",
                        "walka z rakiem",
                        "walka z bezrobociem",
                        "walka o przetrwanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действия, направленные на преодоление трудностей, болезней или жизненных невзгод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "борьба",
                                "target_lang_sense_clarification": "преодоление трудностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e72d4479-d18a-39a3-8c54-65563b7845f7",
                    "sense_definition": "Rywalizacja sportowa, często w sportach kontaktowych, mająca na celu wyłonienie zwycięzcy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "combat_sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oglądaliśmy wczoraj emocjonującą <walkę> bokserską o tytuł mistrza świata.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели вчера захватывающий боксерский <w>бой</w> за титул чемпиона мира."
                            }
                        },
                        {
                            "text": "Sędzia przerwał <walkę>, ponieważ jeden z zawodników doznał kontuzji.",
                            "target_lang_translations": {
                                "ru": "Судья остановил <w>поединок</w>, так как один из спортсменов получил травму."
                            }
                        }
                    ],
                    "synonyms": [
                        "mecz",
                        "starcie",
                        "rywalizacja"
                    ],
                    "common_phrases": [
                        "walka bokserska",
                        "czysta walka",
                        "sztuki walki",
                        "walka wieczoru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивное состязание, обычно в контактных видах спорта, для выявления победителя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поединок",
                                "target_lang_sense_clarification": "спортивная встреча двух противников"
                            },
                            {
                                "target_lang_word": "бой",
                                "target_lang_sense_clarification": "в боксе и единоборствах"
                            },
                            {
                                "target_lang_word": "схватка",
                                "target_lang_sense_clarification": "эпизод борьбы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70d62327-2de9-36ee-8c44-20111a676b6c",
                    "sense_definition": "Dążenie do osiągnięcia przewagi nad kimś lub zdobycia czegoś, o co ubiegają się inni (np. władzy, wpływów).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "struggle_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W parlamencie trwa ostra <walka> polityczna o poparcie nowej ustawy.",
                            "target_lang_translations": {
                                "ru": "В парламенте идет острая политическая <w>борьба</w> за поддержку нового закона."
                            }
                        },
                        {
                            "text": "Między kandydatami toczy się zacięta <walka> o głosy wyborców.",
                            "target_lang_translations": {
                                "ru": "Между кандидатами идет ожесточенная <w>борьба</w> за голоса избирателей."
                            }
                        }
                    ],
                    "synonyms": [
                        "rywalizacja",
                        "konflikt"
                    ],
                    "common_phrases": [
                        "walka o władzę",
                        "walka polityczna",
                        "walka konkurencyjna",
                        "walka o wpływy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стремление добиться превосходства или получить то, на что претендуют другие (власть, влияние)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "борьба",
                                "target_lang_sense_clarification": "соперничество за власть или ресурсы"
                            },
                            {
                                "target_lang_word": "соперничество",
                                "target_lang_sense_clarification": "конкуренция"
                            }
                        ]
                    }
                }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5998f755-c143-30c0-90b6-41a6d7849a0a",
                    "sense_definition": "Zmieniać kształt przedmiotu na inny, zazwyczaj gorszy, nienaturalny lub niepożądany.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_physical_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zbyt ciasne obuwie może trwale <w>deformować</w> stopy małego dziecka.",
                            "target_lang_translations": {
                                "ru": "Слишком тесная обувь может навсегда <w>деформировать</w> стопы маленького ребенка."
                            }
                        },
                        {
                            "text": "Wysoka temperatura zaczęła powoli <w>deformować</w> plastikową obudowę urządzenia.",
                            "target_lang_translations": {
                                "ru": "Высокая температура начала медленно <w>деформировать</w> пластиковый корпус устройства."
                            }
                        }
                    ],
                    "synonyms": [
                        "zniekształcać",
                        "odkształcać",
                        "defasonować"
                    ],
                    "antonyms": [
                        "prostować"
                    ],
                    "common_phrases": [
                        "deformować kręgosłup",
                        "deformować powierzchnię",
                        "deformować ciało"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменять форму предмета на другую, обычно худшую, неестественную или нежелательную."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "деформировать",
                                "target_lang_sense_clarification": "изменять форму"
                            },
                            {
                                "target_lang_word": "искажать",
                                "target_lang_sense_clarification": "делать форму неправильной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1451f57-b698-30e4-96b0-684d048f15ec",
                    "sense_definition": "Tracić swój pierwotny, właściwy kształt pod wpływem czynników zewnętrznych; ulegać zniekształceniu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_physical_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tanie drewno pod wpływem wilgoci może się <w>deformować</w> i pękać.",
                            "target_lang_translations": {
                                "ru": "Дешевая древесина под воздействием влаги может <w>деформироваться</w> и трескаться."
                            }
                        },
                        {
                            "text": "Ten metal jest bardzo wytrzymały i nie powinien się <w>deformować</w> nawet przy dużym nacisku.",
                            "target_lang_translations": {
                                "ru": "Этот металл очень прочный и не должен <w>деформироваться</w> даже при сильном давлении."
                            }
                        }
                    ],
                    "synonyms": [
                        "zniekształcać się",
                        "odkształcać się",
                        "defasonować się"
                    ],
                    "common_phrases": [
                        "deformować się pod naciskiem",
                        "deformować się od gorąca",
                        "deformować się z czasem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Терять свою первоначальную, правильную форму под воздействием внешних факторов; подвергаться деформации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "деформироваться",
                                "target_lang_sense_clarification": "терять форму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49c09440-1f8e-375f-b338-c1ce04a876a6",
                    "sense_definition": "Przedstawiać fakty, informacje lub rzeczywistość w sposób nieprawdziwy, zmieniając ich sens.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nieuczciwi dziennikarze potrafią celowo <w>deformować</w> rzeczywistość, aby wywołać sensację.",
                            "target_lang_translations": {
                                "ru": "Нечестные журналисты могут намеренно <w>искажать</w> действительность, чтобы вызвать сенсацию."
                            }
                        },
                        {
                            "text": "Jego uprzedzenia zaczęły <w>deformować</w> obiektywny osąd sytuacji.",
                            "target_lang_translations": {
                                "ru": "Его предрассудки начали <w>искажать</w> объективную оценку ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypaczać",
                        "zniekształcać"
                    ],
                    "common_phrases": [
                        "deformować rzeczywistość",
                        "deformować fakty",
                        "deformować prawdę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представлять факты, информацию или реальность неправдиво, изменяя их смысл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искажать",
                                "target_lang_sense_clarification": "факты, реальность"
                            },
                            {
                                "target_lang_word": "извращать",
                                "target_lang_sense_clarification": "смысл, истину"
                            }
                        ]
                    }
                }
            ]
        }
    ]

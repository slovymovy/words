        "variety",
        "vary",
        "various"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "65e2bd01-15b4-3075-9373-fa64a8ffb834",
                    "sense_definition": "The state or quality of being marked with different colors, patches, streaks, or spots.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "state_of_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist admired the subtle <w>variegation</w> in the marble floor.",
                            "target_lang_translations": {
                                "ru": "Художник восхищался тонкой <w>пестротой</w> мраморного пола."
                            }
                        },
                        {
                            "text": "There was a beautiful <w>variegation</w> of colors in the sunset sky.",
                            "target_lang_translations": {
                                "ru": "В закатном небе было прекрасное <w>разноцветье</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mottling",
                        "dappling",
                        "streakiness"
                    ],
                    "antonyms": [
                        "uniformity",
                        "monotony"
                    ],
                    "common_phrases": [
                        "color variegation",
                        "subtle variegation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние или свойство быть окрашенным в разные цвета, иметь пятна, полосы или крапинки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пестрота",
                                "target_lang_sense_clarification": "общее значение; наличие разных цветов или пятен"
                            },
                            {
                                "target_lang_word": "разноцветье",
                                "target_lang_sense_clarification": "обилие разнообразных красок"
                            },
                            {
                                "target_lang_word": "пятнистость",
                                "target_lang_sense_clarification": "наличие пятен другого цвета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74a0d616-e232-309b-8389-d77d4ab51432",
                    "sense_definition": "A variation in color within different zones of the same plant, typically on leaves or petals.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "state_of_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gardeners often prize the white and green <w>variegation</w> on the leaves.",
                            "target_lang_translations": {
                                "ru": "Садоводы часто ценят бело-зеленую <w>пестролистность</w>."
                            }
                        },
                        {
                            "text": "Lack of sunlight can sometimes cause a plant to lose its <w>variegation</w>.",
                            "target_lang_translations": {
                                "ru": "Недостаток солнечного света иногда может привести к тому, что растение потеряет свою <w>пестролистность</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leaf variegation",
                        "white variegation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменение окраски в различных зонах одного и того же растения, обычно на листьях или лепестках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пестролистность",
                                "target_lang_sense_clarification": "ботанический термин; наличие узоров на листьях"
                            },
                            {
                                "target_lang_word": "вариегатность",
                                "target_lang_sense_clarification": "специальный термин в растениеводстве"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
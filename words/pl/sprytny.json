    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9cb1bb27-f5d7-3975-a589-0ad8470ba081",
                    "sense_definition": "Odznaczający się sprytem, umiejący szybko orientować się w sytuacji i radzić sobie z problemami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest bardzo <w>sprytny</w> i zawsze potrafi znaleźć wyjście z trudnej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Мой брат очень <w>смекалистый</w> и всегда может найти выход из трудной ситуации."
                            }
                        },
                        {
                            "text": "Ten <w>sprytny</w> lis zakradł się do kurnika, omijając wszystkie pułapki.",
                            "target_lang_translations": {
                                "ru": "Этот <w>хитрый</w> лис пробрался в курятник, обойдя все ловушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaradny",
                        "pomysłowy",
                        "przebiegły",
                        "chytry"
                    ],
                    "antonyms": [
                        "niezaradny",
                        "naiwny",
                        "gapowaty"
                    ],
                    "common_phrases": [
                        "sprytny jak lis",
                        "sprytny gracz",
                        "sprytna bestia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий смекалкой, умеющий быстро ориентироваться в ситуации и находить выход из проблем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смекалистый",
                                "target_lang_sense_clarification": "находчивый, с практическим умом"
                            },
                            {
                                "target_lang_word": "сообразительный",
                                "target_lang_sense_clarification": "быстро схватывающий суть"
                            },
                            {
                                "target_lang_word": "хитрый",
                                "target_lang_sense_clarification": "лукавый, использующий уловки (часто о лисе)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cccc851-2318-38fa-82ec-1f6fddab151c",
                    "sense_definition": "Świadczący o czyimś sprycie; pomyślany lub wykonany w inteligentny, pomysłowy sposób.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "thing_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>sprytne</w> urządzenie pozwala zaoszczędzić mnóstwo czasu w kuchni.",
                            "target_lang_translations": {
                                "ru": "Это <w>хитроумное</w> устройство позволяет сэкономить кучу времени на кухне."
                            }
                        },
                        {
                            "text": "Wymyślili <w>sprytny</w> plan, dzięki któremu udało im się zaskoczyć konkurencję.",
                            "target_lang_translations": {
                                "ru": "Они придумали <w>хитрый</w> план, благодаря которому им удалось застать конкурентов врасплох."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomysłowy",
                        "przemyślny",
                        "zręczny"
                    ],
                    "antonyms": [
                        "nieprzemyślany",
                        "banalny"
                    ],
                    "common_phrases": [
                        "sprytny plan",
                        "sprytne rozwiązanie",
                        "sprytny sposób"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свидетельствующий о чьей-то смекалке; придуманный или выполненный умным, изобретательным способом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хитроумный",
                                "target_lang_sense_clarification": "изобретательный, затейливый (об устройстве)"
                            },
                            {
                                "target_lang_word": "хитрый",
                                "target_lang_sense_clarification": "ловкий, искусный (о плане)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "sparing",
        "sparingly",
        "sparingness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "702f9c16-1a9c-3dd4-b6d1-eb28b24e0f34",
                    "sense_definition": "Available for use because it is not currently occupied, needed, or used.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_extra",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you have any <w>spare</w> change for the parking meter?",
                            "target_lang_translations": {
                                "ru": "У тебя есть <w>лишняя</w> мелочь для паркомата?"
                            }
                        },
                        {
                            "text": "We have a <w>spare</w> room where you can stay for the night.",
                            "target_lang_translations": {
                                "ru": "У нас есть <w>свободная</w> комната, где ты можешь переночевать."
                            }
                        }
                    ],
                    "synonyms": [
                        "free",
                        "unoccupied",
                        "extra",
                        "leftover"
                    ],
                    "antonyms": [
                        "occupied",
                        "busy",
                        "used"
                    ],
                    "common_phrases": [
                        "spare time",
                        "spare room",
                        "spare change",
                        "spare cash"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доступный для использования, так как он не занят или не нужен в данный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "о месте или времени"
                            },
                            {
                                "target_lang_word": "ли́шний",
                                "target_lang_sense_clarification": "о предметах (деньгах, билетах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "898c4f92-4bac-37e1-ae75-cdc93870cc18",
                    "sense_definition": "Kept as an extra item to be used if the main one is lost, broken, or used up.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_extra",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is always a good idea to keep a <w>spare</w> key with a trusted neighbor.",
                            "target_lang_translations": {
                                "ru": "Всегда полезно держать <w>запасной</w> ключ у надежного соседа."
                            }
                        },
                        {
                            "text": "The driver had to change the flat tire with a <w>spare</w> one.",
                            "target_lang_translations": {
                                "ru": "Водителю пришлось заменить спущенное колесо на <w>запасное</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "backup",
                        "reserve",
                        "extra",
                        "replacement"
                    ],
                    "antonyms": [
                        "primary",
                        "main"
                    ],
                    "common_phrases": [
                        "spare tire",
                        "spare key",
                        "spare part",
                        "spare battery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хранимый как дополнительный предмет на случай потери, поломки или использования основного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запасно́й",
                                "target_lang_sense_clarification": "о ключах, шинах, деталях"
                            },
                            {
                                "target_lang_word": "резервный",
                                "target_lang_sense_clarification": "о копиях, фондах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "feeaf29f-d91a-3d78-90d7-d40d9e3f38e6",
                    "sense_definition": "Thin and lacking flesh; lean (usually describing a person's body).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_lean",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was a tall, <w>spare</w> man with sharp features and grey hair.",
                            "target_lang_translations": {
                                "ru": "Это был высокий <w>сухощавый</w> мужчина с острыми чертами лица и седыми волосами."
                            }
                        },
                        {
                            "text": "Her <w>spare</w> frame made her look fragile, but she was actually quite strong.",
                            "target_lang_translations": {
                                "ru": "Из-за <w>худощавого</w> телосложения она казалась хрупкой, но на самом деле была довольно сильной."
                            }
                        }
                    ],
                    "synonyms": [
                        "lean",
                        "gaunt",
                        "thin",
                        "slender"
                    ],
                    "antonyms": [
                        "fleshy",
                        "corpulent",
                        "stout",
                        "fat"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Худощавый, жилистый, с небольшим количеством жира (обычно о телосложении человека)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сухощавый",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "худощавый",
                                "target_lang_sense_clarification": "стройный, тонкий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4664bf14-b75c-3332-bf11-78602d3f2cd5",
                    "sense_definition": "Simple and plain in style; without unnecessary decoration or details.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_lean",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The writer is known for his <w>spare</w>, elegant prose that avoids excessive description.",
                            "target_lang_translations": {
                                "ru": "Писатель известен своей <w>лаконичной</w>, элегантной прозой, избегающей излишних описаний."
                            }
                        },
                        {
                            "text": "The room was furnished in a <w>spare</w> style, containing only a bed and a chair.",
                            "target_lang_translations": {
                                "ru": "Комната была обставлена в <w>аскетичном</w> стиле: в ней были только кровать и стул."
                            }
                        }
                    ],
                    "synonyms": [
                        "austere",
                        "plain",
                        "unadorned",
                        "simple"
                    ],
                    "antonyms": [
                        "ornate",
                        "elaborate"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Простой и строгий по стилю; без лишних украшений или деталей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лаконичный",
                                "target_lang_sense_clarification": "о стиле или языке"
                            },
                            {
                                "target_lang_word": "скупой",
                                "target_lang_sense_clarification": "сдержанный, без излишеств"
                            },
                            {
                                "target_lang_word": "строгий",
                                "target_lang_sense_clarification": "об обстановке или дизайне"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "12679adc-fe15-3e7c-9985-f188466c94eb",
                    "sense_definition": "To be able to give or share something (such as time or money) because one has enough of it.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_give",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>spare</w> a moment to help me with this report?",
                            "target_lang_translations": {
                                "ru": "Можешь <w>уделить</w> минутку, чтобы помочь мне с этим отчетом?"
                            }
                        },
                        {
                            "text": "I cannot <w>spare</w> any money right now because I have bills to pay.",
                            "target_lang_translations": {
                                "ru": "Я сейчас не могу <w>выделить</w> денег, так как мне нужно оплатить счета."
                            }
                        }
                    ],
                    "synonyms": [
                        "give",
                        "grant",
                        "afford"
                    ],
                    "common_phrases": [
                        "spare a minute",
                        "spare a dime",
                        "spare some time",
                        "spare a thought"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь возможность дать или уделить что-либо (например, время или деньги), так как этого достаточно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уделять",
                                "target_lang_sense_clarification": "время или внимание"
                            },
                            {
                                "target_lang_word": "выделять",
                                "target_lang_sense_clarification": "деньги или ресурсы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2575c6b6-2721-3d22-a5b6-009d74b15a1a",
                    "sense_definition": "To choose not to kill, punish, or harm someone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_mercy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king decided to <w>spare</w> the prisoner's life after hearing his plea.",
                            "target_lang_translations": {
                                "ru": "Король решил <w>пощадить</w> жизнь узника, выслушав его мольбу."
                            }
                        },
                        {
                            "text": "Please <w>spare</w> me, I promise I will never do it again!",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>пощадите</w> меня, я обещаю, что больше никогда этого не сделаю!"
                            }
                        }
                    ],
                    "synonyms": [
                        "save",
                        "pardon",
                        "forgive"
                    ],
                    "antonyms": [
                        "punish",
                        "kill",
                        "harm",
                        "destroy"
                    ],
                    "common_phrases": [
                        "spare someone's life",
                        "spare the rod"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять решение не убивать, не наказывать или не причинять вреда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пощадить",
                                "target_lang_sense_clarification": "сохранить жизнь, не наказать"
                            },
                            {
                                "target_lang_word": "помиловать",
                                "target_lang_sense_clarification": "официально простить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d0b3d19-240b-3c2a-85a2-6cc081e2a6cd",
                    "sense_definition": "To prevent someone from having to experience or deal with something unpleasant.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_mercy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I wanted to <w>spare</w> you the pain of knowing the truth.",
                            "target_lang_translations": {
                                "ru": "Я хотел <w>избавить</w> тебя от боли знания правды."
                            }
                        },
                        {
                            "text": "Please <w>spare</w> me the boring details and just tell me the result.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>избавь</w> меня от скучных подробностей и просто скажи результат."
                            }
                        }
                    ],
                    "synonyms": [
                        "save"
                    ],
                    "common_phrases": [
                        "spare someone the trouble",
                        "spare the details",
                        "spare no expense",
                        "spare no effort"
                    ],
                    "target_lang_definitions": {
                        "ru": "Избавить кого-либо от необходимости испытывать или делать что-то неприятное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избавлять",
                                "target_lang_sense_clarification": "от подробностей, хлопот"
                            },
                            {
                                "target_lang_word": "уберегать",
                                "target_lang_sense_clarification": "от боли, неприятностей"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e0492e82-e2ef-3140-987b-e5a389dd6cef",
                    "sense_definition": "An extra item, such as a tire or part, kept to replace a broken or lost one.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_extra",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure you have a jack and a <w>spare</w> in the trunk before you travel.",
                            "target_lang_translations": {
                                "ru": "Убедись, что у тебя в багажнике есть домкрат и <w>запаска</w> перед поездкой."
                            }
                        },
                        {
                            "text": "We keep a few <w>spares</w> in the warehouse just in case the machines break down.",
                            "target_lang_translations": {
                                "ru": "Мы держим несколько <w>запчастей</w> на складе на случай поломки машин."
                            }
                        }
                    ],
                    "synonyms": [
                        "replacement",
                        "backup"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запасной предмет (например, колесо или деталь), хранящийся для замены сломанного или потерянного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запаска",
                                "target_lang_sense_clarification": "разговорное, о колесе"
                            },
                            {
                                "target_lang_word": "запчасть",
                                "target_lang_sense_clarification": "запасная деталь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdd9101f-5292-319d-81a0-e404295283e8",
                    "sense_definition": "A score in bowling where all pins are knocked down with two balls.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He picked up a <w>spare</w> in the tenth frame to win the game.",
                            "target_lang_translations": {
                                "ru": "Он выбил <w>спэр</w> в десятом фрейме и выиграл игру."
                            }
                        },
                        {
                            "text": "It is often harder to convert a split into a <w>spare</w> than to get a strike.",
                            "target_lang_translations": {
                                "ru": "Часто сложнее превратить сплит в <w>спэр</w>, чем выбить страйк."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "bowling"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация в боулинге, когда все кегли сбиты двумя шарами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спэр",
                                "target_lang_sense_clarification": "термин боулинга"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a49cd442-4154-3598-b6fa-dc9072b51939",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Spare</w> was known in the village for his antique shop.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Спэр</w> был известен в деревне своим антикварным магазином."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "61a25130-fe21-3690-8580-17df6b1cbdf6",
                    "sense_definition": "Zadawać ciosy, uderzać kogoś lub coś wielokrotnie, sprawiając ból lub niszcząc.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wolno <w>bić</w> innych dzieci w przedszkolu.",
                            "target_lang_translations": {
                                "ru": "Нельзя <w>бить</w> других детей в детском саду."
                            }
                        },
                        {
                            "text": "Mężczyzna ze złości zaczął <w>bić</w> pięścią w stół.",
                            "target_lang_translations": {
                                "ru": "Мужчина от злости начал <w>бить</w> кулаком по столу."
                            }
                        }
                    ],
                    "synonyms": [
                        "uderzać",
                        "tłuc",
                        "lać"
                    ],
                    "antonyms": [
                        "głaskać",
                        "przytulać"
                    ],
                    "common_phrases": [
                        "bić brawo",
                        "bić po twarzy",
                        "bić pięścią"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить удары, бить кого-то или что-то многократно, причиняя боль или разрушая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "наносить удары"
                            },
                            {
                                "target_lang_word": "ударять",
                                "target_lang_sense_clarification": "совершать удар"
                            },
                            {
                                "target_lang_word": "колотить",
                                "target_lang_sense_clarification": "сильно бить (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb5813eb-cb8b-387e-8dd8-0d5de51657b7",
                    "sense_definition": "O sercu: rytmicznie kurczyć się i rozkurczać, pompując krew i dając oznaki życia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "rhythm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej <w>serce</w> biło bardzo szybko ze strachu.",
                            "target_lang_translations": {
                                "ru": "Её <w>сердце</w> билось очень быстро от страха."
                            }
                        },
                        {
                            "text": "Lekarz sprawdził, czy serce pacjenta nadal <w>bije</w>.",
                            "target_lang_translations": {
                                "ru": "Врач проверил, <w>бьётся</w> ли ещё сердце пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "walić",
                        "pulsować",
                        "kołatać"
                    ],
                    "common_phrases": [
                        "serce bije",
                        "puls bije",
                        "mocno bić"
                    ],
                    "target_lang_definitions": {
                        "ru": "О сердце: ритмично сокращаться и расслабляться, перекачивая кровь и подавая признаки жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "биться",
                                "target_lang_sense_clarification": "о сердце или пульсе"
                            },
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "издавать ритмичный звук"
                            }
                        ]

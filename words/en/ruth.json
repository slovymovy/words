                {
                    "sense_id": "137c0187-07e5-3ec1-bc0e-4bc91d1dce15",
                    "sense_definition": "A female given name from Hebrew.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My aunt <w>Ruth</w> makes the best apple pie in the family.",
                            "target_lang_translations": {
                                "ru": "Моя тетя <w>Рут</w> печет лучший яблочный пирог в семье."
                            }
                        },
                        {
                            "text": "She decided to name her newborn daughter <w>Ruth</w> after her grandmother.",
                            "target_lang_translations": {
                                "ru": "Она решила назвать свою новорожденную дочь <w>Рут</w> в честь бабушки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя еврейского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Рут",
                                "target_lang_sense_clarification": "современное имя"
                            },

                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Leaf</w> has lived in this neighborhood for over forty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Лиф</w> живет в этом районе уже более сорока лет."

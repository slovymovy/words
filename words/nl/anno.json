                            },
                            {
                                "target_lang_word": "in the time of",
                                "target_lang_sense_clarification": "referring to an era"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в наше время",
                                "target_lang_sense_clarification": "о современности"
                            },
                            {
                                "target_lang_word": "сегодня",
                                "target_lang_sense_clarification": "в значении 'в наши дни'"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f9128ebc-49fe-3c0c-a92d-b74b77a07d12",
                    "sense_definition": "Great in size, and fine or imposing in appearance or impression; magnificent.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "magnificent_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The palace has a <w>grand</w> entrance hall decorated with gold.",
                            "target_lang_translations": {
                                "ru": "Дворец имеет <w>величественный</w> вестибюль, украшенный золотом."
                            }
                        },
                        {
                            "text": "From the top of the hill, we had a <w>grand</w> view of the entire valley.",
                            "target_lang_translations": {
                                "ru": "С вершины холма нам открылся <w>великолепный</w> вид на всю долину."
                            }
                        }
                    ],
                    "synonyms": [
                        "great",
                        "large"
                    ],
                    "common_phrases": [
                        "grand scale",
                        "grand design",
                        "grand finale",
                        "grand total"
                    ],
                    "target_lang_definitions": {
                        "ru": "Огромный по размеру, прекрасный или внушительный по внешнему виду или впечатлению; величественный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грандиозный",
                                "target_lang_sense_clarification": "о планах, событиях или сооружениях"
                            },
                            {
                                "target_lang_word": "величественный",
                                "target_lang_sense_clarification": "производящий сильное впечатление внешним видом"
                            },
                            {
                                "target_lang_word": "великолепный",
                                "target_lang_sense_clarification": "очень красивый, роскошный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c35e411-eeec-38e1-ac8a-6e103a86766c",
                    "sense_definition": "Having higher rank, dignity, or importance than others of the same category.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rank_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>grand</w> duke hosted a lavish dinner for the visiting diplomats.",
                            "target_lang_translations": {
                                "ru": "<w>Великий</w> князь устроил пышный ужин для приезжих дипломатов."
                            }
                        },
                        {
                            "text": "He achieved the title of <w>grand</w> master after years of playing chess.",
                            "target_lang_translations": {
                                "ru": "Он получил титул <w>гроссмейстера</w> после многих лет игры в шахматы."
                            }
                        }
                    ],
                    "synonyms": [
                        "arch-"
                    ],
                    "common_phrases": [
                        "grand master",
                        "grand jury",
                        "grand prix",
                        "grand slam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий более высокий ранг, титул или важность по сравнению с другими в той же категории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "великий",
                                "target_lang_sense_clarification": "в титулах (напр. Великий князь)"
                            },
                            {
                                "target_lang_word": "главный",
                                "target_lang_sense_clarification": "основной, важнейший"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9516a99c-53d2-3db9-ac77-3ac9fadeaa89",
                    "sense_definition": "Fine, lovely, or very good.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "colloquial_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't worry about the arrangements, everything will be <w>grand</w>.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся об организации, всё будет <w>отлично</w>."
                            }
                        },
                        {
                            "text": "We had a <w>grand</w> time catching up with old friends yesterday.",
                            "target_lang_translations": {
                                "ru": "Мы <w>замечательно</w> провели время, общаясь со старыми друзьями вчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "great"
                    ],
                    "common_phrases": [
                        "be grand",
                        "grand job"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Common in Irish and British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекрасный, милый или очень хороший."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отличный",
                                "target_lang_sense_clarification": "разговорное выражение одобрения"
                            },
                            {
                                "target_lang_word": "замечательный",
                                "target_lang_sense_clarification": "приятный, хороший"
                            }
                        ]

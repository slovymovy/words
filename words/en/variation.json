    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "68946ad5-9d34-3aef-bc8f-0ed6164d96f5",
                    "sense_definition": "A change or difference in amount, level, or condition.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a wide <w>variation</w> in house prices across the different regions.",
                            "target_lang_translations": {
                                "ru": "Существует большой <w>разброс</w> цен на жилье в разных регионах."
                            }
                        },
                        {
                            "text": "The slight <w>variation</w> in temperature can affect the outcome of the experiment.",
                            "target_lang_translations": {
                                "ru": "Незначительное <w>изменение</w> температуры может повлиять на результат эксперимента."
                            }
                        }
                    ],
                    "synonyms": [
                        "magnetic declination"
                    ],
                    "common_phrases": [
                        "wide variation",
                        "slight variation",
                        "seasonal variation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменение или различие в количестве, уровне или состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изменение",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "разброс",
                                "target_lang_sense_clarification": "разница в значениях, диапазоне"
                            },
                            {
                                "target_lang_word": "колебание",
                                "target_lang_sense_clarification": "непостоянство уровня или состояния"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72441db1-e8d4-38b8-9de8-f586a5842b9d",
                    "sense_definition": "A thing that is different from other things in the same general group; a different version of something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This soup is a delicious <w>variation</w> on a traditional family recipe.",
                            "target_lang_translations": {
                                "ru": "Этот суп — вкусная <w>вариация</w> на тему традиционного семейного рецепта."
                            }
                        },
                        {
                            "text": "We played a <w>variation</w> of the game that included extra rules.",
                            "target_lang_translations": {
                                "ru": "Мы играли в <w>вариант</w> этой игры, включающий дополнительные правила."
                            }
                        }
                    ],
                    "common_phrases": [
                        "variation on",
                        "variation of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечто, отличающееся от других вещей той же группы; иная версия чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вариант",
                                "target_lang_sense_clarification": "версия, видоизменение"
                            },
                            {
                                "target_lang_word": "разновидность",
                                "target_lang_sense_clarification": "вид, тип"
                            },
                            {
                                "target_lang_word": "вариация",
                                "target_lang_sense_clarification": "творческая переработка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ee70714-205e-3ed3-83fe-3d2297465833",
                    "sense_definition": "A difference in physical traits or genetic makeup among individuals of a group.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Genetic <w>variation</w> is essential for a species to adapt to changing environments.",
                            "target_lang_translations": {
                                "ru": "Генетическая <w>изменчивость</w> необходима виду для адаптации к меняющимся условиям среды."
                            }
                        },
                        {
                            "text": "The study focused on the <w>variation</w> in beak size among the island birds.",
                            "target_lang_translations": {
                                "ru": "Исследование было сосредоточено на <w>различиях</w> в размере клюва среди птиц острова."
                            }
                        }
                    ],
                    "common_phrases": [
                        "genetic variation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Различие в физических признаках или генетическом строении среди особей группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изменчивость",
                                "target_lang_sense_clarification": "биологическое свойство"
                            },
                            {
                                "target_lang_word": "различие",
                                "target_lang_sense_clarification": "отличие в признаках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b358d30-dcc1-3132-8ca0-fb13afbe56ff",
                    "sense_definition": "A musical technique where a theme is repeated with changes to the rhythm, melody, or harmony.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The composer wrote a set of <w>variations</w> based on a popular folk tune.",
                            "target_lang_translations": {
                                "ru": "Композитор написал цикл <w>вариаций</w>, основанных на популярной народной мелодии."
                            }
                        },
                        {
                            "text": "Her performance of the final <w>variation</w> was technically flawless and moving.",
                            "target_lang_translations": {
                                "ru": "Её исполнение финальной <w>вариации</w> было технически безупречным и трогательным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "theme and variations"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальная техника, при которой тема повторяется с изменениями ритма, мелодии или гармонии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вариация",
                                "target_lang_sense_clarification": "музыкальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "835eb7a8-3688-3b3e-bf79-50ec4f49e234",
                    "sense_definition": "The angle between magnetic north and true north.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain adjusted the ship's course to account for magnetic <w>variation</w>.",
                            "target_lang_translations": {
                                "ru": "Капитан скорректировал курс корабля с учетом магнитного <w>склонения</w>."
                            }

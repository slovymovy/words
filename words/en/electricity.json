    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5ef78d14-1b2d-32e5-869c-c147190933f1",
                    "sense_definition": "A form of energy provided through wires that is used to power machines, lights, and heating.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_energy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to pay the <w>electricity</w> bill before the end of the month.",
                            "target_lang_translations": {
                                "ru": "Нам нужно оплатить счет за <w>электричество</w> до конца месяца."
                            }
                        },
                        {
                            "text": "Solar panels can generate enough <w>electricity</w> to power the entire house.",
                            "target_lang_translations": {
                                "ru": "Солнечные панели могут вырабатывать достаточно <w>электроэнергии</w>, чтобы обеспечить питанием весь дом."
                            }
                        },
                        {
                            "text": "Please turn off the lights when you leave to save <w>electricity</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, выключай свет, когда уходишь, чтобы экономить <w>электричество</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "power",
                        "energy",
                        "current"
                    ],
                    "common_phrases": [
                        "electricity bill",
                        "generate electricity",
                        "save electricity",
                        "conduct electricity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма энергии, передаваемая по проводам, которая используется для питания машин, освещения и отопления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "электричество",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "электроэнергия",
                                "target_lang_sense_clarification": "энергетический ресурс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "024be901-fa8c-330d-adee-6301bc91d8aa",
                    "sense_definition": "A natural phenomenon caused by the movement and interaction of charged subatomic particles.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_energy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children learned about static <w>electricity</w> by rubbing balloons on their hair.",
                            "target_lang_translations": {
                                "ru": "Дети узнали о статическом <w>электричестве</w>, потирая воздушные шарики о волосы."
                            }
                        },
                        {
                            "text": "Rubber is a material that does not conduct <w>electricity</w> very well.",
                            "target_lang_translations": {
                                "ru": "Резина — это материал, который очень плохо проводит <w>электричество</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "static electricity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Природное явление, вызванное движением и взаимодействием заряженных субатомных частиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "электричество",
                                "target_lang_sense_clarification": "физическое явление"
                            },
                            {
                                "target_lang_word": "ток",
                                "target_lang_sense_clarification": "направленное движение частиц"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f5d64f1-a85f-3f0d-9811-952ed005b90c",
                    "sense_definition": "A strong feeling of excitement, tension, or energy in a situation or among a group of people.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was a strange <w>electricity</w> in the air as the two rivals faced each other.",
                            "target_lang_translations": {
                                "ru": "В воздухе витало странное <w>напряжение</w>, когда два соперника встретились лицом к лицу."
                            }
                        },
                        {
                            "text": "The band's performance created a wave of <w>electricity</w> throughout the cheering crowd.",
                            "target_lang_translations": {
                                "ru": "Выступление группы вызвало волну <w>возбуждения</w> в ликующей толпе."

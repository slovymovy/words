    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "891f9725-edab-390b-9eb5-a36f7c1b2ec8",
                    "sense_definition": "Saai en vervelend doordat er weinig of geen afwisseling is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "boring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jan deed <w>eentonig</w> werk waarbij hij de hele dag dezelfde handeling moest herhalen.",
                            "target_lang_translations": {
                                "en": "Jan did <w>monotonous</w> work where he had to repeat the same action all day.",
                                "ru": "Ян выполнял <w>монотонную</w> работу, при которой ему приходилось весь день повторять одни и те же действия."
                            }
                        },
                        {
                            "text": "Ze vond het dagelijkse leven tijdens de lange winter erg <w>eentonig</w>.",
                            "target_lang_translations": {
                                "en": "She found daily life during the long winter very <w>dull</w>.",
                                "ru": "Она считала повседневную жизнь во время долгой зимы очень <w>однообразной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "saai",
                        "vervelend",
                        "monotoon"
                    ],
                    "antonyms": [
                        "afwisselend",
                        "gevarieerd"
                    ],
                    "common_phrases": [
                        "eentonig werk",
                        "een eentonig leven",
                        "saai en eentonig"
                    ],
                    "target_lang_definitions": {
                        "en": "Dull and tedious due to little or no variety.",
                        "ru": "Скучный и утомительный из-за отсутствия или недостатка разнообразия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monotonous",
                                "target_lang_sense_clarification": "lacking variety"
                            },
                            {
                                "target_lang_word": "tedious",
                                "target_lang_sense_clarification": "boring and repetitive"
                            },
                            {
                                "target_lang_word": "dull",
                                "target_lang_sense_clarification": "uninteresting"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "однообразный",
                                "target_lang_sense_clarification": "лишенный разнообразия"
                            },
                            {
                                "target_lang_word": "монотонный",
                                "target_lang_sense_clarification": "скучный и повторяющийся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "319df7f7-01cc-3bfd-9c64-fa370318293f",
                    "sense_definition": "Klinkend met slechts één toonhoogte, zonder variatie in klank.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij wist niet waar de <w>eentonige</w> brom in de kamer vandaan kwam.",
                            "target_lang_translations": {
                                "en": "He didn't know where the <w>monotonous</w> hum in the room was coming from.",
                                "ru": "Он не знал, откуда доносилось <w>монотонное</w> гудение в комнате."
                            }
                        },
                        {
                            "text": "De leraar sprak met een <w>eentonige</w> stem waardoor de leerlingen hun aandacht verloren.",
                            "target_lang_translations": {
                                "en": "The teacher spoke in a <w>monotonous</w> voice, causing the students to lose their attention.",
                                "ru": "Учитель говорил <w>монотонным</w> голосом, из-за чего ученики потеряли внимание."
                            }
                        }
                    ],
                    "synonyms": [
                        "monotoon"
                    ],
                    "common_phrases": [
                        "een eentonige stem",
                        "een eentonig geluid",
                        "eentonig gezoem"
                    ],
                    "target_lang_definitions": {
                        "en": "Sounding with only one pitch, without variation in tone.",
                        "ru": "Звучащий на одной высоте, без изменений тона."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monotonous",
                                "target_lang_sense_clarification": "unvarying in pitch"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "монотонный",
                                "target_lang_sense_clarification": "об интонации или звуке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
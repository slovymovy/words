{
    "word_family": [
        "afleiden",
        "afleider"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "04bc46c2-b33d-339a-b529-1fddca2cbbb3",
                    "sense_definition": "Een gebeurtenis of activiteit die de gedachten op iets anders richt; verstrooiing of ontspanning.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zoek wat <w>afleiding</w> na een lange dag hard werken.",
                            "target_lang_translations": {
                                "ru": "Я ищу какого-нибудь <w>отвлечения</w> после долгого дня тяжелой работы."
                            }
                        },
                        {
                            "text": "Sporten is voor mij een goede <w>afleiding</w> van de dagelijkse stress.",
                            "target_lang_translations": {
                                "ru": "Спорт для меня — хорошее <w>отвлечение</w> от повседневного стресса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "afleiding zoeken",
                        "afleiding vinden",
                        "voor afleiding zorgen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Событие или занятие, которое переключает мысли на что-то другое; развлечение или отдых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвлечение",
                                "target_lang_sense_clarification": "переключение внимания"
                            },
                            {
                                "target_lang_word": "развлечение",
                                "target_lang_sense_clarification": "досуг, отдых"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b08b0eeb-fdd8-35bd-ae7f-583cd84519c2",
                    "sense_definition": "Een woord dat gevormd is uit een ander woord door toevoeging van een voor- of achtervoegsel.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het woord 'bakker' is een <w>afleiding</w> van het werkwoord 'bakken'.",
                            "target_lang_translations": {
                                "ru": "Слово 'bakker' — это <w>производное</w> от глагола 'bakken'."
                            }
                        },
                        {
                            "text": "In de taalkunde maken we onderscheid tussen een samenstelling en een <w>afleiding</w>.",
                            "target_lang_translations": {
                                "ru": "В лингвистике мы различаем сложное слово и <w>производное</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "afl."
                    ],
                    "antonyms": [
                        "samenstelling"
                    ],
                    "common_phrases": [
                        "morfologische afleiding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, образованное от другого слова путем добавления приставки или суффикса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "производное слово",
                                "target_lang_sense_clarification": "лингвистика"
                            },
                            {
                                "target_lang_word": "деривация",
                                "target_lang_sense_clarification": "процесс словообразования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c8ddafc-62f5-33ca-837b-3305fcbb8b23",
                    "sense_definition": "Een reeks logische stappen waarmee een stelling of formule wordt bewezen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>afleiding</w> van deze wiskundige formule is erg complex.",
                            "target_lang_translations": {
                                "ru": "<w>Вывод</w> этой математической формулы очень сложен."
                            }
                        },
                        {
                            "text": "Als de <w>afleiding</w> correct is, dan is het bewijs geleverd.",
                            "target_lang_translations": {
                                "ru": "Если <w>вывод</w> верен, то доказательство получено."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wiskundige afleiding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последовательность логических шагов, с помощью которых доказывается утверждение или формула."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вывод",
                                "target_lang_sense_clarification": "логическое умозаключение или получение формулы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
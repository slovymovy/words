                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находить",
                                "target_lang_sense_clarification": "время, силы"
                            },
                            {
                                "target_lang_word": "изыскивать",
                                "target_lang_sense_clarification": "средства, возможности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9e21b7f-b1e8-3824-bc0c-96536f13cddf",
                    "sense_definition": "Oceniać coś lub kogoś w określony sposób; wyrabiać sobie zdanie na jakiś temat.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <znajduję> w tym nowym filmie niczego śmiesznego ani interesującego.",
                            "target_lang_translations": {
                                "ru": "Я не <нахожу> в этом новом фильме ничего смешного или интересного."
                            }
                        },
                        {
                            "text": "Wielu krytyków <znajduje> ten obraz niezwykle pięknym i wzruszającym.",
                            "target_lang_translations": {
                                "ru": "Многие критики <находят> эту картину невероятно красивой и трогательной."
                            }
                        }
                    ],
                    "synonyms": [
                        "oceniać",
                        "uważać",
                        "mieć opinię o…"
                    ],
                    "common_phrases": [
                        "znajdować przyjemność",
                        "znajdować interesującym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценивать что-то или кого-то определенным образом; составлять мнение о чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находить",
                                "target_lang_sense_clarification": "считать, полагать"
                            },
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "иметь мнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e688f474-528d-391d-bddb-434f8ac3cc72",
                    "sense_definition": "Trafiać do jakiejś sytuacji lub stanu, często niespodziewanie (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagle <znajdujemy> się w bardzo trudnej sytuacji finansowej bez pomocy.",
                            "target_lang_translations": {
                                "ru": "Внезапно мы <оказываемся> в очень сложной финансовой ситуации без помощи."
                            }
                        },
                        {
                            "text": "Nasza firma <znajduje> się obecnie w fazie bardzo dynamicznego rozwoju.",
                            "target_lang_translations": {
                                "ru": "Наша компания сейчас <находится> в фазе очень динамичного развития."
                            }
                        }
                    ],
                    "synonyms": [

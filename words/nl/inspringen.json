{
    "word_family": [
        "insprong",
        "springen",
        "sprong"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4671b77f-8d4a-3412-b26d-8dc62376b920",
                    "sense_definition": "Zich met een sprong in een ruimte, vloeistof of opening begeven.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is zonder aarzelen het koude water <w>ingesprongen</w>.",
                            "target_lang_translations": {
                                "en": "He <w>jumped into</w> the cold water without hesitation."
                            }
                        },
                        {
                            "text": "De hond <w>sprong</w> vrolijk de openstaande kofferbak <w>in</w>.",
                            "target_lang_translations": {
                                "en": "The dog happily <w>jumped into</w> the open trunk."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To enter a space, liquid, or opening with a jump."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jump in",
                                "target_lang_sense_clarification": "general movement into a space"
                            },
                            {
                                "target_lang_word": "jump into",
                                "target_lang_sense_clarification": "movement towards the inside of something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ddcea73-78a8-327f-926f-3f21d2002c95",
                    "sense_definition": "Tijdelijk iemands taak overnemen of direct reageren op een situatie om te helpen of kansen te benutten.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omdat mijn collega ziek is, moet ik vandaag voor hem <w>inspringen</w>.",
                            "target_lang_translations": {
                                "en": "Because my colleague is sick, I have to <w>fill in</w> for him today."
                            }
                        },
                        {
                            "text": "Het bedrijf <w>springt</w> slim <w>in</w> op de veranderende behoefte van de consument.",
                            "target_lang_translations": {
                                "en": "The company cleverly <w>responds</w> to the changing needs of the consumer."
                            }
                        }
                    ],
                    "synonyms": [
                        "invallen",
                        "bijspringen",
                        "overnemen"
                    ],
                    "common_phrases": [
                        "op iets inspringen",
                        "voor iemand inspringen"
                    ],
                    "target_lang_definitions": {
                        "en": "To temporarily take over a task, intervene to help, or respond to a situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fill in",
                                "target_lang_sense_clarification": "to substitute for someone"
                            },
                            {
                                "target_lang_word": "step in",
                                "target_lang_sense_clarification": "to intervene or help out"
                            },
                            {
                                "target_lang_word": "respond",
                                "target_lang_sense_clarification": "to react to a situation or opportunity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34d841dc-5bd9-33f6-9aa5-c3a87d982136",
                    "sense_definition": "De eerste regel van een tekstdeel iets verder naar rechts laten beginnen dan de rest.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "typography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet bij het begin van elke nieuwe alinea een stukje <w>inspringen</w>.",
                            "target_lang_translations": {
                                "en": "You must <w>indent</w> a bit at the beginning of every new paragraph."
                            }
                        },
                        {
                            "text": "De tekstverwerker laat de regels automatisch <w>inspringen</w>.",
                            "target_lang_translations": {
                                "en": "The word processor <w>indents</w> the lines automatically."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To start a line of text further from the margin than the rest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "indent",
                                "target_lang_sense_clarification": "typography"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "107e2f33-d9b3-3136-a54b-c6a2acae1e76",
                    "sense_definition": "Een stukje naar achteren wijken ten opzichte van een lijn of vlak.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "spatial_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gevel van het gebouw <w>springt</w> op de derde verdieping iets <w>in</w>.",
                            "target_lang_translations": {
                                "en": "The facade of the building <w>recedes</w> slightly on the third floor."
                            }
                        },
                        {
                            "text": "Door de <w>inspringende</w> hoek ontstaat er een beschutte plek voor de voordeur.",
                            "target_lang_translations": {
                                "en": "The <w>recessed</w> angle creates a sheltered spot for the front door."
                            }
                        }
                    ],
                    "synonyms": [
                        "terugwijken"
                    ],
                    "antonyms": [
                        "uitsteken"
                    ],
                    "target_lang_definitions": {
                        "en": "To be positioned further back relative to a line or surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recede",
                                "target_lang_sense_clarification": "to move or be further back"
                            },
                            {
                                "target_lang_word": "be set back",
                                "target_lang_sense_clarification": "architecture"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
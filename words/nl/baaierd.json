            "senses": [
                {
                    "sense_id": "bca63df1-19e4-389b-b43c-386b3f4e0146",
                    "sense_definition": "Een grote, onoverzichtelijke hoeveelheid, mengeling of verwarring.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chaos_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de zware storm veranderde de tuin in een <w>baaierd</w> van takken en bladeren.",
                            "target_lang_translations": {
                                "ru": "После сильного шторма сад превратился в <w>хаос</w> из веток и листьев."
                            }
                        },
                        {
                            "text": "In de <w>baaierd</w> van stemmen op de markt kon ik mijn vriend niet meer horen.",
                            "target_lang_translations": {
                                "ru": "В <w>мешанине</w> голосов на рынке я больше не мог слышать своего друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "chaos",
                        "warboel",
                        "rommelboel"
                    ],
                    "common_phrases": [
                        "een baaierd van indrukken",
                        "een baaierd van geluiden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое, беспорядочное скопление, смесь или путаница."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хаос",
                                "target_lang_sense_clarification": "беспорядок"
                            },
                            {
                                "target_lang_word": "мешанина",
                                "target_lang_sense_clarification": "беспорядочная смесь"
                            },
                            {
                                "target_lang_word": "неразбериха",
                                "target_lang_sense_clarification": "путаница"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c49415e4-a84c-3ce1-ab21-f621a4b1b359",
                    "sense_definition": "De ongevormde, chaotische oertoestand die volgens scheppingsverhalen aan de schepping van de wereld voorafging.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "chaos_primordial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dichter beschreef hoe de aarde ontstond uit een duistere <w>baaierd</w>.",
                            "target_lang_translations": {
                                "ru": "Поэт описал, как земля возникла из темного <w>хаоса</w>."
                            }
                        },
                        {
                            "text": "Volgens de mythe bracht de schepper orde in de oorspronkelijke <w>baaierd</w>.",
                            "target_lang_translations": {
                                "ru": "Согласно мифу, творец навел порядок в первозданном <w>хаосе</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "chaos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бесформенное, хаотичное первобытное состояние, предшествовавшее сотворению мира."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хаос",
                                "target_lang_sense_clarification": "первозданная бездна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec65d81d-dd1c-3da8-a9a5-5c730ed61d1e",
                    "sense_definition": "Een historische instelling die tijdelijk onderdak bood aan armen, vreemdelingen of zwervers.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "shelter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zeventiende eeuw konden reizigers zonder geld terecht in de plaatselijke <w>baaierd</w>.",
                            "target_lang_translations": {
                                "ru": "В семнадцатом веке путешественники без денег могли остановиться в местном <w>странноприимном доме</w>."
                            }
                        },
                        {
                            "text": "Het oude gebouw deed vroeger dienst als <w>baaierd</w> voor de armen van de stad.",
                            "target_lang_translations": {
                                "ru": "Старое здание раньше служило <w>приютом</w> для городской бедноты."
                            }
                        }
                    ],

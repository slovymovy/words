        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a26fcb19-9837-39f1-bbd7-7e8418673276",
                    "sense_definition": "The District of Columbia, a federal district of the United States where the capital city, Washington, is located.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "place_dc",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They traveled to <w>DC</w> to visit the White House and the museums.",
                            "target_lang_translations": {
                                "ru": "Они поехали в <w>округ Колумбия</w>, чтобы посетить Белый дом и музеи."
                            }
                        },
                        {
                            "text": "Living in <w>DC</w> offers many opportunities for careers in politics.",
                            "target_lang_translations": {
                                "ru": "Жизнь в <w>округе Колумбия</w> предлагает много возможностей для карьеры в политике."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Washington DC",
                        "DC area"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "United States"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Округ Колумбия, федеральный округ США, где расположена столица Вашингтон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "округ Колумбия",
                                "target_lang_sense_clarification": "административная единица"
                            },
                            {
                                "target_lang_word": "Вашингтон",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2f3efbc-ca4a-3c32-8d36-437bd107df30",
                    "sense_definition": "Short for Detective Comics, a major American comic book publisher known for superheroes like Batman and Superman.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "brand_dc",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "The new movie features several famous <w>DC</w> characters working together.",
                            "target_lang_translations": {
                                "ru": "В новом фильме участвуют несколько известных персонажей <w>DC</w>, работающих вместе."
                            }
                        },
                        {
                            "text": "He has a large collection of vintage <w>DC</w> comic books.",
                            "target_lang_translations": {
                                "ru": "У него большая коллекция старинных комиксов <w>DC</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "DC Universe",
                        "DC movies"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от Detective Comics, крупного американского издательства комиксов, известного такими супергероями, как Бэтмен и Супермен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "DC",
                                "target_lang_sense_clarification": "бренд/издательство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "176b9fb7-ef07-3175-add5-83f36355631f",
                    "sense_definition": "Initialism of <w>direct</w> <w>current</w>: an electric current that flows in one direction only.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "electricity_dc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most batteries supply <w>DC</w> power to small electronic devices.",
                            "target_lang_translations": {
                                "ru": "Большинство батареек поставляют питание <w>постоянного тока</w> для небольших электронных устройств."
                            }
                        },
                        {
                            "text": "You need an adapter to convert the AC from the wall outlet into <w>DC</w>.",
                            "target_lang_translations": {
                                "ru": "Вам нужен адаптер, чтобы преобразовать переменный ток из розетки в <w>постоянный ток</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "AC"
                    ],
                    "common_phrases": [
                        "DC power",
                        "DC motor",
                        "AC-DC"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от direct current (постоянный ток): электрический ток, который течет только в одном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постоянный ток",
                                "target_lang_sense_clarification": "электричество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bc55adb-20b1-38c1-87d9-86c822b6c162",
                    "sense_definition": "Initialism of <w>detective</w> <w>constable</w>: a police rank used in the UK and other Commonwealth countries.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "police_dc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>DC</w> interviewed the witness at the police station yesterday.",
                            "target_lang_translations": {
                                "ru": "<w>Детектив-констебль</w> допросил свидетеля в полицейском участке вчера."
                            }
                        },
                        {
                            "text": "She was promoted to <w>DC</w> after serving three years as a uniformed officer.",
                            "target_lang_translations": {
                                "ru": "Её повысили до <w>детектива-констебля</w> после трех лет службы патрульным офицером."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK, Commonwealth"
                        },
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от detective constable (детектив-констебль): полицейское звание в Великобритании и других странах Содружества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детектив-констебль",
                                "target_lang_sense_clarification": "полицейское звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "915de944-722b-3b42-a65c-00acf1bd508a",
                    "sense_definition": "Initialism of <w>distribution</w> <w>center</w>: a facility where goods are stored before being distributed to retailers or customers.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "logistics_dc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shipment has just arrived at the regional <w>DC</w> for sorting.",
                            "target_lang_translations": {
                                "ru": "Груз только что прибыл в региональный <w>распределительный центр</w> для сортировки."
                            }
                        },
                        {
                            "text": "The company is building a new <w>DC</w> to speed up their delivery times.",
                            "target_lang_translations": {
                                "ru": "Компания строит новый <w>распределительный центр</w>, чтобы ускорить доставку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от distribution center (распределительный центр): объект, где товары хранятся перед отправкой в магазины или клиентам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распределительный центр",
                                "target_lang_sense_clarification": "логистика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50136cab-be35-3fd5-934b-1824243df048",
                    "sense_definition": "Initialism of <w>disconnection</w>: an instance of losing the connection to an internet network or server, especially in gaming.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "gaming_dc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player suffered a <w>DC</w> right in the middle of the important match.",
                            "target_lang_translations": {
                                "ru": "У игрока случился <w>дисконнект</w> прямо посреди важного матча."
                            }
                        },
                        {
                            "text": "Frequent <w>DCs</w> made the online game impossible to play smoothly.",
                            "target_lang_translations": {
                                "ru": "Частые <w>дисконнекты</w> сделали нормальную онлайн-игру невозможной."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gaming"
                        },
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от disconnection (разрыв соединения): случай потери связи с интернетом или сервером, особенно в играх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дисконнект",
                                "target_lang_sense_clarification": "игровой сленг"
                            },
                            {
                                "target_lang_word": "разрыв соединения",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "26cc98f1-5f8e-3636-9222-6e33b888b79f",
                    "sense_definition": "To become disconnected from a network or server.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gaming_dc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I keep <w>DCing</w> whenever the server gets too crowded with players.",
                            "target_lang_translations": {
                                "ru": "Я постоянно <w>вылетаю</w>, когда сервер переполнен игроками."
                            }
                        },
                        {
                            "text": "He <w>DC'd</w> from the chat room without saying goodbye to anyone.",
                            "target_lang_translations": {
                                "ru": "Он <w>отключился</w> от чата, не попрощавшись ни с кем."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gaming/internet"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отключиться от сети или сервера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вылететь",
                                "target_lang_sense_clarification": "сленг, потерять связь"
                            },
                            {
                                "target_lang_word": "отключиться",
                                "target_lang_sense_clarification": "потерять соединение"
                            }
                        ]
                    }
                }
            ]
        },
        {

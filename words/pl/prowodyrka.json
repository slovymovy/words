{
    "word_family": [
        "prowodyr"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "429951a6-e0dd-3fa2-82b5-94059a2ca0c0",
                    "sense_definition": "Żeńska forma rzeczownika <w>prowodyr</w>; kobieta, która stoi na czele grupy i inicjuje działania, często o charakterze buntu, spisku lub zamieszania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "leader_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To Żaneta była <w>prowodyrką</w> całego zajścia.",
                            "target_lang_translations": {
                                "ru": "Именно Жанета была <w>зачинщицей</w> всего инцидента."
                            }
                        },
                        {
                            "text": "Nauczyciele wezwali rodziców uczennicy, która okazała się główną <w>prowodyrką</w> hałasu na lekcji.",
                            "target_lang_translations": {
                                "ru": "Учителя вызвали родителей ученицы, которая оказалась главной <w>зачинщицей</w> шума на уроке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "główna prowodyrka",
                        "prowodyrka zamieszania",
                        "prowodyrka buntu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская форма существительного <w>prowodyr</w>; женщина, которая возглавляет группу и инициирует действия, часто имеющие характер бунта, заговора или беспорядков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачинщица",
                                "target_lang_sense_clarification": "инициатор ссоры, драки или беспорядков"
                            },
                            {
                                "target_lang_word": "подстрекательница",
                                "target_lang_sense_clarification": "та, кто побуждает к негативным действиям"
                            },
                            {
                                "target_lang_word": "предводительница",
                                "target_lang_sense_clarification": "главарь группы (часто иронично или в контексте шалостей)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bc28a69-deb4-3258-a1a9-b21d42154219",
                    "sense_definition": "Żeńska forma rzeczownika <w>prowodyr</w>; kobieta prowadząca tańce i zabawy podczas uroczystości, na przykład na weselu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "leader_dance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako doświadczona <w>prowodyrka</w>, zachęciła wszystkich gości do wyjścia na parkiet.",
                            "target_lang_translations": {
                                "ru": "Как опытная <w>распорядительница</w>, она побудила всех гостей выйти на танцпол."
                            }
                        },
                        {
                            "text": "Wesoła <w>prowodyrka</w> prowadziła barwny korowód przez całą salę balową.",
                            "target_lang_translations": {
                                "ru": "Веселая <w>предводительница</w> вела красочный хоровод через весь бальный зал."
                            }
                        }
                    ],
                    "synonyms": [
                        "wodzirejka"
                    ],
                    "common_phrases": [
                        "weselna prowodyrka",
                        "prowodyrka tańców"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская форма существительного <w>prowodyr</w>; женщина, ведущая танцы и игры во время торжеств, например, на свадьбе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распорядительница",
                                "target_lang_sense_clarification": "ведущая танцев или бала"
                            },
                            {
                                "target_lang_word": "заводила",
                                "target_lang_sense_clarification": "инициатор веселья, душа компании (разг.)"
                            },
                            {
                                "target_lang_word": "ведущая",
                                "target_lang_sense_clarification": "организатор развлечений на празднике"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    ],
                    "common_phrases": [
                        "lider tabeli",
                        "lider wyścigu",
                        "koszulka lidera",
                        "zostać liderem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортсмен или команда, занимающие первое место в соревнованиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лидер",

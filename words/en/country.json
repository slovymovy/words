                    "synonyms": [
                        "sovereign state",
                        "sovereign country",
                        "native",
                        "country"
                    ],
                    "common_phrases": [
                        "foreign country",
                        "developing country",
                        "across the country",
                        "home country"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория нации; суверенное государство или регион, отличающийся своими институтами и народом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страна",
                                "target_lang_sense_clarification": "территория, государство"
                            },
                            {
                                "target_lang_word": "государство",
                                "target_lang_sense_clarification": "политическая организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2952ba35-9f82-3caf-a8d7-02deeeae091b",
                    "sense_definition": "A rural area, as opposed to a town or city; the countryside.",

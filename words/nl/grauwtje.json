                },
                {
                    "sense_id": "d946076d-4e61-3e6d-b85d-c0ae9cf40a9d",
                    "sense_definition": "Een schilderij dat volledig in grijstinten is uitgevoerd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kunstenaar maakte een gedetailleerd <w>grauwtje</w> als voorstudie voor het grote werk.",
                            "target_lang_translations": {

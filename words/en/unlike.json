    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "59630d2c-c22d-3a19-ad01-abbea02f7198",
                    "sense_definition": "Different from; not similar to.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "different_from",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The taste of this fruit is <w>unlike</w> anything I have eaten before.",
                            "target_lang_translations": {
                                "ru": "Вкус этого фрукта <w>не похож на</w> всё, что я ел раньше."
                            }
                        },
                        {
                            "text": "Her teaching style is <w>unlike</w> mine, but it is still very effective.",
                            "target_lang_translations": {
                                "ru": "Её стиль преподавания <w>не похож на</w> мой, но всё же очень эффективен."
                            }
                        }
                    ],
                    "synonyms": [
                        "different from",
                        "dissimilar to"
                    ],
                    "antonyms": [
                        "like",
                        "similar to"
                    ],
                    "common_phrases": [
                        "unlike anything",
                        "unlike any other"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличный от; непохожий на."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непохожий на",
                                "target_lang_sense_clarification": "имеющий отличия"
                            },
                            {
                                "target_lang_word": "отличный от",
                                "target_lang_sense_clarification": "другой, отличающийся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa524cb3-6301-30e7-9dbb-f0e02607b220",
                    "sense_definition": "Used to indicate a contrast with someone or something else.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "contrast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Unlike</w> his brother, he loves playing sports and being outdoors.",
                            "target_lang_translations": {
                                "ru": "<w>В отличие от</w> своего брата, он любит заниматься спортом и бывать на свежем воздухе."
                            }
                        },
                        {
                            "text": "Claudia hardly ever drinks coffee, <w>unlike</w> Phillip who drinks it every day.",
                            "target_lang_translations": {
                                "ru": "Клаудия почти никогда не пьёт кофе, <w>в отличие от</w> Филиппа, который пьёт его каждый день."
                            }
                        }
                    ],
                    "synonyms": [
                        "in contrast to",
                        "as opposed to"
                    ],
                    "antonyms": [
                        "like"
                    ],
                    "common_phrases": [
                        "unlike most people",
                        "unlike many others"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на противопоставление кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в отличие от",
                                "target_lang_sense_clarification": "по сравнению с"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f72a43cd-6af1-389f-bf32-efdd85be877c",
                    "sense_definition": "Not typical of the character or usual behavior of someone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>unlike</w> him to be late for such an important meeting.",
                            "target_lang_translations": {
                                "ru": "Это <w>не похоже на</w> него — опаздывать на такую важную встречу."
                            }
                        },
                        {
                            "text": "Her sudden angry outburst was very <w>unlike</w> her.",
                            "target_lang_translations": {
                                "ru": "Её внезапная вспышка гнева была совсем <w>не похожа на</w> неё."
                            }
                        }
                    ],
                    "synonyms": [
                        "uncharacteristic of"
                    ],
                    "antonyms": [
                        "typical of",
                        "like"
                    ],
                    "common_phrases": [
                        "unlike him to",
                        "unlike her to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несвойственный характеру или обычному поведению кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не свойственный",
                                "target_lang_sense_clarification": "нетипичный для"
                            },
                            {
                                "target_lang_word": "не похожий на",
                                "target_lang_sense_clarification": "о поведении"
                            }
                        ]
                    }
                }
            ]
        },
        {

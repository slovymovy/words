            "senses": [
                {
                    "sense_id": "2470b9d2-9f00-3caa-8b2a-e3c154c10a4d",
                    "sense_definition": "Potoczne określenie młodej, atrakcyjnej dziewczyny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",

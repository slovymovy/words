    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e4a35302-be37-3256-9ed1-ac7347cb5678",
                    "sense_definition": "Something that is needed or demanded as a necessary condition or obligation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "necessity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A valid passport is a legal <w>requirement</w> for international travel.",
                            "target_lang_translations": {
                                "ru": "Действующий паспорт является юридическим <w>требованием</w> для международных путешествий."
                            }
                        },
                        {
                            "text": "You must meet the minimum <w>requirements</w> to apply for this job.",
                            "target_lang_translations": {
                                "ru": "Вы должны соответствовать минимальным <w>требованиям</w>, чтобы подать заявку на эту работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "necessity",
                        "prerequisite",
                        "condition",
                        "essential"
                    ],
                    "antonyms": [
                        "option"
                    ],
                    "common_phrases": [
                        "meet requirements",
                        "minimum requirement",
                        "legal requirement",
                        "entry requirement"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что необходимо или требуется как обязательное условие или обязанность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "требование",
                                "target_lang_sense_clarification": "обязательное условие, правило"
                            },
                            {
                                "target_lang_word": "необходимое условие",

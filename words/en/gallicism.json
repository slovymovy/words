            "senses": [
                {
                    "sense_id": "c2927ce8-7068-3ce0-b6d3-96d00f2b1b58",
                    "sense_definition": "A word or phrase borrowed from the French language.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The student used a <w>gallicism</w> in his essay to sound more sophisticated.",
                            "target_lang_translations": {
                                "ru": "Студент использовал <w>галлицизм</w> в своем эссе, чтобы звучать более утонченно."
                            }
                        },
                        {
                            "text": "Critics noted that the translation contained a common <w>gallicism</w> that sounded unnatural in English.",
                            "target_lang_translations": {
                                "ru": "Критики отметили, что перевод содержал распространенный <w>галлицизм</w>, который звучал неестественно на английском языке."
                            }
                        }
                    ],
                    "synonyms": [
                        "Frenchism"
                    ],
                    "common_phrases": [
                        "use a gallicism",
                        "common gallicism"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово или оборот речи, заимствованные из французского языка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "галлицизм",
                                "target_lang_sense_clarification": "лингвистический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "971fb9b1-fd2e-3a3c-a767-ad159b9014df",
                    "sense_definition": "A behavior or characteristic typical of French people.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her expressive shrug was a charming <w>gallicism</w> she picked up in Paris.",
                            "target_lang_translations": {
                                "ru": "Ее выразительное пожатие плечами было очаровательной <w>французской манерой</w>, которую она переняла в Париже."
                            }
                        },
                        {
                            "text": "He displayed a certain <w>gallicism</w> in his approach to long, leisurely meals.",
                            "target_lang_translations": {
                                "ru": "В его подходе к долгим, неспешным трапезам проявлялась некая <w>французская черта</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "typical gallicism"
                    ],
                    "target_lang_definitions": {

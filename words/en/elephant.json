                {
                    "sense_id": "9e69e148-4154-3cf5-a3ca-be5ed4ff8870",
                    "sense_definition": "A very large grey mammal with a long trunk, large ears, and tusks, native to Africa and Asia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>elephant</w> used its trunk to pick up a peanut from the ground.",
                            "target_lang_translations": {
                                "ru": "<w>Слон</w> использовал свой хобот, чтобы поднять с земли арахис."
                            }
                        },
                        {
                            "text": "We saw a herd of wild <w>elephants</w> during our safari in Kenya.",
                            "target_lang_translations": {
                                "ru": "Мы видели стадо диких <w>слонов</w> во время сафари в Кении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "African elephant",
                        "Asian elephant",
                        "white elephant",
                        "elephant in the room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень крупное серое млекопитающее с длинным хоботом, большими ушами и бивнями, обитающее в Африке и Азии."
                    },
                    "translations": {

                    ],
                    "antonyms": [
                        "całkowity",
                        "końcowy"
                    ],
                    "common_phrases": [
                        "płaca podstawowa",
                        "wersja podstawowa",
                        "wymiar podstawowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определяющий постоянную, исходную величину (например, зарплаты или услуги), без учета надбавок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "базовый",
                                "target_lang_sense_clarification": "исходный, стартовый"
                            },
                            {
                                "target_lang_word": "основной",
                                "target_lang_sense_clarification": "об окладе"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1cc902cc-a344-3339-9ed6-20338c0405ea",
                    "sense_definition": "Zawierający dużo tłuszczu (zwykle o jedzeniu).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_composition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zalecił mi unikać <w>tłustych</w> potraw ze względu na wysoki cholesterol.",
                            "target_lang_translations": {
                                "ru": "Врач посоветовал мне избегать <w>жирных</w> блюд из-за высокого холестерина."
                            }
                        },
                        {
                            "text": "To <w>tłuste</w> mleko jest znacznie smaczniejsze do kawy niż chude.",
                            "target_lang_translations": {
                                "ru": "Это <w>жирное</w> молоко гораздо вкуснее в кофе, чем обезжиренное."
                            }
                        }
                    ],
                    "antonyms": [
                        "chudy",
                        "beztłuszczowy"
                    ],
                    "common_phrases": [
                        "tłuste mleko",
                        "tłusta śmietana",
                        "tłuste mięso",
                        "tłusta potrawa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий много жира (обычно о еде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жирный",
                                "target_lang_sense_clarification": "о пище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b468e428-c732-37cd-9a78-8e1544ca4728",
                    "sense_definition": "O człowieku lub zwierzęciu: mający nadmiar tkanki tłuszczowej, otyły.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>tłusty</w> kot ledwo mieści się w swoim małym legowisku.",
                            "target_lang_translations": {
                                "ru": "Мой <w>жирный</w> кот едва помещается в свою маленькую лежанку."
                            }
                        },
                        {
                            "text": "Ten <w>tłusty</w> mężczyzna z trudem wchodził po schodach na trzecie piętro.",
                            "target_lang_translations": {
                                "ru": "Этот <w>тучный</w> мужчина с трудом поднимался по лестнице на третий этаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "gruby",
                        "opasły",
                        "spasiony",
                        "gw. (Śląsk Cieszyński) pencaty"
                    ],
                    "antonyms": [
                        "chudy"
                    ],
                    "common_phrases": [
                        "tłusty brzuch",
                        "tłusty kot",
                        "tłusta świnia"
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке или животном: имеющий избыточный вес, тучный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толстый",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "жирный",
                                "target_lang_sense_clarification": "разговорное, часто грубое"
                            },
                            {
                                "target_lang_word": "тучный",
                                "target_lang_sense_clarification": "офиц., об очень полном человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab2a8859-e75d-3673-a794-f7f1d2ea2c5d",
                    "sense_definition": "Pokryty warstwą tłuszczu, oleisty lub brudny od tłuszczu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "surface_texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę umyć głowę, bo mam już bardzo <w>tłuste</w> włosy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно помыть голову, потому что у меня уже очень <w>жирные</w> волосы."
                            }
                        },
                        {
                            "text": "Na czystym obrusie została brzydka, <w>tłusta</w> plama po rosole.",
                            "target_lang_translations": {
                                "ru": "На чистой скатерти осталось некрасивое <w>жирное</w> пятно от бульона."
                            }
                        }
                    ],
                    "synonyms": [
                        "mazisty"
                    ],
                    "antonyms": [
                        "suchy"
                    ],
                    "common_phrases": [
                        "tłuste włosy",
                        "tłuste plamy",
                        "tłusta cera",
                        "tłuste ręce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытый слоем жира, маслянистый или грязный от жира."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жирный",
                                "target_lang_sense_clarification": "покрытый жиром"
                            },
                            {
                                "target_lang_word": "сальный",
                                "target_lang_sense_clarification": "грязный, засаленный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ab0fb29-e7db-38f7-a1f1-95ead2f67fca",
                    "sense_definition": "O druku lub piśmie: pogrubiony, wyróżniony grubszą kreską.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "typography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Najważniejsze informacje w tekście zostały wyróżnione <w>tłustym</w> drukiem.",
                            "target_lang_translations": {
                                "ru": "Самая важная информация в тексте была выделена <w>жирным</w> шрифтом."
                            }
                        },
                        {
                            "text": "Proszę pisać <w>tłustą</w> kreską, żeby wszystko było dobrze widoczne z daleka.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, пишите <w>жирной</w> чертой, чтобы всё было хорошо видно издалека."
                            }
                        }
                    ],
                    "synonyms": [
                        "gruby"
                    ],
                    "antonyms": [
                        "cienki"
                    ],
                    "common_phrases": [
                        "tłusty druk",
                        "tłusta czcionka",
                        "tłusta kreska"
                    ],
                    "target_lang_definitions": {
                        "ru": "О шрифте или линии: утолщённый, выделенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жирный",
                                "target_lang_sense_clarification": "о шрифте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7df110e7-e827-36c6-b659-ca1a0b19f7f7",
                    "sense_definition": "Potocznie: bardzo dobry, atrakcyjny lub bogaty (o okresie czasu).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To były <w>tłuste</w> lata dla naszej firmy, zarobiliśmy wtedy mnóstwo pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Это были <w>тучные</w> годы для нашей фирмы, мы тогда заработали кучу денег."
                            }
                        },
                        {
                            "text": "Producent przygotował naprawdę <w>tłusty</w> podkład muzyczny pod ten nowy utwór.",
                            "target_lang_translations": {
                                "ru": "Продюсер подготовил действительно <w>жирный</w> музыкальный фон для этого нового трека."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tłuste lata",
                        "tłusty bit",
                        "tłusty czwartek"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное: очень хороший, прибыльный или богатый (о периоде времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тучный",
                                "target_lang_sense_clarification": "о годах, изобильный"
                            },
                            {
                                "target_lang_word": "жирный",
                                "target_lang_sense_clarification": "сленг: отличный, насыщенный"
                            },
                            {
                                "target_lang_word": "сытый",
                                "target_lang_sense_clarification": "о времени: благополучный"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "żydowski",
        "żydówka",
        "żydostwo"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "356a64c0-814b-35f7-a9d0-fc1faf97d333",
                    "sense_definition": "Członek narodu żydowskiego; osoba mająca żydowskie pochodzenie etniczne lub kulturowe (zapisywane wielką literą).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ethnicity",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "Współcześni <Żydzi> pielęgnują tradycje i historię swoich przodków.",
                            "target_lang_translations": {
                                "ru": "Современные <евреи> берегут традиции и историю своих предков."
                            }
                        },
                        {
                            "text": "W tym mieście przed wojną mieszkali obok siebie Polacy i <Żydzi>.",
                            "target_lang_translations": {
                                "ru": "В этом городе до войны жили рядом поляки и <евреи>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Izraelita",
                        "Hebrajczyk"
                    ],
                    "common_phrases": [
                        "naród żydowski",
                        "kultura żydowska",
                        "historia Żydów"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "pisane wielką literą"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член еврейского народа; лицо, имеющее еврейское этническое или культурное происхождение (пишется с большой буквы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "еврей",
                                "target_lang_sense_clarification": "национальность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "360ad689-bb84-3ec4-a521-bce161384e8e",
                    "sense_definition": "Wyznawca judaizmu, religii mojżeszowej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy pobożny <żyd> ściśle przestrzega zasad szabatu.",
                            "target_lang_translations": {
                                "ru": "Каждый набожный <еврей> строго соблюдает правила шаббата."
                            }
                        },
                        {
                            "text": "W synagodze zebrali się ortodoksyjni <żydzi> na wspólną modlitwę.",
                            "target_lang_translations": {
                                "ru": "В синагоге собрались ортодоксальные <иудеи> на совместную молитву."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyznawca judaizmu",
                        "starozakonny"
                    ],
                    "antonyms": [
                        "goj"
                    ],
                    "common_phrases": [
                        "ortodoksyjny żyd",
                        "pobożny żyd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последователь иудаизма, иудейской религии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иудей",
                                "target_lang_sense_clarification": "религиозная принадлежность"
                            },
                            {
                                "target_lang_word": "еврей",
                                "target_lang_sense_clarification": "последователь веры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd0f3214-e3c1-3f68-9159-61e373332bb1",
                    "sense_definition": "Osoba bardzo skąpa, chciwa i nadmiernie oszczędna (określenie obraźliwe).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź taki <żyd> i pożycz mi te kilka złotych na bilet.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <жмотом> и одолжи мне эти несколько злотых на билет."
                            }
                        },
                        {
                            "text": "Zachował się jak <żyd>, nie zostawiając kelnerowi ani grosza napiwku.",
                            "target_lang_translations": {
                                "ru": "Он повел себя как <скряга>, не оставив официанту ни гроша чаевых."
                            }
                        }
                    ],
                    "synonyms": [
                        "skąpiec",
                        "sknera",
                        "chytrus",
                        "dusigrosz",
                        "centuś"
                    ],
                    "antonyms": [
                        "hojny",
                        "szczodry",
                        "rozrzutny"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "obraźliwe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень скупой, жадный и чрезмерно экономный человек (оскорбительное определение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скряга",
                                "target_lang_sense_clarification": "скупой человек"
                            },
                            {
                                "target_lang_word": "жмот",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "жид",
                                "target_lang_sense_clarification": "оскорбительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a987c243-c40a-35ed-978f-85bc2a7afc2b",
                    "sense_definition": "Duża, nieestetyczna plama z atramentu na papierze.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pióro wylało mi w trakcie pisania i w zeszycie zrobił się wielki <żyd>.",
                            "target_lang_translations": {
                                "ru": "Ручка потекла во время письма, и в тетради получилась огромная <клякса>."
                            }
                        },
                        {
                            "text": "Nauczycielka obniżyła ocenę za liczne <żydy> i skreślenia w wypracowaniu.",
                            "target_lang_translations": {
                                "ru": "Учительница снизила оценку за многочисленные <кляксы> и зачеркивания в сочинении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zrobić żyda"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "gwara uczniowska"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое, неэстетичное чернильное пятно на бумаге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клякса",
                                "target_lang_sense_clarification": "чернильное пятно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f3a810f9-4c3f-39c1-bae8-931415ff75cf",
                    "sense_definition": "Met veel betekenis, invloed of waarde; iets waar men veel aandacht aan besteedt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <w>belangrijk</w> dat je goed naar de leraar luistert.",
                            "target_lang_translations": {
                                "en": "It is <w>important</w> that you listen closely to the teacher.",
                                "ru": "<w>Важно</w>, чтобы ты внимательно слушал учителя."
                            }
                        },
                        {
                            "text": "Gezondheid is voor de meeste mensen het <w>belangrijkste</w> in het leven.",
                            "target_lang_translations": {
                                "en": "For most people, health is the most <w>important</w> thing in life.",
                                "ru": "Для большинства людей здоровье — самое <w>важное</w> в жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "voornaam"
                    ],
                    "antonyms": [
                        "onbelangrijk"
                    ],
                    "common_phrases": [
                        "erg belangrijk",
                        "het belangrijkste punt",
                        "belangrijk nieuws"
                    ],
                    "target_lang_definitions": {
                        "en": "Having great meaning, influence, or value; something that requires attention.",
                        "ru": "Имеющий большое значение, влияние или ценность; то, чему уделяется много внимания."
                    },
                    "translations": {

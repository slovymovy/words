{
    "word_family": [
        "prosecute",
        "prosecutor",
        "prosecutorial"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7418b36b-f2c0-3a69-8f10-1616e618d74d",
                    "sense_definition": "The process of officially charging someone with a crime and trying to prove they are guilty in a court of law.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He could face criminal <w>prosecution</w> for his involvement in the fraud scheme.",
                            "target_lang_translations": {
                                "ru": "Ему может грозить <w>уголовное преследование</w> за участие в мошеннической схеме."
                            }
                        },
                        {
                            "text": "The evidence was insufficient to support a successful <w>prosecution</w> against the suspect.",
                            "target_lang_translations": {
                                "ru": "Доказательств было недостаточно для поддержания успешного <w>обвинения</w> против подозреваемого."
                            }
                        }
                    ],
                    "synonyms": [
                        "trial",
                        "legal action",
                        "indictment"
                    ],
                    "antonyms": [
                        "defense"
                    ],
                    "common_phrases": [
                        "face prosecution",
                        "criminal prosecution",
                        "liable to prosecution",
                        "escape prosecution"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс официального предъявления обвинения в совершении преступления и попытка доказать вину в суде"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судебное преследование",
                                "target_lang_sense_clarification": "процесс привлечения к суду"
                            },
                            {
                                "target_lang_word": "уголовное преследование",
                                "target_lang_sense_clarification": "в уголовном праве"
                            },
                            {
                                "target_lang_word": "обвинение",
                                "target_lang_sense_clarification": "предъявление вины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3930254-4c9f-37db-b2df-786948e0f293",
                    "sense_definition": "The lawyers who try to prove in a court of law that someone is guilty of a crime.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_party",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>prosecution</w> called their main witness to the stand early in the trial.",
                            "target_lang_translations": {
                                "ru": "<w>Обвинение</w> вызвало своего главного свидетеля для дачи показаний в начале судебного процесса."
                            }
                        },
                        {
                            "text": "After presenting all the evidence, the <w>prosecution</w> rests its case.",
                            "target_lang_translations": {
                                "ru": "Представив все доказательства, <w>сторона обвинения</w> завершила своё выступление."
                            }
                        }
                    ],
                    "synonyms": [
                        "prosecutor",
                        "state"
                    ],
                    "antonyms": [
                        "defense"
                    ],
                    "common_phrases": [
                        "witness for the prosecution",
                        "prosecution case",
                        "the prosecution rests"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сторона в судебном процессе (юристы), пытающаяся доказать виновность подсудимого"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обвинение",
                                "target_lang_sense_clarification": "сторона в суде"
                            },
                            {
                                "target_lang_word": "сторона обвинения",
                                "target_lang_sense_clarification": "официальный термин"
                            },
                            {
                                "target_lang_word": "прокуратура",
                                "target_lang_sense_clarification": "государственный орган"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff5cf299-b734-394f-ae8e-db81013d1d06",
                    "sense_definition": "The act of continuing with or carrying out a specific task, plan, or war.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "formal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general was responsible for the successful <w>prosecution</w> of the war.",
                            "target_lang_translations": {
                                "ru": "Генерал отвечал за успешное <w>ведение</w> войны."
                            }
                        },
                        {
                            "text": "They dedicated significant resources to the <w>prosecution</w> of their business duties.",
                            "target_lang_translations": {
                                "ru": "Они выделили значительные ресурсы на <w>исполнение</w> своих деловых обязанностей."
                            }
                        }
                    ],
                    "synonyms": [
                        "continuation",
                        "pursuit",
                        "conduct",
                        "execution"
                    ],
                    "common_phrases": [
                        "prosecution of the war"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по продолжению или выполнению определенной задачи, плана или ведению войны"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ведение",
                                "target_lang_sense_clarification": "войны или дел"
                            },
                            {
                                "target_lang_word": "осуществление",
                                "target_lang_sense_clarification": "деятельности или планов"
                            },
                            {
                                "target_lang_word": "исполнение",
                                "target_lang_sense_clarification": "обязанностей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
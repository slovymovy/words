                {
                    "sense_id": "3ee2b41d-575e-3728-b7fc-baf9684c2645",
                    "sense_definition": "Made of wood or resembling wood in appearance, texture, or smell.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "resembling_wood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old library had a distinct <w>woody</w> smell that reminded him of his grandfather's house.",
                            "target_lang_translations": {
                                "ru": "В старой библиотеке стоял отчетливый <w>древесный</w> запах, напоминавший ему о доме дедушки."
                            }
                        },
                        {
                            "text": "Turnips can develop a tough, <w>woody</w> texture if they are left in the ground too long.",
                            "target_lang_translations": {
                                "ru": "Репа может приобрести жесткую, <w>деревянистую</w> текстуру, если ее слишком долго не выкапывать."
                            }

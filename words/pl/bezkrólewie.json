    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2335b6ef-2062-393f-8e3b-ee04e91dd254",
                    "sense_definition": "Okres pomiędzy śmiercią lub abdykacją króla a koronacją jego następcy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "political_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po śmierci ostatniego władcy w kraju nastało długie <w>bezkrólewie</w>.",
                            "target_lang_translations": {
                                "ru": "После смерти последнего правителя в стране наступило долгое <w>междуцарствие</w>."
                            }
                        },
                        {
                            "text": "Ten burzliwy okres bezkrólewia zakończył się wreszcie wyborem nowego monarchy.",
                            "target_lang_translations": {
                                "ru": "Этот бурный период <w>междуцарствия</w> закончился наконец избранием нового монарха."
                            }
                        }
                    ],
                    "synonyms": [
                        "interregnum",
                        "międzykrólewie"
                    ],
                    "common_phrases": [
                        "okres bezkrólewia",
                        "czas bezkrólewia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период между смертью или отречением короля и коронацией его преемника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "междуцарствие",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5263d77f-53af-3fd0-94a5-6f16738b3956",
                    "sense_definition": "Chaos, anarchia lub brak porządku w grupie lub organizacji spowodowany brakiem osoby kierującej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chaos_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy dyrektor nagle zrezygnował, w firmie zapanowało kompletne <w>bezkrólewie</w>.",
                            "target_lang_translations": {
                                "ru": "Когда директор внезапно уволился, в фирме воцарилось полное <w>безначалие</w>."
                            }
                        },
                        {
                            "text": "Przez to organizacyjne <w>bezkrólewie</w> nikt nie wiedział, jakie ma obowiązki.",
                            "target_lang_translations": {
                                "ru": "Из-за этого организационного <w>безначалия</w> никто не знал, какие у него обязанности."
                            }
                        }
                    ],
                    "synonyms": [
                        "anarchia",
                        "bezrząd",
                        "bezhołowie"
                    ],
                    "common_phrases": [
                        "panuje bezkrólewie",
                        "totalne bezkrólewie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хаос, анархия или отсутствие порядка в группе или организации, вызванные отсутствием руководителя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безначалие",
                                "target_lang_sense_clarification": "отсутствие руководства"
                            },
                            {
                                "target_lang_word": "безвластие",
                                "target_lang_sense_clarification": "отсутствие власти"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
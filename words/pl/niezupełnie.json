    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "dbc63e65-43fc-36f3-b919-45b987ab1941",
                    "sense_definition": "Słowo używane do wyrażenia braku pełnej zgody z rozmówcą lub wątpliwości co do trafności jakiegoś stwierdzenia; łagodniejsza forma zaprzeczenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pragmatic_marker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "– Czy jesteś zadowolony z wyniku meczu? – <w>Niezupełnie</w>.",
                            "target_lang_translations": {
                                "ru": "– Ты доволен результатом матча? – <w>Не совсем</w>."
                            }
                        },
                        {
                            "text": "To <w>niezupełnie</w> tak, jak myślisz, sytuacja jest bardziej skomplikowana.",
                            "target_lang_translations": {
                                "ru": "Это <w>не совсем</w> так, как ты думаешь, ситуация сложнее."
                            }
                        }
                    ],
                    "synonyms": [
                        "niecałkowicie",
                        "niecałkowicie",
                        "niecałkowicie"
                    ],
                    "antonyms": [
                        "dokładnie",
                        "właśnie"
                    ],
                    "common_phrases": [
                        "to niezupełnie tak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое для выражения неполного согласия или сомнения; мягкая форма отрицания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не совсем",
                                "target_lang_sense_clarification": "выражение неполного согласия"
                            },
                            {
                                "target_lang_word": "не вполне",
                                "target_lang_sense_clarification": "более формальный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d334315-5478-3dd8-a217-abca997530c6",
                    "sense_definition": "W sposób niecałkowity, nie w pełni lub nie do końca; w ograniczonym stopniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego zachowanie było <w>niezupełnie</w> zgodne z przyjętymi zasadami.",
                            "target_lang_translations": {
                                "ru": "Его поведение <w>не вполне</w> соответствовало принятым правилам."
                            }
                        },
                        {
                            "text": "Czułem się w tym nowym miejscu <w>niezupełnie</w> bezpiecznie.",
                            "target_lang_translations": {
                                "ru": "Я чувствовал себя в этом новом месте <w>не совсем</w> безопасно."
                            }
                        }
                    ],
                    "synonyms": [
                        "częściowo",
                        "niecałkowicie",
                        "niecałkowicie"
                    ],
                    "antonyms": [
                        "całkowicie",
                        "zupełnie",
                        "w pełni"
                    ],
                    "common_phrases": [
                        "niezupełnie jasne",
                        "niezupełnie gotowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не полностью, не до конца, в ограниченной степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не совсем",
                                "target_lang_sense_clarification": "не полностью"
                            },
                            {
                                "target_lang_word": "не вполне",
                                "target_lang_sense_clarification": "не в полной мере"
                            },
                            {
                                "target_lang_word": "не полностью",
                                "target_lang_sense_clarification": "частично"
                            }
                        ]
                    }

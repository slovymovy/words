                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рубить",
                                "target_lang_sense_clarification": "говорить резко и прямо"
                            },
                            {
                                "target_lang_word": "выкладывать",
                                "target_lang_sense_clarification": "рассказывать всё начистоту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41fc8958-2e88-3f89-8018-6dab7ed56082",
                    "sense_definition": "Pić alkohol w dużych ilościach, często szybko i łapczywie",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wczorajszej imprezie wszyscy <walili> wódkę kieliszek za kieliszkiem.",
                            "target_lang_translations": {
                                "ru": "На вчерашней вечеринке все <глушили> водку рюмку за рюмкой."
                            }
                        },
                        {
                            "text": "Nie możesz codziennie <walić> taniego wina, bo zniszczysz sobie zdrowie.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь каждый день <хлестать> дешевое вино, потому что испортишь себе здоровье."
                            }
                        }
                    ],
                    "synonyms": [
                        "chlać",
                        "pić",
                        "łoić",
                        "grzać"
                    ],
                    "common_phrases": [

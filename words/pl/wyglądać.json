        "wgląd"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2222090e-77d8-3e10-ba91-1465b4276968",
                    "sense_definition": "Mieć określony wygląd zewnętrzny, prezentować się w pewien sposób",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "appearance_impression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twoja nowa sukienka sprawia, że <wyglądasz> bardzo elegancko.",
                            "target_lang_translations": {
                                "ru": "В твоем новом платье ты <выглядишь> очень элегантно."
                            }
                        },
                        {
                            "text": "Mój brat <wygląda> na starszego niż jest w rzeczywistości.",
                            "target_lang_translations": {
                                "ru": "Мой брат <выглядит> старше, чем он есть на самом деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "zazierać",
                        "wyzierać"
                    ],
                    "common_phrases": [
                        "wyglądać świetnie",
                        "wyglądać na zmęczonego",
                        "dobrze wyglądać",
                        "wyglądać jak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь определенный внешний вид, смотреться"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выглядеть",
                                "target_lang_sense_clarification": "иметь вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbf3bd27-ad44-3c3c-b4c2-e0dd253613a9",
                    "sense_definition": "Sprawiać wrażenie, zdawać się wskazywać na coś (często w konstrukcjach bezosobowych)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "appearance_impression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Wygląda> na to, że musimy zmienić nasze plany na weekend.",
                            "target_lang_translations": {
                                "ru": "<Похоже> на то, что нам придется изменить наши планы на выходные."
                            }
                        },
                        {
                            "text": "Niebo pociemniało i <wyglądało> na burzę.",
                            "target_lang_translations": {
                                "ru": "Небо потемнело, и <похоже было> на грозу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wygląda na to",
                        "wygląda na deszcz",
                        "to źle wygląda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить впечатление, казаться (часто в безличных конструкциях)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "похоже",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "казаться",
                                "target_lang_sense_clarification": "производить впечатление"
                            },
                            {
                                "target_lang_word": "выглядеть",
                                "target_lang_sense_clarification": "о ситуации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41c9c01a-faa2-34d0-84c7-be474d78e6a2",
                    "sense_definition": "Patrzeć na zewnątrz, wychylając się z wnętrza pomieszczenia lub zza przeszkody",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Często <wyglądam> przez okno, obserwując przechodniów na ulicy.",
                            "target_lang_translations": {
                                "ru": "Я часто <выглядываю> в окно, наблюдая за прохожими на улице."
                            }
                        },
                        {
                            "text": "Pies <wyglądał> z budy, szczekając na listonosza.",
                            "target_lang_translations": {
                                "ru": "Собака <выглядывала> из конуры, лая на почтальона."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyzierać"
                    ],
                    "common_phrases": [
                        "wyglądać przez okno",
                        "wyglądać na ulicę",
                        "wyglądać na zewnątrz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смотреть наружу, высовываясь из помещения или из-за преграды"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выглядывать",
                                "target_lang_sense_clarification": "смотреть наружу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d71b7eb-710c-3851-848f-d38556a19ee8",
                    "sense_definition": "Oczekiwać na czyjeś nadejście, intensywnie patrząc w stronę, z której ma nadejść",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "visual_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyna stała na brzegu i z nadzieją <wyglądała> statku.",
                            "target_lang_translations": {
                                "ru": "Девушка стояла на берегу и с надеждой <высматривала> корабль."
                            }
                        },
                        {
                            "text": "Z niecierpliwością <wyglądamy> dobrych wieści od lekarza.",
                            "target_lang_translations": {
                                "ru": "Мы с нетерпением <ждем> хороших новостей от врача."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypatrywać",
                        "oczekiwać"
                    ],
                    "common_phrases": [
                        "wyglądać gości",
                        "wyglądać powrotu",
                        "wyglądać ratunku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ожидать чьего-либо появления, всматриваясь вдаль"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высматривать",
                                "target_lang_sense_clarification": "искать глазами"
                            },
                            {
                                "target_lang_word": "ожидать",
                                "target_lang_sense_clarification": "ждать появления"
                            },
                            {
                                "target_lang_word": "поджидать",
                                "target_lang_sense_clarification": "ждать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2627038-80d1-3efa-9895-2bba29b6deda",
                    "sense_definition": "Być częściowo widocznym, wystawać zza czegoś",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "protruding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spod koca <wyglądała> tylko mała głowa kota.",
                            "target_lang_translations": {
                                "ru": "Из-под одеяла <выглядывала> только маленькая голова кота."
                            }
                        },
                        {
                            "text": "Wiosną pierwsze kwiaty nieśmiało <wyglądają> spod śniegu.",
                            "target_lang_translations": {
                                "ru": "Весной первые цветы робко <выглядывают> из-под снега."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyzierać"
                    ],
                    "common_phrases": [
                        "słońce wygląda",
                        "wyglądać zza chmur",
                        "wyglądać z kieszeni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть частично видным, выступать из-за чего-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выглядывать",
                                "target_lang_sense_clarification": "виднеться"
                            },
                            {
                                "target_lang_word": "торчать",
                                "target_lang_sense_clarification": "выступать наружу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    ],
                    "common_phrases": [
                        "warmte afgeven",
                        "warmte opnemen",
                        "warmte vasthouden"
                    ],
                    "target_lang_definitions": {
                        "en": "A form of energy produced by the movement of atoms and molecules.",
                        "ru": "Форма энергии, возникающая в результате движения атомов и молекул."
                    },

{
    "word_family": [
        "Mosaic"
    ],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c41538c1-3dbe-3b16-9eff-b1f0afb78c05",
                    "sense_definition": "The Hebrew prophet and leader who, according to Abrahamic religions, led the Israelites out of slavery in Egypt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "biblical_figure",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "The story describes how <w>Moses</w> parted the Red Sea to save his people.",
                            "target_lang_translations": {
                                "ru": "История описывает, как <w>Моисей</w> разделил Красное море, чтобы спасти свой народ."
                            }
                        },
                        {
                            "text": "Many important laws in the scriptures are attributed to <w>Moses</w>.",
                            "target_lang_translations": {
                                "ru": "Многие важные законы в писаниях приписываются <w>Моисею</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Moshe"
                    ],
                    "common_phrases": [
                        "Law of Moses"
                    ],
                    "target_lang_definitions": {
                        "ru": "Еврейский пророк и лидер, который, согласно авраамическим религиям, вывел израильтян из рабства в Египте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Моисей",
                                "target_lang_sense_clarification": "библейский пророк"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d212e16c-1bed-3ff9-8270-c85bb4d514ba",
                    "sense_definition": "A male given name of Hebrew origin.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "given_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We decided to name our newborn son <w>Moses</w>.",
                            "target_lang_translations": {
                                "ru": "Мы решили назвать нашего новорожденного сына <w>Моисеем</w>."
                            }
                        },
                        {
                            "text": "My friend <w>Moses</w> plays the guitar very well.",
                            "target_lang_translations": {
                                "ru": "Мой друг <w>Мозес</w> очень хорошо играет на гитаре."
                            }
                        }
                    ],
                    "synonyms": [
                        "Moishe",
                        "Moshe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя еврейского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Моисей",
                                "target_lang_sense_clarification": "мужское имя (традиционное)"
                            },
                            {
                                "target_lang_word": "Мозес",
                                "target_lang_sense_clarification": "мужское имя (в англоязычном контексте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b75b2717-09fb-31dc-8623-a9b6720c81cf",
                    "sense_definition": "A surname derived from the given name.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Moses</w> will see you in his office now.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Мозес</w> примет вас в своем кабинете сейчас."
                            }
                        },
                        {
                            "text": "The <w>Moses</w> family has lived in this town for generations.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Мозес</w> живет в этом городе уже несколько поколений."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "transferred from the given name"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, образованная от данного имени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мозес",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d18a730-ee42-3772-bd78-51aca758dbf8",
                    "sense_definition": "A pseudonym used for Harriet Tubman, an American abolitionist and political activist.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_alias",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Harriet Tubman was often called <w>Moses</w> by those she helped escape to freedom.",
                            "target_lang_translations": {
                                "ru": "Гарриет Табмен часто называли <w>Моисеем</w> те, кому она помогла бежать на свободу."
                            }
                        },
                        {
                            "text": "The enslaved people whispered the name <w>Moses</w> with hope.",
                            "target_lang_translations": {
                                "ru": "Порабощенные люди с надеждой шептали имя <w>Моисей</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Псевдоним Гарриет Табмен, американской аболиционистки и политической активистки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Моисей",
                                "target_lang_sense_clarification": "прозвище исторической личности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3fb7d0b-ec20-3bf4-8592-ecda7045fee9",
                    "sense_definition": "A dialect of the Columbia-Wenatchi language spoken in the Pacific Northwest.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "language",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Linguists are working to preserve the <w>Moses</w> dialect.",
                            "target_lang_translations": {
                                "ru": "Лингвисты работают над сохранением диалекта <w>мозес</w>."
                            }
                        },
                        {
                            "text": "Very few fluent speakers of <w>Moses</w> remain today.",
                            "target_lang_translations": {
                                "ru": "Сегодня осталось очень мало людей, свободно говорящих на <w>мозес</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Диалект языка коламбия-уэнатчи, на котором говорят на северо-западе Тихого океана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мозес",
                                "target_lang_sense_clarification": "название диалекта"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "aa356de3-80cf-3dac-8d65-02d486b01f2d",
                    "sense_definition": "An exclamation used to express surprise, shock, or dismay.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Holy <w>Moses</w>, look at the size of that wave!",
                            "target_lang_translations": {
                                "ru": "Святой <w>Моисей</w>, посмотри на размер этой волны!"
                            }
                        },
                        {
                            "text": "<w>Moses</w>, I did not expect to see you here!",
                            "target_lang_translations": {
                                "ru": "<w>Боже</w>, я не ожидал увидеть тебя здесь!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "Holy Moses"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in the phrase 'Holy Moses'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, используемое для выражения удивления, шока или смятения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Боже",
                                "target_lang_sense_clarification": "восклицание"
                            },
                            {
                                "target_lang_word": "Святой Моисей",
                                "target_lang_sense_clarification": "идиоматическое выражение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
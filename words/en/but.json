{
    "word_family": [],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "5495fbcd-9e7c-3eb9-afef-c5159b595492",
                    "sense_definition": "Used to introduce a clause that contrasts with or contradicts the preceding clause or sentence.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "contrast_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is very old <w>but</w> still looks active and healthy.",
                            "target_lang_translations": {
                                "ru": "Она очень стара, <w>но</w> всё ещё выглядит активной и здоровой."
                            }
                        },
                        {
                            "text": "I would like to go with you, <w>but</w> I have too much work to do.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы пойти с тобой, <w>но</w> у меня слишком много работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "however",
                        "although",
                        "yet",
                        "nevertheless"
                    ],
                    "common_phrases": [
                        "but also",
                        "but then",
                        "but still"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения части предложения, которая противопоставляется или противоречит предыдущей части или предложению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "но",
                                "target_lang_sense_clarification": "противительный союз"
                            },
                            {
                                "target_lang_word": "однако",
                                "target_lang_sense_clarification": "союз, выражающий противопоставление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4979f1b-313b-3bd4-8e9d-a9025f95fb36",
                    "sense_definition": "Used at the beginning of a sentence to express opposition, surprise, or protest regarding a previous remark or situation.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "contrast_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>But</w> I never said you could drive my car!",
                            "target_lang_translations": {
                                "ru": "<w>Но</w> я никогда не говорил, что ты можешь брать мою машину!"
                            }
                        },
                        {
                            "text": "<w>But</w> why do we have to leave so early?",
                            "target_lang_translations": {
                                "ru": "<w>Но</w> почему мы должны уезжать так рано?"
                            }
                        }
                    ],
                    "synonyms": [
                        "however",
                        "yet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в начале предложения для выражения возражения, удивления или протеста по поводу предыдущего замечания или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "но",
                                "target_lang_sense_clarification": "в начале предложения для возражения"
                            },
                            {
                                "target_lang_word": "ведь",
                                "target_lang_sense_clarification": "частица для усиления возражения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1e52c79-2d5d-3bd1-87c7-111e80cbb045",
                    "sense_definition": "Used after a negative clause to introduce what is true instead; on the contrary.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "correction_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The problem is not the price <w>but</w> the quality of the product.",
                            "target_lang_translations": {
                                "ru": "Проблема не в цене, <w>а</w> в качестве продукта."
                            }
                        },
                        {
                            "text": "He did not run away <w>but</w> stood his ground firmly.",
                            "target_lang_translations": {
                                "ru": "Он не убежал, <w>а</w> твёрдо стоял на своём."
                            }
                        }
                    ],
                    "synonyms": [
                        "rather",
                        "on the contrary"
                    ],
                    "common_phrases": [
                        "not only but also"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется после отрицания для введения того, что является истинным; напротив."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "союз после отрицания (не..., а...)"
                            },
                            {
                                "target_lang_word": "но",
                                "target_lang_sense_clarification": "противительный союз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2b488e9-4769-3365-a51e-03bd80b17d51",
                    "sense_definition": "Except that; used to introduce a subordinate clause that qualifies a negative statement.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "exception_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot <w>but</w> feel sorry for the way they were treated.",
                            "target_lang_translations": {
                                "ru": "Я не могу <w>не</w> сочувствовать тому, как с ними обошлись."
                            }
                        },
                        {
                            "text": "There is no one here <w>but</w> wishes you the very best.",
                            "target_lang_translations": {
                                "ru": "Здесь нет никого, кто <w>не</w> желал бы вам всего наилучшего."
                            }
                        }
                    ],
                    "synonyms": [
                        "except that"
                    ],
                    "common_phrases": [
                        "cannot but"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "За исключением того, что; используется для введения придаточного предложения, уточняющего отрицательное утверждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроме как",
                                "target_lang_sense_clarification": "за исключением того, что"
                            },
                            {
                                "target_lang_word": "не... не",
                                "target_lang_sense_clarification": "двойное отрицание (не могу не...)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f1afc63-de9a-38b5-8bda-95a4879466a8",
                    "sense_definition": "Without it also being the case that; unless.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "exception_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It never rains <w>but</w> it pours.",
                            "target_lang_translations": {
                                "ru": "Беда не приходит <w>одна</w>."
                            }
                        },
                        {
                            "text": "I never hear that old song <w>but</w> I think of my childhood home.",
                            "target_lang_translations": {
                                "ru": "Я никогда не слышу эту старую песню, <w>не</w> вспоминая о родительском доме."
                            }
                        }
                    ],
                    "synonyms": [
                        "unless"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "idiomatic or archaic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Без того, чтобы; если не."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чтобы не",
                                "target_lang_sense_clarification": "в конструкциях типа «не бывает так, чтобы не...»"
                            },
                            {
                                "target_lang_word": "без того чтобы",
                                "target_lang_sense_clarification": "устаревшее или книжное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "c4cdd102-3241-31c8-996e-4093aa8f4503",
                    "sense_definition": "Apart from; except for; excluding.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exception_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Everyone <w>but</w> the manager left the office early today.",
                            "target_lang_translations": {
                                "ru": "Все, <w>кроме</w> менеджера, ушли из офиса сегодня рано."
                            }
                        },
                        {
                            "text": "I have no choice <w>but</w> to accept their offer.",
                            "target_lang_translations": {
                                "ru": "У меня нет иного выбора, <w>кроме как</w> принять их предложение."
                            }
                        }
                    ],
                    "synonyms": [
                        "except",
                        "apart from",
                        "barring",
                        "save for",
                        "excluding"
                    ],
                    "antonyms": [
                        "including"
                    ],
                    "common_phrases": [
                        "nothing but",
                        "anything but",
                        "all but",
                        "everything but"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помимо; за исключением; исключая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроме",
                                "target_lang_sense_clarification": "предлог исключения"
                            },
                            {
                                "target_lang_word": "за исключением",
                                "target_lang_sense_clarification": "предлог"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e7683ae8-b2f7-324f-8f20-887f2b156a85",
                    "sense_definition": "Merely; only; just; no more than.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "limitation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We can <w>but</w> hope that the weather improves by tomorrow.",
                            "target_lang_translations": {
                                "ru": "Нам остаётся <w>лишь</w> надеяться, что погода улучшится к завтрашнему дню."
                            }
                        },
                        {
                            "text": "Life is <w>but</w> a dream for those who live without worry.",
                            "target_lang_translations": {
                                "ru": "Жизнь — это <w>всего лишь</w> сон для тех, кто живёт без забот."
                            }
                        }
                    ],
                    "synonyms": [
                        "only",
                        "merely",
                        "just",
                        "simply"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишь; только; всего лишь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лишь",
                                "target_lang_sense_clarification": "ограничительная частица"
                            },
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "ограничительная частица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db5b32b0-07f6-36b2-9dfc-f871120048e1",
                    "sense_definition": "Though; however (used at the end of a sentence).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "contrast_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I don't really want to go to the party; I'll drive you there <w>but</w>.",
                            "target_lang_translations": {
                                "ru": "Я не очень хочу идти на вечеринку; <w>однако</w> я тебя туда отвезу."
                            }
                        },
                        {
                            "text": "It was a difficult match, <w>but</w> we won <w>but</w>.",
                            "target_lang_translations": {
                                "ru": "Это был трудный матч, <w>но</w> мы всё же выиграли."
                            }
                        }
                    ],
                    "synonyms": [
                        "though",
                        "however"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "common in Australian, Scottish, and Irish English"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хотя; однако (используется в конце предложения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "однако",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "впрочем",
                                "target_lang_sense_clarification": "вводное слово"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4e99db7c-690f-3157-b89c-623331bdb550",
                    "sense_definition": "An objection, caveat, or excuse.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "objection_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I want this work finished by noon, and I will hear no <w>buts</w> about it.",
                            "target_lang_translations": {
                                "ru": "Я хочу, чтобы эта работа была закончена к полудню, и не потерплю никаких <w>но</w>."
                            }
                        },
                        {
                            "text": "There is always a big <w>but</w> whenever he promises to help us.",
                            "target_lang_translations": {
                                "ru": "Всегда есть большое <w>но</w>, когда он обещает нам помочь."
                            }
                        }
                    ],
                    "synonyms": [
                        "objection"
                    ],
                    "common_phrases": [
                        "no ifs ands or buts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возражение, оговорка или оправдание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "но",
                                "target_lang_sense_clarification": "существительное (никаких «но»)"
                            },
                            {
                                "target_lang_word": "возражение",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6264273a-4733-3697-b7eb-d95c5dd06141",
                    "sense_definition": "To raise an objection; to use the word 'but'.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "objection_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>But</w> me no buts, just do as you are told.",
                            "target_lang_translations": {
                                "ru": "Не <w>возражай</w> мне, просто делай, что велено."
                            }
                        },
                        {
                            "text": "He <w>butted</w> in with another excuse before I could finish speaking.",
                            "target_lang_translations": {
                                "ru": "Он <w>встрял</w> с очередным оправданием, прежде чем я успел закончить."
                            }
                        }
                    ],
                    "synonyms": [
                        "object"
                    ],
                    "common_phrases": [
                        "but me no buts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выдвигать возражение; использовать слово «но»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возражать",
                                "target_lang_sense_clarification": "глагол"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e4eb1b42-ec02-3d9f-8698-7136afaeca1a",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>But</w> is listed in the directory.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Бат</w> есть в справочнике."
                            }
                        },
                        {
                            "text": "We met a family named <w>But</w> during our travels.",
                            "target_lang_translations": {
                                "ru": "Мы встретили семью по фамилии <w>Бат</w> во время наших путешествий."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бат",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a4fd8914-46ea-3ea7-907a-96673a1fd8fc",
                    "sense_definition": "С большим количеством света или насыщенным цветом.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literal_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летнее солнце <w>ярко</w> освещало зелёную поляну.",
                            "target_lang_translations": {
                                "en": "The summer sun illuminated the green meadow <w>brightly</w>."
                            }
                        },
                        {
                            "text": "В окнах дома напротив <w>ярко</w> горел свет.",
                            "target_lang_translations": {
                                "en": "Lights were burning <w>brightly</w> in the windows of the house opposite."
                            }
                        }
                    ],
                    "synonyms": [
                        "светло"
                    ],
                    "antonyms": [
                        "тускло"
                    ],
                    "common_phrases": [
                        "ярко светить",
                        "ярко гореть",
                        "ярко освещать"
                    ],
                    "target_lang_definitions": {
                        "en": "With a large amount of light or saturated color."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brightly",
                                "target_lang_sense_clarification": "emitting or reflecting much light"
                            },
                            {
                                "target_lang_word": "vividly",
                                "target_lang_sense_clarification": "intense color"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffd48ebb-11de-36c2-8121-911ddfd9a9f6",
                    "sense_definition": "Впечатляюще, выразительно, привлекая внимание.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_impressive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Актриса <w>ярко</w> сыграла свою роль в новом фильме.",
                            "target_lang_translations": {
                                "en": "The actress played her role <w>brilliantly</w> in the new film."
                            }
                        },
                        {
                            "text": "Спортсмены нашей команды <w>ярко</w> выступили на соревнованиях.",
                            "target_lang_translations": {
                                "en": "Our team's athletes performed <w>impressively</w> at the competition."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ярко выступить",
                        "ярко одеваться",
                        "ярко жить"
                    ],
                    "target_lang_definitions": {
                        "en": "Impressively, expressively, or attracting attention."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brilliantly",
                                "target_lang_sense_clarification": "regarding performance or talent"
                            },
                            {
                                "target_lang_word": "strikingly",
                                "target_lang_sense_clarification": "attracting attention"
                            },
                            {
                                "target_lang_word": "impressively",
                                "target_lang_sense_clarification": "evoking admiration"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43fbdf91-b760-314a-9d08-25a4d0758d1a",
                    "sense_definition": "Очень заметно, отчётливо или очевидно.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_obvious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта ситуация <w>ярко</w> показала все недостатки системы.",
                            "target_lang_translations": {
                                "en": "This situation <w>starkly</w> showed all the flaws of the system."
                            }
                        },
                        {
                            "text": "В романе <w>ярко</w> отражены исторические события того времени.",
                            "target_lang_translations": {
                                "en": "The historical events of that time are <w>vividly</w> reflected in the novel."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ярко выраженный",
                        "ярко демонстрировать",
                        "ярко иллюстрировать"
                    ],
                    "target_lang_definitions": {
                        "en": "Very noticeably, distinctly, or obviously."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clearly",
                                "target_lang_sense_clarification": "easy to perceive or understand"
                            },
                            {
                                "target_lang_word": "vividly",
                                "target_lang_sense_clarification": "producing strong, clear images in the mind"
                            },
                            {
                                "target_lang_word": "starkly",
                                "target_lang_sense_clarification": "unpleasantly clear or obvious"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "coś",
        "cokolwiek",
        "nieco",
        "codziennie",
        "corocznie"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "4f5ac21c-b9d9-3414-b037-2b7c91cc7a00",
                    "sense_definition": "Zaimek pytajny używany do zadawania pytań o rzeczy, zjawiska, sytuacje lub treść wypowiedzi.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_interrogative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Co</w> robisz dzisiaj wieczorem?",
                            "target_lang_translations": {
                                "ru": "<w>Что</w> ты делаешь сегодня вечером?"
                            }
                        },
                        {
                            "text": "<w>Co</w> to jest za dziwne urządzenie?",
                            "target_lang_translations": {
                                "ru": "<w>Что</w> это за странное устройство?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "co to",
                        "co słychać",
                        "po co",
                        "na co",
                        "co robić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопросительное местоимение, используемое для вопросов о предметах, явлениях, ситуациях или содержании высказывания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "вопросительное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "113dd094-a5a8-3f5f-81aa-e01f52094019",
                    "sense_definition": "Zaimek względny łączący zdanie nadrzędne z podrzędnym, odnoszący się do rzeczy lub osób; w mowie potocznej często zastępuje zaimek 'który'.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_relative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest ten chłopak, <w>co</w> mieszka obok nas.",
                            "target_lang_translations": {
                                "ru": "Это тот парень, <w>который</w> живёт рядом с нами."
                            }
                        },
                        {
                            "text": "Zgubiłem telefon, <w>co</w> dostałem od brata.",
                            "target_lang_translations": {
                                "ru": "Я потерял телефон, <w>который</w> получил от брата."
                            }
                        }
                    ],
                    "synonyms": [
                        "który",
                        "jaki"
                    ],
                    "common_phrases": [
                        "wszystko co",
                        "to co",
                        "ten co"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "standardowo używa się zaimka 'który'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относительное местоимение, связывающее главное предложение с придаточным; в разговорной речи часто заменяет местоимение «который»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "который",
                                "target_lang_sense_clarification": "относительное местоимение"
                            },
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2644e4d-269b-3d1a-a38e-f784ace50543",
                    "sense_definition": "Potoczne określenie pytające o przyczynę lub cel zachowania; dlaczego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "pronoun_why",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Co</w> się tak na mnie patrzysz?",
                            "target_lang_translations": {
                                "ru": "<w>Чего</w> ты так на меня смотришь?"
                            }
                        },
                        {
                            "text": "<w>Co</w> ona taka smutna dzisiaj siedzi?",
                            "target_lang_translations": {
                                "ru": "<w>Чего</w> она такая грустная сегодня сидит?"
                            }
                        }
                    ],
                    "synonyms": [
                        "że"
                    ],
                    "common_phrases": [
                        "co się stało"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное вопросительное слово, спрашивающее о причине или цели поведения; почему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чего",
                                "target_lang_sense_clarification": "разговорное «почему»"
                            },
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "вопрос о причине"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "79f4b66f-4af5-312b-8c96-94f5a1b803df",
                    "sense_definition": "Przyimek używany do określania częstotliwości powtarzania się jakiegoś zdarzenia w czasie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "prep_frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat dzwoni do mnie <w>co</w> tydzień.",
                            "target_lang_translations": {
                                "ru": "Мой брат звонит мне <w>каждую</w> неделю."
                            }
                        },
                        {
                            "text": "Autobus do centrum odjeżdża <w>co</w> piętnaście minut.",
                            "target_lang_translations": {
                                "ru": "Автобус в центр отправляется <w>каждые</w> пятнадцать минут."
                            }
                        }
                    ],
                    "synonyms": [
                        "każdego…"
                    ],
                    "common_phrases": [
                        "co dzień",
                        "co rok",
                        "co chwila",
                        "co miesiąc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлог, используемый для указания частоты повторения события во времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каждый",
                                "target_lang_sense_clarification": "периодичность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "f5afeaa9-1874-3a89-8c4e-5cbfdcf46f6d",
                    "sense_definition": "Słowo używane, gdy nie usłyszało się lub nie zrozumiało wypowiedzi rozmówcy, albo w celu wyrażenia zdziwienia; forma ta może być odebrana jako nieuprzejma.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intj_what",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Co</w>? Możesz powtórzyć, bo nie słyszałem?",
                            "target_lang_translations": {
                                "ru": "<w>Что</w>? Можешь повторить, я не слышал?"
                            }
                        },
                        {
                            "text": "<w>Co</w>? Naprawdę wygraliśmy ten mecz?",
                            "target_lang_translations": {
                                "ru": "<w>Что</w>? Мы правда выиграли этот матч?"
                            }
                        }
                    ],
                    "synonyms": [
                        "słucham",
                        "proszę"
                    ],
                    "common_phrases": [
                        "i co",
                        "no co"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często uważane za niegrzeczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое, когда говорящий не расслышал или не понял сказанного, либо для выражения удивления; может восприниматься как невежливое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "переспрос"
                            },
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "разговорный переспрос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3aaaeb0-4444-3c63-b426-cee424eaecbf",
                    "sense_definition": "Słowo używane na końcu zdania, służące do podtrzymania uwagi rozmówcy lub upewnienia się co do jego zgody.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "intj_tag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Całkiem ładna pogoda dzisiaj, <w>co</w>?",
                            "target_lang_translations": {
                                "ru": "Погода сегодня вполне хорошая, <w>да</w>?"
                            }
                        },
                        {
                            "text": "To był naprawdę trudny egzamin, <w>co</w>?",
                            "target_lang_translations": {
                                "ru": "Это был действительно трудный экзамен, <w>да</w>?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в конце предложения для поддержания внимания собеседника или получения подтверждения его согласия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "вопрос-тег"
                            },
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "подтверждение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "7107cfa0-8254-31bb-a716-e292679043a7",
                    "sense_definition": "Spójnik występujący w porównaniach, wskazujący na tożsamość lub podobieństwo cech.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "conj_comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam dokładnie ten sam problem <w>co</w> ty.",
                            "target_lang_translations": {
                                "ru": "У меня точно такая же проблема, <w>как</w> и у тебя."
                            }
                        },
                        {
                            "text": "On zarabia tyle samo pieniędzy <w>co</w> jego szef.",
                            "target_lang_translations": {
                                "ru": "Он зарабатывает столько же денег, <w>сколько</w> его начальник."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ten sam co",
                        "tyle samo co",
                        "tak samo co"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, используемый в сравнениях, указывающий на тождество или сходство признаков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "сравнение"
                            },
                            {
                                "target_lang_word": "сколько",
                                "target_lang_sense_clarification": "в сравнении количества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "329c6eda-7527-333b-8882-c22ce11ee044",
                    "sense_definition": "Potoczny spójnik wprowadzający zdanie podrzędne, używany zamiennie ze słowem 'że'.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "conj_that",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myślałem, <w>co</w> już poszedłeś do domu.",
                            "target_lang_translations": {
                                "ru": "Я думал, <w>что</w> ты уже пошёл домой."
                            }
                        },
                        {
                            "text": "Wydaje mi się, <w>co</w> to nie jest prawda.",
                            "target_lang_translations": {
                                "ru": "Мне кажется, <w>что</w> это неправда."
                            }
                        }
                    ],
                    "synonyms": [
                        "że"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорный союз, вводящий придаточное предложение; используется вместо слова «że» (что)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "изъяснительный союз"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e45293e3-2f1e-3aa7-876e-0db5abe0688f",
                    "sense_definition": "Skrót od: centralne ogrzewanie; system ogrzewania budynków.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_abbrev",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym bloku zepsuło się <w>CO</w> i jest zimno.",
                            "target_lang_translations": {
                                "ru": "В нашем доме сломалось <w>ЦО</w>, и стало холодно."
                            }
                        },
                        {
                            "text": "Musimy zapłacić wysoki rachunek za <w>CO</w>.",
                            "target_lang_translations": {
                                "ru": "Мы должны заплатить большой счет за <w>отопление</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "c.o."
                    ],
                    "common_phrases": [
                        "piec CO",
                        "instalacja CO"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от: центральное отопление; система отопления зданий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ЦО",
                                "target_lang_sense_clarification": "аббревиатура"
                            },
                            {
                                "target_lang_word": "центральное отопление",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
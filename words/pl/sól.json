    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "01eb276d-0a7d-384e-98bb-ad98b1bf7152",
                    "sense_definition": "Biała, krystaliczna substancja o słonym smaku, używana powszechnie do przyprawiania potraw i konserwowania żywności.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy możesz podać mi <w>sól</w> i pieprz?",
                            "target_lang_translations": {
                                "ru": "Ты можешь передать мне <w>соль</w> и перец?"
                            }
                        },
                        {
                            "text": "Lekarze zalecają, aby ograniczyć <w>sól</w> w codziennej diecie.",
                            "target_lang_translations": {
                                "ru": "Врачи рекомендуют ограничить <w>соль</w> в ежедневном рационе."
                            }
                        }
                    ],
                    "synonyms": [
                        "sól kuchenna",
                        "chlorek sodu"
                    ],
                    "common_phrases": [
                        "szczypta soli",
                        "sól morska",
                        "sól kamienna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Белое кристаллическое вещество с соленым вкусом, широко используемое для приправы блюд и консервирования продуктов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соль",
                                "target_lang_sense_clarification": "пищевая"
                            },
                            {
                                "target_lang_word": "поваренная соль",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f963015c-4ce2-3ba7-ac16-c5b6d2622aeb",
                    "sense_definition": "Związek chemiczny o budowie jonowej, powstający zazwyczaj w wyniku reakcji kwasu z zasadą.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda morska zawiera rozpuszczone <w>sole</w> mineralne.",
                            "target_lang_translations": {
                                "ru": "Морская вода содержит растворенные минеральные <w>соли</w>."
                            }
                        },
                        {
                            "text": "Na lekcji chemii uczyliśmy się, jak wytrącać <w>sole</w> z roztworów.",
                            "target_lang_translations": {
                                "ru": "На уроке химии мы учились осаждать <w>соли</w> из растворов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sole mineralne",
                        "sole kwasów"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Химическое соединение ионного строения, образующееся обычно в результате реакции кислоты с основанием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соль",
                                "target_lang_sense_clarification": "химия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9b1bb7b8-8018-3801-bd0b-0ad9736ce91f",
                    "sense_definition": "Forma trybu rozkazującego (druga osoba liczby pojedynczej) czasownika <w>solić</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <w>sól</w> zupy, zanim jej nie spróbujesz.",
                            "target_lang_translations": {
                                "ru": "Не <w>соли</w> суп, пока не попробуешь его."
                            }
                        },
                        {
                            "text": "<w>Sól</w> ostrożnie, bo łatwo zepsuć smak potrawy.",
                            "target_lang_translations": {
                                "ru": "<w>Соли</w> осторожно, потому что легко испортить вкус блюда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nie sól tyle"
                    ],
                    "traits": [
                        {

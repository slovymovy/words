                    }
                },
                {
                    "sense_id": "bb1a884a-613b-32d9-a3d1-7576b0c87192",
                    "sense_definition": "Naczynie, zwykle drewniane, używane dawniej na wsi, np. na mleko.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodyni postawiła pełny <sąd> z mlekiem na stole.",

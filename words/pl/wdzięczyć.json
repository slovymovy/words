            "pos": "verb",
            "senses": [
                {
                    "sense_id": "643a35ea-35db-357f-88c9-8a3d6633e5a2",
                    "sense_definition": "Zachowywać się w sposób przesadnie miły, kokieteryjny i nienaturalny, często robiąc słodkie miny, aby się komuś przypodobać.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda dziewczyna <w>wdzięczyła</w> się do przystojnego kelnera, licząc na darmowy deser.",
                            "target_lang_translations": {
                                "ru": "Молодая девушка <w>кокетничала</w> с симпатичным официантом, рассчитывая на бесплатный десерт."
                            }
                        },
                        {
                            "text": "Nie lubię, gdy ktoś <w>wdzięczy</w> się do szefa tylko po to, by uzyskać awans.",
                            "target_lang_translations": {
                                "ru": "Я не люблю, когда кто-то <w>строит глазки</w> шефу только для того, чтобы получить повышение."
                            }
                        }
                    ],
                    "synonyms": [
                        "przymilać się",
                        "kokietować",
                        "mizdrzyć się"
                    ],
                    "common_phrases": [
                        "wdzięczyć się do kogoś",
                        "wdzięczyć się przed lustrem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя неестественно мило, кокетливо, часто делая умильные гримасы, чтобы понравиться кому-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жеманиться",
                                "target_lang_sense_clarification": "вести себя манерно, неестественно"
                            },
                            {
                                "target_lang_word": "кокетничать",
                                "target_lang_sense_clarification": "стараться понравиться своим поведением"
                            },
                            {
                                "target_lang_word": "строить глазки",
                                "target_lang_sense_clarification": "идиома: кокетливо смотреть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05f6dd0e-6c91-3f44-8ce4-134c2b9c7098",
                    "sense_definition": "Dodawać komuś lub czemuś uroku, czynić przyjemnym dla oka; ozdabiać.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Delikatny rumieniec <w>wdzięczył</w> jej bladą twarz.",
                            "target_lang_translations": {
                                "ru": "Нежный румянец <w>украшал</w> её бледное лицо."
                            }
                        },
                        {
                            "text": "Proste kwiaty polne <w>wdzięczyły</w> ten skromny stół.",
                            "target_lang_translations": {
                                "ru": "Простые полевые цветы <w>украшали</w> этот скромный стол."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdobić",
                        "upiększać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать кому-либо или чему-либо очарование, делать приятным для глаз; украшать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украшать",
                                "target_lang_sense_clarification": "делать красивым"
                            },
                            {
                                "target_lang_word": "красить",
                                "target_lang_sense_clarification": "придавать красоту, быть к лицу"
                            }
                        ]
                    }
                }
            ]
        }

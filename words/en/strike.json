    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "053126a5-b724-3b1a-807d-a25f495ae42b",
                    "sense_definition": "To hit someone or something forcefully with a hand, weapon, or other object.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car <w>struck</w> the tree with great force.",
                            "target_lang_translations": {
                                "ru": "Машина с огромной силой <w>врезалась</w> в дерево."
                            }
                        },
                        {
                            "text": "He used a hammer to <w>strike</w> the nail into the wood.",
                            "target_lang_translations": {
                                "ru": "Он использовал молоток, чтобы <w>забить</w> гвоздь в дерево."
                            }
                        }
                    ],
                    "synonyms": [
                        "strike work"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно ударить кого-то или что-то рукой, оружием или другим предметом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ударять",
                                "target_lang_sense_clarification": "наносить удар"
                            },
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "наносить побои или удары"
                            },
                            {
                                "target_lang_word": "ударить",
                                "target_lang_sense_clarification": "совершить удар (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "362ea621-6197-3e47-9557-d2714e90cbd1",
                    "sense_definition": "To indicate the time by making a sound, typically a bell or chime in a clock.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to leave before the clock <w>strikes</w> twelve.",
                            "target_lang_translations": {
                                "ru": "Нам нужно уйти до того, как часы <w>пробьют</w> двенадцать."
                            }
                        },
                        {
                            "text": "The church bell <w>struck</w> the hour loudly.",
                            "target_lang_translations": {
                                "ru": "Церковный колокол громко <w>пробил</w> час."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clock strikes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывать время звуковым сигналом (обычно о часах или колоколе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "издавать звон (о часах)"
                            },
                            {
                                "target_lang_word": "пробить",
                                "target_lang_sense_clarification": "обозначить время ударами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3be37dc-43a9-32a4-920f-a8b2c96a4cdd",
                    "sense_definition": "To cause a flame to ignite by friction, usually referring to a match.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ignition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>struck</w> a match to light the candles on the cake.",
                            "target_lang_translations": {
                                "ru": "Он <w>чиркнул</w> спичкой, чтобы зажечь свечи на торте."
                            }
                        },
                        {
                            "text": "It was too windy to <w>strike</w> a light outside.",
                            "target_lang_translations": {
                                "ru": "Было слишком ветрено, чтобы <w>зажечь</w> огонь на улице."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strike a match",
                        "strike a light"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зажечь пламя трением (обычно спичку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чиркать",
                                "target_lang_sense_clarification": "спичкой"
                            },
                            {
                                "target_lang_word": "зажигать",
                                "target_lang_sense_clarification": "воспламенять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42673e6b-f7df-3b5f-aa36-2f22bf3913a0",
                    "sense_definition": "To stop working as a form of organized protest against an employer to achieve better conditions.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "protest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory workers voted to <w>strike</w> for higher wages.",
                            "target_lang_translations": {
                                "ru": "Рабочие фабрики проголосовали за то, чтобы <w>бастовать</w> ради повышения зарплаты."
                            }
                        },
                        {
                            "text": "Teachers may <w>strike</w> if the government cuts education funding.",
                            "target_lang_translations": {
                                "ru": "Учителя могут <w>забастовать</w>, если правительство урежет финансирование образования."
                            }
                        }
                    ],
                    "synonyms": [
                        "strike work"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекращать работу в знак организованного протеста против работодателя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бастовать",
                                "target_lang_sense_clarification": "участвовать в забастовке"
                            },
                            {
                                "target_lang_word": "забастовать",
                                "target_lang_sense_clarification": "начать забастовку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06db88aa-5698-38ec-abbb-a0f1c3a148de",
                    "sense_definition": "To attack suddenly and violently.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The snake <w>struck</w> at the mouse in the grass.",
                            "target_lang_translations": {
                                "ru": "Змея <w>бросилась</w> на мышь в траве."
                            }
                        },
                        {
                            "text": "Enemy forces planned to <w>strike</w> at dawn.",
                            "target_lang_translations": {
                                "ru": "Вражеские силы планировали <w>нанести удар</w> на рассвете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strike fear",
                        "strike terror"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно и жестоко атаковать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нападать",
                                "target_lang_sense_clarification": "атаковать"
                            },
                            {
                                "target_lang_word": "атаковать",
                                "target_lang_sense_clarification": "совершать нападение"
                            },
                            {
                                "target_lang_word": "нанести удар",
                                "target_lang_sense_clarification": "военное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95547348-af7b-3313-8609-e7addba5162e",
                    "sense_definition": "To cause someone to have a particular feeling or idea; to come into the mind suddenly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mental_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It <w>struck</w> me that I had forgotten to lock the door.",
                            "target_lang_translations": {
                                "ru": "Меня <w>осенило</w>, что я забыл запереть дверь."
                            }
                        },
                        {
                            "text": "Her behavior <w>struck</w> him as very unusual.",
                            "target_lang_translations": {
                                "ru": "Её поведение <w>показалось</w> ему очень необычным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strike someone as",
                        "strike a chord"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произвести впечатление или внезапно прийти в голову."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поражать",
                                "target_lang_sense_clarification": "производить впечатление"
                            },
                            {
                                "target_lang_word": "осенять",
                                "target_lang_sense_clarification": "внезапно приходить в голову"
                            },
                            {
                                "target_lang_word": "казаться",
                                "target_lang_sense_clarification": "производить впечатление (каким-либо)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8caba716-9909-3d2e-ab2d-0e078dc7b9cd",
                    "sense_definition": "To reach, make, or ratify an agreement, deal, or compromise.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two companies managed to <w>strike</w> a deal after long negotiations.",
                            "target_lang_translations": {
                                "ru": "Двум компаниям удалось <w>заключить</w> сделку после долгих переговоров."
                            }
                        },
                        {
                            "text": "It is difficult to <w>strike</w> a balance between work and family life.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>найти</w> баланс между работой и семейной жизнью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strike a deal",
                        "strike a balance",
                        "strike a bargain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достичь соглашения или заключить сделку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заключать",
                                "target_lang_sense_clarification": "сделку или соглашение"
                            },
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "соглашения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b67b94f4-905d-3fca-b48e-0fc47c5efe33",
                    "sense_definition": "To discover a source of something valuable, such as gold or oil.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "discovery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many people moved west hoping to <w>strike</w> gold.",
                            "target_lang_translations": {
                                "ru": "Многие люди переехали на запад в надежде <w>найти</w> золото."
                            }
                        },
                        {
                            "text": "The drilling team was lucky to <w>strike</w> oil on the first try.",
                            "target_lang_translations": {
                                "ru": "Буровой бригаде повезло <w>обнаружить</w> нефть с первой попытки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strike gold",
                        "strike oil"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнаружить источник чего-либо ценного, например, золота или нефти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находить",
                                "target_lang_sense_clarification": "обнаруживать залежи"
                            },
                            {
                                "target_lang_word": "обнаруживать",
                                "target_lang_sense_clarification": "открывать месторождение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0386000b-ab96-311f-ae79-878e6921fb9b",
                    "sense_definition": "To remove or delete something, such as a word or sentence from a record.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge ordered the clerk to <w>strike</w> the comment from the record.",
                            "target_lang_translations": {
                                "ru": "Судья приказал секретарю <w>вычеркнуть</w> комментарий из протокола."
                            }
                        },
                        {
                            "text": "Please <w>strike</w> my name from the mailing list.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>удалите</w> моё имя из списка рассылки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strike from",
                        "strike off"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or legal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалить или вычеркнуть что-либо (например, слово из записи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вычёркивать",
                                "target_lang_sense_clarification": "удалять из текста"
                            },
                            {
                                "target_lang_word": "удалять",
                                "target_lang_sense_clarification": "исключать из списка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a0d3bb5a-214f-3239-8082-df8cf2c2618d",
                    "sense_definition": "A period of time when workers stop working to force an employer to agree to their demands.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "protest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train drivers voted to go on <w>strike</w> next week.",
                            "target_lang_translations": {
                                "ru": "Машинисты поездов проголосовали за то, чтобы выйти на <w>забастовку</w> на следующей неделе."
                            }
                        },
                        {
                            "text": "The general <w>strike</w> paralyzed the entire city for three days.",
                            "target_lang_translations": {
                                "ru": "Всеобщая <w>забастовка</w> парализовала весь город на три дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "walkout#Noun"
                    ],
                    "antonyms": [
                        "lockout#Noun"
                    ],
                    "common_phrases": [
                        "on strike",
                        "hunger strike",
                        "general strike"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени, когда работники прекращают работу, чтобы добиться выполнения требований."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забастовка",
                                "target_lang_sense_clarification": "организованный протест"
                            },
                            {
                                "target_lang_word": "стачка",
                                "target_lang_sense_clarification": "массовое прекращение работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8453a55e-018f-39bd-9526-ab46d1d29d5c",
                    "sense_definition": "A sudden military attack, often carried out by aircraft or missiles.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The army launched a preemptive <w>strike</w> against the enemy base.",
                            "target_lang_translations": {
                                "ru": "Армия нанесла упреждающий <w>удар</w> по вражеской базе."
                            }
                        },
                        {
                            "text": "Civilians were warned to seek shelter before the air <w>strike</w> began.",
                            "target_lang_translations": {
                                "ru": "Гражданских предупредили искать укрытие до начала воздушного <w>удара</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "air strike",
                        "drone strike",
                        "preemptive strike"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапная военная атака, часто с воздуха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удар",
                                "target_lang_sense_clarification": "военная атака"
                            },
                            {
                                "target_lang_word": "атака",
                                "target_lang_sense_clarification": "нападение"
                            },
                            {
                                "target_lang_word": "налёт",
                                "target_lang_sense_clarification": "воздушная атака"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23075336-a36f-3166-94a9-059b50223511",
                    "sense_definition": "In baseball, a pitch that is counted against the batter because they swung and missed or the ball passed through the strike zone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sport_baseball",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The umpire called a <w>strike</w> even though the batter did not swing.",
                            "target_lang_translations": {
                                "ru": "Судья объявил <w>страйк</w>, хотя бьющий не сделал замаха."
                            }
                        },
                        {
                            "text": "With two <w>strikes</w> against him, he had to be careful.",
                            "target_lang_translations": {
                                "ru": "С двумя <w>страйками</w> против него, ему нужно было быть осторожным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "three strikes"
                    ],
                    "target_lang_definitions": {
                        "ru": "В бейсболе: подача, которая засчитывается против бьющего при промахе или попадании в зону страйка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страйк",
                                "target_lang_sense_clarification": "в бейсболе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b39b4bba-bce1-3e53-b7d5-aa4063c5e748",
                    "sense_definition": "The discovery of a valuable resource, such as oil or minerals, in the ground.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "discovery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lucky oil <w>strike</w> made the small town rich overnight.",
                            "target_lang_translations": {
                                "ru": "Удачная нефтяная <w>находка</w> сделала маленький городок богатым в одночасье."
                            }
                        },
                        {
                            "text": "Geologists are investigating the site of a potential gold <w>strike</w>.",
                            "target_lang_translations": {
                                "ru": "Геологи исследуют место потенциального <w>месторождения</w> золота."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oil strike",
                        "lucky strike"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнаружение ценного ресурса (например, нефти) в земле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находка",
                                "target_lang_sense_clarification": "обнаружение ископаемых"
                            },
                            {
                                "target_lang_word": "открытие",
                                "target_lang_sense_clarification": "месторождения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e688bbb2-6189-3027-8b64-04d4c378b73d",
                    "sense_definition": "In bowling, the act of knocking down all ten pins with the first ball of a frame.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sport_bowling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bowled a <w>strike</w> in the final frame to win the game.",
                            "target_lang_translations": {
                                "ru": "Она выбила <w>страйк</w> в последнем фрейме и выиграла игру."
                            }
                        },
                        {
                            "text": "Getting a <w>strike</w> is much harder than it looks.",
                            "target_lang_translations": {
                                "ru": "Выбить <w>страйк</w> гораздо сложнее, чем кажется."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В боулинге: сбивание всех десяти кеглей первым шаром фрейма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страйк",
                                "target_lang_sense_clarification": "в боулинге"
                            }
                        ]
                    }
                }
            ]
        },
        {

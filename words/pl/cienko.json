            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "963c589f-94ca-3aa1-93db-52c18e53c55b",
                    "sense_definition": "W sposób charakteryzujący się małą grubością lub średnicą; warstwą o niewielkiej grubości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kup mi, proszę, jakiś <w>cienko</w> piszący flamaster.",
                            "target_lang_translations": {
                                "ru": "Купи мне, пожалуйста, какой-нибудь <w>тонко</w> пишущий фломастер."
                            }
                        },
                        {
                            "text": "Mama zawsze <w>cienko</w> obiera ziemniaki na obiad.",
                            "target_lang_translations": {
                                "ru": "Мама всегда <w>тонко</w> чистит картошку на обед."
                            }
                        }
                    ],
                    "antonyms": [
                        "grubo"
                    ],
                    "common_phrases": [
                        "cienko pokrojony",
                        "cienko ubrany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имея малую толщину или диаметр; тонким слоем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тонко",
                                "target_lang_sense_clarification": "о толщине предмета или слоя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8929ab8f-590b-3932-bd56-6c6406f48dd2",
                    "sense_definition": "Słabo, źle; w sposób wskazujący na braki (np. finansowe), trudną sytuację lub niską jakość.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z moją wypłatą jest w tym miesiącu bardzo <w>cienko</w>.",
                            "target_lang_translations": {
                                "ru": "С моей зарплатой в этом месяце очень <w>туго</w>."
                            }
                        },
                        {
                            "text": "Nasza drużyna zagrała dzisiaj wyjątkowo <w>cienko</w> i przegrała mecz.",
                            "target_lang_translations": {
                                "ru": "Наша команда сыграла сегодня исключительно <w>слабо</w> и проиграла матч."
                            }
                        }
                    ],
                    "synonyms": [
                        "piskliwie",
                        "piskliwie"
                    ],
                    "antonyms": [
                        "dobrze",
                        "świetnie"
                    ],
                    "common_phrases": [
                        "cienko prząść",
                        "cienko widzieć"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Плохо, слабо; указывая на нехватку чего-либо (например, денег) или низкое качество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туго",
                                "target_lang_sense_clarification": "о трудном положении, нехватке денег"
                            },
                            {
                                "target_lang_word": "слабо",
                                "target_lang_sense_clarification": "о низком уровне исполнения"
                            },
                            {
                                "target_lang_word": "плохо",
                                "target_lang_sense_clarification": "общее отрицательное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d11ee0c0-9027-3c2e-88ab-5775a3da7717",
                    "sense_definition": "Wysokim, piskliwym tonem głosu lub dźwięku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mały ptaszek zaćwierkał <w>cienko</w> na gałęzi drzewa.",
                            "target_lang_translations": {
                                "ru": "Маленькая птичка <w>тонко</w> чирикнула на ветке дерева."
                            }
                        },
                        {
                            "text": "Jeśli nie oddasz długu, będziesz <w>cienko</w> piszczał!",
                            "target_lang_translations": {
                                "ru": "Если не вернешь долг, будешь <w>жалобно</w> пищать!"
                            }
                        }
                    ],
                    "synonyms": [
                        "piskliwie"
                    ],
                    "antonyms": [
                        "grubo",
                        "nisko"
                    ],
                    "common_phrases": [
                        "cienko śpiewać",
                        "cienko piszczeć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высоким, писклявым тоном голоса или звука."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тонко",
                                "target_lang_sense_clarification": "о высоком звуке"
                            },
                            {
                                "target_lang_word": "пискляво",
                                "target_lang_sense_clarification": "о резком, высоком голосе"
                            }
                        ]
                    }
                }
            ]
        }
    ]

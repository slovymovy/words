    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "678761b2-49de-3a91-8bb0-c86d7739a9ac",
                    "sense_definition": "Naar een bepaalde plaats of in een bepaalde richting.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waar ga je dit weekend <heen>?",
                            "target_lang_translations": {
                                "en": "Where are you going <to> this weekend?",
                                "ru": "<Куда> ты поедешь в эти выходные?"
                            }
                        },
                        {
                            "text": "We moeten daar zo snel mogelijk <heen>.",
                            "target_lang_translations": {
                                "en": "We need to go <there> as soon as possible.",
                                "ru": "Нам нужно попасть <туда> как можно скорее."
                            }
                        }
                    ],
                    "synonyms": [
                        "naartoe"
                    ],
                    "antonyms": [
                        "vandaan"
                    ],
                    "common_phrases": [
                        "waarheen",
                        "daarheen",
                        "nergens heen"
                    ],
                    "target_lang_definitions": {
                        "en": "To a certain place or in a certain direction.",
                        "ru": "В определенное место или в определенном направлении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "indicating direction towards a place"
                            },
                            {
                                "target_lang_word": "towards",
                                "target_lang_sense_clarification": "in the direction of"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "туда",
                                "target_lang_sense_clarification": "указание направления от говорящего"
                            },
                            {
                                "target_lang_word": "куда",
                                "target_lang_sense_clarification": "в вопросительных конструкциях (waarheen)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66b7a80b-46dc-3c60-94be-7db4065dd60c",
                    "sense_definition": "Eerst de ene kant op en vervolgens terug; onderdeel van de vaste uitdrukking 'heen en weer'.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De veerboot vaart de hele dag <heen> en weer tussen de eilanden.",
                            "target_lang_translations": {
                                "en": "The ferry sails <back> and forth between the islands all day.",
                                "ru": "Паром целый день ходит <туда> и обратно между островами."
                            }
                        },
                        {
                            "text": "Hij liep zenuwachtig <heen> en weer in de wachtkamer.",
                            "target_lang_translations": {
                                "en": "He walked nervously <back> and forth in the waiting room.",
                                "ru": "Он нервно ходил <туда>-сюда в приемной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "heen en weer"
                    ],
                    "target_lang_definitions": {
                        "en": "First in one direction and then back; part of the fixed expression 'heen en weer' (back and forth).",
                        "ru": "Сначала в одну сторону, а затем обратно; часть устойчивого выражения 'heen en weer'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "back",
                                "target_lang_sense_clarification": "in the phrase 'back and forth'"
                            },
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "in the phrase 'to and fro'"
                            },
                            {
                                "target_lang_word": "forth",
                                "target_lang_sense_clarification": "movement away from a starting point"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "туда",
                                "target_lang_sense_clarification": "в выражении 'туда и обратно' или 'туда-сюда'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "777e2170-025a-33d1-a21a-97126d490a1f",
                    "sense_definition": "Gebruikt in combinatie met voorzetsels (zoals 'om', 'over', 'door') om een beweging of positie ten opzichte van een object aan te duiden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "postpositional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk goed om je <heen> voordat je oversteekt.",
                            "target_lang_translations": {
                                "en": "Look carefully <around> you before you cross.",
                                "ru": "Хорошенько посмотри <вокруг> себя, прежде чем переходить улицу."
                            }
                        },
                        {
                            "text": "Je moet voorzichtig over het hek <heen> klimmen.",
                            "target_lang_translations": {
                                "en": "You must climb carefully <over> the fence.",
                                "ru": "Тебе нужно осторожно перелезть <через> забор."
                            }
                        }
                    ],
                    "common_phrases": [
                        "om zich heen",
                        "eroverheen",
                        "dwars doorheen"
                    ],
                    "target_lang_definitions": {
                        "en": "Used in combination with prepositions (such as 'om', 'over', 'door') to indicate movement or position relative to an object.",
                        "ru": "Используется с предлогами (например, 'om', 'over') для указания движения или положения относительно объекта."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "around",
                                "target_lang_sense_clarification": "when used with 'om'"
                            },
                            {
                                "target_lang_word": "over",
                                "target_lang_sense_clarification": "when used with 'over'"
                            },
                            {
                                "target_lang_word": "through",
                                "target_lang_sense_clarification": "when used with 'door'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вокруг",
                                "target_lang_sense_clarification": "в сочетании с 'om'"
                            },
                            {
                                "target_lang_word": "через",
                                "target_lang_sense_clarification": "в сочетании с 'over'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1faa6a1-e2f3-3958-8e05-347057064e32",
                    "sense_definition": "In een toestand van ernstige verwarring, uitputting, ziekte of dronkenschap verkerend.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "idiomatic_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na die zware operatie was de patiënt ver <heen>.",
                            "target_lang_translations": {
                                "en": "After that heavy surgery, the patient was far <gone>.",
                                "ru": "После этой тяжелой операции пациент был совсем <плох>."
                            }
                        },
                        {
                            "text": "Hij had zoveel gedronken dat hij behoorlijk ver <heen> was.",
                            "target_lang_translations": {
                                "en": "He had drunk so much that he was pretty far <gone>.",
                                "ru": "Он столько выпил, что был уже совсем <невменяем>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ver heen zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "In a state of severe confusion, exhaustion, illness, or drunkenness.",
                        "ru": "В состоянии сильного замешательства, истощения, болезни или опьянения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gone",
                                "target_lang_sense_clarification": "in the phrase 'far gone'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "плох",
                                "target_lang_sense_clarification": "о состоянии здоровья"
                            },
                            {
                                "target_lang_word": "невменяем",
                                "target_lang_sense_clarification": "о психическом состоянии или опьянении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e37ab08-6942-35f1-ad1c-8376e9f80602",
                    "sense_definition": "Weg van de huidige locatie; vandaan.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koetsier gaf de paarden de sporen en reed <heen>.",
                            "target_lang_translations": {
                                "en": "The coachman spurred the horses and rode <away>.",
                                "ru": "Кучер пришпорил лошадей и помчался <прочь>."
                            }
                        },
                        {
                            "text": "Zij spoedden zich <heen> om het nieuws te verspreiden.",
                            "target_lang_translations": {
                                "en": "They hurried <away> to spread the news.",
                                "ru": "Они поспешили <прочь>, чтобы распространить новость."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mainly found in older literature or fixed compounds like 'heengaan'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Away from the current location; thence.",
                        "ru": "Прочь от текущего местоположения; вдаль."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "away",
                                "target_lang_sense_clarification": "departing from a place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прочь",
                                "target_lang_sense_clarification": "удаление от места"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6fb25a40-ebc3-351f-a57a-283a57b6c66b",
                    "sense_definition": "Een grasachtige plantensoort (Bolboschoenus maritimus) uit de cypergrassenfamilie die in vochtige gebieden groeit.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Langs de oevers van de rivier groeit veel <heen>.",
                            "target_lang_translations": {
                                "en": "Along the banks of the river grows a lot of <sea clubrush>.",
                                "ru": "Вдоль берегов реки растет много <клубнекамыша>."
                            }
                        },
                        {
                            "text": "De eenden verschuilen zich graag tussen de <heen>.",
                            "target_lang_translations": {
                                "en": "The ducks like to hide among the <sea clubrush>.",
                                "ru": "Утки любят прятаться в <клубнекамыше>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zeebies"
                    ],
                    "target_lang_definitions": {
                        "en": "A grass-like plant species (Bolboschoenus maritimus) of the sedge family growing in moist areas.",
                        "ru": "Травянистое растение семейства осоковых (клубнекамыш), растущее во влажных местах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sea clubrush",
                                "target_lang_sense_clarification": "plant species Bolboschoenus maritimus"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "клубнекамыш",
                                "target_lang_sense_clarification": "ботаническое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]

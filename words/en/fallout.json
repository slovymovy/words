    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc2a13ea-b233-3ab5-9da8-9a97f610f8d0",
                    "sense_definition": "Radioactive dust or particles that are carried into the atmosphere after a nuclear explosion and eventually fall back to the earth.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nuclear_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government evacuated the town to protect residents from dangerous radioactive <w>fallout</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство эвакуировало город, чтобы защитить жителей от опасных радиоактивных <w>осадков</w>."
                            }
                        },
                        {
                            "text": "Scientists monitored the spread of nuclear <w>fallout</w> across the region for several weeks.",
                            "target_lang_translations": {
                                "ru": "Ученые в течение нескольких недель следили за распространением ядерных <w>осадков</w> по региону."
                            }
                        }
                    ],
                    "common_phrases": [
                        "radioactive fallout",
                        "nuclear fallout",
                        "fallout shelter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Радиоактивная пыль или частицы, попадающие в атмосферу после ядерного взрыва и оседающие обратно на землю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радиоактивные осадки",
                                "target_lang_sense_clarification": "радиоактивная пыль"
                            },
                            {
                                "target_lang_word": "выпадение",
                                "target_lang_sense_clarification": "процесс оседания частиц"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db7e0859-e1c6-3e5e-ab49-35b8fa9e717c",
                    "sense_definition": "The unpleasant results or bad effects of an action, event, or situation.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "consequences",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The president is facing severe political <w>fallout</w> following the recent scandal.",
                            "target_lang_translations": {
                                "ru": "Президент сталкивается с серьезными политическими <w>последствиями</w> после недавнего скандала."
                            }
                        },
                        {
                            "text": "It took the company years to recover from the economic <w>fallout</w> of the crisis.",
                            "target_lang_translations": {
                                "ru": "Компании потребовались годы, чтобы оправиться от экономических <w>последствий</w> кризиса."
                            }
                        }
                    ],
                    "synonyms": [
                        "repercussions",
                        "blowback"
                    ],
                    "common_phrases": [
                        "political fallout",
                        "economic fallout",
                        "deal with the fallout",
                        "face the fallout"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприятные результаты или негативные последствия действия, события или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последствия",
                                "target_lang_sense_clarification": "результат события"
                            },
                            {
                                "target_lang_word": "негативные последствия",
                                "target_lang_sense_clarification": "отрицательный эффект"
                            },
                            {
                                "target_lang_word": "отголоски",
                                "target_lang_sense_clarification": "отдаленные последствия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

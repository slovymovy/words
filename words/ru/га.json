    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "98271cd9-74a1-38a4-a6a5-d8870ad4ff71",
                    "sense_definition": "Сокращённое обозначение слова <гектар>; единица измерения площади.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Фермер купил участок земли в десять <га>.",
                            "target_lang_translations": {
                                "en": "The farmer bought a plot of land of ten <w>hectares</w>."
                            }
                        },
                        {
                            "text": "Площадь этого леса составляет тысячи <га>.",
                            "target_lang_translations": {
                                "en": "The area of this forest is thousands of <w>hectares</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "го"
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for the word \"hectare\"; a unit of area measurement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ha",
                                "target_lang_sense_clarification": "abbreviation for hectare"
                            },
                            {
                                "target_lang_word": "hectare",
                                "target_lang_sense_clarification": "unit of area"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "2872648a-34c7-342e-bb2c-67edcca03594",
                    "sense_definition": "Звукоподражание крику гуся.",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гуси вытянули шеи и громко закричали: «<Га>-<га>-<га>!»",
                            "target_lang_translations": {
                                "en": "The geese stretched their necks and cried loudly: \"<w>Honk</w>-<w>honk</w>-<w>honk</w>!\""
                            }
                        },
                        {
                            "text": "С деревенского двора доносилось весёлое «<га>-<га>».",
                            "target_lang_translations": {
                                "en": "A cheerful \"<w>honk</w>-<w>honk</w>\" could be heard from the village courtyard."
                            }
                        }
                    ],
                    "common_phrases": [
                        "га-га-га"
                    ],
                    "target_lang_definitions": {
                        "en": "Onomatopoeia for the cry of a goose."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "honk",
                                "target_lang_sense_clarification": "sound made by a goose"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ccde480d-d570-382a-9a8d-335208b4e688",
                    "sense_definition": "Звукоподражание громкому, грубому или отрывистому смеху.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он запрокинул голову и захохотал: «<Га>-<га>-<га>!»",
                            "target_lang_translations": {
                                "en": "He threw back his head and laughed: \"<w>Ha</w>-<w>ha</w>-<w>ha</w>!\""
                            }
                        },
                        {
                            "text": "В ответ на шутку послышалось лишь грубое «<га>-<га>».",
                            "target_lang_translations": {
                                "en": "In response to the joke, only a rude \"<w>ha</w>-<w>ha</w>\" was heard."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "прост."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Onomatopoeia for loud, coarse, or abrupt laughter."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ha",
                                "target_lang_sense_clarification": "sound of laughter"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "185ec124-4249-3693-8dd9-643fb6cb196d",
                    "sense_definition": "Возглас, выражающий внезапную догадку, удивление, злорадное торжество или сопровождающий резкое движение.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Га>! Вот ты и попался, голубчик!",
                            "target_lang_translations": {
                                "en": "<w>Aha</w>! Now you're caught, my friend!"
                            }
                        },
                        {
                            "text": "<Га>! Я так и знал, что это твоих рук дело.",
                            "target_lang_translations": {
                                "en": "<w>Ha</w>! I knew this was your doing."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation expressing a sudden guess, surprise, malicious triumph, or accompanying a sharp movement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "aha",
                                "target_lang_sense_clarification": "exclamation of discovery or realization"
                            },
                            {
                                "target_lang_word": "ha",
                                "target_lang_sense_clarification": "exclamation of triumph or surprise"
                            }
                        ]
                    }
                }
            ]
        }
    ]

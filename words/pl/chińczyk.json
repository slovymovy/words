    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2dac4a9a-5823-3b16-8f46-4a5d12672834",
                    "sense_definition": "Osoba narodowości chińskiej lub obywatel Chin.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "Mój nowy sąsiad to <w>Chińczyk</w>, który przyjechał do Polski na studia.",
                            "target_lang_translations": {
                                "ru": "Мой новый сосед — <w>китаец</w>, который приехал в Польшу на учебу."
                            }
                        },
                        {
                            "text": "Wielu <w>Chińczyków</w> podróżuje po Europie, aby zwiedzać zabytki.",
                            "target_lang_translations": {
                                "ru": "Многие <w>китайцы</w> путешествуют по Европе, чтобы осматривать достопримечательности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rodowity Chińczyk"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "pisane wielką literą"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек китайской национальности или гражданин Китая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китаец",
                                "target_lang_sense_clarification": "национальность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52a0db58-3c4f-3ccf-9074-7fb2233741ea",
                    "sense_definition": "Popularna gra planszowa dla dwóch do czterech osób, polegająca na przesuwaniu pionków do „domku” na podstawie rzutów kostką.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "game_or_sport_name",
                    "examples": [
                        {
                            "text": "W dzieciństwie często graliśmy całą rodziną w <w>chińczyka</w>.",
                            "target_lang_translations": {
                                "ru": "В детстве мы часто всей семьей играли в <w>лудо</w>."
                            }
                        },
                        {
                            "text": "Zasady <w>chińczyka</w> są bardzo proste, więc nawet małe dzieci mogą w niego grać.",
                            "target_lang_translations": {
                                "ru": "Правила <w>лудо</w> очень простые, поэтому в него могут играть даже маленькие дети."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grać w chińczyka",
                        "partia chińczyka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "pisane małą literą"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Популярная настольная игра для двух-четырех человек, в которой нужно перевести фишки в «домик» бросками кубика."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лудо",
                                "target_lang_sense_clarification": "игра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c2653df-cdee-353c-84bb-febb8c4702a0",
                    "sense_definition": "Potoczne określenie restauracji lub baru serwującego dania kuchni chińskiej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chciało nam się gotować, więc zamówiliśmy jedzenie od <w>chińczyka</w>.",
                            "target_lang_translations": {
                                "ru": "Нам не хотелось готовить, поэтому мы заказали еду из <w>китайского ресторана</w>."
                            }
                        },
                        {
                            "text": "Wczoraj poszliśmy do <w>chińczyka</w> na kurczaka w cieście i ryż.",
                            "target_lang_translations": {
                                "ru": "Вчера мы ходили в <w>китайский ресторан</w> на курицу в кляре с рисом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zamówić chińczyka",
                        "iść do chińczyka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        },
                        {
                            "trait_type": "form",
                            "comment": "pisane małą literą"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название ресторана или бара, где подают блюда китайской кухни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китайский ресторан",
                                "target_lang_sense_clarification": "заведение"
                            },
                            {
                                "target_lang_word": "китайка",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
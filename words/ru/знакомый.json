    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c6dc887a-062c-3bbf-a784-9a8a2f65737a",
                    "sense_definition": "Известный по прошлому опыту, виденный или слышанный ранее; такой, который узнаётся.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В толпе я вдруг увидел <знакомое> лицо и остановился.",
                            "target_lang_translations": {
                                "en": "In the crowd, I suddenly saw a <familiar> face and stopped."
                            }
                        },
                        {
                            "text": "Этот голос показался мне очень <знакомым>, но я не мог вспомнить имя.",
                            "target_lang_translations": {
                                "en": "This voice seemed very <familiar> to me, but I could not remember the name."
                            }
                        }
                    ],
                    "synonyms": [
                        "известный"
                    ],
                    "antonyms": [
                        "незнакомый"
                    ],
                    "common_phrases": [
                        "знакомое лицо",
                        "знакомый голос",
                        "до боли знакомый",
                        "показаться знакомым"
                    ],
                    "target_lang_definitions": {
                        "en": "Known from past experience; recognizable or easily identified."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "familiar",
                                "target_lang_sense_clarification": "easily recognized; known from the past"
                            },
                            {
                                "target_lang_word": "known",
                                "target_lang_sense_clarification": "recognized or identified"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7ae53b3-c916-3d80-bf33-ac7cfb4001cc",
                    "sense_definition": "Состоящий в знакомстве, личных отношениях с кем-либо; имеющий знания или сведения о чём-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я лично <знаком> с директором этой компании.",
                            "target_lang_translations": {
                                "en": "I am personally <acquainted> with the director of this company."
                            }
                        },
                        {
                            "text": "Студенты должны быть <знакомы> с правилами безопасности.",
                            "target_lang_translations": {
                                "en": "Students must be <familiar> with the safety rules."
                            }
                        }
                    ],
                    "synonyms": [
                        "осведомлённый"
                    ],
                    "antonyms": [
                        "незнакомый"
                    ],
                    "common_phrases": [
                        "быть знакомым с",
                        "лично знакомы",
                        "шапочно знакомый"
                    ],
                    "target_lang_definitions": {
                        "en": "Having personal relations with someone or knowledge of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "acquainted",
                                "target_lang_sense_clarification": "having personal knowledge of someone"
                            },
                            {
                                "target_lang_word": "familiar",
                                "target_lang_sense_clarification": "having knowledge of a subject or fact"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "004cf774-a2b5-32b1-8c7a-e46a151156d1",
                    "sense_definition": "Человек, которого кто-либо знает, с которым установлено знакомство, но не близкая дружба.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вчера я встретил старого <знакомого>, которого не видел много лет.",
                            "target_lang_translations": {
                                "en": "Yesterday I met an old <acquaintance> whom I had not seen for many years."
                            }
                        },
                        {
                            "text": "У неё очень широкий круг <знакомых> среди художников.",
                            "target_lang_translations": {
                                "en": "She has a very wide circle of <acquaintances> among artists."
                            }
                        }
                    ],
                    "synonyms": [
                        "знако́мец"
                    ],
                    "antonyms": [
                        "незнакомец"
                    ],
                    "common_phrases": [
                        "старый знакомый",
                        "общий знакомый",
                        "круг знакомых",
                        "хороший знакомый"
                    ],
                    "target_lang_definitions": {
                        "en": "A person one knows, but who is not a close friend."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "acquaintance",
                                "target_lang_sense_clarification": "a person known to one, but usually not a close friend"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
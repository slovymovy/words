{
    "word_family": [
        "driver",
        "driveway",
        "overdrive",
        "screwdriver",
        "drivetrain",
        "driving"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fc5a6594-37cc-3caf-8f94-4ffee6d8b8b8",
                    "sense_definition": "To operate and control the direction and speed of a motor vehicle.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "vehicle_operation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>drive</w> my car to work every single morning.",
                            "target_lang_translations": {
                                "ru": "Я <w>езжу</w> на машине на работу каждое утро."
                            }
                        },
                        {
                            "text": "It is dangerous to <w>drive</w> fast on icy roads.",
                            "target_lang_translations": {
                                "ru": "Опасно быстро <w>ехать</w> по обледенелым дорогам."
                            }
                        }
                    ],
                    "synonyms": [
                        "operate",
                        "steer"
                    ],
                    "common_phrases": [
                        "drive a car",
                        "drive carefully",
                        "learn to drive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Управлять транспортным средством, контролировать его скорость и направление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водить",
                                "target_lang_sense_clarification": "иметь навык или привычку управлять машиной"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "управлять машиной в данный момент"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "контролировать работу техники (формально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b809259-740d-3399-a03f-07e9c4a95d69",
                    "sense_definition": "To take someone to a specific place using a vehicle.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vehicle_operation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My father will <w>drive</w> us to the train station tomorrow.",
                            "target_lang_translations": {
                                "ru": "Папа <w>отвезёт</w> нас на вокзал завтра."
                            }
                        },
                        {
                            "text": "Can you please <w>drive</w> me home after the party?",
                            "target_lang_translations": {
                                "ru": "Можешь <w>подвезти</w> меня домой после вечеринки?"
                            }
                        }
                    ],
                    "synonyms": [
                        "take",
                        "give a lift"
                    ],
                    "common_phrases": [
                        "drive me home",
                        "drive to the airport"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отвозить кого-либо в определенное место на машине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвозить",
                                "target_lang_sense_clarification": "доставлять кого-то до места назначения"
                            },
                            {
                                "target_lang_word": "подвозить",
                                "target_lang_sense_clarification": "брать попутчика или делать одолжение"
                            },
                            {
                                "target_lang_word": "везти",
                                "target_lang_sense_clarification": "перемещать пассажира (процесс)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c43516b-97b9-3be2-bbf5-eff5eca4c708",
                    "sense_definition": "To force someone or something into a particular state or condition, often an unpleasant one.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "force_impel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loud noise from the construction site is going to <w>drive</w> me crazy.",
                            "target_lang_translations": {
                                "ru": "Громкий шум со стройки <w>сведёт</w> меня с ума."
                            }
                        },
                        {
                            "text": "His constant complaining eventually <w>drove</w> her to quit her job.",
                            "target_lang_translations": {
                                "ru": "Его постоянные жалобы в конце концов <w>довели</w> её до увольнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "make",
                        "render"
                    ],
                    "common_phrases": [
                        "drive someone crazy",
                        "drive me mad",
                        "drive to despair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить кого-либо в определенное состояние, часто неприятное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доводить",
                                "target_lang_sense_clarification": "до какого-либо состояния (до слез, до отчаяния)"
                            },
                            {
                                "target_lang_word": "сводить",
                                "target_lang_sense_clarification": "в сочетании 'сводить с ума'"
                            },
                            {
                                "target_lang_word": "вгонять",
                                "target_lang_sense_clarification": "в состояние (в краску, в тоску)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8780854-4069-3000-9159-77d55e46710f",
                    "sense_definition": "To motivate or compel someone to do something; to provide the energy or will for an action.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "force_impel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A strong desire to succeed <w>drives</w> him to work late every night.",
                            "target_lang_translations": {
                                "ru": "Сильное желание преуспеть <w>заставляет</w> его работать допоздна каждую ночь."
                            }
                        },
                        {
                            "text": "Curiosity often <w>drives</w> scientists to discover new things.",
                            "target_lang_translations": {
                                "ru": "Любопытство часто <w>толкает</w> ученых на новые открытия."
                            }
                        }
                    ],
                    "synonyms": [
                        "motivate",
                        "compel",
                        "urge",
                        "push"
                    ],
                    "common_phrases": [
                        "drive the economy",
                        "drive innovation",
                        "drive sales"
                    ],
                    "target_lang_definitions": {
                        "ru": "Побуждать или заставлять кого-то действовать; давать энергию для действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побуждать",
                                "target_lang_sense_clarification": "мотивировать к действию"
                            },
                            {
                                "target_lang_word": "двигать",
                                "target_lang_sense_clarification": "быть движущей силой (любопытство движет им)"
                            },
                            {
                                "target_lang_word": "заставлять",
                                "target_lang_sense_clarification": "принуждать обстоятельствами или желанием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fca23f93-ba43-31de-a281-5d772981fa31",
                    "sense_definition": "To push or force something into a specific position or direction by hitting or pressing it.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need a heavy hammer to <w>drive</w> the long nails into the wood.",
                            "target_lang_translations": {
                                "ru": "Вам понадобится тяжелый молоток, чтобы <w>забить</w> длинные гвозди в дерево."
                            }
                        },
                        {
                            "text": "The strong wind <w>drove</w> the rain against the windowpane.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер <w>гнал</w> дождь в оконное стекло."
                            }
                        }
                    ],
                    "synonyms": [
                        "hammer",
                        "push",
                        "force",
                        "ram"
                    ],
                    "common_phrases": [
                        "drive a nail",
                        "drive a wedge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вбивать или загонять что-то в определенное положение ударами или давлением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забивать",
                                "target_lang_sense_clarification": "вгонять ударами (гвоздь)"
                            },
                            {
                                "target_lang_word": "вбивать",
                                "target_lang_sense_clarification": "внедрять внутрь чего-то"
                            },
                            {
                                "target_lang_word": "гнать",
                                "target_lang_sense_clarification": "перемещать силой ветра или потока"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88388a6a-041d-384f-a960-030bfe257cc0",
                    "sense_definition": "To provide the power or energy that makes a machine or process work.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A large electric motor <w>drives</w> the conveyor belt in the factory.",
                            "target_lang_translations": {
                                "ru": "Большой электродвигатель <w>приводит в движение</w> конвейерную ленту на фабрике."
                            }
                        },
                        {
                            "text": "The wind turbine generates electricity to <w>drive</w> the pumps.",
                            "target_lang_translations": {
                                "ru": "Ветряная турбина вырабатывает электричество, чтобы <w>питать</w> насосы."
                            }
                        }
                    ],
                    "synonyms": [
                        "power",
                        "propel",
                        "operate"
                    ],
                    "common_phrases": [
                        "engine drives the wheels",
                        "steam-driven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечивать энергией работу механизма или процесса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приводить в движение",
                                "target_lang_sense_clarification": "заставлять механизм работать"
                            },
                            {
                                "target_lang_word": "вращать",
                                "target_lang_sense_clarification": "крутить (колеса, вал)"
                            },
                            {
                                "target_lang_word": "питать",
                                "target_lang_sense_clarification": "снабжать энергией"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "090453a6-3dc3-34cf-a785-8d88739d57af",
                    "sense_definition": "A journey made in a car or other vehicle.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "vehicle_trip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went for a long <w>drive</w> through the countryside on Sunday.",
                            "target_lang_translations": {
                                "ru": "В воскресенье мы отправились в долгую <w>поездку</w> по сельской местности."
                            }
                        },
                        {
                            "text": "It is a three-hour <w>drive</w> from here to the coast.",
                            "target_lang_translations": {
                                "ru": "Отсюда до побережья три часа <w>езды</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "trip",
                        "ride",
                        "journey"
                    ],
                    "common_phrases": [
                        "go for a drive",
                        "long drive",
                        "test drive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествие или поездка на автомобиле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поездка",
                                "target_lang_sense_clarification": "путешествие на транспорте"
                            },
                            {
                                "target_lang_word": "езда",
                                "target_lang_sense_clarification": "процесс перемещения"
                            },
                            {
                                "target_lang_word": "катание",
                                "target_lang_sense_clarification": "прогулка на машине ради удовольствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e88c7e20-bbcc-370a-ab9c-d383b7c16536",
                    "sense_definition": "A device in a computer that reads and writes data to a storage disk.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computer_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I saved all my important documents on a portable flash <w>drive</w>.",
                            "target_lang_translations": {
                                "ru": "Я сохранил все важные документы на переносном флеш-<w>накопителе</w>."
                            }
                        },
                        {
                            "text": "The computer's hard <w>drive</w> is full and needs to be cleaned.",
                            "target_lang_translations": {
                                "ru": "Жесткий <w>диск</w> компьютера переполнен, и его нужно почистить."
                            }
                        }
                    ],
                    "synonyms": [
                        "disk drive",
                        "hard drive"
                    ],
                    "common_phrases": [
                        "hard drive",
                        "flash drive",
                        "USB drive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство в компьютере для чтения и записи данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накопитель",
                                "target_lang_sense_clarification": "устройство хранения (флеш-накопитель)"
                            },
                            {
                                "target_lang_word": "диск",
                                "target_lang_sense_clarification": "носитель информации (жесткий диск)"
                            },
                            {
                                "target_lang_word": "дисковод",
                                "target_lang_sense_clarification": "устройство для чтения дисков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97937b1d-5c44-3899-88e9-4a7815046c4a",
                    "sense_definition": "A private road leading from a public street to a house or garage.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "road",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please park your car in the <w>drive</w> so it is off the street.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, припаркуй машину на <w>подъездной дорожке</w>, чтобы она не стояла на улице."
                            }
                        },
                        {
                            "text": "The house has a long <w>drive</w> lined with old oak trees.",
                            "target_lang_translations": {
                                "ru": "У дома есть длинная <w>подъездная аллея</w>, обсаженная старыми дубами."
                            }
                        }
                    ],
                    "synonyms": [
                        "driveway"
                    ],
                    "common_phrases": [
                        "park in the drive",
                        "gravel drive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Частная дорога, ведущая от улицы к дому или гаражу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подъездная дорожка",
                                "target_lang_sense_clarification": "путь к дому"
                            },
                            {
                                "target_lang_word": "выезд",
                                "target_lang_sense_clarification": "место для въезда/выезда машины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "236ab944-4e3e-38e6-9683-11dd30e922a5",
                    "sense_definition": "A strong determination and energy to achieve something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "motivation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has the <w>drive</w> and ambition to become a successful lawyer.",
                            "target_lang_translations": {
                                "ru": "У нее есть <w>целеустремленность</w> и амбиции, чтобы стать успешным юристом."
                            }
                        },
                        {
                            "text": "You need a lot of <w>drive</w> to start your own business.",
                            "target_lang_translations": {
                                "ru": "Нужно много <w>энергии</w>, чтобы начать свой собственный бизнес."
                            }
                        }
                    ],
                    "synonyms": [
                        "ambition",
                        "motivation",
                        "determination",
                        "energy"
                    ],
                    "antonyms": [
                        "laziness",
                        "lack of motivation"
                    ],
                    "common_phrases": [
                        "sex drive",
                        "competitive drive",
                        "lack of drive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное желание, решимость и энергия для достижения цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целеустремленность",
                                "target_lang_sense_clarification": "стремление к достижению цели"
                            },
                            {
                                "target_lang_word": "энергия",
                                "target_lang_sense_clarification": "внутренняя сила"
                            },
                            {
                                "target_lang_word": "напористость",
                                "target_lang_sense_clarification": "упорство в действиях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dc34bf3-26d4-3298-a0a9-c16114609f3d",
                    "sense_definition": "An organized effort by a group of people to achieve a specific goal, often for charity or business.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "campaign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school organized a food <w>drive</w> to help local families in need.",
                            "target_lang_translations": {
                                "ru": "Школа организовала <w>акцию</w> по сбору еды для помощи нуждающимся местным семьям."
                            }
                        },
                        {
                            "text": "The company launched a new sales <w>drive</w> to attract more customers.",
                            "target_lang_translations": {
                                "ru": "Компания запустила новую торговую <w>кампанию</w> для привлечения большего числа клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "campaign",
                        "push",
                        "effort"
                    ],
                    "common_phrases": [
                        "recruitment drive",
                        "charity drive",
                        "blood drive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованные усилия группы людей для достижения конкретной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кампания",
                                "target_lang_sense_clarification": "комплекс мероприятий"
                            },
                            {
                                "target_lang_word": "акция",
                                "target_lang_sense_clarification": "единовременное мероприятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06ce7606-d5f4-3d80-9bd3-abe2bfccea4b",
                    "sense_definition": "A powerful hit or stroke in sports like golf, cricket, or tennis.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He hit a powerful <w>drive</w> down the center of the fairway.",
                            "target_lang_translations": {
                                "ru": "Он выполнил мощный <w>драйв</w> по центру фервея."
                            }
                        },
                        {
                            "text": "The tennis player won the point with a stunning forehand <w>drive</w>.",
                            "target_lang_translations": {
                                "ru": "Теннисист выиграл очко потрясающим <w>драйвом</w> справа."
                            }
                        }
                    ],
                    "synonyms": [
                        "shot",
                        "stroke",
                        "hit"
                    ],
                    "common_phrases": [
                        "line drive",
                        "cover drive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный удар в таких видах спорта, как гольф, крикет или теннис."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "драйв",
                                "target_lang_sense_clarification": "специфический удар в теннисе/гольфе"
                            },
                            {
                                "target_lang_word": "удар",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "971baecb-094f-35cc-a9a8-6ca58d6c0eed",
                    "sense_definition": "A Germanic language closely related to English and descended from northern dialects of Middle English, spoken in parts of Scotland, now especially in the northeastern and southern regions of the country.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "She is learning to read poetry written in <w>Scots</w>.",
                            "target_lang_translations": {
                                "ru": "Она учится читать стихи, написанные на <w>шотландском</w>."
                            }
                        },
                        {
                            "text": "The <w>Scots</w> language has a distinct vocabulary and grammar.",
                            "target_lang_translations": {
                                "ru": "Язык <w>скотс</w> обладает самобытной лексикой и грамматикой."
                            }
                        }
                    ],
                    "synonyms": [
                        "Lowland Scots"
                    ],
                    "target_lang_definitions": {
                        "ru": "Германский язык, тесно связанный с английским и происходящий от северных диалектов среднеанглийского языка; распространен в Шотландии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шотландский язык",
                                "target_lang_sense_clarification": "язык равнинной Шотландии"
                            },
                            {
                                "target_lang_word": "скотс",
                                "target_lang_sense_clarification": "лингвистический термин"
                            }
                        ]
                    }
                }
            ]
        },

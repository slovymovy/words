            "senses": [
                {
                    "sense_id": "3dc180af-c88b-30d3-bc1c-2ada02c5935e",
                    "sense_definition": "A thousand million; the number represented as one followed by nine zeros (10⁹).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "numbers",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The global population has recently reached eight <w>billion</w> people.",
                            "target_lang_translations": {
                                "ru": "Население планеты недавно достигло восьми <w>миллиардов</w> человек."
                            }
                        },
                        {
                            "text": "The government plans to invest several <w>billions</w> of dollars in renewable energy.",
                            "target_lang_translations": {
                                "ru": "Правительство планирует инвестировать несколько <w>миллиардов</w> долларов в возобновляемую энергию."
                            }
                        }
                    ],
                    "synonyms": [
                        "thousand million",
                        "milliard"
                    ],
                    "common_phrases": [
                        "billions of dollars",
                        "billions of people",
                        "multi-billion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тысяча миллионов; число, изображаемое единицей с девятью нулями (10⁹)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миллиард",
                                "target_lang_sense_clarification": "число 10⁹"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62d208ef-2a77-3cdc-ba52-10acb3520bb8",
                    "sense_definition": "An unspecified very large number, used for exaggeration.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hyperbolic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have told you <w>billions</w> of times to clean your room.",
                            "target_lang_translations": {
                                "ru": "Я <w>миллиарды</w> раз говорил тебе убраться в комнате."
                            }
                        },
                        {
                            "text": "There were <w>billions</w> of mosquitoes at the lake last night.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером на озере были <w>миллиарды</w> комаров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "billions of times"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used hyperbolically"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопределенное очень большое число, используемое для преувеличения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миллиард",
                                "target_lang_sense_clarification": "огромное множество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5721c59-728a-3e8d-9ddd-97885f395c58",
                    "sense_definition": "A million million; the number represented as one followed by twelve zeros (10¹²).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "numbers",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In older British texts, a <w>billion</w> referred to a million million rather than a thousand million.",
                            "target_lang_translations": {
                                "ru": "В старых британских текстах <w>биллион</w> означал миллион миллионов, а не тысячу миллионов."
                            }
                        },
                        {
                            "text": "The old definition of a <w>billion</w> is mathematically equal to ten to the power of twelve.",
                            "target_lang_translations": {
                                "ru": "Старое определение <w>биллиона</w> математически равно десяти в двенадцатой степени."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "formerly standard in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Миллион миллионов; число, изображаемое единицей с двенадцатью нулями (10¹²)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "биллион",
                                "target_lang_sense_clarification": "устаревшее значение (10¹²)"
                            },
                            {
                                "target_lang_word": "триллион",
                                "target_lang_sense_clarification": "современный эквивалент числа"
                            }
                        ]
                    }
                }
            ]
        }
    ]

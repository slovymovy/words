    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc75327e-48f9-3853-9bfb-84f47fd51a8f",
                    "sense_definition": "De uiting dat men het met iemand of iets eens is of iets goedkeurt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het publiek reageerde met luide <w>instemming</w> op zijn toespraak.",
                            "target_lang_translations": {
                                "en": "The audience responded to his speech with loud <w>approval</w>.",
                                "ru": "Публика отреагировала на его речь громким <w>одобрением</w>."
                            }
                        },
                        {
                            "text": "Zij knikte ter <w>instemming</w> toen de nieuwe plannen werden gepresenteerd.",
                            "target_lang_translations": {
                                "en": "She nodded in <w>agreement</w> when the new plans were presented.",
                                "ru": "Она кивнула в знак <w>согласия</w>, когда были представлены новые планы."
                            }
                        }
                    ],
                    "synonyms": [
                        "goedkeuring",
                        "akkoord",
                        "bijval"
                    ],
                    "antonyms": [
                        "afkeuring",
                        "onenigheid",
                        "tegenstand"
                    ],
                    "common_phrases": [
                        "met instemming",
                        "knikken van instemming",
                        "algemene instemming"
                    ],
                    "target_lang_definitions": {
                        "en": "The expression of agreeing with someone or something, or approving of something.",
                        "ru": "Выражение согласия с кем-либо или чем-либо, или одобрение чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "agreement",
                                "target_lang_sense_clarification": "general state of agreeing"
                            },
                            {
                                "target_lang_word": "approval",
                                "target_lang_sense_clarification": "positive acceptance or sanction"
                            },
                            {
                                "target_lang_word": "assent",
                                "target_lang_sense_clarification": "expression of concurrence, often used with nodding"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "одобрение",
                                "target_lang_sense_clarification": "положительная оценка или поддержка"
                            },
                            {
                                "target_lang_word": "согласие",
                                "target_lang_sense_clarification": "выражение единства мнений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74bda902-4095-36b2-ab18-b7a53cdd3411",
                    "sense_definition": "De officiële of formele toelating om een bepaalde handeling te verrichten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur verleende zijn <w>instemming</w> aan het verzoek van de werknemers.",
                            "target_lang_translations": {
                                "en": "The director gave his <w>consent</w> to the employees' request.",
                                "ru": "Директор дал свое <w>согласие</w> на просьбу сотрудников."
                            }
                        },
                        {
                            "text": "Zonder schriftelijke <w>instemming</w> van je ouders mag je niet mee op schoolreis.",
                            "target_lang_translations": {
                                "en": "Without written <w>consent</w> from your parents, you may not go on the school trip.",
                                "ru": "Без письменного <w>согласия</w> родителей тебе нельзя ехать в школьную поездку."
                            }
                        }
                    ],
                    "synonyms": [
                        "toestemming",
                        "permissie",
                        "toelating"
                    ],
                    "antonyms": [
                        "weigering",
                        "verbod"
                    ],
                    "common_phrases": [
                        "instemming verlenen",
                        "schriftelijke instemming",
                        "instemming vragen"
                    ],
                    "target_lang_definitions": {
                        "en": "Official or formal permission to perform a specific action.",
                        "ru": "Официальное или формальное разрешение на выполнение определенного действия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "consent",
                                "target_lang_sense_clarification": "formal agreement or permission"
                            },
                            {
                                "target_lang_word": "permission",
                                "target_lang_sense_clarification": "authorization to do something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "согласие",
                                "target_lang_sense_clarification": "официальное разрешение или дозволение"
                            },
                            {
                                "target_lang_word": "разрешение",
                                "target_lang_sense_clarification": "право на совершение действия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

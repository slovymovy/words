    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bbe99f0c-0d2c-3139-82fc-5cb292f21975",
                    "sense_definition": "Plural form of <w>green</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist used several different <w>greens</w> to paint the forest landscape.",
                            "target_lang_translations": {
                                "ru": "Художник использовал несколько разных <w>оттенков зелёного</w>, чтобы написать лесной пейзаж."
                            }
                        },
                        {
                            "text": "Golfers aim to get the ball onto the <w>greens</w> as quickly as possible.",
                            "target_lang_translations": {
                                "ru": "Гольфисты стремятся как можно быстрее загнать мяч на <w>грины</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова \"green\" (зеленый цвет; лужайка для гольфа)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отте́нки зелёного",
                                "target_lang_sense_clarification": "цвета"
                            },
                            {
                                "target_lang_word": "гри́ны",
                                "target_lang_sense_clarification": "в гольфе"
                            },
                            {
                                "target_lang_word": "лужа́йки",
                                "target_lang_sense_clarification": "в гольфе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55fe56b5-2db4-39c6-a1e0-aadf5e7d69c6",
                    "sense_definition": "Edible leaves and stems of plants, such as spinach, kale, or cabbage, eaten as vegetables.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should eat your <w>greens</w> every day to stay healthy and strong.",
                            "target_lang_translations": {
                                "ru": "Тебе следует есть <w>зелень</w> каждый день, чтобы оставаться здоровым и сильным."
                            }
                        },
                        {
                            "text": "She prepared a healthy salad with mixed <w>greens</w> and tomatoes.",
                            "target_lang_translations": {
                                "ru": "Она приготовила полезный салат из смеси <w>зелени</w> и помидоров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leafy greens",
                        "salad greens",
                        "collard greens",
                        "turnip greens"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобные листья и стебли растений, такие как шпинат или капуста, употребляемые в пищу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зе́лень",
                                "target_lang_sense_clarification": "листовые овощи и травы"
                            },
                            {
                                "target_lang_word": "листовы́е о́вощи",
                                "target_lang_sense_clarification": "кулинария"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86c958dd-ac6b-3fb2-be2b-edbb15852acb",
                    "sense_definition": "Cut leafy stems, branches, or plants used for decoration.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "decoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decorated the banquet hall with winter <w>greens</w> and red ribbons.",
                            "target_lang_translations": {
                                "ru": "Мы украсили банкетный зал зимней <w>зеленью</w> и красными лентами."
                            }
                        },
                        {
                            "text": "The florist added some fresh <w>greens</w> to the bouquet of roses.",
                            "target_lang_translations": {
                                "ru": "Флорист добавил немного свежей <w>зелени</w> в букет роз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Christmas greens",
                        "holiday greens"
                    ],
                    "target_lang_definitions": {
                        "ru": "Срезанные стебли, ветки или растения, используемые для украшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зе́лень",
                                "target_lang_sense_clarification": "декоративная"
                            },
                            {
                                "target_lang_word": "листва́",
                                "target_lang_sense_clarification": "для букетов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2b160b7-195f-3eb4-a3b0-ba1e9d07122a",
                    "sense_definition": "The green dress uniform of the United States Marine Corps.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldier looked very proud wearing his service <w>greens</w> for the ceremony.",
                            "target_lang_translations": {
                                "ru": "Солдат выглядел очень гордым в своей служебной <w>зелёной форме</w> на церемонии."
                            }
                        },
                        {
                            "text": "He carefully ironed his <w>greens</w> before the inspection.",
                            "target_lang_translations": {
                                "ru": "Он тщательно выгладил свою <w>зелёную форму</w> перед осмотром."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "US"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "military context"
                        }
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "natchnienie",
        "natchniony"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d23e5692-ec74-35df-beae-2333b9c65d89",
                    "sense_definition": "Wzbudzić w kimś wenę, zapał, silne uczucia lub myśli, często pobudzając do twórczego działania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "inspire_others",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Widok gór <natchnął> poetę do napisania wspaniałego wiersza.",
                            "target_lang_translations": {
                                "ru": "Вид гор <вдохновил> поэта на написание великолепного стихотворения."
                            }
                        },
                        {
                            "text": "Przemówienie lidera <natchnęło> wszystkich zebranych nową nadzieją i optymizmem.",
                            "target_lang_translations": {
                                "ru": "Речь лидера <вдохновила> всех собравшихся новой надеждой и оптимизмом."
                            }
                        }
                    ],
                    "synonyms": [
                        "zainspirować",
                        "nadchnąć"
                    ],
                    "antonyms": [
                        "zniechęcić"
                    ],
                    "common_phrases": [
                        "natchnąć nadzieją",
                        "natchnąć do działania",
                        "natchnąć wiarą",
                        "natchnąć otuchą",
                        "natchnąć optymizmem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызвать в ком-либо вдохновение, энтузиазм, сильные чувства или мысли, часто побуждая к творческой деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдохновить",
                                "target_lang_sense_clarification": "побудить к творчеству или действию"
                            },
                            {
                                "target_lang_word": "воодушевить",
                                "target_lang_sense_clarification": "поднять дух, наполнить энергией"
                            },
                            {
                                "target_lang_word": "вселить",
                                "target_lang_sense_clarification": "внушить чувство (надежду, веру)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba438317-7228-310a-9c47-5083d24cf539",
                    "sense_definition": "Poczuć przypływ weny, emocji lub motywacji pod wpływem zewnętrznego bodźca (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "inspire_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody artysta <natchnął> się dziełami mistrzów renesansu.",
                            "target_lang_translations": {
                                "ru": "Молодой художник <вдохновился> работами мастеров эпохи Возрождения."
                            }
                        },
                        {
                            "text": "Podczas podróży <natchnęliśmy> się lokalną kulturą do zmiany wystroju domu.",
                            "target_lang_translations": {
                                "ru": "Во время путешествия мы <вдохновились> местной культурой на изменение интерьера дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "zainspirować się"
                    ],
                    "common_phrases": [
                        "natchnąć się widokiem",
                        "natchnąć się lekturą",
                        "natchnąć się przykładem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Почувствовать прилив вдохновения, эмоций или мотивации под влиянием внешнего стимула."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдохновиться",
                                "target_lang_sense_clarification": "получить творческий импульс от чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12d925a9-69b0-30f0-ae05-4eb9e576cd68",
                    "sense_definition": "Wtłoczyć powietrze lub napełnić tchem; w sensie przenośnym przekazać ducha lub życie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fill_with_breath",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Według dawnych wierzeń bóstwo <natchnęło> posąg życiem.",
                            "target_lang_translations": {
                                "ru": "Согласно древним верованиям, божество <вдохнуло> жизнь в статую."
                            }
                        },
                        {
                            "text": "Potężny wiatr <natchnął> żagle i statek ruszył w podróż.",
                            "target_lang_translations": {
                                "ru": "Могучий ветер <наполнил> паруса, и корабль отправился в путь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "natchnąć duchem",
                        "natchnąć życiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane głównie w kontekstach biblijnych lub poetyckich"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вдохнуть воздух или наполнить дыханием; в переносном смысле — наделить духом или жизнью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдохнуть",
                                "target_lang_sense_clarification": "наполнить жизнью или воздухом"
                            },
                            {
                                "target_lang_word": "наполнить",
                                "target_lang_sense_clarification": "заполнить (например, паруса ветром)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
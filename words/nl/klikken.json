                    "target_lang_definitions": {
                        "ru": "Множественное число от слова 'klik' (щелчок или группа людей/клан).",
                        "en": "Plural of 'klik' (a click sound or a clique/group of people)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щелчки",
                                "target_lang_sense_clarification": "звуки"
                            },
                            {
                                "target_lang_word": "компании",
                                "target_lang_sense_clarification": "группы людей, кланы"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "clicks",
                                "target_lang_sense_clarification": "sounds"
                            },
                            {
                                "target_lang_word": "cliques",
                                "target_lang_sense_clarification": "groups of people"
                            }
                        ]
                    }
                }
            ]
        }
    ]

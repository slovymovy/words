    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ca9831a3-48d2-330c-85a2-4f3595bf358d",
                    "sense_definition": "Vague, ill-defined, or unclear; lacking definite form or limits.",
                    "learner_level": "C2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The plan for the new project remained <w>nebulous</w> and difficult to understand.",
                            "target_lang_translations": {
                                "ru": "План нового проекта оставался <w>расплывчатым</w> и трудным для понимания."
                            }
                        },
                        {
                            "text": "He had only a <w>nebulous</w> idea of what he wanted to do after graduation.",
                            "target_lang_translations": {
                                "ru": "У него было лишь <w>смутное</w> представление о том, чем он хочет заниматься после выпуска."
                            }
                        }
                    ],
                    "synonyms": [
                        "hazy",
                        "cloudy"
                    ],
                    "antonyms": [
                        "clear"
                    ],
                    "common_phrases": [
                        "nebulous concept",
                        "nebulous idea",
                        "nebulous future"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смутный, неясный или расплывчатый; не имеющий четкой формы или границ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сму́тный",
                                "target_lang_sense_clarification": "об идеях, чувствах или воспоминаниях"
                            },
                            {
                                "target_lang_word": "расплы́вчатый",
                                "target_lang_sense_clarification": "не имеющий четких границ или определенности"
                            },
                            {
                                "target_lang_word": "тума́нный",
                                "target_lang_sense_clarification": "непонятный, загадочный"
                            },
                            {
                                "target_lang_word": "неопределённый",
                                "target_lang_sense_clarification": "точно не установленный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e838460-3d6f-3ab3-a89d-3a3b319a5e9f",
                    "sense_definition": "Resembling a cloud or haze; misty and indistinct.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>nebulous</w> figure appeared slowly through the thick morning fog.",
                            "target_lang_translations": {
                                "ru": "Сквозь густой утренний туман медленно проступила <w>неясная</w> фигура."
                            }
                        },
                        {
                            "text": "The mountain peak was hidden behind a <w>nebulous</w> veil of mist.",
                            "target_lang_translations": {
                                "ru": "Вершина горы была скрыта за <w>туманной</w> завесой мглы."
                            }
                        }
                    ],
                    "synonyms": [
                        "cloudy",
                        "misty",
                        "foggy",
                        "vaporous",
                        "hazy"
                    ],
                    "antonyms": [
                        "clear",
                        "unclouded"
                    ],
                    "common_phrases": [
                        "nebulous shape",
                        "nebulous form"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий облако или дымку; туманный и нечеткий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тума́нный",
                                "target_lang_sense_clarification": "похожий на туман"
                            },
                            {
                                "target_lang_word": "неотчётливый",
                                "target_lang_sense_clarification": "плохо различимый"
                            },
                            {
                                "target_lang_word": "о́блачный",
                                "target_lang_sense_clarification": "похожий на облако"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20142f0c-59a3-39be-a673-1fa1e7fa3226",
                    "sense_definition": "Relating to a nebula (an interstellar cloud of dust and gas).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The telescope captured the <w>nebulous</w> glow of a distant star cluster.",
                            "target_lang_translations": {
                                "ru": "Телескоп запечатлел <w>туманное</w> свечение далекого звездного скопления."
                            }
                        },
                        {
                            "text": "Scientists studied the <w>nebulous</w> gas surrounding the dying star.",
                            "target_lang_translations": {
                                "ru": "Ученые изучали <w>туманный</w> газ, окружающий умирающую звезду."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к туманности (межзвездному облаку пыли и газа)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тума́нный",
                                "target_lang_sense_clarification": "относящийся к туманности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

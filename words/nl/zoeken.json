    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "03bd7805-2dbc-3f8f-be1e-1b0462c8f3b0",
                    "sense_definition": "Proberen iemand of iets te vinden door te kijken of te vragen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "finding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben mijn sleutels kwijt en moet ze nu overal <w>zoeken</w>.",
                            "target_lang_translations": {
                                "en": "I lost my keys and have to <w>look for</w> them everywhere now.",
                                "ru": "Я потерял свои ключи, и теперь мне нужно <w>искать</w> их повсюду."
                            }
                        },
                        {
                            "text": "Zij <w>zoekt</w> naar een nieuwe baan in de buurt van haar huis.",
                            "target_lang_translations": {
                                "en": "She is <w>looking for</w> a new job near her house.",
                                "ru": "Она <w>ищет</w> новую работу недалеко от своего дома."
                            }
                        },
                        {
                            "text": "Heb je wel goed genoeg <w>gezocht</w> in de kast?",
                            "target_lang_translations": {
                                "en": "Did you <w>look</w> carefully enough in the cupboard?",
                                "ru": "Ты достаточно хорошо <w>искал</w> в шкафу?"
                            }
                        }
                    ],
                    "synonyms": [
                        "speuren",
                        "opsporen"
                    ],
                    "antonyms": [
                        "vinden"
                    ],
                    "common_phrases": [
                        "zoeken naar",
                        "ruzie zoeken",
                        "werk zoeken",
                        "iets te zoeken hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "To try to find someone or something by looking or asking.",
                        "ru": "Пытаться найти кого-то или что-то, осматривая места или спрашивая."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look for",
                                "target_lang_sense_clarification": "most common, general use"
                            },
                            {
                                "target_lang_word": "search",
                                "target_lang_sense_clarification": "to look thoroughly or systematically"
                            },
                            {
                                "target_lang_word": "seek",
                                "target_lang_sense_clarification": "formal, or for abstract things like a job"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "пытаться найти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "838b477a-483b-3ffe-8343-a8954e817bb5",
                    "sense_definition": "Moeite doen om een bepaald doel te bereiken; trachten iets te doen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attempting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schrijver <w>zoekt</w> in zijn nieuwe roman de menselijke natuur te doorgronden.",
                            "target_lang_translations": {
                                "en": "In his new novel, the writer <w>seeks</w> to fathom human nature.",
                                "ru": "В своем новом романе писатель <w>стремится</w> постичь человеческую природу."
                            }
                        },
                        {
                            "text": "Hij <w>zocht</w> te ontsnappen aan de drukte van de grote stad.",
                            "target_lang_translations": {
                                "en": "He <w>sought</w> to escape the hustle and bustle of the big city.",
                                "ru": "Он <w>пытался</w> сбежать от суеты большого города."
                            }
                        }
                    ],
                    "synonyms": [
                        "trachten",
                        "pogen",
                        "proberen"
                    ],
                    "common_phrases": [
                        "zoeken te"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To make an effort to achieve a certain goal; to attempt to do something.",
                        "ru": "Прилагать усилия для достижения определенной цели; пытаться что-то сделать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seek",
                                "target_lang_sense_clarification": "formal, usually followed by 'to'"
                            },
                            {
                                "target_lang_word": "attempt",
                                "target_lang_sense_clarification": "to try to do something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "стремиться",
                                "target_lang_sense_clarification": "настойчиво добиваться цели"
                            },
                            {
                                "target_lang_word": "пытаться",
                                "target_lang_sense_clarification": "пробовать совершить действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f22516d7-c3d8-3b33-9fe1-616be1126062",
                    "sense_definition": "W stopniu znacznym, choć nie całkowitym; określenie umiarkowanie dużej intensywności cechy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree_quite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten film był <dość> ciekawy, ale widziałem lepsze.",
                            "target_lang_translations": {
                                "ru": "Этот фильм был <довольно> интересным, но я видел и получше."
                            }
                        },
                        {
                            "text": "Mieszkam <dość> daleko od centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Я живу <довольно> далеко от центра города."
                            }
                        }
                    ],
                    "synonyms": [
                        "całkiem",
                        "w miarę",
                        "stosunkowo",
                        "dosyć"
                    ],
                    "antonyms": [
                        "mało powiedzieć"
                    ],
                    "common_phrases": [
                        "dość dobrze",
                        "dość często",
                        "dość długo",
                        "dość duży"
                    ],
                    "target_lang_definitions": {
                        "ru": "В значительной степени, хотя и не полностью; обозначение умеренно высокой интенсивности признака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "довольно",
                                "target_lang_sense_clarification": "в значительной степени, весьма"
                            },
                            {
                                "target_lang_word": "достаточно",
                                "target_lang_sense_clarification": "в значении «довольно», «вполне»"
                            },
                            {
                                "target_lang_word": "весьма",
                                "target_lang_sense_clarification": "очень (более формально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84e3e8b9-52b7-3cc0-b381-2d2a51137834",
                    "sense_definition": "W stopniu wystarczającym; tyle, ile jest wymagane.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sufficiency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie przygotowałeś się <dość> starannie do tego egzaminu.",
                            "target_lang_translations": {
                                "ru": "Ты не <достаточно> тщательно подготовился к этому экзамену."
                            }
                        },
                        {
                            "text": "Nie <dość>, że się spóźnił, to jeszcze zapomniał portfela.",
                            "target_lang_translations": {
                                "ru": "<Мало> того, что он опоздал, так еще и забыл кошелек."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystarczająco",
                        "dosyć"
                    ],
                    "common_phrases": [
                        "nie dość że"
                    ],
                    "target_lang_definitions": {
                        "ru": "В достаточной степени; столько, сколько требуется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достаточно",
                                "target_lang_sense_clarification": "в необходимой мере"
                            },
                            {
                                "target_lang_word": "довольно",
                                "target_lang_sense_clarification": "хватает (наречие)"
                            }
                        ]
                    }
                }
            ]
        },
        {

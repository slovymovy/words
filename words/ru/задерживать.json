{
    "word_family": [
        "задержка",
        "задержание",
        "держать",
        "держаться",
        "сдерживать",
        "удерживать",
        "поддерживать",
        "содержать",
        "выдерживать",
        "придерживать"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f232bff7-4306-307d-87f4-fa29ddb476d5",
                    "sense_definition": "Вынуждать кого-либо остаться где-либо, отнимать у кого-либо время, не отпускать.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "restrict_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Извините, я не хочу вас <w>задерживать</w>, если вы спешите на встречу.",
                            "target_lang_translations": {
                                "en": "Sorry, I don't want to <w>keep</w> you if you are in a hurry for a meeting."
                            }
                        },
                        {
                            "text": "Начальник <w>задержал</w> меня в офисе до позднего вечера из-за срочного отчета.",
                            "target_lang_translations": {
                                "en": "The boss <w>kept</w> me at the office until late in the evening because of an urgent report."
                            }
                        }
                    ],
                    "synonyms": [
                        "стопорить"
                    ],
                    "antonyms": [
                        "отпускать"
                    ],
                    "common_phrases": [
                        "не смею задерживать",
                        "задерживать на работе",
                        "задерживать гостей"
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone to stay somewhere, take up their time, or prevent them from leaving."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "to cause someone to remain or stay; to take up someone's time"
                            },
                            {
                                "target_lang_word": "detain",
                                "target_lang_sense_clarification": "to officially or formally prevent someone from leaving"
                            },
                            {
                                "target_lang_word": "hold up",
                                "target_lang_sense_clarification": "to delay someone, often by external circumstances"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e6481fc-ae0a-32e1-8c6c-8efdcf9c0852",
                    "sense_definition": "Приостанавливать или замедлять ход, развитие чего-либо; препятствовать движению.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "delay_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный снегопад <w>задержал</w> вылет самолета на несколько часов.",
                            "target_lang_translations": {
                                "en": "Heavy snowfall <w>delayed</w> the plane's departure for several hours."
                            }
                        },
                        {
                            "text": "Высокие горы <w>задерживают</w> холодный воздух и защищают долину.",
                            "target_lang_translations": {
                                "en": "High mountains <w>hold back</w> the cold air and protect the valley."
                            }
                        }
                    ],
                    "synonyms": [
                        "замедлять",
                        "приостанавливать",
                        "стопорить",
                        "стопорить"
                    ],
                    "antonyms": [
                        "ускорять",
                        "торопить"
                    ],
                    "common_phrases": [
                        "задерживать дыхание",
                        "задерживать движение",
                        "задерживать развитие"
                    ],
                    "target_lang_definitions": {
                        "en": "To suspend or slow down the progress or development of something; to hinder movement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "delay",
                                "target_lang_sense_clarification": "to make something happen later than expected"
                            },
                            {
                                "target_lang_word": "slow down",
                                "target_lang_sense_clarification": "to reduce the speed of a process or movement"
                            },
                            {
                                "target_lang_word": "hold up",
                                "target_lang_sense_clarification": "to stop or obstruct the flow or progress of something"
                            },
                            {
                                "target_lang_word": "hamper",
                                "target_lang_sense_clarification": "to impede or hinder movement or progress"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36b4cb1c-4fe6-3e17-993d-45286c6b0624",
                    "sense_definition": "Не отдавать или не выплачивать что-либо вовремя; опаздывать с выдачей.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "delay_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания уже два месяца <w>задерживает</w> выплату зарплаты своим сотрудникам.",
                            "target_lang_translations": {
                                "en": "The company has been <w>delaying</w> salary payments to its employees for two months already."
                            }
                        },
                        {
                            "text": "Пожалуйста, не <w>задерживайте</w> библиотечные книги дольше положенного срока.",
                            "target_lang_translations": {
                                "en": "Please do not <w>keep</w> library books longer than the due date."
                            }
                        }
                    ],
                    "antonyms": [
                        "возвращать",
                        "выплачивать"
                    ],
                    "common_phrases": [
                        "задерживать зарплату",
                        "задерживать выплаты",
                        "задерживать сдачу"
                    ],
                    "target_lang_definitions": {
                        "en": "To not give, return, or pay something on time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "delay",
                                "target_lang_sense_clarification": "to be late in paying or giving something"
                            },
                            {
                                "target_lang_word": "withhold",
                                "target_lang_sense_clarification": "to refuse to give something that is due"
                            },
                            {
                                "target_lang_word": "hold back",
                                "target_lang_sense_clarification": "to keep something instead of giving or releasing it"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01e2ea62-7d3c-3c0a-932a-2146b27c7d0b",
                    "sense_definition": "Брать под стражу, лишать свободы (обычно о действиях полиции или органов власти).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "restrict_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция <w>задержала</w> грабителя прямо на месте преступления.",
                            "target_lang_translations": {
                                "en": "The police <w>arrested</w> the robber right at the crime scene."
                            }
                        },
                        {
                            "text": "Пограничники имеют право <w>задерживать</w> подозрительных лиц для проверки документов.",
                            "target_lang_translations": {
                                "en": "Border guards have the right to <w>detain</w> suspicious persons to check their documents."
                            }
                        }
                    ],
                    "synonyms": [
                        "стопорить"
                    ],
                    "antonyms": [
                        "освобождать",
                        "отпускать"
                    ],
                    "common_phrases": [
                        "задерживать преступника",
                        "задерживать подозреваемого",
                        "задерживать нарушителя"
                    ],
                    "target_lang_definitions": {
                        "en": "To take into custody or deprive of liberty (usually by police or authorities)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "detain",
                                "target_lang_sense_clarification": "to hold in custody, often temporarily for questioning"
                            },
                            {
                                "target_lang_word": "arrest",
                                "target_lang_sense_clarification": "to take into legal custody for a crime"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d41e6c7e-e079-328d-8b80-1e43c82622da",
                    "sense_definition": "Останавливать или сосредоточивать взгляд или внимание на ком-либо или чём-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он на секунду <w>задержал</w> взгляд на её лице, пытаясь вспомнить имя.",
                            "target_lang_translations": {
                                "en": "He <w>fixed</w> his gaze on her face for a second, trying to remember her name."
                            }
                        },
                        {
                            "text": "Не стоит долго <w>задерживать</w> внимание на мелких и незначительных деталях.",
                            "target_lang_translations": {
                                "en": "It is not worth <w>dwelling</w> on small and insignificant details for too long."
                            }
                        }
                    ],
                    "synonyms": [
                        "стопорить"
                    ],
                    "antonyms": [
                        "отводить"
                    ],
                    "common_phrases": [
                        "задерживать взгляд",
                        "задерживать внимание",
                        "задерживать взор"
                    ],
                    "target_lang_definitions": {
                        "en": "To fix or focus one's eyes or attention on someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fix",
                                "target_lang_sense_clarification": "used with 'gaze' or 'eyes' - to look steadily"
                            },
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "to maintain focus or attention on something"
                            },
                            {
                                "target_lang_word": "dwell on",
                                "target_lang_sense_clarification": "to think or speak about something for a long time"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
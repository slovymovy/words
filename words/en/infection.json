                {
                    "sense_id": "686dded2-09af-3450-ab4a-163cfe1cd14f",
                    "sense_definition": "A disease or condition caused by harmful organisms, such as bacteria or viruses, entering the body.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor prescribed a course of antibiotics to treat the bacterial <w>infection</w>.",
                            "target_lang_translations": {
                                "ru": "Врач назначил курс антибиотиков для лечения бактериальной <w>инфекции</w>."
                            }
                        },
                        {
                            "text": "She had to stay in bed for a week because of a severe chest <w>infection</w>.",
                            "target_lang_translations": {
                                "ru": "Ей пришлось неделю пролежать в постели из-за тяжелой <w>инфекции</w> дыхательных путей."
                            }
                        }
                    ],
                    "synonyms": [
                        "illness",
                        "disease",
                        "sickness"
                    ],
                    "common_phrases": [
                        "viral infection",
                        "bacterial infection",
                        "chest infection",
                        "ear infection",
                        "fight an infection"
                    ],
                    "target_lang_definitions": {
                        "ru": "Болезнь или состояние, вызванное проникновением в организм вредоносных организмов, таких как бактерии или вирусы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инфекция",
                                "target_lang_sense_clarification": "болезнь, вызванная микроорганизмами"
                            },
                            {
                                "target_lang_word": "инфекционное заболевание",
                                "target_lang_sense_clarification": "медицинский термин"
                            },
                            {
                                "target_lang_word": "зараза",
                                "target_lang_sense_clarification": "разговорное; болезнь или возбудитель болезни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86220be3-053f-3deb-b0af-758191307c09",
                    "sense_definition": "The act or process of passing germs or disease to someone or something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Strict hygiene rules are necessary to prevent the <w>infection</w> of other patients in the hospital.",
                            "target_lang_translations": {
                                "ru": "Строгие правила гигиены необходимы для предотвращения <w>заражения</w> других пациентов в больнице."
                            }
                        },
                        {
                            "text": "Cleaning the wound immediately reduces the risk of <w>infection</w> from dirty surfaces.",
                            "target_lang_translations": {
                                "ru": "Немедленная обработка раны снижает риск <w>заражения</w> от грязных поверхностей."
                            }
                        }
                    ],
                    "synonyms": [

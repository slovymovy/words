                {
                    "sense_id": "2fd1eea0-b771-37a5-a152-984b919be4f8",
                    "sense_definition": "Een toestand van boosheid en onenigheid; een conflict tussen personen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "conflict_situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij kregen <w>ruzie</w> en keerden elkaar woedend de rug toe.",
                            "target_lang_translations": {
                                "en": "They got into an <w>argument</w> and turned their backs on each other in anger.",
                                "ru": "Между ними произошла <w>ссора</w>, и они в ярости отвернулись друг от друга."
                            }
                        },
                        {
                            "text": "Een stel maakte <w>ruzie</w> over het menu in het restaurant.",
                            "target_lang_translations": {
                                "en": "A couple was having an <w>argument</w> about the menu in the restaurant.",
                                "ru": "Пара <w>ссорилась</w> из-за меню в ресторане."
                            }
                        }
                    ],
                    "synonyms": [
                        "conflict",
                        "onenigheid",
                        "geschil",
                        "twist"
                    ],
                    "antonyms": [
                        "vrede",
                        "harmonie"
                    ],
                    "common_phrases": [
                        "ruzie maken",
                        "ruzie hebben",
                        "ruzie zoeken",
                        "slaande ruzie"

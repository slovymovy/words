                    "sense_id": "3e81a811-eec2-3108-9f9b-4bf236a9a967",
                    "sense_definition": "Czwarta część godziny, czyli 15 minut.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czekam na ciebie przed kinem już cały <w>kwadrans</w>.",
                            "target_lang_translations": {
                                "ru": "Я жду тебя у кинотеатра уже целую <w>четверть часа</w>."
                            }
                        },
                        {
                            "text": "U dentysty spędziłam trzy <w>kwadranse</w>, w tym dwa w poczekalni.",
                            "target_lang_translations": {
                                "ru": "У стоматолога я провела три <w>четверти часа</w>, из них две — в приемной."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) sztwierć"
                    ],
                    "common_phrases": [
                        "kwadrans akademicki",
                        "za kwadrans"
                    ],
                    "target_lang_definitions": {
                        "ru": "Четвёртая часть часа, 15 минут."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "четверть часа",
                                "target_lang_sense_clarification": "промежуток времени"
                            },
                            {
                                "target_lang_word": "четверть",
                                "target_lang_sense_clarification": "в контексте времени (напр. без четверти)"
                            },
                            {
                                "target_lang_word": "пятнадцать минут",
                                "target_lang_sense_clarification": "эквивалент значения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8b5c461-83ab-3e8b-868d-90dc81e4a6df",
                    "sense_definition": "Drobna rzymska moneta z brązu, warta jedną czwartą asa.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pieniądz brązowy stanowiły mosiężne sesterce oraz miedziane asy i <w>kwadranse</w>.",
                            "target_lang_translations": {
                                "ru": "Бронзовые деньги состояли из латунных сестерциев, а также медных ассов и <w>квадрансов</w>."
                            }
                        },
                        {
                            "text": "W starożytnym Rzymie za jeden <w>kwadrans</w> można było skorzystać z łaźni.",
                            "target_lang_translations": {
                                "ru": "В Древнем Риме за один <w>квадранс</w> можно было посетить баню."
                            }
                        }
                    ],
                    "synonyms": [
                        "quadrans",
                        "kodrantes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкая римская бронзовая монета, равная одной четвёртой асса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квадранс",
                                "target_lang_sense_clarification": "римская монета"
                            }
                        ]
                    }
                }
            ]
        }

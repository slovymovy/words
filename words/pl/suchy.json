    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4497c744-1fda-39e2-9c4b-d1ba4b98eabd",
                    "sense_definition": "Pozbawiony wilgoci, niepokryty płynem; stanowiący przeciwieństwo mokrego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po godzinie na słońcu ręcznik był już całkowicie <w>suchy</w>.",
                            "target_lang_translations": {
                                "ru": "После часа на солнце полотенце было уже полностью <w>сухим</w>."
                            }
                        },
                        {
                            "text": "Muszę użyć balsamu, ponieważ mam bardzo <w>suchą</w> skórę na dłoniach.",
                            "target_lang_translations": {
                                "ru": "Мне нужно использовать бальзам, так как у меня очень <w>сухая</w> кожа на ладонях."
                            }
                        }
                    ],
                    "antonyms": [
                        "mokry",
                        "wilgotny"
                    ],
                    "common_phrases": [
                        "suchy lód",
                        "sucha skóra",
                        "suchy prowiant",
                        "suchą stopą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишённый влаги, не покрытый жидкостью; противоположность мокрому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сухой",
                                "target_lang_sense_clarification": "не мокрый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dce43337-6a5a-3085-8d72-4e11689991f6",
                    "sense_definition": "O pogodzie lub klimacie: charakteryzujący się brakiem lub małą ilością opadów deszczu.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To było wyjątkowo <w>suche</w> lato, co spowodowało zagrożenie pożarowe.",
                            "target_lang_translations": {
                                "ru": "Это было исключительно <w>сухое</w> лето, что создало пожарную угрозу."
                            }
                        },
                        {
                            "text": "W tym regionie panuje <w>suchy</w> klimat, idealny dla astmatyków.",
                            "target_lang_translations": {
                                "ru": "В этом регионе царит <w>сухой</w> климат, идеальный для астматиков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pora sucha",
                        "suchy klimat",
                        "suchy rok"
                    ],
                    "target_lang_definitions": {
                        "ru": "О погоде или климате: характеризующийся отсутствием или малым количеством осадков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сухой",
                                "target_lang_sense_clarification": "о климате"
                            },
                            {
                                "target_lang_word": "засушливый",
                                "target_lang_sense_clarification": "о погоде или периоде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "225774c9-d881-3ca6-943c-e1f50a739168",
                    "sense_definition": "O pieczywie: bez dodatków takich jak masło, dżem czy wędlina; jedzony bez niczego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na śniadanie zjadłem tylko <w>suchą</w> bułkę i wypiłem herbatę.",
                            "target_lang_translations": {
                                "ru": "На завтрак я съел только <w>пустую</w> булку и выпил чаю."
                            }
                        },
                        {
                            "text": "Nie lubię jeść <w>suchego</w> chleba, wolę go z masłem.",
                            "target_lang_translations": {
                                "ru": "Я не люблю есть <w>пустой</w> хлеб, предпочитаю с маслом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "suchy chleb",
                        "sucha bułka"
                    ],
                    "target_lang_definitions": {
                        "ru": "О хлебобулочных изделиях: без добавок, таких как масло, джем или ветчина; употребляемый без всего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пустой",
                                "target_lang_sense_clarification": "без добавок (разг.)"
                            },
                            {
                                "target_lang_word": "сухой",
                                "target_lang_sense_clarification": "без масла или жидкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e47b07eb-edef-32a1-9bb4-e287a06824e5",
                    "sense_definition": "O człowieku: bardzo szczupły, niemający tkanki tłuszczowej.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest wysoki i bardzo <w>suchy</w>, mimo że dużo je.",
                            "target_lang_translations": {
                                "ru": "Мой брат высокий и очень <w>сухощавый</w>, хотя много ест."
                            }
                        },
                        {
                            "text": "Ten chłopak jest tak <w>suchy</w>, że widać mu wszystkie żebra.",
                            "target_lang_translations": {
                                "ru": "Этот парень такой <w>худой</w>, что у него видны все рёбра."
                            }
                        }
                    ],
                    "synonyms": [
                        "chudy",
                        "kościsty"
                    ],
                    "antonyms": [
                        "gruby"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe thin people, sometimes pejoratively"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке: очень худой, не имеющий жировой ткани."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сухощавый",
                                "target_lang_sense_clarification": "стройный, жилистый"
                            },
                            {
                                "target_lang_word": "сухой",
                                "target_lang_sense_clarification": "худощавый"
                            },
                            {
                                "target_lang_word": "худой",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b1e5d22-a268-3721-874b-468784585b4d",
                    "sense_definition": "Pozbawiony emocji, uczuć, czysto rzeczowy lub nudny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor przedstawił nam tylko <w>suche</w> fakty dotyczące budżetu.",
                            "target_lang_translations": {
                                "ru": "Директор представил нам только <w>сухие</w> факты касательно бюджета."
                            }
                        },
                        {
                            "text": "Jego wykład był tak <w>suchy</w> i monotonny, że prawie zasnąłem.",
                            "target_lang_translations": {
                                "ru": "Его лекция была такой <w>сухой</w> и монотонной, что я почти уснул."
                            }
                        }
                    ],
                    "synonyms": [
                        "beznamiętny",
                        "drętwy"
                    ],
                    "common_phrases": [
                        "suchy fakt",
                        "suchy żart",
                        "suchy ton"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишённый эмоций, чувств, чисто деловой или скучный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сухой",
                                "target_lang_sense_clarification": "сдержанный, безэмоциональный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "404edfc9-6371-3622-93b8-2386ec48cc5e",
                    "sense_definition": "O dźwięku: krótki, ostry, przypominający łamanie czegoś twardego lub chrapliwy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ciszy nocnej rozległ się nagle <w>suchy</w> trzask łamanej gałęzi.",
                            "target_lang_translations": {
                                "ru": "В ночной тишине внезапно раздался <w>сухой</w> треск ломающейся ветки."
                            }
                        },
                        {
                            "text": "Męczy mnie <w>suchy</w> kaszel, który nie pozwala mi spać.",
                            "target_lang_translations": {
                                "ru": "Меня мучает <w>сухой</w> кашель, который не даёт мне спать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "suchy kaszel",
                        "suchy trzask"
                    ],
                    "target_lang_definitions": {
                        "ru": "О звуке: короткий, резкий, напоминающий ломание чего-то твёрдого или хриплый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сухой",
                                "target_lang_sense_clarification": "резкий, отрывистый"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "88e7d3c4-1963-388e-b52f-577173ceb226",
                    "sense_definition": "The act or process of casting a vote to make a choice or decision, especially in an election.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Electronic <w>voting</w> has made the process faster in many districts.",
                            "target_lang_translations": {
                                "ru": "Электронное <w>голосование</w> ускорило этот процесс во многих округах."
                            }
                        },
                        {
                            "text": "Public <w>voting</w> determines the winner of the reality show.",
                            "target_lang_translations": {
                                "ru": "<w>Голосование</w> публики определяет победителя реалити-шоу."
                            }
                        }
                    ],
                    "synonyms": [
                        "balloting",
                        "polling",
                        "election"
                    ],
                    "antonyms": [
                        "abstention"
                    ],
                    "common_phrases": [
                        "early voting",
                        "voting system",
                        "voting booth",
                        "public voting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс подачи голоса для принятия решения или выбора, особенно во время выборов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голосование",
                                "target_lang_sense_clarification": "процесс выбора"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6d4d2f5a-731b-3a3a-8669-673a9bd3907f",
                    "sense_definition": "Present participle and gerund of <w>vote</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They are <w>voting</w> for the new class president this afternoon.",
                            "target_lang_translations": {
                                "ru": "Сегодня днем они <w>голосуют</w> за нового старосту класса."
                            }
                        },
                        {
                            "text": "She spent the whole day <w>voting</w> on different committee proposals.",
                            "target_lang_translations": {
                                "ru": "Она провела весь день, <w>голосуя</w> по различным предложениям комитета."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола vote (голосовать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голосовать",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "голосование",
                                "target_lang_sense_clarification": "герундий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a558a37c-4e05-3846-84b0-27360f79cfea",
                    "sense_definition": "Possessing or conferring the legal right to cast a vote, often used regarding members of a group or financial shares.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Only <w>voting</w> members are allowed to participate in the final decision.",
                            "target_lang_translations": {
                                "ru": "Только члены <w>с правом голоса</w> допускаются к участию в принятии окончательного решения."
                            }
                        },
                        {
                            "text": "He bought <w>voting</w> stock to ensure he had a say in the company's future.",
                            "target_lang_translations": {
                                "ru": "Он купил <w>голосующие</w> акции, чтобы гарантировать себе право голоса в будущем компании."
                            }
                        }
                    ],
                    "antonyms": [
                        "nonvoting"
                    ],
                    "common_phrases": [
                        "voting rights",
                        "voting age",
                        "voting member",
                        "voting share"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий правом голоса или дающий такое право (часто используется в отношении членов группы или финансовых акций)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с правом голоса",
                                "target_lang_sense_clarification": "обладающий правом"
                            },
                            {
                                "target_lang_word": "голосующий",
                                "target_lang_sense_clarification": "дающий право голоса"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
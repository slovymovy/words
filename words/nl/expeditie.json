                {
                    "sense_id": "dc3a8e21-3b51-3ed2-a17a-116b5833238c",
                    "sense_definition": "Een militaire actie of veldtocht naar een ander gebied.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_journey",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De generaal leidde een militaire <w>expeditie</w> om het grensgebied te beveiligen.",
                            "target_lang_translations": {
                                "en": "The general led a military <w>expedition</w> to secure the border area.",
                                "ru": "Генерал возглавил военную <w>экспедицию</w> для обеспечения безопасности пограничной зоны."
                            }
                        },
                        {
                            "text": "In de geschiedenisboeken lazen we over de mislukte <w>expeditie</w> van het leger.",
                            "target_lang_translations": {
                                "en": "In the history books, we read about the army's failed <w>expedition</w>.",
                                "ru": "В учебниках истории мы читали о неудачном <w>походе</w> армии."
                            }
                        }
                    ],
                    "synonyms": [
                        "veldtocht",
                        "krijgstocht"
                    ],
                    "common_phrases": [
                        "strafexpeditie",
                        "militaire expeditie"
                    ],
                    "target_lang_definitions": {
                        "en": "A military action or campaign to another territory.",
                        "ru": "Военная операция или поход на другую территорию."
                    },

                {
                    "sense_id": "50729a73-b52e-3a15-8c83-379e4f38695d",
                    "sense_definition": "Rodzaj ciemnego, gazowanego napoju chłodzącego produkowanego przez koncern PepsiCo.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "Zawsze do obiadu zamawiam zimną <w>pepsi</w> z lodem.",
                            "target_lang_translations": {
                                "ru": "Я всегда заказываю к обеду холодную <w>пепси</w> со льдом."
                            }
                        },
                        {
                            "text": "Wolę pić <w>pepsi</w> niż zwykłą wodę gazowaną.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю пить <w>пепси</w>, чем обычную газировку."
                            }
                        }
                    ],
                    "synonyms": [
                        "pepsi-cola"
                    ],
                    "common_phrases": [
                        "zimna pepsi",
                        "puszka pepsi",
                        "szklanka pepsi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид темного газированного прохладительного напитка, производимого компанией PepsiCo."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пепси",
                                "target_lang_sense_clarification": "напиток"
                            },
                            {
                                "target_lang_word": "пепси-кола",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]

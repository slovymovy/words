                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft <w>al</w> het water opgedronken.",
                            "target_lang_translations": {
                                "ru": "Он выпил <w>всю</w> воду.",
                                "en": "He drank <w>all</w> the water."
                            }
                        },
                        {
                            "text": "Ze heeft <w>al</w> haar vrienden uitgenodigd voor het feest.",
                            "target_lang_translations": {
                                "ru": "Она пригласила <w>всех</w> своих друзей на вечеринку.",
                                "en": "She invited <w>all</w> her friends to the party."
                            }
                        },
                        {
                            "text": "Met <w>al</w> zijn macht probeerde hij de deur te openen.",
                            "target_lang_translations": {
                                "ru": "Со <w>всей</w> своей силой он пытался открыть дверь.",
                                "en": "He tried to open the door with <w>all</w> his might."
                            }
                        }
                    ],
                    "synonyms": [
                        "wel",
                        "ook al"
                    ],
                    "antonyms": [
                        "geen"
                    ],
                    "common_phrases": [
                        "al het",
                        "al mijn",
                        "al de"
                    ],
                    "target_lang_definitions": {
                        "ru": "Все количество или полное число чего-либо.",
                        "en": "The entire quantity or complete number of something."
                    },
                    "translations": {

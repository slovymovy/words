                },
                {
                    "sense_id": "9f2b5b1e-42a0-37d1-b990-69548e9d4bba",
                    "sense_definition": "Budynek, w którym mieści się placówka dla dzieci.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowe <w>przedszkole</w> zostało pomalowane na jasne, wesołe kolory.",

{
    "word_family": [
        "nawiedzenie",
        "nawiedzony"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2518c87-194f-3f88-aedb-ec99e338b74c",
                    "sense_definition": "Pojawiać się w jakimś miejscu lub ukazywać się komuś, budząc lęk (zwykle o duchach, zjawach lub siłach nadprzyrodzonych).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "supernatural_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miejscowi wierzą, że stary zamek <w>nawiedzają</w> duchy dawnych rycerzy.",
                            "target_lang_translations": {
                                "ru": "Местные верят, что в старом замке <w>обитают</w> духи древних рыцарей."
                            }
                        },
                        {
                            "text": "Opuszczony dom na wzgórzu podobno <w>nawiedza</w> tajemnicza zjawa.",
                            "target_lang_translations": {
                                "ru": "Говорят, что в заброшенном доме на холме <w>обитает</w> таинственный призрак."
                            }
                        }
                    ],
                    "synonyms": [
                        "prześladować",
                        "nękać",
                        "ogarniać"
                    ],
                    "common_phrases": [
                        "nawiedzać dom",
                        "duchy nawiedzają",
                        "nawiedzany przez duchy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появляться в каком-либо месте или показываться кому-либо, внушая страх (обычно о духах, призраках или сверхъестественных силах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обитать",
                                "target_lang_sense_clarification": "водиться (о привидениях)"
                            },
                            {
                                "target_lang_word": "являться",
                                "target_lang_sense_clarification": "приходить (о призраках)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71e51050-94ef-3fd5-b179-bb2878e8e5ff",
                    "sense_definition": "Uporczywie powracać w świadomości, nie dając spokoju (o myślach, wspomnieniach, snach lub uczuciach).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Koszmary senne zaczęły go <w>nawiedzać</w> każdej nocy po wypadku.",
                            "target_lang_translations": {
                                "ru": "Ночные кошмары начали <w>преследовать</w> его каждую ночь после аварии."
                            }
                        },
                        {
                            "text": "Ciemne myśli często <w>nawiedzały</w> ją, gdy zostawała sama w domu.",
                            "target_lang_translations": {
                                "ru": "Мрачные мысли часто <w>посещали</w> её, когда она оставалась дома одна."
                            }
                        }
                    ],
                    "synonyms": [
                        "nękać",
                        "prześladować",
                        "ogarniać"
                    ],
                    "common_phrases": [
                        "nawiedzają mnie sny",
                        "nawiedzają myśli",
                        "nawiedzać w snach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво возвращаться в сознании, не давая покоя (о мыслях, воспоминаниях, снах или чувствах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преследовать",
                                "target_lang_sense_clarification": "не давать покоя"
                            },
                            {
                                "target_lang_word": "посещать",
                                "target_lang_sense_clarification": "приходить на ум"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e527e60-7ae9-36d9-895c-13c709ec285a",
                    "sense_definition": "Występować na danym obszarze, powodując szkody, cierpienie lub zniszczenie (zwykle o klęskach żywiołowych, chorobach lub wojnach).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "disaster_occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ostatnich latach ten region często <w>nawiedzały</w> gwałtowne powodzie.",
                            "target_lang_translations": {
                                "ru": "В последние годы этот регион часто <w>постигали</w> сильные наводнения."
                            }
                        },
                        {
                            "text": "Kraj <w>nawiedziła</w> susza, która zniszczyła większość upraw rolniczych.",
                            "target_lang_translations": {
                                "ru": "Страну <w>поразила</w> засуха, уничтожившая большую часть сельскохозяйственных культур."
                            }
                        }
                    ],
                    "synonyms": [
                        "dotykać"
                    ],
                    "common_phrases": [
                        "nawiedzać kraj",
                        "klęska nawiedziła",
                        "nawiedzać region"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить на определенной территории, вызывая разрушения, страдания или уничтожение (обычно о стихийных бедствиях, болезнях или войнах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постигать",
                                "target_lang_sense_clarification": "случаться (о бедствиях)"
                            },
                            {
                                "target_lang_word": "поражать",
                                "target_lang_sense_clarification": "наносить удар"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa95bd04-feb3-3e85-b1dc-58a96ef3c2a7",
                    "sense_definition": "Przychodzić do kogoś lub do jakiegoś miejsca z wizytą, często w celu modlitwy lub oddania czci.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "visit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pielgrzymi licznie <w>nawiedzają</w> to sanktuarium w okresie świątecznym.",
                            "target_lang_translations": {
                                "ru": "Паломники в большом количестве <w>посещают</w> это святилище в праздничный период."
                            }
                        },
                        {
                            "text": "Wierni udali się na cmentarz, aby <w>nawiedzać</w> groby swoich bliskich.",
                            "target_lang_translations": {
                                "ru": "Верующие отправились на кладбище, чтобы <w>навестить</w> могилы своих близких."
                            }
                        }
                    ],
                    "synonyms": [
                        "odwiedzać"
                    ],
                    "common_phrases": [
                        "nawiedzać kościół",
                        "nawiedzać grób",
                        "nawiedzać sanktuarium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить к кому-либо или в какое-либо место с визитом, часто для молитвы или почитания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посещать",
                                "target_lang_sense_clarification": "приходить с визитом"
                            },
                            {
                                "target_lang_word": "навещать",
                                "target_lang_sense_clarification": "проведывать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "27e4cf6d-d1d8-3160-9c75-d706fefc94a2",
                    "sense_definition": "Wegstromen of naar een lager gelegen punt stromen, meestal gezegd van vloeistoffen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het regenwater kan via deze geul makkelijk <w>afvloeien</w>.",
                            "target_lang_translations": {
                                "ru": "Дождевая вода может легко <w>стекать</w> по этому желобу.",
                                "en": "The rainwater can easily <w>drain away</w> via this trench."
                            }
                        },
                        {
                            "text": "Zorg dat het overtollige water kan <w>afvloeien</w> naar de sloot.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что излишки воды могут <w>стекать</w> в канаву.",
                                "en": "Ensure that the excess water can <w>run off</w> into the ditch."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegstromen",
                        "weglopen"
                    ],
                    "antonyms": [
                        "toestromen"
                    ],
                    "common_phrases": [
                        "water laten afvloeien",
                        "snel afvloeien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утекать или течь в более низкое место, обычно говорится о жидкостях.",
                        "en": "To flow away or flow to a lower point, usually said of liquids."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стекать",
                                "target_lang_sense_clarification": "двигаться вниз (о жидкости)"
                            },
                            {
                                "target_lang_word": "утекать",
                                "target_lang_sense_clarification": "покидать место (о жидкости)"
                            },
                            {
                                "target_lang_word": "сливаться",
                                "target_lang_sense_clarification": "удаляться через сток"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "drain away",
                                "target_lang_sense_clarification": "liquids leaving an area"
                            },
                            {
                                "target_lang_word": "run off",
                                "target_lang_sense_clarification": "surface water flowing away"
                            },
                            {
                                "target_lang_word": "flow off",
                                "target_lang_sense_clarification": "general movement of liquid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "126e2029-fa1c-327a-ba34-1a91a757a8b8",
                    "sense_definition": "Het bedrijf moeten verlaten vanwege ontslag, vaak in het kader van een reorganisatie of inkrimping.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dismissal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de reorganisatie moesten honderd werknemers gedwongen <w>afvloeien</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за реорганизации сто сотрудников были вынуждены <w>уйти</w>.",
                                "en": "Due to the reorganization, a hundred employees were forced to <w>leave</w>."
                            }
                        },
                        {
                            "text": "Het bedrijf laat personeel via een sociaal plan <w>afvloeien</w>.",
                            "target_lang_translations": {
                                "ru": "Компания позволяет персоналу <w>уйти</w> по социальному плану.",
                                "en": "The company is letting staff <w>go</w> via a social plan."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontslagen worden",
                        "vertrekken"
                    ],
                    "antonyms": [
                        "aangenomen worden"
                    ],
                    "common_phrases": [
                        "natuurlijk afvloeien",
                        "gedwongen afvloeien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть вынужденным покинуть компанию из-за увольнения, часто в рамках реорганизации или сокращения штатов.",
                        "en": "To be forced to leave a company due to dismissal, often in the context of reorganization or downsizing."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попадать под сокращение",
                                "target_lang_sense_clarification": "быть уволенным из-за реорганизации"
                            },
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "покидать место работы (эвфемизм)"
                            },
                            {
                                "target_lang_word": "быть уволенным",
                                "target_lang_sense_clarification": "потерять работу"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "be laid off",
                                "target_lang_sense_clarification": "lose one's job due to reorganization"
                            },
                            {
                                "target_lang_word": "be made redundant",
                                "target_lang_sense_clarification": "UK term for job loss due to downsizing"
                            },
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "euphemism for losing a job"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "239bfb0c-3502-3670-aa15-78e883d0ec5f",
                    "sense_definition": "Natte inkt drogen door er vloeipapier op te drukken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij moest de brief voorzichtig <w>afvloeien</w> voordat hij hem in de envelop deed.",
                            "target_lang_translations": {
                                "ru": "Ему нужно было осторожно <w>промокнуть</w> письмо, прежде чем положить его в конверт.",
                                "en": "He had to carefully <w>blot</w> the letter before putting it in the envelope."
                            }
                        },
                        {
                            "text": "Vroeger leerden kinderen op school hoe ze hun schrijfwerk moesten <w>afvloeien</w>.",
                            "target_lang_translations": {
                                "ru": "Раньше дети в школе учились <w>промокать</w> свои письменные работы.",
                                "en": "In the past, children learned at school how to <w>blot</w> their writing."
                            }
                        }
                    ],
                    "synonyms": [
                        "afdeppen"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to the time when ink pens and blotting paper were common"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сушить мокрые чернила, прижимая к ним промокательную бумагу.",
                        "en": "To dry wet ink by pressing blotting paper onto it."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "промокать",
                                "target_lang_sense_clarification": "сушить чернила бумагой"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "blot",
                                "target_lang_sense_clarification": "dry ink with paper"
                            }
                        ]
                    }
                }
            ]
        }

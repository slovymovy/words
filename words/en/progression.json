                    ],
                    "target_lang_definitions": {
                        "ru": "Серия музыкальных аккордов, исполняемых в определенном порядке"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последовательность",
                                "target_lang_sense_clarification": "аккордов"
                            },
                            {
                                "target_lang_word": "прогрессия",
                                "target_lang_sense_clarification": "музыкальный термин"
                            }

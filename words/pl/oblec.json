    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9716a93c-9b58-3519-9bc3-c2fd89feba7c",
                    "sense_definition": "Otoczyć wojskiem umocnione miejsce (np. miasto, twierdzę) w celu odcięcia go od świata i zmuszenia obrońców do poddania się.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_military_surround",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wojska nieprzyjaciela postanowiły <w>oblec</w> stolicę przed nadejściem zimy.",
                            "target_lang_translations": {
                                "ru": "Войска неприятеля решили <w>осадить</w> столицу до наступления зимы."
                            }
                        },
                        {
                            "text": "Rycerze otrzymali rozkaz, aby szczelnie <w>oblec</w> zamek i nikogo nie wypuszczać.",
                            "target_lang_translations": {
                                "ru": "Рыцари получили приказ плотно <w>осадить</w> замок и никого не выпускать."
                            }
                        }
                    ],
                    "synonyms": [
                        "okrążyć",
                        "osaczyć"
                    ],
                    "antonyms": [
                        "odblokować",
                        "oswobodzić"
                    ],
                    "common_phrases": [
                        "oblec twierdzę",
                        "oblec miasto",
                        "oblec zamek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окружить войсками укрепленное место (например, город или крепость) с целью отрезать его от внешнего мира и принудить защитников к сдаче."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осадить",
                                "target_lang_sense_clarification": "город, крепость"
                            },
                            {
                                "target_lang_word": "окружить",
                                "target_lang_sense_clarification": "войсками"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dedcb9d2-8d06-3b24-8d98-98f41362f341",
                    "sense_definition": "Nałożyć powłoczkę lub poszewkę na pościel (kołdrę, poduszkę).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_cover_clothe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę szybko <w>oblec</w> świeżą pościel dla przyjeżdżających gości.",
                            "target_lang_translations": {
                                "ru": "Мне нужно быстро <w>застелить</w> свежее постельное белье для приезжающих гостей."
                            }
                        },
                        {
                            "text": "Pomóż mi proszę <w>oblec</w> kołdrę, bo sama sobie nie poradzę.",
                            "target_lang_translations": {
                                "ru": "Помоги мне, пожалуйста, <w>заправить</w> одеяло, так как я сама не справлюсь."
                            }
                        }
                    ],
                    "synonyms": [
                        "powlec"
                    ],
                    "antonyms": [
                        "rozebrać",
                        "zdjąć"
                    ],
                    "common_phrases": [
                        "oblec pościel",
                        "oblec kołdrę",
                        "oblec poduszki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надеть наволочку или пододеяльник на постельные принадлежности (одеяло, подушку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заправить",
                                "target_lang_sense_clarification": "одеяло, подушку"
                            },
                            {
                                "target_lang_word": "надеть",
                                "target_lang_sense_clarification": "наволочку, пододеяльник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73765c31-5ee4-3df0-8343-22a720398c85",
                    "sense_definition": "Zgromadzić się tłumnie wokół kogoś lub czegoś, nie dając możliwości przejścia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_military_surround",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fani zdołali <w>oblec</w> samochód gwiazdy, domagając się autografów.",
                            "target_lang_translations": {
                                "ru": "Фанаты сумели <w>обступить</w> машину звезды, требуя автографов."
                            }
                        },
                        {
                            "text": "Dziennikarze chcieli <w>oblec</w> polityka wychodzącego z budynku sejmu.",
                            "target_lang_translations": {
                                "ru": "Журналисты хотели <w>окружить</w> политика, выходящего из здания сейма."
                            }
                        }
                    ],
                    "synonyms": [
                        "obstąpić",
                        "otoczyć",
                        "osaczyć"
                    ],
                    "common_phrases": [
                        "tłum obległ",
                        "oblec gromadnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собраться толпой вокруг кого-то или чего-то, преграждая путь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обступить",
                                "target_lang_sense_clarification": "толпой"
                            },
                            {
                                "target_lang_word": "окружить",
                                "target_lang_sense_clarification": "взять в кольцо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3864e7f9-97ea-3272-baa7-f01fd7c03396",
                    "sense_definition": "Ubrać kogoś w strój, okryć czymś (często w kontekście uroczystym, podniosłym lub metaforycznym).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_cover_clothe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Służba pomogła królowi <w>oblec</w> ciężkie, ceremonialne szaty.",
                            "target_lang_translations": {
                                "ru": "Слуги помогли королю <w>надеть</w> тяжелые церемониальные одежды."
                            }
                        },
                        {
                            "text": "Artysta postanowił <w>oblec</w> rzeźbę w jedwabny materiał na czas transportu.",
                            "target_lang_translations": {
                                "ru": "Художник решил <w>облачить</w> скульптуру в шелковую ткань на время транспортировки."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyodziać",
                        "ubrać",
                        "okryć"
                    ],
                    "antonyms": [
                        "rozebrać",
                        "obnażyć"
                    ],
                    "common_phrases": [
                        "oblec w szaty",
                        "oblec w purpurę",
                        "oblec ciałem"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одеть кого-то в одежду или покрыть чем-то (часто в торжественном или метафорическом контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облачить",
                                "target_lang_sense_clarification": "торжественно одеть"
                            },
                            {
                                "target_lang_word": "надеть",
                                "target_lang_sense_clarification": "одежду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ae67d2e-8b60-3d74-9bd6-818564f3e4af",
                    "sense_definition": "Ubrać się, przywdziać na siebie strój (zwykle w formie zwrotnej: oblec się).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_cover_clothe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz musiał <w>oblec</w> się w zbroję przed nadchodzącą bitwą.",
                            "target_lang_translations": {
                                "ru": "Рыцарь должен был <w>облачиться</w> в доспехи перед грядущей битвой."
                            }
                        },
                        {
                            "text": "Na czas pokuty postanowił <w>oblec</w> się w skromny, lniany wór.",
                            "target_lang_translations": {
                                "ru": "На время покаяния он решил <w>облачиться</w> в скромное льняное рубище."
                            }
                        }
                    ],
                    "synonyms": [
                        "ubrać się",
                        "przyodziać się"
                    ],
                    "antonyms": [
                        "rozebrać się"
                    ],
                    "common_phrases": [
                        "oblec się w zbroję",
                        "oblec się w szaty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одеться, надеть на себя одежду (обычно используется с возвратным местоимением 'się')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облачиться",
                                "target_lang_sense_clarification": "одеться торжественно или в доспехи"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
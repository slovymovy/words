                    "sense_id": "ea4632a3-146b-3b85-92da-53c5ed97498f",
                    "sense_definition": "Taki, w którym znajduje się zbyt wielu ludzi lub rzeczy; zatłoczony.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten autobus jest tak <w>przepełniony</w>, że nie damy rady do niego wsiąść.",
                            "target_lang_translations": {
                                "ru": "Этот автобус так <w>переполнен</w>, что мы не сможем в него сесть."
                            }
                        },
                        {
                            "text": "W godzinach szczytu pociąg podmiejski był niemiłosiernie <w>przepełniony</w>.",
                            "target_lang_translations": {
                                "ru": "В час пик пригородный поезд был немилосердно <w>переполнен</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tłoczny",
                        "zatłoczony"
                    ],
                    "antonyms": [
                        "pusty",
                        "wyludniony"
                    ],
                    "common_phrases": [

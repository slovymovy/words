    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "403b5d02-e899-398d-a151-4c13210b40d3",
                    "sense_definition": "Wymienny element umieszczany wewnątrz innego przedmiotu, np. w długopisie, zniczu lub kominku, służący do jego działania.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić nowy niebieski <w>wkład</w> do mojego ulubionego długopisu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый синий <w>стержень</w> для моей любимой ручки."
                            }
                        },
                        {
                            "text": "Przed pójściem na cmentarz wymieniła wypalony <w>wkład</w> w zniczu.",
                            "target_lang_translations": {
                                "ru": "Перед походом на кладбище она заменила прогоревший <w>вкладыш</w> в лампаде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wkład do długopisu",
                        "wkład do znicza",
                        "wkład kominkowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сменный элемент, помещаемый внутрь другого предмета (например, ручки, лампады или камина) для обеспечения его работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стержень",
                                "target_lang_sense_clarification": "для ручки"
                            },
                            {
                                "target_lang_word": "вкладыш",
                                "target_lang_sense_clarification": "сменный элемент"
                            },
                            {
                                "target_lang_word": "топка",
                                "target_lang_sense_clarification": "для камина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c8d49a9-d21d-3ca9-8eb8-ce34064f5856",
                    "sense_definition": "To, co ktoś wnosi do wspólnej pracy, projektu lub osiągnięcia; osobisty udział w jakiejś sprawie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_contribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>wkład</w> w sukces tego projektu był naprawdę ogromny.",
                            "target_lang_translations": {
                                "ru": "Его <w>вклад</w> в успех этого проекта был действительно огромным."
                            }
                        },
                        {
                            "text": "Dziękujemy za twój duży <w>wkład</w> w rozwój naszej lokalnej społeczności.",
                            "target_lang_translations": {
                                "ru": "Благодарим за твой большой <w>вклад</w> в развитие нашего местного сообщества."
                            }
                        }
                    ],
                    "synonyms": [
                        "udział"
                    ],
                    "common_phrases": [
                        "wkład własny",
                        "wkład pracy",
                        "wkład w rozwój"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что кто-то вносит в общее дело, проект или достижение; личное участие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вклад",
                                "target_lang_sense_clarification": "участие в деле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bdb5fc6-608a-3323-8e1b-eaf210b1e80d",
                    "sense_definition": "Środki pieniężne lub majątkowe wniesione do banku, spółki lub przeznaczone na określony cel inwestycyjny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wspólnicy wnieśli do nowej spółki <w>wkład</w> o równej wartości.",
                            "target_lang_translations": {
                                "ru": "Партнёры внесли в новую компанию <w>вклад</w> равной стоимости."
                            }
                        },
                        {
                            "text": "Bank gwarantuje bezpieczeństwo każdego <w>wkładu</w> oszczędnościowego swoich klientów.",
                            "target_lang_translations": {
                                "ru": "Банк гарантирует безопасность каждого сберегательного <w>вклада</w> своих клиентов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wkład pieniężny",
                        "wkład bankowy",
                        "wkład niepieniężny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежные средства или имущество, внесённые в банк, компанию или предназначенные для инвестиций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вклад",
                                "target_lang_sense_clarification": "банковский депозит"
                            },
                            {
                                "target_lang_word": "взнос",
                                "target_lang_sense_clarification": "доля в капитале"
                            },
                            {
                                "target_lang_word": "вложение",
                                "target_lang_sense_clarification": "инвестиция"
                            }
                        ]
                    }
                }
            ]
        }
    ]

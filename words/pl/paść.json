    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d95ad8bd-15e5-3d6a-8c63-1ead10b3451f",
                    "sense_definition": "Przewrócić się na ziemię, stracić równowagę lub opaść na dół (o ludziach i przedmiotach).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fall_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczony maratończyk <w>padł</w> na trawę za linią mety.",
                            "target_lang_translations": {
                                "ru": "Уставший марафонец <w>упал</w> на траву за финишной чертой."
                            }
                        },
                        {
                            "text": "Wszystkie książki <w>padły</w> na podłogę z wielkim hukiem.",
                            "target_lang_translations": {
                                "ru": "Все книги <w>рухнули</w> на пол с громким грохотом."
                            }
                        }
                    ],
                    "synonyms": [
                        "upaść"
                    ],
                    "common_phrases": [
                        "paść na kolana",
                        "paść na twarz",
                        "paść na ziemię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упасть на землю, потерять равновесие или свалиться вниз (о людях и предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упасть",
                                "target_lang_sense_clarification": "на землю"
                            },
                            {
                                "target_lang_word": "рухнуть",
                                "target_lang_sense_clarification": "с шумом или тяжело"
                            },
                            {
                                "target_lang_word": "свалиться",
                                "target_lang_sense_clarification": "разг. упасть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adcd3028-6ec7-3338-8fda-d26d71a9e96b",
                    "sense_definition": "Przestać działać (o urządzeniach) lub umrzeć (o zwierzętach, potocznie o ludziach).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "die_break_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój stary laptop ostatecznie <w>padł</w> wczoraj wieczorem.",
                            "target_lang_translations": {
                                "ru": "Мой старый ноутбук окончательно <w>сдох</w> вчера вечером."
                            }
                        },
                        {
                            "text": "Bateria w telefonie <w>padła</w> w połowie rozmowy.",
                            "target_lang_translations": {
                                "ru": "Батарея в телефоне <w>села</w> посреди разговора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bateria padła",
                        "system padł",
                        "silnik padł"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "w odniesieniu do urządzeń lub ludzi"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать работать (о технике) или умереть (о животных, разг. о людях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдохнуть",
                                "target_lang_sense_clarification": "разг. о технике или животных"
                            },
                            {
                                "target_lang_word": "сесть",
                                "target_lang_sense_clarification": "о батарее"
                            },
                            {
                                "target_lang_word": "сломаться",
                                "target_lang_sense_clarification": "выйти из строя"
                            },
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "умереть (о скоте или высок. о героях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2cbb56c-9c05-31f8-a114-37ac86628aac",
                    "sense_definition": "Zostać wypowiedzianym, zadanym lub zgłoszonym (np. o pytaniu, propozycji, kwocie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "occur_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas dyskusji <w>padła</w> propozycja zmiany planów.",
                            "target_lang_translations": {
                                "ru": "Во время обсуждения <w>прозвучало</w> предложение изменить планы."
                            }
                        },
                        {
                            "text": "Na konferencji nie <w>padło</w> ani jedno pytanie o finanse.",
                            "target_lang_translations": {
                                "ru": "На конференции не <w>прозвучало</w> ни одного вопроса о финансах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "padło pytanie",
                        "padła propozycja",
                        "padła odpowiedź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть высказанным, заданным или предложенным (о вопросе, предложении, сумме)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозвучать",
                                "target_lang_sense_clarification": "быть произнесенным"
                            },
                            {
                                "target_lang_word": "поступить",
                                "target_lang_sense_clarification": "о предложении или вопросе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd49be08-bad0-33d1-b9c1-6b18a2031409",
                    "sense_definition": "Zostać ustanowionym lub osiągniętym (używane w kontekście rekordów i wygranych).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "occur_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wczorajszych zawodach <w>padł</w> nowy rekord świata.",
                            "target_lang_translations": {
                                "ru": "На вчерашних соревнованиях <w>был установлен</w> новый мировой рекорд."
                            }
                        },
                        {
                            "text": "W ostatnim losowaniu <w>padła</w> główna wygrana.",
                            "target_lang_translations": {
                                "ru": "В последнем тираже <w>выпал</w> главный выигрыш."
                            }
                        }
                    ],
                    "common_phrases": [
                        "padł rekord",
                        "padła wygrana",
                        "padł gol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть установленным или достигнутым (используется в контексте рекордов и выигрышей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть установленным",
                                "target_lang_sense_clarification": "о рекорде"
                            },
                            {
                                "target_lang_word": "выпасть",
                                "target_lang_sense_clarification": "о выигрыше"
                            },
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "быть побитым (о рекорде)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4657c31d-bcba-39d3-a8a7-2f1bbb6481a1",
                    "sense_definition": "Pilnować zwierząt gospodarskich jedzących trawę na pastwisku.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "graze_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary pasterz poszedł <w>paść</w> owce na górskiej hali.",
                            "target_lang_translations": {
                                "ru": "Старый пастух пошел <w>пасти</w> овец на горном пастбище."
                            }
                        },
                        {
                            "text": "Musimy <w>paść</w> krowy codziennie rano.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>пасти</w> коров каждое утро."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypasać"
                    ],
                    "common_phrases": [
                        "paść krowy",
                        "paść owce",
                        "paść gęsi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стеречь домашний скот, поедающий траву на пастбище."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пасти",
                                "target_lang_sense_clarification": "скот"
                            }
                        ]

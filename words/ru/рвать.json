            "senses": [
                {
                    "sense_id": "34d41720-4b86-3b68-b451-a6d5b8a978f4",
                    "sense_definition": "Разделять на части или нарушать целостность чего-либо резким движением",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Злой мальчик начал рвать письмо на мелкие кусочки.",
                            "target_lang_translations": {
                                "en": "The angry boy started to <w>tear</w> the letter into small pieces."
                            }
                        },
                        {
                            "text": "Пожалуйста, не надо рвать страницы этой старой книги.",
                            "target_lang_translations": {
                                "en": "Please do not <w>tear</w> the pages of this old book."
                            }
                        },
                        {
                            "text": "Собака может рвать зубами свою игрушку во время игры.",
                            "target_lang_translations": {
                                "en": "A dog can <w>tear</w> its toy with its teeth while playing."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрывать",
                        "раздирать",
                        "раздирать"
                    ],
                    "antonyms": [
                        "склеивать",
                        "соединять",
                        "строить"
                    ],
                    "common_phrases": [
                        "рвать бумагу",
                        "рвать одежду",
                        "рвать на куски",
                        "рвать на части"
                    ],
                    "target_lang_definitions": {
                        "en": "To pull something apart or into pieces with force"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tear",
                                "target_lang_sense_clarification": "general term for pulling apart"
                            },
                            {
                                "target_lang_word": "rip",
                                "target_lang_sense_clarification": "to tear quickly or violently"
                            },
                            {
                                "target_lang_word": "tear apart",
                                "target_lang_sense_clarification": "to destroy by tearing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ab6e34d-df58-3c9f-8786-2b17a5a82930",
                    "sense_definition": "Срывать, отделять от стебля или ветки (цветы, плоды, растения)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы пошли в лес рвать свежие ягоды для варенья.",
                            "target_lang_translations": {
                                "en": "We went to the forest to <w>pick</w> fresh berries for jam."
                            }
                        },
                        {
                            "text": "В этом городском парке строго запрещено рвать цветы.",
                            "target_lang_translations": {
                                "en": "It is strictly forbidden to <w>pick</w> flowers in this city park."
                            }
                        }
                    ],
                    "synonyms": [
                        "срывать",
                        "обрывать"
                    ],
                    "common_phrases": [
                        "рвать цветы",
                        "рвать ягоды",
                        "рвать яблоки",
                        "рвать траву"
                    ],
                    "target_lang_definitions": {
                        "en": "To detach flowers, fruit, or plants from their stems"
                    },
                    "translations": {
                        "en": [

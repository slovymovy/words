                {
                    "sense_id": "05259f5a-4275-3944-a49a-ed3c7859f75c",
                    "sense_definition": "Een verkorte benaming voor een biechtstoel.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De priester nam plaats in de <w>stoel</w> om de biecht te horen.",
                            "target_lang_translations": {
                                "en": "The priest took his place in the <w>confessional</w> to hear the confession.",
                                "ru": "Священник сел в <w>исповедальню</w>, чтобы выслушать исповедь."
                            }
                        },
                        {
                            "text": "Het was stil en donker rondom de oude houten <w>stoel</w> in de kerk.",
                            "target_lang_translations": {
                                "en": "It was quiet and dark around the old wooden <w>confessional</w> in the church.",
                                "ru": "Было тихо и темно вокруг старой деревянной <w>исповедальни</w> в церкви."
                            }
                        }
                    ],
                    "synonyms": [
                        "biechtstoel"
                    ],
                    "target_lang_definitions": {

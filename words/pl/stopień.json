    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "01fdb1df-5597-3c80-a4cc-3d9f48d746df",
                    "sense_definition": "Jednostka miary temperatury (np. w skali Celsjusza lub Fahrenheita).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "measurement_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzisiaj na zewnątrz jest minus pięć <w>stopni</w>.",
                            "target_lang_translations": {
                                "ru": "Сегодня на улице минус пять <w>градусов</w>."
                            }
                        },
                        {
                            "text": "Woda wrze, gdy osiągnie sto <w>stopni</w> Celsjusza.",
                            "target_lang_translations": {
                                "ru": "Вода кипит, когда достигает ста <w>градусов</w> Цельсия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stopni Celsjusza",
                        "dziesięć stopni",
                        "poniżej zera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения температуры (например, по шкале Цельсия или Фаренгейта)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "градус",
                                "target_lang_sense_clarification": "температуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cc88382-f9fc-39a5-8d3f-8543ee3e145a",
                    "sense_definition": "Pojedynczy element schodów, na którym stawia się stopę podczas wchodzenia lub schodzenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj na wysoki <w>stopień</w> przy wejściu do budynku.",
                            "target_lang_translations": {
                                "ru": "Осторожно, высокая <w>ступенька</w> при входе в здание."
                            }
                        },
                        {
                            "text": "Musisz wejść po kamiennych <w>stopniach</w>, aby dostać się na wieżę.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно подняться по каменным <w>ступеням</w>, чтобы попасть на башню."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wysoki stopień",
                        "uważać na stopień",
                        "siedzieć na stopniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный элемент лестницы, на который ставят ногу при подъеме или спуске."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ступень",
                                "target_lang_sense_clarification": "элемент лестницы"
                            },
                            {
                                "target_lang_word": "ступенька",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07e3dba4-17e4-3c1f-b144-94a404ea3d48",
                    "sense_definition": "Ocena postępów w nauce wystawiana uczniowi w szkole.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój syn przyniósł dzisiaj ze szkoły same dobre <w>stopnie</w>.",
                            "target_lang_translations": {
                                "ru": "Мой сын принес сегодня из школы одни хорошие <w>оценки</w>."
                            }
                        },
                        {
                            "text": "Nauczyciel wystawił uczniom końcowe <w>stopnie</w> z matematyki.",
                            "target_lang_translations": {
                                "ru": "Учитель выставил ученикам итоговые <w>оценки</w> по математике."
                            }
                        }
                    ],
                    "synonyms": [
                        "ocena"
                    ],
                    "common_phrases": [
                        "dobre stopnie",
                        "złe stopnie",
                        "dostać stopień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценка успеваемости, выставляемая ученику в школе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценка",
                                "target_lang_sense_clarification": "школьная"
                            },
                            {
                                "target_lang_word": "отметка",
                                "target_lang_sense_clarification": "балл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8df1b9bf-3d7a-3d3e-abed-04a6ecc1047d",
                    "sense_definition": "Poziom nasilenia, intensywności lub zasięgu jakiegoś zjawiska lub cechy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_level",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To rozwiązanie w znacznym <w>stopniu</w> ułatwi nam codzienną pracę.",
                            "target_lang_translations": {
                                "ru": "Это решение в значительной <w>степени</w> облегчит нам повседневную работу."
                            }
                        },
                        {
                            "text": "Nie wiem, w jakim <w>stopniu</w> mogę ci zaufać w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "Не знаю, в какой <w>степени</w> я могу тебе доверять в этом деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "poziom"
                    ],
                    "common_phrases": [
                        "w dużym stopniu",
                        "w pewnym stopniu",
                        "w znacznym stopniu",
                        "w najwyższym stopniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уровень интенсивности или масштаба какого-либо явления или признака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "степень",
                                "target_lang_sense_clarification": "мера, уровень"

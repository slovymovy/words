    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "68c0772d-137b-348d-9187-18c7b8b8012f",
                    "sense_definition": "Mieć z kimś kontakt, wiedzieć, kim ktoś jest, być komuś przedstawionym",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "relacje_osobowe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo dobrze <znam> twojego brata, chodziliśmy razem do szkoły.",
                            "target_lang_translations": {
                                "ru": "Я очень хорошо <знаю> твоего брата, мы вместе ходили в школу."
                            }
                        },
                        {
                            "text": "Czy <znasz> tego wysokiego mężczyznę, który stoi przy wejściu?",
                            "target_lang_translations": {
                                "ru": "Ты <знаешь> этого высокого мужчину, который стоит у входа?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "znać kogoś osobiście",
                        "znać kogoś z widzenia",
                        "dobrze kogoś znać",
                        "miło mi cię poznać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знать кого-то лично, иметь знакомство, быть представленным кому-то"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знать",
                                "target_lang_sense_clarification": "иметь сведения о ком-то, узнавать"
                            },
                            {
                                "target_lang_word": "быть знакомым",
                                "target_lang_sense_clarification": "состоять в знакомстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57959317-b0bf-3e83-9367-1813c47064cf",
                    "sense_definition": "Posiadać wiedzę na temat czegoś, być świadomym istnienia lub cech czegoś",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "wiedza_ogolna",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety nie <znam> odpowiedzi na to trudne pytanie egzaminacyjne.",
                            "target_lang_translations": {
                                "ru": "К сожалению, я не <знаю> ответа на этот трудный экзаменационный вопрос."
                            }
                        },
                        {
                            "text": "Musisz <znać> całą prawdę, nawet jeśli jest ona dla ciebie bolesna.",
                            "target_lang_translations": {
                                "ru": "Ты должен <знать> всю правду, даже если она для тебя болезненна."
                            }
                        }
                    ],
                    "synonyms": [
                        "wiedzieć",
                        "wiedzieć o"
                    ],
                    "common_phrases": [
                        "znać prawdę",
                        "znać odpowiedź",
                        "znać drogę",
                        "znać szczegóły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладать информацией или знаниями о чём-либо, быть осведомлённым"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знать",
                                "target_lang_sense_clarification": "обладать сведениями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31cfc671-2c14-3f60-8186-542f93346f33",
                    "sense_definition": "Posiadać umiejętność posługiwania się czymś, opanować jakąś dziedzinę wiedzy (np. język obcy)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "wiedza_ogolna",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy sąsiad biegle <zna> trzy języki obce, w tym japoński.",
                            "target_lang_translations": {
                                "ru": "Мой новый сосед свободно <знает> три иностранных языка, в том числе японский."
                            }
                        },
                        {
                            "text": "Jeśli <znasz> zasady gry w szachy, możemy rozegrać jedną partię.",
                            "target_lang_translations": {
                                "ru": "Если ты <знаешь> правила игры в шахматы, мы можем сыграть одну партию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "znać język obcy",
                        "znać zasady",
                        "znać nuty",
                        "znać swój fach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Владеть навыком, уметь пользоваться чем-либо или знать какую-либо область (например, язык)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знать",
                                "target_lang_sense_clarification": "владеть информацией или языком"
                            },
                            {
                                "target_lang_word": "владеть",
                                "target_lang_sense_clarification": "уметь пользоваться, знать в совершенстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39514e8c-a0a9-3e96-87a8-a319c26c55f9",
                    "sense_definition": "Utrzymywać ze sobą kontakty, być sobie przedstawionym (używane z zaimkiem zwrotnym „się”)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "relacje_osobowe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice <znają się> jeszcze z czasów studiów w Krakowie.",
                            "target_lang_translations": {
                                "ru": "Мои родители <знакомы> ещё со студенческих времён в Кракове."
                            }
                        },
                        {
                            "text": "My się chyba skądś <znamy>, bo twoja twarz wydaje mi się znajoma.",
                            "target_lang_translations": {
                                "ru": "Мы, кажется, откуда-то <знакомы>, потому что твоё лицо кажется мне знакомым."
                            }
                        }
                    ],
                    "common_phrases": [
                        "znać się od lat",
                        "znać się z widzenia",
                        "znać się jak łyse konie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть знакомыми друг с другом, поддерживать отношения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть знакомыми",
                                "target_lang_sense_clarification": "знать друг друга"
                            },
                            {
                                "target_lang_word": "знаться",
                                "target_lang_sense_clarification": "разг. поддерживать знакомство, общаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61537ba9-6d32-3d23-a056-0982daeb46bb",
                    "sense_definition": "Mieć dużą wiedzę i doświadczenie w określonej dziedzinie, być ekspertem (używane z „się na”)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "wiedza_ogolna",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój ojciec świetnie <zna się> na naprawie starych samochodów.",
                            "target_lang_translations": {
                                "ru": "Твой отец отлично <разбирается> в ремонте старых автомобилей."
                            }
                        },
                        {
                            "text": "Nie <znam się> na modzie, więc nie doradzę ci w wyborze sukienki.",
                            "target_lang_translations": {
                                "ru": "Я не <разбираюсь> в моде, поэтому не посоветую тебе в выборе платья."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyznawać się",
                        "mieć w małym palcu"
                    ],
                    "common_phrases": [
                        "znać się na rzeczy",
                        "znać się na ludziach",
                        "znać się na żartach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть компетентным, иметь опыт и понимание в определённой области"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбираться",
                                "target_lang_sense_clarification": "иметь знания и опыт в чем-либо"
                            },
                            {
                                "target_lang_word": "знать толк",
                                "target_lang_sense_clarification": "понимать суть, быть знатоком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1a65f1b-2627-36c0-9254-f1cbe9b291c7",
                    "sense_definition": "Być widocznym, możliwym do zauważenia po wyglądzie lub zachowaniu (używane w konstrukcjach nieosobowych)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "percepcja",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po jego bladej twarzy było <znać>, że jest bardzo zmęczony i niewyspany.",
                            "target_lang_translations": {
                                "ru": "По его бледному лицу было <видно>, что он очень устал и не выспался."
                            }
                        },
                        {
                            "text": "W tym starym, zabytkowym domu <znać> rękę prawdziwego mistrza architektury.",
                            "target_lang_translations": {
                                "ru": "В этом старом старинном доме <видна> рука настоящего мастера архитектуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "widać"
                    ],
                    "common_phrases": [
                        "znać po sobie",
                        "mało znać",
                        "dawać się znać"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane jako orzecznik (predykatyw), często w formie 'było znać'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть заметным, очевидным по внешним признакам (обычно в безличных конструкциях)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "видно",
                                "target_lang_sense_clarification": "можно заметить, очевидно"
                            },
                            {
                                "target_lang_word": "заметно",
                                "target_lang_sense_clarification": "проявляется внешне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cc05802-d28a-3c19-b90a-37862cd6c1bf",
                    "sense_definition": "Doświadczać czegoś w życiu, stykać się z czymś bezpośrednio (często z czymś negatywnym)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "doswiadczenie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten bezwzględny władca nie <znał> litości dla swoich pokonanych wrogów.",
                            "target_lang_translations": {
                                "ru": "Этот безжалостный правитель не <знал> жалости к своим побеждённым врагам."
                            }
                        },
                        {
                            "text": "W młodości dziadek <znał> biedę i głód, dlatego teraz tak bardzo szanuje jedzenie.",
                            "target_lang_translations": {
                                "ru": "В молодости дедушка <знал> нужду и голод, поэтому сейчас так сильно уважает еду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nie znać litości",
                        "nie znać granic",
                        "znać swoje miejsce",
                        "znać biedę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переживать что-либо на собственном опыте, сталкиваться с чем-либо (часто негативным)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знать",
                                "target_lang_sense_clarification": "испытывать на себе"
                            },
                            {
                                "target_lang_word": "ведать",
                                "target_lang_sense_clarification": "устар./книжн. знать, испытывать (часто в 'не ведать')"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
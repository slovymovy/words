    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0a1284d8-9a1c-3823-99cf-b65334297740",
                    "sense_definition": "Денежная стоимость товара или услуги, которую необходимо заплатить при покупке.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "monetary_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Какая <w>цена</w> у этого красивого платья?",
                            "target_lang_translations": {
                                "en": "What is the <w>price</w> of this beautiful dress?"
                            }
                        },
                        {
                            "text": "В этом магазине очень низкая <w>цена</w> на хлеб.",
                            "target_lang_translations": {
                                "en": "This store has a very low <w>price</w> for bread."
                            }
                        }
                    ],
                    "synonyms": [
                        "стоимость"
                    ],
                    "common_phrases": [
                        "высокая цена",
                        "низкая цена",
                        "цена билета",
                        "рыночная цена"
                    ],
                    "target_lang_definitions": {
                        "en": "The amount of money expected, required, or given in payment for something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "price",
                                "target_lang_sense_clarification": "monetary value"
                            },
                            {
                                "target_lang_word": "cost",
                                "target_lang_sense_clarification": "amount to be paid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "598bf3ad-a646-3da5-a89d-e7c060a55a98",
                    "sense_definition": "Усилия, жертвы или потери, необходимые для достижения какой-либо цели.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_cost",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спортсмен хотел получить золотую медаль любой <w>ценой</w>.",
                            "target_lang_translations": {
                                "en": "The athlete wanted to get the gold medal at any <w>cost</w>."
                            }
                        },
                        {
                            "text": "Победа в этой войне досталась стране дорогой <w>ценой</w>.",
                            "target_lang_translations": {
                                "en": "Victory in this war came to the country at a high <w>price</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "плата"
                    ],
                    "common_phrases": [
                        "любой ценой",
                        "дорогой ценой",
                        "ценой жизни",
                        "ценой усилий"
                    ],
                    "target_lang_definitions": {
                        "en": "The effort, loss, or sacrifice necessary to achieve or obtain something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cost",
                                "target_lang_sense_clarification": "effort or sacrifice"
                            },
                            {
                                "target_lang_word": "price",
                                "target_lang_sense_clarification": "figurative cost"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b3a2c83-7195-347f-9c55-98469e2d83f8",
                    "sense_definition": "Важность, значимость или ценность кого-либо или чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "significance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот опытный сотрудник прекрасно знает себе <w>цену</w>.",
                            "target_lang_translations": {
                                "en": "This experienced employee knows his <w>worth</w> perfectly well."
                            }
                        },
                        {
                            "text": "Твоей искренней поддержке в трудную минуту просто нет <w>цены</w>.",
                            "target_lang_translations": {
                                "en": "Your sincere support in a difficult moment is simply <w>priceless</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "значимость",
                        "ценность"
                    ],
                    "common_phrases": [
                        "знать себе цену",
                        "нет цены"
                    ],
                    "target_lang_definitions": {
                        "en": "The importance, significance, or usefulness of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "worth",
                                "target_lang_sense_clarification": "inherent value or merit"
                            },
                            {
                                "target_lang_word": "value",
                                "target_lang_sense_clarification": "importance or significance"
                            }
                        ]
                    }
                }
            ]
        }

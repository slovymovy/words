{
    "word_family": [
        "attendance",
        "attendant",
        "attention",
        "attentive",
        "attentively",
        "attentiveness",
        "inattention",
        "inattentive"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "66d73048-51fd-3df2-9bbc-e42d9457e451",
                    "sense_definition": "To go to or be present at an event, meeting, or place.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "presence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "All students must <w>attend</w> classes regularly to pass the course.",
                            "target_lang_translations": {
                                "ru": "Все студенты должны регулярно <w>посещать</w> занятия, чтобы сдать курс."
                            }
                        },
                        {
                            "text": "I am unable to <w>attend</w> the meeting tomorrow due to a prior engagement.",
                            "target_lang_translations": {
                                "ru": "Я не смогу <w>присутствовать</w> на завтрашнем собрании из-за запланированной встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "go to",
                        "be present at",
                        "show up"
                    ],
                    "antonyms": [
                        "miss",
                        "skip"
                    ],
                    "common_phrases": [
                        "attend school",
                        "attend a meeting",
                        "attend a conference",
                        "attend church",
                        "attend an event"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить или присутствовать на мероприятии, встрече или в определенном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посещать",
                                "target_lang_sense_clarification": "регулярно приходить куда-либо (в школу, на занятия)"
                            },
                            {
                                "target_lang_word": "присутствовать",
                                "target_lang_sense_clarification": "находиться в определенном месте в данное время"
                            },
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "разг. регулярно бывать где-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d491eb7-2db9-3311-a99c-9f31dcecf99a",
                    "sense_definition": "To deal with, handle, or take care of a task, problem, or person (usually followed by 'to').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have some urgent business to <w>attend</w> to before I can go home.",
                            "target_lang_translations": {
                                "ru": "У меня есть срочные дела, которыми нужно <w>заняться</w>, прежде чем я смогу пойти домой."
                            }
                        },
                        {
                            "text": "Please wait a moment while I <w>attend</w> to this customer.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подождите минуту, пока я <w>займусь</w> этим клиентом."
                            }
                        }
                    ],
                    "synonyms": [
                        "deal with",
                        "handle",
                        "see to",
                        "take care of"
                    ],
                    "antonyms": [
                        "neglect",
                        "ignore"
                    ],
                    "common_phrases": [
                        "attend to business",
                        "attend to details",
                        "attend to a matter",
                        "attend to needs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заниматься чем-либо, решать задачу или уделять время кому-либо (обычно с предлогом 'to')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заниматься",
                                "target_lang_sense_clarification": "делать что-либо, работать над задачей"
                            },
                            {
                                "target_lang_word": "уделять внимание",
                                "target_lang_sense_clarification": "тратить время на кого-либо или что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2362be8-d059-3c58-8877-77b5a241200c",
                    "sense_definition": "To listen carefully or give one's focus to something (often followed by 'to').",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>attend</w> carefully to the instructions given by the safety officer.",
                            "target_lang_translations": {
                                "ru": "Вам следует внимательно <w>прислушаться</w> к инструкциям сотрудника по безопасности."
                            }
                        },
                        {
                            "text": "He failed to <w>attend</w> to the warnings and made a serious mistake.",
                            "target_lang_translations": {
                                "ru": "Он не <w>уделил внимания</w> предупреждениям и совершил серьезную ошибку."
                            }
                        }
                    ],
                    "synonyms": [
                        "pay attention",
                        "listen",
                        "heed",
                        "note"
                    ],
                    "antonyms": [
                        "disregard",
                        "ignore"
                    ],
                    "common_phrases": [
                        "attend to instructions",
                        "attend carefully"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внимательно слушать или сосредотачиваться на чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уделять внимание",
                                "target_lang_sense_clarification": "фокусироваться на чем-либо"
                            },
                            {
                                "target_lang_word": "прислушиваться",
                                "target_lang_sense_clarification": "принимать во внимание сказанное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f9ddb24-af0c-3918-b3d7-e074e40f6b44",
                    "sense_definition": "To look after or provide professional service to someone, such as a patient or a person of high rank.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "service",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor <w>attended</w> the sick child throughout the night.",
                            "target_lang_translations": {
                                "ru": "Врач <w>наблюдал</w> больного ребенка всю ночь."
                            }
                        },
                        {
                            "text": "Several servants <w>attended</w> the queen during the ceremony.",
                            "target_lang_translations": {
                                "ru": "Несколько слуг <w>сопровождали</w> королеву во время церемонии."
                            }
                        }
                    ],
                    "synonyms": [
                        "tend",
                        "care for",
                        "look after",
                        "wait on",
                        "serve"
                    ],
                    "antonyms": [
                        "neglect"
                    ],
                    "common_phrases": [
                        "attend a patient",
                        "attend the king"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ухаживать за кем-либо или оказывать профессиональные услуги (пациенту, важному лицу)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухаживать",
                                "target_lang_sense_clarification": "заботиться о больном или немощном"
                            },
                            {
                                "target_lang_word": "сопровождать",
                                "target_lang_sense_clarification": "находиться рядом для оказания услуг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4169887-b923-333f-ae0f-a607648c8d31",
                    "sense_definition": "To happen at the same time as something else or be a result of it.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Great risks often <w>attend</w> such dangerous expeditions into the unknown.",
                            "target_lang_translations": {
                                "ru": "Огромные риски часто <w>сопутствуют</w> таким опасным экспедициям в неизведанное."
                            }
                        },
                        {
                            "text": "Confusion and panic <w>attended</w> the sudden announcement of the closure.",
                            "target_lang_translations": {
                                "ru": "Замешательство и паника <w>сопровождали</w> внезапное объявление о закрытии."
                            }
                        }
                    ],
                    "synonyms": [
                        "accompany",
                        "be associated with"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить одновременно с чем-либо или быть следствием этого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопутствовать",
                                "target_lang_sense_clarification": "происходить вместе с чем-то"
                            },
                            {
                                "target_lang_word": "сопровождать",
                                "target_lang_sense_clarification": "идти вместе с каким-либо явлением"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
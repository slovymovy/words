    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "57f12d5a-c030-313c-93f7-dfb5138122ee",
                    "sense_definition": "Bawić się w sposób hałaśliwy, żywiołowy i beztroski.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "play_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy kota nie ma, myszy <w>harcują</w>.",
                            "target_lang_translations": {
                                "ru": "Кот из дома — мыши <w>в пляс</w>."
                            }
                        },
                        {
                            "text": "Dzieci przez cały dzień <w>harcowały</w> na placu zabaw, biegając i krzycząc.",
                            "target_lang_translations": {
                                "ru": "Дети весь день <w>резвились</w> на детской площадке, бегая и крича."
                            }
                        }
                    ],
                    "synonyms": [
                        "baraszkować",
                        "dokazywać",
                        "figlować",
                        "swawolić"
                    ],
                    "common_phrases": [
                        "myszy harcują",
                        "wesoło harcować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Играть шумно, оживленно и беззаботно; веселиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резвиться",
                                "target_lang_sense_clarification": "играть, бегать и шуметь (обычно о детях или животных)"
                            },
                            {
                                "target_lang_word": "шалить",
                                "target_lang_sense_clarification": "баловаться, проказничать"
                            },
                            {
                                "target_lang_word": "беситься",
                                "target_lang_sense_clarification": "вести себя очень шумно и буйно (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "358b24f4-f962-3490-8da2-4f1677d236fc",
                    "sense_definition": "Staczać pojedynki konne przed frontem wojsk przed rozpoczęciem głównej bitwy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military_history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerze wyjechali przed szereg, aby <w>harcować</w> z przeciwnikiem ku pokrzepieniu serc.",
                            "target_lang_translations": {
                                "ru": "Рыцари выехали перед строем, чтобы <w>сразиться</w> с противником для поднятия духа."
                            }
                        },
                        {
                            "text": "Lekka jazda zaczęła <w>harcować</w> na przedpolu, próbując sprowokować wroga do ataku.",
                            "target_lang_translations": {
                                "ru": "Лёгкая кавалерия начала <w>завязывать стычки</w> на предполье, пытаясь спровоцировать врага на атаку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "harcować na koniu",
                        "harcować przed bitwą"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "odnosi się do dawnych zwyczajów rycerskich"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Участвовать в конных поединках перед строем войск до начала главной битвы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гарцевать",
                                "target_lang_sense_clarification": "сражаться, биться на поединках (устар.)"
                            },
                            {
                                "target_lang_word": "участвовать в стычках",
                                "target_lang_sense_clarification": "вести мелкие бои"
                            }
                        ]
                    }
                }
            ]
        }
    ]

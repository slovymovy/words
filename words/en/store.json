{
    "word_family": [
        "storage",
        "storeroom",
        "storehouse",
        "storekeeper",
        "storefront"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5cd42737-9e0e-3773-8765-cfac65c3521c",
                    "sense_definition": "A place where items may be purchased; a shop.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to get some milk from the grocery <w>store</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить молока в продуктовом <w>магазине</w>."
                            }
                        },
                        {
                            "text": "The clothing <w>store</w> in the mall is having a big sale today.",
                            "target_lang_translations": {
                                "ru": "В <w>магазине</w> одежды в торговом центре сегодня большая распродажа."
                            }
                        }
                    ],
                    "synonyms": [
                        "shop",
                        "retail store",
                        "boutique"
                    ],
                    "common_phrases": [
                        "grocery store",
                        "department store",
                        "convenience store",
                        "hardware store",
                        "online store"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где можно купить товары; магазин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магазин",
                                "target_lang_sense_clarification": "торговое предприятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53038c35-dfb8-396f-a7a8-72f92852c21f",
                    "sense_definition": "A supply or quantity of something kept for future use.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We kept a large <w>store</w> of firewood to last through the winter.",
                            "target_lang_translations": {
                                "ru": "Мы держали большой <w>запас</w> дров, чтобы хватило на всю зиму."
                            }
                        },
                        {
                            "text": "He has a vast <w>store</w> of interesting stories to tell at parties.",
                            "target_lang_translations": {
                                "ru": "У него есть огромный <w>запас</w> интересных историй, которые он рассказывает на вечеринках."
                            }
                        }
                    ],
                    "synonyms": [
                        "stock",
                        "supply"
                    ],
                    "common_phrases": [
                        "in store",
                        "set store by",
                        "store of value"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запас или количество чего-либо, отложенное для будущего использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запас",
                                "target_lang_sense_clarification": "резерв"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa469545-2408-36df-b5ef-78b8aaae41d8",
                    "sense_definition": "A place where items are accumulated or routinely kept; a warehouse or storage room.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The equipment is kept in the <w>store</w> behind the main building.",
                            "target_lang_translations": {
                                "ru": "Оборудование хранится на <w>складе</w> за главным зданием."
                            }
                        },
                        {
                            "text": "This building used to be a <w>store</w> for old tires.",
                            "target_lang_translations": {
                                "ru": "Это здание раньше было <w>складом</w> для старых шин."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где накапливаются или хранятся предметы; склад или кладовая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склад",
                                "target_lang_sense_clarification": "помещение для хранения"
                            },
                            {
                                "target_lang_word": "хранилище",
                                "target_lang_sense_clarification": "место сбережения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e453864-c776-3731-a9bd-885ea3a44d39",
                    "sense_definition": "Memory or storage space in a computer system.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The computer's main <w>store</w> was upgraded to handle more data.",
                            "target_lang_translations": {
                                "ru": "Основная <w>память</w> компьютера была модернизирована для обработки большего объема данных."
                            }
                        },
                        {
                            "text": "Data is moved from the backing <w>store</w> to the processor for execution.",
                            "target_lang_translations": {
                                "ru": "Данные перемещаются из внешнего <w>хранилища</w> в процессор для выполнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "memory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Память или пространство для хранения данных в компьютерной системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "память",
                                "target_lang_sense_clarification": "компьютерная"
                            },
                            {
                                "target_lang_word": "хранилище",
                                "target_lang_sense_clarification": "данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15587fe3-706b-31bd-8ba2-230e6d97ca5c",
                    "sense_definition": "A great quantity or number; abundance.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king possessed a great <w>store</w> of gold and jewels.",
                            "target_lang_translations": {
                                "ru": "Король владел огромным <w>запасом</w> золота и драгоценностей."
                            }
                        },
                        {
                            "text": "She had a <w>store</w> of patience that seemed never to run out.",
                            "target_lang_translations": {
                                "ru": "У нее был такой <w>запас</w> терпения, который, казалось, никогда не иссякнет."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "literary or poetic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество; изобилие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запас",
                                "target_lang_sense_clarification": "большое количество"
                            },
                            {
                                "target_lang_word": "изобилие",
                                "target_lang_sense_clarification": "множество"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ecae4ed8-972c-382d-97c4-065def3e98f3",
                    "sense_definition": "To keep something in a specific place for future use.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>store</w> these books in the attic until we need them again.",
                            "target_lang_translations": {
                                "ru": "Я буду <w>хранить</w> эти книги на чердаке, пока они нам снова не понадобятся."
                            }
                        },
                        {
                            "text": "You should <w>store</w> the cookies in an airtight jar to keep them fresh.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>хранить</w> печенье в герметичной банке, чтобы оно оставалось свежим."
                            }
                        }
                    ],
                    "common_phrases": [
                        "store away",
                        "store safely",
                        "store properly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Держать что-либо в определенном месте для использования в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хранить",
                                "target_lang_sense_clarification": "беречь, содержать"
                            },
                            {
                                "target_lang_word": "складировать",
                                "target_lang_sense_clarification": "помещать на склад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b7d7d0a-d642-380b-b5c6-a94c5aaa5624",
                    "sense_definition": "To save data or information in a computer or electronic device.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hard drive <w>stores</w> all your documents and photos.",
                            "target_lang_translations": {
                                "ru": "Жесткий диск <w>хранит</w> все ваши документы и фотографии."
                            }
                        },
                        {
                            "text": "This application <w>stores</w> your password securely in the cloud.",
                            "target_lang_translations": {
                                "ru": "Это приложение надежно <w>хранит</w> ваш пароль в облаке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "store data",
                        "store information"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сохранять данные или информацию в компьютере или электронном устройстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хранить",
                                "target_lang_sense_clarification": "содержать информацию"
                            },
                            {
                                "target_lang_word": "сохранять",
                                "target_lang_sense_clarification": "записывать данные"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35c4dbe4-bc23-35a6-83dd-007e788b7b41",
                    "sense_definition": "To have the capacity to contain or hold something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new warehouse <w>stores</w> twice as much grain as the old one.",
                            "target_lang_translations": {
                                "ru": "Новый склад <w>вмещает</w> в два раза больше зерна, чем старый."
                            }
                        },
                        {
                            "text": "They sell special boxes that <w>store</w> twenty-four jars each.",
                            "target_lang_translations": {
                                "ru": "Они продают специальные коробки, которые <w>вмещают</w> по двадцать четыре банки каждая."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь способность вмещать или содержать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вмещать",
                                "target_lang_sense_clarification": "содержать в себе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
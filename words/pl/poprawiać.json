{
    "word_family": [
        "poprawa",
        "poprawny",
        "poprawka",
        "poprawiny",
        "poprawność"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ca583fa7-f738-31ac-b60e-a5f16b327370",
                    "sense_definition": "Czynić coś lepszym, bardziej wartościowym lub sprawniejszym; podnosić jakość czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta dobra wiadomość natychmiast <w>poprawiła</w> jej humor.",
                            "target_lang_translations": {
                                "ru": "Эта хорошая новость мгновенно <w>улучшила</w> её настроение."
                            }
                        },
                        {
                            "text": "Musimy stale <w>poprawiać</w> jakość naszych usług, aby klienci byli zadowoleni.",
                            "target_lang_translations": {
                                "ru": "Мы должны постоянно <w>улучшать</w> качество наших услуг, чтобы клиенты были довольны."
                            }
                        }
                    ],
                    "synonyms": [
                        "polepszać",
                        "ulepszać"
                    ],
                    "antonyms": [
                        "pogarszać"
                    ],
                    "common_phrases": [
                        "poprawiać humor",
                        "poprawiać jakość",
                        "poprawiać sytuację",
                        "poprawiać zdrowie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то лучше, более ценным или эффективным; повышать качество чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "улучшать",
                                "target_lang_sense_clarification": "качество, ситуацию"
                            },
                            {
                                "target_lang_word": "поправлять",
                                "target_lang_sense_clarification": "здоровье, материальное положение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94c3466c-4486-3cb1-9221-216b05c9ff09",
                    "sense_definition": "Sprawdzać coś i usuwać znalezione błędy, pomyłki lub nieprawidłowości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczycielka przez cały wieczór <w>poprawiała</w> wypracowania swoich uczniów.",
                            "target_lang_translations": {
                                "ru": "Учительница весь вечер <w>проверяла</w> сочинения своих учеников."
                            }
                        },
                        {
                            "text": "Autor musiał <w>poprawić</w> tekst artykułu przed jego ostateczną publikacją.",
                            "target_lang_translations": {
                                "ru": "Автор должен был <w>исправить</w> текст статьи перед её окончательной публикацией."
                            }
                        }
                    ],
                    "synonyms": [
                        "korygować"
                    ],
                    "common_phrases": [
                        "poprawiać błędy",
                        "poprawiać tekst",
                        "poprawiać dyktando"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверять что-то и устранять найденные ошибки, промахи или неточности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исправлять",
                                "target_lang_sense_clarification": "ошибки, недочеты"
                            },
                            {
                                "target_lang_word": "корректировать",
                                "target_lang_sense_clarification": "текст, данные"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f4bc181-8975-3feb-8e5a-99243807b6fc",
                    "sense_definition": "Zmieniać lekko ułożenie czegoś (np. ubrania, włosów, pozycji ciała), aby wyglądało porządniej lub było wygodniejsze.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyna nerwowo <w>poprawiała</w> włosy, patrząc w lustro.",
                            "target_lang_translations": {
                                "ru": "Девушка нервно <w>поправляла</w> волосы, глядя в зеркало."
                            }
                        },
                        {
                            "text": "Mężczyzna w garniturze <w>poprawił</w> krawat tuż przed wejściem na salę.",
                            "target_lang_translations": {
                                "ru": "Мужчина в костюме <w>поправил</w> галстук прямо перед входом в зал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "poprawiać włosy",
                        "poprawiać krawat",
                        "poprawiać okulary",
                        "poprawiać poduszkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слегка изменять положение чего-либо (например, одежды, волос, позы), чтобы выглядеть опрятнее или для удобства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поправлять",
                                "target_lang_sense_clarification": "одежду, прическу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1111ed67-c1b8-3e30-96ec-114ad0a41f3b",
                    "sense_definition": "Robić coś ponownie w celu uzyskania lepszego wyniku, np. ocenę w szkole lub wynik sportowy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli dostaniesz jedynkę, będziesz musiał <w>poprawiać</w> ten sprawdzian w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Если получишь единицу, тебе придётся <w>пересдавать</w> эту контрольную на следующей неделе."
                            }
                        },
                        {
                            "text": "Sportowiec ciężko trenował, aby <w>poprawić</w> swój rekord życiowy.",
                            "target_lang_translations": {
                                "ru": "Спортсмен упорно тренировался, чтобы <w>улучшить</w> свой личный рекорд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "poprawiać ocenę",
                        "poprawiać sprawdzian",
                        "poprawiać wynik",
                        "poprawiać rekord"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то заново с целью получения лучшего результата, например, оценки в школе или спортивного достижения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исправлять",
                                "target_lang_sense_clarification": "оценку"
                            },
                            {
                                "target_lang_word": "пересдавать",
                                "target_lang_sense_clarification": "экзамен, тест"
                            },
                            {
                                "target_lang_word": "улучшать",
                                "target_lang_sense_clarification": "результат, рекорд"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
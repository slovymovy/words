                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "насыщать",
                                "target_lang_sense_clarification": "утолять голод"
                            },
                            {
                                "target_lang_word": "наполнять",
                                "target_lang_sense_clarification": "желудок"
                            }
                        ]
                    }
                }
            ]
        },
        {

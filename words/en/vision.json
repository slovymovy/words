    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "095d81c5-ac12-3da4-8172-bffa929428bc",
                    "sense_definition": "The physical ability to see.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sight_ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor tested my <w>vision</w> and said I need to wear glasses.",
                            "target_lang_translations": {
                                "ru": "Врач проверил мое <w>зрение</w> и сказал, что мне нужно носить очки."
                            }
                        },
                        {
                            "text": "Cats have excellent night <w>vision</w>, allowing them to hunt in the dark.",
                            "target_lang_translations": {
                                "ru": "У кошек отличное ночное <w>зрение</w>, позволяющее им охотиться в темноте."
                            }
                        }
                    ],
                    "synonyms": [
                        "sight",
                        "eyesight"
                    ],
                    "antonyms": [
                        "blindness"
                    ],
                    "common_phrases": [
                        "blurred vision",
                        "peripheral vision",
                        "night vision",
                        "field of vision"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая способность видеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зре́ние",
                                "target_lang_sense_clarification": "способность видеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d892cf76-771a-3bb1-8850-3708387fc3a2",
                    "sense_definition": "An idea or mental image of something in the future, often used as a goal or plan.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mental_image_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The founder shared her <w>vision</w> for the company's growth over the next decade.",
                            "target_lang_translations": {
                                "ru": "Основательница поделилась своим <w>видением</w> роста компании на следующее десятилетие."
                            }
                        },
                        {
                            "text": "We need a clear <w>vision</w> of what we want to achieve before we start the project.",
                            "target_lang_translations": {
                                "ru": "Нам нужно четкое <w>представление</w> о том, чего мы хотим достичь, прежде чем начинать проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "dream",
                        "aspiration"
                    ],
                    "common_phrases": [
                        "vision for the future",
                        "strategic vision",
                        "global vision",
                        "clear vision"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идея или мысленный образ будущего, часто используемые как цель или план."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ви́дение",
                                "target_lang_sense_clarification": "взгляд на вещи, концепция"
                            },
                            {
                                "target_lang_word": "представле́ние",
                                "target_lang_sense_clarification": "понимание, образ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4a071a0-bb1c-3721-a40b-2db2fb18b099",
                    "sense_definition": "The ability to think about or plan the future with imagination or wisdom.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_image_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A great leader must have the <w>vision</w> to anticipate changes in the market.",
                            "target_lang_translations": {
                                "ru": "Великий лидер должен обладать <w>дальновидностью</w>, чтобы предвидеть изменения на рынке."
                            }
                        },
                        {
                            "text": "His lack of <w>vision</w> caused the project to fail in the long run.",
                            "target_lang_translations": {
                                "ru": "Его отсутствие <w>дальновидности</w> привело к провалу проекта в долгосрочной перспективе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "man of vision",
                        "lack of vision"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность думать о будущем или планировать его с воображением или мудростью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дальнови́дность",
                                "target_lang_sense_clarification": "способность предвидеть"
                            },
                            {
                                "target_lang_word": "прозорли́вость",
                                "target_lang_sense_clarification": "проницательность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5658bfff-1806-3252-997e-8ae464b2cdc2",
                    "sense_definition": "A religious or mystical experience of seeing a supernatural appearance.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "supernatural_experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The saint claimed to have had a <w>vision</w> of angels descending from the sky.",
                            "target_lang_translations": {
                                "ru": "Святой утверждал, что у него было <w>видение</w> ангелов, спускающихся с небес."
                            }
                        },
                        {
                            "text": "During his meditation, he experienced a powerful <w>vision</w> that changed his life.",
                            "target_lang_translations": {
                                "ru": "Во время медитации он испытал сильное <w>видение</w>, которое изменило его жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "apparition"
                    ],
                    "common_phrases": [
                        "have a vision",
                        "religious vision"
                    ],
                    "target_lang_definitions": {
                        "ru": "Религиозный или мистический опыт наблюдения сверхъестественного явления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виде́ние",
                                "target_lang_sense_clarification": "мистическое явление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6be293be-a683-30fa-b8dc-731f2ed85568",
                    "sense_definition": "Something imaginary that one thinks one sees; a hallucination.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "supernatural_experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Exhausted by the heat, the traveler saw a <w>vision</w> of water in the desert.",
                            "target_lang_translations": {
                                "ru": "Измученный жарой, путник увидел <w>мираж</w> воды в пустыне."
                            }
                        },
                        {
                            "text": "He realized that the ghost was just a <w>vision</w> created by his tired mind.",
                            "target_lang_translations": {
                                "ru": "Он понял, что призрак был всего лишь <w>видением</w>, созданным его уставшим разумом."
                            }
                        }
                    ],
                    "synonyms": [
                        "hallucination",
                        "mirage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то воображаемое, что человеку кажется видимым; галлюцинация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виде́ние",
                                "target_lang_sense_clarification": "призрак, плод воображения"
                            },
                            {
                                "target_lang_word": "мира́ж",
                                "target_lang_sense_clarification": "оптическое явление"
                            },
                            {
                                "target_lang_word": "галлюцина́ция",
                                "target_lang_sense_clarification": "обман чувств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4613ff99-d499-3127-bac6-6616c764b04a",
                    "sense_definition": "A person or sight of extraordinary beauty.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "beauty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was a <w>vision</w> in her elegant white wedding dress.",
                            "target_lang_translations": {
                                "ru": "Она была просто <w>загляденье</w> в своем элегантном белом свадебном платье."
                            }
                        },
                        {
                            "text": "The garden in full bloom was a <w>vision</w> of color and life.",
                            "target_lang_translations": {
                                "ru": "Цветущий сад был <w>дивом</w> красок и жизни."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vision of beauty",
                        "vision of loveliness"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in a complimentary or literary context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или зрелище необычайной красоты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загляде́нье",
                                "target_lang_sense_clarification": "разг. о ком-то очень красивом"
                            },
                            {
                                "target_lang_word": "ди́вное зре́лище",
                                "target_lang_sense_clarification": "красивый вид"
                            },
                            {
                                "target_lang_word": "виде́ние",
                                "target_lang_sense_clarification": "поэт. неземной образ"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "162a6689-2677-36df-b46c-116fd48da359",
                    "sense_definition": "To imagine or picture something in the mind.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_imagine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>visioned</w> a world where everyone lived in peace and harmony.",
                            "target_lang_translations": {
                                "ru": "Она <w>представляла</w> себе мир, где все жили в мире и гармонии."
                            }
                        },
                        {
                            "text": "It is difficult to <w>vision</w> the scale of the universe without seeing pictures.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>вообразить</w> масштабы вселенной, не видя фотографий."
                            }
                        }
                    ],
                    "synonyms": [
                        "envision"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "less common than 'envision' or 'visualize'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Воображать или представлять что-либо в уме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представля́ть себе",
                                "target_lang_sense_clarification": "рисовать в воображении"
                            },
                            {
                                "target_lang_word": "вообража́ть",
                                "target_lang_sense_clarification": "фантазировать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
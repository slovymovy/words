{
    "word_family": [
        "niszczyć",
        "zniszczenie",
        "niszczyciel",
        "niezniszczalny",
        "wyniszczyć"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e94a7785-a8da-3f4d-ae7e-12346da40ba4",
                    "sense_definition": "Spowodować, że coś przestaje istnieć, traci swoje właściwości lub nie nadaje się do dalszego użytku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "destroy_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pożar doszczętnie <w>zniszczył</w> stary, drewniany budynek na skraju miasta.",
                            "target_lang_translations": {
                                "ru": "Пожар дотла <w>уничтожил</w> старое деревянное здание на окраине города."
                            }
                        },
                        {
                            "text": "Nie pozwól, aby jedno głupie nieporozumienie <w>zniszczyło</w> twoją karierę.",
                            "target_lang_translations": {
                                "ru": "Не позволь, чтобы одно глупое недоразумение <w>разрушило</w> твою карьеру."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrujnować",
                        "zdewastować",
                        "unicestwić",
                        "popsuć"
                    ],
                    "antonyms": [
                        "naprawić",
                        "odbudować",
                        "ocalić",
                        "stworzyć"
                    ],
                    "common_phrases": [
                        "doszczętnie zniszczyć",
                        "zniszczyć życie",
                        "zniszczyć dowody",
                        "zniszczyć mienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы что-то перестало существовать, потеряло свои свойства или стало непригодным для использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уничтожить",
                                "target_lang_sense_clarification": "полностью истребить, прекратить существование чего-либо"
                            },
                            {
                                "target_lang_word": "разрушить",
                                "target_lang_sense_clarification": "повредить структуру, сломать (здание, планы, жизнь)"
                            },
                            {
                                "target_lang_word": "испортить",
                                "target_lang_sense_clarification": "привести в негодность, ухудшить состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa0bb2a0-b656-34b0-9afd-c5aa6bbf738a",
                    "sense_definition": "Ulec uszkodzeniu lub zużyciu, stać się niezdatnym do użytku (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "destroy_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje ulubione buty szybko się <w>zniszczyły</w> podczas chodzenia po górach.",
                            "target_lang_translations": {
                                "ru": "Мои любимые ботинки быстро <w>износились</w> во время ходьбы по горам."
                            }
                        },
                        {
                            "text": "Delikatny materiał <w>zniszczył</w> się w praniu, ponieważ temperatura wody była zbyt wysoka.",
                            "target_lang_translations": {
                                "ru": "Деликатная ткань <w>испортилась</w> при стирке, так как температура воды была слишком высокой."
                            }
                        }
                    ],
                    "synonyms": [
                        "zużyć się",
                        "popsuć się"
                    ],
                    "common_phrases": [
                        "zniszczyć się w praniu",
                        "szybko się zniszczyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прийти в негодность, повредиться или износиться (обычно используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испортиться",
                                "target_lang_sense_clarification": "стать непригодным для использования, повредиться"
                            },
                            {
                                "target_lang_word": "износиться",
                                "target_lang_sense_clarification": "стать негодным от длительного употребления (об одежде, обуви)"
                            },
                            {
                                "target_lang_word": "разрушиться",
                                "target_lang_sense_clarification": "развалиться, прийти в упадок"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
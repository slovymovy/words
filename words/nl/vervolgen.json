{
    "word_family": [
        "vervolg",
        "vervolging",
        "vervolgens",
        "vervolger"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7bd6b858-31c8-31b3-a554-0ce18b0962f6",
                    "sense_definition": "Verdergaan met een handeling of activiteit, vaak na een korte onderbreking.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "continue_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de lunchpauze zullen we de vergadering <w>vervolgen</w>.",
                            "target_lang_translations": {
                                "en": "After the lunch break, we will <w>continue</w> the meeting.",
                                "ru": "После обеденного перерыва мы <w>продолжим</w> собрание."
                            }
                        },
                        {
                            "text": "De reiziger besloot zijn weg te <w>vervolgen</w> richting het zuiden.",
                            "target_lang_translations": {
                                "en": "The traveler decided to <w>continue</w> his journey towards the south.",
                                "ru": "Путешественник решил <w>продолжить</w> свой путь на юг."
                            }
                        }
                    ],
                    "synonyms": [
                        "voortzetten",
                        "continueren",
                        "doorgaan"
                    ],
                    "antonyms": [
                        "stoppen",
                        "beëindigen",
                        "staken"
                    ],
                    "common_phrases": [
                        "zijn weg vervolgen",
                        "een studie vervolgen",
                        "het gesprek vervolgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To proceed with an action or activity, often after a short interruption.",
                        "ru": "Продолжать действие или деятельность, часто после короткого перерыва."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "continue",
                                "target_lang_sense_clarification": "to carry on with an activity"
                            },
                            {
                                "target_lang_word": "resume",
                                "target_lang_sense_clarification": "to start again after a pause"
                            },
                            {
                                "target_lang_word": "proceed",
                                "target_lang_sense_clarification": "to move forward, often formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "продолжать",
                                "target_lang_sense_clarification": "делать дальше"
                            },
                            {
                                "target_lang_word": "возобновлять",
                                "target_lang_sense_clarification": "после перерыва"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dae94048-0d3c-31bf-b7f4-4788866aab3d",
                    "sense_definition": "Iemand voor de rechter brengen om hem of haar te laten bestraffen voor een misdrijf.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het openbaar ministerie zal de verdachte strafrechtelijk <w>vervolgen</w>.",
                            "target_lang_translations": {
                                "en": "The Public Prosecution Service will <w>prosecute</w> the suspect.",
                                "ru": "Прокуратура будет <w>преследовать</w> подозреваемого в уголовном порядке."
                            }
                        },
                        {
                            "text": "Winkeldieven worden in deze stad altijd <w>vervolgd</w>.",
                            "target_lang_translations": {
                                "en": "Shoplifters are always <w>prosecuted</w> in this city.",
                                "ru": "Магазинные воры в этом городе всегда <w>преследуются</w> по закону."
                            }
                        }
                    ],
                    "synonyms": [
                        "berechten",
                        "aanklagen"
                    ],
                    "antonyms": [
                        "vrijspreken",
                        "seponeren"
                    ],
                    "common_phrases": [
                        "strafrechtelijk vervolgen",
                        "besluiten te vervolgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring someone to court to have them punished for a crime.",
                        "ru": "Подвергать судебному преследованию, привлекать к ответственности за преступление."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prosecute",
                                "target_lang_sense_clarification": "legal action against someone"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "преследовать",
                                "target_lang_sense_clarification": "в судебном порядке"
                            },
                            {
                                "target_lang_word": "привлекать к ответственности",
                                "target_lang_sense_clarification": "юридически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d0f0bc8-5fba-3c1b-a7ee-2f8e5a2629b2",
                    "sense_definition": "Iemand stelselmatig vijandig behandelen, onderdrukken of opjagen, vaak vanwege geloof, ras of overtuiging.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "oppression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In dat land worden mensen <w>vervolgd</w> vanwege hun politieke ideeën.",
                            "target_lang_translations": {
                                "en": "In that country, people are <w>persecuted</w> because of their political ideas.",
                                "ru": "В этой стране людей <w>преследуют</w> за их политические взгляды."
                            }
                        },
                        {
                            "text": "De minderheidsgroep werd jarenlang <w>vervolgd</w> door het regime.",
                            "target_lang_translations": {
                                "en": "The minority group was <w>persecuted</w> by the regime for years.",
                                "ru": "Группа меньшинства годами <w>преследовалась</w> режимом."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderdrukken",
                        "treiteren"
                    ],
                    "antonyms": [
                        "beschermen",
                        "vrijlaten"
                    ],
                    "common_phrases": [
                        "vervolgd worden",
                        "politiek vervolgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To treat someone systematically in a hostile manner, oppress, or harass, often due to beliefs, race, or conviction.",
                        "ru": "Систематически угнетать или притеснять, часто по религиозным или политическим мотивам."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "persecute",
                                "target_lang_sense_clarification": "to oppress or harass systematically"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "преследовать",
                                "target_lang_sense_clarification": "угнетать"
                            },
                            {
                                "target_lang_word": "притеснять",
                                "target_lang_sense_clarification": "дискриминировать"
                            }
                        ]
                    }
                }
            ]
        },

            "senses": [
                {
                    "sense_id": "2d0767d5-aa75-3f93-93dd-7432120df7e6",
                    "sense_definition": "Een golfvormige beweging maken; op en neer bewegen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het graan <w>golfde</w> in de wind.",
                            "target_lang_translations": {
                                "en": "The grain <w>rippled</w> in the wind.",
                                "ru": "Зерно <w>волновалось</w> на ветру."
                            }
                        },
                        {
                            "text": "Haar lange haren <w>golfden</w> over haar schouders toen ze liep.",
                            "target_lang_translations": {
                                "en": "Her long hair <w>waved</w> over her shoulders as she walked.",
                                "ru": "Ее длинные волосы <w>струились</w> по плечам, когда она шла."
                            }
                        }
                    ],
                    "synonyms": [
                        "deinen"
                    ],
                    "common_phrases": [
                        "golvend haar",
                        "golvend landschap"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a wave-like movement; to move up and down.",
                        "ru": "Двигаться волнообразно; подниматься и опускаться."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "undulate",

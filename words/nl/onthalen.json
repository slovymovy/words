                        "ontvangen",
                        "fêteren"
                    ],
                    "antonyms": [
                        "wegsturen",
                        "negeren"
                    ],
                    "common_phrases": [
                        "warm onthalen",
                        "feestelijk onthalen",
                        "onthalen op"
                    ],
                    "target_lang_definitions": {

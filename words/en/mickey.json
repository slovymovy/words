                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения перемещения компьютерной мыши, представляющая наименьшее обнаруживаемое движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "микки",
                                "target_lang_sense_clarification": "единица измерения"
                            }
                        ]
                    }
                }

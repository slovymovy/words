                    }
                },
                {
                    "sense_id": "46e8a1bc-64cc-315d-8f19-4822c015259b",
                    "sense_definition": "The place where the most active, difficult, or practical work is happening in a job or situation, as opposed to management or theory.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After years in the <w>trenches</w> of local politics, she finally ran for mayor.",
                            "target_lang_translations": {
                                "ru": "После многих лет <w>на передовой</w> местной политики она наконец баллотировалась в мэры."
                            }
                        },
                        {
                            "text": "The manager visited the staff in the <w>trenches</w> to understand the real problems they faced.",
                            "target_lang_translations": {
                                "ru": "Менеджер посетил сотрудников <w>на местах</w>, чтобы понять реальные проблемы, с которыми они сталкиваются."
                            }
                        }
                    ],
                    "synonyms": [
                        "front line",
                        "coalface"
                    ],
                    "antonyms": [
                        "ivory tower"
                    ],
                    "common_phrases": [
                        "in the trenches",
                        "from the trenches"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или ситуация, где выполняется самая тяжелая, практическая работа, в отличие от руководства или теории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передовая",
                                "target_lang_sense_clarification": "место активных действий или борьбы"
                            },
                            {
                                "target_lang_word": "полевые условия",
                                "target_lang_sense_clarification": "непосредственная работа на месте"
                            }
                        ]
                    }
                }
            ]
        },
        {

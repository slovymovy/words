{
    "word_family": [
        "hebberig",
        "onhebbelijk",
        "hebbeding"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d644606b-93ef-370c-9e4d-fbd17ac59cb7",
                    "sense_definition": "In bezit hebben; eigenaar zijn van iets.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <heb> een nieuwe fiets gekregen voor mijn verjaardag.",
                            "target_lang_translations": {
                                "en": "I <have> received a new bike for my birthday."
                            }
                        },
                        {
                            "text": "Hebt u even tijd om mij te helpen?",
                            "target_lang_translations": {
                                "en": "Do you <have> a moment to help me?"
                            }
                        }
                    ],
                    "synonyms": [
                        "bezitten"
                    ],
                    "common_phrases": [
                        "tijd hebben",
                        "geld hebben",
                        "zin hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "To possess; to be the owner of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "to possess or own"
                            },
                            {
                                "target_lang_word": "own",
                                "target_lang_sense_clarification": "to have legal possession of"
                            },
                            {
                                "target_lang_word": "possess",
                                "target_lang_sense_clarification": "formal: to have or own"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59914ac9-f938-36a9-9c69-cc1d554f3c6b",
                    "sense_definition": "Hulpwerkwoord dat samen met een voltooid deelwoord de voltooide tijd vormt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "auxiliary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <heb> het koekje al opgegeten.",
                            "target_lang_translations": {
                                "en": "I <have> already eaten the cookie."
                            }
                        },
                        {
                            "text": "Zij <hadden> hun auto net gewassen toen het begon te regenen.",
                            "target_lang_translations": {
                                "en": "They <had> just washed their car when it started raining."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "hulpwerkwoord"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Auxiliary verb used with a past participle to form the perfect tense."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "auxiliary verb"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e2f2417-d029-39fb-94a1-7f36166d58fb",
                    "sense_definition": "Als onderdeel of eigenschap bevatten; bestaan uit.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "characteristics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een auto <heeft> vier wielen en een stuur.",
                            "target_lang_translations": {
                                "en": "A car <has> four wheels and a steering wheel."
                            }
                        },
                        {
                            "text": "Deze kamer <heeft> twee grote ramen.",
                            "target_lang_translations": {
                                "en": "This room <has> two large windows."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To contain as a part or feature; to consist of."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "to include or feature"
                            },
                            {
                                "target_lang_word": "contain",
                                "target_lang_sense_clarification": "to have within"
                            },
                            {
                                "target_lang_word": "feature",
                                "target_lang_sense_clarification": "to have as a distinct part"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d356a484-bbcb-3b25-96cd-1f1c37b36186",
                    "sense_definition": "In een familierelatie staan tot iemand.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "relationships",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <heeft> drie kinderen en twee kleinkinderen.",
                            "target_lang_translations": {
                                "en": "He <has> three children and two grandchildren."
                            }
                        },
                        {
                            "text": "Zij <heeft> een broer die in het buitenland woont.",
                            "target_lang_translations": {
                                "en": "She <has> a brother who lives abroad."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be in a family relationship with someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "to be related to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4fccf2ec-3fec-3b20-a59d-e210382d042c",
                    "sense_definition": "Lijden aan een ziekte, pijn of ongemak.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan niet komen werken want ik <heb> griep.",
                            "target_lang_translations": {
                                "en": "I cannot come to work because I <have> the flu."
                            }
                        },
                        {
                            "text": "Hij <heeft> vreselijke hoofdpijn na die lange vergadering.",
                            "target_lang_translations": {
                                "en": "He <has> a terrible headache after that long meeting."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pijn hebben",
                        "honger hebben",
                        "dorst hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "To suffer from an illness, pain, or discomfort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "to suffer from a condition"
                            },
                            {
                                "target_lang_word": "suffer from",
                                "target_lang_sense_clarification": "to be affected by illness or pain"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8b01006-ab63-3510-ba30-9db49d33f5da",
                    "sense_definition": "Spreken over een bepaald onderwerp.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "topic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waar <heeft> u het eigenlijk over?",
                            "target_lang_translations": {
                                "en": "What are you actually <talking> about?"
                            }
                        },
                        {
                            "text": "We <hebben> het over de plannen voor de vakantie.",
                            "target_lang_translations": {
                                "en": "We are <talking> about the plans for the holiday."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het hebben over"
                    ],
                    "target_lang_definitions": {
                        "en": "To speak about a specific subject."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "talk about",
                                "target_lang_sense_clarification": "to discuss a topic"
                            },
                            {
                                "target_lang_word": "discuss",
                                "target_lang_sense_clarification": "to talk about something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9ac074c-6c3a-370b-b250-80bcfd149f9f",
                    "sense_definition": "Verplicht zijn om iets te doen (in combinatie met 'te').",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Daar <hebben> we ons helaas bij neer te leggen.",
                            "target_lang_translations": {
                                "en": "Unfortunately, we <have> to resign ourselves to that."
                            }
                        },
                        {
                            "text": "Dat <heb> je maar te doen, of je wilt of niet.",
                            "target_lang_translations": {
                                "en": "You just <have> to do that, whether you want to or not."
                            }
                        }
                    ],
                    "synonyms": [
                        "moeten"
                    ],
                    "common_phrases": [
                        "te doen hebben",
                        "te maken hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "To be obliged to do something (used with 'te')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have to",
                                "target_lang_sense_clarification": "to be required to"
                            },
                            {
                                "target_lang_word": "must",
                                "target_lang_sense_clarification": "to be obliged to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd098f7f-2bbb-3251-8472-17fe44b30226",
                    "sense_definition": "Nut of voordeel ervaren van iets.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "benefit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Daar <heb> je in deze situatie helemaal niets aan.",
                            "target_lang_translations": {
                                "en": "You <have> no use for that in this situation."
                            }
                        },
                        {
                            "text": "Ik <heb> veel aan jouw advies gehad.",
                            "target_lang_translations": {
                                "en": "I <got> a lot out of your advice."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iets aan hebben",
                        "niets aan hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "To experience use or benefit from something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get out of",
                                "target_lang_sense_clarification": "to derive benefit from"
                            },
                            {
                                "target_lang_word": "benefit from",
                                "target_lang_sense_clarification": "to receive an advantage"
                            },
                            {
                                "target_lang_word": "be of use",
                                "target_lang_sense_clarification": "to be helpful"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3dd169b-deb1-31ab-919c-b0eced072227",
                    "sense_definition": "Kunnen verdragen of accepteren (meestal met 'kunnen').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tolerance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan zijn arrogante gedrag echt niet <hebben>.",
                            "target_lang_translations": {
                                "en": "I really cannot <stand> his arrogant behavior."
                            }
                        },
                        {
                            "text": "Die extra vertraging kunnen we er nu niet bij <hebben>.",
                            "target_lang_translations": {
                                "en": "We cannot <have> that extra delay right now."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niet kunnen hebben",
                        "erbij kunnen hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "To be able to endure or accept (usually with 'can')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand",
                                "target_lang_sense_clarification": "to tolerate or endure"
                            },
                            {
                                "target_lang_word": "tolerate",
                                "target_lang_sense_clarification": "to accept or endure"
                            },
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "to endure something unpleasant"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04af63d4-527a-3431-9a13-dd941213fd07",
                    "sense_definition": "In bedwang houden; gevangen hebben genomen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie <heeft> de verdachte eindelijk te pakken.",
                            "target_lang_translations": {
                                "en": "The police finally <have> the suspect."
                            }
                        },
                        {
                            "text": "De rebellen <hebben> de hoofdstad volledig in handen.",
                            "target_lang_translations": {
                                "en": "The rebels <have> the capital completely in their hands."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To control; to have captured."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have caught",
                                "target_lang_sense_clarification": "to have seized"
                            },
                            {
                                "target_lang_word": "have got",
                                "target_lang_sense_clarification": "to have in custody"
                            },
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "to keep in one's grasp"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb6bf7ec-26dc-3579-a4e2-b9f1b01edc67",
                    "sense_definition": "Het bezit; alle goederen die iemand heeft.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "possession_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn hele <hebben> en houden paste in één koffer.",
                            "target_lang_translations": {
                                "en": "His entire <possessions> fit in one suitcase."
                            }
                        },
                        {
                            "text": "Zij verloor haar hele <hebben> en houden door de brand.",
                            "target_lang_translations": {
                                "en": "She lost her entire <belongings> in the fire."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hebben en houden"
                    ],
                    "target_lang_definitions": {
                        "en": "Possessions; all goods that someone owns."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "possessions",
                                "target_lang_sense_clarification": "things that you own"
                            },
                            {
                                "target_lang_word": "belongings",
                                "target_lang_sense_clarification": "personal items"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
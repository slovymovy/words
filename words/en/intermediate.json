            "senses": [
                {
                    "sense_id": "e1e35c4f-7556-38cc-8fcc-10f5b305e8da",
                    "sense_definition": "Being between two extremes, or in the middle of a range.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "between_extremes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The course is designed for <w>intermediate</w> learners who want to advance.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "98827e5d-b703-3da3-9071-1e2b5d58f4c9",
                    "sense_definition": "Duży ptak drapieżny o zakrzywionym dziobie, mocnych szponach i szerokich skrzydłach, symbolizujący siłę i władzę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wysoko na niebie krążył potężny <w>orzeł</w>, wypatrując swojej zdobyczy.",
                            "target_lang_translations": {
                                "ru": "Высоко в небе кружил могучий <w>орёл</w>, высматривая свою добычу."
                            }
                        },
                        {
                            "text": "Ten gatunek <w>orła</w> buduje swoje gniazda na niedostępnych półkach skalnych.",
                            "target_lang_translations": {
                                "ru": "Этот вид <w>орла</w> строит свои гнёзда на недоступных скальных уступах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "orzeł bielik",
                        "orzeł przedni",
                        "wzrok orła",
                        "wolny jak orzeł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная хищная птица с изогнутым клювом, мощными когтями и широкими крыльями, символизирующая силу и власть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "орёл",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe284c9d-eb70-30a5-9a66-a661c18b5ca8",
                    "sense_definition": "Wizerunek orła będący godłem państwa, herbem miasta lub symbolem organizacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdej polskiej szkole na ścianie wisi godło przedstawiające białego <w>orła</w>.",
                            "target_lang_translations": {
                                "ru": "В каждой польской школе на стене висит герб, изображающий белого <w>орла</w>."
                            }
                        },
                        {
                            "text": "Żołnierze z dumą nosili na czapkach metalowego <w>orła</w> w koronie.",
                            "target_lang_translations": {
                                "ru": "Солдаты с гордостью носили на фуражках металлического <w>орла</w> в короне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "orzeł biały",
                        "orzeł w koronie",
                        "godło Polski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображение орла, являющееся государственным гербом, гербом города или символом организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "орёл",
                                "target_lang_sense_clarification": "символ, герб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be2e9c8f-8c8d-365b-9d96-618c78018d96",
                    "sense_definition": "Strona monety, na której znajduje się godło państwowe; przeciwieństwo reszki.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "coin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aby rozstrzygnąć spór, rzuciliśmy monetą, obstawiając <w>orła</w> lub reszkę.",
                            "target_lang_translations": {
                                "ru": "Чтобы разрешить спор, мы подбросили монету, загадав <w>орла</w> или решку."
                            }
                        },
                        {
                            "text": "Jeśli wypadnie <w>orzeł</w>, idziemy do kina, a jeśli reszka, zostajemy w domu.",
                            "target_lang_translations": {
                                "ru": "Если выпадет <w>орёл</w>, мы идём в кино, а если решка — остаёмся дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "awers"
                    ],
                    "antonyms": [
                        "reszka",
                        "rewers"
                    ],
                    "common_phrases": [
                        "orzeł czy reszka",
                        "rzucać orła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сторона монеты, на которой изображён государственный герб; противоположность решке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "орёл",
                                "target_lang_sense_clarification": "сторона монеты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "039c293f-7c40-3119-b629-6ee066861304",
                    "sense_definition": "Osoba bardzo zdolna, bystra i utalentowana w jakiejś dziedzinie (często używane ironicznie).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat to prawdziwy <w>orzeł</w> z matematyki, wygrywa wszystkie konkursy.",
                            "target_lang_translations": {
                                "ru": "Мой брат — настоящий <w>ас</w> в математике, он выигрывает все конкурсы."
                            }
                        },
                        {
                            "text": "Niestety nie jestem <w>orłem</w> z chemii i muszę brać dodatkowe korepetycje.",
                            "target_lang_translations": {
                                "ru": "К сожалению, я не <w>гений</w> в химии и мне приходится брать дополнительные уроки."
                            }
                        }
                    ],
                    "synonyms": [
                        "as",
                        "geniusz",
                        "prymus"
                    ],
                    "common_phrases": [
                        "orzeł z matematyki",
                        "być orłem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень способный, сообразительный и талантливый в какой-либо области человек (часто используется иронично)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ас",
                                "target_lang_sense_clarification": "мастер своего дела"
                            },
                            {
                                "target_lang_word": "гений",
                                "target_lang_sense_clarification": "одаренный человек"
                            },
                            {
                                "target_lang_word": "орёл",
                                "target_lang_sense_clarification": "разг., часто иронично"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "b73d11cd-6572-3b41-a268-1cc6393f7e96",
                    "sense_definition": "Polskie nazwisko.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Orzeł</w> został poproszony o podpisanie dokumentów w biurze.",
                            "target_lang_translations": {
                                "ru": "Господина <w>Ожела</w> попросили подписать документы в офисе."
                            }
                        },
                        {
                            "text": "W spisie lokatorów widnieje nazwisko państwa <w>Orłów</w>.",
                            "target_lang_translations": {
                                "ru": "В списке жильцов значится фамилия семьи <w>Ожел</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ожел",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd4f14bc-5f7f-3e6a-9efb-d07829f7a5ce",
                    "sense_definition": "Gwiazdozbiór nieba równikowego widoczny w Polsce latem, zawierający jasną gwiazdę Altair.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "constellation",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Podczas letniej nocy bez trudu odnaleźliśmy na niebie gwiazdozbiór <w>Orła</w>.",
                            "target_lang_translations": {
                                "ru": "Летней ночью мы без труда нашли на небе созвездие <w>Орла</w>."
                            }
                        },
                        {
                            "text": "Altair jest najjaśniejszą gwiazdą, która wchodzi w skład konstelacji <w>Orła</w>.",
                            "target_lang_translations": {
                                "ru": "Альтаир — самая яркая звезда, входящая в состав созвездия <w>Орла</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Aql"
                    ],
                    "common_phrases": [
                        "gwiazdozbiór Orła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Экваториальное созвездие, видимое в Польше летом, содержащее яркую звезду Альтаир."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Орёл",
                                "target_lang_sense_clarification": "созвездие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4df17e7-e4db-3077-a2ff-f1df1ea3685d",
                    "sense_definition": "Miasto w zachodniej części Rosji położone nad rzeką Oką.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "city",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Miasto <w>Orzeł</w> jest ważnym ośrodkiem przemysłowym w tym regionie.",
                            "target_lang_translations": {
                                "ru": "Город <w>Орёл</w> является важным промышленным центром в этом регионе."
                            }
                        },
                        {
                            "text": "Podczas podróży po Rosji zatrzymaliśmy się na krótki postój w <w>Orle</w>.",
                            "target_lang_translations": {
                                "ru": "Во время путешествия по России мы остановились на короткий отдых в <w>Орле</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в западной части России, расположенный на реке Оке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Орёл",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        }
    ]

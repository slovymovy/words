    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "afffa769-04c2-3123-b99c-9e1487258696",
                    "sense_definition": "To combine two or more substances or things together to form one mass or whole.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "combine_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>mix</w> the flour and sugar in a large bowl.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>смешайте</w> муку и сахар в большой миске."
                            }
                        },
                        {
                            "text": "Oil and water do not <w>mix</w> no matter how much you stir them.",
                            "target_lang_translations": {
                                "ru": "Масло и вода не <w>смешиваются</w>, сколько бы вы их ни мешали."
                            }
                        }
                    ],
                    "synonyms": [
                        "blend",
                        "combine",
                        "mingle",
                        "mix together",
                        "intermix"
                    ],
                    "antonyms": [
                        "unmix"
                    ],
                    "common_phrases": [
                        "mix well",
                        "mix together",
                        "mix ingredients"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединять два или более вещества или предмета вместе, образуя единую массу или целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смешивать",
                                "target_lang_sense_clarification": "соединять в одно целое"
                            },
                            {
                                "target_lang_word": "перемешивать",
                                "target_lang_sense_clarification": "мешать до однородности"
                            },
                            {
                                "target_lang_word": "мешать",
                                "target_lang_sense_clarification": "совершать круговые движения ложкой или инструментом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "089fb7e7-b063-37ab-a2ae-0cfdea3489c7",
                    "sense_definition": "To meet and talk with different people in a social setting.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He finds it difficult to <w>mix</w> with strangers at large parties.",
                            "target_lang_translations": {
                                "ru": "Ему трудно <w>общаться</w> с незнакомцами на больших вечеринках."
                            }
                        },
                        {
                            "text": "The host encouraged all the guests to <w>mix</w> and enjoy the evening.",
                            "target_lang_translations": {
                                "ru": "Хозяин призвал всех гостей <w>общаться</w> и наслаждаться вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "mingle"
                    ],
                    "common_phrases": [
                        "mix with people",
                        "mix and mingle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встречаться и разговаривать с разными людьми в социальной обстановке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общаться",
                                "target_lang_sense_clarification": "разговаривать, проводить время с людьми"
                            },
                            {
                                "target_lang_word": "смешиваться",
                                "target_lang_sense_clarification": "находиться среди других людей, быть частью толпы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e30590d7-d21b-36d3-9208-6a1f50ed9c63",
                    "sense_definition": "To combine different recorded sounds or tracks to create a final piece of music.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineer will <w>mix</w> the rhythm tracks down to a single track.",
                            "target_lang_translations": {
                                "ru": "Звукорежиссер <w>сведёт</w> ритм-треки в одну дорожку."
                            }
                        },
                        {
                            "text": "She spent the whole night <w>mixing</w> the new song in the studio.",
                            "target_lang_translations": {
                                "ru": "Она провела всю ночь, <w>сводя</w> новую песню в студии."
                            }
                        }
                    ],
                    "synonyms": [
                        "blend"
                    ],
                    "common_phrases": [
                        "mix a track",
                        "mix audio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединять различные записанные звуки или дорожки для создания окончательного музыкального произведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сводить",
                                "target_lang_sense_clarification": "профессиональный термин для объединения звуковых дорожек"
                            },
                            {
                                "target_lang_word": "микшировать",
                                "target_lang_sense_clarification": "технический процесс смешивания звука"
                            },
                            {
                                "target_lang_word": "миксовать",
                                "target_lang_sense_clarification": "разговорный вариант, часто используется диджеями"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "52e83a0b-9f25-3754-a29e-9de0e42722c4",
                    "sense_definition": "A combination of different things, people, or qualities.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "combine_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd at the concert was a <w>mix</w> of young and old people.",
                            "target_lang_translations": {
                                "ru": "Толпа на концерте представляла собой <w>смесь</w> молодых и пожилых людей."
                            }
                        },
                        {
                            "text": "We need the right <w>mix</w> of skills to complete this project successfully.",
                            "target_lang_translations": {
                                "ru": "Нам нужно правильное <w>сочетание</w> навыков, чтобы успешно завершить этот проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "blend"
                    ],
                    "common_phrases": [
                        "good mix",
                        "mix of things",
                        "right mix"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сочетание различных вещей, людей или качеств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смесь",
                                "target_lang_sense_clarification": "соединение разнородных элементов"
                            },
                            {
                                "target_lang_word": "сочетание",
                                "target_lang_sense_clarification": "гармоничное соединение качеств или свойств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7d02622-c496-3a45-834c-c2255f3c7fab",
                    "sense_definition": "A commercially prepared powder containing ingredients to make a specific food.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I bought a box of chocolate cake <w>mix</w> from the grocery store.",
                            "target_lang_translations": {
                                "ru": "Я купил коробку <w>смеси</w> для шоколадного торта в продуктовом магазине."
                            }
                        },
                        {
                            "text": "You just need to add water to the pancake <w>mix</w>.",
                            "target_lang_translations": {
                                "ru": "Вам просто нужно добавить воды в <w>смесь</w> для блинов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cake mix",
                        "pancake mix"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовая сухая смесь, содержащая ингредиенты для приготовления определенного блюда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смесь",
                                "target_lang_sense_clarification": "сухой полуфабрикат для выпечки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de0e907b-30a0-35e7-9271-8004949b236a",
                    "sense_definition": "The finished version of a musical recording produced by combining tracks.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have almost finished the final <w>mix</w> for this song.",
                            "target_lang_translations": {
                                "ru": "Я почти закончил финальный <w>микс</w> для этой песни."
                            }
                        },
                        {
                            "text": "The rhythm <w>mix</w> sounds a bit muddy and needs adjustment.",
                            "target_lang_translations": {
                                "ru": "Ритм-<w>микс</w> звучит немного грязновато и требует настройки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовая версия музыкальной записи, созданная путем объединения дорожек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "микс",
                                "target_lang_sense_clarification": "музыкальная композиция или набор треков"
                            },
                            {
                                "target_lang_word": "сведение",
                                "target_lang_sense_clarification": "результат процесса обработки звука"
                            }
                        ]
                    }
                }
            ]
        },

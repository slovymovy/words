    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d822d067-dabf-38c8-bcdc-90eed7627f9f",
                    "sense_definition": "Dingen of personen door elkaar halen en de ene voor de andere aanzien.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mistake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>verwar</w> de namen van mijn buren steeds met elkaar.",
                            "target_lang_translations": {
                                "en": "I keep <w>confusing</w> my neighbors' names with each other.",
                                "ru": "Я постоянно <w>путаю</w> имена моих соседей."
                            }
                        },
                        {
                            "text": "Dit medicijn is niet te <w>verwarren</w> met gewone pijnstillers.",
                            "target_lang_translations": {
                                "en": "This medicine is not to be <w>confused</w> with ordinary painkillers.",
                                "ru": "Это лекарство нельзя <w>спутать</w> с обычными обезболивающими."
                            }
                        }
                    ],
                    "synonyms": [
                        "door elkaar halen",
                        "verwisselen"
                    ],
                    "antonyms": [
                        "onderscheiden"
                    ],
                    "common_phrases": [
                        "niet te verwarren met",
                        "namen verwarren"
                    ],
                    "target_lang_definitions": {
                        "en": "To mistake one person or thing for another; to mix up.",
                        "ru": "Ошибочно принимать одно за другое; путать вещи или людей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "confuse",
                                "target_lang_sense_clarification": "mistake one for another"
                            },
                            {
                                "target_lang_word": "mix up",
                                "target_lang_sense_clarification": "interchange accidentally"
                            },
                            {
                                "target_lang_word": "mistake",
                                "target_lang_sense_clarification": "identify incorrectly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "путать",
                                "target_lang_sense_clarification": "принимать одно за другое"
                            },
                            {
                                "target_lang_word": "спутать",
                                "target_lang_sense_clarification": "совершенный вид; ошибиться в распознавании"
                            },
                            {
                                "target_lang_word": "перепутать",
                                "target_lang_sense_clarification": "поменять местами по ошибке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4743937e-57af-30e4-b1b0-3367ed6d6ae7",
                    "sense_definition": "Iemand onzeker maken of het denkvermogen tijdelijk verstoren; in de war brengen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "confuse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vele verkeersborden <w>verwarren</w> de automobilisten op dit drukke kruispunt.",
                            "target_lang_translations": {
                                "en": "The many traffic signs <w>confuse</w> the drivers at this busy intersection.",
                                "ru": "Множество дорожных знаков <w>сбивают с толку</w> водителей на этом оживленном перекрестке."
                            }
                        },
                        {
                            "text": "Je moet de jury niet <w>verwarren</w> met allerlei irrelevante details.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>confuse</w> the jury with all kinds of irrelevant details.",
                                "ru": "Не стоит <w>запутывать</w> присяжных всякими несущественными деталями."
                            }
                        }
                    ],
                    "synonyms": [
                        "in de war brengen",
                        "verbijsteren"
                    ],
                    "antonyms": [
                        "verduidelijken"
                    ],
                    "common_phrases": [
                        "iemand verwarren"
                    ],
                    "target_lang_definitions": {
                        "en": "To make someone feel uncertain or unable to think clearly.",
                        "ru": "Приводить кого-либо в замешательство, лишать уверенности или ясности мыслей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "confuse",
                                "target_lang_sense_clarification": "make uncertain"
                            },
                            {
                                "target_lang_word": "bewilder",
                                "target_lang_sense_clarification": "puzzle completely"
                            },
                            {
                                "target_lang_word": "perplex",
                                "target_lang_sense_clarification": "complicate or puzzle"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сбивать с толку",
                                "target_lang_sense_clarification": "дезориентировать"
                            },
                            {
                                "target_lang_word": "запутывать",
                                "target_lang_sense_clarification": "усложнять понимание"
                            },
                            {
                                "target_lang_word": "смущать",
                                "target_lang_sense_clarification": "приводить в замешательство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9782dffb-31b2-31f9-bde5-bebbb9e8f61a",
                    "sense_definition": "Draden, haren of andere lijnen in elkaar laten draaien zodat ze vast komen te zitten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tangle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De visnetten <w>verwarden</w> in de schroef van de kleine boot.",
                            "target_lang_translations": {
                                "en": "The fishing nets <w>got tangled</w> in the propeller of the small boat.",
                                "ru": "Рыболовные сети <w>запутались</w> в винте маленькой лодки."
                            }
                        },
                        {
                            "text": "Pas op dat de lijnen van de vlieger niet <w>verwarren</w> door de harde wind.",
                            "target_lang_translations": {
                                "en": "Watch out that the kite lines do not <w>tangle</w> due to the strong wind.",
                                "ru": "Следи, чтобы стропы воздушного змея не <w>спутались</w> из-за сильного ветра."
                            }
                        }
                    ],
                    "synonyms": [
                        "verstrikken",
                        "in de knoop raken"
                    ],
                    "antonyms": [
                        "ontwarren"
                    ],
                    "common_phrases": [
                        "verward raken"
                    ],
                    "target_lang_definitions": {
                        "en": "To twist threads, hair, or lines together so they become stuck.",
                        "ru": "Беспорядочно переплетать нити, волосы или провода."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tangle",
                                "target_lang_sense_clarification": "twist together"
                            },
                            {
                                "target_lang_word": "entangle",
                                "target_lang_sense_clarification": "catch in a snarl"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спутывать",
                                "target_lang_sense_clarification": "делать запутанным"
                            },
                            {
                                "target_lang_word": "запутывать",
                                "target_lang_sense_clarification": "приводить в беспорядок (о нитях)"
                            }
                        ]
                    }
                }
            ]
        }

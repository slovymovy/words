    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6fc46445-a088-30e7-a5ea-fc6106936d96",
                    "sense_definition": "A railway vehicle consisting of connected carriages or wagons that carries passengers or goods along a track.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>train</w> arrived at the station five minutes late.",
                            "target_lang_translations": {
                                "ru": "<w>Поезд</w> прибыл на станцию с пятиминутным опозданием."
                            }
                        },
                        {
                            "text": "We decided to travel to London by <w>train</w> instead of driving.",
                            "target_lang_translations": {
                                "ru": "Мы решили поехать в Лондон на <w>поезде</w>, а не на машине."
                            }
                        }
                    ],
                    "synonyms": [
                        "railway train"
                    ],
                    "common_phrases": [
                        "by train",
                        "catch a train",
                        "miss a train",
                        "get on the train",
                        "train station"
                    ],
                    "target_lang_definitions": {
                        "ru": "Железнодорожное транспортное средство, состоящее из соединенных вагонов, которое перевозит пассажиров или грузы по рельсам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поезд",
                                "target_lang_sense_clarification": "транспортное средство"
                            },
                            {
                                "target_lang_word": "состав",
                                "target_lang_sense_clarification": "группа вагонов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52bfe1c1-372d-340e-8ba0-d33af5710259",
                    "sense_definition": "A connected series of events, thoughts, or actions that follow one another.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I lost my <w>train</w> of thought when the telephone rang.",
                            "target_lang_translations": {
                                "ru": "Я потерял <w>ход</w> мыслей, когда зазвонил телефон."
                            }
                        },
                        {
                            "text": "The incident set in motion a tragic <w>train</w> of events.",
                            "target_lang_translations": {
                                "ru": "Этот инцидент запустил трагическую <w>череду</w> событий."
                            }
                        }
                    ],
                    "synonyms": [
                        "sequence",
                        "chain",
                        "series",
                        "progression"
                    ],
                    "common_phrases": [
                        "train of thought",
                        "train of events"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связная последовательность событий, мыслей или действий, следующих друг за другом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ход",
                                "target_lang_sense_clarification": "мыслей"
                            },
                            {
                                "target_lang_word": "череда",
                                "target_lang_sense_clarification": "событий"
                            },
                            {
                                "target_lang_word": "цепь",
                                "target_lang_sense_clarification": "последовательность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89158ea4-ca90-3fde-bc25-ab9f6d01a11b",
                    "sense_definition": "The long back part of a dress or skirt that trails along the ground behind the wearer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bride wore a beautiful white gown with a long lace <w>train</w>.",
                            "target_lang_translations": {
                                "ru": "Невеста была в красивом белом платье с длинным кружевным <w>шлейфом</w>."
                            }
                        },
                        {
                            "text": "She had to lift her <w>train</w> carefully as she walked up the stairs.",
                            "target_lang_translations": {
                                "ru": "Ей приходилось осторожно приподнимать свой <w>шлейф</w>, поднимаясь по лестнице."
                            }
                        }
                    ],
                    "synonyms": [
                        "trail"
                    ],
                    "common_phrases": [
                        "wedding dress train"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная задняя часть платья или юбки, которая волочится по земле позади идущего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлейф",
                                "target_lang_sense_clarification": "часть платья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82069a51-2b60-3b10-ad93-508b876aa579",
                    "sense_definition": "A line of vehicles or animals traveling together.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_moving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wagon <w>train</w> moved slowly across the dusty plains.",
                            "target_lang_translations": {
                                "ru": "<w>Обоз</w> медленно двигался по пыльным равнинам."
                            }
                        },
                        {
                            "text": "A <w>train</w> of camels carried goods across the desert.",
                            "target_lang_translations": {
                                "ru": "<w>Караван</w> верблюдов перевозил товары через пустыню."
                            }
                        }
                    ],
                    "synonyms": [
                        "convoy",
                        "caravan",
                        "procession"
                    ],
                    "common_phrases": [
                        "wagon train",
                        "camel train",
                        "mule train"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вереница транспортных средств или животных, движущихся вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "караван",
                                "target_lang_sense_clarification": "верблюдов, судов"

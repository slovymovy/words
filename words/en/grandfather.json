    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "672b23cb-cef6-3ca0-a04a-4bf5523980cf",
                    "sense_definition": "The father of one's father or mother.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "family_members",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>grandfather</w> tells us stories about his childhood every Sunday.",
                            "target_lang_translations": {
                                "ru": "Мой <w>дедушка</w> каждое воскресенье рассказывает нам истории о своем детстве."
                            }
                        },
                        {
                            "text": "She went to the park with her maternal <w>grandfather</w>.",
                            "target_lang_translations": {
                                "ru": "Она пошла в парк со своим <w>дедушкой</w> по материнской линии."
                            }
                        }
                    ],
                    "synonyms": [
                        "grandpa",
                        "granddad",
                        "grandpapa",
                        "gramps"
                    ],
                    "antonyms": [
                        "grandmother",
                        "grandchild",
                        "grandson",
                        "granddaughter"
                    ],
                    "common_phrases": [
                        "maternal grandfather",
                        "paternal grandfather",
                        "great-grandfather"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отец отца или матери."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дедушка",
                                "target_lang_sense_clarification": "общеупотребительное, нейтральное или ласковое"
                            },
                            {
                                "target_lang_word": "дед",
                                "target_lang_sense_clarification": "разговорное, более формальное или просторечное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a9b4e3d-6c60-3876-8e02-3e165d065fda",
                    "sense_definition": "A male ancestor from an earlier generation, often used to refer to the founders of a community or nation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ancestry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We must preserve the traditions of our <w>grandfathers</w> for future generations.",
                            "target_lang_translations": {
                                "ru": "Мы должны сохранить традиции наших <w>дедов</w> для будущих поколений."
                            }
                        },
                        {
                            "text": "The land of our <w>grandfathers</w> is sacred to the tribe.",
                            "target_lang_translations": {
                                "ru": "Земля наших <w>предков</w> священна для племени."
                            }
                        }
                    ],
                    "synonyms": [
                        "forefather",
                        "ancestor",
                        "grandsire"
                    ],
                    "common_phrases": [
                        "our grandfathers",
                        "land of our grandfathers"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "often used in literary or rhetorical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужской предок из раннего поколения; часто используется для обозначения основателей общины или нации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предок",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "дед",
                                "target_lang_sense_clarification": "обычно во множественном числе: деды"
                            },
                            {
                                "target_lang_word": "праотец",
                                "target_lang_sense_clarification": "книжное, архаичное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0f356b5-4a59-332c-bd2f-22f497655ad6",
                    "sense_definition": "An archived version of a file that is two generations older than the current version, preceding the father file.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computing_backup",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The system administrator restored the database from the <w>grandfather</w> tape.",
                            "target_lang_translations": {
                                "ru": "Системный администратор восстановил базу данных с ленты «<w>дед</w>»."
                            }
                        },
                        {
                            "text": "In this backup rotation, the <w>grandfather</w> file is stored offsite for safety.",
                            "target_lang_translations": {
                                "ru": "В этой схеме ротации резервного копирования файл «<w>дед</w>» хранится вне офиса для безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grandfather file",
                        "grandfather-father-son"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical computing term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Архивная версия файла, которая на два поколения старше текущей версии (в схеме ротации резервного копирования)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дед",
                                "target_lang_sense_clarification": "в схеме «дед — отец — сын»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ac3c7e6e-12fc-36b9-87a4-e6b194344301",
                    "sense_definition": "To exempt someone or something from a new law or regulation, allowing them to continue operating under previous rules.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_exemption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Existing members were <w>grandfathered</w> in and did not have to pay the increased fee.",
                            "target_lang_translations": {
                                "ru": "Действующих участников <w>оставили на старых условиях</w>, и им не пришлось платить повышенный взнос."
                            }
                        },
                        {
                            "text": "The old building was <w>grandfathered</w> regarding the new fire safety codes.",
                            "target_lang_translations": {
                                "ru": "Старое здание <w>освободили</w> от соблюдения новых норм пожарной безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "grandfather in"
                    ],
                    "common_phrases": [
                        "grandfather in",
                        "be grandfathered"
                    ],
                    "target_lang_definitions": {
                        "ru": "Освобождать кого-либо или что-либо от действия нового закона или постановления, позволяя продолжать деятельность по старым правилам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освобождать от новых правил",
                                "target_lang_sense_clarification": "юридический контекст"
                            },
                            {
                                "target_lang_word": "сохранять старые условия",
                                "target_lang_sense_clarification": "применять «дедушкину оговорку»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bee6d5ff-cf28-3292-8199-645f11d24aef",
                    "sense_definition": "To perform the role or duties of a grandfather.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "family_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>grandfathered</w> the young boy with patience and kindness.",
                            "target_lang_translations": {
                                "ru": "Он <w>опекал</w> мальчика с терпением и добротой, как дедушка."
                            }
                        },
                        {
                            "text": "After retiring, he spent his days <w>grandfathering</w> his growing brood of descendants.",
                            "target_lang_translations": {
                                "ru": "Выйдя на пенсию, он проводил дни, <w>нянчась</w> со своим растущим выводком потомков."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять роль или обязанности дедушки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть дедушкой",
                                "target_lang_sense_clarification": "исполнять роль деда"
                            },
                            {
                                "target_lang_word": "нянчиться",
                                "target_lang_sense_clarification": "проявлять заботу, как дедушка"
                            }
                        ]
                    }
                }
            ]
        }
    ]

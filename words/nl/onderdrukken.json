{
    "word_family": [
        "onderdrukking",
        "onderdrukker"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1c225c2b-5708-3f02-8026-e1f539d03eb8",
                    "sense_definition": "Een emotie, lichamelijke reactie of uiting met moeite tegenhouden en niet laten merken",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "suppress_restrain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij kon haar tranen niet langer <w>onderdrukken</w> toen ze het slechte nieuws hoorde.",
                            "target_lang_translations": {
                                "en": "She could no longer <w>hold back</w> her tears when she heard the bad news.",
                                "ru": "Она больше не могла <w>сдерживать</w> слезы, когда услышала плохие новости."
                            }
                        },
                        {
                            "text": "Hij <w>onderdrukte</w> met moeite zijn neiging om hard te gaan lachen.",
                            "target_lang_translations": {
                                "en": "He <w>suppressed</w> his urge to laugh out loud with difficulty.",
                                "ru": "Он с трудом <w>подавил</w> желание громко рассмеяться."
                            }
                        }
                    ],
                    "synonyms": [
                        "beklemmen",
                        "beklemmen"
                    ],
                    "antonyms": [
                        "uiten"
                    ],
                    "common_phrases": [
                        "een lach onderdrukken",
                        "tranen onderdrukken",
                        "woede onderdrukken",
                        "een gaap onderdrukken"
                    ],
                    "target_lang_definitions": {
                        "en": "To hold back an emotion, physical reaction, or expression with difficulty and not let it show",
                        "ru": "Сдерживать эмоцию, физическую реакцию или проявление чего-либо с усилием, не давая этому проявиться"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suppress",
                                "target_lang_sense_clarification": "general term for holding back feelings or reactions"
                            },
                            {
                                "target_lang_word": "hold back",
                                "target_lang_sense_clarification": "often used for tears or emotions"
                            },
                            {
                                "target_lang_word": "stifle",
                                "target_lang_sense_clarification": "specifically for laughter, yawns, or sounds"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сдерживать",
                                "target_lang_sense_clarification": "не давать проявиться (слезам, смеху)"
                            },
                            {
                                "target_lang_word": "подавлять",
                                "target_lang_sense_clarification": "заглушать чувство или позыв (зевоту, гнев)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4867dcf9-5280-39ce-98fa-9f63bb7aad47",
                    "sense_definition": "Met macht of geweld een einde maken aan verzet of opstand",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "suppress_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De troepen <w>onderdrukten</w> het verzet in de stad binnen twee weken.",
                            "target_lang_translations": {
                                "en": "The troops <w>quelled</w> the resistance in the city within two weeks.",
                                "ru": "Войска <w>подавили</w> сопротивление в городе в течение двух недель."
                            }
                        },
                        {
                            "text": "De regering probeerde de demonstraties met geweld te <w>onderdrukken</w>.",
                            "target_lang_translations": {
                                "en": "The government tried to <w>suppress</w> the demonstrations with force.",
                                "ru": "Правительство пыталось силой <w>подавить</w> демонстрации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het verzet onderdrukken",
                        "een opstand onderdrukken",
                        "rellen onderdrukken"
                    ],
                    "target_lang_definitions": {
                        "en": "To put an end to resistance or rebellion using power or force",
                        "ru": "Силой или властью положить конец сопротивлению или восстанию"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suppress",
                                "target_lang_sense_clarification": "formal term for stopping resistance"
                            },
                            {
                                "target_lang_word": "quell",
                                "target_lang_sense_clarification": "often used for riots or unrest"
                            },
                            {
                                "target_lang_word": "put down",
                                "target_lang_sense_clarification": "phrasal verb for crushing a rebellion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подавлять",
                                "target_lang_sense_clarification": "силой прекращать (восстание, бунт)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1906d79-c8b1-333c-8446-c1b4e557b253",
                    "sense_definition": "Een volk of groep mensen in onvrijheid houden en onrechtvaardig behandelen",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "suppress_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dictator <w>onderdrukte</w> zijn volk jarenlang en stond geen kritiek toe.",
                            "target_lang_translations": {
                                "en": "The dictator <w>oppressed</w> his people for years and allowed no criticism.",
                                "ru": "Диктатор годами <w>угнетал</w> свой народ и не допускал никакой критики."
                            }
                        },
                        {
                            "text": "In dat land worden religieuze minderheden systematisch <w>onderdrukt</w>.",
                            "target_lang_translations": {
                                "en": "In that country, religious minorities are systematically <w>oppressed</w>.",
                                "ru": "В этой стране религиозные меньшинства систематически <w>притесняются</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "opprimeren"
                    ],
                    "antonyms": [
                        "bevrijden"
                    ],
                    "common_phrases": [
                        "een volk onderdrukken",
                        "minderheden onderdrukken"
                    ],
                    "target_lang_definitions": {
                        "en": "To keep a people or group in a state of unfreedom and treat them unjustly",
                        "ru": "Держать народ или группу людей в неволе и несправедливо обращаться с ними"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oppress",
                                "target_lang_sense_clarification": "systematic mistreatment of a group"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "угнетать",
                                "target_lang_sense_clarification": "жестоко эксплуатировать, не давать свободы"
                            },
                            {
                                "target_lang_word": "притеснять",
                                "target_lang_sense_clarification": "ущемлять в правах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84ed1727-ceea-380a-98b6-449417d021e9",
                    "sense_definition": "Iets onder het oppervlak van een vloeistof duwen",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "push_under",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>drukte</w> het stuk hout <w>onder</w> om te kijken of het bleef drijven.",
                            "target_lang_translations": {
                                "en": "He <w>pushed</w> the piece of wood <w>under</w> to see if it would float.",
                                "ru": "Он <w>погрузил</w> кусок дерева <w>под воду</w>, чтобы посмотреть, всплывет ли он."
                            }
                        },
                        {
                            "text": "De tuinman <w>drukte</w> de bollen voorzichtig <w>onder</w> in de zachte aarde.",
                            "target_lang_translations": {
                                "en": "The gardener carefully <w>pushed</w> the bulbs <w>under</w> into the soft soil.",
                                "ru": "Садовник осторожно <w>вдавил</w> луковицы в мягкую землю."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To push something below the surface of a liquid",
                        "ru": "Погружать что-либо под поверхность жидкости или мягкой субстанции нажатием"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "push under",
                                "target_lang_sense_clarification": "literal physical action"
                            },
                            {
                                "target_lang_word": "submerge",
                                "target_lang_sense_clarification": "to put completely under water"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "погружать",
                                "target_lang_sense_clarification": "опускать под воду нажатием"
                            },
                            {
                                "target_lang_word": "вдавливать",
                                "target_lang_sense_clarification": "вжимать внутрь (в землю)"
                            }
                        ]
                    }
                }
            ]
        },

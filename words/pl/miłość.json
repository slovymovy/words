    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "893f913c-b186-30e2-b68f-8033e5c6ab8c",
                    "sense_definition": "Głębokie uczucie przywiązania, troski i pożądania, którym darzy się inną osobę.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_core",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>miłość</w> do żony była widoczna w każdym geście.",
                            "target_lang_translations": {
                                "ru": "Его <w>любовь</w> к жене была видна в каждом жесте."
                            }
                        },
                        {
                            "text": "Prawdziwa <w>miłość</w> przetrwa każdą próbę czasu.",
                            "target_lang_translations": {
                                "ru": "Настоящая <w>любовь</w> переживёт любое испытание временем."
                            }
                        }
                    ],
                    "synonyms": [
                        "zauroczenie",
                        "uwielbienie"
                    ],
                    "antonyms": [
                        "nienawiść"
                    ],
                    "common_phrases": [
                        "miłość od pierwszego wejrzenia",
                        "wyznać miłość",
                        "nieszczęśliwa miłość",
                        "ślepa miłość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубокое чувство привязанности, заботы и влечения к другому человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любо́вь",
                                "target_lang_sense_clarification": "глубокое чувство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81384778-40ec-395d-acd3-39e9149149f9",
                    "sense_definition": "Osoba, którą się kocha i która jest obiektem czyichś uczuć.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek był wielką <w>miłością</w> jej życia.",
                            "target_lang_translations": {
                                "ru": "Марек был большой <w>любовью</w> её жизни."
                            }
                        },
                        {
                            "text": "Spotkałem moją dawną <w>miłość</w> na spacerze w parku.",
                            "target_lang_translations": {
                                "ru": "Я встретил свою давнюю <w>любовь</w> на прогулке в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "ukochany",
                        "ukochana",
                        "kochanie"
                    ],
                    "common_phrases": [
                        "stara miłość nie rdzewieje",
                        "pierwsza miłość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, которого любят и который является объектом чьих-то чувств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любо́вь",
                                "target_lang_sense_clarification": "любимый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5412c8d-ac8f-342c-9e6f-7bd99b928fdb",
                    "sense_definition": "Silna więź emocjonalna łącząca ludzi, a także człowieka z Bogiem, ideą lub ojczyzną.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_core",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W kościele często mówi się o <w>miłości</w> do Boga i bliźniego.",
                            "target_lang_translations": {
                                "ru": "В церкви часто говорят о <w>любви</w> к Богу и ближнему."
                            }
                        },
                        {
                            "text": "Matczyna <w>miłość</w> jest bezwarunkowa i niezwykle silna.",
                            "target_lang_translations": {
                                "ru": "Материнская <w>любовь</w> безусловна и невероятно сильна."
                            }
                        }
                    ],
                    "synonyms": [
                        "adoracja",
                        "uwielbienie"
                    ],
                    "antonyms": [
                        "nienawiść"
                    ],
                    "common_phrases": [
                        "miłość bliźniego",
                        "miłość do ojczyzny",
                        "miłość braterska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильная эмоциональная связь, объединяющая людей, а также человека с Богом, идеей или родиной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любо́вь",
                                "target_lang_sense_clarification": "духовная привязанность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c756bb08-3a29-3092-85d6-c32fee5ee89d",
                    "sense_definition": "Wielkie zamiłowanie do czegoś, pasja lub hobby sprawiające przyjemność.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdyby nie jego <w>miłość</w> do latania, nigdy nie zostałby pilotem.",
                            "target_lang_translations": {
                                "ru": "Если бы не его <w>любовь</w> к полетам, он никогда не стал бы пилотом."
                            }
                        },
                        {
                            "text": "Dzieliła ich <w>miłość</w> do muzyki klasycznej i podróży.",
                            "target_lang_translations": {
                                "ru": "Их объединяла <w>любовь</w> к классической музыке и путешествиям."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasja",
                        "fascynacja"
                    ],
                    "common_phrases": [
                        "robić coś z miłością"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное увлечение чем-либо, страсть или хобби, доставляющее удовольствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любо́вь",
                                "target_lang_sense_clarification": "страсть, увлечение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78567345-b761-33d6-8b77-e568384d67fd",
                    "sense_definition": "Kontakt fizyczny prowadzący do zbliżenia seksualnego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W filmie pokazano scenę, w której bohaterowie uprawiali <w>miłość</w>.",
                            "target_lang_translations": {
                                "ru": "В фильме показали сцену, в которой герои занимались <w>любовью</w>."
                            }
                        },
                        {
                            "text": "Dla nich fizyczna <w>miłość</w> była dopełnieniem uczucia.",
                            "target_lang_translations": {
                                "ru": "Для них физическая <w>любовь</w> была дополнением чувства."
                            }
                        }
                    ],
                    "synonyms": [
                        "seks",
                        "kochanie się"
                    ],
                    "antonyms": [
                        "celibat",
                        "wstrzemięźliwość"
                    ],
                    "common_phrases": [
                        "uprawiać miłość",
                        "francuska miłość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физический контакт, ведущий к сексуальной близости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любо́вь",
                                "target_lang_sense_clarification": "интимная близость"
                            }
                        ]
                    }
                }
            ]
        },
        {

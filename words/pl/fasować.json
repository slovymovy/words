                },
                {
                    "sense_id": "3a19bbbc-9898-37f0-851b-a4aed7bbb8a9",
                    "sense_definition": "Wydawać lub rozdzielać produkty z magazynu lub zapasów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "distribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Magazynier zaczął <fasować> mundury dla nowych rekrutów.",

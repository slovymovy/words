                        {
                            "text": "The path <w>bears</w> north towards the mountains.",
                            "target_lang_translations": {
                                "ru": "Тропа <w>ведет</w> на север к горам."
                            }
                        }
                    ],
                    "synonyms": [
                        "turn",
                        "go",
                        "veer"
                    ],
                    "common_phrases": [
                        "bear left",
                        "bear right"

                        {
                            "text": "Her new dress was <w>destroyed</w> by the spilled juice.",
                            "target_lang_translations": {
                                "ru": "Её новое платье было <w>испорчено</w> пролитым соком."
                            }
                        }
                    ],
                    "synonyms": [
                        "soiled",
                        "dirty",
                        "muddy"
                    ],

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cd0e39e4-2ec2-3c81-95c6-944f92a7aa7d",
                    "sense_definition": "Causing severe emotional shock and distress.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "psychological_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Losing a job unexpectedly can be a <w>traumatic</w> experience for anyone.",
                            "target_lang_translations": {
                                "ru": "Неожиданная потеря работы может стать <w>травмирующим</w> опытом для любого человека."
                            }
                        },
                        {
                            "text": "The children received counseling after the <w>traumatic</w> event at their school.",
                            "target_lang_translations": {
                                "ru": "Дети получили психологическую помощь после <w>травмирующего</w> события в их школе."
                            }
                        }
                    ],
                    "synonyms": [
                        "distressing",
                        "shocking",
                        "disturbing"
                    ],
                    "antonyms": [
                        "untraumatic"
                    ],
                    "common_phrases": [
                        "traumatic experience",
                        "traumatic event",
                        "deeply traumatic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий сильное эмоциональное потрясение и страдание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "травмирующий",
                                "target_lang_sense_clarification": "наносящий психологическую травму"
                            },
                            {
                                "target_lang_word": "травматичный",
                                "target_lang_sense_clarification": "болезненный для психики"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af57d0d3-1850-3220-86a3-06036a75fb3e",
                    "sense_definition": "Relating to or caused by a physical injury or wound.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was hospitalized with a <w>traumatic</w> brain injury after the car crash.",
                            "target_lang_translations": {
                                "ru": "Он был госпитализирован с <w>травматическим</w> повреждением мозга после автокатастрофы."
                            }
                        },
                        {
                            "text": "The surgeon specializes in treating <w>traumatic</w> wounds resulting from accidents.",
                            "target_lang_translations": {
                                "ru": "Хирург специализируется на лечении <w>травматических</w> ран, полученных в результате несчастных случаев."
                            }
                        }
                    ],
                    "antonyms": [
                        "non-traumatic"
                    ],
                    "common_phrases": [
                        "traumatic brain injury",
                        "traumatic injury",
                        "traumatic shock"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с телесными повреждениями или вызванный ими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "травматический",
                                "target_lang_sense_clarification": "относящийся к физическим травмам"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e5eaf28b-8c57-3803-97d3-66eee7b972ef",
                    "sense_definition": "A medicine or remedy applied to heal wounds.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The apothecary prepared a <w>traumatic</w> to dress the soldier's deep cut.",
                            "target_lang_translations": {
                                "ru": "Аптекарь приготовил <w>ранозаживляющее средство</w>, чтобы перевязать глубокий порез солдата."
                            }
                        },
                        {
                            "text": "Ancient medical texts describe various herbs used as a <w>traumatic</w> for battle injuries.",
                            "target_lang_translations": {
                                "ru": "Древние медицинские тексты описывают различные травы, использовавшиеся как <w>ранозаживляющее средство</w> при боевых ранениях."
                            }
                        }
                    ],
                    "synonyms": [
                        "vulnerary"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical medical term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лекарственное средство для заживления ран (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ранозаживляющее средство",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

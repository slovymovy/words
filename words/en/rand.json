                    "sense_id": "14779197-37c3-3b71-b1f2-c0056ad3f2ea",
                    "sense_definition": "The standard unit of currency used in South Africa, divided into 100 cents.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "currency_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The exchange rate of the <w>rand</w> has fluctuated significantly this year.",
                            "target_lang_translations": {
                                "ru": "Обменный курс <w>ранда</w> в этом году значительно колебался."
                            }
                        },
                        {
                            "text": "He paid two hundred <w>rands</w> for the ticket to Cape Town.",
                            "target_lang_translations": {
                                "ru": "Он заплатил двести <w>рандов</w> за билет до Кейптауна."
                            }
                        }
                    ],
                    "common_phrases": [
                        "South African rand",
                        "strong rand",
                        "weak rand",
                        "rand exchange rate"
                    ],
                    "target_lang_definitions": {

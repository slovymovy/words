    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "99656ef7-f3a5-3c59-8fb8-aa47190c0f7d",
                    "sense_definition": "Net zo goed; met hetzelfde resultaat of dezelfde geldigheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt het <w>evengoed</w> laten.",
                            "target_lang_translations": {
                                "en": "You might <w>just as well</w> leave it.",
                                "ru": "Ты можешь <w>с таким же успехом</w> этого не делать."
                            }
                        },
                        {
                            "text": "We kunnen <w>evengoed</w> met de trein gaan als met de auto.",
                            "target_lang_translations": {
                                "en": "We can <w>just as well</w> go by train as by car.",
                                "ru": "Мы можем <w>с таким же успехом</w> поехать на поезде, как и на машине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "evengoed kunnen"
                    ],
                    "target_lang_definitions": {
                        "en": "Just as well; with the same result or validity.",
                        "ru": "Так же хорошо; с тем же результатом или той же обоснованностью."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "just as well",
                                "target_lang_sense_clarification": "indicating equal validity or preference"
                            },
                            {
                                "target_lang_word": "equally",
                                "target_lang_sense_clarification": "to the same degree"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "с таким же успехом",
                                "target_lang_sense_clarification": "указывает на равную вероятность или предпочтение"
                            },
                            {
                                "target_lang_word": "так же",
                                "target_lang_sense_clarification": "в равной степени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73f65fcd-1946-3c86-bd47-4b511bcd95cb",
                    "sense_definition": "Ondanks de omstandigheden of tegenargumenten; toch.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het regende hard, maar we hebben ons <w>evengoed</w> prima vermaakt.",
                            "target_lang_translations": {
                                "en": "It rained hard, but we enjoyed ourselves <w>just the same</w>.",
                                "ru": "Шел сильный дождь, но мы <w>всё равно</w> отлично провели время."
                            }
                        },
                        {
                            "text": "Hij was erg moe, maar ging <w>evengoed</w> naar de vergadering.",
                            "target_lang_translations": {
                                "en": "He was very tired, but went to the meeting <w>anyway</w>.",
                                "ru": "Он очень устал, но <w>всё равно</w> пошел на собрание."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Despite the circumstances or counterarguments; anyway.",
                        "ru": "Несмотря на обстоятельства или возражения; всё равно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "anyway",
                                "target_lang_sense_clarification": "regardless of what happened before"
                            },
                            {
                                "target_lang_word": "just the same",
                                "target_lang_sense_clarification": "despite the circumstances"
                            },
                            {
                                "target_lang_word": "nevertheless",
                                "target_lang_sense_clarification": "formal; in spite of that"
                            },
                            {
                                "target_lang_word": "still",
                                "target_lang_sense_clarification": "continuing despite opposition"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "всё равно",
                                "target_lang_sense_clarification": "независимо от обстоятельств"
                            },
                            {
                                "target_lang_word": "тем не менее",
                                "target_lang_sense_clarification": "формальное выражение уступки"
                            },
                            {
                                "target_lang_word": "всё же",
                                "target_lang_sense_clarification": "вопреки ожиданиям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7535ade1-fe5b-37d0-8f21-9ce64518679a",
                    "sense_definition": "Alles bij elkaar genomen; uiteindelijk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Evengoed</w> is het een geslaagde dag geweest.",
                            "target_lang_translations": {

{
    "word_family": [
        "crewmember",
        "crewman",
        "crewer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5b4112c3-0199-3007-9c3a-17d5402e4536",
                    "sense_definition": "A group of people, often staff, who operate a large facility or vehicle such as a ship, airplane, or train.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "functional_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The flight <w>crew</w> prepared the cabin for landing.",
                            "target_lang_translations": {
                                "ru": "Лётный <w>экипаж</w> подготовил салон к посадке."
                            }
                        },
                        {
                            "text": "If you need assistance, please ask a member of the <w>crew</w>.",
                            "target_lang_translations": {
                                "ru": "Если вам нужна помощь, пожалуйста, обратитесь к члену <w>экипажа</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ship's company",
                        "complement",
                        "team"
                    ],
                    "common_phrases": [
                        "cabin crew",
                        "flight crew",
                        "ground crew",
                        "train crew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, управляющих транспортным средством, например судном, самолётом или поездом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экипаж",
                                "target_lang_sense_clarification": "самолёта, судна, танка"
                            },
                            {
                                "target_lang_word": "команда",
                                "target_lang_sense_clarification": "судна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "367af9c2-a6de-3ace-8c8e-54d2f6a91828",
                    "sense_definition": "A group of people working together on a specific physical task or job.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "functional_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The construction <w>crew</w> repaired the bridge over the weekend.",
                            "target_lang_translations": {
                                "ru": "Строительная <w>бригада</w> отремонтировала мост за выходные."
                            }
                        },
                        {
                            "text": "A skeleton <w>crew</w> remained at the factory during the holidays.",
                            "target_lang_translations": {
                                "ru": "На заводе во время праздников осталась лишь дежурная <w>бригада</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "team",
                        "gang"
                    ],
                    "common_phrases": [
                        "skeleton crew",
                        "road crew",
                        "construction crew",
                        "emergency crew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, работающих вместе над конкретной физической задачей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бригада",
                                "target_lang_sense_clarification": "рабочая, строительная"
                            },
                            {
                                "target_lang_word": "команда",
                                "target_lang_sense_clarification": "группа работников"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be27a19f-4081-39b2-827e-d0e62cfbcf78",
                    "sense_definition": "The group of technical workers on a film, television, or stage production who are not actors.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "functional_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cast and <w>crew</w> celebrated after the final scene was filmed.",
                            "target_lang_translations": {
                                "ru": "Актёры и <w>съёмочная группа</w> праздновали после съёмки последней сцены."
                            }
                        },
                        {
                            "text": "She works on the camera <w>crew</w> for a major news channel.",
                            "target_lang_translations": {
                                "ru": "Она работает в операторской <w>группе</w> крупного новостного канала."
                            }
                        }
                    ],
                    "synonyms": [
                        "staff",
                        "stagehands"
                    ],
                    "common_phrases": [
                        "cast and crew",
                        "film crew",
                        "camera crew",
                        "sound crew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа технических специалистов на съёмках фильма, телепередачи или в театре (исключая актёров)."

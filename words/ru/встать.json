    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "30141dbd-244e-359e-aec7-6f420766ed87",
                    "sense_definition": "Принять стоячее положение, подняться на ноги (из положения сидя или лёжа).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ученик <встал>, когда в класс вошёл учитель.",
                            "target_lang_translations": {
                                "en": "The student <stood up> when the teacher entered the classroom."
                            }
                        },
                        {
                            "text": "Он устало <встал> со стула и подошёл к окну.",
                            "target_lang_translations": {
                                "en": "He wearily <got up> from the chair and walked to the window."
                            }
                        }
                    ],
                    "synonyms": [
                        "подняться"
                    ],
                    "antonyms": [
                        "сесть",
                        "опуститься"
                    ],
                    "common_phrases": [
                        "встать со стула",
                        "тяжело встать",
                        "резко встать"
                    ],
                    "target_lang_definitions": {
                        "en": "To assume a standing position; to rise to one's feet from sitting or lying down."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand up",
                                "target_lang_sense_clarification": "rise to feet"
                            },
                            {
                                "target_lang_word": "get up",
                                "target_lang_sense_clarification": "rise from a seat"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bac7465-1f87-37e5-a929-3af21d18d7f1",
                    "sense_definition": "Подняться с постели после сна или болезни.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обычно <встаю> в семь утра, чтобы успеть на работу.",
                            "target_lang_translations": {
                                "en": "I usually <get up> at seven in the morning to be on time for work."
                            }
                        },
                        {
                            "text": "Больной уже <встал> и может немного ходить по комнате.",
                            "target_lang_translations": {
                                "en": "The patient has already <gotten up> and can walk around the room a little."
                            }
                        }
                    ],
                    "synonyms": [
                        "подняться",
                        "стать"
                    ],
                    "antonyms": [
                        "лечь"
                    ],
                    "common_phrases": [
                        "рано встать",
                        "встать с постели",
                        "встать по будильнику"
                    ],
                    "target_lang_definitions": {
                        "en": "To rise from bed after sleeping or illness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get up",
                                "target_lang_sense_clarification": "leave one's bed"
                            },
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "formal; wake and leave bed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef8b36e1-1675-372e-a889-26d6c5ebc4b1",
                    "sense_definition": "Занять определённое место, расположиться где-либо (в очереди, у стены и т.п.).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "positioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <встали> в конец длинной очереди за билетами.",
                            "target_lang_translations": {
                                "en": "We <got in> at the end of the long line for tickets."
                            }
                        },
                        {
                            "text": "Охранник <встал> у двери и внимательно смотрел на посетителей.",
                            "target_lang_translations": {
                                "en": "The guard <stood> by the door and watched the visitors carefully."
                            }
                        }
                    ],
                    "synonyms": [
                        "стать",
                        "расположиться"
                    ],
                    "antonyms": [
                        "уйти",
                        "покинуть"
                    ],
                    "common_phrases": [
                        "встать в очередь",
                        "встать у стены",
                        "встать в круг",
                        "встать на место"
                    ],
                    "target_lang_definitions": {
                        "en": "To occupy a specific place or position (in a line, by a wall, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand",
                                "target_lang_sense_clarification": "take a position"
                            },
                            {
                                "target_lang_word": "get in",
                                "target_lang_sense_clarification": "join a line/queue"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d643baf-c7bb-3460-a663-c582d38361ce",
                    "sense_definition": "Перестать двигаться или работать (о механизмах, часах, транспорте).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мои наручные часы внезапно <встали>.",
                            "target_lang_translations": {
                                "en": "My wristwatch suddenly <stopped>."
                            }
                        },
                        {
                            "text": "Поезд <встал> посреди поля из-за поломки локомотива.",
                            "target_lang_translations": {
                                "en": "The train <stopped> in the middle of the field due to a locomotive breakdown."
                            }
                        }
                    ],
                    "synonyms": [
                        "остановиться",
                        "застыть"
                    ],
                    "antonyms": [
                        "заработать",
                        "покинуть",
                        "двинуться"
                    ],
                    "common_phrases": [
                        "часы встали",
                        "мотор встал",
                        "работа встала"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cease moving or functioning (of mechanisms, watches, transport)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "cease function"
                            },
                            {
                                "target_lang_word": "halt",
                                "target_lang_sense_clarification": "come to a stop"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8430cce6-4b56-385c-879a-973ead9e9f1e",
                    "sense_definition": "Возникнуть, появиться (о вопросе, проблеме, задаче).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед нами <встала> сложная задача, которую нужно решить немедленно.",
                            "target_lang_translations": {
                                "en": "A difficult task <arose> before us which needs to be solved immediately."
                            }
                        },
                        {
                            "text": "Неожиданно <встал> вопрос о финансировании проекта.",
                            "target_lang_translations": {
                                "en": "The question of project funding unexpectedly <came up>."
                            }
                        }
                    ],
                    "synonyms": [
                        "возникнуть",
                        "появиться",
                        "предстать"
                    ],
                    "antonyms": [
                        "пропасть",
                        "забыться"
                    ],
                    "common_phrases": [
                        "встал вопрос",
                        "встала проблема",
                        "встала задача",
                        "встать перед глазами"
                    ],
                    "target_lang_definitions": {
                        "en": "To emerge or appear (of a question, problem, or task)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arise",
                                "target_lang_sense_clarification": "come into being"
                            },
                            {
                                "target_lang_word": "come up",
                                "target_lang_sense_clarification": "appear for discussion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "615702d0-8b3c-37ab-9ba1-7bac35312443",
                    "sense_definition": "Подняться над горизонтом (о небесных светилах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солнце уже <встало> и ярко освещало зелёную долину.",
                            "target_lang_translations": {
                                "en": "The sun had already <risen> and was brightly illuminating the green valley."
                            }
                        },
                        {
                            "text": "Над тёмным лесом медленно <встала> полная луна.",
                            "target_lang_translations": {
                                "en": "A full moon slowly <rose> over the dark forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "взойти",
                        "подняться"
                    ],
                    "antonyms": [
                        "сесть",
                        "зайти",
                        "опуститься"
                    ],
                    "common_phrases": [
                        "солнце встало",
                        "луна встала"
                    ],
                    "target_lang_definitions": {
                        "en": "To rise above the horizon (of celestial bodies)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "ascend (sun/moon)"
                            },
                            {
                                "target_lang_word": "come up",
                                "target_lang_sense_clarification": "appear above horizon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b87fc57-820d-3eb5-b263-292c80826808",
                    "sense_definition": "Принять определённую позу (на колени, на цыпочки и т.п.).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "positioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький мальчик <встал> на цыпочки, чтобы увидеть игрушку на полке.",
                            "target_lang_translations": {
                                "en": "The little boy <stood> on his tiptoes to see the toy on the shelf."
                            }
                        },
                        {
                            "text": "Он <встал> на колени и попросил прощения.",
                            "target_lang_translations": {
                                "en": "He <got> on his knees and asked for forgiveness."
                            }
                        }
                    ],
                    "synonyms": [
                        "остановиться"
                    ],
                    "common_phrases": [
                        "встать на колени",
                        "встать на цыпочки",
                        "встать на четвереньки"
                    ],
                    "target_lang_definitions": {
                        "en": "To assume a specific posture (on knees, on tiptoes, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get on",
                                "target_lang_sense_clarification": "assume a posture (knees)"
                            },
                            {
                                "target_lang_word": "stand",
                                "target_lang_sense_clarification": "balance on (tiptoes)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac466108-41c9-3d25-b2bb-d2bdc7eb8a5b",
                    "sense_definition": "Выступить в защиту кого-либо или чего-либо, или против кого-либо.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Весь народ дружно <встал> на защиту своей родины.",
                            "target_lang_translations": {
                                "en": "The entire nation <stood up> together in defense of their homeland."
                            }
                        },
                        {
                            "text": "Я решил <встать> на сторону своего друга в этом сложном споре.",
                            "target_lang_translations": {
                                "en": "I decided to <take> my friend's side in this difficult argument."
                            }
                        }
                    ],
                    "common_phrases": [
                        "встать на защиту",
                        "встать на сторону",
                        "встать горой",
                        "встать на пути"
                    ],
                    "target_lang_definitions": {
                        "en": "To act in defense of someone/something, or to take a side."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand up for",
                                "target_lang_sense_clarification": "defend"
                            },
                            {
                                "target_lang_word": "take the side of",
                                "target_lang_sense_clarification": "support in a dispute"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3917cfec-ca65-3ab9-a2b0-5f4b9e92013f",
                    "sense_definition": "Покрыться льдом, замёрзнуть (о реке, озере).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "state_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой река <встала>, и дети катались по льду на коньках.",
                            "target_lang_translations": {
                                "en": "In winter the river <froze over>, and children skated on the ice."
                            }
                        },
                        {
                            "text": "Лёд на озере ещё не <встал>, поэтому выходить на него опасно.",
                            "target_lang_translations": {
                                "en": "The ice on the lake has not <set> yet, so it is dangerous to walk on it."
                            }
                        }
                    ],
                    "synonyms": [
                        "замёрзнуть",
                        "застыть"
                    ],
                    "antonyms": [
                        "тронуться"
                    ],
                    "common_phrases": [
                        "река встала",
                        "лёд встал"
                    ],
                    "target_lang_definitions": {
                        "en": "To become covered with ice; to freeze (of a river or lake)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "freeze over",
                                "target_lang_sense_clarification": "become ice-covered"
                            },
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "harden (of ice)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4b68808-6c77-3239-bbd4-3db63b921b9c",
                    "sense_definition": "Обойтись в какую-либо сумму денег.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ремонт старого автомобиля <встал> нам в круглую сумму.",
                            "target_lang_translations": {
                                "en": "Repairing the old car <cost> us a round sum."
                            }
                        },
                        {
                            "text": "Эта поездка <встанет> тебе очень дорого, если не купить билеты заранее.",
                            "target_lang_translations": {
                                "en": "This trip will <cost> you dearly if you don't buy tickets in advance."
                            }
                        }
                    ],
                    "common_phrases": [
                        "встать в копеечку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To amount to a certain sum of money."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cost",
                                "target_lang_sense_clarification": "require payment"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
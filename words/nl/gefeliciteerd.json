{
    "word_family": [
        "feliciteren",
        "felicitatie"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "06243cf4-0638-3b21-8495-b4a643a5e66e",
                    "sense_definition": "Een gelukwens bij een verjaardag, jubileum of een bijzonder resultaat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iedereen riep luid <w>gefeliciteerd</w> toen de winnaar over de finish kwam.",
                            "target_lang_translations": {
                                "ru": "Все громко кричали «<w>поздравляем</w>», когда победитель пересек финишную черту."
                            }
                        },
                        {
                            "text": "Ik zeg alvast <w>gefeliciteerd</w> met je verjaardag, want morgen ben ik er niet.",
                            "target_lang_translations": {
                                "ru": "Я заранее говорю «<w>поздравляю</w>» с днем рождения, так как завтра меня не будет."
                            }
                        }
                    ],
                    "synonyms": [
                        "proficiat"
                    ],
                    "common_phrases": [
                        "van harte gefeliciteerd",
                        "gefeliciteerd met je verjaardag",
                        "nogmaals gefeliciteerd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поздравление с днем рождения, юбилеем или особым достижением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поздравляю",
                                "target_lang_sense_clarification": "междометие"
                            },
                            {
                                "target_lang_word": "мои поздравления",
                                "target_lang_sense_clarification": "фраза"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f129a48e-e123-3e58-9462-ada7357c3729",
                    "sense_definition": "Voltooid deelwoord van <w>feliciteren</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb mijn broer gisteren uitgebreid <w>gefeliciteerd</w> met zijn nieuwe baan.",
                            "target_lang_translations": {
                                "ru": "Вчера я от души <w>поздравил</w> брата с его новой работой."
                            }
                        },
                        {
                            "text": "Zij werden door al hun vrienden <w>gefeliciteerd</w> met het goede nieuws.",
                            "target_lang_translations": {
                                "ru": "Все друзья <w>поздравили</w> их с хорошими новостями."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие прошедшего времени от глагола feliciteren (поздравлять)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поздравил",
                                "target_lang_sense_clarification": "прошедшее время, ед. ч."
                            },
                            {
                                "target_lang_word": "поздравили",
                                "target_lang_sense_clarification": "прошедшее время, мн. ч."
                            }
                        ]

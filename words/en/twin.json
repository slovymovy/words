    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c57597f1-4e38-3614-9698-449aa543dc15",
                    "sense_definition": "One of two children or animals born at the same birth.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "biological_pair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My sister is my <w>twin</w>, but we do not look exactly alike.",
                            "target_lang_translations": {
                                "ru": "Моя сестра — моя <w>двойняшка</w>, но мы не совсем похожи."
                            }
                        },
                        {
                            "text": "She gave birth to healthy <w>twins</w> yesterday morning.",
                            "target_lang_translations": {
                                "ru": "Вчера утром она родила здоровых <w>близнецов</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "identical twin",
                        "fraternal twin",
                        "twin brother",
                        "twin sister"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из двух детей или животных, рожденных при одних родах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "близнец",
                                "target_lang_sense_clarification": "общий термин, часто подразумевает идентичных близнецов"
                            },
                            {
                                "target_lang_word": "двойняшка",
                                "target_lang_sense_clarification": "разговорный термин для разнояйцевых (непохожих) близнецов"
                            },
                            {
                                "target_lang_word": "близняшка",
                                "target_lang_sense_clarification": "разговорная, уменьшительно-ласкательная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4449723f-3fc1-32cd-ba24-4e8edf2f3705",
                    "sense_definition": "A hotel room that contains two separate single beds.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "accommodation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We booked a <w>twin</w> because we are just friends traveling together.",
                            "target_lang_translations": {
                                "ru": "Мы забронировали <w>номер с двумя кроватями</w>, потому что мы просто друзья, путешествующие вместе."
                            }
                        },
                        {
                            "text": "Does the hotel have a <w>twin</w> available for tonight?",
                            "target_lang_translations": {
                                "ru": "Есть ли в отеле свободный <w>номер с двумя кроватями</w> на сегодня?"
                            }

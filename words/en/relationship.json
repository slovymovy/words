    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c43f5eb5-e459-3652-baaf-d9b3febefc7d",
                    "sense_definition": "A romantic or sexual involvement between people.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has been in a steady <w>relationship</w> for over three years.",
                            "target_lang_translations": {
                                "ru": "Она состоит в постоянных <w>отношениях</w> уже более трех лет."
                            }
                        },
                        {
                            "text": "They decided to end their <w>relationship</w> but agreed to remain friends.",
                            "target_lang_translations": {
                                "ru": "Они решили прекратить свои <w>отношения</w>, но договорились остаться друзьями."
                            }
                        }
                    ],
                    "synonyms": [
                        "romance",
                        "partnership",
                        "affair"
                    ],
                    "common_phrases": [
                        "in a relationship",
                        "serious relationship",
                        "romantic relationship",
                        "long-term relationship"
                    ],
                    "target_lang_definitions": {
                        "ru": "Романтическая или сексуальная связь между людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отношения",
                                "target_lang_sense_clarification": "романтическая связь"
                            },
                            {
                                "target_lang_word": "роман",
                                "target_lang_sense_clarification": "любовная интрига"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "любовная связь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "138addaf-78ea-3daf-9707-286b80359d88",
                    "sense_definition": "The way in which two or more people or groups behave towards and deal with each other.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is important to maintain a good working <w>relationship</w> with your colleagues.",
                            "target_lang_translations": {
                                "ru": "Важно поддерживать хорошие рабочие <w>отношения</w> с коллегами."
                            }
                        },
                        {
                            "text": "The <w>relationship</w> between the two neighboring countries has improved significantly.",
                            "target_lang_translations": {
                                "ru": "<w>Отношения</w> между двумя соседними странами значительно улучшились."
                            }
                        }
                    ],
                    "synonyms": [
                        "rapport",
                        "interaction",
                        "bond"
                    ],
                    "common_phrases": [
                        "working relationship",
                        "good relationship",
                        "close relationship",
                        "personal relationship"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характер поведения и взаимодействия двух или более людей или групп по отношению друг к другу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отношения",
                                "target_lang_sense_clarification": "взаимодействие"
                            },
                            {
                                "target_lang_word": "взаимоотношения",
                                "target_lang_sense_clarification": "взаимная связь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1aab639d-abb5-308b-acd1-4b10e374123d",
                    "sense_definition": "A connection, association, or link between two or more things.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists have found a direct <w>relationship</w> between smoking and lung disease.",
                            "target_lang_translations": {
                                "ru": "Ученые обнаружили прямую <w>связь</w> между курением и заболеваниями легких."
                            }
                        },
                        {
                            "text": "The <w>relationship</w> between supply and demand often determines the price of goods.",
                            "target_lang_translations": {
                                "ru": "<w>Взаимосвязь</w> спроса и предложения часто определяет цену товаров."
                            }
                        }
                    ],
                    "synonyms": [
                        "connection",
                        "association",
                        "link",
                        "correlation"
                    ],
                    "antonyms": [
                        "disconnection",
                        "separation"
                    ],
                    "common_phrases": [
                        "relationship between",
                        "direct relationship",
                        "causal relationship"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связь, ассоциация или звено между двумя или более вещами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "соединение"
                            },
                            {
                                "target_lang_word": "взаимосвязь",
                                "target_lang_sense_clarification": "взаимная обусловленность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb67e5df-3166-3d90-b3c8-829b847ccfdb",
                    "sense_definition": "The state of being related by blood or marriage.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer asked the witness about his <w>relationship</w> to the victim.",
                            "target_lang_translations": {
                                "ru": "Полицейский спросил свидетеля о его <w>родстве</w> с жертвой."
                            }
                        },
                        {
                            "text": "We have a distant <w>relationship</w> through my mother's side of the family.",
                            "target_lang_translations": {
                                "ru": "У нас дальнее <w>родство</w> по линии матери."
                            }
                        }
                    ],
                    "synonyms": [
                        "kinship",
                        "family tie"
                    ],
                    "common_phrases": [
                        "blood relationship",
                        "family relationship"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние связи по крови или браку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родство",
                                "target_lang_sense_clarification": "кровная связь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "735ff23d-acd1-3e91-ab64-9378f5ed9711",
                    "sense_definition": "The links between the x-values and y-values of ordered pairs of numbers, especially coordinates.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The graph clearly illustrates the linear <w>relationship</w> between the two variables.",
                            "target_lang_translations": {
                                "ru": "График четко иллюстрирует линейную <w>зависимость</w> между двумя переменными."
                            }
                        },
                        {
                            "text": "Students were asked to calculate the mathematical <w>relationship</w> shown by the coordinates.",
                            "target_lang_translations": {
                                "ru": "Студентов попросили вычислить математическую <w>зависимость</w>, показанную координатами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "linear relationship",
                        "inverse relationship"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связь между значениями переменных, выраженная математически."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зависимость",
                                "target_lang_sense_clarification": "функциональная связь"
                            },
                            {
                                "target_lang_word": "соотношение",
                                "target_lang_sense_clarification": "пропорция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fe6772f-c8cd-3f0a-9b16-11e0b2824dfb",

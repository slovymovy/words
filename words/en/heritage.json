    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8bb0f91f-d2e9-3178-b267-5944db217975",
                    "sense_definition": "The history, traditions, buildings, and objects that a country or society has had for many years and considers important.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city is very proud of its rich cultural <w>heritage</w>.",
                            "target_lang_translations": {
                                "ru": "Город очень гордится своим богатым культурным <w>наследием</w>."
                            }
                        },
                        {
                            "text": "We must work together to preserve our national <w>heritage</w> for future generations.",
                            "target_lang_translations": {
                                "ru": "Мы должны работать вместе, чтобы сохранить наше национальное <w>наследие</w> для будущих поколений."
                            }
                        }
                    ],
                    "synonyms": [
                        "tradition",
                        "legacy",
                        "culture"
                    ],
                    "common_phrases": [
                        "cultural heritage",
                        "national heritage",
                        "rich heritage",
                        "preserve heritage"
                    ],
                    "target_lang_definitions": {
                        "ru": "История, традиции и культурные ценности, которые страна или общество хранит на протяжении многих лет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наследие",
                                "target_lang_sense_clarification": "культурное или историческое"
                            },
                            {
                                "target_lang_word": "достояние",
                                "target_lang_sense_clarification": "национальное богатство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2c84a7a-188b-31e3-8d2e-4fbc56251dbd",
                    "sense_definition": "Relating to a person's cultural background or a language spoken at home, distinct from the dominant society.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university offers a specialized course for <w>heritage</w> speakers of Spanish.",
                            "target_lang_translations": {
                                "ru": "Университет предлагает специальный курс для носителей <w>унаследованного</w> испанского языка."
                            }
                        },
                        {
                            "text": "She wants to reconnect with her <w>heritage</w> language by visiting her grandparents.",
                            "target_lang_translations": {
                                "ru": "Она хочет восстановить связь с языком своих <w>предков</w>, навестив бабушку и дедушку."
                            }
                        }
                    ],
                    "synonyms": [
                        "background",
                        "roots"
                    ],
                    "common_phrases": [
                        "heritage speaker",
                        "heritage language"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к культурным корням человека или языку, на котором говорят в семье, в отличие от доминирующего в обществе языка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наследие",
                                "target_lang_sense_clarification": "культурное происхождение"
                            },
                            {
                                "target_lang_word": "корни",
                                "target_lang_sense_clarification": "происхождение, предки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f074f425-e662-3449-ad3e-4d7daf01fb22",
                    "sense_definition": "Property or possessions passed down to an heir; an inheritance.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The large farm was part of his family <w>heritage</w>.",
                            "target_lang_translations": {
                                "ru": "Большая ферма была частью его семейного <w>наследия</w>."
                            }
                        },
                        {
                            "text": "She received a small <w>heritage</w> after her grandfather passed away.",
                            "target_lang_translations": {
                                "ru": "Она получила небольшое <w>наследство</w> после смерти дедушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "inheritance",
                        "bequest",
                        "patrimony"
                    ],
                    "common_phrases": [
                        "family heritage",
                        "claim one's heritage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имущество или собственность, переходящие к наследнику."
                    },
                    "translations": {
                        "ru": [

            "senses": [
                {
                    "sense_id": "acdcbbdc-e809-3454-b8db-affbd0251cc4",
                    "sense_definition": "Met uitzondering van; niet inbegrepen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "exclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iedereen in de klas was aanwezig, <w>behalve</w> mijn beste vriend.",
                            "target_lang_translations": {
                                "ru": "Все в классе присутствовали, <w>кроме</w> моего лучшего друга."
                            }
                        },
                        {
                            "text": "Ik lust eigenlijk alle soorten groenten, <w>behalve</w> spruitjes en witlof.",
                            "target_lang_translations": {
                                "ru": "На самом деле я люблю все виды овощей, <w>за исключением</w> брюссельской капусты и цикория."
                            }
                        },
                        {
                            "text": "Alles wat ze bezat, <w>behalve</w> haar grammofoon, leek stijlvol en modern.",
                            "target_lang_translations": {
                                "ru": "Всё, чем она владела, <w>кроме</w> её граммофона, казалось стильным и современным."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitgezonderd",
                        "behoudens",
                        "uitgenomen"
                    ],
                    "antonyms": [
                        "inclusief"
                    ],
                    "common_phrases": [
                        "alles behalve",
                        "niemand behalve",
                        "behalve als"
                    ],
                    "target_lang_definitions": {
                        "ru": "За исключением; не включая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроме",
                                "target_lang_sense_clarification": "за исключением кого-либо или чего-либо"
                            },
                            {
                                "target_lang_word": "за исключением",
                                "target_lang_sense_clarification": "исключая из общего числа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e947ce5-4b80-3eb0-b5d8-08e48cd10559",
                    "sense_definition": "In aanvulling op; naast het genoemde ook nog iets anders.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Behalve</w> Nederlands spreekt zij ook heel goed Engels en Duits.",
                            "target_lang_translations": {
                                "ru": "<w>Помимо</w> нидерландского, она также очень хорошо говорит по-английски и по-немецки."
                            }
                        },
                        {
                            "text": "<w>Behalve</w> mijn huiswerk moet ik vanavond ook nog de afwas doen.",
                            "target_lang_translations": {
                                "ru": "<w>Кроме</w> домашнего задания, мне сегодня вечером нужно ещё помыть посуду."
                            }
                        }
                    ],
                    "synonyms": [
                        "naast"
                    ],
                    "common_phrases": [
                        "behalve dat",
                        "behalve dit"
                    ],
                    "target_lang_definitions": {
                        "ru": "В дополнение к; наряду с упомянутым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помимо",
                                "target_lang_sense_clarification": "в дополнение к чему-либо"
                            },
                            {
                                "target_lang_word": "кроме",
                                "target_lang_sense_clarification": "вдобавок к, не считая"
                            }
                        ]
                    }
                }
            ]
        }
    ]

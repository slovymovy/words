                },
                {
                    "sense_id": "b80ea4bf-9a5f-371d-aa10-be9ccae30d44",
                    "sense_definition": "rzeczownik odczasownikowy od <w>oświecić</w>: rzucenie na coś światła fizycznego",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_action_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagłe <w>oświecenie</w> sceny reflektorami zaskoczyło wszystkich aktorów.",
                            "target_lang_translations": {
                                "ru": "Внезапное <w>освещение</w> сцены прожекторами застало всех актеров врасплох."
                            }

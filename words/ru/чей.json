            "senses": [
                {
                    "sense_id": "63042634-293e-3141-b038-dfa358f5a1b1",
                    "sense_definition": "Вопросительное слово, используемое для выяснения того, кому принадлежит предмет или лицо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "interrogative_possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Чей</w> это рюкзак лежит на стуле?"
                        },
                        {
                            "text": "Я не знаю, <w>чей</w> сейчас ход в игре."
                        }
                    ],
                    "common_phrases": [

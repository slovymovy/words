    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "35dc3247-2ade-331f-a472-1ae7d250494b",
                    "sense_definition": "Mężczyzna lub chłopiec narodowości włoskiej, obywatel Włoch.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "italy_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy sąsiad to <w>Włoch</w>, który prowadzi pizzerię.",
                            "target_lang_translations": {
                                "ru": "Мой новый сосед — <w>итальянец</w>, который держит пиццерию."
                            }
                        },
                        {
                            "text": "Każdy rodowity <w>Włoch</w> jest dumny ze swojej kuchni narodowej.",
                            "target_lang_translations": {
                                "ru": "Каждый коренной <w>итальянец</w> гордится своей национальной кухней."
                            }
                        }
                    ],
                    "synonyms": [
                        "Italianiec"
                    ],
                    "common_phrases": [
                        "rodowity Włoch",
                        "temperamentny Włoch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина или мальчик итальянской национальности, гражданин Италии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "итальянец",
                                "target_lang_sense_clarification": "национальность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce90204c-2dc5-3c05-9b47-3b3c92631fbd",
                    "sense_definition": "Zgrubienie od słowa <w>włos</w>; pojedynczy, gruby lub nieestetyczny włos, zwłaszcza na ciele.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W zupie pływał długi, czarny <w>włoch</w>.",
                            "target_lang_translations": {
                                "ru": "В супе плавал длинный черный <w>волос</w>."
                            }
                        },
                        {
                            "text": "Ten <w>włoch</w> na twoim płaszczu należy chyba do psa.",
                            "target_lang_translations": {
                                "ru": "Этот <w>волос</w> на твоем пальто, наверное, собачий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czarny włoch",
                        "psi włoch"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "augmentative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличительная форма от слова \"włos\"; отдельный толстый или неприятный волос, особенно на теле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волос",
                                "target_lang_sense_clarification": "отдельный"
                            },
                            {
                                "target_lang_word": "волосина",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "044efe26-29f5-3a84-abd2-865fdbbbefb6",
                    "sense_definition": "Forma dopełniacza liczby mnogiej nazwy geograficznej <w>Włochy</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "italy_related",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W tym roku po raz pierwszy jedziemy do <w>Włoch</w>.",
                            "target_lang_translations": {
                                "ru": "В этом году мы впервые едем в <w>Италию</w>."
                            }
                        },
                        {
                            "text": "Większość win w tym sklepie pochodzi z <w>Włoch</w>.",
                            "target_lang_translations": {
                                "ru": "Большинство вин в этом магазине — из <w>Италии</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do Włoch",
                        "z Włoch",
                        "mapa Włoch"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа географического названия Włochy (Италия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Италия",
                                "target_lang_sense_clarification": "страна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1444db37-ebaa-3da4-bac1-37c8826a9705",
                    "sense_definition": "Polskie nazwisko męskie lub żeńskie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Włoch</w> został wybrany na przewodniczącego rady.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Влох</w> был избран председателем совета."
                            }
                        },
                        {
                            "text": "Nie znam nikogo o nazwisku <w>Włoch</w>.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого по фамилии <w>Влох</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская мужская или женская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Влох",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
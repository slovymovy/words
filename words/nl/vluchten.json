                {
                    "sense_id": "94399b72-6a52-3617-94ce-06e03cad9653",
                    "sense_definition": "Snel weggaan om te ontkomen aan gevaar of een onveilige situatie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dieven <w>vluchtten</w> direct toen ze de politie zagen aankomen.",
                            "target_lang_translations": {
                                "ru": "Воры сразу же <w>сбежали</w>, когда увидели приближающуюся полицию."
                            }
                        },
                        {
                            "text": "Tijdens de oorlog zijn veel mensen naar een veilig buurland <w>gevlucht</w>.",
                            "target_lang_translations": {
                                "ru": "Во время войны многие люди <w>бежали</w> в безопасную соседнюю страну."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontsnappen",
                        "wegrennen",
                        "ontkomen"
                    ],
                    "antonyms": [
                        "blijven",
                        "standhouden"
                    ],
                    "common_phrases": [
                        "op de vlucht slaan",
                        "het land uit vluchten",
                        "voor het gevaar vluchten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро уходить или убегать, чтобы спастись от опасности или покинуть небезопасную ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бежать",

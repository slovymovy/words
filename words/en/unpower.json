                        "activate",
                        "energize"
                    ],
                    "common_phrases": [
                        "unpower the system",
                        "unpower the device"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекратить подачу питания на устройство или систему; выключить или деактивировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обесточить",

        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "84dc7c36-79e0-3b47-8740-e8aa86ba6ebf",
                    "sense_definition": "To employ or apply something for a specific purpose or function.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "utilize_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can I <w>use</w> your pen for a moment?"
                        },
                        {
                            "text": "We <w>use</w> this knife to slice the bread."
                        }
                    ],
                    "synonyms": [
                        "employ",
                        "utilize",
                        "apply"
                    ],
                    "common_phrases": [
                        "use a tool",
                        "use a method",
                        "easy to use"
                    ]
                },
                {
                    "sense_id": "af6fa34c-ef06-3f44-ac27-34430fe54f87",
                    "sense_definition": "To have done something regularly in the past but not anymore (found in the form 'used to').",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "habitual_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>used</w> to play football every weekend when I was younger."
                        },
                        {
                            "text": "She didn't <w>use</w> to like spicy food, but now she loves it."
                        }
                    ],
                    "common_phrases": [
                        "used to be",
                        "used to do",
                        "didn't use to"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "followed by infinitive"
                        }
                    ]
                },
                {
                    "sense_id": "0b4920d3-5c72-3f02-af7a-592b36f4563f",
                    "sense_definition": "To be or become familiar with something through experience (found in 'be used to' or 'get used to').",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "accustom_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>used</w> to waking up early for work."
                        },
                        {
                            "text": "It took him a while to get <w>used</w> to the cold weather."
                        }
                    ],
                    "common_phrases": [
                        "get used to",
                        "be used to"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "followed by noun or gerund"
                        }
                    ]
                },
                {

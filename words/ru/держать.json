                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "restrain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция <w>держала</w> подозреваемого в камере предварительного заключения три дня."
                        },
                        {
                            "text": "Никто тебя здесь силой не <w>держит</w>, ты можешь уйти в любой момент."
                        }
                    ],
                    "synonyms": [
                        "удерживать",
                        "задерживать",
                        "не пускать"
                    ],
                    "antonyms": [
                        "отпускать",
                        "освобождать"
                    ],
                    "common_phrases": [

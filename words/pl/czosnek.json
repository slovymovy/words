            "senses": [
                {
                    "sense_id": "e3cfadb1-18da-31b5-bed9-358daf2b8fe8",
                    "sense_definition": "Nazwisko męskie używane w Polsce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "surname_male",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Czosnek</w> został wybrany na przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Чоснек</w> был избран председателем совета жилого комплекса."
                            }
                        },
                        {
                            "text": "Rozmawiałem wczoraj z Markiem <w>Czosnkiem</w> o nowym projekcie.",
                            "target_lang_translations": {
                                "ru": "Вчера я разговаривал с Марком <w>Чоснеком</w> о новом проекте."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "male"
                        }
                    ],
                    "target_lang_definitions": {

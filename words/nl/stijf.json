            "senses": [
                {
                    "sense_id": "b2634e29-98e5-3ded-8590-a78e9bafa266",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>stijven</w>.",
                    "learner_level": "C2",

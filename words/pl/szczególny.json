                {
                    "sense_id": "a985f41c-0c5d-3c79-80f2-dd99aaa82971",
                    "sense_definition": "Taki, który wyróżnia się czymś spośród innych, jest inny niż wszystkie; wyjątkowy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "unique_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To był dla nas bardzo <w>szczególny</w> dzień, pełen wzruszeń.",
                            "target_lang_translations": {
                                "ru": "Это был для нас очень <w>особенный</w> день, полный волнений."
                            }
                        },
                        {
                            "text": "Ten region ma swój <w>szczególny</w> klimat, którego nie znajdziesz nigdzie indziej.",
                            "target_lang_translations": {
                                "ru": "У этого региона свой <w>особый</w> климат, которого не найдешь нигде больше."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyjątkowy",
                        "niezwykły",
                        "nieprzeciętny",
                        "osobliwy",
                        "ponadprzeciętny"
                    ],
                    "antonyms": [
                        "zwykły",
                        "zwyczajny",
                        "przeciętny",
                        "typowy"
                    ],
                    "common_phrases": [

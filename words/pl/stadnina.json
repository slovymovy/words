    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1562a5f-d2d4-33a3-95ed-2cc94ed4ade3",
                    "sense_definition": "Ośrodek zajmujący się hodowlą lub reprodukcją koni.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "place_breeding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po szkole chodziliśmy do <w>stadniny</w> pomagać przy koniach.",
                            "target_lang_translations": {
                                "ru": "После школы мы ходили на <w>конезавод</w> помогать с лошадьми."
                            }
                        },
                        {
                            "text": "Słynna <w>stadnina</w> koni arabskich przyciąga wielu turystów z zagranicy.",
                            "target_lang_translations": {
                                "ru": "Знаменитый <w>конный завод</w> арабских скакунов привлекает много туристов из-за границы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "państwowa stadnina",
                        "stadnina koni",
                        "prywatna stadnina"
                    ],
                    "target_lang_definitions": {
                        "ru": "Центр, занимающийся разведением или репродукцией лошадей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конный завод",
                                "target_lang_sense_clarification": "предприятие по разведению лошадей"
                            },
                            {
                                "target_lang_word": "конезавод",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b13fa34-9a2b-3eae-980a-f79c97ae6932",
                    "sense_definition": "Miejsce, w którym trzyma się konie i prowadzi nauki jazdy konnej dla gości.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "place_recreation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W weekendy jeździmy całą rodziną do <w>stadniny</w> na lekcje jazdy.",
                            "target_lang_translations": {
                                "ru": "По выходным мы всей семьей ездим на <w>конюшню</w> на уроки верховой езды."
                            }
                        },
                        {
                            "text": "Ta <w>stadnina</w> organizuje letnie obozy jeździeckie dla dzieci i młodzieży.",
                            "target_lang_translations": {
                                "ru": "Этот <w>конный клуб</w> организует летние лагеря верховой езды для детей и молодежи."
                            }
                        }
                    ],
                    "synonyms": [
                        "ośrodek jeździecki"
                    ],
                    "common_phrases": [
                        "jechać do stadniny",
                        "jazda w stadninie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где содержат лошадей и проводят уроки верховой езды для посетителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конюшня",
                                "target_lang_sense_clarification": "помещение для лошадей"
                            },
                            {
                                "target_lang_word": "конный клуб",
                                "target_lang_sense_clarification": "центр верховой езды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6114cac0-2822-32fd-bd49-1dc56f48d2be",
                    "sense_definition": "Ogół koni hodowanych w jednym gospodarstwie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała <w>stadnina</w> została wypuszczona na zielone pastwiska wczesnym rankiem.",
                            "target_lang_translations": {
                                "ru": "Весь <w>табун</w> выпустили на зеленые пастбища рано утром."
                            }
                        },
                        {
                            "text": "Hrabia był bardzo dumny, że jego <w>stadnina</w> zdobyła liczne nagrody na wystawach.",
                            "target_lang_translations": {
                                "ru": "Граф очень гордился тем, что его <w>конный завод</w> завоевал многочисленные награды на выставках."
                            }
                        }
                    ],
                    "synonyms": [
                        "stado"
                    ],
                    "common_phrases": [
                        "liczna stadnina"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность лошадей, разводимых в одном хозяйстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "табун",
                                "target_lang_sense_clarification": "стадо лошадей"
                            },
                            {
                                "target_lang_word": "поголовье",
                                "target_lang_sense_clarification": "общее количество животных"
                            }
                        ]
                    }
                }
            ]
        }
    ]

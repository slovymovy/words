            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f3660017-a5d6-3417-85a6-9915dfab9d3d",
                    "sense_definition": "W sposób ciągły, stały lub powtarzający się w regularnych odstępach czasu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ceny paliwa na stacjach <w>systematycznie</w> rosną od początku roku.",
                            "target_lang_translations": {
                                "ru": "Цены на топливо на заправках <w>систематически</w> растут с начала года."
                            }
                        },
                        {
                            "text": "Staram się <w>systematycznie</w> biegać, aby poprawić swoją kondycję fizyczną.",
                            "target_lang_translations": {
                                "ru": "Я стараюсь <w>регулярно</w> бегать, чтобы улучшить свою физическую форму."
                            }
                        }
                    ],
                    "synonyms": [
                        "regularnie",
                        "stale",
                        "cyklicznie"
                    ],
                    "antonyms": [
                        "nieregularnie",
                        "sporadycznie"
                    ],
                    "common_phrases": [
                        "systematycznie rosnąć",
                        "systematycznie spadać",
                        "ćwiczyć systematycznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непрерывно, постоянно или повторяясь через регулярные промежутки времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "систематически",
                                "target_lang_sense_clarification": "постоянно, регулярно"
                            },
                            {
                                "target_lang_word": "регулярно",
                                "target_lang_sense_clarification": "через равные промежутки времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c667fe57-e066-31ad-8488-57c263a369f6",
                    "sense_definition": "W sposób uporządkowany, dokładny i zgodny z ustalonym planem lub metodą.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <w>systematycznie</w> porządkować swoje dokumenty, żeby nic nie zgubić.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>систематично</w> приводить в порядок свои документы, чтобы ничего не потерять."
                            }
                        },
                        {
                            "text": "Zespół badawczy zaczął <w>systematycznie</w> analizować zebrane dane.",
                            "target_lang_translations": {
                                "ru": "Исследовательская группа начала <w>систематично</w> анализировать собранные данные."
                            }
                        }
                    ],
                    "synonyms": [
                        "metodycznie",
                        "planowo"
                    ],
                    "antonyms": [
                        "chaotycznie",
                        "bezładnie",
                        "niesystematycznie"
                    ],
                    "common_phrases": [
                        "pracować systematycznie",
                        "uczyć się systematycznie",
                        "działać systematycznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упорядоченно, последовательно, следуя определённому плану или методу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "систематично",
                                "target_lang_sense_clarification": "методично, последовательно"
                            },
                            {
                                "target_lang_word": "планомерно",
                                "target_lang_sense_clarification": "согласно плану"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f46a1aa2-7019-3fc9-8ebd-08711347aecf",
                    "sense_definition": "Z punktu widzenia systematyki, czyli naukowej klasyfikacji organizmów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten gatunek jest <w>systematycznie</w> bliski wilkom, choć wygląda inaczej.",
                            "target_lang_translations": {
                                "ru": "Этот вид <w>систематически</w> близок к волкам, хотя и выглядит иначе."
                            }
                        },
                        {
                            "text": "Roślina ta została <w>systematycznie</w> opisana i sklasyfikowana w zeszłym wieku.",
                            "target_lang_translations": {
                                "ru": "Это растение было <w>систематически</w> описано и классифицировано в прошлом веке."
                            }
                        }
                    ],
                    "synonyms": [
                        "taksonomicznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "С точки зрения систематики (научной классификации организмов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "систематически",
                                "target_lang_sense_clarification": "в биологической классификации"
                            }
                        ]
                    }
                }
            ]
        }
    ]

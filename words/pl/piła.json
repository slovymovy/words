                {
                    "sense_id": "4740cc81-886e-3a25-8a49-cfbc5e683180",
                    "sense_definition": "zgrubienie od słowa <w>piłka</w>; często używane w kontekście sportowym na określenie silnie kopniętej piłki",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napastnik posłał taką <piłę> w stronę bramki, że bramkarz nie zdążył zareagować.",
                            "target_lang_translations": {
                                "ru": "Нападающий послал такой <мяч> в сторону ворот, что вратарь не успел среагировать."
                            }
                        },
                        {
                            "text": "Podaj mi tę <piłę>, bo chcemy zacząć mecz.",
                            "target_lang_translations": {
                                "ru": "Пасуй мне этот <мяч>, мы хотим начать матч."
                            }

                    "sense_id": "03eb0993-e95b-3c80-9230-c6090645a19e",
                    "sense_definition": "Heel erg koud.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "temperature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wind waaide hard en het was <w>steenkoud</w> buiten.",
                            "target_lang_translations": {
                                "en": "The wind was blowing hard and it was <w>freezing</w> outside.",
                                "ru": "Дул сильный ветер, и на улице было <w>очень холодно</w>."
                            }
                        },
                        {
                            "text": "De nachten werden <w>steenkoud</w> waardoor ik met al mijn kleren aan moest slapen.",
                            "target_lang_translations": {
                                "en": "The nights became <w>freezing cold</w>, so I had to sleep with all my clothes on.",
                                "ru": "Ночи становились <w>ледяными</w>, поэтому мне приходилось спать во всей одежде."
                            }
                        }
                    ],
                    "synonyms": [
                        "ijskoud",
                        "doodkoud",
                        "inkoud",
                        "stervenskoud"
                    ],
                    "antonyms": [
                        "bloedheet",
                        "snikheet"
                    ],
                    "common_phrases": [
                        "steenkoud water",
                        "steenkoud zijn",
                        "steenkoude handen"
                    ],
                    "target_lang_definitions": {
                        "en": "Very cold.",
                        "ru": "Очень холодный; пронизывающий холод."
                    },

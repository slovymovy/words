    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "97adc5f6-89c0-3110-9168-231a3c797a01",
                    "sense_definition": "Having good luck or success by chance.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_of_luck",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>lucky</w> winner of the lottery was announced on television yesterday.",
                            "target_lang_translations": {
                                "ru": "<w>Счастливый</w> победитель лотереи был объявлен по телевидению вчера."
                            }
                        },
                        {
                            "text": "I feel very <w>lucky</w> to have such supportive friends and family.",
                            "target_lang_translations": {
                                "ru": "Я чувствую себя очень <w>счастливым</w>, что у меня такие отзывчивые друзья и семья."
                            }
                        }
                    ],
                    "synonyms": [
                        "fortunate",
                        "successful",
                        "happy"
                    ],
                    "antonyms": [
                        "unlucky",
                        "unfortunate"
                    ],
                    "common_phrases": [
                        "lucky winner",
                        "lucky guess",
                        "get lucky",
                        "lucky day"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий удачей или добившийся успеха благодаря счастливому случаю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "везу́чий",
                                "target_lang_sense_clarification": "разг.; о человеке, которому часто везет"
                            },
                            {
                                "target_lang_word": "уда́чливый",
                                "target_lang_sense_clarification": "о человеке, которому сопутствует удача"
                            },
                            {
                                "target_lang_word": "счастли́вый",
                                "target_lang_sense_clarification": "удачный, благополучный (например, победитель)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85fb9f9e-2cbe-36e4-843a-9c60188c99ea",
                    "sense_definition": "Bringing or resulting in good luck.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cause_of_luck",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He always carries a <w>lucky</w> coin in his pocket for good fortune.",
                            "target_lang_translations": {
                                "ru": "Он всегда носит в кармане <w>счастливую</w> монету на удачу."
                            }
                        },
                        {
                            "text": "It was a <w>lucky</w> coincidence that we arrived at the station simultaneously.",
                            "target_lang_translations": {
                                "ru": "Это было <w>удачное</w> совпадение, что мы прибыли на станцию одновременно."
                            }
                        }
                    ],
                    "synonyms": [
                        "auspicious",
                        "favorable",
                        "propitious",
                        "fortuitous",
                        "serendipitous"
                    ],
                    "antonyms": [
                        "unlucky",
                        "cursed"
                    ],
                    "common_phrases": [
                        "lucky charm",
                        "lucky number",
                        "lucky break",
                        "lucky star"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приносящий удачу или являющийся результатом удачного стечения обстоятельств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "счастли́вый",
                                "target_lang_sense_clarification": "приносящий счастье (о предметах, талисманах)"
                            },
                            {
                                "target_lang_word": "уда́чный",
                                "target_lang_sense_clarification": "успешный, завершившийся удачей (о событиях)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e56cd5b7-ff12-3aa2-bd83-631163e8b883",

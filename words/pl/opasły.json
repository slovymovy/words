    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1ebf7fd3-7410-3fb5-bb48-d91cdf9e24d8",
                    "sense_definition": "Bardzo gruby, otyły, posiadający dużą ilość tkanki tłuszczowej (zwykle o ludziach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_fatness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>opasły</w> brzuch trząsł się przy każdym głośnym śmiechu.",
                            "target_lang_translations": {
                                "ru": "Его <w>толстый</w> живот трясся при каждом громком смехе."
                            }
                        },
                        {
                            "text": "W wygodnym fotelu zasiadł <w>opasły</w> mężczyzna i zamówił kolejny deser.",
                            "target_lang_translations": {
                                "ru": "В удобное кресло сел <w>тучный</w> мужчина и заказал очередной десерт."
                            }
                        }
                    ],
                    "synonyms": [
                        "otyły",
                        "gruby",
                        "tłusty"
                    ],
                    "antonyms": [
                        "chudy",
                        "szczupły"
                    ],
                    "common_phrases": [
                        "opasły brzuch",
                        "opasła postać",
                        "opasły mnich"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень толстый, тучный, имеющий много жира (обычно о людях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тучный",
                                "target_lang_sense_clarification": "о человеке: очень толстый, грузный"
                            },
                            {
                                "target_lang_word": "толстый",
                                "target_lang_sense_clarification": "имеющий лишний вес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1ee6df7-398a-31e3-907b-85be86b703e8",
                    "sense_definition": "O książce, dokumencie lub teczce: bardzo gruby, obszerny, zawierający wiele stron.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Student musiał przeczytać ten <w>opasły</w> podręcznik w zaledwie jeden weekend.",
                            "target_lang_translations": {
                                "ru": "Студент должен был прочитать этот <w>толстый</w> учебник всего за одни выходные."
                            }
                        },
                        {
                            "text": "Na biurku sędziego leżał <w>opasły</w> tom akt dotyczących skomplikowanej sprawy.",
                            "target_lang_translations": {
                                "ru": "На столе судьи лежал <w>увесистый</w> том дела, касающегося сложного случая."
                            }
                        }
                    ],
                    "synonyms": [
                        "gruby",
                        "wielki",
                        "ogromny"
                    ],
                    "antonyms": [
                        "cienki"
                    ],
                    "common_phrases": [
                        "opasły tom",
                        "opasła teczka",
                        "opasłe tomisko",
                        "opasła księga"
                    ],
                    "target_lang_definitions": {
                        "ru": "О книге, документе или папке: очень толстый, объемный, содержащий много страниц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толстый",
                                "target_lang_sense_clarification": "о книге: содержащий много страниц"
                            },
                            {
                                "target_lang_word": "увесистый",
                                "target_lang_sense_clarification": "тяжелый и большой по объему"
                            },
                            {
                                "target_lang_word": "объемистый",
                                "target_lang_sense_clarification": "занимающий много места, большой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81f5f7e3-f200-3710-a579-87763a3033f9",
                    "sense_definition": "Dobrze wykarmiony i utuczony (zazwyczaj w odniesieniu do zwierząt hodowlanych).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_fatness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik z dumą prezentował na targu swoje <w>opasłe</w> krowy.",
                            "target_lang_translations": {
                                "ru": "Фермер с гордостью показывал на рынке своих <w>откормленных</w> коров."
                            }
                        },
                        {
                            "text": "W zagrodzie stał <w>opasły</w> byk przygotowany do dorocznej wystawy.",
                            "target_lang_translations": {
                                "ru": "В загоне стоял <w>откормленный</w> бык, подготовленный к ежегодной выставке."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypasiony",
                        "tłusty"
                    ],
                    "antonyms": [
                        "chudy",
                        "wychudzony"
                    ],
                    "common_phrases": [
                        "opasły wół",
                        "opasłe bydło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошо откормленный (обычно о сельскохозяйственных животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "откормленный",
                                "target_lang_sense_clarification": "о животных: упитанный, нагулявший жир"
                            },
                            {
                                "target_lang_word": "тучный",
                                "target_lang_sense_clarification": "жирный, упитанный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

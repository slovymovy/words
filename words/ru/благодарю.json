    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "9d55c5e2-27d0-3818-8c49-6e08c31e47eb",
                    "sense_definition": "Вежливое слово, которое используют для выражения признательности; то же, что «спасибо».",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "gratitude_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Вот ваш билет. — <w>Благодарю</w> вас.",
                            "target_lang_translations": {
                                "en": "— Here is your ticket. — <w>Thank</w> you."
                            }
                        },
                        {
                            "text": "<w>Благодарю</w> за приятный вечер и вкусный ужин.",
                            "target_lang_translations": {
                                "en": "<w>Thank you</w> for the pleasant evening and delicious dinner."
                            }
                        }
                    ],
                    "synonyms": [
                        "спаси́бо"
                    ],
                    "common_phrases": [
                        "благодарю вас",
                        "сердечно благодарю",
                        "искренне благодарю"
                    ],
                    "target_lang_definitions": {
                        "en": "A polite word used to express appreciation; same as \"thank you\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thank you",
                                "target_lang_sense_clarification": "expression of gratitude"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7ee0711f-d7ca-30f1-b3da-4deb6cf7c7da",
                    "sense_definition": "Форма первого лица единственного числа настоящего времени глагола <w>благодарить</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "gratitude_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>благодарю</w> всех своих друзей за поддержку.",
                            "target_lang_translations": {
                                "en": "I <w>thank</w> all my friends for their support."
                            }
                        },
                        {
                            "text": "В этом письме я <w>благодарю</w> учителя за его труд.",
                            "target_lang_translations": {
                                "en": "In this letter, I <w>thank</w> the teacher for his work."
                            }
                        }
                    ],
                    "common_phrases": [
                        "благодарю за внимание",
                        "благодарю за всё",
                        "благодарю судьбу"

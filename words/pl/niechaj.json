                    "target_lang_definitions": {
                        "ru": "Форма повелительного наклонения (2-е лицо ед. ч.) глагола \"niechać\" (оставить, перестать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставь",

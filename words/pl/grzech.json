                {
                    "sense_id": "0b9a16eb-5a4c-37ac-b66d-730357bd5a7e",
                    "sense_definition": "Celowe przekroczenie norm moralnych uznawanych w danej religii; uczynek niezgodny z przykazaniami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_sin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spowiedź to sakrament, w którym wyznajemy swoje <grzechy>.",
                            "target_lang_translations": {
                                "ru": "Исповедь — это таинство, в котором мы исповедуем свои <грехи>."
                            }
                        },
                        {
                            "text": "Ksiądz wytłumaczył wiernym, czym jest <grzech> śmiertelny.",
                            "target_lang_translations": {
                                "ru": "Священник объяснил верующим, что такое смертный <грех>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przewinienie",
                        "wina",
                        "nieprawość"
                    ],
                    "antonyms": [
                        "cnota"
                    ],
                    "common_phrases": [
                        "popełnić grzech",
                        "grzech śmiertelny",
                        "grzech pierworodny",
                        "odpuścić grzechy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умышленное нарушение религиозных и моральных норм; поступок, противоречащий заповедям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грех",
                                "target_lang_sense_clarification": "религиозное понятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "437079ed-5c40-34fd-bd33-cfbde8faa2a8",
                    "sense_definition": "Czyn naganny, błąd lub wykroczenie przeciwko jakimś zasadom, niekoniecznie religijnym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "misdeed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marnowanie jedzenia to w dzisiejszych czasach prawdziwy <grzech>.",
                            "target_lang_translations": {
                                "ru": "Выбрасывать еду в наше время — настоящий <грех>."
                            }
                        },
                        {
                            "text": "Jej jedynym <grzechem> była zbytnia ufność wobec obcych ludzi.",
                            "target_lang_translations": {
                                "ru": "Её единственным <грехом> была чрезмерная доверчивость к незнакомцам."
                            }
                        }
                    ],
                    "synonyms": [
                        "błąd",
                        "wykroczenie",
                        "występek"
                    ],
                    "common_phrases": [
                        "grzech zaniechania",
                        "wart grzechu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предосудительный поступок, ошибка или нарушение каких-либо правил (в переносном смысле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грех",
                                "target_lang_sense_clarification": "проступок, ошибка"
                            },
                            {
                                "target_lang_word": "проступок",
                                "target_lang_sense_clarification": "вина, нарушение"

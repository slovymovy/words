            "senses": [
                {
                    "sense_id": "24a150e7-5a52-35e4-a730-bdb50ca349c3",
                    "sense_definition": "A man, generally a younger one.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I saw a cool <w>dude</w> wearing sunglasses at the park.",

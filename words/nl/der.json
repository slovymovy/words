    "entries": [
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "6bc433ca-9e38-3e79-a1e0-fa7c696c3859",
                    "sense_definition": "De oude naamvalsvorm (genitief meervoud) van <w>de</w>, die 'van de' betekent.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "genitive_article",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het Koninkrijk <w>der</w> Nederlanden is een soevereine staat in Europa.",
                            "target_lang_translations": {
                                "en": "The Kingdom <w>of the</w> Netherlands is a sovereign state in Europe.",
                                "ru": "Королевство <w>Нидерландов</w> — суверенное государство в Европе."
                            }
                        },
                        {
                            "text": "Hij herinnerde zich de heldendaden <w>der</w> voorvaderen.",
                            "target_lang_translations": {
                                "en": "He remembered the heroic deeds <w>of the</w> ancestors.",
                                "ru": "Он вспоминал подвиги <w>предков</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Koninkrijk der Nederlanden",
                        "der goden"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "in modern Nederlands meestal vervangen door 'van de'"
                        },
                        {
                            "trait_type": "form",
                            "comment": "genitief meervoud"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The archaic genitive plural form of 'de', meaning 'of the'.",
                        "ru": "Устаревшая форма родительного падежа (множественное число) определенного артикля <w>de</w>, соответствующая предлогу 'van' (из/от/принадлежность)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of the",
                                "target_lang_sense_clarification": "genitive plural article"
                            }
                        ],
                        "ru": []
                    }
                },
                {
                    "sense_id": "d265dc3c-e541-3a2f-b20f-9c51995fda5f",
                    "sense_definition": "De oude naamvalsvorm (genitief enkelvoud) van <w>de</w> (vrouwelijk), die 'van de' betekent.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "genitive_article",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de loop <w>der</w> jaren is de stad enorm gegroeid.",
                            "target_lang_translations": {
                                "en": "In the course <w>of the</w> years, the city has grown enormously.",
                                "ru": "С течением <w>лет</w> город сильно разросся."
                            }
                        },
                        {
                            "text": "Zij bestudeert de geschiedenis <w>der</w> Nederlandse taal.",
                            "target_lang_translations": {
                                "en": "She studies the history <w>of the</w> Dutch language.",
                                "ru": "Она изучает историю нидерландского <w>языка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de loop der tijd",
                        "in de loop der eeuwen",
                        "moeder der smarten"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "in modern Nederlands meestal vervangen door 'van de'"
                        },
                        {
                            "trait_type": "form",
                            "comment": "genitief enkelvoud vrouwelijk"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The archaic genitive singular feminine form of 'de', meaning 'of the'.",
                        "ru": "Устаревшая форма родительного падежа (единственное число, женский род) определенного артикля <w>de</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of the",
                                "target_lang_sense_clarification": "genitive singular feminine article"
                            }
                        ],
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a7b64ad1-4a60-3a0d-aed4-fe24eee09767",
                    "sense_definition": "Een verkorte vorm van <w>daar</w>, gebruikt om naar een plaats te verwijzen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adverb_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben <w>der</w> nog nooit geweest, maar ik wil er graag heen.",
                            "target_lang_translations": {
                                "en": "I have never been <w>there</w>, but I would like to go.",
                                "ru": "Я <w>там</w> еще никогда не был, но очень хочу туда поехать."
                            }
                        },
                        {
                            "text": "Wie is <w>der</w> eigenlijk aan de beurt?",
                            "target_lang_translations": {
                                "en": "Who is <w>there</w> actually next?",
                                "ru": "Кто <w>там</w> вообще на очереди?"
                            }
                        }
                    ],
                    "synonyms": [
                        "daar",
                        "ginder"
                    ],
                    "antonyms": [
                        "hier"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "spreektaal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A shortened form of 'daar' (there), used to refer to a place.",
                        "ru": "Разговорная сокращенная форма наречия <w>daar</w>, используемая для указания на место."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "there",
                                "target_lang_sense_clarification": "colloquial reduction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "там",
                                "target_lang_sense_clarification": "указание на местоположение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

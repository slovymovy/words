    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bd4863ef-a17d-35be-af7b-e59ac786a5df",
                    "sense_definition": "Wykonać krótki, pojedynczy ruch w jedną stronę, zachwiać się lub poruszyć czymś w ten sposób.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny podmuch wiatru sprawił, że drzewo niebezpiecznie <w>wahnęło</w> się w stronę domu.",
                            "target_lang_translations": {
                                "ru": "Сильный порыв ветра заставил дерево опасно <w>качнуться</w> в сторону дома."
                            }
                        },
                        {
                            "text": "Ptak <w>wahnął</w> skrzydłami i wzbił się wysoko w powietrze.",
                            "target_lang_translations": {
                                "ru": "Птица <w>махнула</w> крыльями и взмыла высоко в воздух."
                            }
                        }
                    ],
                    "synonyms": [
                        "bujnąć",
                        "kołysnąć",
                        "machnąć",
                        "zachwiać"
                    ],
                    "antonyms": [
                        "unieruchomić",
                        "zatrzymać"
                    ],
                    "common_phrases": [
                        "wahnąć się na nogach",
                        "wahnąć ręką",
                        "wahnąć skrzydłem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать короткое, одиночное движение в одну сторону, покачнуться или двинуть чем-то таким образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "качнуться",
                                "target_lang_sense_clarification": "сделать движение в сторону"
                            },
                            {
                                "target_lang_word": "махнуть",
                                "target_lang_sense_clarification": "сделать взмах (напр. крыльями)"
                            },
                            {
                                "target_lang_word": "покачнуться",
                                "target_lang_sense_clarification": "потерять равновесие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4a86dbf-ac12-34d6-98a2-58f903beb24a",
                    "sense_definition": "Poczuć chwilową niepewność przed podjęciem decyzji lub działania; stracić na moment zdecydowanie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciał już wejść do gabinetu, ale <w>wahnął</w> się w ostatniej chwili.",
                            "target_lang_translations": {
                                "ru": "Он уже хотел войти в кабинет, но в последний момент <w>поколебался</w>."
                            }
                        },
                        {
                            "text": "Żołnierz nie <w>wahnął</w> się ani przez moment, wykonując rozkaz dowódcy.",
                            "target_lang_translations": {
                                "ru": "Солдат не <w>дрогнул</w> ни на мгновение, выполняя приказ командира."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawahać się",
                        "zastanowić się"
                    ],
                    "antonyms": [
                        "zdecydować się",
                        "postanowić"
                    ],
                    "common_phrases": [
                        "wahnąć się z decyzją",
                        "ani razu się nie wahnąć"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane głównie jako czasownik zwrotny (wahnąć się)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Почувствовать мгновенную неуверенность перед принятием решения или действием; на мгновение потерять решимость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поколебаться",
                                "target_lang_sense_clarification": "испытать сомнение"
                            },
                            {
                                "target_lang_word": "дрогнуть",
                                "target_lang_sense_clarification": "потерять решимость"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
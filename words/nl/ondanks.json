    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "7f36fd91-924b-3610-84de-f6a4cb72a01f",
                    "sense_definition": "In weerwil van; zonder dat een bepaalde omstandigheid het gevolg belet.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gingen wandelen <w>ondanks</w> de zware regen.",
                            "target_lang_translations": {
                                "ru": "Мы пошли гулять <w>несмотря на</w> сильный дождь."
                            }
                        },
                        {
                            "text": "<w>Ondanks</w> zijn ziekte kwam hij toch naar school.",
                            "target_lang_translations": {
                                "ru": "<w>Несмотря на</w> свою болезнь, он всё же пришёл в школу."
                            }
                        }
                    ],
                    "antonyms": [
                        "dankzij"
                    ],
                    "common_phrases": [
                        "ondanks alles",
                        "ondanks het feit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопреки чему-либо; невзирая на обстоятельства, которые могли бы помешать результату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несмотря на",
                                "target_lang_sense_clarification": "предлог; наиболее общее значение"
                            },
                            {
                                "target_lang_word": "вопреки",
                                "target_lang_sense_clarification": "предлог; указывает на действие наперекор чему-то"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "ac59da59-2345-336a-aff0-94d8d0d32858",
                    "sense_definition": "Drukt een tegenstelling uit met het voorafgaande; hoewel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij slaagde voor het examen, <w>ondanks</w> dat hij niet had geleerd.",
                            "target_lang_translations": {
                                "ru": "Он сдал экзамен, <w>несмотря на то, что</w> не учился."
                            }
                        },
                        {
                            "text": "<w>Ondanks</w> dat ze moe was, bleef ze de hele avond lachen.",
                            "target_lang_translations": {
                                "ru": "<w>Несмотря на то, что</w> она устала, она продолжала смеяться весь вечер."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoewel"
                    ],
                    "common_phrases": [
                        "ondanks dat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает противопоставление предыдущему; хотя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несмотря на то, что",
                                "target_lang_sense_clarification": "союз; используется перед придаточным предложением"
                            },
                            {
                                "target_lang_word": "хотя",
                                "target_lang_sense_clarification": "союз; выражает уступку"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "b31448a9-d860-3f86-a25c-d2a4184b6f11",
                    "sense_definition": "Zmieniać swoje miejsce pobytu, poruszać się w określonym kierunku lub podróżować (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dużym mieście najlepiej <w>przemieszczać</w> się metrem lub tramwajem, aby uniknąć korków.",
                            "target_lang_translations": {
                                "ru": "В большом городе лучше всего <w>перемещаться</w> на метро или трамвае, чтобы избежать пробок."
                            }
                        },
                        {
                            "text": "Wojska wroga szybko <w>przemieściły</w> się na północ, by ominąć trudny teren przy jeziorze.",
                            "target_lang_translations": {
                                "ru": "Войска врага быстро <w>переместились</w> на север, чтобы обойти сложную местность у озера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szybko się przemieszczać",
                        "przemieszczać się pieszo",
                        "przemieszczać się autem",
                        "swobodnie się przemieszczać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Менять свое местонахождение, двигаться в определенном направлении или путешествовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перемещаться",
                                "target_lang_sense_clarification": "менять местоположение"
                            },
                            {
                                "target_lang_word": "передвигаться",
                                "target_lang_sense_clarification": "двигаться в пространстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eecf809b-3486-3639-bb4e-eb0e222e28c3",
                    "sense_definition": "Powodować zmianę położenia kogoś lub czegoś; transportować obiekt lub osobę w inne miejsce.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracownicy magazynu musieli ręcznie <w>przemieszczać</w> ciężkie pudła na drugą stronę hali.",
                            "target_lang_translations": {
                                "ru": "Работники склада должны были вручную <w>перемещать</w> тяжелые коробки на другую сторону зала."
                            }
                        },
                        {
                            "text": "Silny wiatr potrafi <w>przemieszczać</w> ogromne masy piasku, tworząc nowe wydmy.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер способен <w>перемещать</w> огромные массы песка, создавая новые дюны."
                            }
                        }
                    ],
                    "synonyms": [
                        "przenosić",
                        "przesuwać"
                    ],
                    "common_phrases": [
                        "przemieszczać ładunki",
                        "przemieszczać wojska",
                        "przemieszczać towary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Менять положение кого-либо или чего-либо; транспортировать объект или человека в другое место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перемещать",

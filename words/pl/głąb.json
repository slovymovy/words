    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6d0e4803-3f0f-3c92-93ab-245098fce58f",
                    "sense_definition": "Wewnętrzna, oddalona od krawędzi część jakiejś przestrzeni lub obszaru; często używane w wyrażeniach określających kierunek ruchu do środka.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "space_interior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Turysci ruszyli w <głąb> lasu, aby znaleźć ukryte jezioro.",
                            "target_lang_translations": {
                                "ru": "Туристы направились в <глубь> леса, чтобы найти скрытое озеро."
                            }
                        },
                        {
                            "text": "Statek wypłynął w <głąb> morza i wkrótce zniknął nam z oczu.",
                            "target_lang_translations": {
                                "ru": "Корабль уплыл в <глубь> моря и вскоре исчез у нас из виду."
                            }
                        }
                    ],
                    "synonyms": [
                        "wnętrze",
                        "środek"
                    ],
                    "antonyms": [
                        "wierzch",
                        "powierzchnia",
                        "skraj"
                    ],
                    "common_phrases": [
                        "w głąb",
                        "w głąb lądu",
                        "w głąb siebie",
                        "w głąb kraju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренняя, удаленная от края часть какого-либо пространства или области; часто используется в выражениях, обозначающих направление движения внутрь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глубь",
                                "target_lang_sense_clarification": "внутренняя часть, глубина"
                            },
                            {
                                "target_lang_word": "глубина",
                                "target_lang_sense_clarification": "пространство внутри чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ba8c315-e246-3e91-a524-1d8aa6579219",
                    "sense_definition": "Obraźliwe określenie osoby, którą uważa się za mało inteligentną, nierozgarniętą lub głupią.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź taki <głąb> i spróbuj wreszcie zrozumieć tę prostą zasadę.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <тупицей> и попытайся наконец понять это простое правило."
                            }
                        },
                        {
                            "text": "Mój sąsiad to straszny <głąb>, z którym nie da się normalnie porozmawiać.",
                            "target_lang_translations": {
                                "ru": "Мой сосед — ужасный <болван>, с которым невозможно нормально поговорить."
                            }
                        }
                    ],
                    "synonyms": [
                        "głupek",
                        "tępak",
                        "matoł",
                        "idiota",
                        "baran",
                        "cymbał"
                    ],
                    "antonyms": [
                        "mądrala",
                        "geniusz",
                        "inteligent"
                    ],
                    "common_phrases": [
                        "głąb kapuściany",
                        "skończony głąb",
                        "ciemny jak głąb"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оскорбительное определение человека, которого считают малоинтеллектуальным, несообразительным или глупым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тупица",
                                "target_lang_sense_clarification": "глупый человек"
                            },
                            {
                                "target_lang_word": "болван",
                                "target_lang_sense_clarification": "бестолковый человек"
                            },
                            {
                                "target_lang_word": "дубина",
                                "target_lang_sense_clarification": "разговорное, грубое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c584cc36-a5e0-317f-a9ab-b6b58c4207ba",
                    "sense_definition": "Twarda, środkowa łodyga znajdująca się wewnątrz główki kapusty.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plant_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy kroisz kapustę na gołąbki, musisz najpierw wyciąć twardy <głąb>.",
                            "target_lang_translations": {
                                "ru": "Когда режешь капусту на голубцы, нужно сначала вырезать твердую <кочерыжку>."
                            }
                        },
                        {
                            "text": "Dzieci dawniej lubiły chrupać surowy <głąb>, ponieważ jest słodki i soczysty.",
                            "target_lang_translations": {
                                "ru": "Раньше дети любили грызть сырую <кочерыжку>, потому что она сладкая и сочная."
                            }
                        }
                    ],
                    "synonyms": [
                        "kaczan"
                    ],
                    "common_phrases": [
                        "wyciąć głąb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый центральный стебель, находящийся внутри кочана капусты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кочерыжка",
                                "target_lang_sense_clarification": "стебель капусты"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "5ced342c-e506-3963-a694-2b12951d130b",
                    "sense_definition": "Polskie nazwisko męskie lub żeńskie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Głąb> został wybrany na przewodniczącego naszej wspólnoty mieszkaniowej.",
                            "target_lang_translations": {
                                "ru": "Господин <Гломб> был избран председателем нашего жилищного товарищества."
                            }
                        },
                        {
                            "text": "Na liście obecności widnieje nazwisko Anna <Głąb>.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих значится фамилия Анна <Гломб>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская мужская или женская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гломб",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fd2b85ce-b8b7-3576-bd2c-5c7e5b7a0af0",
                    "sense_definition": "Forma trybu rozkazującego (druga osoba liczby pojedynczej) czasownika <głębić>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <głąb> tego dołu bardziej, bo dokopiesz się do wody.",
                            "target_lang_translations": {
                                "ru": "Не <углубляй> эту яму сильнее, а то докопаешься до воды."
                            }
                        },
                        {
                            "text": "<Głąb> swoją wiedzę, czytając mądre książki każdego dnia.",
                            "target_lang_translations": {
                                "ru": "<Углубляй> свои знания, читая умные книги каждый день."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма повелительного наклонения (второе лицо единственного числа) глагола głębić."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "углубляй",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
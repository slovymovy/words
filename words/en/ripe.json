    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d7616302-770c-3b20-8988-de07e045d459",
                    "sense_definition": "Fully grown and ready to be harvested, eaten, or used (of fruit, grain, or vegetables).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "maturity_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bananas are yellow and <w>ripe</w> enough to eat.",
                            "target_lang_translations": {
                                "ru": "Бананы желтые и достаточно <w>спе́лые</w>, чтобы их есть."
                            }
                        },
                        {
                            "text": "Farmers wait until the wheat is fully <w>ripe</w> before harvesting it.",
                            "target_lang_translations": {
                                "ru": "Фермеры ждут, пока пшеница станет полностью <w>зре́лой</w>, прежде чем собирать урожай."
                            }
                        }

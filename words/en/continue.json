    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6077aa0d-7d50-3634-ad24-3f39aa77a431",
                    "sense_definition": "To keep doing an activity or to make it last longer without stopping.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "proceed_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>continue</w> with your work while I am away."
                        },
                        {
                            "text": "The rain is expected to <w>continue</w> for the rest of the day."
                        },
                        {
                            "text": "She wants to <w>continue</w> learning English until she is fluent."
                        }
                    ],
                    "synonyms": [
                        "carry on",
                        "keep on",
                        "go on with",
                        "proceed with",
                        "keep up"
                    ],
                    "antonyms": [
                        "stop",
                        "discontinue",
                        "terminate"
                    ],
                    "common_phrases": [

                {
                    "sense_id": "54daf1c8-43fe-3d5c-80e4-d4e92af0245e",
                    "sense_definition": "Datgene wat men gekocht heeft; het gekochte voorwerp.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader kwam zijn nieuwste <w>aankoop</w> trots aan me tonen.",
                            "target_lang_translations": {
                                "ru": "Мой отец пришел с гордостью показать мне свою новую <w>покупку</w>."
                            }
                        },
                        {
                            "text": "Deze dure jas was helaas een slechte <w>aankoop</w>.",
                            "target_lang_translations": {
                                "ru": "Это дорогое пальто, к сожалению, оказалось неудачной <w>покупкой</w>."
                            }

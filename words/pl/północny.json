{
    "word_family": [
        "północ"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4eb46394-90e1-3591-a72c-269a901275c7",
                    "sense_definition": "dotyczący północy jako strony świata, znajdujący się na północy lub pochodzący z tego kierunku",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi dziadkowie mieszkają w <północnej> części kraju.",
                            "target_lang_translations": {
                                "ru": "Мои дедушка и бабушка живут в <северной> части страны."
                            }
                        },
                        {
                            "text": "Zimny wiatr <północny> wiał przez całą noc.",
                            "target_lang_translations": {
                                "ru": "Холодный <северный> ветер дул всю ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "nordowy"
                    ],
                    "antonyms": [
                        "południowy"
                    ],
                    "common_phrases": [
                        "biegun północny",
                        "wiatr północny",
                        "Ameryka Północna",
                        "północna półkula"
                    ],
                    "target_lang_definitions": {
                        "ru": "относящийся к северу как стороне света, находящийся на севере или происходящий с этого направления"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "северный",
                                "target_lang_sense_clarification": "сторона света"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d123f84-1237-3abd-b874-69e604cb9d54",
                    "sense_definition": "odbywający się o północy (o godzinie 24:00) lub dotyczący tej pory nocy",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wróciłem do Warszawy <północnym> ekspresem z Krakowa.",
                            "target_lang_translations": {
                                "ru": "Я вернулся в Варшаву <полуночным> экспрессом из Кракова."
                            }
                        },
                        {
                            "text": "Czekałem na <północny> autobus prawie godzinę.",
                            "target_lang_translations": {
                                "ru": "Я ждал <полуночный> автобус почти час."
                            }
                        }
                    ],
                    "antonyms": [
                        "południowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "происходящий в полночь (в 24:00) или относящийся к этому времени суток"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полуночный",
                                "target_lang_sense_clarification": "время суток"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
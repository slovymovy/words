    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dc5e4579-4531-3be8-84fe-f05c03db328a",
                    "sense_definition": "Образовываться, возникать, составляться в единое целое.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В небе медленно <формировались> тёмные грозовые облака.",
                            "target_lang_translations": {
                                "en": "Dark storm clouds were slowly <forming> in the sky."
                            }
                        },
                        {
                            "text": "Цена на этот товар <формируется> в зависимости от спроса на рынке.",
                            "target_lang_translations": {
                                "en": "The price of this product <is formed> depending on market demand."
                            }
                        }
                    ],
                    "synonyms": [
                        "образовываться",
                        "создаваться",
                        "возникать"
                    ],
                    "antonyms": [
                        "разрушаться",
                        "распадаться"
                    ],
                    "common_phrases": [
                        "цена формируется",
                        "команда формируется",
                        "мнение формируется"
                    ],
                    "target_lang_definitions": {
                        "en": "To come into existence, arise, or be constituted into a single whole."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "form",
                                "target_lang_sense_clarification": "to come into existence or take a specific shape"
                            },
                            {
                                "target_lang_word": "take shape",
                                "target_lang_sense_clarification": "to become distinct or organized"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a93ac87-76a0-3468-a886-d571cf09e1f0",
                    "sense_definition": "Приобретать устойчивые черты, зрелость или определённые качества (о характере, личности, взглядах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "development",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Характер ребёнка <формируется> под влиянием родителей и школы.",
                            "target_lang_translations": {
                                "en": "A child's character <is formed> under the influence of parents and school."
                            }
                        },
                        {
                            "text": "Его политические взгляды <формировались> в течение многих лет.",
                            "target_lang_translations": {
                                "en": "His political views <took shape> over many years."
                            }
                        }
                    ],
                    "synonyms": [
                        "складываться",
                        "развиваться"
                    ],
                    "common_phrases": [
                        "характер формируется",
                        "личность формируется",
                        "взгляды формируются"
                    ],
                    "target_lang_definitions": {
                        "en": "To acquire stable traits, maturity, or specific qualities (regarding character, personality, or views)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "develop",
                                "target_lang_sense_clarification": "to grow or mature in character or mind"
                            },
                            {
                                "target_lang_word": "be formed",
                                "target_lang_sense_clarification": "to be molded or shaped by influence"
                            },
                            {
                                "target_lang_word": "take shape",
                                "target_lang_sense_clarification": "to become established (of views or ideas)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f0a595b-7920-3e80-b9b2-a5a9c332f48f",
                    "sense_definition": "Физически развиваться, расти и приобретать взрослые формы (об организме).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "development",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В подростковом возрасте организм человека активно <формируется>.",
                            "target_lang_translations": {
                                "en": "In adolescence, the human body actively <develops>."
                            }
                        },
                        {
                            "text": "Костная система у детей <формируется> постепенно и требует витаминов.",
                            "target_lang_translations": {
                                "en": "The skeletal system in children <forms> gradually and requires vitamins."
                            }
                        }
                    ],
                    "synonyms": [
                        "развиваться",
                        "расти"
                    ],
                    "common_phrases": [
                        "организм формируется",
                        "тело формируется"
                    ],
                    "target_lang_definitions": {
                        "en": "To develop physically, grow, and acquire adult forms (regarding an organism)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "develop",
                                "target_lang_sense_clarification": "to grow physically towards maturity"
                            },
                            {
                                "target_lang_word": "form",
                                "target_lang_sense_clarification": "to take physical structure (e.g., bones, organs)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
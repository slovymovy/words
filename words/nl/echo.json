    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cd6c6329-0268-3c81-9d5b-bd24f49f9169",
                    "sense_definition": "Een geluid dat tegen een oppervlak botst en terugkaatst, waardoor het opnieuw te horen is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sound_reflection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de grote, lege hal hoorde ik een duidelijke <w>echo</w> van mijn voetstappen.",
                            "target_lang_translations": {
                                "en": "In the large, empty hall, I heard a clear <w>echo</w> of my footsteps.",
                                "ru": "В большом пустом холле я услышал отчетливое <w>эхо</w> своих шагов."
                            }
                        },
                        {
                            "text": "De kinderen riepen hard in de tunnel om de <w>echo</w> te testen.",
                            "target_lang_translations": {
                                "en": "The children shouted loudly in the tunnel to test the <w>echo</w>.",
                                "ru": "Дети громко кричали в туннеле, чтобы проверить <w>эхо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "weergalm",
                        "weerkaatsing",
                        "nagalm"
                    ],
                    "common_phrases": [
                        "een echo horen",
                        "de echo van",
                        "als een echo"
                    ],
                    "target_lang_definitions": {
                        "en": "A sound that hits a surface and reflects, making it audible again.",
                        "ru": "Звук, который отражается от поверхности и возвращается обратно, становясь снова слышимым."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "echo",
                                "target_lang_sense_clarification": "sound reflection"
                            },
                            {
                                "target_lang_word": "reverberation",
                                "target_lang_sense_clarification": "prolonged sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "эхо",
                                "target_lang_sense_clarification": "акустическое явление"
                            },
                            {
                                "target_lang_word": "отголосок",
                                "target_lang_sense_clarification": "отраженный звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0456d0b1-a800-3845-bca8-414d6139e667",
                    "sense_definition": "Een medisch onderzoek waarbij met geluidsgolven een beeld van de binnenkant van het lichaam wordt gemaakt; kort voor echografie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_scan",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zwangere vrouw ging naar het ziekenhuis om een <w>echo</w> te laten maken.",
                            "target_lang_translations": {
                                "en": "The pregnant woman went to the hospital to have an <w>ultrasound</w> done.",
                                "ru": "Беременная женщина пошла в больницу, чтобы сделать <w>УЗИ</w>."
                            }
                        },
                        {
                            "text": "Op de <w>echo</w> konden de artsen zien dat de baby gezond was.",
                            "target_lang_translations": {
                                "en": "On the <w>ultrasound</w>, the doctors could see that the baby was healthy.",
                                "ru": "На <w>УЗИ</w> врачи увидели, что ребенок здоров."
                            }
                        }
                    ],
                    "synonyms": [
                        "echografie",
                        "echoscopie"
                    ],
                    "common_phrases": [
                        "een echo maken",
                        "op de echo",
                        "de 20-wekenecho"
                    ],
                    "target_lang_definitions": {
                        "en": "A medical examination using sound waves to create an image of the inside of the body; short for echography.",
                        "ru": "Медицинское обследование, при котором с помощью звуковых волн создается изображение внутренних органов; сокращение от эхографии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ultrasound",
                                "target_lang_sense_clarification": "medical scan"
                            },
                            {
                                "target_lang_word": "sonogram",
                                "target_lang_sense_clarification": "image produced by ultrasound"
                            },
                            {
                                "target_lang_word": "echo",
                                "target_lang_sense_clarification": "short for echocardiogram"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "УЗИ",
                                "target_lang_sense_clarification": "ультразвуковое исследование"
                            },
                            {
                                "target_lang_word": "ультразвук",
                                "target_lang_sense_clarification": "медицинская процедура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06041fe7-9742-3436-9578-c7676be994e2",
                    "sense_definition": "Een herhaald elektronisch signaal, vaak gebruikt in radar- of sonartechnologie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_reflection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De radar ving een zwakke <w>echo</w> op van het naderende vliegtuig.",
                            "target_lang_translations": {
                                "en": "The radar picked up a weak <w>echo</w> from the approaching aircraft.",
                                "ru": "Радар уловил слабое <w>эхо</w> от приближающегося самолета."
                            }
                        },
                        {
                            "text": "Door de storing was er een hinderlijke <w>echo</w> op de telefoonlijn te horen.",
                            "target_lang_translations": {
                                "en": "Due to the interference, an annoying <w>echo</w> could be heard on the phone line.",
                                "ru": "Из-за помех на телефонной линии было слышно раздражающее <w>эхо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "reflectie"
                    ],
                    "common_phrases": [
                        "radar echo"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A repeated electronic signal, often used in radar or sonar technology.",
                        "ru": "Повторяющийся электронный сигнал, часто используемый в радарных или гидролокационных технологиях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "echo",
                                "target_lang_sense_clarification": "reflected signal"
                            },
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "radar signal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "эхо-сигнал",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "эхо",
                                "target_lang_sense_clarification": "помехи или отражение сигнала"
                            }
                        ]
                    }
                }
            ]
        },
        {

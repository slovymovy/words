{
    "word_family": [
        "wedden",
        "weddenschap",
        "wedstrijd"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2e3b5ee6-894e-34f9-be34-0d485c6be9d4",
                    "sense_definition": "Vervoeging van <w>wedden</w>: eerste persoon enkelvoud tegenwoordige tijd, gebiedende wijs en tweede persoon enkelvoud (bij inversie).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>wed</w> dat het morgen gaat regenen.",
                            "target_lang_translations": {
                                "ru": "Я <w>держу пари</w>, что завтра пойдет дождь."
                            }
                        },
                        {
                            "text": "<w>Wed</w> jij wel eens op sportwedstrijden?",
                            "target_lang_translations": {
                                "ru": "Ты когда-нибудь <w>делаешь ставки</w> на спортивные состязания?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik wed dat",
                        "wed je mee"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола <w>wedden</w>: первое лицо единственного числа, повелительное наклонение или второе лицо единственного числа (при инверсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спорить",
                                "target_lang_sense_clarification": "заключать пари, утверждать с уверенностью"
                            },
                            {
                                "target_lang_word": "держать пари",
                                "target_lang_sense_clarification": "фразеологизм"
                            },
                            {
                                "target_lang_word": "делать ставки",
                                "target_lang_sense_clarification": "в азартных играх или спорте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "de621087-d46f-323e-957e-8af60866c54e",
                    "sense_definition": "Een ondiepe, doorwaadbare plaats in een rivier of beek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "water_crossing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ruiters staken de rivier over bij het <w>wed</w>.",
                            "target_lang_translations": {
                                "ru": "Всадники переправились через реку через <w>брод</w>."
                            }
                        },
                        {
                            "text": "Omdat de brug kapot was, moesten we het <w>wed</w> gebruiken.",
                            "target_lang_translations": {
                                "ru": "Так как мост был сломан, нам пришлось использовать <w>брод</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorde"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкое место в реке или ручье, через которое можно перейти или переехать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брод",
                                "target_lang_sense_clarification": "мелкое место реки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d18fe204-7fd5-3cd2-a5ab-44d2da08e70c",
                    "sense_definition": "Een drinkplaats voor dieren, vaak een ondiep stuk water.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "water_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer bracht de koeien naar het <w>wed</w> om te drinken.",
                            "target_lang_translations": {
                                "ru": "Фермер отвел коров на <w>водопой</w>, чтобы они попили."
                            }
                        },
                        {
                            "text": "In de zomer koelden de paarden af in het <w>wed</w>.",
                            "target_lang_translations": {
                                "ru": "Летом лошади охлаждались на <w>водопое</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wedde"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где животные пьют воду; часто мелководье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водопой",
                                "target_lang_sense_clarification": "место для питья животных"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "сообщение",
        "сообщаться"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "60e2cad0-64ca-31a3-a969-7933f303e1c4",
                    "sense_definition": "Передать информацию, довести до чьего-либо сведения, уведомить кого-либо о чём-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <сообщить> вам очень важную новость.",
                            "target_lang_translations": {
                                "en": "I want to <tell> you some very important news."
                            }
                        },
                        {
                            "text": "Пожалуйста, <сообщите> нам, когда вы приедете в город.",
                            "target_lang_translations": {
                                "en": "Please <let> us <know> when you arrive in the city."
                            }
                        },
                        {
                            "text": "В новостях <сообщили> о результатах футбольного матча.",
                            "target_lang_translations": {
                                "en": "The news <reported> the results of the football match."
                            }
                        }
                    ],
                    "synonyms": [
                        "проинформировать",
                        "уведомить",
                        "оповестить",
                        "передать",
                        "рассказать"
                    ],
                    "antonyms": [
                        "утаить",
                        "скрыть"
                    ],
                    "common_phrases": [
                        "сообщить новость",
                        "сообщить подробности",
                        "сообщить в полицию",
                        "сообщить по секрету",
                        "сообщить о решении"
                    ],
                    "target_lang_definitions": {
                        "en": "To convey information, bring to someone's attention, or notify someone about something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inform",
                                "target_lang_sense_clarification": "to officially tell someone about something"
                            },
                            {
                                "target_lang_word": "report",
                                "target_lang_sense_clarification": "to give a spoken or written account of something"
                            },
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "general word for communicating information"
                            },
                            {
                                "target_lang_word": "let know",
                                "target_lang_sense_clarification": "common phrase meaning to inform"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bff0b86-5011-3906-8f79-14950cd01df8",
                    "sense_definition": "Придать чему-либо какие-либо свойства, качества или характер (обычно в книжной или технической речи).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "imparting_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мощный двигатель <сообщил> ракете необходимую для взлёта скорость.",
                            "target_lang_translations": {
                                "en": "The powerful engine <imparted> the necessary takeoff speed to the rocket."
                            }
                        },
                        {
                            "text": "Это событие <сообщило> нашей встрече особую торжественность.",
                            "target_lang_translations": {
                                "en": "This event <imparted> a special solemnity to our meeting."
                            }
                        }
                    ],
                    "synonyms": [
                        "придать"
                    ],
                    "common_phrases": [
                        "сообщить ускорение",
                        "сообщить скорость",
                        "сообщить вид",
                        "сообщить импульс"
                    ],
                    "target_lang_definitions": {
                        "en": "To impart properties, qualities, or character to something (usually in bookish or technical contexts)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "impart",
                                "target_lang_sense_clarification": "to bestow a quality or physical quantity (like speed)"
                            },
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "to provide a quality or characteristic"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
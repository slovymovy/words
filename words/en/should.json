    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "45e36543-c585-37de-8942-552b549d5b5d",
                    "sense_definition": "Used to give advice, make recommendations, or express what is the right or best thing to do.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "obligation_advice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You <w>should</w> go to the doctor if you still feel sick tomorrow."
                        },
                        {
                            "text": "Drivers <w>should</w> always wear a seat belt for their own safety."
                        },
                        {
                            "text": "What <w>should</w> I do about the broken window in the kitchen?"
                        }
                    ],
                    "synonyms": [
                        "ought"
                    ],
                    "antonyms": [
                        "shouldn't"
                    ],
                    "common_phrases": [
                        "you should go",
                        "should I do",
                        "you should try",
                        "I should say"
                    ]
                },
                {
                    "sense_id": "0d644588-253e-3690-ad9c-806f5c9f0ca7",
                    "sense_definition": "Used to indicate that something is likely to happen or is expected to be the case.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "probability_expectation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The package <w>should</w> be here by Friday afternoon if there are no delays."
                        },
                        {
                            "text": "It <w>should</w> be enough food for everyone coming to the party."
                        },
                        {
                            "text": "They <w>should</w> have finished the project by now."
                        }
                    ],
                    "synonyms": [
                        "ought"
                    ],
                    "common_phrases": [
                        "should be enough",
                        "it should be",
                        "should have finished",
                        "should be fine"
                    ]
                },
                {
                    "sense_id": "037ee025-ed15-3da5-b504-4e99cdd80209",
                    "sense_definition": "Used to emphasize that something is very impressive, surprising, or shocking.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You <w>should</w> see the view from the top of the mountain; it is breathtaking."
                        },
                        {
                            "text": "If you think that is loud, you <w>should</w> hear him play the drums."
                        }
                    ],
                    "common_phrases": [
                        "you should see",
                        "you should hear"
                    ]
                },
                {
                    "sense_id": "a7f94e2d-7b51-37a9-b13b-7607346be305",
                    "sense_definition": "Used to express a condition, possibility, or polite suggestion, often in formal contexts.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "conditional_formal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Should</w> you need any further assistance, please do not hesitate to contact us."
                        },
                        {
                            "text": "I <w>should</w> think that he would apologize after making such a mistake."
                        },
                        {
                            "text": "If I <w>should</w> be late, please start the meeting without me."
                        }
                    ],
                    "common_phrases": [
                        "should you need",
                        "I should think",
                        "if I should"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ]
                },
                {
                    "sense_id": "d780aac4-7958-3f56-90e5-fb2f1ac0785f",
                    "sense_definition": "Used as the past form of 'shall' in reported speech.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reported_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I told him that I <w>should</w> be busy all day tomorrow."
                        },
                        {
                            "text": "She said that we <w>should</w> receive the letter by the end of the week."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often replaced by 'would' in modern usage"
                        }
                    ]
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "35beef20-6bfe-3782-8c33-0479dc3a75a0",
                    "sense_definition": "A statement or thought about what ought to happen, often contrasted with reality.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "psychological_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to let go of the <w>shoulds</w> and focus on what is actually possible."
                        },
                        {
                            "text": "Her life was ruled by a long list of <w>shoulds</w> imposed by her parents."
                        }
                    ],
                    "common_phrases": [
                        "list of shoulds",
                        "the shoulds and"
                    ]
                }
            ]
        }
    ]

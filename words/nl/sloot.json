                    "common_phrases": [
                        "in de sloot",
                        "over de sloot",
                        "slootje springen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкий искусственный канал, вырытый для отвода воды или разделения лугов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канава",
                                "target_lang_sense_clarification": "дренажная или сточная"
                            },
                            {
                                "target_lang_word": "ров",
                                "target_lang_sense_clarification": "разделительный или глубокий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2becfe42-63b9-3e25-8c2b-593d886ca04d",
                    "sense_definition": "Een aanzienlijke, grote hoeveelheid (meestal van een vloeistof).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4a0ee8a5-262c-30a9-acf0-941574a1e40c",
                    "sense_definition": "To move steadily and continuously in a current or stream, usually describing liquids, gases, or electricity.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fluid_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The river <w>flows</w> all the way to the ocean.",
                            "target_lang_translations": {
                                "ru": "Река <w>течёт</w> до самого океана."
                            }
                        },
                        {
                            "text": "Blood <w>flows</w> through your veins to keep you alive.",
                            "target_lang_translations": {
                                "ru": "Кровь <w>течёт</w> по венам, поддерживая в вас жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "run",
                        "stream",
                        "course",
                        "rush"
                    ],
                    "antonyms": [
                        "halt"
                    ],
                    "common_phrases": [
                        "flow freely",
                        "flow into",
                        "flow out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться равномерно и непрерывно в потоке (обычно о жидкостях, газах или электричестве)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "двигаться потоком (о жидкости)"
                            },
                            {
                                "target_lang_word": "струиться",
                                "target_lang_sense_clarification": "течь плавной или тонкой струей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c3be229-ac1d-33e2-bd15-eacd51ebf589",
                    "sense_definition": "To proceed, move, or be produced smoothly, gracefully, and without interruption.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_smoothness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The conversation <w>flowed</w> naturally for hours.",
                            "target_lang_translations": {
                                "ru": "Беседа естественно <w>текла</w> в течение нескольких часов."
                            }
                        },
                        {
                            "text": "Traffic is <w>flowing</w> smoothly on the highway today.",
                            "target_lang_translations": {
                                "ru": "Транспортный поток сегодня <w>движется</w> по шоссе плавно."
                            }
                        }
                    ],
                    "synonyms": [
                        "glide"
                    ],
                    "antonyms": [
                        "stutter"
                    ],
                    "common_phrases": [
                        "flow smoothly",
                        "flow together"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить, двигаться или производиться плавно, грациозно и без перерывов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "протекать плавно (о беседе, времени)"
                            },
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "развиваться или двигаться (о процессе, транспорте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e10523b0-09de-37c3-80d5-f34b4103f208",
                    "sense_definition": "To hang loosely and wave gently.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hanging_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her long hair <w>flowed</w> down her back in soft waves.",
                            "target_lang_translations": {
                                "ru": "Её длинные волосы мягкими волнами <w>струились</w> по спине."
                            }
                        },
                        {
                            "text": "The queen wore a dress with a long cape that <w>flowed</w> behind her.",
                            "target_lang_translations": {
                                "ru": "Королева была в платье с длинной накидкой, которая <w>струилась</w> позади неё."
                            }
                        }
                    ],
                    "synonyms": [
                        "stream"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободно свисать и мягко колыхаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "струиться",
                                "target_lang_sense_clarification": "ниспадать волнами (о волосах, ткани)"
                            },
                            {
                                "target_lang_word": "развеваться",
                                "target_lang_sense_clarification": "колыхаться на ветру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cc95e4f-971f-35dc-aa8a-d811ac0f0f1f",
                    "sense_definition": "To rise and move towards the land (used of the tide).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "tide_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tide <w>flows</w> and ebbs twice every day.",
                            "target_lang_translations": {
                                "ru": "Вода <w>приливает</w> и отливает дважды в день."
                            }
                        },
                        {
                            "text": "We must wait until the water <w>flows</w> back into the bay.",
                            "target_lang_translations": {
                                "ru": "Мы должны подождать, пока вода <w>прильёт</w> обратно в бухту."
                            }
                        }
                    ],
                    "antonyms": [
                        "ebb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подниматься и двигаться к суше (о приливе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приливать",
                                "target_lang_sense_clarification": "прибывать (о воде)"
                            },
                            {
                                "target_lang_word": "подниматься",
                                "target_lang_sense_clarification": "увеличиваться в уровне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fd73b5f-febb-375a-9dd7-dd728dd5a59d",
                    "sense_definition": "To come from a source or be caused by something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emanate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Great wealth often <w>flows</w> from hard work and innovation.",
                            "target_lang_translations": {
                                "ru": "Большое богатство часто <w>вытекает</w> из упорного труда и инноваций."
                            }
                        },
                        {
                            "text": "Many benefits <w>flow</w> from living a healthy lifestyle.",
                            "target_lang_translations": {
                                "ru": "Из здорового образа жизни <w>вытекает</w> много преимуществ."
                            }
                        }
                    ],
                    "synonyms": [
                        "come in"
                    ],
                    "common_phrases": [
                        "flow from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить из источника или быть вызванным чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытекать",
                                "target_lang_sense_clarification": "быть следствием чего-либо"
                            },
                            {
                                "target_lang_word": "проистекать",
                                "target_lang_sense_clarification": "брать начало, происходить"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "zachodni",
        "zachodzić"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "99f22ed3-602f-3851-bd7f-c78a6be57fc9",
                    "sense_definition": "Jedna z czterech głównych stron świata, wskazująca kierunek, w którym zachodzi słońce; przeciwieństwo wschodu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_astronomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy iść cały czas na <w>zachód</w>, żeby dotrzeć do miasta.",
                            "target_lang_translations": {
                                "ru": "Мы должны всё время идти на <w>запад</w>, чтобы добраться до города."
                            }
                        },
                        {
                            "text": "Wiatr zmienił kierunek i teraz wieje z <w>zachodu</w>.",
                            "target_lang_translations": {
                                "ru": "Ветер сменил направление и теперь дует с <w>запада</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "wschód"
                    ],
                    "common_phrases": [
                        "na zachód",
                        "na zachodzie",
                        "z zachodu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из четырех главных сторон света, указывающая направление, где заходит солнце; противоположность востоку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за́пад",
                                "target_lang_sense_clarification": "сторона света"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0424d1c8-5dfd-30fa-976d-fc6cf0733998",
                    "sense_definition": "Moment dnia, w którym słońce chowa się za horyzontem, kończąc dzień.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction_astronomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem <w>zachód</w> słońca następuje bardzo późno.",
                            "target_lang_translations": {
                                "ru": "Летом <w>закат</w> солнца наступает очень поздно."
                            }
                        },
                        {
                            "text": "Lubię spacerować po plaży tuż przed <w>zachodem</w>.",
                            "target_lang_translations": {
                                "ru": "Я люблю гулять по пляжу прямо перед <w>закатом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmierzch",
                        "zmrok"
                    ],
                    "antonyms": [
                        "wschód"
                    ],
                    "common_phrases": [
                        "zachód słońca",
                        "o zachodzie",
                        "przed zachodem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время суток, когда солнце скрывается за горизонтом, завершая день."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зака́т",
                                "target_lang_sense_clarification": "время суток, явление"
                            },
                            {
                                "target_lang_word": "захо́д",
                                "target_lang_sense_clarification": "исчезновение светила за горизонтом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f39bde72-93f6-3d58-ab4f-e651ac883c4c",
                    "sense_definition": "Duży wysiłek, starania lub kłopot związany z załatwianiem jakiejś sprawy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "effort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naprawa tego starego samochodu nie jest warta <w>zachodu</w>.",
                            "target_lang_translations": {
                                "ru": "Ремонт этой старой машины не стоит <w>хлопот</w>."
                            }
                        },
                        {
                            "text": "Przygotowanie tej uroczystości wymagało od nas wiele <w>zachodu</w>.",
                            "target_lang_translations": {
                                "ru": "Подготовка этого торжества потребовала от нас много <w>хлопот</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szkoda zachodu",
                        "niewarte zachodu",
                        "dużo zachodu",
                        "bez zachodu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большие усилия, старания или хлопоты, связанные с решением какого-либо дела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хло́поты",
                                "target_lang_sense_clarification": "заботы, затруднения"
                            },
                            {
                                "target_lang_word": "возня́",
                                "target_lang_sense_clarification": "разг. суета, хлопотное дело"
                            },
                            {
                                "target_lang_word": "уси́лия",
                                "target_lang_sense_clarification": "старания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "bb5284d2-2a76-347a-90a4-b6840b3644fb",
                    "sense_definition": "Określenie krajów Europy Zachodniej, Ameryki Północnej i innych państw należących do tego kręgu kulturowego i politycznego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geopolitics",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Kultura <w>Zachodu</w> miała duży wpływ na rozwój tego regionu.",
                            "target_lang_translations": {
                                "ru": "Культура <w>Запада</w> оказала большое влияние на развитие этого региона."
                            }
                        },
                        {
                            "text": "Wielu młodych ludzi wyjeżdża na <w>Zachód</w> w poszukiwaniu lepszej pracy.",
                            "target_lang_translations": {
                                "ru": "Многие молодые люди уезжают на <w>Запад</w> в поисках лучшей работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "Okcydent"
                    ],
                    "antonyms": [
                        "Wschód"
                    ],
                    "common_phrases": [
                        "kraje Zachodu",
                        "cywilizacja Zachodu",
                        "Dzikim Zachód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначение стран Западной Европы, Северной Америки и других государств, принадлежащих к этому культурному и политическому кругу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "За́пад",
                                "target_lang_sense_clarification": "геополитика"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
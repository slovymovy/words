            "senses": [
                {
                    "sense_id": "e1753a9e-ea54-3ce6-a97c-f0c291b42527",
                    "sense_definition": "De lege ruimte aan de rand van een bladzijde waar geen tekst staat.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er stond een opmerking in de <w>marge</w>.",
                            "target_lang_translations": {
                                "ru": "На <w>полях</w> было примечание.",
                                "en": "There was a note in the <w>margin</w>."
                            }
                        },
                        {
                            "text": "De leraar schreef de correcties in de <w>marge</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель написал исправления на <w>полях</w>.",
                                "en": "The teacher wrote the corrections in the <w>margin</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kantlijn"
                    ],
                    "common_phrases": [
                        "in de marge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пустое пространство по краям страницы, где нет текста.",
                        "en": "The empty space at the edge of a page where there is no text."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поле",
                                "target_lang_sense_clarification": "страницы"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "margin",
                                "target_lang_sense_clarification": "page edge"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "debdebf3-2594-3d54-be54-b4856b02c272",
                    "sense_definition": "De ruimte of vrijheid om te handelen of af te wijken binnen bepaalde grenzen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is niet veel <w>marge</w> in deze zaak.",
                            "target_lang_translations": {
                                "ru": "В этом деле мало <w>свободы действий</w>.",
                                "en": "There is not much <w>leeway</w> in this matter."
                            }
                        },
                        {
                            "text": "We hebben een veilige <w>marge</w> ingebouwd voor onvoorziene kosten.",
                            "target_lang_translations": {
                                "ru": "Мы заложили надежный <w>запас</w> на непредвиденные расходы.",
                                "en": "We have built in a safe <w>margin</w> for unforeseen costs."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een veilige marge",
                        "ruime marge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пространство или свобода действий в определенных границах.",
                        "en": "The space or freedom to act or deviate within certain limits."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запас",
                                "target_lang_sense_clarification": "резерв"
                            },
                            {
                                "target_lang_word": "свобода действий",
                                "target_lang_sense_clarification": "возможность маневра"
                            }
                        ],
                        "en": [

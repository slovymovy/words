{
    "word_family": [
        "eenheid",
        "enig",
        "eenzaam",
        "eens",
        "verenigen",
        "eenerlei"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "12bd214e-4d32-3d90-b96a-b7b91d184dbc",
                    "sense_definition": "Gebruikt om een specifiek, enkel exemplaar aan te duiden, vaak met nadruk of in tegenstelling tot andere.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "specific_contrastive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Weet je nog die <w>ene</w> keer dat we samen naar het strand gingen?",
                            "target_lang_translations": {
                                "en": "Do you remember that <w>one</w> time we went to the beach together?",
                                "ru": "Помнишь тот <w>один</w> раз, когда мы вместе ездили на пляж?"
                            }
                        },
                        {
                            "text": "Aan de <w>ene</w> kant wil ik graag mee, maar aan de andere kant heb ik geen tijd.",
                            "target_lang_translations": {
                                "en": "On the <w>one</w> hand I would like to go, but on the other hand I don't have time.",
                                "ru": "С <w>одной</w> стороны, я хочу пойти, но с другой стороны, у меня нет времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "specifieke",
                        "betreffende"
                    ],
                    "antonyms": [
                        "andere"
                    ],
                    "common_phrases": [
                        "aan de ene kant",
                        "die ene keer",
                        "de ene"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to single out a specific instance, often for emphasis or contrast.",
                        "ru": "Используется для обозначения конкретного, единичного экземпляра, часто с акцентом или в противопоставлении другим."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "one",
                                "target_lang_sense_clarification": "specific or contrastive"
                            },
                            {
                                "target_lang_word": "particular",
                                "target_lang_sense_clarification": "specific instance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "один",
                                "target_lang_sense_clarification": "конкретный, тот самый"
                            },
                            {
                                "target_lang_word": "тот самый",
                                "target_lang_sense_clarification": "усилительное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0b9477a-699b-3f2e-9456-811e8cb66717",
                    "sense_definition": "Een zekere; gebruikt voor een persoonsnaam om aan te geven dat de persoon onbekend is bij de spreker.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "indefinite_unknown",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ene</w> meneer Jansen heeft vanmorgen gebeld voor een afspraak.",
                            "target_lang_translations": {
                                "en": "<w>A certain</w> Mr. Jansen called this morning for an appointment.",
                                "ru": "<w>Некий</w> господин Янсен звонил сегодня утром, чтобы договориться о встрече."
                            }
                        },
                        {
                            "text": "Het pakketje werd bezorgd door <w>ene</w> Pieter, die ik verder niet ken.",
                            "target_lang_translations": {
                                "en": "The package was delivered by <w>some</w> Pieter, whom I don't know otherwise.",
                                "ru": "Посылку доставил <w>некий</w> Питер, которого я совсем не знаю."
                            }
                        }
                    ],
                    "synonyms": [
                        "zekere"
                    ],
                    "common_phrases": [
                        "ene meneer",
                        "ene mevrouw"
                    ],
                    "target_lang_definitions": {
                        "en": "Used before a name to indicate a person unknown to the speaker.",
                        "ru": "Некий; используется перед именем человека, чтобы указать, что он неизвестен говорящему."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "a certain",
                                "target_lang_sense_clarification": "unspecified person"
                            },
                            {
                                "target_lang_word": "some",
                                "target_lang_sense_clarification": "unknown person"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "некий",
                                "target_lang_sense_clarification": "перед именем неизвестного лица"
                            },
                            {
                                "target_lang_word": "какой-то",
                                "target_lang_sense_clarification": "разговорное, неопределенное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "64011836-dff8-3a0c-b236-ed1778d1bb71",
                    "sense_definition": "De ene persoon of zaak in een reeks of vergelijking, vaak gevolgd door 'de andere'.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "specific_contrastive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil je die <w>ene</w> hebben, of toch liever die andere?",
                            "target_lang_translations": {
                                "en": "Do you want to have that <w>one</w>, or would you prefer that other one?",
                                "ru": "Ты хочешь <w>тот</w>, или всё-таки другой?"
                            }
                        },
                        {
                            "text": "De <w>ene</w> werkt hard, terwijl de ander liever de hele dag luiert.",
                            "target_lang_translations": {
                                "en": "The <w>one</w> works hard, while the other prefers to laze around all day.",
                                "ru": "<w>Один</w> работает усердно, в то время как другой предпочитает бездельничать весь день."
                            }
                        }
                    ],
                    "antonyms": [
                        "andere"
                    ],
                    "common_phrases": [
                        "de ene en de andere"
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to a specific person or thing in a comparison, usually contrasted with another.",
                        "ru": "Один (человек или предмет) в ряду или сравнении, часто в сочетании с «другой»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "one",
                                "target_lang_sense_clarification": "in contrast to another"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "один",
                                "target_lang_sense_clarification": "в паре с «другой»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "adb79c5e-50a4-30f4-85cc-849bf71e5676",
                    "sense_definition": "Vorm van <w>één</w> die gebruikt wordt bij ritmisch tellen of om nadruk te leggen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rhythmic_counting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen speelden verstoppertje en telden hardop: <w>ene</w>, tweeë, drieë.",
                            "target_lang_translations": {
                                "en": "The children played hide-and-seek and counted out loud: <w>one</w>, two, three.",
                                "ru": "Дети играли в прятки и считали вслух: <w>раз</w>, два, три."
                            }
                        },
                        {
                            "text": "Ze stond de hele tijd op haar <w>ene</w> been te balanceren.",
                            "target_lang_translations": {
                                "en": "She stood balancing on her <w>one</w> leg the whole time.",
                                "ru": "Она всё время балансировала на <w>одной</w> ноге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ene tweeë drieë"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rhythmic or emphatic"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A variation of 'one' used for rhythm or emphasis.",
                        "ru": "Форма числительного «один», используемая при ритмическом счете или для ударения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "one",
                                "target_lang_sense_clarification": "emphatic or rhythmic"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раз",
                                "target_lang_sense_clarification": "при счете"
                            },
                            {
                                "target_lang_word": "один",
                                "target_lang_sense_clarification": "числительное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "263b1f79-f2ba-3b17-b298-aad0a8fb9f92",
                    "sense_definition": "Verouderde verbuiging van het onbepaalde lidwoord <w>een</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude teksten leest men soms over <w>ene</w> grote onderneming.",
                            "target_lang_translations": {
                                "en": "In old texts one sometimes reads about <w>a</w> large undertaking.",
                                "ru": "В старых текстах иногда читают о <w>неком</w> крупном предприятии."
                            }
                        },
                        {
                            "text": "Hij sprak met <w>ene</w> deftigheid die niet meer van deze tijd is.",
                            "target_lang_translations": {
                                "en": "He spoke with <w>a</w> dignity that is no longer of this time.",
                                "ru": "Он говорил с <w>некой</w> важностью, которая уже не свойственна нашему времени."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An archaic or formal form of the indefinite article.",
                        "ru": "Устаревшая форма неопределенного артикля."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "a",
                                "target_lang_sense_clarification": "indefinite article"
                            },
                            {
                                "target_lang_word": "an",
                                "target_lang_sense_clarification": "indefinite article"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "некий",
                                "target_lang_sense_clarification": "архаичное или книжное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
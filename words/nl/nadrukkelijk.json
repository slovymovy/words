            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1b28d5e7-241c-3c95-9abb-53fee6db7ce2",
                    "sense_definition": "Met klem of nadruk; heel duidelijk en beslist.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emphatic_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur gaf een <w>nadrukkelijke</w> waarschuwing aan het personeel.",
                            "target_lang_translations": {
                                "ru": "Директор сделал <w>строгое</w> предупреждение персоналу."
                            }
                        },
                        {
                            "text": "Ondanks zijn <w>nadrukkelijke</w> aanwezigheid werd hij door niemand aangesproken.",
                            "target_lang_translations": {
                                "ru": "Несмотря на его <w>явное</w> присутствие, никто к нему не обратился."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitdrukkelijk",
                        "beslist",
                        "duidelijk"
                    ],
                    "antonyms": [
                        "onnadrukkelijk"
                    ],
                    "common_phrases": [
                        "een nadrukkelijke waarschuwing",
                        "nadrukkelijk aanwezig"
                    ],
                    "target_lang_definitions": {
                        "ru": "С нажимом, решительный или очень ясный; не оставляющий сомнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настойчивый",
                                "target_lang_sense_clarification": "упорный, решительный"
                            },
                            {
                                "target_lang_word": "категорический",
                                "target_lang_sense_clarification": "безапелляционный, решительный"
                            },
                            {
                                "target_lang_word": "явный",
                                "target_lang_sense_clarification": "очевидный, заметный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "761ef766-240d-31aa-8768-d68ec93f87a5",
                    "sense_definition": "Op een manier die duidelijk maakt dat men het meent; met klem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emphatic_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <w>nadrukkelijk</w> gevraagd om de deur dicht te doen.",
                            "target_lang_translations": {
                                "ru": "Я <w>настойчиво</w> просил закрыть дверь."
                            }
                        },
                        {
                            "text": "Het is <w>nadrukkelijk</w> verboden om hier te roken.",
                            "target_lang_translations": {
                                "ru": "Здесь <w>строго</w> запрещено курить."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitdrukkelijk",
                        "duidelijk"
                    ],
                    "antonyms": [
                        "onnadrukkelijk"
                    ],
                    "common_phrases": [
                        "nadrukkelijk vragen",
                        "nadrukkelijk verboden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво, ясно давая понять важность; категорически."
                    },
                    "translations": {
                        "ru": [
                            {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "47009660-64c5-37ac-8450-f09dd02a829d",
                    "sense_definition": "Duży drapieżny ssak z rodziny kotowatych, charakteryzujący się żółtobrązową sierścią w czarne, poprzeczne pręgi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj podczas wycieczki do zoo widzieliśmy ogromnego <w>tygrysa</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера во время экскурсии в зоопарк мы видели огромного <w>тигра</w>."
                            }
                        },
                        {
                            "text": "Ten drapieżny <w>tygrys</w> poluje głównie w nocy.",
                            "target_lang_translations": {
                                "ru": "Этот хищный <w>тигр</w> охотится в основном ночью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tygrys bengalski",
                        "drapieżny tygrys",
                        "walczyć jak tygrys",
                        "papierowy tygrys"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное хищное млекопитающее семейства кошачьих, отличающееся желтовато-бурой шерстью с черными поперечными полосами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тигр",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b66f6375-7301-39c1-b206-483f10d06805",
                    "sense_definition": "Państwo lub region charakteryzujący się bardzo szybkim wzrostem gospodarczym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "economy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Korea Południowa była przez lata nazywana azjatyckim <w>tygrysem</w>.",
                            "target_lang_translations": {
                                "ru": "Южную Корею годами называли азиатским <w>тигром</w>."
                            }
                        },
                        {
                            "text": "Eksperci zastanawiają się, który kraj będzie nowym gospodarczym <w>tygrysem</w> Europy.",
                            "target_lang_translations": {
                                "ru": "Эксперты гадают, какая страна станет новым экономическим <w>тигром</w> Европы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "azjatyckie tygrysy",
                        "gospodarczy tygrys",
                        "wschodzące tygrysy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in economic contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Государство или регион, характеризующиеся очень быстрым экономическим ростом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тигр",
                                "target_lang_sense_clarification": "экономика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2122be65-9677-3b74-ab64-050f808f846d",
                    "sense_definition": "Niemiecki czołg ciężki używany przez Wehrmacht podczas II wojny światowej.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum broni pancernej stoi odrestaurowany <w>tygrys</w> z czasów wojny.",
                            "target_lang_translations": {
                                "ru": "В музее бронетанковой техники стоит отреставрированный <w>Тигр</w> времен войны."
                            }
                        },
                        {
                            "text": "Pojawienie się niemieckiego <w>tygrysa</w> na polu bitwy budziło strach.",
                            "target_lang_translations": {
                                "ru": "Появление немецкого <w>Тигра</w> на поле битвы внушало страх."
                            }
                        }
                    ],
                    "common_phrases": [

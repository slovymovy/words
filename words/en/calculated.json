    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "36f928ff-9f2f-3562-bfc3-7889ed489984",
                    "sense_definition": "Simple past and past participle of <w>calculate</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>calculated</w> the total cost of the trip before booking the tickets.",
                            "target_lang_translations": {
                                "ru": "Она <w>рассчитала</w> общую стоимость поездки перед бронированием билетов."
                            }
                        },
                        {
                            "text": "We have <w>calculated</w> that the project will take approximately three months to complete.",
                            "target_lang_translations": {
                                "ru": "Мы <w>подсчитали</w>, что завершение проекта займет примерно три месяца."
                            }
                        }
                    ],
                    "synonyms": [
                        "computed",
                        "reckoned",
                        "worked out"
                    ],
                    "antonyms": [
                        "guessed",
                        "estimated"
                    ],
                    "common_phrases": [
                        "calculated the cost",
                        "calculated the distance",
                        "calculated the risk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола calculate (вычислять, рассчитывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассчитал",
                                "target_lang_sense_clarification": "произвел расчет"
                            },
                            {
                                "target_lang_word": "вычислил",
                                "target_lang_sense_clarification": "определил математически"
                            },
                            {
                                "target_lang_word": "подсчитал",
                                "target_lang_sense_clarification": "сосчитал количество или итог"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0aca68fd-72e1-37d2-b9f9-438f4484a452",
                    "sense_definition": "Carefully thought out or planned in order to achieve a specific effect or result.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "planned_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Taking the new job was a <w>calculated</w> risk that eventually paid off for him.",
                            "target_lang_translations": {
                                "ru": "Переход на новую работу был <w>взвешенным</w> риском, который в итоге оправдался."
                            }
                        },
                        {
                            "text": "The politician made a <w>calculated</w> decision to avoid answering the difficult question.",
                            "target_lang_translations": {
                                "ru": "Политик принял <w>продуманное</w> решение уйти от ответа на сложный вопрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "deliberate",
                        "intentional",
                        "premeditated",
                        "planned"
                    ],
                    "antonyms": [
                        "spontaneous",
                        "unintentional",
                        "accidental"
                    ],
                    "common_phrases": [
                        "calculated risk",
                        "calculated decision",
                        "calculated move",
                        "calculated insult"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщательно продуманный или спланированный для достижения определенного результата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продуманный",
                                "target_lang_sense_clarification": "спланированный заранее"
                            },
                            {
                                "target_lang_word": "взвешенный",
                                "target_lang_sense_clarification": "обдуманный, например, о риске"
                            },
                            {
                                "target_lang_word": "преднамеренный",
                                "target_lang_sense_clarification": "сделанный с умыслом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "646aa44d-8ec2-354b-a284-3b1b14260a5e",
                    "sense_definition": "Arrived at or determined by using mathematics or logic.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>calculated</w> value was slightly different from the one we measured in the lab.",
                            "target_lang_translations": {
                                "ru": "<w>Расчетное</w> значение немного отличалось от того, что мы измерили в лаборатории."
                            }
                        },
                        {
                            "text": "Please check if the <w>calculated</w> total includes the sales tax.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте, включает ли <w>вычисленная</w> итоговая сумма налог с продаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "computed",
                        "reckoned"
                    ],
                    "antonyms": [
                        "estimated"
                    ],
                    "common_phrases": [
                        "calculated value",
                        "calculated amount",
                        "calculated total"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полученный или определенный с помощью математических вычислений или логики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расчетный",
                                "target_lang_sense_clarification": "теоретический, полученный расчетом"
                            },
                            {
                                "target_lang_word": "вычисленный",
                                "target_lang_sense_clarification": "найденный путем вычислений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fc6bf53-a3dd-3845-94bd-2c813ca80fa6",
                    "sense_definition": "Acting in a cool, analytical, and unemotional way, often to serve one's own interests.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "planned_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His response was cold and <w>calculated</w>, showing no sign of empathy for the victims.",
                            "target_lang_translations": {
                                "ru": "Его ответ был холодным и <w>расчетливым</w>, не выказывающим никакого сочувствия к жертвам."
                            }
                        },
                        {
                            "text": "She is a very <w>calculated</w> person who never acts without a strategy.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>расчетливый</w> человек, который никогда не действует без стратегии."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheming",
                        "manipulative",
                        "cold"
                    ],
                    "antonyms": [

{
    "word_family": [
        "renewal",
        "renewable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8460f233-6903-3e78-9846-91629c523ea0",
                    "sense_definition": "To extend the period of time for which something is valid, such as a license, contract, or loan.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "extend_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to go to the office to <w>renew</w> my passport before our trip.",
                            "target_lang_translations": {
                                "ru": "Мне нужно зайти в офис, чтобы <w>продлить</w> паспорт перед нашей поездкой."
                            }
                        },
                        {
                            "text": "You can easily <w>renew</w> your subscription online with a credit card.",
                            "target_lang_translations": {
                                "ru": "Вы можете легко <w>продлить</w> подписку онлайн с помощью кредитной карты."
                            }
                        }
                    ],
                    "synonyms": [
                        "renewal",
                        "renewal"
                    ],
                    "common_phrases": [
                        "renew a passport",
                        "renew a contract",
                        "renew a subscription",
                        "renew a license"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продлевать срок действия чего-либо, например, лицензии, договора или кредита."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продлева́ть",
                                "target_lang_sense_clarification": "увеличивать срок действия (несов. вид)"
                            },
                            {
                                "target_lang_word": "продли́ть",
                                "target_lang_sense_clarification": "увеличить срок действия (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c208326-cb64-31ee-b61d-bc4d85fe1563",
                    "sense_definition": "To start something again, often with fresh energy or determination, after an interruption.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "start_again",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police decided to <w>renew</w> the investigation after finding new evidence.",
                            "target_lang_translations": {
                                "ru": "Полиция решила <w>возобновить</w> расследование после обнаружения новых улик."
                            }
                        },
                        {
                            "text": "They met for coffee to <w>renew</w> their friendship after years of being apart.",
                            "target_lang_translations": {
                                "ru": "Они встретились выпить кофе, чтобы <w>возобновить</w> дружбу после долгих лет разлуки."
                            }
                        }
                    ],
                    "synonyms": [
                        "renewal",
                        "renewal",
                        "renewal"
                    ],
                    "antonyms": [
                        "discontinue",
                        "stop"
                    ],
                    "common_phrases": [
                        "renew efforts",
                        "renew a friendship",
                        "renew an attack",
                        "renew vows"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать что-либо заново, часто с новыми силами или решимостью, после перерыва."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возобновля́ть",
                                "target_lang_sense_clarification": "начинать снова после перерыва (несов. вид)"
                            },
                            {
                                "target_lang_word": "возобнови́ть",
                                "target_lang_sense_clarification": "начать снова после перерыва (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5a86a32-6a70-33bb-aa34-60c6a73ab66e",
                    "sense_definition": "To make something new again or restore it to its original fresh condition.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "restore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A good night's sleep will help <w>renew</w> your strength for tomorrow.",
                            "target_lang_translations": {
                                "ru": "Хороший ночной сон поможет <w>восстановить</w> силы на завтра."
                            }
                        },
                        {
                            "text": "The heavy rain helped to <w>renew</w> the dry fields and fill the rivers.",
                            "target_lang_translations": {
                                "ru": "Сильный дождь помог <w>оживить</w> сухие поля и наполнить реки."
                            }
                        }
                    ],
                    "synonyms": [
                        "renewal",
                        "renewal",
                        "renewal"
                    ],
                    "antonyms": [
                        "damage",
                        "ruin"
                    ],
                    "common_phrases": [
                        "renew one's strength",
                        "renew one's youth",
                        "renew energy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо снова новым или восстанавливать до исходного свежего состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восстана́вливать",
                                "target_lang_sense_clarification": "возвращать силы, приводить в прежнее состояние"
                            },
                            {
                                "target_lang_word": "обновля́ть",
                                "target_lang_sense_clarification": "делать снова свежим, возрождать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d295077-c9b2-31d3-bb76-f94027c67a21",
                    "sense_definition": "To replace something that is broken, worn out, or used up.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "replace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic told us we needed to <w>renew</w> the brake pads on the car.",
                            "target_lang_translations": {
                                "ru": "Механик сказал нам, что нужно <w>заменить</w> тормозные колодки на машине."
                            }
                        },
                        {
                            "text": "We must <w>renew</w> our supply of water before we continue the journey.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>пополнить</w> запасы воды, прежде чем продолжить путешествие."
                            }
                        }
                    ],
                    "synonyms": [
                        "renewal",
                        "renewal"
                    ],
                    "common_phrases": [
                        "renew supplies",
                        "renew parts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заменять что-либо сломанное, изношенное или израсходованное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обновля́ть",
                                "target_lang_sense_clarification": "заменять устаревшее новым"
                            },
                            {
                                "target_lang_word": "заменя́ть",
                                "target_lang_sense_clarification": "ставить другое вместо старого"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "33b6a89c-4776-39d4-a877-0d9afdebc855",
                    "sense_definition": "The act of renewing something; a renewal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>renew</w> of the contract took longer than expected.",
                            "target_lang_translations": {
                                "ru": "<w>Продление</w> контракта заняло больше времени, чем ожидалось."
                            }
                        },
                        {
                            "text": "They awaited the <w>renew</w> of the spring season.",
                            "target_lang_translations": {
                                "ru": "Они ждали <w>обновления</w> весеннего сезона."
                            }
                        }
                    ],
                    "synonyms": [
                        "renewal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по обновлению или продлению чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обновле́ние",
                                "target_lang_sense_clarification": "процесс замены или восстановления"
                            },
                            {
                                "target_lang_word": "продле́ние",
                                "target_lang_sense_clarification": "увеличение срока действия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
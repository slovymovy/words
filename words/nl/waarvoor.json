            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0ef438c7-ff51-31db-9750-ff341d69b3a5",
                    "sense_definition": "Vragend: met welk doel of om welke reden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "interrogative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je me uitleggen <w>waarvoor</w> dit apparaat precies dient?",
                            "target_lang_translations": {
                                "en": "Can you explain to me <w>what</w> this device is exactly used <w>for</w>?"
                            }
                        },
                        {
                            "text": "Ik begrijp niet goed <w>waarvoor</w> hij zich zo druk maakt.",
                            "target_lang_translations": {
                                "en": "I don't quite understand <w>what</w> he is getting so worked up <w>for</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "waartoe"
                    ],
                    "common_phrases": [
                        "waarvoor dient dit",
                        "waarvoor is dat",
                        "waarvoor dank"
                    ],
                    "target_lang_definitions": {
                        "en": "Interrogative: for what purpose or reason."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "what ... for",
                                "target_lang_sense_clarification": "asking about purpose or reason"
                            },
                            {
                                "target_lang_word": "for what",
                                "target_lang_sense_clarification": "formal interrogative"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62449207-b775-3df4-b6d5-c777e9441d11",
                    "sense_definition": "Betrekkelijk: verwijst terug naar iets wat eerder genoemd is, in de betekenis van 'voor hetwelk'.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is het project <w>waarvoor</w> we de afgelopen maanden hard hebben gewerkt.",
                            "target_lang_translations": {
                                "en": "This is the project <w>for which</w> we have worked hard the past few months."
                            }
                        },
                        {
                            "text": "Er zijn bepaalde regels <w>waarvoor</w> iedereen binnen de organisatie respect moet tonen.",
                            "target_lang_translations": {
                                "en": "There are certain rules <w>for which</w> everyone within the organization must show respect."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de reden waarvoor",
                        "het doel waarvoor"
                    ],
                    "target_lang_definitions": {
                        "en": "Relative: refers back to something mentioned earlier, meaning 'for which'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for which",
                                "target_lang_sense_clarification": "relative reference"
                            },
                            {
                                "target_lang_word": "that ... for",
                                "target_lang_sense_clarification": "informal relative reference"
                            }
                        ]
                    }
                }

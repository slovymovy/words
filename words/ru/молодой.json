    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "00d187dd-2840-35d1-a2b7-7059575cd498",
                    "sense_definition": "Находящийся в начале жизни, проживший мало лет (о людях, животных, деревьях).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "age_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот <w>молодой</w> человек помог мне донести тяжёлую сумку.",
                            "target_lang_translations": {
                                "en": "This <w>young</w> man helped me carry the heavy bag."
                            }
                        },
                        {
                            "text": "Моя сестра ещё слишком <w>молода</w>, чтобы водить машину.",
                            "target_lang_translations": {
                                "en": "My sister is still too <w>young</w> to drive a car."
                            }
                        }
                    ],
                    "synonyms": [
                        "юный",
                        "младой"
                    ],
                    "antonyms": [
                        "старый",
                        "пожилой",
                        "старший"
                    ],
                    "common_phrases": [
                        "молодой человек",
                        "молодая семья",
                        "молодое поколение",
                        "самый молодой"
                    ],
                    "target_lang_definitions": {
                        "en": "In the early stage of life or growth; having lived for a short time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "young",
                                "target_lang_sense_clarification": "general meaning"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff941b44-01e4-3708-9db5-a31a06c6200c",
                    "sense_definition": "Недавно появившийся, выросший или приготовленный (о продуктах, растениях, явлениях).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "freshness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На ужин мы сварили вкусную <w>молодую</w> картошку с укропом.",
                            "target_lang_translations": {
                                "en": "For dinner, we boiled delicious <w>new</w> potatoes with dill."
                            }
                        },
                        {
                            "text": "В магазине продаётся <w>молодое</w> вино урожая этого года.",
                            "target_lang_translations": {
                                "en": "The shop sells <w>young</w> wine from this year's harvest."
                            }
                        }
                    ],
                    "synonyms": [
                        "свежий",
                        "новый",
                        "недавний"
                    ],
                    "antonyms": [
                        "старый",
                        "давний"
                    ],
                    "common_phrases": [
                        "молодой картофель",
                        "молодое вино",
                        "молодой месяц",
                        "молодая трава"
                    ],
                    "target_lang_definitions": {
                        "en": "Recently produced, harvested, or formed; fresh."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "new",
                                "target_lang_sense_clarification": "of produce, e.g., potatoes"
                            },
                            {
                                "target_lang_word": "young",
                                "target_lang_sense_clarification": "of wine or plants"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09ac6e07-1e3b-38a2-943e-07d229a54e8e",
                    "sense_definition": "Свойственный юности; бодрый, энергичный, полный сил (переносное значение).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "age_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Несмотря на возраст, у него был удивительно <w>молодой</w> и звонкий голос.",
                            "target_lang_translations": {
                                "en": "Despite his age, he had a surprisingly <w>youthful</w> and ringing voice."
                            }
                        },
                        {
                            "text": "Бабушка сохранила <w>молодой</w> задор и любовь к путешествиям.",
                            "target_lang_translations": {
                                "en": "Grandma preserved her <w>youthful</w> enthusiasm and love for travel."
                            }
                        }
                    ],
                    "antonyms": [
                        "старческий"
                    ],
                    "common_phrases": [
                        "молодой голос",
                        "молодой задор",
                        "душой молод"
                    ],
                    "target_lang_definitions": {
                        "en": "Having the qualities of youth; energetic and vigorous."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "youthful",
                                "target_lang_sense_clarification": "having the appearance or qualities of youth"
                            },
                            {
                                "target_lang_word": "young",
                                "target_lang_sense_clarification": "in spirit or appearance"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9f3897e4-8df5-331c-b89f-67799a6c072a",
                    "sense_definition": "Жених или новобрачный (часто используется во множественном числе для обозначения супружеской пары).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "wedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гости радостно встречали <w>молодых</w> у входа в ресторан.",
                            "target_lang_translations": {
                                "en": "The guests joyfully welcomed the <w>newlyweds</w> at the restaurant entrance."
                            }
                        },
                        {
                            "text": "<w>Молодой</w> нежно смотрел на свою невесту во время танца.",
                            "target_lang_translations": {
                                "en": "The <w>groom</w> looked tenderly at his bride during the dance."
                            }
                        }
                    ],
                    "synonyms": [
                        "молодожён"
                    ],
                    "common_phrases": [

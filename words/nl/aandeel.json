                {
                    "sense_id": "1268c9da-e422-328b-a74d-28e244dd252e",
                    "sense_definition": "Een waardepapier dat bewijst dat men mede-eigenaar is van een bedrijf en recht geeft op een deel van de winst.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader heeft een <w>aandeel</w> in dat grote bedrijf en hoopt op een goed dividend.",
                            "target_lang_translations": {
                                "en": "My father has a <w>share</w> in that big company and hopes for a good dividend."
                            }
                        },
                        {
                            "text": "De belegger verkocht al zijn <w>aandelen</w> toen de koers plotseling daalde.",
                            "target_lang_translations": {
                                "en": "The investor sold all his <w>shares</w> when the price suddenly dropped."
                            }
                        }
                    ],
                    "synonyms": [
                        "effect",
                        "waardepapier"
                    ],
                    "common_phrases": [
                        "aandelen kopen",
                        "aandelen verkopen",
                        "in aandelen beleggen",
                        "ergens aandelen in hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "A security proving that one is a co-owner of a company and entitled to a part of the profit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "share",
                                "target_lang_sense_clarification": "financial unit of ownership"
                            },
                            {

{
    "word_family": [
        "różańcowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "824724e9-2ba9-33bb-9ef3-ad7aacc95a2c",
                    "sense_definition": "Sznur z paciorkami służący do odliczania odmawianych modlitw w Kościele katolickim",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rosary_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia zawsze nosiła przy sobie mały, drewniany <w>różaniec</w>.",
                            "target_lang_translations": {
                                "ru": "Бабушка всегда носила с собой маленькие деревянные <w>чётки</w>."
                            }
                        },
                        {
                            "text": "Dostałem piękny <w>różaniec</w> jako pamiątkę z pielgrzymki.",
                            "target_lang_translations": {
                                "ru": "Я получил красивые <w>чётки</w> в качестве сувенира из паломничества."
                            }
                        }
                    ],
                    "synonyms": [
                        "sznur modlitewny"
                    ],
                    "common_phrases": [
                        "drewniany różaniec",
                        "paciorki różańca",
                        "srebrny różaniec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шнур с бусинами, служащий для отсчета читаемых молитв в Католической церкви"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чётки",
                                "target_lang_sense_clarification": "предмет"
                            },
                            {
                                "target_lang_word": "розарий",
                                "target_lang_sense_clarification": "католические чётки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40d62a2f-e7b0-3e0e-b81a-c659aefd9f99",
                    "sense_definition": "Katolicka modlitwa składająca się z cyklu określonych pacierzy odmawianych w ustalonej kolejności",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rosary_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczorem cała nasza rodzina wspólnie odmawia <w>różaniec</w>.",
                            "target_lang_translations": {
                                "ru": "Вечером вся наша семья вместе читает <w>розарий</w>."
                            }
                        },
                        {
                            "text": "Papież zachęcał wiernych, aby codziennie odmawiali <w>różaniec</w> w intencji pokoju.",
                            "target_lang_translations": {
                                "ru": "Папа призывал верующих ежедневно читать <w>розарий</w> за мир."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odmawiać różaniec",
                        "tajemnice różańca",
                        "modlitwa różańcowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Католическая молитва, состоящая из цикла определенных молитв, читаемых в установленном порядке"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "розарий",
                                "target_lang_sense_clarification": "молитва"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6680d226-495d-3ed2-9cba-47b0c67743c3",
                    "sense_definition": "Nabożeństwo kościelne odprawiane zazwyczaj w październiku, podczas którego wierni wspólnie się modlą",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rosary_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W październiku wiele dzieci chodzi do kościoła na <w>różaniec</w>.",
                            "target_lang_translations": {
                                "ru": "В октябре многие дети ходят в церковь на <w>розарий</w>."
                            }
                        },
                        {
                            "text": "Spotkamy się przed bramą kościoła zaraz po <w>różańcu</w>.",
                            "target_lang_translations": {
                                "ru": "Мы встретимся у ворот церкви сразу после <w>розария</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nabożeństwo różańcowe"
                    ],
                    "common_phrases": [
                        "iść na różaniec",
                        "zostać po różańcu",
                        "październikowy różaniec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Церковное богослужение, совершаемое обычно в октябре, во время которого верующие молятся вместе"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "розарий",
                                "target_lang_sense_clarification": "богослужение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "3e8f77e9-455a-3402-afb6-dfded62bec0c",
                    "sense_definition": "Вывести из состояния сна, заставить проснуться.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, <w>разбуди</w> меня завтра ровно в семь часов утра.",
                            "target_lang_translations": {
                                "en": "Please <w>wake</w> me <w>up</w> tomorrow at exactly seven o'clock in the morning."
                            }
                        },
                        {
                            "text": "Громкий телефонный звонок неожиданно <w>разбудил</w> всех в доме.",
                            "target_lang_translations": {
                                "en": "A loud telephone ring unexpectedly <w>woke</w> everyone in the house."
                            }
                        }
                    ],
                    "synonyms": [
                        "пробуди́ть"
                    ],
                    "antonyms": [
                        "усыпить"
                    ],
                    "common_phrases": [
                        "разбудить утром",
                        "разбудить звонком",
                        "боюсь разбудить",
                        "разбудить ребенка"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause someone to stop sleeping."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wake up",
                                "target_lang_sense_clarification": "most common, transitive"
                            },
                            {
                                "target_lang_word": "wake",
                                "target_lang_sense_clarification": "transitive"
                            },
                            {
                                "target_lang_word": "awaken",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39417d30-d49b-30b2-b2d5-c4a430684afd",
                    "sense_definition": "Вызвать или усилить какие-либо чувства, мысли, воспоминания или желания.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта старая фотография <w>разбудила</w> в нём приятные воспоминания о детстве.",
                            "target_lang_translations": {
                                "en": "This old photograph <w>awakened</w> pleasant childhood memories in him."
                            }
                        },
                        {
                            "text": "Интересный рассказ учителя <w>разбудил</w> у студентов желание изучать историю.",
                            "target_lang_translations": {
                                "en": "The teacher's interesting story <w>awakened</w> in the students a desire to study history."
                            }
                        }
                    ],
                    "synonyms": [
                        "пробуди́ть"
                    ],
                    "antonyms": [
                        "усыпить"
                    ],
                    "common_phrases": [
                        "разбудить интерес",
                        "разбудить любопытство",
                        "разбудить воспоминания",
                        "разбудить зверя"
                    ],
                    "target_lang_definitions": {
                        "en": "To evoke or stimulate feelings, thoughts, memories, or desires."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "awaken",
                                "target_lang_sense_clarification": "memories, feelings, interest"
                            },
                            {
                                "target_lang_word": "arouse",
                                "target_lang_sense_clarification": "interest, curiosity, desires"
                            },
                            {
                                "target_lang_word": "stir up",
                                "target_lang_sense_clarification": "memories, emotions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "355b8f8d-9856-32d2-b283-0d53419e86da",
                    "sense_definition": "Вывести из состояния бездействия или апатии, побудить к активности.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый руководитель смог <w>разбудить</w> коллектив и вдохновить сотрудников на работу.",
                            "target_lang_translations": {
                                "en": "The new manager was able to <w>wake</w> <w>up</w> the team and inspire the employees to work."
                            }
                        },
                        {
                            "text": "Серьёзный кризис <w>разбудил</w> общество от долгого политического сна.",
                            "target_lang_translations": {
                                "en": "A serious crisis <w>roused</w> society from a long political slumber."
                            }
                        }
                    ],
                    "synonyms": [
                        "пробуди́ть"
                    ],
                    "common_phrases": [
                        "разбудить от спячки",
                        "разбудить сознание"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring out of a state of inaction or apathy; to prompt activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rouse",
                                "target_lang_sense_clarification": "from apathy or inaction"
                            },
                            {
                                "target_lang_word": "wake up",
                                "target_lang_sense_clarification": "to stimulate activity"
                            },
                            {
                                "target_lang_word": "stir",
                                "target_lang_sense_clarification": "to action"
                            }
                        ]
                    }
                }
            ]
        }
    ]

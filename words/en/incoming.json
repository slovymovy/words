{
    "word_family": [
        "income",
        "outcome",
        "coming",
        "shortcoming"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "44300725-1038-3538-b9f5-2b4333d3252d",
                    "sense_definition": "Coming in or arriving at a place.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>incoming</w> tide covered the rocks on the beach very quickly.",
                            "target_lang_translations": {
                                "ru": "<w>Наступающий</w> прилив очень быстро накрыл камни на пляже."
                            }
                        },
                        {
                            "text": "Please check your <w>incoming</w> mail for the confirmation link I sent.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте <w>входящую</w> почту на наличие ссылки для подтверждения, которую я отправил."
                            }
                        }
                    ],
                    "synonyms": [
                        "approaching",
                        "arriving",
                        "entering"
                    ],
                    "antonyms": [
                        "outgoing"
                    ],
                    "common_phrases": [
                        "incoming call",
                        "incoming mail",
                        "incoming tide",
                        "incoming data"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибывающий или поступающий в определенное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "входящий",
                                "target_lang_sense_clarification": "о звонках, почте, данных"
                            },
                            {
                                "target_lang_word": "прибывающий",
                                "target_lang_sense_clarification": "о транспорте, пассажирах"
                            },
                            {
                                "target_lang_word": "наступающий",
                                "target_lang_sense_clarification": "о приливе, природных явлениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3c4e71a-f72e-3882-82d1-291ccc587369",
                    "sense_definition": "Recently elected or appointed to a position and about to start official duties.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "succession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>incoming</w> president promised to lower taxes during the first month.",
                            "target_lang_translations": {
                                "ru": "<w>Вступающий в должность</w> президент пообещал снизить налоги в течение первого месяца."
                            }
                        },
                        {
                            "text": "Staff members prepared a detailed report for the <w>incoming</w> administration.",
                            "target_lang_translations": {
                                "ru": "Сотрудники подготовили подробный отчет для <w>новой</w> администрации."
                            }
                        }
                    ],
                    "synonyms": [
                        "new",
                        "succeeding",
                        "elect"
                    ],
                    "antonyms": [
                        "outgoing"
                    ],
                    "common_phrases": [
                        "incoming president",
                        "incoming administration",
                        "incoming government",
                        "incoming chairman"
                    ],
                    "target_lang_definitions": {
                        "ru": "Недавно избранный или назначенный на должность и готовящийся приступить к обязанностям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вступающий в должность",
                                "target_lang_sense_clarification": "официальный термин"
                            },
                            {
                                "target_lang_word": "новый",
                                "target_lang_sense_clarification": "об администрации, руководстве"
                            },
                            {
                                "target_lang_word": "будущий",
                                "target_lang_sense_clarification": "о преемнике"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "636ac51b-0e3a-3bbb-af23-13e46e3ab3f4",
                    "sense_definition": "The act of arriving or things that arrive, often referring to money or goods.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accountant tracked all <w>incomings</w> and outgoings of the business carefully.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер тщательно отслеживал все <w>доходы</w> и расходы бизнеса."
                            }
                        },
                        {
                            "text": "We monitored the <w>incomings</w> of goods at the warehouse dock.",
                            "target_lang_translations": {
                                "ru": "Мы контролировали <w>поступление</w> товаров на складской платформе."
                            }
                        }
                    ],
                    "synonyms": [
                        "arrival",
                        "inflow"
                    ],
                    "common_phrases": [
                        "incomings and outgoings"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс поступления или прибывшие вещи, часто относится к деньгам или товарам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доход",
                                "target_lang_sense_clarification": "денежные поступления"
                            },
                            {
                                "target_lang_word": "поступление",
                                "target_lang_sense_clarification": "товаров или средств"
                            },
                            {
                                "target_lang_word": "приход",
                                "target_lang_sense_clarification": "бухгалтерский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adb50f92-c04b-3559-80c3-f942d07c11e0",
                    "sense_definition": "Ammunition or projectiles fired by an enemy towards one's position.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "military_projectile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers took cover in the trench to avoid the heavy <w>incoming</w>.",
                            "target_lang_translations": {
                                "ru": "Солдаты укрылись в окопе, чтобы избежать сильного <w>обстрела</w>."
                            }
                        },
                        {
                            "text": "We need immediate air support to stop the <w>incoming</w> from the ridge.",
                            "target_lang_translations": {
                                "ru": "Нам нужна немедленная поддержка с воздуха, чтобы остановить <w>огонь</w> с хребта."
                            }
                        }
                    ],
                    "synonyms": [
                        "fire",
                        "bombardment",
                        "shelling"
                    ],
                    "common_phrases": [
                        "heavy incoming",
                        "take incoming"
                    ],
                    "target_lang_definitions": {
                        "ru": "Боеприпасы или снаряды, выпущенные противником в сторону чьей-либо позиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обстрел",
                                "target_lang_sense_clarification": "действие противника"
                            },
                            {
                                "target_lang_word": "огонь",
                                "target_lang_sense_clarification": "стрельба в сторону позиции"
                            },
                            {
                                "target_lang_word": "прилёт",
                                "target_lang_sense_clarification": "разговорное, военный жаргон"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "bb648e93-2eab-372e-b702-375420f54e9e",
                    "sense_definition": "Used to warn people that something, usually dangerous, is approaching.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_projectile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sergeant screamed \"<w>Incoming</w>!\" to warn his squad of the attack.",
                            "target_lang_translations": {
                                "ru": "Сержант закричал: \"<w>Ложись</w>!\", чтобы предупредить свой отряд об атаке."
                            }
                        },
                        {
                            "text": "Everyone ducked when someone yelled \"<w>Incoming</w>!\" across the field.",
                            "target_lang_translations": {
                                "ru": "Все пригнулись, когда кто-то крикнул через поле: \"<w>Ложись</w>!\""
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для предупреждения людей о приближении чего-то опасного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ложись!",
                                "target_lang_sense_clarification": "команда при атаке"
                            },
                            {
                                "target_lang_word": "Обстрел!",
                                "target_lang_sense_clarification": "предупреждение об опасности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
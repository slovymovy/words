    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "45d8d17b-ae33-32ef-88df-24526ed2f4b2",
                    "sense_definition": "Situated on the outer side of something; not inside.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He waited for her <w>outside</w> the front door.",
                            "target_lang_translations": {
                                "ru": "Он ждал её <w>за</w> входной дверью."
                            }
                        },
                        {
                            "text": "There is a large garden <w>outside</w> the house.",
                            "target_lang_translations": {
                                "ru": "<w>Возле</w> дома есть большой сад."
                            }
                        }
                    ],
                    "antonyms": [
                        "inside"
                    ],
                    "common_phrases": [
                        "outside the box",
                        "outside the door"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный на внешней стороне чего-либо; не внутри."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снаружи",
                                "target_lang_sense_clarification": "на внешней стороне"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "позади или вне границ чего-либо"
                            },
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "возле, рядом с выходом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f968389-75cc-35aa-9b7f-9d5bb36f2678",

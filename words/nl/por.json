    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1dcd453a-4543-393f-a2d4-023e10ea027d",
                    "sense_definition": "Een stoot in iemands zijde, gewoonlijk om hem wakker of alert te maken",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geef hem maar een <w>por</w>, want hij staat te slapen op zijn benen.",
                            "target_lang_translations": {
                                "ru": "Просто дай ему <w>тычок</w>, а то он спит на ходу."
                            }
                        },
                        {
                            "text": "Ik voelde een zachte <w>por</w> in mijn rug toen de voorstelling begon.",
                            "target_lang_translations": {
                                "ru": "Я почувствовал легкий <w>тычок</w> в спину, когда началось представление."
                            }
                        }
                    ],
                    "synonyms": [
                        "stoot",
                        "duw",
                        "stomp"
                    ],
                    "common_phrases": [
                        "een por geven",
                        "een por krijgen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тычок или толчок в бок, обычно с целью разбудить кого-то или привлечь внимание"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тычок",
                                "target_lang_sense_clarification": "резкое касание"
                            },
                            {
                                "target_lang_word": "толчок",
                                "target_lang_sense_clarification": "физическое воздействие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "051ea25e-d210-34bd-838b-28d97d283d3c",
                    "sense_definition": "Een vrouwelijke student",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft gisteren een leuke <w>por</w> ontmoet in de kroeg.",
                            "target_lang_translations": {
                                "ru": "Вчера в баре он встретил симпатичную <w>студентку</w>."
                            }
                        },
                        {
                            "text": "Is die nieuwe <w>por</w> al lid geworden van de vereniging?",
                            "target_lang_translations": {
                                "ru": "Эта новая <w>студентка</w> уже вступила в ассоциацию?"
                            }
                        }
                    ],
                    "synonyms": [
                        "studente"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "studententaal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Студентка (на студенческом жаргоне)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "студентка",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f547b847-8c82-3d27-b99f-48e1db5dbc8d",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>porren</w>",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>por</w> even in het vuur om het weer aan te wakkeren.",
                            "target_lang_translations": {
                                "ru": "Я немного <w>ворошу</w> огонь, чтобы раздуть его снова."
                            }
                        },
                        {
                            "text": "Ik <w>por</w> mijn broer wakker omdat we vroeg moeten vertrekken.",
                            "target_lang_translations": {
                                "ru": "Я <w>тыкаю</w> брата, чтобы разбудить, потому что нам нужно рано уезжать."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени от глагола porren (тыкать, ворошить)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тыкаю",
                                "target_lang_sense_clarification": "касаюсь пальцем или предметом"
                            },
                            {
                                "target_lang_word": "ворошу",
                                "target_lang_sense_clarification": "мешаю угли или огонь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6949215-4215-3ad0-963e-95fd2886d099",
                    "sense_definition": "Gebiedende wijs van <w>porren</w>",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Por</w> hem eens, misschien let hij dan eindelijk op.",
                            "target_lang_translations": {
                                "ru": "<w>Ткни</w> его разок, может, он наконец обратит внимание."
                            }
                        },
                        {
                            "text": "<w>Por</w> niet zo hard, dat doet pijn!",
                            "target_lang_translations": {
                                "ru": "Не <w>тыкай</w> так сильно, это больно!"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола porren (ткни, толкни)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ткни",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            },
                            {
                                "target_lang_word": "толкни",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "125a8bf4-6dec-32fe-95dd-707d7e76146f",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>porren</w>",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Por</w> je hem even aan als de film begint?",
                            "target_lang_translations": {
                                "ru": "<w>Толкнешь</w> его, когда начнется фильм?"
                            }
                        },
                        {
                            "text": "Waarom <w>por</w> je steeds in mijn zij?",
                            "target_lang_translations": {
                                "ru": "Почему ты постоянно <w>тыкаешь</w> мне в бок?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Второе лицо единственного числа настоящего времени от глагола porren (тыкаешь, толкаешь)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тыкаешь",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "толкаешь",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }

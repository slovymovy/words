                {
                    "sense_id": "1a6165c8-a81f-37e4-be16-5b38ed38f843",
                    "sense_definition": "Plotseling en met geweld aanvallen om iets te stelen of iemand te overmeesteren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemaskerde mannen <overvielen> de bank in het centrum van de stad.",
                            "target_lang_translations": {
                                "en": "The masked men <robbed> the bank in the city center.",
                                "ru": "Люди в масках <w>ограбили</w> банк в центре города."
                            }
                        },
                        {
                            "text": "Hij werd gisteravond laat op straat <overvallen> en van zijn geld beroofd.",
                            "target_lang_translations": {
                                "en": "He was <mugged> late last night on the street and robbed of his money.",
                                "ru": "Вчера поздно вечером на него <w>напали</w> на улице и отобрали деньги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een bank overvallen",
                        "iemand overvallen"
                    ],
                    "target_lang_definitions": {
                        "en": "To attack suddenly and with violence in order to steal or overpower.",
                        "ru": "Внезапно напасть с применением силы, чтобы ограбить или захватить."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rob",
                                "target_lang_sense_clarification": "to steal from a person or place using force"
                            },
                            {
                                "target_lang_word": "raid",
                                "target_lang_sense_clarification": "to make a sudden attack on a place"
                            },
                            {
                                "target_lang_word": "ambush",
                                "target_lang_sense_clarification": "to attack by surprise from a concealed position"
                            },
                            {
                                "target_lang_word": "mug",
                                "target_lang_sense_clarification": "to attack and rob someone in a public place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ограбить",
                                "target_lang_sense_clarification": "отнять имущество силой"
                            },
                            {
                                "target_lang_word": "совершить налёт",
                                "target_lang_sense_clarification": "внезапно атаковать место (банк, магазин)"

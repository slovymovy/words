                {
                    "sense_id": "896630f3-c7bd-3897-989a-25311e7037fb",
                    "sense_definition": "Met een schaar materiaal in stukken verdelen of korter maken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cutting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kapper gaat morgen mijn haar <w>knippen</w>.",
                            "target_lang_translations": {
                                "ru": "Завтра парикмахер будет <w>стричь</w> мне волосы.",
                                "en": "The hairdresser is going to <w>cut</w> my hair tomorrow."
                            }
                        },
                        {
                            "text": "Wat ben jij mooie figuren aan het <w>knippen</w>!",
                            "target_lang_translations": {
                                "ru": "Какие красивые фигуры ты <w>вырезаешь</w>!",
                                "en": "What beautiful figures you are <w>cutting</w>!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "haar knippen",
                        "nagels knippen",
                        "in stukjes knippen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрезать материал на части или укорачивать его с помощью ножниц.",
                        "en": "To divide material into pieces or make shorter using scissors."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стричь",
                                "target_lang_sense_clarification": "волосы, ногти, шерсть"
                            },
                            {
                                "target_lang_word": "резать",
                                "target_lang_sense_clarification": "ножницами (бумагу, ткань)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "cut",
                                "target_lang_sense_clarification": "general / with scissors"
                            },
                            {
                                "target_lang_word": "trim",
                                "target_lang_sense_clarification": "hair, plants"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08568d21-353a-39df-a370-8c0b9dd2625b",
                    "sense_definition": "Geselecteerde digitale gegevens verwijderen en tijdelijk in het geheugen opslaan om ze elders te plaatsen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je mag die tabel wel uit mijn presentatie <w>knippen</w>.",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>вырезать</w> эту таблицу из моей презентации.",
                                "en": "You can <w>cut</w> that table from my presentation."
                            }
                        },
                        {
                            "text": "Als je de tekst wilt verplaatsen, moet je eerst <w>knippen</w> en dan plakken.",
                            "target_lang_translations": {
                                "ru": "Если хочешь переместить текст, нужно сначала <w>вырезать</w>, а затем вставить.",
                                "en": "If you want to move the text, you have to <w>cut</w> and then paste first."
                            }
                        }
                    ],
                    "antonyms": [
                        "plakken"
                    ],
                    "common_phrases": [
                        "knippen en plakken"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалить выбранные цифровые данные и временно сохранить их в буфере обмена для вставки в другом месте.",
                        "en": "To remove selected digital data and store it temporarily in memory to place elsewhere."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырезать",
                                "target_lang_sense_clarification": "компьютерный термин"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "cut",
                                "target_lang_sense_clarification": "computing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "072318c1-bade-36fc-8ef8-27940c0bbf4a",

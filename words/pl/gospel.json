{
    "word_family": [
        "gospelowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dd7084ad-02bd-3177-9ec9-33ca52f68bab",
                    "sense_definition": "Styl muzyki chrześcijańskiej wywodzący się z kultury afroamerykańskiej, charakteryzujący się dużą ekspresją i rytmicznością.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "music_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdą niedzielę chór kościelny śpiewa energetyczny <w>gospel</w>.",
                            "target_lang_translations": {
                                "ru": "Каждое воскресенье церковный хор поет энергичный <w>госпел</w>."
                            }
                        },
                        {
                            "text": "Ten festiwal promuje muzykę <w>gospel</w> i przyciąga tłumy słuchaczy.",
                            "target_lang_translations": {
                                "ru": "Этот фестиваль продвигает музыку <w>госпел</w> и привлекает толпы слушателей."
                            }
                        }
                    ],
                    "synonyms": [
                        "muzyka gospel"
                    ],
                    "common_phrases": [
                        "muzyka gospel",
                        "chór gospel",
                        "festiwal gospel",
                        "śpiewać gospel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стиль христианской музыки, зародившийся в афроамериканской культуре, характеризующийся высокой экспрессией и ритмичностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "госпел",
                                "target_lang_sense_clarification": "музыкальный жанр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "237335ac-3a15-3bdd-a292-cb50151be23b",
                    "sense_definition": "Pojedynczy utwór muzyczny skomponowany i wykonany w stylu gospel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "musical_piece",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zakończenie koncertu artyści wykonali znany <w>gospel</w>.",
                            "target_lang_translations": {
                                "ru": "В завершение концерта артисты исполнили известный <w>госпел</w>."
                            }
                        },
                        {
                            "text": "To był bardzo wzruszający <w>gospel</w>, który poruszył całą publiczność.",
                            "target_lang_translations": {
                                "ru": "Это был очень трогательный <w>госпел</w>, который тронул всю публику."
                            }
                        }
                    ],
                    "synonyms": [
                        "pieśń gospel"
                    ],
                    "common_phrases": [
                        "zaśpiewać gospel",
                        "znany gospel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельное музыкальное произведение, сочиненное и исполненное в стиле госпел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "госпел",
                                "target_lang_sense_clarification": "песня или композиция"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
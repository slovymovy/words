            "senses": [
                {
                    "sense_id": "42c9c697-b297-30dc-ab86-9fd41ee9424d",
                    "sense_definition": "Datgene wat in iets anders zit of bewaard wordt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "content_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben erg benieuwd naar de <w>inhoud</w> van dit pakketje.",
                            "target_lang_translations": {
                                "en": "I am very curious about the <w>contents</w> of this package.",
                                "ru": "Мне очень любопытно узнать <w>содержимое</w> этого пакета."
                            }
                        },
                        {
                            "text": "De <w>inhoud</w> van de fles is bijna op.",
                            "target_lang_translations": {
                                "en": "The <w>contents</w> of the bottle are almost gone.",
                                "ru": "<w>Содержимое</w> бутылки почти закончилось."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de inhoud van",
                        "naar de inhoud"
                    ],
                    "target_lang_definitions": {
                        "en": "That which is inside something else or is kept there.",
                        "ru": "То, что находится внутри чего-либо или хранится там."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "contents",
                                "target_lang_sense_clarification": "items inside a container"
                            },
                            {
                                "target_lang_word": "content",
                                "target_lang_sense_clarification": "substance contained"
                            }

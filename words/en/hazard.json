    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "21e20659-0968-3930-bac1-ebd7521ae886",
                    "sense_definition": "A potential source of danger or a situation that poses a risk to safety.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "danger_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ice on the road is a major driving <w>hazard</w> during the winter.",
                            "target_lang_translations": {
                                "ru": "Гололед на дороге — это серьезная <w>опасность</w> для вождения зимой."
                            }
                        },
                        {
                            "text": "The old wiring in the house was declared a serious fire <w>hazard</w>.",
                            "target_lang_translations": {
                                "ru": "Старая проводка в доме была признана серьезной пожарной <w>угрозой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bet",
                        "wager"
                    ],
                    "common_phrases": [
                        "fire hazard",
                        "safety hazard",
                        "health hazard",
                        "choking hazard",
                        "occupational hazard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потенциальный источник опасности или ситуация, представляющая угрозу безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опасность",
                                "target_lang_sense_clarification": "ситуация или объект, способные причинить вред"
                            },
                            {
                                "target_lang_word": "угроза",
                                "target_lang_sense_clarification": "наличие опасности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7427539-87e4-391b-996e-c9fe86db568f",
                    "sense_definition": "The chance of suffering harm, loss, or danger.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "danger_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He encountered the enemy at the <w>hazard</w> of his reputation and life.",
                            "target_lang_translations": {
                                "ru": "Он вступил в схватку с врагом с <w>риском</w> для своей репутации и жизни."
                            }
                        },
                        {
                            "text": "Soldiers often face great <w>hazard</w> in the line of duty.",
                            "target_lang_translations": {
                                "ru": "Солдаты часто подвергаются огромному <w>риску</w> при исполнении служебных обязанностей."
                            }
                        }
                    ],
                    "synonyms": [
                        "adventure",
                        "fortune",
                        "luck"
                    ],
                    "antonyms": [
                        "safety",
                        "security"
                    ],
                    "common_phrases": [
                        "at hazard",
                        "in hazard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вероятность пострадать, понести убытки или столкнуться с опасностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "риск",
                                "target_lang_sense_clarification": "возможная опасность или неудача"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a341c40-de0f-363d-ada4-6f823cabaefb",
                    "sense_definition": "A permanent feature on a golf course, such as a bunker or body of water, designed to obstruct play.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports_obstacle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The golfer was frustrated when his ball landed directly in the water <w>hazard</w>.",
                            "target_lang_translations": {
                                "ru": "Гольфист был расстроен, когда его мяч приземлился прямо в водное <w>препятствие</w>."
                            }
                        },
                        {
                            "text": "She carefully chipped the ball over the sand <w>hazard</w> to reach the green.",
                            "target_lang_translations": {
                                "ru": "Она аккуратно перебросила мяч через песчаное <w>препятствие</w>, чтобы попасть на грин."
                            }
                        }
                    ],
                    "common_phrases": [
                        "water hazard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянный элемент на поле для гольфа (например, бункер или водоем), усложняющий игру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "препятствие",
                                "target_lang_sense_clarification": "в спорте: преграда на пути мяча"
                            },
                            {
                                "target_lang_word": "помеха",
                                "target_lang_sense_clarification": "то, что мешает или затрудняет действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bcdbce2-b88d-3c45-b628-2627e3cc311f",
                    "sense_definition": "Chance; unpredictable events or luck.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "chance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will stand the <w>hazard</w> of the die.",
                            "target_lang_translations": {
                                "ru": "Я доверюсь <w>случаю</w> игральной кости."
                            }
                        },
                        {
                            "text": "By <w>hazard</w>, we met at the station just as the train was arriving.",
                            "target_lang_translations": {
                                "ru": "По <w>случайности</w>, мы встретились на станции как раз в тот момент, когда прибывал поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "fortune",
                        "luck",
                        "adventure"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "literary or poetic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Случай, непредсказуемое событие или удача (устаревшее или книжное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "случай",
                                "target_lang_sense_clarification": "непредвиденное событие, стечение обстоятельств"
                            },
                            {
                                "target_lang_word": "случайность",
                                "target_lang_sense_clarification": "отсутствие закономерности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43dda548-ab74-31b1-b386-cd33520b3ad8",
                    "sense_definition": "An old game of chance played with dice, popular in former centuries.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gentlemen gathered in the club to play <w>hazard</w> and other games of chance.",
                            "target_lang_translations": {
                                "ru": "Джентльмены собрались в клубе, чтобы сыграть в <w>газар</w> и другие азартные игры."
                            }
                        },
                        {
                            "text": "He lost a significant sum of money playing <w>hazard</w> last night.",
                            "target_lang_translations": {
                                "ru": "Он проиграл значительную сумму денег, играя в <w>газар</w> прошлой ночью."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical game"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная азартная игра в кости, популярная в прошлые века."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газар",
                                "target_lang_sense_clarification": "название старинной игры в кости"
                            },
                            {
                                "target_lang_word": "игра в кости",
                                "target_lang_sense_clarification": "общий термин для подобных игр"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "590d544d-1bb9-3988-8bb0-d295a247f705",
                    "sense_definition": "To risk doing something, or to make a suggestion or guess that might be wrong.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "risk_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you don't know the answer, you can still <w>hazard</w> a guess.",
                            "target_lang_translations": {
                                "ru": "Если вы не знаете ответа, вы все равно можете <w>рискнуть</w> предположить."
                            }
                        },
                        {
                            "text": "I would not <w>hazard</w> my savings on such a volatile investment.",
                            "target_lang_translations": {
                                "ru": "Я бы не стал <w>рисковать</w> своими сбережениями ради таких нестабильных инвестиций."
                            }
                        }
                    ],
                    "synonyms": [
                        "bet",
                        "wager"
                    ],
                    "common_phrases": [
                        "hazard a guess",
                        "hazard an opinion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рискнуть сделать что-то или высказать предположение, которое может оказаться ошибочным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рискнуть",
                                "target_lang_sense_clarification": "совершить действие наудачу"
                            },
                            {
                                "target_lang_word": "осмелиться",
                                "target_lang_sense_clarification": "решиться на что-то"
                            }
                        ]
                    }
                }
            ]
        },
        {

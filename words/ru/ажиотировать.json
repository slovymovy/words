    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c64b2e21-3136-3bf1-8518-e634a29ff092",
                    "sense_definition": "Заниматься спекуляцией, скупая и перепродавая товары или ценности для получения быстрой прибыли.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нечестные торговцы начали <w>ажиотировать</w>, искусственно завышая цены на продукты первой необходимости.",
                            "target_lang_translations": {
                                "en": "Dishonest merchants began to <w>profiteer</w>, artificially inflating prices for essential products."
                            }
                        },
                        {
                            "text": "В периоды кризиса многие пытаются <w>ажиотировать</w> на дефицитных товарах.",
                            "target_lang_translations": {
                                "en": "During periods of crisis, many try to <w>speculate</w> on scarce goods."
                            }
                        }
                    ],
                    "synonyms": [
                        "спекулировать",
                        "барышничать"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "современный синоним — спекулировать"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To engage in speculation by buying and reselling goods or valuables for quick profit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speculate",
                                "target_lang_sense_clarification": "buying and selling goods"
                            },
                            {
                                "target_lang_word": "profiteer",
                                "target_lang_sense_clarification": "making unfair profit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27ad26a0-8937-311a-8574-2ce7e74ec75d",
                    "sense_definition": "Играть на бирже, совершая рискованные сделки для получения прибыли от разницы курсов.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодой брокер решил активно <w>ажиотировать</w> акциями железнодорожных компаний.",
                            "target_lang_translations": {
                                "en": "The young broker decided to actively <w>speculate</w> in railway company stocks."
                            }
                        },
                        {
                            "text": "Ему нравилось <w>ажиотировать</w> на бирже, чувствуя азарт финансовой игры.",
                            "target_lang_translations": {
                                "en": "He liked to <w>speculate</w> on the stock exchange, feeling the excitement of the financial game."
                            }
                        }
                    ],
                    "synonyms": [
                        "играть на бирже"
                    ],
                    "common_phrases": [
                        "ажиотировать на бирже",
                        "ажиотировать акциями"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To play the stock market by making risky deals to profit from exchange rate differences."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speculate",
                                "target_lang_sense_clarification": "on the stock market"
                            }
                        ]
                    }
                }
            ]
        }

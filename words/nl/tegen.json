                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "отношение или поведение по отношению к кому-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "982bfe28-5645-3cad-93f7-dca2eebf81d1",

            "pos": "name",
            "senses": [
                {
                    "sense_id": "7ac80ffe-fb76-38c2-a514-ebe9fbdfa1ce",
                    "sense_definition": "Een werelddeel dat ten noorden van Afrika en ten westen van Azië ligt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "continent",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Frankrijk en Duitsland zijn grote landen in <w>Europa</w>.",
                            "target_lang_translations": {
                                "ru": "Франция и Германия — большие страны в <w>Европе</w>."
                            }
                        },
                        {
                            "text": "Wij gaan deze zomer met de trein door <w>Europa</w> reizen.",
                            "target_lang_translations": {
                                "ru": "Этим летом мы собираемся путешествовать по <w>Европе</w> на поезде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in heel Europa",
                        "reis door Europa",
                        "kaart van Europa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть света, расположенная к северу от Африки и к западу от Азии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Европа",
                                "target_lang_sense_clarification": "часть света"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38c628ea-e45d-3cb1-b997-d68cd81c6f61",
                    "sense_definition": "De Europese Unie; het politieke en economische samenwerkingsverband van Europese landen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "eu",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "<w>Europa</w> heeft nieuwe regels opgesteld voor de bescherming van het milieu.",
                            "target_lang_translations": {
                                "ru": "<w>Европа</w> установила новые правила охраны окружающей среды."
                            }
                        },
                        {
                            "text": "De leiders van <w>Europa</w> komen regelmatig bijeen in Brussel voor overleg.",
                            "target_lang_translations": {
                                "ru": "Лидеры <w>Европы</w> регулярно собираются в Брюсселе для переговоров."
                            }
                        }
                    ],
                    "synonyms": [
                        "EU",
                        "Europese Unie"
                    ],
                    "common_phrases": [
                        "het verenigd Europa",
                        "grenzen van Europa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Европейский союз; политическое и экономическое объединение европейских стран."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Европа",
                                "target_lang_sense_clarification": "Европейский союз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf2d2557-bb61-3941-a112-265b97a55a74",
                    "sense_definition": "Een van de vier grote manen van de planeet Jupiter.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Wetenschappers vermoeden dat er onder het ijs van <w>Europa</w> een oceaan ligt.",
                            "target_lang_translations": {
                                "ru": "Ученые предполагают, что подо льдом <w>Европы</w> находится океан."
                            }
                        },
                        {
                            "text": "De ruimtesonde stuurde scherpe foto's van het oppervlak van <w>Europa</w> naar de aarde.",
                            "target_lang_translations": {
                                "ru": "Космический зонд прислал на Землю четкие фотографии поверхности <w>Европы</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "maan Europa",
                        "oppervlak van Europa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из четырех крупных спутников планеты Юпитер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Европа",
                                "target_lang_sense_clarification": "спутник Юпитера"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b68e2380-243c-3129-8c56-8daf6a91852b",
                    "sense_definition": "Een Fenicische prinses uit de Griekse mythologie die door Zeus werd ontvoerd.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mythology",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "De oppergod Zeus veranderde zichzelf in een witte stier om <w>Europa</w> te schaken.",
                            "target_lang_translations": {
                                "ru": "Верховный бог Зевс превратился в белого быка, чтобы похитить <w>Европу</w>."
                            }
                        },
                        {
                            "text": "Het werelddeel is volgens de legende vernoemd naar de prinses <w>Europa</w>.",
                            "target_lang_translations": {
                                "ru": "Согласно легенде, часть света названа в честь принцессы <w>Европы</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ontvoering van Europa",
                        "mythe van Europa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Финикийская принцесса из греческой мифологии, похищенная Зевсом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Европа",

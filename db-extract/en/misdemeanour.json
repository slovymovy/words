                "translations": [
                    {
                        "translation_id": "4a3b9625-3d93-45fa-af86-82df2b21b680",
                        "source_entry_id": "4c2808da-ef94-3299-b096-81850e8805d5",
                        "source_sense_id": "e1e5dbf9-a250-3cbc-9386-f9d6a2ec87c1",
                        "source_sense_description": "sdÜ",
                        "target_lang_word": "Vergehen",
                        "target_lang_code": "de",
                        "target_sense_ids": [],
                        "cross_references": [
                            {
                                "target_word": "Vergehen",
                                "target_lang_code": "de",
                                "target_senses": [
                                    {
                                        "sense_id": "bcadf59a-f718-3b2f-ae86-cc6f171c4123",
                                        "entry_id": "2af1369e-09c6-3d53-bde6-b011797cca2c",
                                        "glosses": [
                                            "eine Handlung, die gegen die Moral oder gegen das Gesetz verstößt"
                                        ],
                                        "tags": [],
                                        "examples": [
                                            {
                                                "example_id": "ec22f99f-331f-4437-85a7-15fbf335484e",
                                                "sense_id": "bcadf59a-f718-3b2f-ae86-cc6f171c4123",
                                                "text": "»Er kann sie nicht ansprechen, dachte ich, das ist hier nicht üblich, das ist nirgendwo üblich, aber hier ist es ein unverzeihliches Vergehen«."
                                            },
                                            {
                                                "example_id": "a7723242-7c79-4d7b-a7d9-daf6f42e6876",
                                                "sense_id": "bcadf59a-f718-3b2f-ae86-cc6f171c4123",
                                                "text": "„Er wußte zwar noch nicht, wessen man den Methodisten beschuldigte, aber dessen rätselhaftes Betragen verriet zu deutlich, wie er sich irgendeines Vergehens bewußt sein mußte.“"
                                            }
                                        ],
                                        "sense_index_json": "\"1\""
                                    },
                                    {
                                        "sense_id": "7860af36-42ce-3658-b8b0-72e966a8d469",
                                        "entry_id": "2af1369e-09c6-3d53-bde6-b011797cca2c",
                                        "glosses": [
                                            "Straftat, die im Mindestmaß mit Geldstrafe oder Freiheitsstrafe von unter einem Jahr bedroht ist (§ 12 II StGB)"
                                        ],
                                        "tags": [
                                            "deutsches Strafrecht"
                                        ],
                                        "examples": [
                                            {
                                                "example_id": "3819427f-da76-46fd-8594-5a49c862cd3d",
                                                "sense_id": "7860af36-42ce-3658-b8b0-72e966a8d469",
                                                "text": "Die gefährliche Körperverletzung gemäß § 224 StGB ist ein Vergehen."
                                            },
                                            {
                                                "example_id": "6d1181bf-f71b-4e9f-a026-136d6c82b6bd",
                                                "sense_id": "7860af36-42ce-3658-b8b0-72e966a8d469",
                                                "text": "„Der Versuch eines Verbrechens ist stets strafbar, der Versuch eines Vergehens nur dann, wenn das Gesetz es ausdrücklich bestimmt.“"
                                            },
                                            {
                                                "example_id": "41add50b-a12c-4d1e-984c-5534da94b082",
                                                "sense_id": "7860af36-42ce-3658-b8b0-72e966a8d469",
                                                "text": "„Von all diesen Dingen wäre, glaube ich, heute nur noch das Vergehen als Tierquälerei zu ahnden.“"
                                            },
                                            {
                                                "example_id": "4fadacd7-6c99-4460-834c-e7e04cf101f3",
                                                "sense_id": "7860af36-42ce-3658-b8b0-72e966a8d469",
                                                "text": "„Gegen die Hundehalterin wurde ein Strafverfahren wegen eines Vergehens nach dem Tierschutzgesetz eingeleitet.“"
                                            }
                                        ],
                                        "sense_index_json": "\"2\""
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "translation_id": "b41a8238-5086-48a8-b580-f6ec980564b9",
                        "source_entry_id": "4c2808da-ef94-3299-b096-81850e8805d5",
                        "source_sense_id": "e1e5dbf9-a250-3cbc-9386-f9d6a2ec87c1",

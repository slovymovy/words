                },
                {
                    "sense_id": "14e1e9f0-ee36-3458-8ec6-19af652aafa3",
                    "sense_definition": "An advanced academic degree granted to a student who has successfully completed a course of study at a postgraduate level.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "academic_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is planning to study for a <masters> in engineering next year.",
                            "target_lang_translations": {
                                "ru": "В следующем году она планирует поступить в <магистратуру> по инженерному делу."
                            }

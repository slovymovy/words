        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8d1d362f-ea55-3fad-b311-66267d4c3b7e",
                    "sense_definition": "To pierce or strike with a pointed object, often a spear or something similar.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "weapon_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He used a fork to <w>spear</w> the last pickle in the jar.",
                            "target_lang_translations": {
                                "ru": "Он использовал вилку, чтобы <w>наколоть</w> последний маринованный огурец в банке."
                            }
                        },
                        {
                            "text": "The bird stood motionless in the water, waiting to <w>spear</w> a fish.",
                            "target_lang_translations": {
                                "ru": "Птица стояла неподвижно в воде, ожидая момента, чтобы <w>пронзить</w> рыбу."
                            }
                        }
                    ],
                    "synonyms": [
                        "blank",
                        "cut"
                    ],
                    "common_phrases": [
                        "spear a fish",
                        "spear a piece"
                    ],
                    "target_lang_definitions": {
                        "ru": "Протыкать или ударять заострённым предметом, часто копьём или чем-то похожим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накалывать",
                                "target_lang_sense_clarification": "на вилку или острый предмет"
                            },
                            {
                                "target_lang_word": "пронзать",
                                "target_lang_sense_clarification": "протыкать насквозь"
                            },
                            {
                                "target_lang_word": "гарпунить",
                                "target_lang_sense_clarification": "бить острогой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bdf1174-4a39-39ca-b8cd-16453c558050",
                    "sense_definition": "To tackle an opponent by ramming into them with one's helmet (in football) or striking with a stick (in hockey).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The referee called a penalty because the defender decided to <w>spear</w> the quarterback.",
                            "target_lang_translations": {
                                "ru": "Судья назначил штрафной, потому что защитник решил <w>протаранить</w> квотербека шлемом."
                            }
                        },
                        {
                            "text": "He was ejected from the game after he tried to <w>spear</w> his opponent.",
                            "target_lang_translations": {
                                "ru": "Его удалили из игры после того, как он попытался <w>ткнуть</w> соперника клюшкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "blank",
                        "cut"
                    ],
                    "target_lang_definitions": {
                        "ru": "Атаковать противника ударом шлема (в футболе) или тычком клюшки (в хоккее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таранить",
                                "target_lang_sense_clarification": "ударить шлемом"
                            },
                            {
                                "target_lang_word": "бить колющим ударом",
                                "target_lang_sense_clarification": "клюшкой в хоккее"
                            }
                        ]
                    }
                }
            ]
        },

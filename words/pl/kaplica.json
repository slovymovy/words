{
    "word_family": [
        "kapliczka",
        "kaplicowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "17e02e13-79bd-3c38-8857-9541001341f8",
                    "sense_definition": "Niewielki, wolnostojący budynek służący celom religijnym, mniejszy od kościoła.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przy leśnej drodze stoi zabytkowa <w>kaplica</w>, którą opiekują się mieszkańcy wsi.",
                            "target_lang_translations": {
                                "ru": "У лесной дороги стоит старинная <w>часовня</w>, за которой ухаживают жители деревни."
                            }
                        },
                        {
                            "text": "Pogrzeb rozpoczął się mszą żałobną w cmentarnej <w>kaplicy</w>.",
                            "target_lang_translations": {
                                "ru": "Похороны начались с заупокойной мессы в кладбищенской <w>часовне</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kapliczka"
                    ],
                    "common_phrases": [
                        "kaplica cmentarna",
                        "przydrożna kaplica"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое отдельно стоящее здание религиозного назначения, меньше церкви."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "часовня",
                                "target_lang_sense_clarification": "небольшая христианская постройка"
                            },
                            {
                                "target_lang_word": "капелла",
                                "target_lang_sense_clarification": "в католической архитектуре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64f612fd-0809-3da4-8cd6-a613aad66d24",
                    "sense_definition": "Wydzielona część większego budynku (np. kościoła, zamku, szpitala) z ołtarzem, przeznaczona do odprawiania nabożeństw.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W bocznej <w>kaplicy</w> katedry znajduje się słynny obraz Matki Boskiej.",
                            "target_lang_translations": {
                                "ru": "В боковой <w>капелле</w> собора находится знаменитая икона Божьей Матери."
                            }
                        },
                        {
                            "text": "Pacjenci mogą w każdej chwili skorzystać z <w>kaplicy</w> szpitalnej na parterze.",
                            "target_lang_translations": {
                                "ru": "Пациенты могут в любой момент воспользоваться больничной <w>часовней</w> на первом этаже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kaplica szpitalna",
                        "kaplica zamkowa",
                        "kaplica boczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная часть большого здания (например, церкви, замка, больницы) с алтарем, предназначенная для богослужений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капелла",
                                "target_lang_sense_clarification": "часть храма или замка"
                            },
                            {
                                "target_lang_word": "придел",
                                "target_lang_sense_clarification": "пристройка в православном храме"
                            },
                            {
                                "target_lang_word": "домовая церковь",
                                "target_lang_sense_clarification": "в учреждении или частном доме"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "18165364-0119-3f36-9d00-c4bb959ce638",
                    "sense_definition": "Słowo oznaczające koniec czegoś, porażkę lub sytuację bez wyjścia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli nie zdążymy na ostatni autobus do domu, to będzie <w>kaplica</w>.",
                            "target_lang_translations": {
                                "ru": "Если мы не успеем на последний автобус домой, то нам <w>крышка</w>."
                            }
                        },
                        {
                            "text": "Zepsuł nam się silnik na środku pustkowia, no to <w>kaplica</w>.",
                            "target_lang_translations": {
                                "ru": "У нас сломался двигатель посреди глуши, ну всё, <w>кранты</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "koniec",
                        "klapa",
                        "porażka"
                    ],
                    "antonyms": [
                        "sukces"
                    ],
                    "common_phrases": [
                        "no to kaplica",
                        "totalna kaplica"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, означающее конец чего-либо, провал или безвыходную ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крышка",
                                "target_lang_sense_clarification": "разговорное; конец всему"
                            },
                            {
                                "target_lang_word": "кранты",
                                "target_lang_sense_clarification": "сленг; безвыходная ситуация"
                            },
                            {
                                "target_lang_word": "труба",
                                "target_lang_sense_clarification": "разговорное; дело плохо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
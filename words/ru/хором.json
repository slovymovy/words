    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0f9c6c6f-1ced-3b4c-9ad7-bcaf8d4ffd26",
                    "sense_definition": "Группой лиц, в хоровом исполнении (обычно о пении).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "singing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дети в школе учились петь <w>хором</w> народные песни.",
                            "target_lang_translations": {
                                "en": "The children at school learned to sing folk songs <w>in chorus</w>."
                            }
                        },
                        {
                            "text": "Гости за столом <w>хором</w> затянули известную мелодию.",
                            "target_lang_translations": {
                                "en": "The guests at the table began singing a famous melody <w>in chorus</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "вместе"
                    ],
                    "antonyms": [
                        "соло"
                    ],
                    "common_phrases": [
                        "петь хором",
                        "исполнять хором"
                    ],
                    "target_lang_definitions": {
                        "en": "In a group, chorally (usually referring to singing)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in chorus",
                                "target_lang_sense_clarification": "singing together"
                            },
                            {
                                "target_lang_word": "chorally",
                                "target_lang_sense_clarification": "adverb describing singing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9244c6ce-6b03-3c60-896d-bb127dcb3094",
                    "sense_definition": "Все сразу, одновременно, в один голос (о речи, крике или смехе).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "simultaneously",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ученики <w>хором</w> поздоровались, когда учитель вошёл в класс.",
                            "target_lang_translations": {
                                "en": "The students said hello <w>in unison</w> when the teacher entered the classroom."
                            }
                        },
                        {
                            "text": "Зрители в зале <w>хором</w> смеялись над шутками артиста.",
                            "target_lang_translations": {
                                "en": "The audience in the hall laughed <w>in unison</w> at the artist's jokes."
                            }
                        }
                    ],
                    "synonyms": [
                        "одновременно",
                        "дружно",
                        "вместе",
                        "в один голос"
                    ],
                    "antonyms": [
                        "поодиночке",
                        "порознь"
                    ],
                    "common_phrases": [
                        "отвечать хором",
                        "смеяться хором",
                        "кричать хором",
                        "повторять хором"
                    ],
                    "target_lang_definitions": {
                        "en": "All at once, simultaneously, or in one voice (referring to speech, shouting, or laughter)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in unison",
                                "target_lang_sense_clarification": "speaking or acting simultaneously"
                            },
                            {
                                "target_lang_word": "in chorus",
                                "target_lang_sense_clarification": "speaking together"
                            },
                            {
                                "target_lang_word": "all together",
                                "target_lang_sense_clarification": "simultaneously"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76c57ed7-b1a0-3655-a6fa-514bef81bc61",
                    "sense_definition": "Совместными усилиями, сообща.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "jointly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы навалились <w>хором</w> и смогли сдвинуть тяжёлый шкаф.",
                            "target_lang_translations": {
                                "en": "We pushed <w>all together</w> and managed to move the heavy wardrobe."
                            }
                        },
                        {
                            "text": "Родственники <w>хором</w> уговаривали его не уезжать из города.",
                            "target_lang_translations": {
                                "en": "The relatives were persuading him <w>all together</w> not to leave the city."
                            }
                        }
                    ],
                    "synonyms": [
                        "сообща",
                        "вместе",
                        "скопом"
                    ],
                    "antonyms": [
                        "в одиночку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "By joint efforts, collaboratively."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "all together",
                                "target_lang_sense_clarification": "collaboratively"
                            },
                            {
                                "target_lang_word": "jointly",
                                "target_lang_sense_clarification": "by combined effort"
                            }
                        ]
                    }
                }
            ]
        },
        {

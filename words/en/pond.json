                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "океан",
                                "target_lang_sense_clarification": "в идиомах типа «за океаном»"
                            },
                            {
                                "target_lang_word": "Атлантика",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8ce81ec8-1dd4-3701-933d-8fb89bda6fb3",
                    "sense_definition": "To collect or accumulate in a pool, often referring to water on a surface.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "water_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the heavy storm, water began to <w>pond</w> on the flat roof.",
                            "target_lang_translations": {
                                "ru": "После сильного шторма вода начала <w>скапливаться</w> на плоской крыше."
                            }
                        },
                        {
                            "text": "Poor drainage caused the rain to <w>pond</w> in the middle of the driveway.",
                            "target_lang_translations": {
                                "ru": "Из-за плохого дренажа дождевая вода стала <w>застаиваться</w> посреди подъездной дорожки."
                            }
                        }
                    ],
                    "synonyms": [
                        "pool",
                        "accumulate"
                    ],
                    "antonyms": [
                        "drain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скапливаться, образуя лужи или заводи (о воде на поверхности)."
                    },
                    "translations": {
                        "ru": [
                            {

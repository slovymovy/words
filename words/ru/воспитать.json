                        "взрастить"
                    ],
                    "common_phrases": [
                        "воспитать чемпиона",
                        "воспитать ученика",
                        "воспитать патриота",
                        "воспитать лидера",
                        "воспитать профессионала"
                    ],
                    "target_lang_definitions": {

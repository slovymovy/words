                    }
                },
                {
                    "sense_id": "51cd4a3c-2412-3c18-8d72-2a4506c9da15",
                    "sense_definition": "Uroczyste i prawne ustanowienie kogoś na urzędzie, zwłaszcza kościelnym.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religious_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biskup dokonał kanonicznej <w>instytucji</w> nowego proboszcza w parafii.",
                            "target_lang_translations": {
                                "ru": "Епископ совершил каноническую <w>институцию</w> нового настоятеля в приходе."
                            }

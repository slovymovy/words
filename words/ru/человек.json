    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb83538a-c210-339d-8747-a5c6ec75ea36",
                    "sense_definition": "Живое разумное существо, обладающее даром речи, мышления и способностью создавать орудия труда; представитель рода Homo sapiens.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот <w>человек</w> помог мне найти дорогу к вокзалу.",
                            "target_lang_translations": {
                                "en": "This <w>man</w> helped me find the way to the station."
                            }
                        },
                        {
                            "text": "Каждый <w>человек</w> хочет быть счастливым и здоровым.",
                            "target_lang_translations": {
                                "en": "Every <w>person</w> wants to be happy and healthy."
                            }
                        }
                    ],
                    "synonyms": [
                        "землянин",
                        "гомо сапиенс"
                    ],
                    "antonyms": [
                        "животное",
                        "зверь",
                        "робот",
                        "бог",
                        "нечеловек"
                    ],
                    "common_phrases": [
                        "молодой человек",
                        "добрый человек",
                        "честный человек",
                        "родной человек"
                    ],
                    "target_lang_definitions": {
                        "en": "A living, thinking being; a member of the human species."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "person",
                                "target_lang_sense_clarification": "individual"
                            },
                            {
                                "target_lang_word": "man",
                                "target_lang_sense_clarification": "adult male or person"
                            },
                            {
                                "target_lang_word": "human",
                                "target_lang_sense_clarification": "biological species"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e598eac-66ca-3cd9-9906-1a6548b6707f",
                    "sense_definition": "Употребляется в значении «люди» в родительном падеже множественного числа при сочетании с количественными числительными.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На экскурсию записалось десять <w>человек</w>.",
                            "target_lang_translations": {
                                "en": "Ten <w>people</w> signed up for the tour."
                            }
                        },
                        {
                            "text": "В комнате находилось пять <w>человек</w>, и все молчали.",
                            "target_lang_translations": {
                                "en": "There were five <w>people</w> in the room, and everyone was silent."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пять человек",
                        "несколько человек",
                        "сто человек",
                        "много человек"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "счётная форма"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to mean \"people\" when counting specific numbers."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "people",
                                "target_lang_sense_clarification": "plural counting form"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "031c1563-6efc-36b0-aae1-2c56d9f1e05a",
                    "sense_definition": "Человеческий род в целом, человечество (обычно в единственном числе).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Взаимодействие <w>человека</w> и природы — важная проблема современности.",
                            "target_lang_translations": {
                                "en": "The interaction between <w>man</w> and nature is an important issue of modern times."
                            }
                        },
                        {
                            "text": "<w>Человек</w> освоил космос и побывал на Луне.",
                            "target_lang_translations": {
                                "en": "<w>Man</w> has mastered space and visited the Moon."
                            }
                        }
                    ],
                    "common_phrases": [
                        "права человека",
                        "судьба человека"
                    ],
                    "target_lang_definitions": {
                        "en": "The human race as a whole; mankind."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "man",
                                "target_lang_sense_clarification": "humanity collectively"
                            },
                            {
                                "target_lang_word": "humanity",
                                "target_lang_sense_clarification": "human race"
                            },
                            {
                                "target_lang_word": "mankind",
                                "target_lang_sense_clarification": "collective"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e7a0fae-a11a-34a5-9160-40800a029ba9",
                    "sense_definition": "Личность, обладающая высокими моральными качествами, достоинством и душой.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В любой ситуации нужно оставаться <w>человеком</w> и не терять совести.",
                            "target_lang_translations": {
                                "en": "In any situation, one must remain a <w>human being</w> and not lose one's conscience."
                            }
                        },
                        {
                            "text": "Он поступил как настоящий <w>человек</w>, защитив слабого.",
                            "target_lang_translations": {
                                "en": "He acted like a real <w>human being</w> by protecting the weak."
                            }
                        }
                    ],
                    "antonyms": [
                        "нелюдь",
                        "зверь"
                    ],
                    "common_phrases": [
                        "с большой буквы",
                        "настоящий человек",
                        "будь человеком"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "в значении «достойная личность»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person possessing high moral qualities and dignity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "human being",
                                "target_lang_sense_clarification": "moral agent"
                            },
                            {
                                "target_lang_word": "decent person",
                                "target_lang_sense_clarification": "moral quality"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9592c1cd-017f-3275-822e-0a588f4bc5e2",
                    "sense_definition": "Слуга в трактире, ресторане или гостинице; официант.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Посетитель подозвал <w>человека</w> и попросил принести счёт.",
                            "target_lang_translations": {
                                "en": "The visitor called the <w>waiter</w> and asked for the bill."
                            }
                        },
                        {
                            "text": "<w>Человек</w> быстро накрыл на стол и подал горячий чай.",
                            "target_lang_translations": {
                                "en": "The <w>waiter</w> quickly set the table and served hot tea."
                            }
                        }
                    ],
                    "synonyms": [
                        "официант",
                        "слуга",
                        "половой"
                    ],
                    "antonyms": [
                        "хозяин",
                        "трактирщик"
                    ],
                    "common_phrases": [
                        "эй человек"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "историческое"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A servant or waiter in a restaurant or hotel (archaic)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "waiter",
                                "target_lang_sense_clarification": "archaic"
                            },
                            {

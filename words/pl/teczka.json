    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5fb306bf-20cb-3713-ab20-8d098aaf6fa8",
                    "sense_definition": "Prostokątna, usztywniana torba z uchwytem, służąca zazwyczaj do noszenia dokumentów lub laptopa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "bag_container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój tata codziennie zabiera do biura czarną, skórzaną <w>teczkę</w>.",
                            "target_lang_translations": {
                                "ru": "Мой папа каждый день берет в офис черный кожаный <w>портфель</w>."
                            }
                        },
                        {
                            "text": "Mężczyzna postawił ciężką <w>teczkę</w> na podłodze i usiadł zmęczony na krześle.",
                            "target_lang_translations": {
                                "ru": "Мужчина поставил тяжелый <w>портфель</w> на пол и устало сел на стул."
                            }
                        }
                    ],
                    "synonyms": [
                        "aktówka",
                        "neseser"
                    ],
                    "common_phrases": [
                        "skórzana teczka",
                        "nosić w teczce",
                        "otworzyć teczkę",
                        "ciężka teczka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямоугольная жесткая сумка с ручкой, обычно используемая для переноски документов или ноутбука."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портфель",
                                "target_lang_sense_clarification": "сумка для документов"
                            },
                            {
                                "target_lang_word": "дипломат",
                                "target_lang_sense_clarification": "жесткий плоский чемоданчик"
                            },
                            {
                                "target_lang_word": "кейс",
                                "target_lang_sense_clarification": "современный жесткий портфель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5db3c1fb-8623-3cca-8ab4-bc76ae2fe988",
                    "sense_definition": "Okładka wykonana z tektury lub tworzywa sztucznego, służąca do przechowywania i porządkowania luźnych kartek.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "stationery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczeń wyciągnął z plecaka kolorową <w>teczkę</w>, w której trzymał swoje rysunki.",
                            "target_lang_translations": {
                                "ru": "Ученик вытащил из рюкзака цветную <w>папку</w>, в которой хранил свои рисунки."
                            }
                        },
                        {
                            "text": "Muszę kupić nową <w>teczkę</w> z gumką, żeby uporządkować domowe rachunki.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую <w>папку</w> на резинке, чтобы привести в порядок домашние счета."
                            }
                        }
                    ],
                    "synonyms": [
                        "skoroszyt"
                    ],
                    "common_phrases": [
                        "teczka na dokumenty",
                        "teczka z gumką",
                        "teczka wiązana",
                        "teczka rysunkowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обложка из картона или пластика, служащая для хранения и упорядочивания отдельных листов бумаги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "папка",
                                "target_lang_sense_clarification": "канцелярская принадлежность"
                            },
                            {
                                "target_lang_word": "скоросшиватель",
                                "target_lang_sense_clarification": "папка с механизмом для бумаг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "729435a0-d8b7-3154-a67d-432ab218c9f7",
                    "sense_definition": "Zbiór dokumentów i akt dotyczących konkretnej osoby lub sprawy, często w kontekście administracyjnym lub historycznym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "documents_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dziale kadr założono nową <w>teczkę</w> dla każdego zatrudnionego pracownika.",
                            "target_lang_translations": {
                                "ru": "В отделе кадров завели новое <w>личное дело</w> на каждого нанятого сотрудника."
                            }
                        },
                        {
                            "text": "Historycy znaleźli w archiwum tajną <w>teczkę</w> zawierającą informacje o dawnym szpiegu.",
                            "target_lang_translations": {
                                "ru": "Историки нашли в архиве секретную <w>папку</w>, содержащую информацию о бывшем шпионе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "teczka personalna",
                        "teczka pracownicza",
                        "mieć na kogoś teczkę",
                        "tajna teczka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собрание документов и актов, касающихся конкретного лица или дела, часто в административном или историческом контексте."
                    },
                    "translations": {
                        "ru": [
                            {

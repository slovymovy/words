    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "df8812d6-4536-36a9-8631-4d52a57e1670",
                    "sense_definition": "Nasiono roślin zbożowych, traw lub innych roślin uprawnych, przeznaczone do siewu lub spożycia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "seeds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik wyszedł wczesnym rankiem na pole, aby zasiać <w>ziarno</w>.",
                            "target_lang_translations": {
                                "ru": "Фермер вышел рано утром в поле, чтобы посеять <w>зерно</w>."
                            }
                        },
                        {
                            "text": "Kury na wiejskim podwórku chętnie dziobały rozsypane <w>ziarno</w>.",
                            "target_lang_translations": {
                                "ru": "Куры на деревенском дворе охотно клевали рассыпанное <w>зерно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ziarnko",
                        "ziarnko"
                    ],
                    "common_phrases": [
                        "ziarno pszenicy",
                        "ziarno kawy",
                        "mielić ziarno",
                        "siać ziarno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Семя злаковых растений, трав или других культур, предназначенное для посева или употребления в пищу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зерно",
                                "target_lang_sense_clarification": "семя злаков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36301e5f-bbcb-34f0-8c15-f9d3541cf042",
                    "sense_definition": "Bardzo mała, twarda cząstka jakiejś substancji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "particles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas spaceru po plaży wpadło mi do oka <w>ziarno</w> piasku.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки по пляжу мне в глаз попала <w>песчинка</w>."
                            }
                        },
                        {
                            "text": "Do gotującej się zupy wrzuciłem dwa czarne <w>ziarna</w> pieprzu.",
                            "target_lang_translations": {
                                "ru": "В кипящий суп я бросил две черные <w>горошины</w> перца."
                            }
                        }
                    ],
                    "synonyms": [
                        "ziarnko"
                    ],
                    "common_phrases": [
                        "ziarno piasku",
                        "ziarno pieprzu",
                        "ziarno maku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень маленькая, твердая частица какого-либо вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крупинка",
                                "target_lang_sense_clarification": "маленькая частица"
                            },
                            {
                                "target_lang_word": "зерно",
                                "target_lang_sense_clarification": "частица"
                            },
                            {
                                "target_lang_word": "горошина",
                                "target_lang_sense_clarification": "о перце"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fe88722-c0e0-35f3-b87e-2d157dfc6486",
                    "sense_definition": "Zaczątek, źródło lub ślad jakiegoś uczucia, stanu lub zjawiska.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdej, nawet najdziwniejszej plotce, zazwyczaj kryje się <w>ziarno</w> prawdy.",
                            "target_lang_translations": {
                                "ru": "В каждой, даже самой странной сплетне, обычно скрывается <w>зерно</w> истины."
                            }
                        },
                        {
                            "text": "Jego nieprzemyślane słowa zasiały <w>ziarno</w> niepewności w całym zespole.",
                            "target_lang_translations": {
                                "ru": "Его необдуманные слова посеяли <w>зерно</w> сомнения во всем коллективе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ziarno prawdy",
                        "ziarno niepewności",
                        "ziarno niezgody",
                        "zasiać ziarno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начало, источник или след какого-либо чувства, состояния или явления (переносное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зерно",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57b5dada-19ca-3be1-8004-8f8fc234ebe8",
                    "sense_definition": "Widoczna, drobnoziarnista struktura obrazu na fotografii, wynikająca z właściwości błony fotograficznej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "photography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To stare czarno-białe zdjęcie ma bardzo wyraźne i artystyczne <w>ziarno</w>.",
                            "target_lang_translations": {
                                "ru": "У этой старой черно-белой фотографии очень четкое и художественное <w>зерно</w>."
                            }
                        },
                        {
                            "text": "Użycie filmu o wysokiej czułości często powoduje większe <w>ziarno</w> na odbitkach.",
                            "target_lang_translations": {
                                "ru": "Использование пленки с высокой чувствительностью часто приводит к более крупному <w>зерну</w> на отпечатках."
                            }
                        }
                    ],
                    "common_phrases": [
                        "widoczne ziarno",
                        "duże ziarno"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "in photography context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Видимая зернистая структура изображения на фотографии, возникающая из-за свойств фотопленки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зерно",
                                "target_lang_sense_clarification": "в фотографии"
                            },
                            {
                                "target_lang_word": "зернистость",
                                "target_lang_sense_clarification": "свойство изображения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7eb7876-3da7-39d3-bd52-1f95425453cc",
                    "sense_definition": "Pojedynczy kryształ wchodzący w skład struktury materiału polikrystalicznego, np. metalu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "crystallography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod mikroskopem elektronowym można dokładnie obserwować poszczególne <w>ziarna</w> metalu.",
                            "target_lang_translations": {
                                "ru": "Под электронным микроскопом можно точно рассмотреть отдельные <w>зерна</w> металла."
                            }
                        },
                        {
                            "text": "Wielkość <w>ziarna</w> ma kluczowy wpływ na twardość i wytrzymałość tego stopu.",
                            "target_lang_translations": {
                                "ru": "Размер <w>зерна</w> имеет ключевое влияние на твердость и прочность этого сплава."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical jargon in metallurgy/geology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный кристалл, входящий в состав структуры поликристаллического материала (например, металла)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зерно",
                                "target_lang_sense_clarification": "кристалл"
                            }
                        ]
                    }
                }
            ]
        }
    ]

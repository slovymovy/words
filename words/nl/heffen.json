    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f75ed7f9-4017-39fd-b3af-37ed592eb13e",
                    "sense_definition": "Iets in opwaartse richting bewegen of optillen, vaak op een plechtige of bewuste manier.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>hieven</w> het glas om hem nog vele gezonde jaren toe te wensen.",
                            "target_lang_translations": {
                                "en": "They <w>raised</w> their glasses to wish him many more healthy years.",
                                "ru": "Они <w>подняли</w> бокалы, чтобы пожелать ему долгих лет здоровья."
                            }
                        },
                        {
                            "text": "Hij <w>hief</w> zijn wijs- en middelvingers in het vredesteken en vertrok.",
                            "target_lang_translations": {
                                "en": "He <w>raised</w> his index and middle fingers in the peace sign and left.",
                                "ru": "Он <w>поднял</w> указательный и средний пальцы в знаке мира и ушел."
                            }
                        }
                    ],
                    "synonyms": [
                        "tillen"
                    ],
                    "common_phrases": [
                        "het glas heffen",
                        "het hoofd heffen",
                        "een lied aanheffen"
                    ],
                    "target_lang_definitions": {
                        "en": "To move something in an upward direction or lift it, often in a solemn or deliberate manner.",
                        "ru": "Перемещать что-либо вверх или поднимать, часто торжественно или намеренно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "raise",
                                "target_lang_sense_clarification": "move upwards (e.g. a glass, a hand)"
                            },
                            {
                                "target_lang_word": "lift",
                                "target_lang_sense_clarification": "pick up or elevate"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поднимать",
                                "target_lang_sense_clarification": "двигать вверх (напр. руку, бокал)"
                            },
                            {
                                "target_lang_word": "воздевать",
                                "target_lang_sense_clarification": "торжественно поднимать (напр. руки к небу)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ce03747-b937-3744-9cb3-bc80f62b35ac",
                    "sense_definition": "Een geldbedrag, meestal belasting of tol, officieel vorderen of laten betalen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "financial_charge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op deze goederen wordt veel belasting <w>geheven</w>.",
                            "target_lang_translations": {
                                "en": "High taxes are <w>levied</w> on these goods.",
                                "ru": "С этих товаров <w>взимается</w> большой налог."
                            }
                        },
                        {
                            "text": "De gemeente besloot om extra parkeergeld te <w>heffen</w> in het centrum.",
                            "target_lang_translations": {
                                "en": "The municipality decided to <w>charge</w> extra parking fees in the center.",
                                "ru": "Муниципалитет решил <w>взимать</w> дополнительную плату за парковку в центре."
                            }
                        }
                    ],
                    "synonyms": [
                        "innen"
                    ],
                    "common_phrases": [
                        "belasting heffen",
                        "tol heffen",
                        "accijns heffen"
                    ],
                    "target_lang_definitions": {
                        "en": "To officially demand or require payment of a sum of money, usually tax or toll.",
                        "ru": "Официально требовать или взимать денежную сумму, обычно налог или пошлину."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "levy",
                                "target_lang_sense_clarification": "collect officially (e.g. taxes)"
                            },
                            {
                                "target_lang_word": "impose",
                                "target_lang_sense_clarification": "forcefully establish (e.g. taxes, rules)"
                            },
                            {
                                "target_lang_word": "charge",
                                "target_lang_sense_clarification": "ask as a price or fee"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "взимать",
                                "target_lang_sense_clarification": "собирать официально (налоги, плату)"
                            },
                            {
                                "target_lang_word": "облагать",
                                "target_lang_sense_clarification": "накладывать налог или сбор"
                            }
                        ]
                    }
                }
            ]

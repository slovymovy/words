    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d5ea16cb-5469-3463-975b-a40c2ef120fe",
                    "sense_definition": "A living organism, such as a tree, shrub, or herb, that typically grows in the ground and absorbs water through its roots.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "living_organism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She watered the <w>plant</w> on the windowsill every morning before breakfast.",
                            "target_lang_translations": {
                                "ru": "Она поливала <w>растение</w> на подоконнике каждое утро перед завтраком."
                            }
                        },
                        {
                            "text": "This type of <w>plant</w> needs a lot of sunlight to grow well.",
                            "target_lang_translations": {
                                "ru": "Этому виду <w>растений</w> нужно много солнечного света, чтобы хорошо расти."
                            }
                        }
                    ],
                    "common_phrases": [
                        "indoor plant",
                        "garden plant",
                        "native plant",
                        "plant species"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живой организм, такой как дерево, кустарник или трава, который обычно растет в земле и поглощает воду корнями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растение",
                                "target_lang_sense_clarification": "биол. организм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dbfe047-0aff-33fe-84fe-70fbd564cb9e",
                    "sense_definition": "A large factory or industrial facility where power is produced or goods are manufactured.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "industrial_facility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My father has worked at the car assembly <w>plant</w> for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мой отец проработал на автосборочном <w>заводе</w> более двадцати лет."
                            }
                        },
                        {
                            "text": "The nuclear power <w>plant</w> supplies electricity to the entire city.",
                            "target_lang_translations": {
                                "ru": "Атомная электро<w>станция</w> снабжает электричеством весь город."
                            }
                        }
                    ],
                    "synonyms": [
                        "plant",
                        "seed"
                    ],
                    "common_phrases": [
                        "power plant",
                        "manufacturing plant",
                        "processing plant",
                        "assembly plant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная фабрика или промышленный объект, где производится энергия или изготавливаются товары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завод",
                                "target_lang_sense_clarification": "промышленное предприятие"
                            },
                            {
                                "target_lang_word": "фабрика",
                                "target_lang_sense_clarification": "производственное предприятие"
                            },
                            {
                                "target_lang_word": "электростанция",
                                "target_lang_sense_clarification": "в контексте 'power plant'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e244509-cf46-37cc-bad2-d23caaecd72a",
                    "sense_definition": "Heavy machinery and equipment used in industry, construction, or earthmoving.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "industrial_machinery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The construction company invested heavily in new <w>plant</w> and equipment for the project.",
                            "target_lang_translations": {
                                "ru": "Строительная компания вложила значительные средства в новую <w>технику</w> и оборудование для проекта."
                            }
                        },
                        {
                            "text": "Operators of heavy <w>plant</w> must undergo rigorous safety training before starting work.",
                            "target_lang_translations": {
                                "ru": "Операторы тяжелой <w>техники</w> должны пройти строгую подготовку по технике безопасности перед началом работы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "heavy plant",
                        "plant hire",
                        "construction plant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжелая техника и оборудование, используемые в промышленности, строительстве или земляных работах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "техника",
                                "target_lang_sense_clarification": "машины и механизмы"
                            },
                            {
                                "target_lang_word": "оборудование",
                                "target_lang_sense_clarification": "оснащение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f63fd9f1-6b45-3ad7-9184-8d522ffe00f4",
                    "sense_definition": "An object placed secretly to deceive someone or to incriminate them.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "deception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The detective realized the gun was a <w>plant</w> intended to frame the suspect.",
                            "target_lang_translations": {
                                "ru": "Детектив понял, что пистолет был <w>подброшен</w>, чтобы подставить подозреваемого."
                            }
                        },
                        {
                            "text": "He claimed the evidence was a <w>plant</w> by his political enemies.",
                            "target_lang_translations": {
                                "ru": "Он утверждал, что доказательства были <w>фальшивкой</w> от его политических врагов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, тайно подброшенный с целью обмана или обвинения кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подброшенная улика",
                                "target_lang_sense_clarification": "ложное доказательство"
                            },
                            {
                                "target_lang_word": "фальшивка",
                                "target_lang_sense_clarification": "разг. подделка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9baec587-79d6-33c5-b9c7-28d243f79e64",
                    "sense_definition": "A person placed in a group or audience to act as a spy or to influence others.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "deception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer acted as a <w>plant</w> inside the criminal organization to gather intelligence.",
                            "target_lang_translations": {
                                "ru": "Полицейский действовал как <w>крот</w> внутри преступной организации для сбора сведений."
                            }
                        },
                        {
                            "text": "Everyone suspected the heckler was a <w>plant</w> sent to disrupt the meeting.",
                            "target_lang_translations": {
                                "ru": "Все подозревали, что крикун был <w>подсадной уткой</w>, подосланной сорвать собрание."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, внедренный в группу или аудиторию, чтобы шпионить или влиять на других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подсадная утка",
                                "target_lang_sense_clarification": "разг. провокатор"
                            },
                            {
                                "target_lang_word": "крот",
                                "target_lang_sense_clarification": "шпион"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b70f0778-acc2-3a40-90ea-3b1f3fd41e94",
                    "sense_definition": "To put a seed, bulb, or young plant into the ground so that it can grow.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "living_organism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to <w>plant</w> some roses in the front garden this spring.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>посадить</w> несколько роз в палисаднике этой весной."
                            }
                        },
                        {
                            "text": "Farmers usually <w>plant</w> their crops early in the season to ensure a good harvest.",
                            "target_lang_translations": {
                                "ru": "Фермеры обычно <w>сажают</w> свои культуры в начале сезона, чтобы обеспечить хороший урожай."
                            }
                        }
                    ],
                    "synonyms": [
                        "seed"
                    ],
                    "common_phrases": [
                        "plant a tree",
                        "plant seeds",
                        "plant a garden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать семя, луковицу или молодое растение в землю, чтобы оно могло расти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сажать",
                                "target_lang_sense_clarification": "процесс посадки"
                            },
                            {
                                "target_lang_word": "посадить",
                                "target_lang_sense_clarification": "завершенное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aaafd804-c799-32db-a6d8-4b7cd6ddb800",
                    "sense_definition": "To place or set something firmly in a specific position.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had to <w>plant</w> his feet firmly to stop from sliding on the ice.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось твердо <w>упереть</w> ноги, чтобы не поскользнуться на льду."
                            }
                        },
                        {
                            "text": "The mountaineer climbed up to <w>plant</w> a flag on the snowy summit.",
                            "target_lang_translations": {
                                "ru": "Альпинист поднялся, чтобы <w>водрузить</w> флаг на заснеженной вершине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plant one's feet",
                        "plant a kiss"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать или устанавливать что-либо твердо в определенном положении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упереть",
                                "target_lang_sense_clarification": "ноги"
                            },
                            {
                                "target_lang_word": "водрузить",
                                "target_lang_sense_clarification": "флаг"
                            },
                            {
                                "target_lang_word": "поставить",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23ed86db-1252-373a-829c-e5d6ea9c6031",
                    "sense_definition": "To hide an object or place a person secretly, often for deceptive purposes.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "deception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The corrupt officer tried to <w>plant</w> drugs in the suspect's car.",
                            "target_lang_translations": {
                                "ru": "Коррумпированный офицер пытался <w>подбросить</w> наркотики в машину подозреваемого."
                            }
                        },
                        {
                            "text": "Terrorists managed to <w>plant</w> a device inside the building before escaping.",
                            "target_lang_translations": {
                                "ru": "Террористам удалось <w>заложить</w> устройство внутри здания перед побегом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plant evidence",
                        "plant a bomb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прятать предмет или тайно размещать человека, часто с целью обмана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подбросить",
                                "target_lang_sense_clarification": "улики, наркотики"
                            },
                            {
                                "target_lang_word": "заложить",
                                "target_lang_sense_clarification": "бомбу"
                            },
                            {
                                "target_lang_word": "подложить",
                                "target_lang_sense_clarification": "спрятать что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a6a773d-3663-3f37-bdb0-c658edfebc44",
                    "sense_definition": "To establish or settle an idea, colony, or group of people.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "establish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher hoped to <w>plant</w> a love of reading in her students.",
                            "target_lang_translations": {
                                "ru": "Учитель надеялась <w>привить</w> любовь к чтению своим ученикам."
                            }

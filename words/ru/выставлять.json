            "senses": [
                {
                    "sense_id": "6a33d021-fb1c-3477-b4dc-fe8548beb24e",
                    "sense_definition": "Помещать предметы в публичном месте для всеобщего обозрения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_display",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Музей планирует <w>выставлять</w> эту редкую коллекцию картин только один месяц."
                        },
                        {
                            "text": "Магазин каждое утро <w>выставляет</w> свежие продукты на витрину."
                        }
                    ],
                    "synonyms": [
                        "демонстрировать",
                        "показывать",
                        "экспонировать"
                    ],
                    "antonyms": [
                        "скрывать",
                        "утаивать"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "70273eff-9d28-3be4-b0af-681737dfd741",
                    "sense_definition": "Ojciec jednego z rodziców (ojca lub matki).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>dziadek</w> ma osiemdziesiąt lat i cieszy się dobrym zdrowiem.",
                            "target_lang_translations": {
                                "ru": "Моему <w>дедушке</w> восемьдесят лет, и он в добром здравии."
                            }
                        },
                        {
                            "text": "W każde wakacje odwiedzam mojego <w>dziadka</w>, który mieszka na wsi.",
                            "target_lang_translations": {
                                "ru": "Каждые каникулы я навещаю своего <w>дедушку</w>, который живет в деревне."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziadek ojczysty",
                        "dziadek macierzysty",
                        "gw. (Górny Śląsk) starzyk"
                    ],
                    "common_phrases": [
                        "przyszywany dziadek",
                        "kochany dziadek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отец одного из родителей (отца или матери)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дедушка",
                                "target_lang_sense_clarification": "отец родителя"
                            },
                            {
                                "target_lang_word": "дед",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0807891-8a9b-3d55-813c-b3ccc78965c7",
                    "sense_definition": "Starszy mężczyzna, człowiek w podeszłym wieku.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jakiś miły <w>dziadek</w> ustąpił mi wczoraj miejsca w tramwaju.",
                            "target_lang_translations": {
                                "ru": "Какой-то милый <w>дедушка</w> уступил мне вчера место в трамвае."
                            }
                        },
                        {
                            "text": "Ten <w>dziadek</w> codziennie rano karmi gołębie w parku.",
                            "target_lang_translations": {
                                "ru": "Этот <w>старик</w> каждое утро кормит голубей в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "staruszek"
                    ],
                    "antonyms": [
                        "młodzieniec",
                        "chłopiec"
                    ],
                    "common_phrases": [
                        "leśny dziadek"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "o starszym mężczyźnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пожилой мужчина, человек преклонного возраста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "старик",
                                "target_lang_sense_clarification": "пожилой человек"
                            },
                            {
                                "target_lang_word": "дедушка",
                                "target_lang_sense_clarification": "ласковое обращение или описание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7cf5faa-de2e-33a6-9f2d-c02004780364",
                    "sense_definition": "Przyrząd kuchenny służący do rozłupywania twardych skorup orzechów.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podaj mi proszę <w>dziadka</w> do orzechów, bo nie mogę otworzyć tego laskowego.",
                            "target_lang_translations": {
                                "ru": "Подай мне, пожалуйста, <w>орехокол</w>, так как я не могу расколоть этот фундук."
                            }
                        },
                        {
                            "text": "Kupiłam nowy, solidny <w>dziadek</w>, który poradzi sobie z każdym orzechem.",
                            "target_lang_translations": {
                                "ru": "Я купила новый прочный <w>орехокол</w>, который справится с любым орехом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dziadek do orzechów"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "skrót od: dziadek do orzechów"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кухонный прибор для раскалывания твердой скорлупы орехов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "орехокол",
                                "target_lang_sense_clarification": "инструмент"
                            },
                            {
                                "target_lang_word": "щелкунчик",
                                "target_lang_sense_clarification": "традиционная фигурка или инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1251fb5f-135c-3d25-9cd1-b53dd6f22a34",
                    "sense_definition": "W grze w brydża: partner rozgrywającego, którego karty są wykładane na stół i który nie bierze aktywnego udziału w rozgrywce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Skoro jest nas tylko trzech, to zagrajmy chociaż z <w>dziadkiem</w>.",
                            "target_lang_translations": {
                                "ru": "Раз нас только трое, сыграем хотя бы с <w>болваном</w>."
                            }
                        },
                        {
                            "text": "Jako <w>dziadek</w> mogłem tylko obserwować grę i dokładać karty na polecenie partnera.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>болваном</w>, я мог только наблюдать за игрой и выкладывать карты по указанию партнера."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В бридже: партнер разыгрывающего, чьи карты выкладываются на стол и который не принимает активного участия в игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болван",
                                "target_lang_sense_clarification": "термин в картах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
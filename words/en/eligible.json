{
    "word_family": [
        "eligibility",
        "ineligibility",
        "ineligible"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "840b9313-ff6b-3aee-8941-7fa3cafcc7b1",
                    "sense_definition": "Meeting the necessary conditions to be selected or to participate in something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must be over eighteen to be <w>eligible</w> to vote in the national election.",
                            "target_lang_translations": {
                                "ru": "Вы должны быть старше восемнадцати лет, чтобы <w>иметь право</w> голосовать на национальных выборах."
                            }
                        },
                        {
                            "text": "She is <w>eligible</w> for a full refund because she returned the item within a week.",
                            "target_lang_translations": {
                                "ru": "Она <w>имеет право</w> на полный возврат средств, так как вернула товар в течение недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "qualified"
                    ],
                    "antonyms": [
                        "ineligible",
                        "unqualified"
                    ],
                    "common_phrases": [
                        "eligible for",
                        "eligible to vote",
                        "eligible candidate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отвечающий необходимым условиям для избрания или участия в чём-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "имеющий право",
                                "target_lang_sense_clarification": "обладающий законным основанием для чего-либо"
                            },
                            {
                                "target_lang_word": "подходящий",
                                "target_lang_sense_clarification": "соответствующий установленным критериям"
                            },
                            {
                                "target_lang_word": "правомочный",
                                "target_lang_sense_clarification": "обладающий юридическим правом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "305f97bb-1cd9-3bb9-954f-d1b3a1386341",
                    "sense_definition": "Desirable and suitable as a partner for marriage.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The magazine published a list of the most <w>eligible</w> bachelors in the city.",
                            "target_lang_translations": {
                                "ru": "Журнал опубликовал список самых <w>завидных</w> холостяков города."
                            }
                        },
                        {
                            "text": "Her parents hoped she would find an <w>eligible</w> young man to marry.",
                            "target_lang_translations": {
                                "ru": "Её родители надеялись, что она найдет <w>подходящего</w> молодого человека для замужества."
                            }
                        }
                    ],
                    "antonyms": [
                        "ineligible"
                    ],
                    "common_phrases": [
                        "eligible bachelor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Желательный и подходящий кандидат для вступления в брак."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завидный",
                                "target_lang_sense_clarification": "о женихе или невесте; привлекательный для брака"
                            },
                            {
                                "target_lang_word": "подходящий",
                                "target_lang_sense_clarification": "соответствующий ожиданиям в качестве партнера"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b1cd28f8-db6d-3aa4-a075-b3d7fabc05bd",
                    "sense_definition": "A person who meets the requirements for something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The department created a database of <w>eligibles</w> for the new government program.",
                            "target_lang_translations": {
                                "ru": "Департамент создал базу данных <w>претендентов</w> на участие в новой правительственной программе."
                            }
                        },
                        {
                            "text": "Only a small percentage of <w>eligibles</w> actually applied for the grant.",
                            "target_lang_translations": {
                                "ru": "Лишь небольшой процент <w>лиц, имеющих право</w>, действительно подали заявку на грант."
                            }
                        }
                    ],
                    "common_phrases": [
                        "list of eligibles"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, соответствующий требованиям для получения чего-либо или участия в чём-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "претендент",
                                "target_lang_sense_clarification": "лицо, претендующее на получение чего-либо"
                            },
                            {
                                "target_lang_word": "кандидат",
                                "target_lang_sense_clarification": "лицо, рассматриваемое для участия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
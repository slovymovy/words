            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8f340737-f032-332b-a7c5-b774ceef0430",
                    "sense_definition": "Describing a dead body that has been preserved, often by drying or embalming, to prevent decay.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Archaeologists were amazed to find the <w>mummified</w> remains of a king inside the tomb.",
                            "target_lang_translations": {
                                "ru": "Археологи были поражены, обнаружив <w>мумифицированные</w> останки царя внутри гробницы."
                            }
                        },
                        {
                            "text": "The dry desert air caused the animal to become naturally <w>mummified</w> over time.",
                            "target_lang_translations": {
                                "ru": "Сухой воздух пустыни привел к тому, что животное со временем естественным образом <w>мумифицировалось</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "preserved",
                        "embalmed",
                        "desiccated"
                    ],
                    "antonyms": [
                        "decayed",
                        "decomposed"
                    ],
                    "common_phrases": [
                        "mummified remains",
                        "mummified body",
                        "naturally mummified"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывает мертвое тело, которое было сохранено, часто путем высушивания или бальзамирования, для предотвращения разложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мумифицированный",
                                "target_lang_sense_clarification": "превращенный в мумию, сохраненный"
                            }
                        ]
                    }
                }
            ]
        },
        {

                },
                {
                    "sense_id": "763dba03-5d50-354f-b899-d57f4310b9b9",
                    "sense_definition": "An erection of the penis.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teenager was embarrassed when he woke up with morning <w>wood</w>.",

{
    "word_family": [
        "transferable",
        "transference",
        "transferee",
        "transferor",
        "transferability"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "db2c232b-3e78-3e43-b29f-86edf9bf4c43",
                    "sense_definition": "To move or pass someone or something from one place, person, or situation to another.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>transfer</w> the funds to my savings account.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>переведите</w> средства на мой сберегательный счет."
                            }
                        },
                        {
                            "text": "The company decided to <w>transfer</w> him to the London office.",
                            "target_lang_translations": {
                                "ru": "Компания решила <w>перевести</w> его в лондонский офис."
                            }
                        }
                    ],
                    "synonyms": [
                        "move",
                        "carry over"
                    ],
                    "common_phrases": [
                        "transfer money",
                        "transfer data",
                        "transfer files",
                        "transfer power"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать или передавать кого-либо или что-либо из одного места, от одного лица или ситуации к другому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переводить",
                                "target_lang_sense_clarification": "деньги, сотрудников"
                            },
                            {
                                "target_lang_word": "перемещать",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "физически или абстрактно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6333a630-975c-34c4-b47a-d29a0f2b29fa",
                    "sense_definition": "To change from one vehicle or transportation line to another during a journey.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>transfer</w> to the subway at the next station.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>пересесть</w> на метро на следующей станции."
                            }
                        },
                        {
                            "text": "Passengers must <w>transfer</w> to a bus because of the track maintenance.",
                            "target_lang_translations": {
                                "ru": "Пассажиры должны <w>пересесть</w> на автобус из-за ремонта путей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "transfer trains",
                        "transfer to a bus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пересаживаться с одного транспортного средства на другое во время поездки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пересаживаться",
                                "target_lang_sense_clarification": "смена транспорта"
                            },
                            {
                                "target_lang_word": "делать пересадку",
                                "target_lang_sense_clarification": "фраза"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6fef86d-e6a7-3a02-841b-fad86483cb83",
                    "sense_definition": "To officially arrange for property, rights, or responsibilities to belong to someone else.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to <w>transfer</w> the title of the car to her son.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>передать</w> право собственности на машину своему сыну."
                            }
                        },
                        {
                            "text": "Copyright is automatically <w>transferred</w> to the publisher upon signing.",
                            "target_lang_translations": {
                                "ru": "Авторское право автоматически <w>передается</w> издателю после подписания."
                            }
                        }
                    ],
                    "synonyms": [
                        "assign",
                        "convey",
                        "grant",
                        "change hands"
                    ],
                    "common_phrases": [
                        "transfer ownership",
                        "transfer title",
                        "transfer rights"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально передавать права, собственность или обязанности другому лицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "права, собственность"
                            },
                            {
                                "target_lang_word": "переоформлять",
                                "target_lang_sense_clarification": "документы на другого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f24197b-1207-3af7-ac30-7fa28244e695",
                    "sense_definition": "To copy a design or pattern from one surface to another.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist used a special technique to <w>transfer</w> the image onto the canvas.",
                            "target_lang_translations": {
                                "ru": "Художник использовал специальную технику, чтобы <w>перенести</w> изображение на холст."
                            }
                        },
                        {
                            "text": "You can <w>transfer</w> the pattern to the fabric using a hot iron.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>перевести</w> узор на ткань с помощью горячего утюга."
                            }
                        }
                    ],
                    "synonyms": [
                        "copy",
                        "transpose"
                    ],
                    "target_lang_definitions": {
                        "ru": "Копировать рисунок или узор с одной поверхности на другую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переводить",
                                "target_lang_sense_clarification": "рисунок, узор"
                            },
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "изображение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a15051a6-a89e-3a23-a18d-b264ff22a605",
                    "sense_definition": "The act or process of moving someone or something from one place, person, or position to another.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>transfer</w> of information between the two computers was very fast.",
                            "target_lang_translations": {
                                "ru": "<w>Передача</w> информации между двумя компьютерами прошла очень быстро."
                            }
                        },
                        {
                            "text": "He requested a <w>transfer</w> to a different department within the company.",
                            "target_lang_translations": {
                                "ru": "Он попросил о <w>переводе</w> в другой отдел внутри компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "transferal",
                        "transference"
                    ],
                    "common_phrases": [
                        "bank transfer",
                        "data transfer",
                        "file transfer",
                        "wire transfer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс перемещения кого-либо или чего-либо из одного места, от одного лица или должности к другому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевод",
                                "target_lang_sense_clarification": "денежный, по службе"
                            },
                            {
                                "target_lang_word": "передача",
                                "target_lang_sense_clarification": "данных, информации"
                            },
                            {
                                "target_lang_word": "трансферт",
                                "target_lang_sense_clarification": "финансовый термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1be1d45d-4564-327c-91dd-6a8596af873f",
                    "sense_definition": "The act of changing from one train, bus, or plane to another to continue a journey.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>transfer</w> from the airport to the hotel is included in the price.",
                            "target_lang_translations": {
                                "ru": "<w>Трансфер</w> из аэропорта в отель включен в стоимость."
                            }
                        },
                        {
                            "text": "We have a very tight <w>transfer</w> in Chicago, so we must hurry.",
                            "target_lang_translations": {
                                "ru": "У нас очень короткая <w>пересадка</w> в Чикаго, поэтому нам нужно поторопиться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "airport transfer",
                        "free transfer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смена одного поезда, автобуса или самолета на другой для продолжения путешествия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трансфер",
                                "target_lang_sense_clarification": "услуга перевозки"
                            },
                            {
                                "target_lang_word": "пересадка",
                                "target_lang_sense_clarification": "смена транспорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "034c9f7a-4190-300b-8e6d-c3b929525d0a",
                    "sense_definition": "A ticket or receipt that allows a passenger to continue their journey on another bus or train without paying again.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't forget to ask the driver for a <w>transfer</w> when you pay your fare.",
                            "target_lang_translations": {
                                "ru": "Не забудьте попросить у водителя <w>пересадочный талон</w>, когда будете оплачивать проезд."
                            }
                        },
                        {
                            "text": "This <w>transfer</w> is valid for two hours on any connecting bus.",
                            "target_lang_translations": {
                                "ru": "Этот <w>пересадочный талон</w> действителен в течение двух часов на любом стыковочном автобусе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ask for a transfer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Билет или квитанция, позволяющая пассажиру продолжить поездку на другом автобусе или поезде без повторной оплаты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пересадочный талон",
                                "target_lang_sense_clarification": "билет"
                            },
                            {
                                "target_lang_word": "билет для пересадки",
                                "target_lang_sense_clarification": "проездной документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36d6f63d-90c7-3be0-a513-9a74521ccb8e",
                    "sense_definition": "A student who has moved from one school or university to another.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As a <w>transfer</w>, she had to make new friends quickly.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>переведённой студенткой</w>, ей пришлось быстро заводить новых друзей."
                            }
                        },
                        {
                            "text": "The university accepts many <w>transfers</w> from community colleges.",
                            "target_lang_translations": {
                                "ru": "Университет принимает много <w>студентов по переводу</w> из местных колледжей."
                            }
                        }
                    ],
                    "synonyms": [
                        "transfer student"
                    ],
                    "common_phrases": [
                        "transfer student"
                    ],
                    "target_lang_definitions": {
                        "ru": "Студент, который перешел из одной школы или университета в другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переведённый студент",
                                "target_lang_sense_clarification": "учащийся"
                            },
                            {
                                "target_lang_word": "студент по переводу",
                                "target_lang_sense_clarification": "статус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d64622db-7053-3c45-8de4-4cbdf62344f7",
                    "sense_definition": "A design or pattern that can be moved from one surface to another, usually by heat or pressure.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decorated her t-shirt with a colorful iron-on <w>transfer</w>.",
                            "target_lang_translations": {
                                "ru": "Она украсила свою футболку красочной <w>термонаклейкой</w>."
                            }
                        },
                        {
                            "text": "The model kit includes several water-slide <w>transfers</w> for the wings.",
                            "target_lang_translations": {
                                "ru": "В комплект модели входит несколько <w>переводных картинок</w> для крыльев."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iron-on transfer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рисунок или узор, который можно перенести с одной поверхности на другую, обычно с помощью тепла или давления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переводная картинка",
                                "target_lang_sense_clarification": "декор"
                            },
                            {
                                "target_lang_word": "термонаклейка",
                                "target_lang_sense_clarification": "для одежды"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b675cd3-816f-3402-a035-af177a0a5efd",
                    "sense_definition": "Устройство для автоматического подсчёта количества чего-либо (расхода воды, газа, электроэнергии и т. п.)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "measuring_devices",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В прошлом месяце мы установили в квартире новый <w>счётчик</w> воды.",
                            "target_lang_translations": {
                                "en": "Last month we installed a new water <w>meter</w> in the apartment."
                            }
                        },
                        {
                            "text": "Жильцы обязаны ежемесячно передавать показания электрического <w>счётчика</w>.",
                            "target_lang_translations": {
                                "en": "Tenants are required to submit electric <w>meter</w> readings monthly."
                            }
                        }
                    ],
                    "common_phrases": [
                        "счётчик воды",
                        "газовый счётчик",
                        "электрический счётчик",
                        "показания счётчика",
                        "установить счётчик"
                    ],
                    "target_lang_definitions": {
                        "en": "A device that measures and records the quantity of something consumed, such as water, gas, or electricity"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meter",
                                "target_lang_sense_clarification": "for utilities like water, gas, electricity"
                            },
                            {
                                "target_lang_word": "counter",
                                "target_lang_sense_clarification": "general mechanism for counting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3167345-87d1-34b3-a744-935c2dabeb75",
                    "sense_definition": "Прибор в такси, определяющий стоимость проезда, или устройство в автомобиле для измерения пройденного расстояния",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "measuring_devices",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель такси молча включил <w>счётчик</w> и тронулся с места.",
                            "target_lang_translations": {
                                "en": "The taxi driver silently turned on the <w>meter</w> and drove off."
                            }
                        },
                        {
                            "text": "Мы заплатили за поездку строго по <w>счётчику</w>.",
                            "target_lang_translations": {
                                "en": "We paid for the ride strictly according to the <w>meter</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "таксометр",
                        "одометр"
                    ],
                    "common_phrases": [
                        "включить счётчик",
                        "платить по счётчику",
                        "счётчик такси"
                    ],
                    "target_lang_definitions": {
                        "en": "A device in a taxi that calculates the fare based on distance and time"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meter",
                                "target_lang_sense_clarification": "in a taxi"
                            },
                            {
                                "target_lang_word": "taximeter",
                                "target_lang_sense_clarification": "technical term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20903da1-9d22-34bb-a178-5050017829f4",
                    "sense_definition": "Прибор для регистрации и измерения ионизирующих излучений и ядерных частиц",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "measuring_devices",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Счётчик</w> Гейгера начал издавать тревожный треск.",
                            "target_lang_translations": {
                                "en": "The Geiger <w>counter</w> began to make an alarming crackling sound."
                            }
                        },
                        {
                            "text": "Для измерения уровня радиации физики использовали специальный <w>счётчик</w>.",
                            "target_lang_translations": {
                                "en": "Physicists used a special <w>counter</w> to measure radiation levels."
                            }
                        }
                    ],
                    "common_phrases": [
                        "счётчик Гейгера",
                        "счётчик частиц"
                    ],
                    "target_lang_definitions": {
                        "en": "An instrument used for detecting and measuring ionizing radiation or nuclear particles"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "counter",
                                "target_lang_sense_clarification": "e.g., Geiger counter"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc58842d-8caf-3583-b3de-56265df79f87",
                    "sense_definition": "Человек, в обязанности которого входит подсчёт кого-либо или чего-либо",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На избирательном участке <w>счётчики</w> внимательно проверяли каждый бюллетень.",
                            "target_lang_translations": {
                                "en": "At the polling station, the <w>counters</w> carefully checked every ballot."
                            }
                        },
                        {
                            "text": "Опытный <w>счётчик</w> в банке может пересчитать пачку купюр за несколько секунд.",
                            "target_lang_translations": {
                                "en": "An experienced <w>teller</w> at the bank can count a stack of bills in a few seconds."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person whose job is to count people or things"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "counter",
                                "target_lang_sense_clarification": "person who counts"
                            },
                            {
                                "target_lang_word": "teller",
                                "target_lang_sense_clarification": "bank employee"
                            },
                            {
                                "target_lang_word": "census taker",
                                "target_lang_sense_clarification": "person who counts population"
                            }
                        ]
                    }
                }
            ]
        }
    ]

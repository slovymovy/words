    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8bf67433-216f-3d08-9b96-1ceec20fb7a9",
                    "sense_definition": "The present participle and gerund of <w>drink</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>drinking</w> a glass of fresh orange juice.",
                            "target_lang_translations": {
                                "ru": "Она <w>пьёт</w> стакан свежего апельсинового сока."
                            }
                        },
                        {
                            "text": "They were <w>drinking</w> coffee in the kitchen this morning.",
                            "target_lang_translations": {
                                "ru": "Они <w>пили</w> кофе на кухне сегодня утром."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола drink."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пить",
                                "target_lang_sense_clarification": "глагол"
                            },
                            {
                                "target_lang_word": "выпивать",
                                "target_lang_sense_clarification": "глагол, процесс"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8ac922e5-0739-3c62-b7d0-1817b4ca82f5",
                    "sense_definition": "The act of consuming liquids, especially alcoholic beverages.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "alcohol_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heavy <w>drinking</w> can lead to serious health problems.",
                            "target_lang_translations": {
                                "ru": "Сильное <w>пьянство</w> может привести к серьезным проблемам со здоровьем."
                            }
                        },
                        {
                            "text": "He decided to stop <w>drinking</w> for a month.",
                            "target_lang_translations": {
                                "ru": "Он решил бросить <w>пить</w> на месяц."
                            }
                        }
                    ],
                    "common_phrases": [
                        "heavy drinking",
                        "binge drinking",
                        "underage drinking",
                        "social drinking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по потреблению жидкостей, особенно алкогольных напитков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пьянство",
                                "target_lang_sense_clarification": "чрезмерное употребление алкоголя"
                            },
                            {
                                "target_lang_word": "питьё",
                                "target_lang_sense_clarification": "процесс потребления жидкости"
                            },
                            {
                                "target_lang_word": "употребление алкоголя",
                                "target_lang_sense_clarification": "формальное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f00626e3-d0f7-3f65-845b-3a33df0768f7",

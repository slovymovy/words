    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8e6d9873-c8ae-3100-96b7-02ac235a22e5",
                    "sense_definition": "Een toneelstuk, film of verhaal dat bedoeld is om mensen te laten lachen en meestal een goede afloop heeft.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben gisteren in de bioscoop hard gelachen om een nieuwe <w>komedie</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера в кино мы от души посмеялись над новой <w>комедией</w>."
                            }
                        },
                        {
                            "text": "William Shakespeare schreef naast tragedies ook enkele bekende <w>komedies</w>.",
                            "target_lang_translations": {
                                "ru": "Помимо трагедий, Уильям Шекспир написал также несколько известных <w>комедий</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "blijspel"
                    ],
                    "antonyms": [
                        "tragedie"
                    ],
                    "common_phrases": [
                        "romantische komedie",
                        "een komedie kijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пьеса, фильм или история, предназначенные для того, чтобы рассмешить людей, и обычно имеющие счастливый конец."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комедия",
                                "target_lang_sense_clarification": "жанр кино или театра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98dd968f-075f-356b-b98e-d1b2d73675d7",
                    "sense_definition": "Gedrag dat niet echt is, maar gespeeld wordt om indruk te maken of anderen te misleiden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn tranen waren slechts <w>komedie</w> om medelijden te wekken bij de jury.",
                            "target_lang_translations": {
                                "ru": "Его слезы были лишь <w>комедией</w>, чтобы вызвать жалость у жюри."
                            }
                        },
                        {
                            "text": "Ze had meteen door dat hij <w>komedie</w> aan het spelen was.",
                            "target_lang_translations": {
                                "ru": "Она сразу поняла, что он ломает <w>комедию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "blijspel"
                    ],
                    "common_phrases": [
                        "komedie spelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неискреннее поведение, разыгрываемое с целью произвести впечатление или ввести других в заблуждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "притворство",
                                "target_lang_sense_clarification": "неискреннее поведение"
                            },
                            {
                                "target_lang_word": "комедия",
                                "target_lang_sense_clarification": "фарс, игра на публику"
                            }
                        ]
                    }
                }
            ]
        }
    ]

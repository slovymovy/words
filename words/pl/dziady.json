                {
                    "sense_id": "d8718915-521f-3037-9588-827c321cd427",
                    "sense_definition": "ludowy obrzęd poświęcony duchom przodków, polegający na nawiązywaniu z nimi relacji i ofiarowaniu im jadła",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Adam Mickiewicz opisał obrzęd dziadów w swoim słynnym dramacie.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2beeba58-1749-3fd2-a921-51fef5377d78",
                    "sense_definition": "A thick, clear, sticky substance used for cosmetic purposes, such as styling hair or washing the body.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cosmetic_gel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He put some <w>gel</w> in his hair to keep it in place all day.",
                            "target_lang_translations": {
                                "ru": "Он нанес немного <w>геля</w> на волосы, чтобы прическа держалась весь день."
                            }
                        },
                        {
                            "text": "I need to buy a new bottle of shower <w>gel</w> from the supermarket.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить в супермаркете новый флакон <w>геля</w> для душа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hair gel",
                        "shower gel",
                        "styling gel",
                        "shaving gel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Густое, прозрачное, липкое вещество, используемое в косметических целях, например, для укладки волос или мытья тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гель",
                                "target_lang_sense_clarification": "косметическое средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6336a863-e76f-38e4-acd5-74d2870c67ba",
                    "sense_definition": "A semi-solid substance that is soft and jelly-like, often used in science or medicine.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_gel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist applied a cooling <w>gel</w> to the burn to soothe the pain.",
                            "target_lang_translations": {
                                "ru": "Ученый нанес охлаждающий <w>гель</w> на ожог, чтобы унять боль."
                            }
                        },
                        {
                            "text": "As the mixture cools down, it will turn into a firm <w>gel</w>.",
                            "target_lang_translations": {
                                "ru": "Когда смесь остынет, она превратится в твердый <w>гель</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "jelly"
                    ],
                    "common_phrases": [
                        "silica gel",
                        "aloe vera gel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полутвердое вещество, мягкое и желеобразное, часто используемое в науке или медицине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гель",
                                "target_lang_sense_clarification": "физическое состояние вещества"
                            },
                            {
                                "target_lang_word": "желе",
                                "target_lang_sense_clarification": "студенистая масса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8dd9589-c562-30ed-b1d4-f3890152afab",
                    "sense_definition": "A thin sheet of colored plastic placed in front of a light source to change the color of the light, used in theater or photography.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lighting technician placed a blue <w>gel</w> over the spotlight to create a night scene.",
                            "target_lang_translations": {
                                "ru": "Осветитель поместил синий <w>светофильтр</w> на прожектор, чтобы создать ночную сцену."
                            }
                        },
                        {
                            "text": "We need to replace the torn <w>gel</w> on the main stage light.",
                            "target_lang_translations": {
                                "ru": "Нам нужно заменить порванный <w>светофильтр</w> на главном сценическом прожекторе."
                            }
                        }
                    ],
                    "synonyms": [
                        "filter"
                    ],
                    "common_phrases": [
                        "color gel",
                        "lighting gel"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий лист цветного пластика, помещаемый перед источником света для изменения его цвета (в театре или фотографии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светофильтр",
                                "target_lang_sense_clarification": "для прожектора"
                            },
                            {
                                "target_lang_word": "цветной фильтр",
                                "target_lang_sense_clarification": "пленка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3630434-2d22-3575-aec5-c8d431ab9e72",
                    "sense_definition": "A girl.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old man waved at the young <w>gel</w> walking down the street.",
                            "target_lang_translations": {
                                "ru": "Старик помахал рукой молодой <w>девушке</w>, идущей по улице."
                            }
                        },
                        {
                            "text": "She is a very sensible <w>gel</w> and always does her homework.",
                            "target_lang_translations": {
                                "ru": "Она очень разумная <w>девица</w> и всегда делает уроки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "pronunciation spelling"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "British upper-class slang"

            "senses": [
                {
                    "sense_id": "e9b0a565-7837-3cd4-a887-09d604f38466",
                    "sense_definition": "Meervoud van <w>meer</w>; grote wateroppervlaktes die geheel door land zijn omringd.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer gaan we vaak zwemmen in de nabijgelegen <w>meren</w>.",
                            "target_lang_translations": {
                                "ru": "Летом мы часто ходим плавать в близлежащих <w>озёрах</w>.",
                                "en": "In the summer, we often go swimming in the nearby <w>lakes</w>."
                            }
                        },
                        {
                            "text": "Finland wordt ook wel het land van de duizend <w>meren</w> genoemd.",
                            "target_lang_translations": {
                                "ru": "Финляндию также называют страной тысячи <w>озёр</w>.",
                                "en": "Finland is also called the land of a thousand <w>lakes</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grote meren",
                        "diepe meren"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от <w>meer</w>; большие водные пространства, полностью окруженные сушей.",
                        "en": "Plural of <w>meer</w>; large bodies of water entirely surrounded by land."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "озёра",
                                "target_lang_sense_clarification": "водоёмы"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "lakes",
                                "target_lang_sense_clarification": "bodies of water"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d374dfe-98e0-367f-97c4-768308661bae",
                    "sense_definition": "Een schip of boot met lijnen vastmaken aan de wal, kade of een ander vaartuig.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kapitein gaf het bevel om het schip te <w>meren</w>.",
                            "target_lang_translations": {
                                "ru": "Капитан отдал приказ <w>пришвартовать</w> корабль.",
                                "en": "The captain gave the order to <w>moor</w> the ship."
                            }
                        },
                        {
                            "text": "Het jacht ligt veilig <w>gemeerd</w> in de haven van Monaco.",
                            "target_lang_translations": {
                                "ru": "Яхта надежно <w>пришвартована</w> в порту Монако.",
                                "en": "The yacht is safely <w>moored</w> in the harbor of Monaco."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanleggen",
                        "vastleggen",
                        "vastmaken"
                    ],
                    "antonyms": [
                        "ontmeren",
                        "losmaken"
                    ],
                    "common_phrases": [
                        "een schip meren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привязывать судно или лодку тросами к берегу, причалу или другому судну.",
                        "en": "To fasten a ship or boat with lines to the shore, quay, or another vessel."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "швартовать",
                                "target_lang_sense_clarification": "крепить судно"
                            },
                            {
                                "target_lang_word": "причаливать",
                                "target_lang_sense_clarification": "подходить к причалу"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "moor",
                                "target_lang_sense_clarification": "secure a vessel"
                            },
                            {
                                "target_lang_word": "dock",
                                "target_lang_sense_clarification": "bring into a harbor"
                            }
                        ]
                    }
                }
            ]
        }
    ]

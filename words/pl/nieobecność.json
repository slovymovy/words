{
    "word_family": [
        "nieobecny",
        "nieobecnie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5574120b-7cab-329e-890e-5bc4c110b528",
                    "sense_definition": "Stan, w którym dana osoba nie znajduje się w określonym miejscu lub czasie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci nie mogły pogodzić się z <w>nieobecnością</w> ojca w ich życiu.",
                            "target_lang_translations": {
                                "ru": "Дети не могли смириться с <w>отсутствием</w> отца в их жизни."
                            }
                        },
                        {
                            "text": "Zauważyłem twoją <w>nieobecność</w> na wczorajszym spotkaniu rodzinnym.",
                            "target_lang_translations": {
                                "ru": "Я заметил твое <w>отсутствие</w> на вчерашней семейной встрече."
                            }
                        }
                    ],
                    "synonyms": [
                        "brak",
                        "absencja"
                    ],
                    "antonyms": [
                        "obecność"
                    ],
                    "common_phrases": [
                        "długa nieobecność",
                        "podczas nieobecności",
                        "zauważyć nieobecność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, при котором человек не находится в определенном месте или в определенное время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отсутствие",
                                "target_lang_sense_clarification": "нахождение в другом месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4de75e34-968e-3c3e-b500-1a5e54c4fb92",
                    "sense_definition": "Fakt niepojawienia się w pracy, szkole lub na innych obowiązkowych zajęciach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam obniżone sprawowanie, bo miałem dużo nieusprawiedliwionych <w>nieobecności</w>.",
                            "target_lang_translations": {
                                "ru": "У меня снижена оценка за поведение, так как у меня было много неоправданных <w>пропусков</w>."
                            }
                        },
                        {
                            "text": "Musisz usprawiedliwić każdą <w>nieobecność</w> w szkole zwolnieniem lekarskim.",
                            "target_lang_translations": {
                                "ru": "Ты должен оправдать каждое <w>отсутствие</w> в школе медицинской справкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "absencja"
                    ],
                    "antonyms": [
                        "obecność"
                    ],
                    "common_phrases": [
                        "usprawiedliwić nieobecność",
                        "nieusprawiedliwiona nieobecność",
                        "lista nieobecności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт неявки на работу, в школу или на другие обязательные занятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отсутствие",
                                "target_lang_sense_clarification": "неявка"
                            },
                            {
                                "target_lang_word": "пропуск",
                                "target_lang_sense_clarification": "занятий или работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6e34632-82ce-3a9d-90db-c04e167c0c52",
                    "sense_definition": "Stan psychiczny charakteryzujący się brakiem uwagi, skupienia lub byciem zamyślonym.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>nieobecność</w> podczas ważnej rozmowy była irytująca dla wszystkich.",
                            "target_lang_translations": {
                                "ru": "Его <w>отрешённость</w> во время важного разговора раздражала всех."
                            }
                        },
                        {
                            "text": "Przez chwilową <w>nieobecność</w> uczeń nie usłyszał pytania nauczyciela.",
                            "target_lang_translations": {
                                "ru": "Из-за минутной <w>рассеянности</w> ученик не услышал вопроса учителя."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozkojarzenie",
                        "nieuwaga",
                        "dekoncentracja"
                    ],
                    "common_phrases": [
                        "chwila nieobecności",
                        "nieobecność ducha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психическое состояние, характеризующееся недостатком внимания, сосредоточенности или задумчивостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассеянность",
                                "target_lang_sense_clarification": "невнимательность"
                            },
                            {
                                "target_lang_word": "отрешённость",
                                "target_lang_sense_clarification": "ментальное отсутствие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            },
                            {
                                "target_lang_word": "глохнет",
                                "target_lang_sense_clarification": "о двигателе или механизме"
                            },
                            {
                                "target_lang_word": "погибает",
                                "target_lang_sense_clarification": "трагическая или насильственная смерть"
                            },
                            {
                                "target_lang_word": "угасает",
                                "target_lang_sense_clarification": "постепенное исчезновение (о звуке, свете, надежде)"
                            }

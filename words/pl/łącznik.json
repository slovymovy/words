    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "33fd8b79-2901-32f5-a53b-7d1ca0008cab",
                    "sense_definition": "Znak graficzny w postaci krótkiej poziomej kreski (-), używany w pisowni do łączenia wyrazów złożonych lub przy przenoszeniu części wyrazu do nowej linii.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "punctuation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To podwójne nazwisko pisze się przez <w>łącznik</w>.",
                            "target_lang_translations": {
                                "ru": "Эта двойная фамилия пишется через <w>дефис</w>."
                            }
                        },
                        {
                            "text": "Jeśli słowo nie mieści się w linijce, użyj <w>łącznika</w> i przenieś resztę niżej.",
                            "target_lang_translations": {
                                "ru": "Если слово не помещается в строке, используй <w>дефис</w> и перенеси остальное ниже."
                            }
                        }
                    ],
                    "synonyms": [
                        "dywiz",
                        "kreseczka"
                    ],
                    "common_phrases": [
                        "pisać z łącznikiem",
                        "nazwisko z łącznikiem",
                        "brak łącznika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Графический знак в виде короткой горизонтальной черты (-), используемый при написании сложных слов или переносе части слова на новую строку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дефис",
                                "target_lang_sense_clarification": "знак препинания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edab94c6-c5f2-3204-b92c-434e11096141",
                    "sense_definition": "Element, przedmiot lub urządzenie służące do fizycznego połączenia dwóch części, rur lub przewodów w jedną całość.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mechanical_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hydraulik musiał wymienić pęknięty <w>łącznik</w> pod zlewem.",
                            "target_lang_translations": {
                                "ru": "Сантехнику пришлось заменить треснувший <w>соединитель</w> под раковиной."
                            }
                        },
                        {
                            "text": "Kupiliśmy plastikowy <w>łącznik</w>, aby połączyć te dwa kable.",
                            "target_lang_translations": {
                                "ru": "Мы купили пластиковый <w>соединитель</w>, чтобы соединить эти два кабеля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łącznik rur",
                        "łącznik elektryczny",
                        "łącznik budowlany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент, предмет или устройство, служащее для физического соединения двух частей, труб или проводов в единое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединитель",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "муфта",
                                "target_lang_sense_clarification": "для труб"
                            },
                            {
                                "target_lang_word": "переходник",
                                "target_lang_sense_clarification": "адаптер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60ea7d4e-7463-3170-bd03-39c3627f7c0f",
                    "sense_definition": "Osoba odpowiedzialna za utrzymywanie kontaktu i przekazywanie informacji między różnymi grupami, oddziałami wojskowymi lub organizacjami.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "people_roles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W czasie wojny pracował jako <w>łącznik</w> między sztabem a linią frontu.",
                            "target_lang_translations": {
                                "ru": "Во время войны он работал <w>связным</w> между штабом и линией фронта."
                            }
                        },
                        {
                            "text": "On pełni funkcję <w>łącznika</w> pomiędzy zarządem firmy a związkami zawodowymi.",
                            "target_lang_translations": {
                                "ru": "Он выполняет функцию <w>посредника</w> между правлением компании и профсоюзами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oficer łącznikowy",
                        "pełnić rolę łącznika",
                        "łącznik wywiadu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, отвечающий за поддержание контакта и передачу информации между различными группами, воинскими частями или организациями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связной",
                                "target_lang_sense_clarification": "военный термин"
                            },
                            {
                                "target_lang_word": "посредник",
                                "target_lang_sense_clarification": "между группами людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24d63e67-e869-36d1-9631-80fcde16aaf7",
                    "sense_definition": "Korytarz, przejście lub budynek łączący dwa inne obiekty budowlane, np. szkoły lub bloki szpitalne.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczniowie przechodzą do sali gimnastycznej przez oszklony <w>łącznik</w>.",
                            "target_lang_translations": {
                                "ru": "Ученики проходят в спортзал через застекленный <w>переход</w>."
                            }
                        },
                        {
                            "text": "Szpital wybudował nowy <w>łącznik</w>, aby ułatwić transport pacjentów między oddziałami.",
                            "target_lang_translations": {
                                "ru": "Больница построила новый <w>переход</w>, чтобы облегчить транспортировку пациентов между отделениями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szklany łącznik",
                        "łącznik między budynkami",
                        "przejście łącznikiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Коридор, проход или здание, соединяющее два других строительных объекта, например, школы или больничные корпуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "между зданиями"
                            },
                            {
                                "target_lang_word": "галерея",
                                "target_lang_sense_clarification": "архитектурный элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0389541-e67a-3fc6-b14f-3f7922222193",
                    "sense_definition": "Wyraz (najczęściej forma czasownika „być”), który w zdaniu łączy podmiot z orzecznikiem imiennym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W zdaniu „Jan jest lekarzem” słowo „jest” pełni funkcję <w>łącznika</w>.",
                            "target_lang_translations": {
                                "ru": "В предложении «Ян является врачом» слово «является» выполняет функцию <w>связки</w>."
                            }
                        },
                        {
                            "text": "W niektórych językach <w>łącznik</w> jest pomijany w czasie teraźniejszym.",
                            "target_lang_translations": {
                                "ru": "В некоторых языках <w>связка</w> опускается в настоящем времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "spójka",
                        "kopula"
                    ],
                    "common_phrases": [
                        "funkcja łącznika",
                        "orzeczenie z łącznikiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово (чаще всего форма глагола «быть»), которое в предложении связывает подлежащее с именной частью сказуемого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связка",
                                "target_lang_sense_clarification": "лингвистический термин"
                            },
                            {
                                "target_lang_word": "глагол-связка",
                                "target_lang_sense_clarification": "грамматика"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
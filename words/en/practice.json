                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This firm of solicitors is involved in family law <w>practice</w>.",
                            "target_lang_translations": {
                                "ru": "Эта фирма адвокатов занимается <w>практикой</w> семейного права."
                            }
                        },
                        {
                            "text": "Students must learn the codes of civil <w>practice</w> before taking the bar exam.",
                            "target_lang_translations": {
                                "ru": "Студенты должны изучить кодексы гражданского <w>судопроизводства</w> перед сдачей экзамена на адвоката."
                            }
                        }
                    ],
                    "common_phrases": [
                        "code of practice"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {

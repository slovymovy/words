{
    "word_family": [
        "ochać"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "85815479-c203-3bd1-9720-f7aa9889e349",
                    "sense_definition": "Wykrzyknik służący do wyrażania silnych stanów uczuciowych, takich jak zachwyt, zaskoczenie, przestrach lub żal.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Och</w>, jak tu pięknie wygląda o zachodzie słońca!",
                            "target_lang_translations": {
                                "ru": "<w>Ох</w>, как здесь красиво на закате!"
                            }
                        },
                        {
                            "text": "<w>Och</w>, przepraszam, nie zauważyłem, że tu stoisz.",
                            "target_lang_translations": {
                                "ru": "<w>Ох</w>, извини, я не заметил, что ты здесь стоишь."
                            }
                        }
                    ],
                    "synonyms": [
                        "ach",
                        "ojej",
                        "ojejku"
                    ],
                    "common_phrases": [
                        "och nie",
                        "och tak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, служащее для выражения сильных эмоциональных состояний, таких как восхищение, удивление, испуг или сожаление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ох",
                                "target_lang_sense_clarification": "универсальное выражение удивления, сожаления или восхищения"
                            },
                            {
                                "target_lang_word": "ах",
                                "target_lang_sense_clarification": "выражение сильного восхищения или испуга"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d8334510-2158-3c9e-984f-ac3a99fcb407",
                    "sense_definition": "Wyraz zachwytu lub podziwu, używany niemal wyłącznie w liczbie mnogiej w utartych zwrotach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy goście wydawali z siebie głośne <w>ochy</w> i achy na widok tortu.",
                            "target_lang_translations": {
                                "ru": "Все гости издавали громкие <w>охи</w> и ахи при виде торта."
                            }
                        },
                        {
                            "text": "Recenzje filmu to same <w>ochy</w>, ale mnie się on wcale nie podobał.",
                            "target_lang_translations": {
                                "ru": "Рецензии на фильм — сплошные <w>охи</w>, но мне он совсем не понравился."
                            }
                        }
                    ],
                    "common_phrases": [
                        "achy i ochy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение восторга или восхищения, используемое почти исключительно во множественном числе в устойчивых оборотах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охи",
                                "target_lang_sense_clarification": "возгласы (обычно в сочетании «охи и ахи»)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9aa0bad3-29de-380e-b01b-1eb1b6468c47",
                    "sense_definition": "Przyrządzać posiłek, poddając produkty spożywcze działaniu wysokiej temperatury (np. w wodzie, na parze lub w naczyniu).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cooking_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja mama codziennie <w>gotuje</w> pyszny obiad dla całej rodziny.",
                            "target_lang_translations": {
                                "ru": "Моя мама каждый день <w>готовит</w> вкусный обед для всей семьи."
                            }
                        },
                        {
                            "text": "W weekendy bardzo lubię <w>gotować</w> potrawy z kuchni włoskiej.",
                            "target_lang_translations": {
                                "ru": "По выходным я очень люблю <w>готовить</w> блюда итальянской кухни."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyrządzać",
                        "pichcić",
                        "pitrasić",
                        "kucharzyć"
                    ],
                    "common_phrases": [
                        "gotować obiad",
                        "lubić gotować",
                        "gotować zupę",
                        "gotować na parze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовить пищу, подвергая продукты воздействию высокой температуры (например, в воде, на пару или в посуде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовить",
                                "target_lang_sense_clarification": "заниматься приготовлением пищи"
                            },
                            {
                                "target_lang_word": "варить",
                                "target_lang_sense_clarification": "готовить еду, часто используется как синоним в разговорной речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28d6fc30-77e1-3810-8ada-31dd667c931e",
                    "sense_definition": "Doprowadzać płyn do temperatury wrzenia lub trzymać jedzenie we wrzątku w celu jego zmiękczenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cooking_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <w>gotować</w> ziemniaki w osolonej wodzie przez około dwadzieścia minut.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>варить</w> картофель в подсоленной воде около двадцати минут."
                            }
                        },
                        {
                            "text": "Najpierw należy <w>gotować</w> wodę w czajniku, a potem zalać nią herbatę.",
                            "target_lang_translations": {
                                "ru": "Сначала нужно <w>вскипятить</w> воду в чайнике, а затем залить ею чай."
                            }
                        }
                    ],
                    "antonyms": [
                        "zamrażać",
                        "zmrażać"
                    ],
                    "common_phrases": [
                        "gotować wodę",
                        "gotować mleko",
                        "gotować na wolnym ogniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доводить жидкость до кипения или держать продукты в кипятке для их размягчения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "варить",
                                "target_lang_sense_clarification": "кипятить продукты в воде"
                            },
                            {
                                "target_lang_word": "кипятить",
                                "target_lang_sense_clarification": "доводить жидкость до кипения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f026f4f8-9a20-3a85-9940-6c70c539b081",
                    "sense_definition": "(o cieczy) Wrzeć i wydzielać pęcherzyki pary pod wpływem wysokiej temperatury.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cooking_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda w czajniku już <w>się gotuje</w>, więc zaraz zrobię nam kawę.",
                            "target_lang_translations": {
                                "ru": "Вода в чайнике уже <w>кипит</w>, так что я сейчас сделаю нам кофе."
                            }
                        },
                        {
                            "text": "Zupa <w>gotuje się</w> powoli na kuchence od ponad godziny.",
                            "target_lang_translations": {
                                "ru": "Суп медленно <w>варится</w> на плите уже больше часа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "woda się gotuje",
                        "zupa się gotuje"
                    ],
                    "target_lang_definitions": {
                        "ru": "(о жидкости) Кипеть и выделять пузырьки пара под воздействием высокой температуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кипеть",
                                "target_lang_sense_clarification": "бурлить от нагревания"
                            },
                            {
                                "target_lang_word": "вариться",
                                "target_lang_sense_clarification": "приготавливаться (о супе, еде)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe67f052-9d88-3ce7-b480-f2c23b3f7ad9",
                    "sense_definition": "Czynić przygotowania do jakiegoś wydarzenia lub działania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "preparation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy zawodnicy już <w>gotują</w> się do startu w najważniejszym wyścigu sezonu.",
                            "target_lang_translations": {
                                "ru": "Все спортсмены уже <w>готовятся</w> к старту в важнейшей гонке сезона."
                            }
                        },
                        {
                            "text": "Musimy szybko <w>gotować</w> się do wyjścia, bo pociąg odjeżdża za godzinę.",
                            "target_lang_translations": {
                                "ru": "Мы должны быстро <w>собираться</w> к выходу, так как поезд отходит через час."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gotować się do drogi",
                        "gotować się do wyjazdu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать приготовления к какому-либо событию или действию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовиться",
                                "target_lang_sense_clarification": "подготавливать себя к чему-либо"
                            },
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "снаряжаться, готовиться к выходу или отъезду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09476777-04dd-3a4f-86c1-4dd07b7cd012",
                    "sense_definition": "Odczuwać bardzo silne emocje, zazwyczaj gniew, złość lub niepokój.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aż <w>gotowało</w> się we mnie ze złości, gdy słuchałem tych kłamstw.",
                            "target_lang_translations": {
                                "ru": "Во мне всё аж <w>кипело</w> от злости, когда я слушал эту ложь."
                            }
                        },
                        {
                            "text": "Krew <w>gotuje</w> się w żyłach, kiedy widzi się taką jawną niesprawiedliwość.",
                            "target_lang_translations": {
                                "ru": "Кровь <w>кипит</w> в жилах, когда видишь такую явную несправедливость."
                            }
                        }
                    ],
                    "common_phrases": [
                        "krew się gotuje",
                        "gotować się ze złości"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "metaphorical usage describing emotions"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать очень сильные эмоции, обычно гнев, злость или беспокойство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кипеть",
                                "target_lang_sense_clarification": "перен. быть охваченным сильным чувством"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f08f891-1074-37dd-bdc0-06a9212c56af",
                    "sense_definition": "Sprowadzać na kogoś określony los lub przeznaczenie, zazwyczaj trudne lub przykre.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Swoim nieodpowiedzialnym zachowaniem <w>gotujesz</w> sobie bardzo smutną przyszłość.",
                            "target_lang_translations": {
                                "ru": "Своим безответственным поведением ты <w>готовишь</w> себе очень печальное будущее."
                            }
                        },
                        {
                            "text": "Nie wiedzieli, że los <w>gotuje</w> im taką niespodziankę za kolejnym zakrętem.",
                            "target_lang_translations": {
                                "ru": "Они не знали, что судьба <w>готовит</w> им такой сюрприз за очередным поворотом."
                            }
                        }
                    ],
                    "synonyms": [
                        "przynosić"
                    ],
                    "common_phrases": [
                        "gotować komuś los",
                        "gotować piekło"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary or used in fixed phrases"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предопределять чью-либо судьбу или участь, обычно трудную или неприятную."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовить",
                                "target_lang_sense_clarification": "перен. предназначать, сулить (судьбу, участь)"
                            },
                            {
                                "target_lang_word": "уготавливать",
                                "target_lang_sense_clarification": "книжн. предопределять"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
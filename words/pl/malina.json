    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "12829b36-0d50-3c27-811c-d7f666b62181",
                    "sense_definition": "Jadalny owoc o barwie czerwonej lub różowej, rosnący na kolczastym krzewie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "botanical_fruit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem lubię jeść świeże <w>maliny</w> ze śmietaną i cukrem.",
                            "target_lang_translations": {
                                "ru": "Летом я люблю есть свежую <w>малину</w> со сметаной и сахаром."
                            }
                        },
                        {
                            "text": "Babcia przygotowała na zimę pyszny sok z <w>malin</w>.",
                            "target_lang_translations": {
                                "ru": "Бабушка приготовила на зиму вкусный сок из <w>малины</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sok z malin",
                        "zbierać maliny",
                        "słodki jak malina",
                        "syrop z malin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобный плод красного или розового цвета, растущий на колючем кустарнике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "малина",
                                "target_lang_sense_clarification": "ягода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbfce42a-4dbd-35ae-ba4d-0f4de05fee1f",
                    "sense_definition": "Krzew z rodziny różowatych, często posiadający kolce, uprawiany dla smacznych owoców.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "botanical_fruit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym ogrodzie rośnie duża i rozłożysta <w>malina</w>.",
                            "target_lang_translations": {
                                "ru": "В нашем саду растёт большая и раскидистая <w>малина</w>."
                            }
                        },
                        {
                            "text": "Musisz uważać, bo ta dzika <w>malina</w> ma ostre kolce.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть осторожен, потому что у этой дикой <w>малины</w> острые шипы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "krzak maliny",
                        "sadzić maliny",
                        "dzika malina",
                        "liście maliny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кустарник из семейства розовых, часто имеющий шипы, выращиваемый ради вкусных плодов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "малина",
                                "target_lang_sense_clarification": "кустарник"
                            },
                            {
                                "target_lang_word": "малинник",
                                "target_lang_sense_clarification": "заросли малины"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4328f1f4-731a-364a-96aa-edd26948eeca",
                    "sense_definition": "Żeńskie imię używane w Polsce.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja koleżanka <w>Malina</w> zaprosiła mnie na swoje urodziny.",
                            "target_lang_translations": {
                                "ru": "Моя подруга <w>Малина</w> пригласила меня на свой день рождения."
                            }
                        },
                        {
                            "text": "Imię <w>Malina</w> kojarzy się z latem i słodkimi owocami.",
                            "target_lang_translations": {
                                "ru": "Имя <w>Малина</w> ассоциируется с летом и сладкими ягодами."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя, используемое в Польше."

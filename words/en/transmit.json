{
    "word_family": [
        "transmission",
        "transmitter",
        "transmissible",
        "transmittal"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb69a240-119e-3287-a7ca-cde2bd28137a",
                    "sense_definition": "To send out an electronic signal, radio program, or television broadcast.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "transfer_signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local radio station <w>transmits</w> news and music twenty-four hours a day.",
                            "target_lang_translations": {
                                "ru": "Местная радиостанция <w>передает</w> новости и музыку двадцать четыре часа в сутки."
                            }
                        },
                        {
                            "text": "Satellites are used to <w>transmit</w> data across the globe instantly.",
                            "target_lang_translations": {
                                "ru": "Спутники используются для того, чтобы мгновенно <w>передавать</w> данные по всему миру."
                            }
                        }
                    ],
                    "synonyms": [
                        "broadcast"
                    ],
                    "common_phrases": [
                        "transmit a signal",
                        "transmit data",
                        "transmit information"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправлять электронный сигнал, радиопрограмму или телевизионную трансляцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "сигнал, данные"
                            },
                            {
                                "target_lang_word": "транслировать",
                                "target_lang_sense_clarification": "радио или телепередачу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98e8e875-c31e-376a-8174-d3d7da59f68c",
                    "sense_definition": "To pass a disease or infection from one person or organism to another.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "transfer_bio",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mosquitoes can <w>transmit</w> malaria to humans through their bites.",
                            "target_lang_translations": {
                                "ru": "Комары могут <w>передавать</w> малярию людям через укусы."
                            }
                        },
                        {
                            "text": "The virus is easily <w>transmitted</w> through close contact with an infected person.",
                            "target_lang_translations": {
                                "ru": "Вирус легко <w>передается</w> при тесном контакте с зараженным человеком."
                            }
                        }
                    ],
                    "common_phrases": [
                        "transmit a disease",
                        "transmit a virus",
                        "sexually transmitted"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать болезнь или инфекцию от одного человека или организма другому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "болезнь или вирус"
                            },
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "как переносчик инфекции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db6717fd-d9aa-3a89-b997-9ebb8ad27eff",
                    "sense_definition": "To pass on knowledge, ideas, beliefs, or traditions to others.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "transfer_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is the duty of the older generation to <w>transmit</w> their cultural values to the youth.",
                            "target_lang_translations": {
                                "ru": "Долг старшего поколения — <w>передавать</w> свои культурные ценности молодежи."
                            }
                        },
                        {
                            "text": "The teacher worked hard to <w>transmit</w> her passion for literature to her students.",
                            "target_lang_translations": {
                                "ru": "Учительница усердно работала, чтобы <w>передать</w> свою страсть к литературе ученикам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "transmit knowledge",
                        "transmit culture",
                        "transmit values"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать знания, идеи, убеждения или традиции другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "знания, опыт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ee4364d-dd20-33ee-8dcc-9b658d6f5f99",
                    "sense_definition": "To allow energy, light, sound, or force to pass through a medium or mechanism.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "transfer_physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Water <w>transmits</w> sound waves much faster than air does.",
                            "target_lang_translations": {
                                "ru": "Вода <w>проводит</w> звуковые волны гораздо быстрее, чем воздух."
                            }
                        },
                        {
                            "text": "The gears in the machine <w>transmit</w> power from the engine to the wheels.",
                            "target_lang_translations": {
                                "ru": "Шестерни в механизме <w>передают</w> энергию от двигателя к колесам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "transmit energy",
                        "transmit light",
                        "transmit power"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пропускать энергию, свет, звук или силу через среду или механизм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проводить",
                                "target_lang_sense_clarification": "свет, звук"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "энергию, движение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    }
                },
                {
                    "sense_id": "be612d89-19ce-3deb-9b41-be085ae0abba",
                    "sense_definition": "A deep opening, hole, or abyss in the earth.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The stream vanished underground into a dark <w>swallow</w>.",

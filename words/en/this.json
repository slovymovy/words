{
    "word_family": [],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "e0d202f6-a39d-3d61-8346-944cb0b83fdf",
                    "sense_definition": "Used to identify a specific person or thing close at hand or being indicated.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_deixis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>This</w> classroom is where I learned to read and write.",
                            "target_lang_translations": {
                                "ru": "В <w>этом</w> классе я научился читать и писать."
                            }
                        },
                        {
                            "text": "I really like <w>this</w> book.",
                            "target_lang_translations": {
                                "ru": "Мне очень нравится <w>эта</w> книга."
                            }
                        }
                    ],
                    "antonyms": [
                        "that"
                    ],
                    "common_phrases": [
                        "this one",
                        "this way",
                        "this time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на конкретного человека или предмет, находящийся рядом или на который указывают."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "э́тот",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "э́та",
                                "target_lang_sense_clarification": "женский род"
                            },
                            {
                                "target_lang_word": "э́то",
                                "target_lang_sense_clarification": "средний род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e93922c-c857-34be-8647-eed178a4c486",
                    "sense_definition": "Used to indicate the current or next occurrence of a time period.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "temporal_deixis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is very cold <w>this</w> morning.",
                            "target_lang_translations": {
                                "ru": "<w>Сегодня</w> утром очень холодно."
                            }
                        },
                        {
                            "text": "I plan to go to London <w>this</w> Friday.",
                            "target_lang_translations": {
                                "ru": "Я планирую поехать в Лондон в <w>эту</w> пятницу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "this morning",
                        "this week",
                        "this year",
                        "this time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на текущий или ближайший временной период."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "э́тот",
                                "target_lang_sense_clarification": "о времени (напр. в этом году)"
                            },
                            {
                                "target_lang_word": "теку́щий",
                                "target_lang_sense_clarification": "нынешний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "623683db-d55d-3688-9d10-bc4ca3864ab0",
                    "sense_definition": "Used to refer to a person, thing, or idea that has just been mentioned or is already known.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "discourse_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They have a plan, but <w>this</w> plan is dangerous.",
                            "target_lang_translations": {
                                "ru": "У них есть план, но <w>этот</w> план опасен."
                            }
                        },
                        {
                            "text": "He made a mistake, and <w>this</w> mistake cost him his job.",
                            "target_lang_translations": {
                                "ru": "Он совершил ошибку, и <w>эта</w> ошибка стоила ему работы."
                            }
                        }
                    ],
                    "antonyms": [
                        "that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения человека, предмета или идеи, которые только что упоминались или уже известны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "э́тот",
                                "target_lang_sense_clarification": "упомянутый ранее"
                            },
                            {
                                "target_lang_word": "да́нный",
                                "target_lang_sense_clarification": "формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51b387e0-c903-320d-a44a-dfc8a28c9c27",
                    "sense_definition": "Used to introduce a new person or thing in a story or conversation, implying they are specific.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "introductory_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I met <w>this</w> woman the other day who is allergic to wheat.",
                            "target_lang_translations": {
                                "ru": "На днях я встретил <w>одну</w> женщину, у которой аллергия на пшеницу."
                            }
                        },
                        {
                            "text": "There is <w>this</w> new restaurant downtown that we should try.",
                            "target_lang_translations": {
                                "ru": "В центре есть <w>один</w> новый ресторан, который нам стоит попробовать."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken storytelling"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для представления нового человека или предмета в рассказе, подразумевая их конкретность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оди́н",
                                "target_lang_sense_clarification": "некий, какой-то"
                            },
                            {
                                "target_lang_word": "э́тот",
                                "target_lang_sense_clarification": "разговорное усиление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "412993cd-050b-3f9e-8b2c-25693a6565e9",
                    "sense_definition": "Used to introduce a specific person, thing, or fact that is about to be stated.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "discourse_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When asked what he wanted, he gave <w>this</w> reply: nothing.",
                            "target_lang_translations": {
                                "ru": "Когда его спросили, чего он хочет, он дал <w>такой</w> ответ: ничего."
                            }
                        },
                        {
                            "text": "Please remember <w>this</w> rule: always be polite to others.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, запомните <w>это</w> правило: всегда будьте вежливы с другими."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для представления конкретного человека, предмета или факта, о котором будет сказано далее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тако́й",
                                "target_lang_sense_clarification": "подобный"
                            },
                            {
                                "target_lang_word": "сле́дующий",
                                "target_lang_sense_clarification": "который будет приведен далее"
                            },
                            {
                                "target_lang_word": "э́тот",
                                "target_lang_sense_clarification": "указание на последующее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "769bd021-5314-3f14-952f-4f82105ba9bc",
                    "sense_definition": "Used to refer to oneself, often for emphasis or humor.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "self_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You know who likes brownies? <w>This</w> guy!",
                            "target_lang_translations": {
                                "ru": "Знаешь, кто любит брауни? <w>Этот</w> парень!"
                            }
                        },
                        {
                            "text": "I am tired of waiting, and <w>this</w> girl is going home.",
                            "target_lang_translations": {
                                "ru": "Я устала ждать, и <w>эта</w> девочка идет домой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения самого себя, часто для усиления или с юмором."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "э́тот",
                                "target_lang_sense_clarification": "в сочетании с существительным (этот парень)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "ce625d21-8422-39f2-bdab-1b76976d2d44",
                    "sense_definition": "The person, thing, or idea that is present, nearby, or has just been mentioned.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_deixis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>This</w> is not the item that I ordered.",
                            "target_lang_translations": {
                                "ru": "<w>Это</w> не тот товар, который я заказывал."
                            }
                        },
                        {
                            "text": "<w>This</w> is my friend from school.",
                            "target_lang_translations": {
                                "ru": "<w>Это</w> мой школьный друг."
                            }
                        }
                    ],
                    "antonyms": [
                        "that"
                    ],
                    "common_phrases": [
                        "like this",
                        "this is it",
                        "this is"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, предмет или идея, которые присутствуют, находятся рядом или были только что упомянуты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "э́то",
                                "target_lang_sense_clarification": "указательное местоимение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "5323c7df-6d03-378f-aa3f-e4e3d0a71dbf",
                    "sense_definition": "To the degree or extent indicated; used to emphasize the size, amount, or quality of something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have never been <w>this</w> tired in my life.",
                            "target_lang_translations": {
                                "ru": "Я никогда в жизни не был <w>так</w> утомлен."
                            }
                        },
                        {
                            "text": "We have already come <w>this</w> far, so we cannot turn back now.",
                            "target_lang_translations": {
                                "ru": "Мы зашли уже <w>так</w> далеко, поэтому не можем повернуть назад."
                            }
                        }
                    ],
                    "synonyms": [
                        "+1"
                    ],
                    "antonyms": [
                        "that"
                    ],
                    "common_phrases": [
                        "this much",
                        "this far",
                        "this big"
                    ],
                    "target_lang_definitions": {
                        "ru": "В указанной степени; используется для подчеркивания размера, количества или качества чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "в такой степени"
                            },
                            {
                                "target_lang_word": "насто́лько",
                                "target_lang_sense_clarification": "до такой степени"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "1793434a-ed54-396b-a65d-a57eb7e882cd",
                    "sense_definition": "Used to express strong agreement with a previous statement.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "User A: We need to ban trolls immediately. User B: <w>This</w>!",
                            "target_lang_translations": {
                                "ru": "Пользователь А: Нам нужно немедленно забанить троллей. Пользователь Б: <w>Вот именно</w>!"
                            }
                        },
                        {
                            "text": "When someone says exactly what you were thinking, you just reply: <w>This</w>.",
                            "target_lang_translations": {
                                "ru": "Когда кто-то говорит именно то, о чем вы думали, вы просто отвечаете: <w>В точку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "+1"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения решительного согласия с предыдущим утверждением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вот и́менно",
                                "target_lang_sense_clarification": "выражение полного согласия"
                            },
                            {
                                "target_lang_word": "в то́чку",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e12eca1-ccb2-3902-b48d-42cb76395bb9",
                    "sense_definition": "A specific thing or entity that is being indicated.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "philosophical_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Terms like 'house' do not refer to other <w>thises</w> distinct from these ones here.",
                            "target_lang_translations": {
                                "ru": "Термины вроде «дом» не относятся к другим <w>«это»</w>, отличным от тех, что здесь."
                            }
                        },
                        {
                            "text": "The philosopher argued about the nature of specific <w>thises</w>.",
                            "target_lang_translations": {
                                "ru": "Философ спорил о природе конкретных <w>единичных вещей</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical or philosophical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная вещь или сущность, на которую указывают."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "э́то",
                                "target_lang_sense_clarification": "философское понятие"
                            },
                            {
                                "target_lang_word": "су́щность",
                                "target_lang_sense_clarification": "объект указания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
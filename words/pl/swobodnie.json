            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "76f07c42-b7ee-3846-83a3-0201ec30d923",
                    "sense_definition": "W sposób nieograniczony, nieskrępowany, bez przeszkód.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta koszula jest trochę za ciasna, nie mogę <w>swobodnie</w> ruszać rękami.",
                            "target_lang_translations": {
                                "ru": "Эта рубашка немного тесновата, я не могу <w>свободно</w> двигать руками."
                            }
                        },
                        {
                            "text": "W naszym domu każdy może <w>swobodnie</w> wyrażać swoje opinie i uczucia.",
                            "target_lang_translations": {
                                "ru": "В нашем доме каждый может <w>свободно</w> выражать свои мнения и чувства."
                            }
                        }
                    ],
                    "synonyms": [
                        "wolno",
                        "nieskrępowanie",
                        "luźno"
                    ],
                    "antonyms": [
                        "sztywno",
                        "nieswobodnie"
                    ],
                    "common_phrases": [
                        "czuć się swobodnie",
                        "poruszać się swobodnie",
                        "oddychać swobodnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неограниченно, непринуждённо, без стеснения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свободно",
                                "target_lang_sense_clarification": "без ограничений"
                            },
                            {
                                "target_lang_word": "вольно",
                                "target_lang_sense_clarification": "раскованно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "870c4ec6-bf22-392c-8dba-81041cbd5794",
                    "sense_definition": "Z łatwością, bez wysiłku, płynnie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po kilku latach pobytu za granicą potrafi <w>swobodnie</w> rozmawiać w obcym języku.",
                            "target_lang_translations": {
                                "ru": "После нескольких лет пребывания за границей он может <w>свободно</w> разговаривать на иностранном языке."
                            }
                        },
                        {
                            "text": "Doświadczony pianista <w>swobodnie</w> grał najtrudniejsze fragmenty koncertu.",
                            "target_lang_translations": {
                                "ru": "Опытный пианист <w>свободно</w> играл самые сложные фрагменты концерта."
                            }
                        }
                    ],
                    "synonyms": [
                        "łatwo",
                        "płynnie",
                        "bezproblemowo"
                    ],
                    "antonyms": [
                        "z trudem",
                        "ciężko"
                    ],
                    "common_phrases": [
                        "mówić swobodnie",
                        "rozmawiać swobodnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "С лёгкостью, без усилий, бегло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свободно",
                                "target_lang_sense_clarification": "бегло, без затруднений"
                            },
                            {
                                "target_lang_word": "легко",
                                "target_lang_sense_clarification": "без усилий"
                            }
                        ]
                    }
                }
            ]
        }

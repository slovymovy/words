                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "name_concept",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My friend <w>Raven</w> is coming to the party later tonight.",
                            "target_lang_translations": {
                                "ru": "Моя подруга <w>Рэйвен</w> придет на вечеринку позже сегодня вечером."
                            }

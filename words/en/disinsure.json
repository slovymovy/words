{
    "word_family": [
        "insure",
        "insurance",
        "insurer",
        "uninsure",
        "reinsure",
        "reinsurance"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "df889bfe-d033-3a53-a970-e4d1188bcb3e",
                    "sense_definition": "To take away or cancel the insurance coverage of someone or something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company threatened to <w>disinsure</w> the property if the safety standards were not met.",
                            "target_lang_translations": {
                                "ru": "Компания пригрозила <w>лишить страховки</w> имущество, если нормы безопасности не будут соблюдены."
                            }
                        },
                        {
                            "text": "They decided to <w>disinsure</w> the old vehicle because the premiums were too high.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>прекратить страхование</w> старого автомобиля, так как взносы были слишком высоки."
                            }
                        }
                    ],
                    "synonyms": [
                        "uninsure"
                    ],
                    "antonyms": [
                        "insure"
                    ],
                    "common_phrases": [
                        "disinsure a vehicle",
                        "disinsure property"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишить страхового покрытия или аннулировать страховку кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лишить страховки",
                                "target_lang_sense_clarification": "отменить страховое покрытие"
                            },
                            {
                                "target_lang_word": "прекратить страхование",
                                "target_lang_sense_clarification": "остановить действие страховки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28ef1df2-03c9-316d-af92-3922e600d1d7",
                    "sense_definition": "To make something insecure or put it in danger.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His reckless actions served only to <w>disinsure</w> the stability of the entire project.",
                            "target_lang_translations": {
                                "ru": "Его безрассудные действия лишь <w>поставили под угрозу</w> стабильность всего проекта."
                            }
                        },
                        {
                            "text": "The sudden storm began to <w>disinsure</w> the safety of the climbers on the mountain.",
                            "target_lang_translations": {
                                "ru": "Внезапный шторм начал <w>угрожать</w> безопасности альпинистов на горе."
                            }
                        }
                    ],
                    "synonyms": [
                        "endanger",
                        "imperil"
                    ],
                    "antonyms": [
                        "secure",
                        "protect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать что-либо небезопасным или подвергнуть опасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвергнуть опасности",
                                "target_lang_sense_clarification": "поставить под угрозу"
                            },
                            {
                                "target_lang_word": "сделать ненадежным",
                                "target_lang_sense_clarification": "лишить безопасности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
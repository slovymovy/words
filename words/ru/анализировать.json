    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5692e0db-30a0-3a4d-9d9a-0eff23d04f5b",
                    "sense_definition": "Исследовать и разбирать что-либо на составные части или элементы для детального изучения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы должны тщательно <w>анализировать</w> все полученные результаты перед принятием решения.",
                            "target_lang_translations": {
                                "en": "We must carefully <w>analyze</w> all the results obtained before making a decision."
                            }
                        },
                        {
                            "text": "Автор книги пытается <w>анализировать</w> причины этого сложного исторического события.",
                            "target_lang_translations": {
                                "en": "The book's author attempts to <w>analyze</w> the causes of this complex historical event."
                            }
                        }
                    ],
                    "synonyms": [
                        "изучать",
                        "исследовать",
                        "разбирать",
                        "рассматривать"
                    ],
                    "antonyms": [
                        "синтезировать"
                    ],
                    "common_phrases": [
                        "анализировать ситуацию",
                        "анализировать данные",
                        "анализировать ошибки",
                        "анализировать информацию"
                    ],
                    "target_lang_definitions": {
                        "en": "To investigate and break something down into constituent parts or elements for detailed study."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "analyze",
                                "target_lang_sense_clarification": "to study or examine in detail"
                            },
                            {
                                "target_lang_word": "examine",
                                "target_lang_sense_clarification": "to inspect closely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acf10935-8a5e-3294-b64a-87e5ac26375a",
                    "sense_definition": "Проводить лабораторное исследование для определения состава вещества.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лаборант начал <w>анализировать</w> химический состав воды из местной реки.",
                            "target_lang_translations": {
                                "en": "The lab technician began to <w>analyze</w> the chemical composition of the water from the local river."
                            }
                        },
                        {
                            "text": "Врачи будут <w>анализировать</w> образцы крови пациента для точной постановки диагноза.",
                            "target_lang_translations": {
                                "en": "Doctors will <w>analyze</w> the patient's blood samples to make an accurate diagnosis."
                            }
                        }
                    ],
                    "synonyms": [
                        "исследовать"
                    ],
                    "common_phrases": [
                        "анализировать состав",
                        "анализировать пробу",
                        "анализировать кровь"
                    ],
                    "target_lang_definitions": {
                        "en": "To conduct a laboratory examination to determine the composition of a substance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "analyze",
                                "target_lang_sense_clarification": "scientific or chemical analysis"
                            },
                            {
                                "target_lang_word": "test",
                                "target_lang_sense_clarification": "to check composition or properties"
                            }
                        ]
                    }
                }
            ]

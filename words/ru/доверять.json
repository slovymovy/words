    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0af91ff7-dde3-3855-946e-d146d56d324e",
                    "sense_definition": "Испытывать уверенность в чьей-либо честности, искренности и порядочности; полагаться на кого-либо или что-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В отношениях очень важно уметь <доверять> партнёру.",
                            "target_lang_translations": {
                                "en": "In relationships, it is very important to be able to <w>trust</w> a partner."
                            }
                        },
                        {
                            "text": "Я привык <доверять> своей интуиции в сложных ситуациях.",
                            "target_lang_translations": {
                                "en": "I am used to <w>trusting</w> my intuition in difficult situations."
                            }
                        }
                    ],
                    "synonyms": [
                        "верить",
                        "полагаться"
                    ],
                    "antonyms": [
                        "подозревать",
                        "сомневаться",
                        "опасаться"
                    ],
                    "common_phrases": [
                        "доверять людям",
                        "слепо доверять",
                        "доверять себе",
                        "всецело доверять"
                    ],
                    "target_lang_definitions": {
                        "en": "To feel confidence in someone's honesty, sincerity, and decency; to rely on someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trust",
                                "target_lang_sense_clarification": "to have confidence in someone or something"
                            },
                            {
                                "target_lang_word": "rely on",
                                "target_lang_sense_clarification": "to depend on with confidence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ba68e26-754e-3805-a3ab-7eccbc6baa25",
                    "sense_definition": "Передавать что-либо в чьё-либо распоряжение, поручать кому-либо выполнение чего-либо или сообщать что-то секретное.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <доверять> воспитание детей только опытным педагогам.",
                            "target_lang_translations": {
                                "en": "We decided to <w>entrust</w> the upbringing of the children only to experienced educators."
                            }
                        },
                        {
                            "text": "Не стоит <доверять> свои секреты малознакомым людям.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>trust</w> your secrets to people you hardly know."
                            }
                        }
                    ],
                    "synonyms": [
                        "поручать",
                        "вверять"
                    ],
                    "common_phrases": [
                        "доверять тайну",
                        "доверять ключи",
                        "доверять работу",
                        "доверять жизнь"
                    ],
                    "target_lang_definitions": {
                        "en": "To place something at someone's disposal, to assign a task to someone, or to communicate a secret."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "entrust",
                                "target_lang_sense_clarification": "to assign the care of something to someone"
                            },
                            {
                                "target_lang_word": "confide",
                                "target_lang_sense_clarification": "to share secrets or private information"
                            },
                            {
                                "target_lang_word": "trust",
                                "target_lang_sense_clarification": "to give something to someone for safekeeping"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
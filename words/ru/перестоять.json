    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7c1a6262-1d12-3a40-910c-4fa44371fbe3",
                    "sense_definition": "Испортиться или потерять надлежащее качество из-за слишком долгого хранения, приготовления или нахождения в каком-либо состоянии.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот чай слишком горький, потому что успел <w>перестоять</w> в заварочном чайнике.",
                            "target_lang_translations": {
                                "en": "This tea is too bitter because it managed to <w>steep too long</w> in the teapot."
                            }
                        },
                        {
                            "text": "Если пшеница <w>перестоит</w> на полях, зерно начнёт осыпаться на землю.",
                            "target_lang_translations": {
                                "en": "If the wheat <w>stands too long</w> in the fields, the grain will start falling to the ground."
                            }
                        }
                    ],
                    "synonyms": [
                        "испортиться",
                        "перезреть"
                    ],
                    "common_phrases": [
                        "чай перестоял",
                        "тесто перестояло",
                        "перестоять на корню"
                    ],
                    "target_lang_definitions": {
                        "en": "To spoil or lose quality due to being kept, cooked, or left standing for too long."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand too long",
                                "target_lang_sense_clarification": "general context"
                            },
                            {
                                "target_lang_word": "steep too long",
                                "target_lang_sense_clarification": "tea or infusions"
                            },
                            {
                                "target_lang_word": "overripen",
                                "target_lang_sense_clarification": "crops or harvest"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39267a05-ea86-346a-9e4f-36b287349508",
                    "sense_definition": "Простоять (находиться в вертикальном положении или бездействии) дольше, чем нужно или чем следует.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лошадь может захромать, если <w>перестоит</w> в конюшне без регулярных прогулок.",
                            "target_lang_translations": {
                                "en": "A horse can go lame if it <w>stands too long</w> in the stable without regular walks."
                            }
                        },
                        {
                            "text": "Раствор не должен <w>перестоять</w>, иначе он затвердеет и станет непригодным для работы.",
                            "target_lang_translations": {
                                "en": "The mortar must not <w>sit too long</w>, otherwise it will harden and become unfit for work."
                            }
                        }
                    ],
                    "antonyms": [
                        "недостоять"
                    ],
                    "common_phrases": [
                        "перестоять в очереди"
                    ],
                    "target_lang_definitions": {
                        "en": "To stand or remain inactive longer than necessary or appropriate."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand too long",
                                "target_lang_sense_clarification": "general or animals"
                            },
                            {
                                "target_lang_word": "sit too long",
                                "target_lang_sense_clarification": "mixtures or materials"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34ed1299-74da-32c3-9bf1-e373c7649dde",
                    "sense_definition": "Провести время стоя в каком-либо месте, ожидая окончания чего-либо (обычно непогоды).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили зайти под навес и <w>перестоять</w> внезапный ливень.",
                            "target_lang_translations": {
                                "en": "We decided to go under the shelter and <w>wait out</w> the sudden downpour."
                            }
                        },
                        {
                            "text": "Путники надеялись <w>перестоять</w> бурю в заброшенной хижине лесника.",
                            "target_lang_translations": {
                                "en": "The travelers hoped to <w>wait out</w> the storm in the forester's abandoned hut."
                            }
                        }
                    ],
                    "synonyms": [
                        "переждать"
                    ],
                    "common_phrases": [
                        "перестоять дождь",
                        "перестоять грозу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To spend time standing somewhere waiting for something (usually bad weather) to end."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wait out",
                                "target_lang_sense_clarification": "weather or events"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "японский",
                                "target_lang_sense_clarification": "о языке, письменности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

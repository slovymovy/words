                    "frequency": "High",
                    "semantic_group_id": "medical_order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor wrote a <w>prescription</w> for antibiotics to treat the infection."
                        },
                        {
                            "text": "You cannot buy this strong medication without a valid <w>prescription</w>."
                        }
                    ],
                    "synonyms": [
                        "Rx"
                    ],
                    "common_phrases": [
                        "write a prescription",
                        "fill a prescription",
                        "prescription drugs"
                    ]
                },
                {

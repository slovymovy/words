                            {
                                "target_lang_word": "направлять",
                                "target_lang_sense_clarification": "руководить процессом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fae865d7-b37b-3e52-96e0-703afb3b1c77",

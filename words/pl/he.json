    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "4378fc3c-37fa-38c7-910d-14a4d3094db8",
                    "sense_definition": "Słowo używane w mowie potocznej na końcu pytania lub zdania, służące do upewnienia się, czy rozmówca słucha, rozumie lub zgadza się z wypowiedzią.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To był naprawdę świetny film, <w>he</w>?",
                            "target_lang_translations": {
                                "ru": "Это был действительно отличный фильм, <w>а</w>?"
                            }
                        },
                        {
                            "text": "Mówiłem ci, że to się tak skończy, <w>he</w>?",
                            "target_lang_translations": {
                                "ru": "Я же говорил тебе, что это так кончится, <w>да</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "hę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в разговорной речи в конце вопроса или предложения, чтобы убедиться, что собеседник слушает, понимает или согласен со сказанным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "вопросительная частица"
                            },
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "запрос подтверждения"
                            },
                            {
                                "target_lang_word": "э",
                                "target_lang_sense_clarification": "междометие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af21a993-0acb-317a-948f-659d76e2c63c",
                    "sense_definition": "Okrzyk wydawany przez powożącego w celu poganiania zwierząt pociągowych, takich jak konie lub woły, aby ruszyły z miejsca.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woźnica krzyknął głośno <w>he</w>, a konie natychmiast ruszyły z kopyta.",
                            "target_lang_translations": {
                                "ru": "Кучер громко крикнул <w>но</w>, и лошади тут же рванули с места."
                            }
                        },
                        {
                            "text": "Gospodarz zawołał <w>he</w> na woły, by ciągnęły wóz pod górę.",
                            "target_lang_translations": {
                                "ru": "Хозяин крикнул волам <w>но</w>, чтобы они тащили воз в гору."
                            }
                        }
                    ],
                    "common_phrases": [
                        "he wio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, издаваемый возницей для понукания тягловых животных (лошадей или волов), чтобы они тронулись с места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "но",
                                "target_lang_sense_clarification": "команда животным"
                            },
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "понукание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
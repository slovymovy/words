                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "crisis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krajowa gospodarka znalazła się w stanie głębokiej <w>zapaści</w>.",
                            "target_lang_translations": {

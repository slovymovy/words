                },
                {
                    "sense_id": "236fc5fe-5dcd-32db-a947-910dd9379d66",
                    "sense_definition": "Een van de min of meer gelijke onderdelen waaruit het lichaam van bepaalde dieren of planten is opgebouwd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het lichaam van de regenworm bestaat uit vele kleine <w>segmenten</w>.",
                            "target_lang_translations": {
                                "en": "The body of the earthworm consists of many small <w>segments</w>.",
                                "ru": "Тело дождевого червя состоит из множества маленьких <w>сегментов</w>."
                            }
                        },
                        {
                            "text": "Elk <w>segment</w> van het insect bevat specifieke organen.",
                            "target_lang_translations": {
                                "en": "Each <w>segment</w> of the insect contains specific organs.",
                                "ru": "Каждый <w>сегмент</w> насекомого содержит специфические органы."
                            }
                        }
                    ],
                    "synonyms": [
                        "lid",
                        "ring"
                    ],
                    "common_phrases": [
                        "segmenten van het lichaam",
                        "afzonderlijk segment"
                    ],
                    "target_lang_definitions": {
                        "en": "One of the similar parts that make up the body of certain animals or plants.",
                        "ru": "Одна из повторяющихся частей, из которых состоит тело некоторых животных или растений."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "segment",
                                "target_lang_sense_clarification": "biological body part"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сегмент",
                                "target_lang_sense_clarification": "биология"
                            },
                            {
                                "target_lang_word": "членик",
                                "target_lang_sense_clarification": "часть тела членистоногих/червей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af8b7c85-70b8-317f-b05a-aa124b14b9b2",
                    "sense_definition": "Een deel van een cirkel dat wordt ingesloten door een boog en een koorde, of een deel van een bol.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bereken de oppervlakte van het gekleurde <w>segment</w> in de cirkel.",
                            "target_lang_translations": {
                                "en": "Calculate the area of the colored <w>segment</w> in the circle.",
                                "ru": "Вычислите площадь закрашенного <w>сегмента</w> в круге."
                            }
                        },
                        {
                            "text": "De lijn verdeelt de cirkel in een groot en een klein <w>segment</w>.",
                            "target_lang_translations": {
                                "en": "The line divides the circle into a large and a small <w>segment</w>.",
                                "ru": "Линия делит круг на большой и маленький <w>сегмент</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cirkelsegment"
                    ],
                    "common_phrases": [
                        "segment van een cirkel",
                        "bolvormig segment"
                    ],
                    "target_lang_definitions": {
                        "en": "A part of a circle enclosed by an arc and a chord, or a part of a sphere.",
                        "ru": "Часть круга, ограниченная дугой и хордой, или часть шара."
                    },
                    "translations": {

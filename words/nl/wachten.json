                        },
                        {
                            "trait_type": "dated",
                            "comment": "often used in formal or archaic contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Остерегаться кого-либо или чего-либо; проявлять осторожность, чтобы избежать неприятностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остерегаться",
                                "target_lang_sense_clarification": "быть осторожным по отношению к кому-то/чему-то"
                            },
                            {
                                "target_lang_word": "беречься",
                                "target_lang_sense_clarification": "защищать себя от опасности"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8fb141ab-14e0-34e6-8901-be99c3725f5a",
                    "sense_definition": "Płacić pieniędzmi za towary lub usługi; przeznaczać środki finansowe na jakiś cel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "money_spending",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie lubię <wydawać> pieniędzy na niepotrzebne rzeczy.",
                            "target_lang_translations": {
                                "ru": "Я не люблю <тратить> деньги на ненужные вещи."
                            }
                        },
                        {
                            "text": "W zeszłym miesiącu <wydaliśmy> bardzo dużo na jedzenie.",
                            "target_lang_translations": {
                                "ru": "В прошлом месяце мы <потратили> очень много на еду."
                            }
                        }
                    ],
                    "synonyms": [
                        "płacić",
                        "wydatkować",
                        "trwonić"
                    ],
                    "antonyms": [
                        "oszczędzać",
                        "gromadzić"
                    ],
                    "common_phrases": [
                        "wydawać pieniądze",
                        "wydawać majątek",
                        "wydawać na zakupy",
                        "lekką ręką wydawać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Платить деньгами за товары или услуги; расходовать финансовые средства на какую-либо цель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тратить",
                                "target_lang_sense_clarification": "деньги"
                            },
                            {
                                "target_lang_word": "расходовать",
                                "target_lang_sense_clarification": "средства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38b8fbd4-fd73-3e90-937b-a19a1d56dcdf",
                    "sense_definition": "Sprawiać wrażenie, że coś jest jakieś lub że sytuacja wygląda w określony sposób (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To zadanie <wydaje> się trudniejsze, niż myślałem.",
                            "target_lang_translations": {
                                "ru": "Это задание <кажется> более трудным, чем я думал."
                            }
                        },
                        {
                            "text": "Ona <wydaje> się być bardzo miłą osobą.",
                            "target_lang_translations": {
                                "ru": "Она <кажется> очень милым человеком."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdawać się",
                        "wyglądać"
                    ],
                    "common_phrases": [
                        "wydawać się dziwnym",
                        "wydawać się prawdą",
                        "wydawać się znajomym"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "requires 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить впечатление, выглядеть определенным образом (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "казаться",
                                "target_lang_sense_clarification": "производить впечатление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "566764f5-1843-340a-ba9a-501dbcb6f8d0",
                    "sense_definition": "Zwracać klientowi różnicę pieniędzy, gdy zapłacił kwotę wyższą niż cena towaru.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "money_transaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ekspedientka <wydała> mi resztę co do grosza.",
                            "target_lang_translations": {
                                "ru": "Продавщица <выдала> мне сдачу до копейки."
                            }
                        },
                        {
                            "text": "Czy może mi pan <wydać> z dwustu złotych?",
                            "target_lang_translations": {
                                "ru": "Можете ли вы <дать сдачи> с двухсот злотых?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "wydawać resztę",
                        "nie mieć jak wydać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвращать клиенту разницу в деньгах, если он заплатил сумму больше стоимости товара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давать сдачу",
                                "target_lang_sense_clarification": "возвращать разницу"
                            },
                            {
                                "target_lang_word": "выдавать",
                                "target_lang_sense_clarification": "сдачу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e91f7efe-d417-3b59-94ff-f2fb4bcdd121",
                    "sense_definition": "Opublikować i udostępnić do sprzedaży dzieło, takie jak książka, czasopismo, płyta lub gra.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "publishing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To wydawnictwo <wydaje> głównie literaturę piękną.",
                            "target_lang_translations": {
                                "ru": "Это издательство <издает> в основном художественную литературу."
                            }
                        },
                        {
                            "text": "Zespół planuje <wydać> nową płytę w przyszłym roku.",
                            "target_lang_translations": {
                                "ru": "Группа планирует <выпустить> новый альбом в следующем году."
                            }
                        }
                    ],
                    "synonyms": [
                        "publikować",
                        "drukować",
                        "ogłaszać"
                    ],
                    "common_phrases": [
                        "wydawać książkę",
                        "wydawać album",
                        "wydawać gazetę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публиковать и выпускать в продажу произведение, например книгу, журнал, музыкальный альбом или игру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "издавать",
                                "target_lang_sense_clarification": "книги, журналы"
                            },
                            {
                                "target_lang_word": "выпускать",
                                "target_lang_sense_clarification": "альбомы, игры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "771594a4-83b2-321f-b715-6c6e8dcaa0c5",
                    "sense_definition": "Przekazywać coś komuś oficjalnie, dostarczać dokumenty lub towary osobom uprawnionym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "distribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Urząd <wydaje> paszporty w ciągu trzydziestu dni.",
                            "target_lang_translations": {
                                "ru": "Учреждение <выдает> паспорта в течение тридцати дней."
                            }
                        },
                        {
                            "text": "Magazynier <wydaje> towar tylko na podstawie zamówienia.",
                            "target_lang_translations": {
                                "ru": "Кладовщик <выдает> товар только на основании заказа."
                            }
                        }
                    ],
                    "synonyms": [
                        "wręczać",
                        "przekazywać",
                        "rozdawać"
                    ],
                    "antonyms": [
                        "przyjmować",
                        "odbierać"
                    ],
                    "common_phrases": [
                        "wydawać dokumenty",
                        "wydawać posiłki",
                        "wydawać przepustki",
                        "wydawać towar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально передавать что-либо кому-либо, предоставлять документы или товары уполномоченным лицам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдавать",
                                "target_lang_sense_clarification": "документы, товары"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3590c1a1-b88f-3851-8b99-b63aab0abb38",
                    "sense_definition": "Ogłaszać oficjalną decyzję, wyrok sądowy, opinię lub rozkaz.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd wkrótce <wyda> wyrok w tej głośnej sprawie.",
                            "target_lang_translations": {
                                "ru": "Суд вскоре <вынесет> приговор по этому громкому делу."
                            }
                        },
                        {
                            "text": "Generał <wydał> rozkaz do natychmiastowego ataku.",
                            "target_lang_translations": {
                                "ru": "Генерал <отдал> приказ к немедленной атаке."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogłaszać",
                        "ustanawiać",
                        "orzekać"
                    ],
                    "common_phrases": [
                        "wydawać wyrok",
                        "wydawać rozkaz",
                        "wydawać decyzję",
                        "wydawać opinię"

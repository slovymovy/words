{
    "word_family": [
        "оказать",
        "оказываться",
        "оказание"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "737e98af-4b50-341e-8214-15b93cdaaa45",
                    "sense_definition": "Выясниться, стать известным как обладающий определёнными свойствами, качествами или статусом.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "turn_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот новый фильм <оказался> очень интересным и смешным.",
                            "target_lang_translations": {
                                "en": "This new movie <turned out> to be very interesting and funny."
                            }
                        },
                        {
                            "text": "В итоге мой друг <оказался> прав в этом споре.",
                            "target_lang_translations": {
                                "en": "In the end, my friend <turned out> to be right in this argument."
                            }
                        }
                    ],
                    "synonyms": [
                        "обнаружиться"
                    ],
                    "common_phrases": [
                        "оказаться правым",
                        "оказаться полезным",
                        "оказаться сложнее",
                        "оказаться правдой"
                    ],
                    "target_lang_definitions": {
                        "en": "To become known or revealed as having certain properties, qualities, or status; to result."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn out",
                                "target_lang_sense_clarification": "to become apparent or known"
                            },
                            {
                                "target_lang_word": "prove",
                                "target_lang_sense_clarification": "to turn out to be (often used with adjectives)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a04c18bf-ff2f-33b4-9576-2869a43eed4f",
                    "sense_definition": "Неожиданно попасть в какое-либо место или ситуацию; очутиться.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "end_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы случайно <оказались> в центре незнакомого города.",
                            "target_lang_translations": {
                                "en": "We accidentally <found ourselves> in the center of an unfamiliar city."
                            }
                        },
                        {
                            "text": "Я не хочу снова <оказаться> в такой сложной ситуации.",
                            "target_lang_translations": {
                                "en": "I don't want to <end up> in such a difficult situation again."
                            }
                        }
                    ],
                    "synonyms": [
                        "очутиться"
                    ],
                    "common_phrases": [
                        "оказаться дома",
                        "оказаться в ловушке",
                        "оказаться на улице",
                        "оказаться рядом"
                    ],
                    "target_lang_definitions": {
                        "en": "To unexpectedly find oneself in a specific place or situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find oneself",
                                "target_lang_sense_clarification": "to discover one is in a particular place or situation"
                            },
                            {
                                "target_lang_word": "end up",
                                "target_lang_sense_clarification": "to arrive in a place or situation eventually or unexpectedly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ac78120-4dee-3dd1-aa75-f1aa504399ea",
                    "sense_definition": "Быть в наличии, найтись где-либо (часто при поиске или необходимости).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "be_found",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В кармане куртки не <оказалось> ключей от квартиры.",
                            "target_lang_translations": {
                                "en": "There <turned out> to be no apartment keys in the jacket pocket."
                            }
                        },
                        {
                            "text": "У меня с собой случайно <оказалась> интересная книга.",
                            "target_lang_translations": {
                                "en": "I accidentally <happened to have> an interesting book with me."
                            }
                        }
                    ],
                    "synonyms": [
                        "найтись"
                    ],
                    "common_phrases": [
                        "оказаться под рукой",
                        "не оказалось денег",
                        "оказаться на месте"
                    ],
                    "target_lang_definitions": {
                        "en": "To be present, available, or found (often when searching or needing something)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn out to be",
                                "target_lang_sense_clarification": "indicating presence or existence"
                            },
                            {
                                "target_lang_word": "be found",
                                "target_lang_sense_clarification": "to be discovered"
                            },
                            {
                                "target_lang_word": "happen to have",
                                "target_lang_sense_clarification": "used when the subject possesses something unexpectedly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49163c48-034a-35d6-b915-2a4e3f0259c6",
                    "sense_definition": "Стать заметным, проявиться (обычно о способностях или талантах).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "manifest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его музыкальный талант <оказался> ещё в раннем детстве.",
                            "target_lang_translations": {
                                "en": "His musical talent <manifested> itself in early childhood."
                            }
                        },
                        {
                            "text": "В её характере <оказалась> неожиданная для всех твёрдость.",
                            "target_lang_translations": {
                                "en": "An unexpected firmness <showed itself> in her character."
                            }
                        }
                    ],
                    "synonyms": [
                        "проявиться"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become noticeable or evident (usually regarding abilities or talents)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "manifest",
                                "target_lang_sense_clarification": "to show itself clearly"
                            },
                            {
                                "target_lang_word": "show itself",
                                "target_lang_sense_clarification": "to become visible or apparent"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
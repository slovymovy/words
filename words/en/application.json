    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e117d8f-6e91-3f50-8b2e-50575d9098a5",
                    "sense_definition": "A computer program or piece of software designed to perform a specific task for the user.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I downloaded a new <w>application</w> to help me organize my schedule.",
                            "target_lang_translations": {
                                "ru": "Я скачал новое <w>приложение</w>, чтобы помочь себе организовать расписание."
                            }
                        },
                        {
                            "text": "This <w>application</w> allows you to edit photos on your phone.",
                            "target_lang_translations": {
                                "ru": "Это <w>приложение</w> позволяет редактировать фотографии на телефоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "app",
                        "program",
                        "software"
                    ],
                    "common_phrases": [
                        "mobile application",
                        "web application",
                        "download an application"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа или программное обеспечение, предназначенное для выполнения определенной задачи для пользователя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приложение",
                                "target_lang_sense_clarification": "программное обеспечение"
                            },
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "компьютерная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54ff05a5-8e05-3954-b8ea-4a32f82d652e",
                    "sense_definition": "A formal written request to be considered for a position, admission to a school, or a grant.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She filled out an <w>application</w> for a visa at the embassy.",
                            "target_lang_translations": {
                                "ru": "Она заполнила <w>заявление</w> на визу в посольстве."
                            }
                        },
                        {
                            "text": "The university rejected his <w>application</w> because it was incomplete.",
                            "target_lang_translations": {
                                "ru": "Университет отклонил его <w>заявление</w>, потому что оно было неполным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "job application",
                        "application form",
                        "submit an application"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный письменный запрос на рассмотрение кандидатуры на должность, поступление в учебное заведение или получение гранта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заявление",
                                "target_lang_sense_clarification": "официальное обращение"
                            },
                            {
                                "target_lang_word": "заявка",
                                "target_lang_sense_clarification": "запрос на участие или получение"
                            },
                            {
                                "target_lang_word": "анкета",
                                "target_lang_sense_clarification": "бланк заявления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e720a24-a917-3a87-a068-67f9d300d8de",
                    "sense_definition": "The practical use or relevance of a theory, method, or rule in a specific situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>application</w> of this scientific theory to real life is very difficult.",
                            "target_lang_translations": {
                                "ru": "<w>Применение</w> этой научной теории в реальной жизни очень затруднительно."
                            }
                        },
                        {
                            "text": "This new technology has a practical <w>application</w> in the medical field.",
                            "target_lang_translations": {
                                "ru": "Эта новая технология находит практическое <w>применение</w> в медицине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "practical application",
                        "application of the law",
                        "wide application"
                    ],
                    "target_lang_definitions": {
                        "ru": "Практическое использование или применимость теории, метода или правила в конкретной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "применение",
                                "target_lang_sense_clarification": "использование на практике"
                            },
                            {
                                "target_lang_word": "использование",
                                "target_lang_sense_clarification": "употребление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31cfa51b-1de6-3806-a442-88014cef25fa",
                    "sense_definition": "The act of putting a substance, such as paint, cream, or medicine, onto a surface or part of the body.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor recommended the daily <w>application</w> of this ointment to the affected area.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал ежедневное <w>нанесение</w> этой мази на пораженный участок."
                            }
                        },
                        {
                            "text": "Careful <w>application</w> of the varnish is necessary to protect the wood.",
                            "target_lang_translations": {
                                "ru": "Для защиты дерева необходимо аккуратное <w>нанесение</w> лака."
                            }
                        }
                    ],
                    "common_phrases": [
                        "topical application",
                        "external application",
                        "application of heat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по нанесению вещества, такого как краска, крем или лекарство, на поверхность или часть тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нанесение",
                                "target_lang_sense_clarification": "покрытие поверхности веществом"
                            },
                            {
                                "target_lang_word": "применение",
                                "target_lang_sense_clarification": "использование средства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66d2001b-e0b3-3a48-81ab-f6bccfd59e2f",
                    "sense_definition": "Sustained effort, close attention, and hard work directed towards a task.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He succeeded in his studies through sheer <w>application</w> and discipline.",
                            "target_lang_translations": {
                                "ru": "Он преуспел в учебе благодаря исключительному <w>усердию</w> и дисциплине."
                            }
                        },
                        {
                            "text": "The complex project required a lot of <w>application</w> to detail.",
                            "target_lang_translations": {
                                "ru": "Сложный проект требовал большого <w>внимания</w> к деталям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "with application"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упорный труд, пристальное внимание и усердие, направленные на выполнение задачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усердие",
                                "target_lang_sense_clarification": "старательность"
                            },
                            {
                                "target_lang_word": "прилежание",
                                "target_lang_sense_clarification": "в учебе или работе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
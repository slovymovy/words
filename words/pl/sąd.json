    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a7928128-f867-3beb-b00a-ae255a1b3fcb",
                    "sense_definition": "Instytucja państwowa zajmująca się rozstrzyganiem sporów prawnych oraz orzekaniem o winie lub niewinności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legal_institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <sąd> jest znany z wydawania surowych wyroków w sprawach karnych.",
                            "target_lang_translations": {
                                "ru": "Этот <суд> известен вынесением суровых приговоров по уголовным делам."
                            }
                        },
                        {
                            "text": "Musimy złożyć oficjalny wniosek do <sądu> rejonowego.",
                            "target_lang_translations": {
                                "ru": "Мы должны подать официальное заявление в районный <суд>."
                            }
                        }
                    ],
                    "synonyms": [
                        "trybunał"
                    ],
                    "common_phrases": [
                        "Sąd Najwyższy",
                        "podać do sądu",
                        "wyrok sądu",
                        "sąd apelacyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государственный орган, занимающийся разрешением правовых споров и вынесением решений о виновности или невиновности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суд",
                                "target_lang_sense_clarification": "государственный орган"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21668514-15cd-3de6-ae78-bc312a4c1b68",
                    "sense_definition": "Budynek lub miejsce, w którym odbywają się rozprawy sądowe i urzędują sędziowie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "legal_institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd w Grójcu mieści się przy ulicy Budowlanej.",
                            "target_lang_translations": {
                                "ru": "<Суд> в Груйце находится на улице Будовляна."
                            }
                        },
                        {
                            "text": "Przed <sądem> zebrał się tłum dziennikarzy czekających na werdykt.",
                            "target_lang_translations": {
                                "ru": "Перед <судом> собралась толпа журналистов, ожидающих вердикта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iść do sądu",
                        "gmach sądu",
                        "przed sądem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание или место, где проходят судебные заседания и работают судьи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суд",
                                "target_lang_sense_clarification": "здание"
                            },
                            {
                                "target_lang_word": "здание суда",
                                "target_lang_sense_clarification": "место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28f29775-9a93-31ea-afe7-958803289820",
                    "sense_definition": "Opinia, pogląd na jakiś temat lub ocena czyjegoś postępowania.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój <sąd> o Marku jest niesprawiedliwy i krzywdzący.",
                            "target_lang_translations": {
                                "ru": "Твоё <суждение> о Марке несправедливо и обидно."
                            }
                        },
                        {
                            "text": "Nie powinieneś wydawać ostatecznych <sądów> bez poznania wszystkich faktów.",
                            "target_lang_translations": {
                                "ru": "Ты не должен выносить окончательных <суждений> без знания всех фактов."
                            }
                        }
                    ],
                    "synonyms": [
                        "opinia",
                        "pogląd",
                        "zdanie",
                        "przekonanie"
                    ],
                    "common_phrases": [
                        "moim sądem",
                        "wydać sąd",
                        "sąd o czymś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мнение, взгляд на какую-либо тему или оценка чьих-либо действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суждение",
                                "target_lang_sense_clarification": "мнение, оценка"
                            },
                            {
                                "target_lang_word": "мнение",
                                "target_lang_sense_clarification": "взгляд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08bc0910-cf93-3f6f-9ac2-e998f925be7b",
                    "sense_definition": "Proces sądowy, rozprawa mająca na celu rozstrzygnięcie sprawy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czeka go <sąd> za wszystkie popełnione przestępstwa.",
                            "target_lang_translations": {
                                "ru": "Его ждет <суд> за все совершенные преступления."
                            }
                        },
                        {
                            "text": "W religii chrześcijańskiej wierzy się w <sąd> nad duszami po śmierci.",
                            "target_lang_translations": {
                                "ru": "В христианской религии верят в <суд> над душами после смерти."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozprawa",
                        "proces",
                        "sądzenie"
                    ],
                    "common_phrases": [
                        "Sąd Ostateczny",
                        "dzień sądu",
                        "sąd nad kimś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Судебный процесс, разбирательство, имеющее целью разрешение дела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суд",
                                "target_lang_sense_clarification": "процесс, разбирательство"
                            },
                            {
                                "target_lang_word": "судебный процесс",
                                "target_lang_sense_clarification": "процедура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "666ce3ba-5971-3450-beb7-eacd6797794f",
                    "sense_definition": "Zespół sędziów (lub jeden sędzia) orzekający w danej sprawie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę wstać, <sąd> idzie na naradę.",
                            "target_lang_translations": {
                                "ru": "Встать, <суд> идет на совещание."
                            }
                        },
                        {
                            "text": "Wysoki <sąd> z pewnością weźmie pod uwagę te okoliczności łagodzące.",
                            "target_lang_translations": {
                                "ru": "Высокий <суд>, безусловно, примет во внимание эти смягчающие обстоятельства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Wysoki Sądzie",
                        "skład sądu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состав судей (или один судья), выносящий решение по данному делу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суд",
                                "target_lang_sense_clarification": "состав судей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b041e273-74cf-318a-a324-bde491614182",
                    "sense_definition": "W logice: zdanie stwierdzające określony stan rzeczy, któremu można przypisać prawdę lub fałsz.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <sąd> logiczny jest fałszywy, ponieważ przeczy faktom.",
                            "target_lang_translations": {
                                "ru": "Это логическое <суждение> ложно, так как противоречит фактам."
                            }
                        },
                        {
                            "text": "Student musiał przeanalizować strukturę tego <sądu> na egzaminie z logiki.",
                            "target_lang_translations": {
                                "ru": "Студент должен был проанализировать структуру этого <суждения> на экзамене по логике."
                            }
                        }
                    ],
                    "synonyms": [
                        "stwierdzenie",
                        "zdanie"
                    ],
                    "common_phrases": [
                        "sąd logiczny",
                        "sąd analityczny",
                        "sąd syntetyczny"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "logika"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В логике: утверждение об определенном положении вещей, которое может быть истинным или ложным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суждение",
                                "target_lang_sense_clarification": "логическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f55d803-040b-3085-b3d9-2b038245fa39",
                    "sense_definition": "Zdolność trzeźwego, logicznego myślenia i oceniania sytuacji.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trudnej sytuacji zachował zdrowy <sąd> i nie wpadł w panikę.",
                            "target_lang_translations": {
                                "ru": "В трудной ситуации он сохранил здравый <рассудок> и не впал в панику."
                            }
                        },
                        {
                            "text": "Stracił <sąd> ze strachu i zaczął uciekać na oślep.",
                            "target_lang_translations": {
                                "ru": "Он потерял <рассудок> от страха и начал бежать куда глаза глядят."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozsądek"
                    ],
                    "common_phrases": [
                        "zdrowy sąd",
                        "stracić sąd"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie rzadziej używane w tym znaczeniu"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность трезво, логически мыслить и оценивать ситуацию."
                    },
                    "translations": {
                        "ru": [

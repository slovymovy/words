{
    "word_family": [
        "dalen",
        "dal"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5bf6ecdc-e3ff-32a8-a52e-039bc5868f9a",
                    "sense_definition": "Het proces van minder of lager worden in aantal, waarde of niveau.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "decrease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>daling</w> van de werkloosheid is goed nieuws voor de economie.",
                            "target_lang_translations": {
                                "en": "The <w>drop</w> in unemployment is good news for the economy.",
                                "ru": "<w>Снижение</w> уровня безработицы — хорошая новость для экономики."
                            }
                        },
                        {
                            "text": "We verwachten volgende week een lichte <w>daling</w> van de temperatuur.",
                            "target_lang_translations": {
                                "en": "We expect a slight <w>drop</w> in temperature next week.",
                                "ru": "На следующей неделе мы ожидаем небольшое <w>понижение</w> температуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "afname",
                        "vermindering",
                        "terugloop"
                    ],
                    "antonyms": [
                        "stijging"
                    ],
                    "common_phrases": [
                        "scherpe daling",
                        "lichte daling",
                        "daling van de prijzen"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of becoming less or lower in number, value, or level.",
                        "ru": "Процесс уменьшения количества, стоимости или уровня."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "reduction in quantity or level"
                            },
                            {
                                "target_lang_word": "decrease",
                                "target_lang_sense_clarification": "process of becoming smaller or less"
                            },
                            {
                                "target_lang_word": "decline",
                                "target_lang_sense_clarification": "gradual reduction in quality or quantity"
                            },
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "reduction in value or amount"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "снижение",
                                "target_lang_sense_clarification": "процесс уменьшения"
                            },
                            {
                                "target_lang_word": "падение",
                                "target_lang_sense_clarification": "резкое уменьшение"
                            },
                            {
                                "target_lang_word": "спад",
                                "target_lang_sense_clarification": "постепенное ухудшение или уменьшение активности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e6969d9-703c-304f-8ff9-80a18acb156d",
                    "sense_definition": "Een beweging van een hoger naar een lager punt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "descent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het vliegtuig zette de <w>daling</w> in richting de landingsbaan.",
                            "target_lang_translations": {
                                "en": "The plane began its <w>descent</w> towards the runway.",
                                "ru": "Самолет начал <w>снижение</w> к взлетно-посадочной полосе."
                            }
                        },
                        {
                            "text": "De <w>daling</w> vanaf de bergtop was steil en gevaarlijk.",
                            "target_lang_translations": {
                                "en": "The <w>descent</w> from the mountain peak was steep and dangerous.",
                                "ru": "<w>Спуск</w> с вершины горы был крутым и опасным."
                            }
                        }
                    ],
                    "synonyms": [
                        "afdaling"
                    ],
                    "antonyms": [
                        "stijging"
                    ],
                    "common_phrases": [
                        "de daling inzetten"
                    ],
                    "target_lang_definitions": {
                        "en": "A movement from a higher to a lower point.",
                        "ru": "Движение с более высокой точки в более низкую."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "descent",
                                "target_lang_sense_clarification": "act of moving downwards"
                            },
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "steep downward slope or fall"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спуск",
                                "target_lang_sense_clarification": "движение вниз по поверхности"
                            },
                            {
                                "target_lang_word": "снижение",
                                "target_lang_sense_clarification": "потеря высоты (например, у самолета)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "976f0db2-f861-362a-bf2e-e56a780c8309",
                    "sense_definition": "Het onbeklemtoonde deel van een versvoet in de poëzie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "linguistic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de metriek wordt het onderscheid tussen heffing en <w>daling</w> gemaakt.",
                            "target_lang_translations": {
                                "en": "In metrics, the distinction is made between arsis and <w>thesis</w>.",
                                "ru": "В метрике делается различие между арсисом и <w>тезисом</w>."
                            }
                        },
                        {
                            "text": "Een trochee bestaat uit een heffing gevolgd door een <w>daling</w>.",
                            "target_lang_translations": {
                                "en": "A trochee consists of a stress followed by an <w>unstressed syllable</w>.",
                                "ru": "Хорей состоит из сильной доли, за которой следует <w>слабая</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "heffing"
                    ],
                    "target_lang_definitions": {
                        "en": "The unstressed part of a metrical foot in poetry.",
                        "ru": "Неударная часть стихотворной стопы в поэзии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thesis",
                                "target_lang_sense_clarification": "unstressed part of a metrical foot"
                            },
                            {
                                "target_lang_word": "unstressed syllable",
                                "target_lang_sense_clarification": "syllable without emphasis"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тезис",
                                "target_lang_sense_clarification": "ненапряженная часть стопы (термин)"
                            },
                            {
                                "target_lang_word": "слабая доля",
                                "target_lang_sense_clarification": "безударная часть ритма"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
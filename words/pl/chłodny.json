                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Chłodny</w> mieszka w tym bloku od wielu lat.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Хлодный</w> живет в этом доме много лет."

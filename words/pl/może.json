            "senses": [
                {
                    "sense_id": "989f0be2-f7c7-36d8-95a8-20c0cc14e37c",
                    "sense_definition": "Słowo nadające wypowiedzi charakter przypuszczenia, oznaczające, że coś jest prawdopodobne, ale niepewne.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "probability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W nocy <w>może</w> padać śnieg z deszczem.",
                            "target_lang_translations": {
                                "ru": "Ночью <w>может</w> пойти снег с дождем."
                            }
                        },
                        {
                            "text": "Pojadę do Kowalskich, <w>może</w> już wrócili z Egiptu.",
                            "target_lang_translations": {
                                "ru": "Поеду к Ковальским, <w>может быть</w>, они уже вернулись из Египта."
                            }
                        }
                    ],
                    "synonyms": [
                        "być może",
                        "możliwe"
                    ],
                    "antonyms": [
                        "na pewno",
                        "z pewnością",
                        "bez wątpienia",
                        "na sto procent"
                    ],
                    "common_phrases": [
                        "a może",
                        "może jutro",
                        "może jednak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, выражающее предположение, означающее, что что-то вероятно, но не точно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "может быть",

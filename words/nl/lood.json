            "senses": [
                {
                    "sense_id": "7cbffa53-5f1f-389c-ae20-8afd3e974a66",
                    "sense_definition": "Een zwaar, donkergrijs metaal (chemisch element Pb).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn 18 kilo zware rugzak, bepakt met voedsel en water, voelde als <w>lood</w> op mijn rug.",
                            "target_lang_translations": {
                                "en": "My 18-kilo backpack, packed with food and water, felt like <w>lead</w> on my back.",
                                "ru": "Мой 18-килограммовый рюкзак, набитый едой и водой, давил мне на спину как <w>свинец</w>."
                            }
                        },
                        {
                            "text": "De oude waterleidingen in dit huis zijn nog van <w>lood</w> gemaakt.",
                            "target_lang_translations": {
                                "en": "The old water pipes in this house are still made of <w>lead</w>.",
                                "ru": "Старые водопроводные трубы в этом доме все еще сделаны из <w>свинца</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zwaar als lood",
                        "lood om oud ijzer"
                    ],
                    "target_lang_definitions": {
                        "en": "A heavy, dark grey metal (chemical element Pb).",
                        "ru": "Тяжелый темно-серый металл (химический элемент Pb)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "metal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "свинец",
                                "target_lang_sense_clarification": "металл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfece7ae-1312-3b76-ad17-c2b313692560",
                    "sense_definition": "Een gewicht aan een touw om te bepalen of iets verticaal is of om de diepte te meten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De metselaar gebruikt een <w>lood</w> om te controleren of de muur recht staat.",
                            "target_lang_translations": {
                                "en": "The mason uses a <w>plumb bob</w> to check if the wall is straight.",
                                "ru": "Каменщик использует <w>отвес</w>, чтобы проверить, ровно ли стоит стена."
                            }
                        },
                        {
                            "text": "Dat schilderij hangt helemaal uit het <w>lood</w>.",
                            "target_lang_translations": {
                                "en": "That painting is hanging completely out of <w>plumb</w>.",
                                "ru": "Эта картина висит совершенно не по <w>отвесу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "schietlood",
                        "paslood"
                    ],
                    "common_phrases": [
                        "uit het lood",
                        "in het lood"
                    ],
                    "target_lang_definitions": {
                        "en": "A weight on a rope used to determine if something is vertical or to measure depth.",
                        "ru": "Груз на веревке для определения вертикали или измерения глубины."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plumb bob",
                                "target_lang_sense_clarification": "tool for verticality"
                            },
                            {
                                "target_lang_word": "plummet",
                                "target_lang_sense_clarification": "weight on a line"
                            },
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "sounding weight"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отвес",
                                "target_lang_sense_clarification": "строительный инструмент"
                            },
                            {
                                "target_lang_word": "лот",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee5e8d2c-a1b0-3de0-9c46-13571ddaa9fc",
                    "sense_definition": "Een oude gewichtsmaat van ongeveer 10 tot 15 gram, vooral bekend van een spreekwoord.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kom op jongen, 't zijn de laatste <w>loodjes</w>!",
                            "target_lang_translations": {
                                "en": "Come on boy, it's the final <w>stretch</w>!",
                                "ru": "Давай, парень, остались последние <w>усилия</w>!"
                            }
                        },
                        {
                            "text": "Vroeger kocht men kruiden per <w>lood</w> op de markt.",
                            "target_lang_translations": {
                                "en": "In the past, people bought herbs by the <w>lood</w> at the market.",
                                "ru": "Раньше на рынке покупали пряности на <w>лот</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de laatste loodjes"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "unit of measurement"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An old unit of weight of approximately 10 to 15 grams, mainly known from a proverb.",
                        "ru": "Старинная мера веса (около 10-15 грамм), известная по пословице."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lood",
                                "target_lang_sense_clarification": "archaic unit of weight"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лот",
                                "target_lang_sense_clarification": "старинная мера веса"
                            }
                        ]
                    }
                }
            ]
        },

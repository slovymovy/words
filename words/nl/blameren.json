{
    "word_family": [
        "blaam",
        "blamage"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ee304484-91ee-3379-99a0-fceef0843738",
                    "sense_definition": "Iemand of zichzelf in het openbaar belachelijk maken of te schande zetten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disgrace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <blameerde> zich enorm door de naam van de gastheer te vergeten.",
                            "target_lang_translations": {
                                "en": "He hugely <embarrassed> himself by forgetting the host's name.",
                                "ru": "Он сильно <опозорился>, забыв имя хозяина."
                            }
                        },
                        {
                            "text": "Met die slechte prestatie <blameerde> het elftal de hele club.",
                            "target_lang_translations": {
                                "en": "With that poor performance, the team <disgraced> the whole club.",
                                "ru": "Этим плохим выступлением команда <опозорила> весь клуб."
                            }
                        }
                    ],
                    "synonyms": [
                        "te schande maken",
                        "onteren",
                        "voor gek zetten"
                    ],
                    "antonyms": [
                        "eren",
                        "prijzen"
                    ],
                    "common_phrases": [
                        "zich blameren",
                        "zich onsterfelijk blameren"
                    ],
                    "target_lang_definitions": {
                        "en": "To publicly ridicule or disgrace someone or oneself.",
                        "ru": "Публично выставлять кого-либо или себя в смешном свете, позорить."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disgrace",
                                "target_lang_sense_clarification": "to bring shame upon someone or something"
                            },
                            {
                                "target_lang_word": "embarrass",
                                "target_lang_sense_clarification": "to cause to feel ashamed or foolish"
                            },
                            {
                                "target_lang_word": "humiliate",
                                "target_lang_sense_clarification": "to make someone feel ashamed and foolish by injuring their dignity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "позорить",
                                "target_lang_sense_clarification": "навлекать позор"
                            },
                            {
                                "target_lang_word": "срамить",
                                "target_lang_sense_clarification": "стыдить, позорить (разг.)"
                            },
                            {
                                "target_lang_word": "выставлять на посмешище",
                                "target_lang_sense_clarification": "делать объектом насмешек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61c525d4-41fd-38d3-a7c1-8b3d527b7117",
                    "sense_definition": "Iemands goede naam aantasten door kwaadsprekerij.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slander",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schrijver probeerde zijn rivalen te <blameren> in zijn nieuwe boek.",
                            "target_lang_translations": {
                                "en": "The writer tried to <discredit> his rivals in his new book.",
                                "ru": "Писатель пытался <опорочить> своих соперников в своей новой книге."
                            }
                        },
                        {
                            "text": "Het is onfatsoenlijk om iemand zonder bewijs te <blameren>.",
                            "target_lang_translations": {
                                "en": "It is indecent to <slander> someone without proof.",
                                "ru": "Неприлично <чернить> кого-то без доказательств."
                            }
                        }
                    ],
                    "synonyms": [
                        "lasteren",
                        "zwartmaken",
                        "belasteren"
                    ],
                    "antonyms": [
                        "ophemelen",
                        "loven"
                    ],
                    "target_lang_definitions": {
                        "en": "To damage someone's good reputation through malicious talk or slander.",
                        "ru": "Портить чью-либо репутацию злословием или клеветой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "discredit",
                                "target_lang_sense_clarification": "to harm the good reputation of"
                            },
                            {
                                "target_lang_word": "slander",
                                "target_lang_sense_clarification": "to make false and damaging statements about"
                            },
                            {
                                "target_lang_word": "defame",
                                "target_lang_sense_clarification": "to damage the good reputation of"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "порочить",
                                "target_lang_sense_clarification": "наносить вред репутации"
                            },
                            {
                                "target_lang_word": "очернять",
                                "target_lang_sense_clarification": "представлять в дурном свете"
                            },
                            {
                                "target_lang_word": "клеветать",
                                "target_lang_sense_clarification": "распространять ложные слухи"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical (law)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный предмет договора, например, недвижимость или земля, указанный в юридическом документе о передаче прав."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объект недвижимости",
                                "target_lang_sense_clarification": "юридический термин"

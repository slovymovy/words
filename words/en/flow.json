                {
                    "sense_id": "7077edb4-7cf2-3f31-83ec-14db1eb2ec9e",
                    "sense_definition": "A smooth and continuous progression or sequence of events, words, or ideas.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_smoothness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accident interrupted the <w>flow</w> of traffic for an hour.",

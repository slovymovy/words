    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4b05e1e0-f73e-3b12-b730-eb493fd50f37",
                    "sense_definition": "Belonging or relating to a particular subject or thing as a right or accessory.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer reviewed all documents <w>appertinent</w> to the complex case.",
                            "target_lang_translations": {
                                "ru": "Юрист изучил все документы, <w>относящиеся</w> к этому сложному делу."
                            }
                        },
                        {
                            "text": "He focused only on the duties <w>appertinent</w> to his specific role.",
                            "target_lang_translations": {
                                "ru": "Он сосредоточился только на обязанностях, <w>присущих</w> его конкретной роли."
                            }
                        }
                    ],
                    "synonyms": [
                        "appurtenant",
                        "pertinent",
                        "relevant"
                    ],
                    "antonyms": [
                        "irrelevant",
                        "unrelated"
                    ],
                    "common_phrases": [
                        "appertinent to"
                    ],
                    "traits": [

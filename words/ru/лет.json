                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сколько тебе <w>лет</w>?",
                            "target_lang_translations": {
                                "en": "How <w>old</w> are you?"
                            }
                        },
                        {
                            "text": "Мы не виделись уже много <w>лет</w>.",
                            "target_lang_translations": {
                                "en": "We haven't seen each other for many <w>years</w>."
                            }
                        },
                        {
                            "text": "Ему исполнилось двадцать <w>лет</w>.",
                            "target_lang_translations": {
                                "en": "He turned twenty <w>years</w> old."
                            }
                        }
                    ],
                    "common_phrases": [

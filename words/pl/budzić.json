            "senses": [
                {
                    "sense_id": "c9269cca-44cc-3d78-ad0d-b92b869f1bb8",
                    "sense_definition": "Przerywać czyjś sen; sprawiać, że ktoś przestaje spać.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sleep_wake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę codziennie <budzić> syna o siódmej rano do szkoły.",
                            "target_lang_translations": {
                                "ru": "Я должен каждый день <будить> сына в семь утра в школу."
                            }
                        },
                        {
                            "text": "Proszę mnie nie <budzić> przed śniadaniem, chcę się wyspać.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <будите> меня перед завтраком, я хочу выспаться."
                            }
                        }
                    ],
                    "antonyms": [
                        "usypiać"
                    ],
                    "common_phrases": [
                        "budzić dzieci",
                        "budzić kogoś rano",
                        "budzić ze snu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прерывать чей-то сон; заставлять кого-то проснуться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "будить",
                                "target_lang_sense_clarification": "выводить из состояния сна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bae31cc-a728-3ebf-b17f-3f71dd1d49ec",
                    "sense_definition": "Przestawać spać i odzyskiwać świadomość (w połączeniu z zaimkiem 'się').",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sleep_wake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zwykle <budzę> się wypoczęty i pełen energii do działania.",
                            "target_lang_translations": {
                                "ru": "Обычно я <просыпаюсь> отдохнувшим и полным энергии для действий."
                            }
                        },
                        {
                            "text": "O której godzinie <budzisz> się w weekendy?",
                            "target_lang_translations": {
                                "ru": "В котором часу ты <просыпаешься> по выходным?"
                            }
                        }
                    ],
                    "antonyms": [
                        "zasypiać"
                    ],
                    "common_phrases": [
                        "budzić się rano",
                        "budzić się wcześnie",
                        "budzić się z krzykiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать спать и приходить в сознание (используется с возвратным местоимением 'się')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просыпаться",
                                "target_lang_sense_clarification": "переставать спать"
                            },
                            {
                                "target_lang_word": "пробуждаться",
                                "target_lang_sense_clarification": "книжн. выходить из сна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "409057ae-7a81-329e-b7a8-f223a1dcec36",
                    "sense_definition": "Wywoływać lub powodować pojawienie się jakichś uczuć, myśli, wątpliwości lub reakcji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego dziwne zachowanie <budzi> moje poważne podejrzenia.",
                            "target_lang_translations": {
                                "ru": "Его странное поведение <вызывает> у меня серьезные подозрения."
                            }
                        },
                        {
                            "text": "Ten nowy projekt <budzi> wiele kontrowersji wśród pracowników.",
                            "target_lang_translations": {
                                "ru": "Этот новый проект <вызывает> много споров среди сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzbudzać",
                        "wywoływać"
                    ],
                    "antonyms": [
                        "tłumić",
                        "gasić"
                    ],
                    "common_phrases": [
                        "budzić wątpliwości",
                        "budzić zaufanie",
                        "budzić strach",
                        "budzić emocje",
                        "budzić kontrowersje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать или порождать какие-либо чувства, мысли, сомнения или реакции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "порождать реакцию или чувство"
                            },
                            {
                                "target_lang_word": "пробуждать",
                                "target_lang_sense_clarification": "активировать чувства или мысли"
                            },
                            {
                                "target_lang_word": "возбуждать",
                                "target_lang_sense_clarification": "стимулировать (интерес, подозрения)"
                            },
                            {
                                "target_lang_word": "будить",
                                "target_lang_sense_clarification": "перен. вызывать к жизни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d7f1dad-a47c-3108-a182-d06e152c566c",
                    "sense_definition": "Poruszać ziemię lub rośliny narzędziami rolniczymi w celu spulchnienia gleby.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnicy wyszli w pole, aby <budzić> buraki motykami.",
                            "target_lang_translations": {
                                "ru": "Фермеры вышли в поле, чтобы <рыхлить> свеклу мотыгами."
                            }
                        },
                        {
                            "text": "Wiosną trzeba <budzić> ziemię, aby przygotować ją pod zasiew.",
                            "target_lang_translations": {
                                "ru": "Весной нужно <взрыхлять> землю, чтобы подготовить её к посеву."
                            }
                        }
                    ],
                    "common_phrases": [
                        "budzić buraki",
                        "budzić ziemię"
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2101c6be-4e98-3311-925d-c44f19f8e34a",
                    "sense_definition": "Zdrobnienie od rzeczownika <w>słońce</w>; pieszczotliwe lub potoczne określenie gwiazdy centralnej Układu Słonecznego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sun_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Słonko</w> mocno dziś przygrzewa, więc warto założyć czapkę.",
                            "target_lang_translations": {
                                "ru": "<w>Солнышко</w> сегодня сильно припекает, поэтому стоит надеть шапку."
                            }
                        },
                        {
                            "text": "Kiedy <słonko> wyjdzie zza chmur, pójdziemy na długi spacer.",
                            "target_lang_translations": {
                                "ru": "Когда <w>солнышко</w> выйдет из-за туч, мы пойдем на долгую прогулку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jasne słonko",
                        "zachodzące słonko",
                        "wychodzi słonko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительно-ласкательная форма от существительного «солнце»; ласковое или разговорное название центральной звезды Солнечной системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солнышко",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное от «солнце»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8499e58d-b4a8-3544-9745-9ae6a15f57bb",
                    "sense_definition": "Pieszczotliwy zwrot do bliskiej, kochanej osoby (np. dziecka lub partnera).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "endearment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przytul się, <w>słonko</w> ty moje, i opowiedz mi o swoim dniu.",
                            "target_lang_translations": {
                                "ru": "Обними меня, <w>солнышко</w> ты мое, и расскажи мне о своем дне."
                            }
                        },
                        {
                            "text": "Nie martw się, <słonko>, wszystko na pewno się ułoży.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся, <w>солнышко</w>, все обязательно образуется."
                            }
                        }
                    ],
                    "synonyms": [
                        "kochanie"
                    ],
                    "common_phrases": [
                        "moje słonko"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое обращение к близкому, любимому человеку (например, к ребенку или партнеру)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солнышко",
                                "target_lang_sense_clarification": "ласковое обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca6e5361-aa1c-3810-a863-abe773431220",
                    "sense_definition": "Forma wołacza liczby pojedynczej rzeczownika <w>słonka</w> (gatunek ptaka).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uciekaj, mała <w>słonko</w>, bo w lesie czai się lis.",
                            "target_lang_translations": {
                                "ru": "Улетай, маленький <w>вальдшнеп</w>, потому что в лесу притаилась лиса."
                            }
                        },
                        {
                            "text": "Gdzie się ukryłaś, płochliwa <w>słonko</w>, pośród tych gęstych traw?",
                            "target_lang_translations": {
                                "ru": "Где ты спрятался, пугливый <w>вальдшнеп</w>, среди этих густых трав?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "vocative singular of słonka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма звательного падежа единственного числа существительного «słonka» (вальдшнеп)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вальдшнеп",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
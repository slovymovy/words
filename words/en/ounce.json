    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "158d5265-c0ef-3524-a8b3-142b6bcd6296",
                    "sense_definition": "A unit of weight equal to one sixteenth of a pound (approximately 28.35 grams).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The recipe requires four <w>ounces</w> of dark chocolate.",
                            "target_lang_translations": {
                                "ru": "Для рецепта требуется четыре <w>унции</w> темного шоколада."
                            }
                        },
                        {
                            "text": "She bought a gold coin that weighed exactly one <w>ounce</w>.",
                            "target_lang_translations": {
                                "ru": "Она купила золотую монету весом ровно в одну <w>унцию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "oz."
                    ],
                    "common_phrases": [
                        "half an ounce",
                        "quarter ounce",
                        "troy ounce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица веса, равная одной шестнадцатой фунта (примерно 28,35 грамма)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "унция",
                                "target_lang_sense_clarification": "единица веса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d8d3842-ba80-3357-9aec-239c928de842",
                    "sense_definition": "A unit of liquid volume equal to one sixteenth of a pint (approximately 30 milliliters).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please add eight fluid <w>ounces</w> of water to the mixture.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, добавьте в смесь восемь жидких <w>унций</w> воды."
                            }
                        },
                        {
                            "text": "This small bottle holds about two <w>ounces</w> of perfume.",
                            "target_lang_translations": {
                                "ru": "В этом маленьком флаконе около двух <w>унций</w> духов."
                            }
                        }
                    ],
                    "synonyms": [
                        "fl. oz."
                    ],
                    "common_phrases": [
                        "fluid ounce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица объема жидкости, равная одной шестнадцатой пинты (примерно 30 миллилитров)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "унция",
                                "target_lang_sense_clarification": "мера объема жидкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8320bb7-c7c5-36eb-9207-2a2daa96b92a",
                    "sense_definition": "A very small amount or a little bit of something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He did not show even an <w>ounce</w> of regret for his mistake.",
                            "target_lang_translations": {
                                "ru": "Он не выказал ни <w>капли</w> сожаления о своей ошибке."
                            }
                        },
                        {
                            "text": "An <w>ounce</w> of prevention is worth a pound of cure.",
                            "target_lang_translations": {
                                "ru": "<w>Грамм</w> профилактики стоит пуда лекарств."
                            }
                        }
                    ],
                    "synonyms": [
                        "modicum"
                    ],
                    "common_phrases": [
                        "ounce of prevention",
                        "ounce of truth",
                        "ounce of luck",
                        "ounce of common sense"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень маленькое количество или небольшая часть чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капля",
                                "target_lang_sense_clarification": "перен. ничтожное количество"
                            },
                            {
                                "target_lang_word": "грамм",
                                "target_lang_sense_clarification": "перен. малая доля"
                            },
                            {
                                "target_lang_word": "унция",
                                "target_lang_sense_clarification": "перен. малая величина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e499352c-d71c-3daa-8780-45c2d5402ea7",
                    "sense_definition": "A large wild cat native to the mountain ranges of Central and South Asia; a snow leopard.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The elusive <w>ounce</w> moves silently across the snowy peaks.",
                            "target_lang_translations": {
                                "ru": "Неуловимый <w>ирбис</w> бесшумно передвигается по заснеженным вершинам."
                            }
                        },
                        {
                            "text": "We saw a drawing of an <w>ounce</w> in the old zoology book.",
                            "target_lang_translations": {
                                "ru": "Мы видели рисунок <w>снежного барса</w> в старой книге по зоологии."
                            }
                        }
                    ],
                    "synonyms": [
                        "snow leopard",
                        "Panthera uncia"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "modern usage typically prefers 'snow leopard'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная дикая кошка, обитающая в горных хребтах Центральной и Южной Азии; снежный барс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ирбис",
                                "target_lang_sense_clarification": "животное"
                            },
                            {
                                "target_lang_word": "снежный барс",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
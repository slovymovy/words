                {
                    "sense_id": "edf09f96-d745-3e0b-81f1-dc8b559dff92",
                    "sense_definition": "Собрание представителей общественной организации, политической партии или профессиональной группы для решения важных вопросов.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Делегаты со всей страны прибыли на очередной <съезд> партии.",
                            "target_lang_translations": {
                                "en": "Delegates from all over the country arrived for the regular party <congress>."
                            }
                        },
                        {
                            "text": "Этот <съезд> писателей стал важным событием в культурной жизни.",
                            "target_lang_translations": {
                                "en": "This writers' <congress> became an important event in cultural life."
                            }
                        }
                    ],
                    "synonyms": [
                        "слёт"
                    ],
                    "common_phrases": [
                        "партийный съезд",
                        "делегат съезда",
                        "внеочередной съезд"
                    ],
                    "target_lang_definitions": {
                        "en": "A formal meeting of representatives of a political party, organization, or professional group."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "congress",
                                "target_lang_sense_clarification": "formal meeting of an organization"
                            },
                            {

{
    "word_family": [
        "igiełka",
        "igielny",
        "igłowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b89b93d-6de7-30bb-9f50-31f75be90d02",
                    "sense_definition": "Cienki, metalowy przedmiot z ostrym końcem i uszkiem na nitkę, używany do ręcznego szycia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tool_piercing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krawcowa nawlekła czerwoną nitkę na <w>igłę</w>.",
                            "target_lang_translations": {
                                "ru": "Портниха вдела красную нитку в <w>иголку</w>."
                            }
                        },
                        {
                            "text": "Ukłułam się <w>igłą</w> w palec podczas przyszywania guzika.",
                            "target_lang_translations": {
                                "ru": "Я укололась <w>иголкой</w> в палец, когда пришивала пуговицу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nawlec igłę",
                        "ucho igielne",
                        "szukać igły w stogu siana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий металлический предмет с острым концом и ушком для нитки, используемый для ручного шитья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иголка",
                                "target_lang_sense_clarification": "для шитья"
                            },
                            {
                                "target_lang_word": "игла",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe87b460-28df-3e5a-9d64-7e0498009318",
                    "sense_definition": "Cienka, pusta w środku rurka z ostrym końcem, nakładana na strzykawkę, służąca do robienia zastrzyków.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tool_piercing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pielęgniarka wyjęła nową <w>igłę</w> ze sterylnego opakowania.",
                            "target_lang_translations": {
                                "ru": "Медсестра достала новую <w>иглу</w> из стерильной упаковки."
                            }
                        },
                        {
                            "text": "Wiele małych dzieci płacze na widok <w>igły</w> u lekarza.",
                            "target_lang_translations": {
                                "ru": "Многие маленькие дети плачут при виде <w>иглы</w> у врача."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sterylna igła",
                        "igła do zastrzyków",
                        "bać się igły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкая полая трубка с острым концом, надеваемая на шприц, служащая для инъекций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игла",
                                "target_lang_sense_clarification": "медицинская"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c349aa6a-f442-3be6-accb-13e2e8c248c1",
                    "sense_definition": "Twardy, wąski liść drzew lub krzewów iglastych, na przykład sosny lub świerka.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod naszą choinką leżało mnóstwo opadłych <w>igieł</w>.",
                            "target_lang_translations": {
                                "ru": "Под нашей ёлкой лежало множество опавших <w>иголок</w>."
                            }
                        },
                        {
                            "text": "Sosnowe <w>igły</w> wydzielają w lesie piękny zapach.",
                            "target_lang_translations": {
                                "ru": "Сосновые <w>иглы</w> источают в лесу прекрасный аромат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "igły sosnowe",
                        "opadanie igieł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жесткий узкий лист хвойных деревьев или кустарников, например сосны или ели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иголка",
                                "target_lang_sense_clarification": "лист хвойных"
                            },
                            {
                                "target_lang_word": "хвоинка",
                                "target_lang_sense_clarification": "один лист"
                            },
                            {
                                "target_lang_word": "игла",
                                "target_lang_sense_clarification": "ботанический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a276978b-5bd6-38f9-8737-d3ea0a1f8203",
                    "sense_definition": "Element gramofonu, który przesuwa się w rowku płyty i odczytuje dźwięk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić nową <w>igłę</w>, bo stara rysuje płyty.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую <w>иглу</w>, потому что старая царапает пластинки."
                            }
                        },
                        {
                            "text": "Delikatnie opuścił ramię gramofonu, aż <w>igła</w> dotknęła winylu.",
                            "target_lang_translations": {
                                "ru": "Он осторожно опустил тонарм, пока <w>игла</w> не коснулась винила."
                            }
                        }
                    ],
                    "common_phrases": [
                        "igła gramofonowa",
                        "wymiana igły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент проигрывателя, который движется по канавке пластинки и считывает звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игла",
                                "target_lang_sense_clarification": "звукоснимателя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94574b88-8ea4-3985-94ee-fac684d71430",
                    "sense_definition": "Przedmiot używany, najczęściej samochód, utrzymany w idealnym stanie technicznym i wizualnym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprzedawca zapewniał, że ten samochód to prawdziwa <w>igła</w>.",
                            "target_lang_translations": {
                                "ru": "Продавец уверял, что эта машина — настоящая <w>конфетка</w>."
                            }
                        },
                        {
                            "text": "Kupiłem dziesięcioletnie auto, ale technicznie jest <w>igła</w>.",
                            "target_lang_translations": {
                                "ru": "Я купил десятилетнее авто, но технически оно — <w>идеал</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stan igła",
                        "auto igła"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "używane głównie w handlu samochodami"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подержанный предмет, чаще всего автомобиль, в идеальном техническом и внешнем состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конфетка",
                                "target_lang_sense_clarification": "сленг, об авто"
                            },
                            {
                                "target_lang_word": "идеал",
                                "target_lang_sense_clarification": "разг., идеальное состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a807cfa-7d44-3849-b17d-3147ed2238a5",
                    "sense_definition": "Mineralny element budujący szkielet niektórych bezkręgowców, na przykład gąbek.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod mikroskopem można zobaczyć wapienne <w>igły</w> gąbki.",
                            "target_lang_translations": {
                                "ru": "Под микроскопом можно увидеть известковые <w>иглы</w> губки."
                            }
                        },
                        {
                            "text": "Kształt <w>igieł</w> pomaga naukowcom rozpoznać gatunek zwierzęcia.",
                            "target_lang_translations": {
                                "ru": "Форма <w>игл</w> помогает учёным определить вид животного."
                            }
                        }
                    ],
                    "synonyms": [
                        "skleryt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Минеральный элемент, образующий скелет некоторых беспозвоночных, например губок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игла",
                                "target_lang_sense_clarification": "биологический термин"
                            },
                            {
                                "target_lang_word": "спикула",
                                "target_lang_sense_clarification": "научный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
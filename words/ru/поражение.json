    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5a6b1361-59a4-393c-9108-721e35d5f227",
                    "sense_definition": "Проигрыш, неудача в борьбе, войне, спортивном состязании или другом конфликте.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В решающем матче наша команда потерпела обидное <w>поражение</w> со счётом ноль один.",
                            "target_lang_translations": {
                                "en": "In the deciding match, our team suffered a bitter <w>defeat</w> with a score of zero to one."
                            }
                        },
                        {
                            "text": "Генерал понимал, что дальнейшее сопротивление приведёт лишь к полному <w>поражению</w> армии.",
                            "target_lang_translations": {
                                "en": "The general understood that further resistance would only lead to the complete <w>defeat</w> of the army."
                            }
                        }
                    ],
                    "synonyms": [
                        "проигрыш",
                        "неудача",
                        "разгром",
                        "фиаско",
                        "крах",
                        "катастрофа"
                    ],
                    "antonyms": [
                        "победа",
                        "успех",
                        "верх"
                    ],
                    "common_phrases": [
                        "потерпеть поражение",
                        "нанести поражение",
                        "признать поражение",
                        "полное поражение"
                    ],
                    "target_lang_definitions": {
                        "en": "A loss or failure in a struggle, war, sports competition, or other conflict."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defeat",
                                "target_lang_sense_clarification": "loss in a contest or battle"
                            },
                            {
                                "target_lang_word": "loss",
                                "target_lang_sense_clarification": "failure to win"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19a0d0c9-bb9f-34a7-b907-618248d9c92a",
                    "sense_definition": "Пагубное, разрушительное воздействие на организм, ткань или объект; повреждение болезнью или оружием.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир отдал приказ открыть огонь на <w>поражение</w> по наступающим танкам противника.",
                            "target_lang_translations": {
                                "en": "The commander gave the order to open fire for <w>effect</w> on the advancing enemy tanks."
                            }
                        },
                        {
                            "text": "Медицинское обследование выявило у пациента серьёзное грибковое <w>поражение</w> кожи.",
                            "target_lang_translations": {
                                "en": "Medical examination revealed a serious fungal <w>lesion</w> of the patient's skin."
                            }
                        }
                    ],
                    "synonyms": [
                        "повреждение"
                    ],
                    "antonyms": [
                        "исцеление",
                        "защита"
                    ],
                    "common_phrases": [
                        "зона поражения",
                        "огонь на поражение",
                        "степень поражения",
                        "очаг поражения"
                    ],
                    "target_lang_definitions": {
                        "en": "A harmful, destructive effect on an organism, tissue, or object; damage caused by disease or weapons."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lesion",
                                "target_lang_sense_clarification": "medical: damage to tissue"
                            },
                            {
                                "target_lang_word": "damage",
                                "target_lang_sense_clarification": "harm or injury"
                            },
                            {
                                "target_lang_word": "destruction",
                                "target_lang_sense_clarification": "severe damage or ruin"
                            },
                            {
                                "target_lang_word": "affection",
                                "target_lang_sense_clarification": "medical: a morbid condition or disease"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50bcc833-de3a-3a6d-916c-5891c04ac13e",
                    "sense_definition": "Лишение или ограничение политических и гражданских прав (юридический термин).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В прошлом веке за некоторые тяжкие преступления предусматривалось <w>поражение</w> в правах.",
                            "target_lang_translations": {
                                "en": "In the last century, <w>deprivation</w> of rights was prescribed for certain grave crimes."
                            }
                        },
                        {
                            "text": "Судебный приговор включал в себя не только штраф, но и временное <w>поражение</w> в избирательных правах.",
                            "target_lang_translations": {
                                "en": "The court sentence included not only a fine but also a temporary <w>deprivation</w> of voting rights."
                            }
                        }
                    ],
                    "common_phrases": [
                        "поражение в правах"
                    ],
                    "target_lang_definitions": {
                        "en": "Deprivation or restriction of political and civil rights (legal term)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deprivation",

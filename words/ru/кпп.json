            "senses": [
                {
                    "sense_id": "23936cdf-2c2d-3dc1-a99f-78b9bdb6882b",
                    "sense_definition": "Сокращение от <w>контрольно-пропускной пункт</w>; специально оборудованный пункт для проверки документов и досмотра транспорта при входе или въезде на охраняемую территорию.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охрана остановила машину на <КПП> для проверки пропусков.",
                            "target_lang_translations": {
                                "en": "Security stopped the car at the <w>checkpoint</w> to check passes."
                            }
                        },
                        {
                            "text": "Все сотрудники обязаны предъявлять удостоверение, проходя через <КПП>.",
                            "target_lang_translations": {
                                "en": "All employees are required to show their ID when passing through the <w>security gate</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пройти через КПП",
                        "дежурный на КПП",
                        "предъявить на КПП"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for \"control-checkpoint\"; a designated point for checking documents and inspecting vehicles upon entry to a secured area."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "checkpoint",
                                "target_lang_sense_clarification": "security post"
                            },
                            {
                                "target_lang_word": "security gate",
                                "target_lang_sense_clarification": "entrance control"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "384313bc-70ce-30ed-bbfb-c1b94a8c0d1f",
                    "sense_definition": "Сокращение от <w>коробка переключения передач</w>; механизм в транспортном средстве, который изменяет, передаёт или прекращает передачу крутящего момента от двигателя к колёсам.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой модели автомобиля установлена автоматическая <КПП>.",
                            "target_lang_translations": {
                                "en": "This car model is equipped with an automatic <w>transmission</w>."
                            }
                        },
                        {
                            "text": "Ремонт <КПП> может стоить довольно дорого для владельца.",
                            "target_lang_translations": {
                                "en": "Repairing the <w>gearbox</w> can be quite expensive for the owner."
                            }
                        }
                    ],
                    "synonyms": [
                        "коробка передач"
                    ],
                    "common_phrases": [
                        "автоматическая КПП",
                        "механическая КПП",
                        "масло для КПП"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for \"gearbox\"; a vehicle mechanism that modifies and transmits torque from the engine to the wheels."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gearbox",
                                "target_lang_sense_clarification": "mechanical component"
                            },
                            {
                                "target_lang_word": "transmission",
                                "target_lang_sense_clarification": "vehicle system"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "412fc5da-7584-3773-ae18-173174602507",
                    "sense_definition": "Сокращение от <w>код причины постановки</w> на учёт; банковский реквизит, который дополняет ИНН и используется для идентификации организации в налоговых органах.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для оплаты счёта необходимо правильно указать ИНН и <КПП> получателя.",
                            "target_lang_translations": {
                                "en": "To pay the invoice, it is necessary to correctly specify the recipient's TIN and <w>KPP</w>."
                            }
                        },
                        {
                            "text": "Бухгалтер проверил <КПП> банка перед отправкой платежа.",
                            "target_lang_translations": {
                                "en": "The accountant checked the bank's <w>KPP</w> before sending the payment."
                            }
                        }
                    ],
                    "common_phrases": [
                        "указать КПП",
                        "ИНН и КПП",
                        "КПП организации"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for \"reason for registration code\"; a tax code used alongside the INN (TIN) to identify an organization's specific branch or location for tax purposes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "KPP",
                                "target_lang_sense_clarification": "Russian tax code acronym"
                            },
                            {
                                "target_lang_word": "Tax Registration Reason Code",
                                "target_lang_sense_clarification": "formal description"
                            }
                        ]
                    }
                }
            ]
        }

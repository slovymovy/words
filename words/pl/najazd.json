                {
                    "sense_id": "9de0d241-0c94-3b6b-b1fa-afa7c4742250",
                    "sense_definition": "Zbrojne wtargnięcie wojsk na terytorium innego państwa w celu jego podboju lub grabieży.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_aggression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Historyczny <w>najazd</w> wojsk szwedzkich na Polskę miał miejsce w siedemnastym wieku.",
                            "target_lang_translations": {
                                "ru": "Историческое <w>нашествие</w> шведских войск на Польшу произошло в семнадцатом веке."
                            }
                        },
                        {
                            "text": "Mieszkańcy przygranicznych wiosek obawiali się, że wkrótce nastąpi kolejny <w>najazd</w> wroga.",
                            "target_lang_translations": {
                                "ru": "Жители приграничных деревень опасались, что вскоре произойдет очередной <w>набег</w> врага."
                            }
                        }
                    ],
                    "synonyms": [
                        "inwazja",
                        "agresja",
                        "napad"
                    ],
                    "common_phrases": [
                        "zbrojny najazd",
                        "najazd tatarski",
                        "najazd wroga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вооруженное вторжение войск на территорию другого государства с целью завоевания или грабежа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нашествие",
                                "target_lang_sense_clarification": "массовое вторжение врага"
                            },
                            {
                                "target_lang_word": "набег",
                                "target_lang_sense_clarification": "внезапное нападение с целью грабежа"
                            },
                            {
                                "target_lang_word": "вторжение",
                                "target_lang_sense_clarification": "вход войск на чужую территорию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4501722-0f7d-33b3-bf75-bbd0570cae42",
                    "sense_definition": "Tłumne, często niespodziewane lub uciążliwe przybycie dużej liczby osób w jedno miejsce.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_aggression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdy słoneczny weekend obserwujemy prawdziwy <w>najazd</w> turystów na nadmorskie plaże.",
                            "target_lang_translations": {
                                "ru": "В каждые солнечные выходные мы наблюдаем настоящее <w>нашествие</w> туристов на морские пляжи."
                            }
                        },
                        {
                            "text": "Sklep przeżył wczoraj <w>najazd</w> klientów z powodu wielkiej wyprzedaży.",
                            "target_lang_translations": {
                                "ru": "Вчера магазин пережил <w>наплыв</w> клиентов из-за большой распродажи."
                            }
                        }
                    ],
                    "synonyms": [
                        "inwazja"
                    ],
                    "common_phrases": [

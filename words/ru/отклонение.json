    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0f06807-08cc-3d5b-a518-6337d6ff6620",
                    "sense_definition": "Несовпадение с нормой, стандартом или ожидаемым значением; расхождение в чем-либо.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "deviation_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В работе двигателя было замечено небольшое <w>отклонение</w> от нормы.",
                            "target_lang_translations": {
                                "en": "A slight <w>deviation</w> from the norm was noticed in the engine's operation."
                            }
                        },
                        {
                            "text": "Любое <w>отклонение</w> от маршрута может быть опасным для группы.",
                            "target_lang_translations": {
                                "en": "Any <w>deviation</w> from the route could be dangerous for the group."
                            }
                        }
                    ],
                    "synonyms": [
                        "несовпадение",
                        "расхождение"
                    ],
                    "antonyms": [
                        "совпадение",
                        "норма",
                        "соответствие"
                    ],
                    "common_phrases": [
                        "отклонение от нормы",
                        "допустимое отклонение",
                        "отклонение от графика",
                        "стандартное отклонение"
                    ],
                    "target_lang_definitions": {
                        "en": "A difference or discrepancy from a norm, standard, or expected value."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deviation",
                                "target_lang_sense_clarification": "departure from a standard, norm, or course"
                            },
                            {
                                "target_lang_word": "divergence",
                                "target_lang_sense_clarification": "moving away or separating from a path or standard"
                            },
                            {
                                "target_lang_word": "abnormality",
                                "target_lang_sense_clarification": "state of being irregular or not normal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fabe3391-0eb5-3433-ad24-4d3afcd5b3d3",
                    "sense_definition": "Действие по глаголу <w>отклонять</w>; отказ принять что-либо или изменение физического направления.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "deviation_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Неожиданное <w>отклонение</w> моей заявки заставило меня искать другие варианты.",
                            "target_lang_translations": {
                                "en": "The unexpected <w>rejection</w> of my application forced me to look for other options."
                            }
                        },
                        {
                            "text": "Магнитное поле вызывает заметное <w>отклонение</w> стрелки компаса.",
                            "target_lang_translations": {
                                "en": "The magnetic field causes a noticeable <w>deflection</w> of the compass needle."
                            }
                        }
                    ],
                    "synonyms": [
                        "несовпадение",
                        "расхождение"
                    ],
                    "antonyms": [
                        "принятие",
                        "одобрение",
                        "согласие"
                    ],
                    "common_phrases": [
                        "отклонение предложения",
                        "отклонение жалобы",
                        "отклонение луча",
                        "отклонение ходатайства"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of refusing to accept something (like a proposal) or causing something to change its physical direction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rejection",
                                "target_lang_sense_clarification": "refusal to accept a proposal, application, or offer"
                            },
                            {
                                "target_lang_word": "deflection",
                                "target_lang_sense_clarification": "change of course or direction (physics, mechanics)"
                            },
                            {
                                "target_lang_word": "dismissal",
                                "target_lang_sense_clarification": "formal rejection, often in legal or official contexts"
                            }
                        ]
                    }
                }
            ]
        }
    ]

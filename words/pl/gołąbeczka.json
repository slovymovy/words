            "senses": [
                {
                    "sense_id": "23a20035-dd35-3e0e-947b-ea4d7f8bcaef",
                    "sense_definition": "Zdrobnienie od słowa <w>gołąbka</w>; mała lub młoda samica gołębia.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "bird_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na parapecie usiadła szara <gołąbeczka> i zaczęła cicho gruchać.",
                            "target_lang_translations": {
                                "ru": "На подоконник села серая <голубка> и начала тихо ворковать."
                            }
                        },
                        {
                            "text": "W parku dzieci rzucały okruchy, a mała <gołąbeczka> podbiegła, by je zjeść.",
                            "target_lang_translations": {
                                "ru": "В парке дети бросали крошки, и маленькая <голубка> подбежала, чтобы их съесть."
                            }
                        }
                    ],
                    "synonyms": [
                        "gołąbka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zdrobnienie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова \"gołąbka\"; маленькая или молодая самка голубя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голубка",
                                "target_lang_sense_clarification": "самка голубя"
                            },
                            {
                                "target_lang_word": "голубочка",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ca10597-521d-3d4a-909f-f350a05f68cd",
                    "sense_definition": "Pieszczotliwe określenie kobiety lub dziewczyny, wyrażające sympatię i czułość.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "endearment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego żona to prawdziwa <gołąbeczka>, zawsze łagodna i uśmiechnięta.",
                            "target_lang_translations": {
                                "ru": "Его жена — настоящая <голубушка>, всегда кроткая и улыбчивая."
                            }
                        },
                        {
                            "text": "Nie płacz, moja <gołąbeczko>, wszystko się jakoś ułoży.",
                            "target_lang_translations": {
                                "ru": "Не плачь, моя <голубка>, всё как-нибудь образуется."
                            }
                        }
                    ],
                    "synonyms": [
                        "kochanie"
                    ],
                    "common_phrases": [
                        "moja gołąbeczka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое обращение к женщине или девушке, выражающее симпатию и нежность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голубушка",
                                "target_lang_sense_clarification": "ласковое обращение"
                            },
                            {
                                "target_lang_word": "голубка",
                                "target_lang_sense_clarification": "ласковое обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3637f06-6878-31be-a2e6-3cb195877c35",
                    "sense_definition": "Forma gramatyczna (dopełniacz lub biernik liczby pojedynczej) rzeczownika <w>gołąbeczek</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie widziałem dzisiaj tego małego <gołąbeczka>, który zwykle tu przylatuje.",
                            "target_lang_translations": {
                                "ru": "Я не видел сегодня этого маленького <голубка>, который обычно сюда прилетает."
                            }
                        },
                        {
                            "text": "Kupiłem synowi pluszowego <gołąbeczka> na urodziny.",
                            "target_lang_translations": {
                                "ru": "Я купил сыну плюшевого <голубка> на день рождения."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "grammatical form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (родительный или винительный падеж единственного числа) существительного \"gołąbeczek\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голубка",
                                "target_lang_sense_clarification": "форма слова \"голубок\""
                            }
                        ]
                    }
                }
            ]
        }
    ]

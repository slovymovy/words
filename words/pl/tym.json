    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "d8b1bf3e-008e-3033-a865-96fb804f1c34",
                    "sense_definition": "Forma gramatyczna (narzędnik lub miejscownik liczby pojedynczej) zaimka wskazującego <w>ten</w> (rodzaj męski) lub <w>to</w> (rodzaj nijaki)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_inflections",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkam w <w>tym</w> domu od urodzenia.",
                            "target_lang_translations": {
                                "ru": "Я живу в <w>этом</w> доме с рождения."
                            }
                        },
                        {
                            "text": "Nie zgadzam się z <w>tym</w> pomysłem.",
                            "target_lang_translations": {
                                "ru": "Я не согласен с <w>этим</w> предложением."
                            }
                        },
                        {
                            "text": "W <w>tym</w> momencie zadzwonił telefon.",
                            "target_lang_translations": {
                                "ru": "В <w>этот</w> момент зазвонил телефон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "w tym roku",
                        "w tym samym",
                        "poza tym",
                        "z tym że",
                        "przed tym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (творительный или предложный падеж единственного числа) указательного местоимения <w>ten</w> (этот/тот) или <w>to</w> (это/то)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этим",
                                "target_lang_sense_clarification": "творительный падеж (от 'этот'/'это')"
                            },
                            {
                                "target_lang_word": "этом",
                                "target_lang_sense_clarification": "предложный падеж (от 'этот'/'это')"
                            },
                            {
                                "target_lang_word": "тем",
                                "target_lang_sense_clarification": "творительный падеж (от 'тот'/'то')"
                            },
                            {
                                "target_lang_word": "том",
                                "target_lang_sense_clarification": "предложный падеж (от 'тот'/'то')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91080737-a970-3a7d-9775-e17d36a997dc",
                    "sense_definition": "Forma gramatyczna (celownik liczby mnogiej) zaimków wskazujących <w>ten</w>, <w>ta</w> oraz <w>to</w>",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_inflections",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przyglądam się <w>tym</w> ludziom z zainteresowaniem.",
                            "target_lang_translations": {
                                "ru": "Я с интересом присматриваюсь к <w>этим</w> людям."
                            }
                        },
                        {
                            "text": "Dzięki <w>tym</w> zmianom firma działa lepiej.",
                            "target_lang_translations": {
                                "ru": "Благодаря <w>этим</w> изменениям фирма работает лучше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ku tym",
                        "przeciwko tym",
                        "dzięki tym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (дательный падеж множественного числа) указательных местоимений <w>ten</w>, <w>ta</w> и <w>to</w>"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этим",
                                "target_lang_sense_clarification": "от 'эти'"
                            },
                            {
                                "target_lang_word": "тем",
                                "target_lang_sense_clarification": "от 'те'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "62ba57fd-35a5-38f0-a53c-b64b6b60ff65",
                    "sense_definition": "Słowo używane w konstrukcjach porównawczych w parze ze słowem 'im', wskazujące na zależność wzrostu jednej cechy od drugiej",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "comparative_structures",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Im więcej czytasz, <w>tym</w> więcej wiesz o świecie.",
                            "target_lang_translations": {
                                "ru": "Чем больше читаешь, <w>тем</w> больше знаешь о мире."
                            }
                        },
                        {
                            "text": "Im szybciej to zrobisz, <w>tym</w> szybciej wrócisz do domu.",
                            "target_lang_translations": {
                                "ru": "Чем быстрее ты это сделаешь, <w>тем</w> быстрее вернешься домой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "im prędzej tym lepiej",
                        "im więcej tym lepiej",
                        "im dalej tym gorzej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в сравнительных конструкциях в паре со словом 'im' (чем), указывающее на зависимость"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тем",
                                "target_lang_sense_clarification": "союзное слово (чем..., тем...)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13737c66-021c-34b2-baa6-1479aae86334",
                    "sense_definition": "Słowo wzmacniające stopień wyższy przymiotnika lub przysłówka, oznaczające 'jeszcze bardziej' w wynikającym kontekście",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "comparative_structures",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli on nie przyjdzie, to <w>tym</w> lepiej dla nas.",
                            "target_lang_translations": {
                                "ru": "Если он не придет, то <w>тем</w> лучше для нас."
                            }
                        },
                        {
                            "text": "<w>Tym</w> bardziej powinieneś się postarać, skoro masz szansę.",
                            "target_lang_translations": {
                                "ru": "<w>Тем</w> более ты должен постараться, раз у тебя есть шанс."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tym bardziej",
                        "tym lepiej",
                        "tym gorzej",
                        "tym niemniej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, усиливающее сравнительную степень прилагательного или наречия, означающее 'еще более' в данном контексте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тем",
                                "target_lang_sense_clarification": "усилительная частица (тем лучше, тем более)"
                            }
                        ]
                    }
                }
            ]
        },

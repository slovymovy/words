                {
                    "sense_id": "3a6d5126-81ed-36f7-bfde-808f5c6241e1",
                    "sense_definition": "Budynek lub sala wyposażona w scenę i widownię, w której wystawiane są sztuki i odbywają się spektakle.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "place_institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj wieczorem poszliśmy całą rodziną do <w>teatru</w> na nową komedię.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы всей семьёй пошли в <w>театр</w> на новую комедию."
                            }
                        },
                        {
                            "text": "Ten zabytkowy <w>teatr</w> znajduje się w samym centrum starego miasta.",
                            "target_lang_translations": {
                                "ru": "Этот старинный <w>театр</w> находится в самом центре старого города."
                            }

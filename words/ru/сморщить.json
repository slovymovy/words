{
    "word_family": [
        "морщина",
        "морщинистый",
        "морщить",
        "наморщить",
        "сморщиваться",
        "сморщивание"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9c7572eb-0394-37c0-9e9e-8982b0f3d251",
                    "sense_definition": "Собрать кожу в складки (обычно на лбу или носу), выражая удивление, недовольство или раздумье.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "facial_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она брезгливо <сморщила> нос от резкого запаха.",
                            "target_lang_translations": {
                                "en": "She squeamishly <wrinkled> her nose at the sharp smell."
                            }
                        },
                        {
                            "text": "Учитель строго <сморщил> лоб, проверяя сложную задачу.",
                            "target_lang_translations": {
                                "en": "The teacher sternly <knitted> his brow while checking the complex problem."
                            }
                        }
                    ],
                    "synonyms": [
                        "наморщить"
                    ],
                    "antonyms": [
                        "разгладить"
                    ],
                    "common_phrases": [
                        "сморщить нос",
                        "сморщить лоб",
                        "смешно сморщить"
                    ],
                    "target_lang_definitions": {
                        "en": "To gather skin into folds (usually on the forehead or nose), expressing surprise, dissatisfaction, or thought."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wrinkle",
                                "target_lang_sense_clarification": "nose or forehead"
                            },
                            {
                                "target_lang_word": "knit",
                                "target_lang_sense_clarification": "brows or forehead"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90b3a80a-40ec-3393-9d65-257cb5467848",
                    "sense_definition": "Сжать губы или всё лицо в гримасу, часто перед плачем, от боли или обиды.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "facial_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький мальчик обиженно <сморщил> губы и заплакал.",
                            "target_lang_translations": {
                                "en": "The little boy resentfully <puckered> his lips and started to cry."
                            }
                        },
                        {
                            "text": "Пациент <сморщил> лицо от внезапной острой боли.",
                            "target_lang_translations": {
                                "en": "The patient <screwed up> his face from the sudden sharp pain."
                            }
                        }
                    ],
                    "synonyms": [
                        "скривить"
                    ],
                    "antonyms": [
                        "улыбнуться"
                    ],
                    "common_phrases": [
                        "сморщить губы",
                        "жалобно сморщить",
                        "сморщить лицо"
                    ],
                    "target_lang_definitions": {
                        "en": "To squeeze lips or the whole face into a grimace, often before crying, from pain or offense."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pucker",
                                "target_lang_sense_clarification": "lips"
                            },
                            {
                                "target_lang_word": "screw up",
                                "target_lang_sense_clarification": "face or eyes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26107621-c934-3314-bfff-e3edbfe590fa",
                    "sense_definition": "Деформировать поверхность предмета, материала или плода, сделав её неровной и покрытой складками.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_deformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Долгая стирка в горячей воде <сморщила> кожу на руках.",
                            "target_lang_translations": {
                                "en": "Long washing in hot water <wrinkled> the skin on the hands."
                            }
                        },
                        {
                            "text": "Жаркое летнее солнце высушило и <сморщило> упавшие яблоки.",
                            "target_lang_translations": {
                                "en": "The hot summer sun dried out and <shriveled> the fallen apples."
                            }
                        }
                    ],
                    "synonyms": [
                        "смять",
                        "съёжить"
                    ],
                    "antonyms": [
                        "расправить",
                        "разгладить"
                    ],
                    "common_phrases": [
                        "вода сморщила",
                        "сморщить бумагу",
                        "сморщить кожу"
                    ],
                    "target_lang_definitions": {
                        "en": "To deform the surface of an object, material, or fruit, making it uneven and covered in folds."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shrivel",
                                "target_lang_sense_clarification": "fruit or drying out"
                            },
                            {
                                "target_lang_word": "wrinkle",
                                "target_lang_sense_clarification": "skin, fabric, or general surface"
                            },
                            {
                                "target_lang_word": "crinkle",
                                "target_lang_sense_clarification": "paper or foil"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "061c4285-0224-3c36-bed6-86b6c3f403bd",
                    "sense_definition": "Praktyki i wierzenia oparte na przekonaniu o istnieniu sił nadprzyrodzonych, które można kontrolować za pomocą zaklęć i rytuałów",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "supernatural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chrześcijanin nie powinien parać się <magią> i okultyzmem.",
                            "target_lang_translations": {
                                "ru": "Христианин не должен заниматься <магией> и оккультизмом."
                            }
                        },
                        {
                            "text": "W dawnych czasach wierzono, że <magia> może sprowadzić deszcz w czasie suszy.",
                            "target_lang_translations": {
                                "ru": "В давние времена верили, что <магия> может вызвать дождь во время засухи."
                            }
                        }
                    ],
                    "synonyms": [
                        "czary",
                        "gusła"
                    ],
                    "common_phrases": [
                        "czarna magia",
                        "biała magia",
                        "uprawiać magię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Практики и верования, основанные на вере в сверхъестественные силы, которые можно контролировать с помощью заклинаний и ритуалов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магия",
                                "target_lang_sense_clarification": "сверхъестественные силы, обряды"
                            },
                            {
                                "target_lang_word": "колдовство",
                                "target_lang_sense_clarification": "чары, магические действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53ed617b-affc-3a3a-aa3e-28074f411b15",
                    "sense_definition": "Niezwykła siła oddziaływania, tajemniczy urok lub atmosfera, która zachwyca i przyciąga",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Działa na mnie <magia> tego starożytnego miasta, do którego zawsze wracam.",
                            "target_lang_translations": {
                                "ru": "На меня действует <магия> этого древнего города, в который я всегда возвращаюсь."
                            }
                        },
                        {
                            "text": "To właśnie <magia> świąt sprawia, że wszyscy są dla siebie bardziej życzliwi.",
                            "target_lang_translations": {
                                "ru": "Именно <магия> праздников делает всех добрее друг к другу."
                            }
                        }
                    ],
                    "synonyms": [
                        "urok"
                    ],
                    "common_phrases": [
                        "magia kina",
                        "magia świąt",
                        "magia słów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Необычайная сила воздействия, таинственное очарование или атмосфера, которая восхищает и притягивает"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магия",
                                "target_lang_sense_clarification": "перен. притягательная сила"
                            },
                            {
                                "target_lang_word": "волшебство",
                                "target_lang_sense_clarification": "чудесная атмосфера"
                            },
                            {
                                "target_lang_word": "очарование",
                                "target_lang_sense_clarification": "шарм, обаяние"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
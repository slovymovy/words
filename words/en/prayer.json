                },
                {
                    "sense_id": "3fc0af9d-eca8-3a6d-8a9a-13d198f3b3a9",
                    "sense_definition": "The specific words or methods used for praying.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spiritual_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every night before bed, the child recites a short <w>prayer</w>.",
                            "target_lang_translations": {
                                "ru": "Каждую ночь перед сном ребенок читает короткую <w>молитву</w>."
                            }
                        },
                        {
                            "text": "The priest read a <w>prayer</w> from the ancient book.",
                            "target_lang_translations": {
                                "ru": "Священник прочитал <w>молитву</w> из древней книги."
                            }
                        }
                    ],
                    "synonyms": [
                        "orison"
                    ],
                    "common_phrases": [
                        "say a prayer",
                        "Lord's Prayer",
                        "morning prayer"
                    ],
                    "target_lang_definitions": {

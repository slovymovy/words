    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a38cb7d5-631a-3727-b5f2-5e578c83c56c",
                    "sense_definition": "An ethnic group comprising families or clans with a common culture, dialect, and leadership, typically larger than a band but smaller than a nation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ethnic_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The indigenous <w>tribe</w> has lived in the rainforest for centuries.",
                            "target_lang_translations": {
                                "ru": "Туземное <w>племя</w> веками жило в тропических лесах."
                            }
                        },
                        {
                            "text": "Members of the nomadic <w>tribe</w> travel across the desert with their herds.",
                            "target_lang_translations": {
                                "ru": "Члены кочевого <w>племени</w> путешествуют по пустыне со своими стадами."
                            }
                        }
                    ],
                    "synonyms": [
                        "tribus",
                        "tribus"
                    ],
                    "common_phrases": [
                        "indigenous tribe",
                        "nomadic tribe",
                        "native tribe",
                        "hill tribe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Этническая группа, состоящая из семей или кланов с общей культурой, диалектом и руководством, обычно больше, чем община, но меньше, чем нация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "племя",
                                "target_lang_sense_clarification": "этническая группа"
                            },
                            {
                                "target_lang_word": "род",
                                "target_lang_sense_clarification": "группа кровных родственников"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7027e913-4ee1-347d-936f-b61634c33643",
                    "sense_definition": "A socially cohesive group of people who share common interests, characteristics, or values.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She finally found her <w>tribe</w> among the local artists and musicians.",
                            "target_lang_translations": {
                                "ru": "Она наконец нашла <w>своих</w> среди местных художников и музыкантов."
                            }
                        },
                        {
                            "text": "It is important to have a supportive <w>tribe</w> when you are starting a new business.",
                            "target_lang_translations": {
                                "ru": "Важно иметь группу <w>единомышленников</w>, которая поддержит, когда начинаешь новый бизнес."
                            }
                        }
                    ],
                    "common_phrases": [
                        "find your tribe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Социально сплоченная группа людей, разделяющих общие интересы, характеристики или ценности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "единомышленники",
                                "target_lang_sense_clarification": "люди с общими интересами"
                            },
                            {
                                "target_lang_word": "свои",
                                "target_lang_sense_clarification": "близкие по духу люди"
                            },
                            {
                                "target_lang_word": "клан",
                                "target_lang_sense_clarification": "сплоченная группа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49ae251b-fd7d-3101-a52f-f2ed6925bbb7",
                    "sense_definition": "A taxonomic rank in biological classification that is above genus and below family.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "taxonomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The botanist classified the new plant species into a specific <w>tribe</w>.",
                            "target_lang_translations": {
                                "ru": "Ботаник отнес новый вид растений к определенной <w>трибе</w>."
                            }
                        },
                        {
                            "text": "Several related genera are grouped together to form a single <w>tribe</w>.",
                            "target_lang_translations": {
                                "ru": "Несколько родственных родов группируются в одну <w>трибу</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Таксономический ранг в биологической классификации, который находится выше рода и ниже семейства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "триба",
                                "target_lang_sense_clarification": "биологический таксон"
                            },
                            {
                                "target_lang_word": "колено",
                                "target_lang_sense_clarification": "таксономический ранг"
                            }
                        ]
                    }
                }
            ]
        },
        {

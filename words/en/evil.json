    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a948ad9a-b4d7-399e-8096-94fbc341a678",
                    "sense_definition": "Morally bad, cruel, or intending to harm others.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "moral_badness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>evil</w> witch cast a powerful spell on the entire kingdom.",
                            "target_lang_translations": {
                                "ru": "<w>Злая</w> ведьма наложила сильное заклятие на всё королевство."
                            }
                        },
                        {
                            "text": "He realized that the villain had an <w>evil</w> plan to steal the money.",
                            "target_lang_translations": {
                                "ru": "Он понял, что у злодея был <w>коварный</w> план украсть деньги."
                            }
                        },
                        {
                            "text": "She felt an <w>evil</w> presence watching her from the shadows of the room.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала <w>зловещее</w> присутствие, наблюдающее за ней из теней комнаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "wicked",
                        "malicious",
                        "malevolent",
                        "immoral",
                        "sinful",
                        "sinister",
                        "vile",
                        "villainous",
                        "nefarious",
                        "bad"
                    ],
                    "antonyms": [
                        "good",
                        "angelic",
                        "moral",
                        "righteous",
                        "well-intentioned",
                        "benign"
                    ],
                    "common_phrases": [
                        "evil eye",
                        "evil spirit",
                        "pure evil",
                        "evil thoughts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Морально плохой, жестокий или желающий причинить вред другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "злой",
                                "target_lang_sense_clarification": "исполненный недоброжелательности, враждебности"
                            },
                            {
                                "target_lang_word": "порочный",
                                "target_lang_sense_clarification": "склонный к безнравственным поступкам"
                            },
                            {
                                "target_lang_word": "злобный",
                                "target_lang_sense_clarification": "выражающий злобу и агрессию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3deee01-0c1e-33bb-9429-00725f476d9d",
                    "sense_definition": "Causing or threatening sorrow, distress, or bad luck.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "harm_calamity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villagers believed that the black cat was an <w>evil</w> sign.",
                            "target_lang_translations": {
                                "ru": "Деревенские жители верили, что черная кошка — это <w>дурной</w> знак."
                            }
                        },
                        {
                            "text": "They fell upon <w>evil</w> times during the long years of the war.",
                            "target_lang_translations": {
                                "ru": "В долгие годы войны для них настали <w>тяжёлые</w> времена."
                            }
                        }
                    ],
                    "synonyms": [
                        "harmful",
                        "injurious",
                        "detrimental",
                        "deleterious",
                        "pernicious"
                    ],
                    "antonyms": [
                        "benign"
                    ],
                    "common_phrases": [
                        "evil omen",
                        "evil days"
                    ],
                    "target_lang_definitions": {

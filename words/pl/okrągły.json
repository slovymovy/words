                    ],
                    "common_phrases": [
                        "okrągłe zdania",
                        "okrągłe słowa",
                        "okrągłe frazesy"
                    ],
                    "target_lang_definitions": {
                        "ru": "О высказывании: сформулированное гладко, но часто уклончивое или лишенное конкретного содержания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обтекаемый",
                                "target_lang_sense_clarification": "уклончивый (о речи)"

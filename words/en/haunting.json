{
    "word_family": [
        "haunt",
        "haunted",
        "haunter"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b6a98bbf-2d41-357e-99dd-810e63a99b72",
                    "sense_definition": "Beautiful, sad, or frightening in a way that is impossible to forget; remaining in the mind.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotional_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The choir sang a <w>haunting</w> melody that brought tears to everyone's eyes.",
                            "target_lang_translations": {
                                "ru": "Хор пел <w>западающую в душу</w> мелодию, от которой у всех на глаза навернулись слезы."
                            }
                        },
                        {
                            "text": "She captured the <w>haunting</w> beauty of the abandoned city in her photographs.",
                            "target_lang_translations": {
                                "ru": "Она запечатлела на своих фотографиях <w>завораживающую</w> красоту заброшенного города."
                            }
                        }
                    ],
                    "synonyms": [
                        "unforgettable",
                        "poignant",
                        "evocative",
                        "memorable"
                    ],
                    "antonyms": [
                        "forgettable"
                    ],
                    "common_phrases": [
                        "haunting melody",
                        "haunting beauty",
                        "haunting image",
                        "haunting memory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Красивый, грустный или пугающий образ, который невозможно забыть; западающий в память."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "западающий в душу",
                                "target_lang_sense_clarification": "о мелодии или красоте, вызывающей сильные эмоции"
                            },
                            {
                                "target_lang_word": "навязчивый",
                                "target_lang_sense_clarification": "о мыслях или воспоминаниях, которые трудно отогнать"
                            },
                            {
                                "target_lang_word": "завораживающий",
                                "target_lang_sense_clarification": "притягательный и таинственный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3b37af71-773b-3a38-a2dc-70986899ae6b",
                    "sense_definition": "Present participle and gerund of <w>haunt</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The memory of that mistake is still <w>haunting</w> him years later.",
                            "target_lang_translations": {
                                "ru": "Воспоминание об этой ошибке всё ещё <w>преследует</w> его спустя годы."
                            }
                        },
                        {
                            "text": "Locals say that a spirit is <w>haunting</w> the old castle on the hill.",
                            "target_lang_translations": {
                                "ru": "Местные говорят, что дух <w>обитает</w> в старом замке на холме."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола haunt (преследовать, обитать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преследующий",
                                "target_lang_sense_clarification": "не дающий покоя (о мыслях, прошлом)"
                            },
                            {
                                "target_lang_word": "обитающий",
                                "target_lang_sense_clarification": "о призраках или духах в определенном месте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "63c1afe3-7415-367a-a927-d577a197b04b",
                    "sense_definition": "An occurrence where a ghost or spirit is believed to visit or inhabit a place.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "paranormal_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family reported a <w>haunting</w> shortly after moving into the Victorian house.",
                            "target_lang_translations": {
                                "ru": "Семья сообщила о <w>появлении призраков</w> вскоре после переезда в викторианский дом."
                            }
                        },
                        {
                            "text": "Paranormal investigators arrived to document the alleged <w>haunting</w> in the library.",
                            "target_lang_translations": {
                                "ru": "Исследователи паранормальных явлений прибыли, чтобы задокументировать предполагаемое <w>присутствие привидений</w> в библиотеке."
                            }
                        }
                    ],
                    "synonyms": [
                        "visitation",
                        "apparition"
                    ],
                    "common_phrases": [
                        "house haunting",
                        "ghostly haunting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Явление, связанное с присутствием призраков или духов в определенном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "появление призраков",
                                "target_lang_sense_clarification": "феномен присутствия духов"
                            },
                            {
                                "target_lang_word": "присутствие нечистой силы",
                                "target_lang_sense_clarification": "разговорное описание паранормальной активности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
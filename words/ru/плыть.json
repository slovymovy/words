    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d62fd269-fb27-33dd-b110-8e65b16ad427",
                    "sense_definition": "Передвигаться по поверхности воды или в воде в определённом направлении (о людях, животных, судах или предметах)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <плыть> к острову на большой лодке.",
                            "target_lang_translations": {
                                "en": "We decided to <w>sail</w> to the island on a large boat."
                            }
                        },
                        {
                            "text": "Спортсмен быстро <плывёт> к финишу, обгоняя соперников.",
                            "target_lang_translations": {
                                "en": "The athlete <w>swims</w> quickly to the finish line, overtaking rivals."
                            }
                        },
                        {
                            "text": "По широкой реке медленно <плывёт> белый теплоход.",
                            "target_lang_translations": {
                                "en": "A white cruise ship slowly <w>sails</w> down the wide river."
                            }
                        }
                    ],
                    "antonyms": [
                        "тонуть"
                    ],
                    "common_phrases": [
                        "плыть по течению",
                        "плыть против течения",
                        "плыть к берегу",
                        "плыть на корабле",
                        "быстро плыть"
                    ],
                    "target_lang_definitions": {
                        "en": "To move on the surface of the water or in the water in a certain direction (about people, animals, ships, or objects)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swim",
                                "target_lang_sense_clarification": "of people or animals moving themselves through water"
                            },
                            {
                                "target_lang_word": "sail",
                                "target_lang_sense_clarification": "of ships or boats traveling on water"
                            },
                            {
                                "target_lang_word": "float",
                                "target_lang_sense_clarification": "of objects moving with the current"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68b19222-75bd-3a67-bfc2-01a8fc7a2c36",
                    "sense_definition": "Плавно и легко перемещаться в пространстве, обычно по воздуху (об облаках, дыме, запахах, звуках)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_air_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По синему летнему небу медленно <плывут> белые облака.",
                            "target_lang_translations": {
                                "en": "White clouds slowly <w>float</w> across the blue summer sky."
                            }
                        },
                        {
                            "text": "Из открытого окна кухни <плыл> чудесный запах свежего пирога.",
                            "target_lang_translations": {
                                "en": "A wonderful smell of fresh pie <w>wafted</w> from the open kitchen window."
                            }
                        }
                    ],
                    "common_phrases": [
                        "облака плывут",
                        "луна плывёт",
                        "звуки плывут"
                    ],
                    "target_lang_definitions": {
                        "en": "To move smoothly and easily in space, usually through the air (about clouds, smoke, smells, sounds)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "float",
                                "target_lang_sense_clarification": "general smooth movement through air"
                            },
                            {
                                "target_lang_word": "drift",
                                "target_lang_sense_clarification": "to be carried slowly by air currents"
                            },
                            {
                                "target_lang_word": "waft",
                                "target_lang_sense_clarification": "specifically for smells or sounds"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4064a5ce-12e4-3813-bf7d-2c21b8cdc121",
                    "sense_definition": "Казаться нечётким, расплывчатым перед глазами или ощущать головокружение",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "perception_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От сильной усталости буквы в книге начали <плыть> перед глазами.",
                            "target_lang_translations": {
                                "en": "Due to severe fatigue, the letters in the book began to <w>swim</w> before my eyes."
                            }
                        },
                        {
                            "text": "У меня <плывёт> голова, мне нужно срочно присесть и выпить воды.",
                            "target_lang_translations": {
                                "en": "My head is <w>swimming</w>, I need to sit down immediately and drink some water."
                            }
                        }
                    ],
                    "synonyms": [
                        "расплываться"
                    ],
                    "common_phrases": [
                        "всё плывёт перед глазами",
                        "голова плывёт",
                        "картинка плывёт"
                    ],
                    "target_lang_definitions": {
                        "en": "To seem indistinct or blurry before the eyes or to feel dizziness"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swim",
                                "target_lang_sense_clarification": "to feel dizzy or see things spinning"
                            },
                            {
                                "target_lang_word": "blur",
                                "target_lang_sense_clarification": "to become unclear to the vision"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11d6ea17-464c-39c5-acc5-3e5db2fec595",
                    "sense_definition": "Плавно и незаметно проходить, протекать (о времени, жизни, событиях)",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "time_passing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дни в деревне <плыли> медленно и спокойно, один за другим.",
                            "target_lang_translations": {
                                "en": "The days in the village <w>drifted</w> by slowly and calmly, one after another."
                            }
                        },
                        {
                            "text": "Годы <плывут>, и мы постепенно меняемся, сами того не замечая.",
                            "target_lang_translations": {
                                "en": "The years <w>float</w> by, and we gradually change without even noticing."
                            }
                        }
                    ],
                    "common_phrases": [
                        "время плывёт",
                        "дни плывут",
                        "годы плывут"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "poetic or literary context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To pass or flow smoothly and imperceptibly (about time, life, events)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drift by",
                                "target_lang_sense_clarification": "of time passing slowly or lazily"
                            },
                            {
                                "target_lang_word": "flow",
                                "target_lang_sense_clarification": "continuous, smooth passage of time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea090b88-795e-38a9-9f77-22aded9ca3aa",
                    "sense_definition": "Терять форму, становиться мягким и растекаться от высокой температуры или влаги",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В такой сильной жаре макияж начинает <плыть> уже через час.",
                            "target_lang_translations": {
                                "en": "In such strong heat, makeup starts to <w>run</w> after just an hour."
                            }
                        },
                        {
                            "text": "Восковая свеча горела неровно и сильно <плыла>, капая на стол.",
                            "target_lang_translations": {
                                "en": "The wax candle was burning unevenly and <w>melting</w> heavily, dripping onto the table."
                            }
                        }
                    ],
                    "synonyms": [
                        "растекаться",
                        "плавиться",
                        "оплывать"
                    ],
                    "common_phrases": [
                        "свеча плывёт",
                        "тушь плывёт",
                        "макияж плывёт"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To lose shape, become soft and spread out due to high temperature or moisture"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "of makeup or dye spreading due to moisture/heat"
                            },
                            {
                                "target_lang_word": "melt",
                                "target_lang_sense_clarification": "of solids losing shape due to heat"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
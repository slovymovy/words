    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "953a4194-df4c-3048-a1a8-b2c24ce04efe",
                    "sense_definition": "Przebyć określony dystans lub trasę, kierując pojazdem lub podróżując nim jako pasażer.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W te wakacje planujemy <przejechać> samochodem całe wybrzeże, zatrzymując się w małych miasteczkach.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы планируем <проехать> на машине всё побережье, останавливаясь в маленьких городках."
                            }
                        },
                        {
                            "text": "Kierowca ciężarówki musiał <przejechać> ponad tysiąc kilometrów bez dłuższego odpoczynku.",
                            "target_lang_translations": {
                                "ru": "Водитель грузовика должен был <проехать> более тысячи километров без длительного отдыха."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozjechać"
                    ],
                    "common_phrases": [
                        "przejechać cały kraj",
                        "przejechać sto kilometrów",
                        "przejechać przez miasto",
                        "przejechać trasę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преодолеть определенное расстояние или маршрут, управляя транспортным средством или находясь в нем в качестве пассажира."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проехать",
                                "target_lang_sense_clarification": "преодолеть расстояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "776880a0-747d-3b09-b208-a56dcc8cf4f4",
                    "sense_definition": "Minąć wyznaczone miejsce zatrzymania się lub cel podróży, nie zatrzymując się tam (często przez pomyłkę).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zaczytałem się w ciekawej książce i niestety <przejechałem> mój przystanek autobusowy.",
                            "target_lang_translations": {
                                "ru": "Я зачитался интересной книгой и, к сожалению, <проехал> свою автобусную остановку."
                            }
                        },
                        {
                            "text": "Jeśli <przejedziesz> ten zjazd z autostrady, będziesz musiał zawracać na kolejnym węźle.",
                            "target_lang_translations": {
                                "ru": "Если ты <проедешь> этот съезд с автострады, тебе придется разворачиваться на следующей развязке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przejechać przystanek",
                        "przejechać zjazd",
                        "przejechać skrzyżowanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Миновать место остановки или назначения, не остановившись там (часто по ошибке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проехать",
                                "target_lang_sense_clarification": "пропустить остановку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8015d5d3-e802-3916-a4b7-fb3f7c871a68",
                    "sense_definition": "Odbyć krótką podróż pojazdem dla przyjemności lub w celu jego przetestowania (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W niedzielne popołudnie lubimy <przejechać> się rowerami po okolicznym lesie.",
                            "target_lang_translations": {
                                "ru": "В воскресный полдень мы любим <прокатиться> на велосипедах по окрестному лесу."
                            }
                        },
                        {
                            "text": "Czy mogę <przejechać> się twoim nowym samochodem, żeby sprawdzić jak działa silnik?",
                            "target_lang_translations": {
                                "ru": "Могу я <проехаться> на твоей новой машине, чтобы проверить, как работает двигатель?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "przejechać się autem",
                        "przejechać się rowerem",
                        "przejechać się po mieście"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершить короткую поездку на транспорте ради удовольствия или для проверки (используется с возвратной частицей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокатиться",
                                "target_lang_sense_clarification": "ради удовольствия"
                            },
                            {
                                "target_lang_word": "проехаться",
                                "target_lang_sense_clarification": "совершить поездку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e289587b-4c53-38ce-9643-864f117e1809",
                    "sense_definition": "Potrącić kogoś lub najechać na coś pojazdem, powodując obrażenia, śmierć lub zniszczenie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "harm_accident",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca nie zauważył zwierzęcia na drodze i niestety przypadkowo je <przejechał>.",
                            "target_lang_translations": {
                                "ru": "Водитель не заметил животное на дороге и, к сожалению, случайно его <переехал>."
                            }
                        },
                        {
                            "text": "To bardzo niebezpieczne przejście dla pieszych, łatwo tu kogoś <przejechać> po zmroku.",
                            "target_lang_translations": {
                                "ru": "Это очень опасный пешеходный переход, здесь легко кого-то <сбить> в темноте."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozjechać"
                    ],
                    "common_phrases": [
                        "przejechać pieszego",
                        "przejechać psa",
                        "prawie przejechać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбить кого-то или наехать на что-то транспортным средством, причинив вред или смерть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переехать",
                                "target_lang_sense_clarification": "наехать колесами"
                            },
                            {
                                "target_lang_word": "задавить",
                                "target_lang_sense_clarification": "насмерть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44d41fd1-ee18-365e-af3e-e35831965ed7",
                    "sense_definition": "Doznać bolesnego rozczarowania w stosunku do kogoś lub czegoś (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotion_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mocno <przejechałem> się na moim wspólniku, który okazał się osobą nieuczciwą.",
                            "target_lang_translations": {
                                "ru": "Я сильно <обжёгся> на моем партнере, который оказался нечестным человеком."
                            }
                        },
                        {
                            "text": "Wielu niedoświadczonych inwestorów <przejechało> się na obietnicach szybkiego i łatwego zysku.",
                            "target_lang_translations": {
                                "ru": "Многие неопытные инвесторы <обожглись> на обещаниях быстрой и легкой прибыли."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawieść się"
                    ],
                    "common_phrases": [
                        "przejechać się na kimś",
                        "przejechać się na ludziach",
                        "przejechać się na interesach"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "użycie przenośne"
                        },
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытать сильное разочарование в ком-то или чем-то; потерпеть неудачу (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обжечься",
                                "target_lang_sense_clarification": "разочароваться"
                            },
                            {
                                "target_lang_word": "прогореть",
                                "target_lang_sense_clarification": "потерпеть неудачу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
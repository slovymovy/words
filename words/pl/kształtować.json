                {
                    "sense_id": "4e266f2f-8ab2-34e8-85ae-788ffe68516d",
                    "sense_definition": "Nadawać przedmiotom lub materiałom określony wygląd zewnętrzny i formę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_shaping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Artysta zaczął delikatnie <kształtować> miękką glinę w piękny wazon.",
                            "target_lang_translations": {
                                "ru": "Художник начал осторожно <формировать> из мягкой глины красивую вазу."
                            }
                        },
                        {
                            "text": "Regularne ćwiczenia na siłowni pozwalają <kształtować> sylwetkę i budować mięśnie.",
                            "target_lang_translations": {
                                "ru": "Регулярные упражнения в спортзале позволяют <формировать> фигуру и наращивать мышцы."
                            }
                        }
                    ],
                    "synonyms": [
                        "formować",
                        "modelować",
                        "lepić"
                    ],
                    "antonyms": [
                        "deformować",
                        "niszczyć",
                        "zniekształcać"
                    ],
                    "common_phrases": [
                        "kształtować sylwetkę",
                        "kształtować teren",
                        "kształtować krajobraz",
                        "kształtować drewno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать предметам или материалам определенный внешний вид и форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формировать",
                                "target_lang_sense_clarification": "создавать форму чего-либо"
                            },
                            {
                                "target_lang_word": "придавать форму",
                                "target_lang_sense_clarification": "обрабатывать материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "681b88cb-4366-3e7f-b0e9-cd9d63b0bc3b",
                    "sense_definition": "Mieć decydujący wpływ na rozwój kogoś lub czegoś, np. osobowości, poglądów lub sytuacji.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice i nauczyciele wspólnie starają się <kształtować> charakter młodego człowieka.",
                            "target_lang_translations": {
                                "ru": "Родители и учителя вместе стараются <формировать> характер молодого человека."
                            }
                        },
                        {
                            "text": "Media mają ogromną moc, by <kształtować> opinię publiczną w ważnych sprawach.",
                            "target_lang_translations": {
                                "ru": "СМИ обладают огромной властью, чтобы <формировать> общественное мнение по важным вопросам."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozwijać",
                        "wyrabiać",
                        "ukierunkowywać",
                        "wpływać"
                    ],
                    "common_phrases": [
                        "kształtować charakter",
                        "kształtować opinię",
                        "kształtować przyszłość",
                        "kształtować wizerunek",
                        "kształtować świadomość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывать решающее влияние на развитие кого-либо или чего-либо, например, личности, взглядов или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формировать",
                                "target_lang_sense_clarification": "влиять на развитие характера или мнения"
                            },
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "задавать направление развития"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f810deb4-7d22-37fd-9846-932526e18895",
                    "sense_definition": "Proces powstawania czegoś, przybierania ostatecznej formy lub wyglądu (używane z zaimkiem \"się\").",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_becoming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa rzeczywistość polityczna w kraju zaczęła <kształtować> się zaraz po wyborach.",
                            "target_lang_translations": {
                                "ru": "Новая политическая реальность в стране начала <складываться> сразу после выборов."
                            }
                        },
                        {
                            "text": "Jego osobowość zaczęła <kształtować> się już we wczesnym dzieciństwie.",
                            "target_lang_translations": {
                                "ru": "Его личность начала <формироваться> еще в раннем детстве."
                            }
                        }
                    ],
                    "synonyms": [
                        "formować się",
                        "tworzyć się",
                        "powstawać",
                        "krystalizować się"
                    ],
                    "antonyms": [
                        "zanikać",
                        "rozpadać się"
                    ],
                    "common_phrases": [

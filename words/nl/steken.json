                {
                    "sense_id": "11c3c25a-3af4-300e-b1a9-5845e154d487",
                    "sense_definition": "Iets ergens in plaatsen, schuiven of doen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "insert",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <stak> de sleutel voorzichtig in het slot van de deur.",
                            "target_lang_translations": {
                                "en": "He carefully <w>put</w> the key in the door lock.",
                                "ru": "Он осторожно <вставил> ключ в дверной замок."
                            }
                        },
                        {
                            "text": "Je mag je vingers nooit in het stopcontact <steken>.",
                            "target_lang_translations": {
                                "en": "You should never <w>stick</w> your fingers in the socket.",
                                "ru": "Никогда нельзя <совать> пальцы в розетку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in je zak steken",
                        "in het stopcontact steken",
                        "de sleutel in het slot steken"
                    ],
                    "target_lang_definitions": {
                        "en": "To place, slide, or put something into something.",
                        "ru": "Помещать, вставлять или засовывать что-либо куда-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put",
                                "target_lang_sense_clarification": "general"
                            },
                            {

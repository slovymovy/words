            "senses": [
                {
                    "sense_id": "b8ced31a-22b7-3800-a1cf-1d81885b31a4",
                    "sense_definition": "Aan de zijkant van of evenwijdig aan iets.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij wandelden rustig <langs> de rivier.",
                            "target_lang_translations": {
                                "en": "We walked quietly <along> the river.",
                                "ru": "Мы спокойно гуляли <w>вдоль</w> реки."
                            }

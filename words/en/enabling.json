    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "345cdc24-9723-3485-aa61-db971c5d5bab",
                    "sense_definition": "Present participle and gerund of <w>enable</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new software is <w>enabling</w> our team to work much faster than before.",
                            "target_lang_translations": {
                                "ru": "Новое программное обеспечение <w>позволяет</w> нашей команде работать намного быстрее, чем раньше."
                            }
                        },
                        {
                            "text": "By <w>enabling</w> location services, you allow the app to find nearby restaurants.",
                            "target_lang_translations": {
                                "ru": "<w>Включив</w> службы геолокации, вы разрешаете приложению находить ближайшие рестораны."
                            }
                        }
                    ],
                    "synonyms": [
                        "allowing",
                        "permitting",
                        "empowering",
                        "facilitating"
                    ],
                    "antonyms": [
                        "preventing",
                        "disabling",
                        "hindering",
                        "blocking"
                    ],
                    "common_phrases": [
                        "enabling environment",
                        "enabling technology",
                        "enabling factor"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола enable (давать возможность, включать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давать возможность",
                                "target_lang_sense_clarification": "делать что-либо возможным для кого-то"
                            },
                            {
                                "target_lang_word": "позволять",
                                "target_lang_sense_clarification": "разрешать или создавать условия для действия"
                            },
                            {
                                "target_lang_word": "активировать",
                                "target_lang_sense_clarification": "включать функцию или настройку (в техническом контексте)"
                            },
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "приводить в действие механизм или опцию"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ceabfb9-2974-3389-b642-2bc4f5674525",
                    "sense_definition": "An act or an instance of making something possible or allowing it to happen, often used in the context of supporting negative behavior.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The constant <w>enabling</w> of his addiction by his family only made the situation worse.",
                            "target_lang_translations": {
                                "ru": "Постоянное <w>потворство</w> его зависимости со стороны семьи только ухудшило ситуацию."
                            }
                        },
                        {
                            "text": "We must stop the <w>enabling</w> of corruption within the organization immediately.",
                            "target_lang_translations": {
                                "ru": "Мы должны немедленно прекратить <w>попустительство</w> коррупции внутри организации."
                            }
                        }
                    ],
                    "synonyms": [
                        "facilitation",
                        "empowerment",
                        "validation"
                    ],
                    "antonyms": [
                        "prevention",
                        "inhibition",
                        "prohibition"
                    ],
                    "common_phrases": [
                        "enabling of",
                        "pattern of enabling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, делающее что-либо возможным; часто используется в психологии как поощрение негативного поведения или зависимостей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потворство",
                                "target_lang_sense_clarification": "содействие чьим-либо слабостям или вредным привычкам"
                            },
                            {
                                "target_lang_word": "попустительство",
                                "target_lang_sense_clarification": "невмешательство в нежелательные или вредные действия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3360426e-2c43-3d0b-a477-67d34e3ae75a",
                    "sense_definition": "Густо падать (о снеге) или выходить клубами (о дыме, паре).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mass_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За окном уже второй день без остановки <w>валит</w> снег.",
                            "target_lang_translations": {
                                "en": "Snow has been <w>falling heavily</w> outside non-stop for two days."
                            }
                        },
                        {
                            "text": "Из трубы завода <w>валит</w> густой чёрный дым.",
                            "target_lang_translations": {
                                "en": "Thick black smoke is <w>pouring</w> from the factory chimney."
                            }
                        }
                    ],
                    "synonyms": [
                        "идти"
                    ],
                    "common_phrases": [
                        "снег валит",
                        "дым валит",
                        "пар валит"
                    ],
                    "target_lang_definitions": {
                        "en": "To fall heavily (of snow) or pour out in thick clouds (of smoke or steam)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pour",
                                "target_lang_sense_clarification": "of smoke or steam"
                            },
                            {
                                "target_lang_word": "billow",
                                "target_lang_sense_clarification": "of smoke"
                            },
                            {
                                "target_lang_word": "fall heavily",
                                "target_lang_sense_clarification": "of snow"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fd33211-7470-3706-99f8-1e5c9e8fbbda",
                    "sense_definition": "Быстро уходить или уезжать, покидать место (обычно поспешно или вынужденно).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам пора <w>валить</w> отсюда, пока не начались проблемы.",
                            "target_lang_translations": {
                                "en": "It's time for us to <w>get out</w> of here before problems start."
                            }
                        },
                        {
                            "text": "Он решил <w>валить</w> домой, потому что вечеринка стала скучной.",
                            "target_lang_translations": {
                                "en": "He decided to <w>bail</w> home because the party got boring."
                            }
                        }
                    ],
                    "synonyms": [
                        "уходить",
                        "уезжать",
                        "уматывать",
                        "убираться"
                    ],
                    "common_phrases": [
                        "пора валить",
                        "надо валить",
                        "валить отсюда"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To leave a place quickly, often hurriedly or to escape a situation (slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get out",
                                "target_lang_sense_clarification": "to leave quickly"
                            },
                            {
                                "target_lang_word": "bail",
                                "target_lang_sense_clarification": "slang, to leave a situation"
                            },
                            {
                                "target_lang_word": "scram",
                                "target_lang_sense_clarification": "slang, imperative or narrative"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51aed598-8a70-33a5-8a18-88384b890512",
                    "sense_definition": "Заставлять падать, опрокидывать на землю; рубить (деревья).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cause_fall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный ураган начал <w>валить</w> старые деревья в парке.",
                            "target_lang_translations": {
                                "en": "A strong hurricane began to <w>topple</w> old trees in the park."
                            }
                        },
                        {
                            "text": "Усталость после долгой работы буквально <w>валит</w> меня с ног.",
                            "target_lang_translations": {
                                "en": "Fatigue after long work is literally <w>knocking</w> me off my feet."
                            }
                        }
                    ],
                    "synonyms": [
                        "сваливать",
                        "заваливать"
                    ],
                    "common_phrases": [
                        "валить лес",
                        "валить деревья",
                        "валить с ног"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause to fall, knock over, or cut down (trees)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "knock down",
                                "target_lang_sense_clarification": "to cause to fall"
                            },
                            {
                                "target_lang_word": "fell",
                                "target_lang_sense_clarification": "trees"
                            },
                            {
                                "target_lang_word": "topple",
                                "target_lang_sense_clarification": "to overturn"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d0a1837-f5f1-3b3d-a6f9-d889a3713625",
                    "sense_definition": "Перекладывать вину или ответственность на другого человека.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не нужно <w>валить</w> свою вину на коллег, если ты сам ошибся.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>shift</w> your blame onto colleagues if you made the mistake yourself."
                            }
                        },
                        {
                            "text": "Он привык <w>валить</w> ответственность за неудачи на обстоятельства.",
                            "target_lang_translations": {
                                "en": "He is used to <w>dumping</w> responsibility for failures on circumstances."
                            }
                        }
                    ],
                    "synonyms": [
                        "сваливать",
                        "перекладывать"
                    ],
                    "common_phrases": [
                        "валить вину",
                        "валить на другого",
                        "валить с больной головы"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To shift blame or responsibility onto someone else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shift",
                                "target_lang_sense_clarification": "blame or responsibility"
                            },
                            {
                                "target_lang_word": "dump",
                                "target_lang_sense_clarification": "responsibility (colloquial)"
                            },
                            {
                                "target_lang_word": "pin",
                                "target_lang_sense_clarification": "blame"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1de0571-5d82-3db7-89b2-6791561e031d",
                    "sense_definition": "Беспорядочно складывать или сбрасывать что-либо в кучу.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cause_fall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие начали <w>валить</w> строительный мусор в огромный контейнер.",
                            "target_lang_translations": {
                                "en": "The workers started to <w>dump</w> construction debris into a huge container."
                            }
                        },
                        {
                            "text": "Не надо <w>валить</w> всю одежду на стул, лучше повесь её в шкаф.",
                            "target_lang_translations": {
                                "en": "Don't <w>pile</w> all the clothes on the chair, better hang them in the closet."
                            }
                        }
                    ],
                    "synonyms": [
                        "сваливать",
                        "заваливать"
                    ],
                    "common_phrases": [
                        "валить в кучу",
                        "валить всё подряд"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To dump or pile things up in a messy, indiscriminate way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dump",
                                "target_lang_sense_clarification": "to throw down carelessly"
                            },
                            {
                                "target_lang_word": "pile",
                                "target_lang_sense_clarification": "to heap up"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9970e708-a636-3acf-abf8-2168e51f5477",
                    "sense_definition": "Идти или двигаться большой массой (о людях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mass_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В выходные дни народ <w>валит</w> за город на отдых.",
                            "target_lang_translations": {
                                "en": "On weekends, people <w>flock</w> out of town to relax."
                            }
                        },
                        {
                            "text": "После окончания концерта зрители начали <w>валить</w> к выходу.",
                            "target_lang_translations": {
                                "en": "After the concert ended, the audience began to <w>pour</w> towards the exit."
                            }
                        }
                    ],
                    "synonyms": [
                        "идти",
                        "двигать"
                    ],
                    "common_phrases": [
                        "народ валит",
                        "толпа валит"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To move in large numbers (of people)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flock",
                                "target_lang_sense_clarification": "move in large numbers"
                            },
                            {
                                "target_lang_word": "pour",
                                "target_lang_sense_clarification": "move continuously in numbers"
                            }
                        ]
                    }
                }

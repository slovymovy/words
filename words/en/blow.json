                {
                    "sense_id": "cec78c42-5b28-3819-9fd6-4f03a9e2bb42",
                    "sense_definition": "Cocaine.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drug",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police found a bag of <w>blow</w> in the suspect's car.",
                            "target_lang_translations": {
                                "ru": "Полиция нашла пакет с <w>коксом</w> в машине подозреваемого."

                    "synonyms": [
                        "grand"
                    ],
                    "antonyms": [
                        "tiny"
                    ],
                    "common_phrases": [
                        "great deal",
                        "great number",
                        "great many",
                        "great care"
                    ],
                    "target_lang_definitions": {

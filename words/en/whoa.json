    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e569ceed-df4b-3c73-96e2-d71af7f0ac00",
                    "sense_definition": "Used to command a horse to stop or slow down, or informally to tell a person to wait or calm down.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stop_command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rider pulled back on the reins and shouted <w>whoa</w> to the galloping horse."
                        },
                        {
                            "text": "<w>Whoa</w> there, you are walking way too fast for me to keep up with you."
                        }
                    ],
                    "synonyms": [
                        "stop",
                        "halt",
                        "wait",
                        "hold on"
                    ],
                    "antonyms": [
                        "giddyup",
                        "giddap"
                    ],
                    "common_phrases": [
                        "whoa there",
                        "whoa Nelly"
                    ]
                },
                {
                    "sense_id": "dea964a7-d37f-3088-b95a-8250daece38d",
                    "sense_definition": "Used to express sudden surprise, amazement, or disbelief.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Whoa</w>, I did not expect to see such a beautiful view from this window."
                        },
                        {
                            "text": "When he told me the price of the car, I just said <w>whoa</w>."
                        }
                    ],
                    "synonyms": [
                        "wow",
                        "gosh"
                    ],
                    "common_phrases": [
                        "like whoa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "7b98054d-8dca-3b3a-9100-6c1c50e51f2f",
                    "sense_definition": "Used as a rhythmic or melodic sound without specific meaning in song lyrics.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd sang along loudly with the <w>whoa</w> part of the chorus."
                        },
                        {
                            "text": "Many pop songs use <w>whoa</w> to fill the melody when there are no lyrics."
                        }
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4c66a110-3999-32ba-b87a-7a0eb288fb83",
                    "sense_definition": "To call out the command \"whoa\" to an animal to make it stop or slow down.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "stop_command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer was <w>whoaing</w> the oxen as they approached the edge of the field."
                        },
                        {
                            "text": "She <w>whoaed</w> her pony gently before it reached the open gate."
                        }
                    ]
                }
            ]
        }
    ]

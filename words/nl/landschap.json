                {
                    "sense_id": "05f67361-7b8f-3198-9658-9fbc23673627",
                    "sense_definition": "Hoe een bepaald gebied eruitziet, met name wat betreft natuur, geologie en bebouwing.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "scenery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de treinreis genoten we van het afwisselende <w>landschap</w>.",
                            "target_lang_translations": {
                                "ru": "Во время поездки на поезде мы наслаждались разнообразным <w>пейзажем</w>."
                            }
                        },
                        {
                            "text": "Nederland staat bekend om zijn vlakke <w>landschap</w> met veel water en molens.",
                            "target_lang_translations": {
                                "ru": "Нидерланды известны своим плоским <w>ландшафтом</w> с обилием воды и мельниц."
                            }
                        }
                    ],
                    "synonyms": [
                        "omgeving",
                        "streek",
                        "decor",
                        "natuur"
                    ],
                    "common_phrases": [
                        "heuvelachtig landschap",
                        "vlak landschap",
                        "stedelijk landschap",
                        "beschermd landschap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешний вид определённой местности, особенно в отношении природы, геологии и застройки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пейзаж",
                                "target_lang_sense_clarification": "вид природы"
                            },
                            {
                                "target_lang_word": "ландшафт",
                                "target_lang_sense_clarification": "географическая характеристика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85146965-7704-3214-b81a-4d2183d7e800",
                    "sense_definition": "Een schilderij of afbeelding met de natuur of een omgeving als hoofdonderwerp.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het museum hangt een prachtig <w>landschap</w> van een beroemde schilder.",
                            "target_lang_translations": {
                                "ru": "В музее висит прекрасный <w>пейзаж</w> известного художника."
                            }
                        },
                        {
                            "text": "De kunstenaar maakte een gedetailleerd <w>landschap</w> van de Franse kust.",
                            "target_lang_translations": {
                                "ru": "Художник создал детальный <w>пейзаж</w> французского побережья."
                            }
                        }
                    ],
                    "synonyms": [
                        "landschapsschilderij"
                    ],
                    "antonyms": [
                        "portret",
                        "stilleven"
                    ],
                    "common_phrases": [
                        "geschilderd landschap",
                        "ingelijst landschap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Картина или изображение, основным предметом которого является природа или окружающая среда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пейзаж",
                                "target_lang_sense_clarification": "жанр живописи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1229ad1e-456c-3e27-899c-6646a9067cd4",
                    "sense_definition": "Een begrensde regio of gebied, vaak in een bestuurlijke, historische of overdrachtelijke context.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "region",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het politieke <w>landschap</w> is de laatste jaren sterk veranderd door nieuwe partijen.",
                            "target_lang_translations": {
                                "ru": "Политический <w>ландшафт</w> сильно изменился за последние годы из-за появления новых партий."
                            }
                        },
                        {
                            "text": "Dit historische <w>landschap</w> had vroeger zijn eigen wetten en bestuur.",
                            "target_lang_translations": {
                                "ru": "Этот исторический <w>регион</w> раньше имел свои собственные законы и управление."
                            }
                        }
                    ],
                    "synonyms": [
                        "gewest",
                        "regio",
                        "gebied"
                    ],
                    "common_phrases": [
                        "politiek landschap",
                        "media-landschap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограниченный регион или область, часто в административном, историческом или переносном контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ландшафт",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "обстановка",
                                "target_lang_sense_clarification": "ситуация"
                            }
                        ]
                    }
                }
            ]

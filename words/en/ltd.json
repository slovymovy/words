    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "91373b73-6dec-38ae-bdbd-ce5c216bf533",
                    "sense_definition": "Abbreviation of <w>limited</w>; used after a company name to indicate that it is a private limited company.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The contract was signed by the director of Smith & Co <w>Ltd</w>.",
                            "target_lang_translations": {
                                "ru": "Контракт был подписан директором «Смит энд Ко <w>Лтд</w>»."
                            }
                        },
                        {
                            "text": "She works for a software firm called Tech Solutions <w>Ltd</w>.",
                            "target_lang_translations": {
                                "ru": "Она работает в фирме по разработке программного обеспечения под названием «Тек Солюшнс <w>Лтд</w>»."
                            }
                        }
                    ],
                    "synonyms": [
                        "Inc.",
                        "LLC"
                    ],
                    "common_phrases": [
                        "Co Ltd",
                        "Pty Ltd",
                        "private Ltd"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «limited»; используется после названия компании для обозначения общества с ограниченной ответственностью (ООО)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ООО",
                                "target_lang_sense_clarification": "общество с ограниченной ответственностью"
                            },
                            {
                                "target_lang_word": "Лтд",
                                "target_lang_sense_clarification": "транслитерация в названиях иностранных компаний"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ad6430f0-4c87-33b0-82f4-c0f93b013523",
                    "sense_definition": "Initialism of <w>long-term</w> <w>disability</w>; insurance coverage that protects an employee from loss of income during a prolonged illness or injury.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He filed a claim for <w>LTD</w> benefits after his accident.",
                            "target_lang_translations": {
                                "ru": "Он подал заявление на получение пособия по <w>длительной нетрудоспособности</w> после несчастного случая."
                            }
                        },
                        {
                            "text": "The company's <w>LTD</w> plan pays sixty percent of the employee's salary.",
                            "target_lang_translations": {
                                "ru": "План страхования от <w>длительной нетрудоспособности</w> в компании выплачивает шестьдесят процентов от зарплаты сотрудника."
                            }
                        }
                    ],
                    "common_phrases": [
                        "LTD benefits",
                        "LTD insurance",
                        "LTD claim"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от «long-term disability»; страховое покрытие, защищающее сотрудника от потери дохода во время длительной болезни или травмы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "длительная нетрудоспособность",
                                "target_lang_sense_clarification": "страховой термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "brotherhood",
        "brotherly",
        "brotherliness"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3016d6ab-8187-388a-925c-aba681144049",
                    "sense_definition": "A male person who has the same parents as another person.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "family_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>brother</w> is two years older than me.",
                            "target_lang_translations": {
                                "ru": "Мой <w>брат</w> на два года старше меня."
                            }
                        },
                        {
                            "text": "She has two <w>brothers</w> and one sister.",
                            "target_lang_translations": {
                                "ru": "У неё два <w>брата</w> и одна сестра."
                            }
                        }
                    ],
                    "synonyms": [
                        "Bro"
                    ],
                    "antonyms": [
                        "sister"
                    ],
                    "common_phrases": [
                        "older brother",
                        "younger brother",
                        "big brother",
                        "little brother"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина или мальчик по отношению к другим детям тех же родителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брат",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "братишка",
                                "target_lang_sense_clarification": "разговорное, ласковое"
                            },
                            {
                                "target_lang_word": "братец",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca97c79b-d02c-318b-bee1-8b46762d11ae",
                    "sense_definition": "A male member of a religious community, trade union, or fraternal organization.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>brothers</w> gathered in the chapel for morning prayer.",
                            "target_lang_translations": {
                                "ru": "<w>Братья</w> собрались в часовне на утреннюю молитву."
                            }
                        },
                        {
                            "text": "He called upon his union <w>brothers</w> to support the strike.",
                            "target_lang_translations": {
                                "ru": "Он призвал своих <w>братьев</w> по профсоюзу поддержать забастовку."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член религиозной общины, профсоюза или братства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брат",
                                "target_lang_sense_clarification": "член сообщества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f85bdd8-c4ec-3f57-b84f-00da8e62f7ab",
                    "sense_definition": "A familiar form of address to a man.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "address_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Excuse me, <w>brother</w>, can you tell me the time?",
                            "target_lang_translations": {
                                "ru": "Извини, <w>брат</w>, не подскажешь время?"
                            }
                        },
                        {
                            "text": "Listen, <w>brother</w>, I don't want any trouble.",
                            "target_lang_translations": {
                                "ru": "Слушай, <w>брат</w>, я не хочу неприятностей."
                            }
                        }
                    ],
                    "synonyms": [
                        "Bro"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to address a man"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамильярное или дружеское обращение к мужчине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брат",
                                "target_lang_sense_clarification": "дружеское обращение"
                            },
                            {
                                "target_lang_word": "братан",
                                "target_lang_sense_clarification": "слэнг, панибратское"
                            },
                            {
                                "target_lang_word": "братишка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9d3cdb2-349f-3058-921f-b48f932be42a",
                    "sense_definition": "A man who shares a common cause, situation, or strong bond with others.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are <w>brothers</w> in arms fighting for the same goal.",
                            "target_lang_translations": {
                                "ru": "Мы <w>братья</w> по оружию, сражающиеся за одну цель."
                            }
                        },
                        {
                            "text": "Despite our differences, we are all <w>brothers</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на наши различия, мы все <w>братья</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brothers in arms",
                        "band of brothers"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, объединенный с другими общим делом, ситуацией или крепкими узами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брат",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a806397-afbb-3fa5-9f80-2c60683d2c6a",
                    "sense_definition": "A title of respect used before the name of a male member of a religious or fraternal order.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "title",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "Please welcome <w>Brother</w> Smith to the podium.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поприветствуйте <w>брата</w> Смита на трибуне."
                            }
                        },
                        {
                            "text": "At the monastery, <w>Brother</w> Stephen supervises the kitchen.",
                            "target_lang_translations": {
                                "ru": "В монастыре <w>брат</w> Стивен руководит кухней."
                            }
                        }
                    ],
                    "synonyms": [
                        "Br."
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул, используемый перед именем члена религиозного или братского ордена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брат",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc1c0004-3f35-3928-9d44-7f6a6f8f52c4",
                    "sense_definition": "A black man, often used as a term of solidarity within the black community.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "ethnicity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a <w>brother</w> who truly understands our history.",
                            "target_lang_translations": {
                                "ru": "Он <w>брат</w>, который по-настоящему понимает нашу историю."
                            }
                        },
                        {
                            "text": "The community came together to support a <w>brother</w> in need.",
                            "target_lang_translations": {
                                "ru": "Община объединилась, чтобы поддержать <w>брата</w> в беде."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "African American Vernacular English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Темнокожий мужчина (термин, выражающий солидарность внутри сообщества)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брат",
                                "target_lang_sense_clarification": "в контексте афроамериканской культуры"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "0a955b3f-5eab-378f-8926-e062f7e2b84a",
                    "sense_definition": "Used to express annoyance, frustration, or surprise.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oh, <w>brother</w>! I forgot my keys again.",
                            "target_lang_translations": {
                                "ru": "О <w>господи</w>! Я снова забыл ключи."
                            }
                        },
                        {
                            "text": "We have to work overtime this weekend? Oh, <w>brother</w>!",
                            "target_lang_translations": {
                                "ru": "Нам придется работать сверхурочно в эти выходные? О <w>господи</w>!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "oh brother"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "expressing exasperation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения раздражения, разочарования или удивления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о господи",
                                "target_lang_sense_clarification": "выражение досады"
                            },
                            {
                                "target_lang_word": "ну и ну",
                                "target_lang_sense_clarification": "выражение удивления или разочарования"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "45b2517c-a927-3fb5-9586-c82b54d67f74",
                    "sense_definition": "To treat someone as a brother.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>brothered</w> his teammates with great affection and loyalty.",
                            "target_lang_translations": {
                                "ru": "Он <w>относился как к братьям</w> к своим товарищам по команде с большой любовью и преданностью."
                            }
                        },
                        {
                            "text": "They <w>brothered</w> him into their secret society.",
                            "target_lang_translations": {
                                "ru": "Они <w>приняли</w> его <w>как брата</w> в свое тайное общество."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относиться к кому-либо как к брату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "относиться как к брату",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
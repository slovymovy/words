{
    "word_family": [
        "appropriation",
        "appropriateness",
        "appropriately",
        "inappropriate",
        "misappropriate",
        "misappropriation"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b6e24e00-5204-30a6-a964-39305eeaa0ec",
                    "sense_definition": "Suitable, acceptable, or correct for the particular circumstances or social situation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "suitability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is not <w>appropriate</w> to wear casual clothes to a formal wedding.",
                            "target_lang_translations": {
                                "ru": "Не <w>уместно</w> приходить в повседневной одежде на официальную свадьбу."
                            }
                        },
                        {
                            "text": "The teacher took <w>appropriate</w> action when she realized the student was struggling.",
                            "target_lang_translations": {
                                "ru": "Учитель принял <w>соответствующие</w> меры, когда поняла, что ученик испытывает трудности."
                            }
                        }
                    ],
                    "synonyms": [
                        "suitable",
                        "fitting",
                        "apt"
                    ],
                    "antonyms": [
                        "inappropriate"
                    ],
                    "common_phrases": [
                        "appropriate behavior",
                        "appropriate action",
                        "appropriate time",
                        "appropriate response"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходящий, приемлемый или правильный для конкретных обстоятельств или социальной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уместный",
                                "target_lang_sense_clarification": "подходящий для данной обстановки или момента"
                            },
                            {
                                "target_lang_word": "соответствующий",
                                "target_lang_sense_clarification": "отвечающий требованиям или условиям"
                            },
                            {
                                "target_lang_word": "подходящий",
                                "target_lang_sense_clarification": "годный, удобный для чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ad4184f1-5b9e-3071-a734-484237babb3d",
                    "sense_definition": "To take something for one's own use, typically without permission or right.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "taking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The army <w>appropriated</w> the private land to build a new base.",
                            "target_lang_translations": {
                                "ru": "Армия <w>присвоила</w> частную землю для строительства новой базы."
                            }
                        },
                        {
                            "text": "He was accused of <w>appropriating</w> company funds for his personal vacation.",
                            "target_lang_translations": {
                                "ru": "Его обвинили в том, что он <w>присвоил</w> средства компании для своего личного отпуска."
                            }
                        }
                    ],
                    "synonyms": [
                        "take",
                        "steal",
                        "help oneself"
                    ],
                    "common_phrases": [
                        "appropriate land",
                        "appropriate culture",
                        "appropriate funds"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Забирать что-либо для собственного использования, обычно без разрешения или права."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присваивать",
                                "target_lang_sense_clarification": "самовольно делать своей собственностью"
                            },
                            {
                                "target_lang_word": "завладевать",
                                "target_lang_sense_clarification": "захватывать силой или хитростью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4cf2bff9-8a73-3a9b-8979-d1df5234721b",
                    "sense_definition": "To set apart money or resources for a specific purpose, often by official or government action.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "allocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government decided to <w>appropriate</w> more money for the education budget.",
                            "target_lang_translations": {
                                "ru": "Правительство решило <w>выделить</w> больше денег на бюджет образования."
                            }
                        },
                        {
                            "text": "Congress <w>appropriated</w> funds to help the victims of the disaster.",
                            "target_lang_translations": {
                                "ru": "Конгресс <w>ассигновал</w> средства для помощи жертвам катастрофы."
                            }
                        }
                    ],
                    "synonyms": [
                        "allocate",
                        "earmark",
                        "set apart"
                    ],
                    "common_phrases": [
                        "appropriate money",
                        "appropriate funds"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделять деньги или ресурсы для определенной цели, часто посредством официальных или правительственных действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выделять",
                                "target_lang_sense_clarification": "предоставлять средства для использования"
                            },
                            {
                                "target_lang_word": "ассигновать",
                                "target_lang_sense_clarification": "назначать выдачу денежных средств (официально)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
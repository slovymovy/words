        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "b6624fd0-13ab-3ebf-a432-ad9f2acb3bfa",
                    "sense_definition": "Возглас, призывающий обратить внимание на какой-либо звук, заставить прислушаться.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attention_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Чу</w> ― дальний выстрел!",
                            "target_lang_translations": {
                                "en": "<w>Hark</w> ― a distant shot!"
                            }
                        },
                        {
                            "text": "<w>Чу</w>! Полночь, и часы пробили двенадцать раз.",
                            "target_lang_translations": {
                                "en": "<w>Hark</w>! Midnight, and the clock struck twelve times."
                            }
                        }
                    ],
                    "synonyms": [
                        "тихо"
                    ],
                    "common_phrases": [
                        "чу, слышишь"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "common in 19th-century literature and poetry"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation calling attention to a sound, urging one to listen."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hark",
                                "target_lang_sense_clarification": "archaic or poetic exclamation to call attention"
                            },
                            {
                                "target_lang_word": "listen",
                                "target_lang_sense_clarification": "imperative to pay attention to a sound"
                            },
                            {
                                "target_lang_word": "hush",
                                "target_lang_sense_clarification": "command to be silent and listen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b18d0a1-20fa-3764-8b27-2d00c2b066d1",
                    "sense_definition": "Употребляется в значении «слышно», «доносится звук».",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "attention_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Чу</w>… петухи пропели в соседней деревне.",
                            "target_lang_translations": {
                                "en": "<w>Listen</w>... roosters crowed in the neighboring village."
                            }
                        },
                        {
                            "text": "<w>Чу</w>!.. в дали пустой гремит колокольчик звонкий.",
                            "target_lang_translations": {
                                "en": "<w>Hark</w>!.. in the empty distance a ringing bell jingles."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "poetic or archaic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to indicate that a sound is heard or coming from somewhere."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hark",
                                "target_lang_sense_clarification": "poetic indication of a sound being heard"
                            },
                            {
                                "target_lang_word": "listen",
                                "target_lang_sense_clarification": "indication to perceive a sound"
                            }
                        ]
                    }
                }
            ]
        },

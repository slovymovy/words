{
    "word_family": [
        "reachable",
        "unreachable",
        "outreach",
        "overreach"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c72db141-ee80-36ab-926d-a541fad52392",
                    "sense_definition": "To arrive at a particular place or destination after traveling.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We finally <w>reached</w> the hotel after a long drive.",
                            "target_lang_translations": {
                                "ru": "Мы наконец <w>добрались</w> до отеля после долгой поездки."
                            }
                        },
                        {
                            "text": "The train will <w>reach</w> the station in ten minutes.",
                            "target_lang_translations": {
                                "ru": "Поезд <w>прибудет</w> на станцию через десять минут."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибывать в определенное место или пункт назначения после путешествия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добираться",
                                "target_lang_sense_clarification": "прибывать с трудом или после долгого пути"
                            },
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "официально или о трудном пути"
                            },
                            {
                                "target_lang_word": "прибывать",
                                "target_lang_sense_clarification": "о транспорте или официальном визите"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62af0307-450d-3b04-8f41-4ac0136a415d",
                    "sense_definition": "To succeed in achieving a particular goal, level, stage, or age.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attainment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>reached</w> the age of ninety last week.",
                            "target_lang_translations": {
                                "ru": "На прошлой неделе он <w>достиг</w> девяностолетнего возраста."
                            }
                        },
                        {
                            "text": "The two parties finally <w>reached</w> an agreement after hours of negotiation.",
                            "target_lang_translations": {
                                "ru": "После многочасовых переговоров стороны наконец <w>достигли</w> соглашения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "reach a decision",
                        "reach an agreement",
                        "reach a conclusion",
                        "reach a goal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добиваться определенной цели, уровня, стадии или возраста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "добиваться цели или уровня"
                            },
                            {
                                "target_lang_word": "доживать",
                                "target_lang_sense_clarification": "до определенного возраста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d484d3ea-5a3b-3b1d-bda7-03d789117659",
                    "sense_definition": "To stretch out a hand or arm in order to touch, hold, or pick up something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>reached</w> for the book on the top shelf.",
                            "target_lang_translations": {
                                "ru": "Она <w>потянулась</w> за книгой на верхней полке."
                            }
                        },
                        {
                            "text": "The child could not quite <w>reach</w> the door handle.",
                            "target_lang_translations": {
                                "ru": "Ребенок не мог <w>достать</w> до дверной ручки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "reach for",
                        "reach out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вытягивать руку, чтобы коснуться, удержать или взять что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тянуться",
                                "target_lang_sense_clarification": "делать движение рукой к чему-либо"
                            },
                            {
                                "target_lang_word": "доставать",
                                "target_lang_sense_clarification": "успешно дотягиваться до предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7003156-5dd1-322d-9571-a29a04344e1f",
                    "sense_definition": "To succeed in communicating with someone, usually by telephone or email.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have been trying to <w>reach</w> you all day.",
                            "target_lang_translations": {
                                "ru": "Я пытался <w>связаться</w> с тобой весь день."
                            }
                        },
                        {
                            "text": "You can <w>reach</w> him at his office number.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>связаться</w> с ним по рабочему номеру."
                            }
                        }
                    ],
                    "synonyms": [
                        "contact",
                        "get hold of",
                        "get in touch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установить связь с кем-либо, обычно по телефону или электронной почте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связываться",
                                "target_lang_sense_clarification": "устанавливать контакт"
                            },
                            {
                                "target_lang_word": "дозваниваться",
                                "target_lang_sense_clarification": "успешно связываться по телефону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4546e9f-a406-3e04-abd7-23bffa6b6527",
                    "sense_definition": "To extend or stretch over a distance or time to a specific point.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The forest <w>reaches</w> all the way to the river bank.",
                            "target_lang_translations": {
                                "ru": "Лес <w>тянется</w> до самого берега реки."
                            }
                        },
                        {
                            "text": "Her long hair <w>reached</w> down to her waist.",
                            "target_lang_translations": {
                                "ru": "Ее длинные волосы <w>доходили</w> до талии."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Простираться или тянуться на определенное расстояние или время до конкретной точки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простираться",
                                "target_lang_sense_clarification": "занимать пространство"
                            },
                            {
                                "target_lang_word": "доходить",
                                "target_lang_sense_clarification": "до определенного предела"
                            },
                            {
                                "target_lang_word": "тянуться",
                                "target_lang_sense_clarification": "быть протяженным"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "81982b39-0229-3185-ae2c-87d2e1ba3ac9",
                    "sense_definition": "The distance within which someone can stretch a hand to touch or grasp something, or the limit of someone's power or influence.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "range_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please keep all medicines out of the <w>reach</w> of children.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, храните все лекарства вне <w>досягаемости</w> детей."
                            }
                        },
                        {
                            "text": "The victory is now within our <w>reach</w>.",
                            "target_lang_translations": {
                                "ru": "Победа теперь в пределах нашей <w>досягаемости</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "out of reach",
                        "within reach",
                        "beyond reach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расстояние, на которое можно вытянуть руку, чтобы коснуться или схватить что-либо; предел чьей-либо власти или влияния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "досягаемость",
                                "target_lang_sense_clarification": "предел доступности"
                            },
                            {
                                "target_lang_word": "доступ",
                                "target_lang_sense_clarification": "возможность приблизиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c9cc910-ac37-3f8c-9610-18ff4b6a3f50",
                    "sense_definition": "The extent or range of application, effect, or influence of something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The global <w>reach</w> of the internet has changed how we communicate.",
                            "target_lang_translations": {
                                "ru": "Глобальный <w>охват</w> интернета изменил то, как мы общаемся."
                            }
                        },
                        {
                            "text": "This new advertising campaign has a very wide <w>reach</w>.",
                            "target_lang_translations": {
                                "ru": "У этой новой рекламной кампании очень широкий <w>охват</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень или диапазон применения, воздействия или влияния чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охват",
                                "target_lang_sense_clarification": "масштаб влияния или аудитории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ba8deb3-89c6-3738-a8be-db950ad3fe69",
                    "sense_definition": "An interpretation or conclusion that goes beyond what is reasonable or supported by evidence; an exaggeration.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "exaggeration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To call him a genius is a bit of a <w>reach</w>.",
                            "target_lang_translations": {
                                "ru": "Назвать его гением — это небольшая <w>натяжка</w>."
                            }
                        },
                        {
                            "text": "It is a <w>reach</w> to assume they are dating just because they had lunch together.",
                            "target_lang_translations": {
                                "ru": "Это <w>натяжка</w> полагать, что они встречаются, только потому, что они вместе пообедали."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Интерпретация или вывод, выходящие за рамки разумного или подтвержденного доказательствами; преувеличение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "натяжка",
                                "target_lang_sense_clarification": "необоснованное допущение"
                            },
                            {
                                "target_lang_word": "преувеличение",
                                "target_lang_sense_clarification": "излишнее обобщение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "300efab8-9ff3-3d97-91d3-9ce2fc5a51a3",
                    "sense_definition": "Acronym for Registration, Evaluation, Authorisation and Restriction of Chemicals; a regulation designed to improve the protection of human health and the environment.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "regulation",
                    "name_type": "document_or_program_name",
                    "examples": [
                        {
                            "text": "All chemical companies in the region must comply with <w>REACH</w> standards.",
                            "target_lang_translations": {
                                "ru": "Все химические компании в регионе должны соблюдать стандарты <w>REACH</w>."
                            }
                        },
                        {
                            "text": "The new policy is based on the principles of <w>REACH</w>.",
                            "target_lang_translations": {
                                "ru": "Новая политика основана на принципах <w>REACH</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Регламент ЕС, касающийся регистрации, оценки, разрешения и ограничения химических веществ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "REACH",
                                "target_lang_sense_clarification": "название регламента"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
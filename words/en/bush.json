    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "71ca7404-42a6-3b46-8789-f14789e02a23",
                    "sense_definition": "A woody plant distinguished from a tree by its multiple stems and lower height.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to plant a rose <w>bush</w> near the front entrance of the house.",
                            "target_lang_translations": {
                                "ru": "Мы решили посадить розовый <w>куст</w> у парадного входа в дом."
                            }
                        },
                        {
                            "text": "The bird flew out of the thick <w>bush</w> when we approached it.",
                            "target_lang_translations": {
                                "ru": "Птица вылетела из густого <w>кустарника</w>, когда мы подошли к нему."
                            }
                        }
                    ],
                    "synonyms": [
                        "shrub"
                    ],
                    "common_phrases": [
                        "rose bush",
                        "beat about the bush",
                        "berry bush"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древесное растение, отличающееся от дерева наличием нескольких стволов и меньшей высотой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куст",
                                "target_lang_sense_clarification": "отдельное растение"
                            },
                            {
                                "target_lang_word": "кустарник",
                                "target_lang_sense_clarification": "вид растения или заросли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "774029f0-37d9-3038-b364-767ff26654a2",
                    "sense_definition": "Tracts of land covered in natural vegetation that are largely undeveloped and uncultivated, especially in Australia, New Zealand, or Canada.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They spent several days hiking through the <w>bush</w> without seeing another person.",
                            "target_lang_translations": {
                                "ru": "Они провели несколько дней, путешествуя по <w>бушу</w>, и не встретили ни души."
                            }
                        },
                        {
                            "text": "Living in the <w>bush</w> requires a good understanding of survival skills.",
                            "target_lang_translations": {
                                "ru": "Жизнь в <w>дикой местности</w> требует хороших навыков выживания."
                            }
                        }
                    ],
                    "synonyms": [
                        "wilderness",
                        "wild"
                    ],
                    "common_phrases": [
                        "the bush",
                        "bush fire"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Australia, New Zealand, Canada"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обширные участки земли, покрытые естественной растительностью, в основном неосвоенные (особенно в Австралии, Новой Зеландии или Канаде)."
                    },
                    "translations": {
                        "ru": [

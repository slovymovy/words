    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "548af64f-5084-32f6-ad2b-c847ed8a47ef",
                    "sense_definition": "An item of furniture used to sit on or in, comprising a seat, legs or wheels, back, and sometimes arm rests, for use by one person.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I sat on my <w>chair</w> to have dinner with my family.",
                            "target_lang_translations": {
                                "ru": "Я сел на свой <w>стул</w>, чтобы поужинать с семьей."
                            }
                        },
                        {
                            "text": "All I need to relax is a comfortable <w>chair</w> and a good book.",
                            "target_lang_translations": {
                                "ru": "Всё, что мне нужно для отдыха, — это удобное <w>кресло</w> и хорошая книга."
                            }
                        }
                    ],
                    "synonyms": [
                        "seat"
                    ],
                    "common_phrases": [
                        "sit on a chair",
                        "comfortable chair",
                        "wooden chair",
                        "pull up a chair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет мебели для сидения одного человека, состоящий из сиденья, ножек или колесиков, спинки и иногда подлокотников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стул",
                                "target_lang_sense_clarification": "предмет мебели"
                            },
                            {
                                "target_lang_word": "кресло",
                                "target_lang_sense_clarification": "мягкий стул с подлокотниками"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a20a2401-1ebb-37b8-8c91-679ee79e51c2",
                    "sense_definition": "The person in charge of a meeting or an organization; a chairperson.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "chair_authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>chair</w> opened the meeting at nine o'clock sharp.",
                            "target_lang_translations": {
                                "ru": "<w>Председатель</w> открыл собрание ровно в девять часов."
                            }
                        },
                        {
                            "text": "She acts as the <w>chair</w> of the planning committee.",
                            "target_lang_translations": {
                                "ru": "Она выполняет обязанности <w>председателя</w> планового комитета."
                            }
                        }
                    ],
                    "synonyms": [
                        "chairperson",
                        "president",
                        "head",
                        "leader"
                    ],
                    "common_phrases": [
                        "address the chair",
                        "in the chair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, возглавляющее собрание или организацию; председатель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "председатель",
                                "target_lang_sense_clarification": "глава собрания или организации"
                            },
                            {
                                "target_lang_word": "глава",
                                "target_lang_sense_clarification": "руководитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "767daeac-bfc1-3d54-ba20-832f392ba832",
                    "sense_definition": "A distinguished professorship at a university.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chair_academic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was appointed to the <w>chair</w> of Mathematics at the university.",
                            "target_lang_translations": {
                                "ru": "Он был назначен на <w>кафедру</w> математики в университете."
                            }
                        },
                        {
                            "text": "She holds a prestigious <w>chair</w> in the history department.",
                            "target_lang_translations": {
                                "ru": "Она занимает престижную <w>должность профессора</w> на историческом факультете."
                            }
                        }
                    ],
                    "synonyms": [
                        "professorship"
                    ],
                    "common_phrases": [
                        "hold a chair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Почетная профессорская должность в университете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кафедра",
                                "target_lang_sense_clarification": "должность заведующего отделением"
                            },
                            {
                                "target_lang_word": "должность профессора",
                                "target_lang_sense_clarification": "академическое звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ee604fd-e7cc-3071-998b-5cb77accc88e",
                    "sense_definition": "The electric chair; a device used for execution by electrocution.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The convicted murderer was sentenced to the <w>chair</w> for his crimes.",
                            "target_lang_translations": {
                                "ru": "Осужденного убийцу приговорили к <w>электрическому стулу</w> за его преступления."
                            }
                        },
                        {
                            "text": "He is afraid that the court will send him to the <w>chair</w>.",
                            "target_lang_translations": {
                                "ru": "Он боится, что суд отправит его на <w>электрический стул</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "send to the chair",
                        "get the chair"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as an ellipsis for 'electric chair'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Электрический стул; устройство, используемое для казни путем удара электрическим током."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "электрический стул",
                                "target_lang_sense_clarification": "орудие казни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a216cb7c-dbd9-395e-8934-55be6fb9db19",
                    "sense_definition": "The seating position of a particular musician in an orchestra, indicating rank.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She plays first <w>chair</w> violin in the city orchestra.",
                            "target_lang_translations": {
                                "ru": "Она играет за первым <w>пультом</w> скрипок в городском оркестре."
                            }
                        },
                        {
                            "text": "He practiced hard for years to earn the first <w>chair</w>.",
                            "target_lang_translations": {
                                "ru": "Он усердно тренировался годами, чтобы заслужить первый <w>пульт</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "first chair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место определенного музыканта в оркестре, указывающее на его ранг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пульт",
                                "target_lang_sense_clarification": "место в оркестре (напр. первый пульт)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c2cd9235-7264-3746-b1bc-d2372a69cb87",
                    "sense_definition": "To act as chairperson at; to preside over a meeting or committee.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "chair_authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bob will <w>chair</w> the meeting tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Боб будет <w>председательствовать</w> на собрании завтра утром."
                            }
                        },
                        {
                            "text": "She was asked to <w>chair</w> the panel discussion on climate change.",
                            "target_lang_translations": {
                                "ru": "Ее попросили <w>возглавить</w> панельную дискуссию об изменении климата."
                            }
                        }
                    ],
                    "synonyms": [
                        "preside",
                        "lead",
                        "conduct",
                        "head"
                    ],
                    "common_phrases": [
                        "chair a meeting",
                        "chair a committee"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять обязанности председателя; руководить собранием или комитетом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "председательствовать",
                                "target_lang_sense_clarification": "вести собрание"
                            },
                            {
                                "target_lang_word": "возглавлять",
                                "target_lang_sense_clarification": "руководить"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "11b129e7-518a-3d1b-b2f6-4a583b988755",
                    "sense_definition": "The official title or form of address for a chairperson.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chair_authority",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "Please address all your questions through the <w>Chair</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, направляйте все ваши вопросы через <w>Председателя</w>."
                            }
                        },
                        {
                            "text": "The <w>Chair</w> recognizes the representative from Ohio.",
                            "target_lang_translations": {
                                "ru": "<w>Председатель</w> предоставляет слово представителю от Огайо."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный титул или форма обращения к председателю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Председатель",
                                "target_lang_sense_clarification": "официальное обращение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
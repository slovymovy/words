    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "adda8f68-c5f5-358a-ba2f-a4b023262fc7",
                    "sense_definition": "Emitting a lot of light; visually dazzling or radiant.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "light_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sun was so <w>bright</w> that I had to wear sunglasses.",
                            "target_lang_translations": {
                                "ru": "Солнце было таким <w>ярким</w>, что мне пришлось надеть солнечные очки."
                            }
                        },
                        {
                            "text": "A <w>bright</w> light shone from the lighthouse across the dark sea.",
                            "target_lang_translations": {
                                "ru": "<w>Яркий</w> свет маяка сиял сквозь тьму моря."
                            }
                        }
                    ],
                    "synonyms": [
                        "shining",
                        "radiant",
                        "luminous",
                        "brilliant"
                    ],
                    "antonyms": [
                        "dark",
                        "dim"
                    ],
                    "common_phrases": [
                        "bright light",
                        "bright sun",
                        "bright star"
                    ],
                    "target_lang_definitions": {
                        "ru": "Излучающий много света; визуально ослепительный или сияющий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яркий",
                                "target_lang_sense_clarification": "об интенсивности света"
                            },
                            {
                                "target_lang_word": "ослепительный",
                                "target_lang_sense_clarification": "очень яркий, слепящий глаза"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15d63bee-21ba-3f35-b388-08747d906702",
                    "sense_definition": "Of a colour: strong and vivid; not pale or muted.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She likes to wear <w>bright</w> colors like yellow and orange.",
                            "target_lang_translations": {
                                "ru": "Ей нравится носить <w>яркие</w> цвета, такие как желтый и оранжевый."
                            }
                        },
                        {
                            "text": "The bird had <w>bright</w> blue feathers that stood out against the green leaves.",
                            "target_lang_translations": {
                                "ru": "У птицы были <w>яркие</w> синие перья, которые выделялись на фоне зеленых листьев."
                            }
                        }
                    ],
                    "synonyms": [
                        "vivid",
                        "brilliant",
                        "bold",
                        "colorful"
                    ],
                    "antonyms": [
                        "pale",
                        "dull",
                        "muted"
                    ],
                    "common_phrases": [
                        "bright red",
                        "bright colors",
                        "bright green"
                    ],
                    "target_lang_definitions": {
                        "ru": "О цвете: сильный и насыщенный; не бледный и не приглушенный."

                },
                {
                    "sense_id": "397c42e3-b8dc-3734-bf0a-7548eb87e097",
                    "sense_definition": "Metalowy pierścień z gwintem nakręcany na śrubę.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nut",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dokręć mocno ten <w>biegun</w>, żeby koło nie odpadło.",
                            "target_lang_translations": {
                                "ru": "Затяни крепко эту <w>гайку</w>, чтобы колесо не отвалилось."
                            }
                        },
                        {
                            "text": "Brakuje mi jednego <w>bieguna</w> do tej śruby.",
                            "target_lang_translations": {
                                "ru": "Мне не хватает одной <w>гайки</w> для этого болта."
                            }
                        }
                    ],
                    "synonyms": [
                        "mutra"
                    ],
                    "traits": [

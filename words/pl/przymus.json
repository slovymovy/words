    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "449c08a2-59e7-39c8-9e0d-550e41eb7f10",
                    "sense_definition": "Działanie polegające na wywieraniu presji na kogoś w celu zmuszenia go do określonego zachowania wbrew jego woli.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "coercion_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna zeznał, że działał pod <w>przymusem</w> i bał się odmówić.",
                            "target_lang_translations": {
                                "ru": "Мужчина дал показания, что действовал по <w>принуждению</w> и боялся отказаться."
                            }
                        },
                        {
                            "text": "Stosowanie <w>przymusu</w> fizycznego jest w tej sytuacji surowo zabronione.",
                            "target_lang_translations": {
                                "ru": "Применение физического <w>принуждения</w> в этой ситуации строго запрещено."
                            }
                        }
                    ],
                    "synonyms": [
                        "presja",
                        "nacisk",
                        "zmuszenie",
                        "przymuszanie"
                    ],
                    "antonyms": [
                        "dobrowolność",
                        "swoboda"
                    ],
                    "common_phrases": [
                        "pod przymusem",
                        "stosować przymus",
                        "bez przymusu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в оказании давления на кого-либо с целью заставить его вести себя определенным образом против его воли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принуждение",
                                "target_lang_sense_clarification": "давление, насилие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "109a9c60-3c71-3777-94f0-51f1b05f2a88",
                    "sense_definition": "Poczucie wewnętrznej konieczności zrobienia czegoś lub stan skrępowania i braku naturalności w zachowaniu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "internal_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czuł wewnętrzny <w>przymus</w>, by sprawdzić, czy na pewno zamknął drzwi.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал внутреннюю <w>необходимость</w> проверить, точно ли он закрыл дверь."
                            }
                        },
                        {
                            "text": "Goście rozmawiali ze sobą z wyraźnym <w>przymusem</w>, unikając trudnych tematów.",
                            "target_lang_translations": {
                                "ru": "Гости разговаривали друг с другом с явной <w>принуждённостью</w>, избегая трудных тем."
                            }
                        }
                    ],
                    "synonyms": [
                        "konieczność",
                        "mus"
                    ],
                    "antonyms": [
                        "naturalność",
                        "spontaniczność"
                    ],
                    "common_phrases": [
                        "z przymusem",
                        "wewnętrzny przymus",
                        "robić z przymusu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство внутренней необходимости сделать что-то или состояние скованности и неестественности в поведении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принуждённость",
                                "target_lang_sense_clarification": "скованность, неестественность"
                            },
                            {
                                "target_lang_word": "необходимость",
                                "target_lang_sense_clarification": "внутренняя потребность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11b46d33-ed38-3669-bf82-2597d196f7a8",
                    "sense_definition": "Obowiązek wynikający z przepisów prawa lub nakaz egzekwowany przez władze.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Polsce istnieje <w>przymus</w> szkolny obejmujący dzieci i młodzież do osiemnastego roku życia.",
                            "target_lang_translations": {
                                "ru": "В Польше существует школьная <w>обязанность</w>, охватывающая детей и молодежь до восемнадцати лет."
                            }
                        },
                        {
                            "text": "Policja ma prawo użyć środków <w>przymusu</w> bezpośredniego w określonych ustawą przypadkach.",
                            "target_lang_translations": {
                                "ru": "Полиция имеет право применить меры непосредственного <w>принуждения</w> в случаях, определенных законом."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymóg",
                        "konieczność"
                    ],
                    "common_phrases": [
                        "przymus szkolny",
                        "środki przymusu",
                        "przymus adwokacki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обязанность, вытекающая из положений закона, или меры воздействия, применяемые властями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принуждение",
                                "target_lang_sense_clarification": "меры воздействия"
                            },
                            {
                                "target_lang_word": "обязанность",
                                "target_lang_sense_clarification": "юридическая, школьная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8263913-bbb1-3af3-b601-b7f9171a4bb6",
                    "sense_definition": "W brydżu i innych grach karcianych: zagranie zmuszające przeciwnika do odrzucenia ważnej karty.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony gracz zastosował <w>przymus</w>, aby zmusić obrońcę do błędu.",
                            "target_lang_translations": {
                                "ru": "Опытный игрок применил <w>сквиз</w>, чтобы заставить защитника ошибиться."
                            }
                        },
                        {
                            "text": "Ten rodzaj <w>przymusu</w> jest trudny do zauważenia dla początkujących brydżystów.",
                            "target_lang_translations": {
                                "ru": "Этот вид <w>сквиза</w> трудно заметить начинающим бриджистам."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В бридже и других карточных играх: ход, заставляющий противника сбросить важную карту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сквиз",
                                "target_lang_sense_clarification": "термин в бридже"
                            },
                            {
                                "target_lang_word": "принуждение",
                                "target_lang_sense_clarification": "игровая ситуация"
                            }
                        ]
                    }
                }
            ]

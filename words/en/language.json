    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4d2f4d5f-684d-3c8a-8583-b68dc6edb974",
                    "sense_definition": "A system of communication used by a particular country or community, consisting of sounds, words, and grammar.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "comm_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is learning to speak a foreign <w>language</w> at school.",
                            "target_lang_translations": {
                                "ru": "Она учится говорить на иностранном <w>языке</w> в школе."
                            }
                        },
                        {
                            "text": "English is the official <w>language</w> in many countries around the world.",
                            "target_lang_translations": {
                                "ru": "Английский является официальным <w>языком</w> во многих странах мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "tongue"
                    ],
                    "common_phrases": [
                        "foreign language",
                        "sign language",
                        "native language",
                        "official language",
                        "second language"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система общения, используемая в определенной стране или сообществе, состоящая из звуков, слов и грамматики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык",
                                "target_lang_sense_clarification": "система знаков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27aa3147-58c2-38a8-a1dc-1d575518745e",
                    "sense_definition": "A method of expressing thoughts or feelings without using spoken words.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "comm_method",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His body <w>language</w> showed that he was nervous about the interview.",
                            "target_lang_translations": {
                                "ru": "Его <w>язык</w> тела показывал, что он нервничал по поводу собеседования."
                            }
                        },
                        {
                            "text": "Music is often described as a universal <w>language</w> that everyone understands.",
                            "target_lang_translations": {
                                "ru": "Музыку часто описывают как универсальный <w>язык</w>, который понимают все."
                            }
                        }
                    ],
                    "common_phrases": [
                        "body language"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ выражения мыслей или чувств без использования устной речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык",
                                "target_lang_sense_clarification": "средство общения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26f8bfbc-ba2b-388e-afc2-396284542e52",
                    "sense_definition": "A system of instructions used to program computers.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "comm_system",
                    "name_type": "no",
                    "examples": [
                        {

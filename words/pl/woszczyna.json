                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Woszczyna</w> został wybrany na przewodniczącego lokalnego stowarzyszenia.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Вощина</w> был избран председателем местной ассоциации."
                            }
                        },
                        {
                            "text": "Nie znam osobiście nikogo o nazwisku <w>Woszczyna</w>.",
                            "target_lang_translations": {

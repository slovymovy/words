                {
                    "sense_id": "59954a51-fedd-30cd-ac25-9574e07a8bbb",
                    "sense_definition": "A small simple building, usually built of wood, used for storing things or sheltering animals.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He keeps his lawnmower and tools in the garden <w>shed</w>.",
                            "target_lang_translations": {
                                "ru": "Он хранит газонокосилку и инструменты в садовом <w>сарае</w>."
                            }
                        },
                        {
                            "text": "The cows were moved into the <w>shed</w> to protect them from the storm.",
                            "target_lang_translations": {
                                "ru": "Коров перевели в <w>сарай</w>, чтобы защитить их от бури."
                            }
                        }
                    ],
                    "common_phrases": [
                        "garden shed",
                        "tool shed",
                        "bike shed",
                        "potting shed"
                    ],
                    "target_lang_definitions": {

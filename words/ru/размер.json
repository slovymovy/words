    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "87c3e4bf-4270-3a90-9793-790934507412",
                    "sense_definition": "Величина предмета в длину, высоту, ширину или объём.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот стол имеет идеальный <w>размер</w> для нашей маленькой кухни.",
                            "target_lang_translations": {
                                "en": "This table is the perfect <w>size</w> for our small kitchen."
                            }
                        },
                        {
                            "text": "Мы были удивлены огромным <w>размером</w> этой коробки.",
                            "target_lang_translations": {
                                "en": "We were surprised by the huge <w>size</w> of this box."
                            }
                        }
                    ],
                    "synonyms": [
                        "величина",
                        "габарит"
                    ],
                    "common_phrases": [
                        "большой размер",
                        "маленький размер",
                        "средний размер",
                        "размер файла"
                    ],
                    "target_lang_definitions": {
                        "en": "The physical magnitude or dimensions of an object in terms of length, height, width, or volume."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "size",
                                "target_lang_sense_clarification": "general physical magnitude"
                            },
                            {
                                "target_lang_word": "dimensions",
                                "target_lang_sense_clarification": "specific measurements of extent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eea7958c-2e3c-376b-a510-8d74c5a3238c",
                    "sense_definition": "Установленная мерка, номер или буква, обозначающие величину одежды, обуви и других изделий.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К сожалению, вашего <w>размера</w> сейчас нет в магазине.",
                            "target_lang_translations": {
                                "en": "Unfortunately, your <w>size</w> is not in the store right now."
                            }
                        },
                        {
                            "text": "Мне нужно купить зимние ботинки на один <w>размер</w> больше.",
                            "target_lang_translations": {
                                "en": "I need to buy winter boots one <w>size</w> bigger."
                            }
                        }
                    ],
                    "common_phrases": [
                        "размер одежды",
                        "размер обуви",
                        "какой у вас размер",
                        "подходящий размер"
                    ],
                    "target_lang_definitions": {
                        "en": "A standardized number or letter indicating the fit of clothing, footwear, or other manufactured items."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "size",
                                "target_lang_sense_clarification": "standardized measure for clothes/shoes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cacda734-e54d-39d4-8892-a1d7b2290307",
                    "sense_definition": "Степень развития, масштаб или размах какого-либо явления или процесса.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ущерб от лесного пожара достиг колоссальных <w>размеров</w>.",
                            "target_lang_translations": {
                                "en": "The damage from the forest fire reached colossal <w>proportions</w>."
                            }
                        },
                        {
                            "text": "Трудно сразу оценить истинный <w>размер</w> этой экологической проблемы.",
                            "target_lang_translations": {
                                "en": "It is difficult to immediately assess the true <w>scale</w> of this environmental problem."
                            }
                        }
                    ],
                    "synonyms": [
                        "масштаб",
                        "размах",
                        "объём"
                    ],
                    "common_phrases": [
                        "размеры бедствия",
                        "в особо крупных размерах",
                        "достигать размеров"
                    ],
                    "target_lang_definitions": {
                        "en": "The extent, scale, or magnitude of a phenomenon, process, or situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scale",
                                "target_lang_sense_clarification": "relative size or extent of a phenomenon"
                            },
                            {
                                "target_lang_word": "proportions",
                                "target_lang_sense_clarification": "dimensions or scope, often of something negative"
                            },
                            {
                                "target_lang_word": "extent",
                                "target_lang_sense_clarification": "the range or degree to which something spreads"
                            },
                            {
                                "target_lang_word": "magnitude",
                                "target_lang_sense_clarification": "great size or importance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b3b31b7-ad68-305f-b6ee-51175e44f732",
                    "sense_definition": "Ритмическая характеристика музыкального произведения, определяющая количество долей в такте.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "meter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Знаменитый вальс был написан композитором в <w>размере</w> три четверти.",
                            "target_lang_translations": {
                                "en": "The famous waltz was written by the composer in three-quarter <w>time</w>."
                            }
                        },
                        {
                            "text": "Дирижёр попросил оркестр обратить внимание на сложный музыкальный <w>размер</w>.",
                            "target_lang_translations": {
                                "en": "The conductor asked the orchestra to pay attention to the complex musical <w>meter</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "музыкальный размер",
                        "размер такта",
                        "размер три четверти"
                    ],
                    "target_lang_definitions": {
                        "en": "The rhythmic structure of a musical composition, indicating the number of beats per measure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "time signature",
                                "target_lang_sense_clarification": "specific musical notation (e.g., 3/4)"
                            },
                            {
                                "target_lang_word": "meter",
                                "target_lang_sense_clarification": "rhythmic structure in music"
                            },
                            {
                                "target_lang_word": "time",
                                "target_lang_sense_clarification": "idiomatic (e.g., waltz time)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e002e84-5165-30e5-895d-66fc5d068085",
                    "sense_definition": "Способ организации звукового ритма в стихосложении.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "meter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ямб — это очень популярный стихотворный <w>размер</w> в русской классической поэзии.",
                            "target_lang_translations": {
                                "en": "Iambus is a very popular poetic <w>meter</w> in Russian classical poetry."
                            }
                        },
                        {
                            "text": "На уроке литературы школьники учились определять <w>размер</w> стихов Пушкина.",
                            "target_lang_translations": {
                                "en": "In literature class, students learned to determine the <w>meter</w> of Pushkin's poems."
                            }
                        }
                    ],
                    "common_phrases": [
                        "стихотворный размер",
                        "определить размер"
                    ],
                    "target_lang_definitions": {
                        "en": "The rhythmic structure or pattern of beats in poetry."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meter",
                                "target_lang_sense_clarification": "poetic rhythm structure"
                            },
                            {
                                "target_lang_word": "measure",
                                "target_lang_sense_clarification": "literary term for poetic rhythm"
                            }
                        ]
                    }
                }
            ]
        }
    ]

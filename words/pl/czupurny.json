            "senses": [
                {
                    "sense_id": "395c8900-0754-39ca-829d-fd038de71899",
                    "sense_definition": "Skory do kłótni, sprzeciwu lub walki; zadziorny, odważny i niepokorny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>czupurny</w> nastolatek zawsze musi mieć ostatnie słowo w dyskusji z rodzicami.",
                            "target_lang_translations": {
                                "ru": "Этот <w>ершистый</w> подросток всегда должен оставить за собой последнее слово в спорах с родителями."
                            }
                        },
                        {
                            "text": "Mimo niewielkiego wzrostu był to bardzo <w>czupurny</w> zawodnik, który nie bał się silniejszych rywali.",
                            "target_lang_translations": {
                                "ru": "Несмотря на небольшой рост, это был очень <w>задиристый</w> игрок, который не боялся более сильных соперников."
                            }
                        }
                    ],
                    "synonyms": [
                        "zadziorny",
                        "niepokorny",
                        "zawadiacki",
                        "buńczuczny",
                        "hardy",
                        "zaczepny",
                        "przekorny",
                        "wojowniczy"
                    ],
                    "antonyms": [
                        "spokojny",
                        "łagodny",
                        "uległy",
                        "pokorny"
                    ],
                    "common_phrases": [
                        "czupurny chłopak",
                        "czupurny charakter",
                        "czupurna mina"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный к спорам и ссорам; задиристый, смелый и непокорный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задиристый",
                                "target_lang_sense_clarification": "склонный задираться, искать ссоры"
                            },
                            {
                                "target_lang_word": "ершистый",
                                "target_lang_sense_clarification": "непокорный, с характером, неуступчивый"
                            },
                            {
                                "target_lang_word": "дерзкий",
                                "target_lang_sense_clarification": "смелый и вызывающий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22d9538b-5bba-34ed-a4b2-662d47b06738",
                    "sense_definition": "O włosach lub sierści: sterczący, nastroszony i nieuporządkowany.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rano wstałem z łóżka i zobaczyłem w lustrze swoje <w>czupurne</w> włosy.",
                            "target_lang_translations": {
                                "ru": "Утром я встал с кровати и увидел в зеркале свои <w>взъерошенные</w> волосы."
                            }
                        },
                        {
                            "text": "Spod wełnianej czapki wystawały jej niesforne i <w>czupurne</w> kosmyki.",
                            "target_lang_translations": {
                                "ru": "Из-под шерстяной шапки выбивались её непослушные и <w>торчащие</w> пряди."
                            }
                        }
                    ],
                    "synonyms": [
                        "zadziorny",
                        "zawadiacki",
                        "zadzierzysty",
                        "chwacki"
                    ],
                    "antonyms": [

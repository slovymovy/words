            "pos": "name",
            "senses": [
                {
                    "sense_id": "068eb65d-54ce-3560-899c-5ff63dbe1283",
                    "sense_definition": "An English, Scottish, and Northern Irish surname originating as a patronymic.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Rainn <w>Wilson</w> believes the show would have to be different today.",
                            "target_lang_translations": {
                                "ru": "Рэйн <w>Уилсон</w> считает, что сегодня шоу должно было бы быть другим."
                            }
                        },
                        {
                            "text": "Mayor Keith <w>Wilson</w> urged the community to stay peaceful during the conference.",
                            "target_lang_translations": {
                                "ru": "Мэр Кит <w>Уилсон</w> призвал общественность сохранять спокойствие во время конференции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "President Wilson",
                        "Senator Wilson",
                        "Coach Wilson"
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "originating as a patronymic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Английская, шотландская и североирландская фамилия, изначально являвшаяся отчеством"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Уилсон",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24c53e30-c72c-343f-97b9-546b8ef9da66",
                    "sense_definition": "A male given name transferred from the surname.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "<w>Wilson</w> was happy to see his friends at the party.",
                            "target_lang_translations": {
                                "ru": "<w>Уилсон</w> был рад видеть своих друзей на вечеринке."
                            }
                        },
                        {
                            "text": "They decided to name their newborn son <w>Wilson</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать своего новорожденного сына <w>Уилсоном</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя, образованное от фамилии"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Уилсон",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64d02b27-dbbc-33d1-ac87-e620ae02689d",
                    "sense_definition": "A name used for various places in the United States, United Kingdom, and Australia.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "place_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The city of <w>Wilson</w> is located in the eastern part of the state.",
                            "target_lang_translations": {
                                "ru": "Город <w>Уилсон</w> расположен в восточной части штата."
                            }
                        },
                        {
                            "text": "We drove through a small town called <w>Wilson</w> on our way to the coast.",
                            "target_lang_translations": {
                                "ru": "По дороге на побережье мы проехали через маленький городок под названием <w>Уилсон</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "korozja",
        "korozyjny",
        "antykorozyjny",
        "przeciwkorozyjny",
        "korodować"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "187cc57e-2046-34cf-80ab-59201a15902c",
                    "sense_definition": "Ulec procesowi korozji, czyli stopniowemu niszczeniu struktury materiału (najczęściej metalu) pod wpływem czynników chemicznych lub atmosferycznych.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stara rura w piwnicy całkowicie <w>skorodowała</w> i zaczęła przeciekać.",
                            "target_lang_translations": {
                                "ru": "Старая труба в подвале полностью <w>проржавела</w> и начала протекать."
                            }
                        },
                        {
                            "text": "Jeśli nie zabezpieczysz tego płotu farbą, metal szybko <w>skoroduje</w> na deszczu.",
                            "target_lang_translations": {
                                "ru": "Если не защитишь этот забор краской, металл быстро <w>заржавеет</w> под дождем."
                            }
                        }
                    ],
                    "synonyms": [
                        "zardzewieć",
                        "zniszczeć"
                    ],
                    "common_phrases": [
                        "całkowicie skorodować",
                        "szybko skorodować",
                        "metal skorodował"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергнуться процессу коррозии, то есть постепенному разрушению структуры материала (чаще всего металла) под воздействием химических или атмосферных факторов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокорродировать",
                                "target_lang_sense_clarification": "технический термин; подвергнуться коррозии"
                            },
                            {
                                "target_lang_word": "заржаветь",
                                "target_lang_sense_clarification": "покрыться ржавчиной (о железе и его сплавах)"
                            },
                            {
                                "target_lang_word": "подвергнуться коррозии",
                                "target_lang_sense_clarification": "официально-деловой стиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c7b7617-4e96-35d5-96e4-8d5df8b42ee3",
                    "sense_definition": "Spowodować zniszczenie jakiegoś materiału (zwykle metalu) poprzez wywołanie reakcji chemicznej lub działanie czynników zewnętrznych.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny kwas wylany na stół laboratoryjny <w>skorodował</w> jego metalowe elementy.",
                            "target_lang_translations": {
                                "ru": "Сильная кислота, пролитая на лабораторный стол, <w>разъела</w> его металлические элементы."
                            }
                        },
                        {
                            "text": "Sól drogowa i wilgoć <w>skorodowały</w> podwozie mojego samochodu w ciągu kilku lat.",
                            "target_lang_translations": {
                                "ru": "Дорожная соль и влага <w>разъели</w> ходовую часть моего автомобиля за несколько лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeżreć"
                    ],
                    "common_phrases": [
                        "skorodować powierzchnię",
                        "skorodować metal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызвать разрушение какого-либо материала (обычно металла) путем химической реакции или воздействия внешних факторов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разъесть",
                                "target_lang_sense_clarification": "разрушить химически (например, кислотой)"
                            },
                            {
                                "target_lang_word": "вызвать коррозию",
                                "target_lang_sense_clarification": "стать причиной разрушения металла"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
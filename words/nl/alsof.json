{
    "word_family": [
        "als",
        "of"
    ],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "4d8ad239-47b9-36b8-9fb8-ad1b61d6f3a0",
                    "sense_definition": "Gebruikt om een vergelijking te maken met een situatie die niet werkelijk is, maar er wel op lijkt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het lijkt <w>alsof</w> het elk moment kan gaan regenen.",
                            "target_lang_translations": {
                                "ru": "Кажется, <w>как будто</w> дождь может пойти в любой момент.",
                                "en": "It looks <w>as if</w> it could start raining at any moment."
                            }
                        },
                        {
                            "text": "Ze keek me aan <w>alsof</w> ze een spook had gezien.",
                            "target_lang_translations": {
                                "ru": "Она посмотрела на меня, <w>словно</w> увидела призрака.",
                                "en": "She looked at me <w>as though</w> she had seen a ghost."
                            }
                        }
                    ],
                    "synonyms": [
                        "net alsof"
                    ],
                    "common_phrases": [
                        "net alsof"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для сравнения с гипотетической ситуацией, которая не является реальной, но похожа на неё.",
                        "en": "Used to compare with a hypothetical situation that is not real but appears similar."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как будто",
                                "target_lang_sense_clarification": "стандартное сравнение"
                            },
                            {
                                "target_lang_word": "словно",
                                "target_lang_sense_clarification": "более литературный вариант"
                            },
                            {
                                "target_lang_word": "будто",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "as if",
                                "target_lang_sense_clarification": "standard comparison"
                            },
                            {
                                "target_lang_word": "as though",
                                "target_lang_sense_clarification": "slightly more formal"
                            },
                            {
                                "target_lang_word": "like",
                                "target_lang_sense_clarification": "informal/colloquial"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46ca43de-4348-39b0-990f-e197ac10fd7d",
                    "sense_definition": "Gebruikt om aan te geven dat iemand een schijn ophoudt of dat een situatie anders wordt voorgesteld dan de werkelijkheid is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen deden <w>alsof</w> ze ridders en prinsessen waren.",
                            "target_lang_translations": {
                                "ru": "Дети делали вид, <w>будто</w> они рыцари и принцессы.",
                                "en": "The children acted <w>as if</w> they were knights and princesses."
                            }
                        },
                        {
                            "text": "Hij ging gewoon door met werken <w>alsof</w> er niets aan de hand was.",
                            "target_lang_translations": {
                                "ru": "Он просто продолжал работать, <w>как будто</w> ничего не случилось.",
                                "en": "He just continued working <w>as though</w> nothing was wrong."
                            }
                        }
                    ],
                    "common_phrases": [
                        "doen alsof",
                        "alsof er niets aan de hand is",
                        "alsof mijn neus bloedt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы показать, что кто-то притворяется или ситуация представляется иначе, чем есть на самом деле.",
                        "en": "Used to indicate that someone is pretending or that a situation is presented differently from reality."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как будто",
                                "target_lang_sense_clarification": "притворство или видимость"
                            },
                            {
                                "target_lang_word": "будто",
                                "target_lang_sense_clarification": "часто используется с глаголами притворства"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "as if",
                                "target_lang_sense_clarification": "used in pretending contexts"
                            },
                            {
                                "target_lang_word": "as though",
                                "target_lang_sense_clarification": "alternative to 'as if'"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
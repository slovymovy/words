    ],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "1b37c3eb-1cc6-3a1a-8aeb-c47a929e4a91",
                    "sense_definition": "A male given name.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My uncle <w>Leo</w> is coming to visit us this weekend.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Лео</w> приезжает навестить нас в эти выходные."
                            }
                        },
                        {
                            "text": "They decided to name their new baby boy <w>Leo</w> after his grandfather.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать своего новорожденного сына <w>Лео</w> в честь дедушки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лео",
                                "target_lang_sense_clarification": "имя"
                            },
                            {
                                "target_lang_word": "Лев",
                                "target_lang_sense_clarification": "русский эквивалент имени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c99a59b5-4973-3711-b1eb-22621513ca98",
                    "sense_definition": "The fifth sign of the zodiac, represented by the Lion, covering the period from late July to late August.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "astrology",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "According to her horoscope, the sun was in <w>Leo</w> when she was born.",
                            "target_lang_translations": {
                                "ru": "Согласно её гороскопу, солнце находилось в знаке <w>Льва</w>, когда она родилась."
                            }
                        },
                        {
                            "text": "The sign of <w>Leo</w> is associated with the element of fire and the sun.",
                            "target_lang_translations": {
                                "ru": "Знак <w>Льва</w> ассоциируется со стихией огня и солнцем."
                            }
                        }
                    ],
                    "synonyms": [
                        "Lion"
                    ],
                    "antonyms": [
                        "Aquarius"
                    ],
                    "common_phrases": [
                        "sign of Leo",
                        "Leo season"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пятый знак зодиака, символизируемый Львом; период с конца июля по конец августа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лев",
                                "target_lang_sense_clarification": "знак зодиака"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93c81998-37dc-3ef7-a0c0-2e4262b9ff90",
                    "sense_definition": "A large constellation in the northern sky, said to resemble a lion.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "On a clear night, you can identify <w>Leo</w> by looking for the bright star Regulus.",
                            "target_lang_translations": {
                                "ru": "Ясной ночью можно найти <w>Льва</w>, отыскав яркую звезду Регул."
                            }
                        },
                        {
                            "text": "The meteor shower appears to radiate from the constellation <w>Leo</w>.",
                            "target_lang_translations": {
                                "ru": "Кажется, что метеорный поток исходит из созвездия <w>Льва</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое созвездие в северном полушарии неба, напоминающее льва."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лев",
                                "target_lang_sense_clarification": "созвездие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b3f34e7-b0d1-3fd2-ac25-9627e1f06a27",
                    "sense_definition": "A surname.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Leo</w> has been teaching mathematics at the high school for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Лео</w> преподает математику в средней школе уже двадцать лет."
                            }
                        },
                        {
                            "text": "We looked up the <w>Leo</w> family in the local telephone directory.",
                            "target_lang_translations": {
                                "ru": "Мы искали семью <w>Лео</w> в местном телефонном справочнике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лео",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "be5c0f2e-c244-3430-a7e9-4ce1cb405e25",
                    "sense_definition": "A person born under the zodiac sign of Leo.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "astrology_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As a <w>Leo</w>, she loves being the center of attention at parties.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>Львом</w>, она любит быть в центре внимания на вечеринках."
                            }
                        },
                        {
                            "text": "He is a typical <w>Leo</w>: generous, confident, and occasionally a bit dramatic.",
                            "target_lang_translations": {
                                "ru": "Он типичный <w>Лев</w>: щедрый, уверенный в себе и иногда немного драматичный."
                            }
                        }
                    ],
                    "synonyms": [
                        "Leonian"
                    ],
                    "common_phrases": [
                        "typical Leo",
                        "proud Leo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, родившийся под знаком зодиака Лев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лев",
                                "target_lang_sense_clarification": "человек этого знака зодиака"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1127d953-1292-31e3-b805-645d9b5c2c8c",
                    "sense_definition": "An acronym for law enforcement officer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "acronyms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The training seminar was attended by <w>LEOs</w> from several neighboring counties.",
                            "target_lang_translations": {
                                "ru": "На учебном семинаре присутствовали <w>сотрудники правоохранительных органов</w> из нескольких соседних округов."
                            }
                        },
                        {
                            "text": "Every <w>LEO</w> is required to carry identification while on duty.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>сотрудник правоохранительных органов</w> обязан иметь при себе удостоверение во время дежурства."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура для сотрудника правоохранительных органов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сотрудник правоохранительных органов",
                                "target_lang_sense_clarification": "значение аббревиатуры"
                            },
                            {
                                "target_lang_word": "полицейский",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d39ae81f-e434-3ad8-9521-8823e367ee25",
                    "sense_definition": "An acronym for low Earth orbit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "acronyms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Satellites in <w>LEO</w> move much faster across the sky than those in geostationary orbit.",
                            "target_lang_translations": {
                                "ru": "Спутники на <w>НОО</w> движутся по небу гораздо быстрее, чем спутники на геостационарной орбите."
                            }
                        },
                        {
                            "text": "The International Space Station maintains a stable position in <w>LEO</w>.",
                            "target_lang_translations": {
                                "ru": "Международная космическая станция сохраняет стабильное положение на <w>НОО</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура для низкой околоземной орбиты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "НОО",
                                "target_lang_sense_clarification": "низкая околоземная орбита"
                            },
                            {
                                "target_lang_word": "низкая околоземная орбита",
                                "target_lang_sense_clarification": "полный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a0364b0-db02-3df0-9343-5dfe439b141a",
                    "sense_definition": "A short form of <w>leotard</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She grabbed her pink <w>leo</w> and rushed to gymnastics practice.",
                            "target_lang_translations": {
                                "ru": "Она схватила свой розовый <w>купальник</w> и помчалась на тренировку по гимнастике."
                            }
                        },
                        {
                            "text": "The dancers wore matching black <w>leos</w> for the dress rehearsal.",
                            "target_lang_translations": {
                                "ru": "Танцоры надели одинаковые черные <w>трико</w> для генеральной репетиции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "clipping"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от слова leotard (купальник, трико)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "купальник",
                                "target_lang_sense_clarification": "гимнастический"
                            },
                            {
                                "target_lang_word": "трико",
                                "target_lang_sense_clarification": "одежда для танцев"
                            }
                        ]
                    }
                }
            ]
        }
    ]

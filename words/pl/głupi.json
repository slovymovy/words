            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7c4024cf-58a5-311b-9045-8c906174c4d6",
                    "sense_definition": "Ograniczony intelektualnie, niemający wiedzy lub inteligencji; także: świadczący o braku rozsądku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intellect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To był bardzo <w>głupi</w> pomysł, żeby iść tam w nocy.",
                            "target_lang_translations": {
                                "ru": "Это была очень <w>глупая</w> идея — пойти туда ночью."
                            }
                        },
                        {
                            "text": "Mój pies jest trochę <w>głupi</w>, ale bardzo go kocham.",
                            "target_lang_translations": {
                                "ru": "Мой пёс немного <w>глупый</w>, но я его очень люблю."
                            }
                        }
                    ],
                    "synonyms": [
                        "niemądry",
                        "bezrozumny",
                        "tępy",
                        "durny",
                        "bezmyślny"
                    ],
                    "antonyms": [
                        "mądry",
                        "inteligentny",
                        "niegłupi"
                    ],
                    "common_phrases": [
                        "głupi żart",
                        "głupi błąd",
                        "głupie pytanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограниченный интеллектуально, не имеющий знаний или интеллекта; также: свидетельствующий об отсутствии здравого смысла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глупый",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "тупой",
                                "target_lang_sense_clarification": "разговорное, грубое"
                            },
                            {
                                "target_lang_word": "бестолковый",
                                "target_lang_sense_clarification": "несообразительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "778daeb0-eeef-32d7-87ce-320df7c06162",
                    "sense_definition": "Naiwny, łatwowierny, dający się łatwo oszukać lub wykorzystać.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź <w>głupi</w>, oni cię tylko oszukują dla pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Не будь <w>глупым</w>, они тебя просто обманывают ради денег."
                            }
                        },
                        {
                            "text": "Jaka ona <w>głupia</w>, że uwierzyła w te wszystkie kłamstwa.",
                            "target_lang_translations": {
                                "ru": "Какая она <w>глупая</w>, что поверила во всю эту ложь."
                            }
                        }
                    ],
                    "synonyms": [
                        "zidiociały"
                    ],
                    "antonyms": [
                        "niegłupi"
                    ],
                    "common_phrases": [
                        "głupi ja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наивный, легковерный, которого легко обмануть или использовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глупый",
                                "target_lang_sense_clarification": "наивный"
                            },
                            {
                                "target_lang_word": "наивный",
                                "target_lang_sense_clarification": "доверчивый"
                            },
                            {
                                "target_lang_word": "несмышлёный",
                                "target_lang_sense_clarification": "неопытный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d89566ce-fe15-3431-aa27-548874c59a72",
                    "sense_definition": "Błahy, nieistotny, pozbawiony głębszego sensu lub wartości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pokłóciliśmy się o jakiś <w>głupi</w> drobiazg, który nie ma znaczenia.",
                            "target_lang_translations": {
                                "ru": "Мы поссорились из-за какой-то <w>дурацкой</w> мелочи, которая не имеет значения."
                            }
                        },
                        {
                            "text": "Muszę wypełnić te <w>głupie</w> formularze, chociaż to strata czasu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно заполнить эти <w>дурацкие</w> бланки, хотя это пустая трата времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "błahy",
                        "nieistotny",
                        "marny"
                    ],
                    "antonyms": [
                        "poważny"
                    ],
                    "common_phrases": [
                        "głupi powód",
                        "głupia sprawa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to express annoyance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пустяковый, несущественный, лишенный глубокого смысла или ценности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дурацкий",
                                "target_lang_sense_clarification": "разговорное, выражает раздражение"
                            },
                            {
                                "target_lang_word": "глупый",
                                "target_lang_sense_clarification": "бессмысленный"
                            },
                            {
                                "target_lang_word": "идиотский",
                                "target_lang_sense_clarification": "разговорное, экспрессивное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e69de960-2525-3b30-94e0-53ee870d65aa",
                    "sense_definition": "Kłopotliwy, niezręczny, wywołujący uczucie zażenowania (zwykle o sytuacji).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znalazłem się w bardzo <w>głupiej</w> sytuacji, gdy pomyliłem imiona gości.",
                            "target_lang_translations": {
                                "ru": "Я оказался в очень <w>глупой</w> ситуации, когда перепутал имена гостей."
                            }
                        },
                        {
                            "text": "Zapadła <w>głupia</w> cisza, której nikt nie chciał przerwać.",
                            "target_lang_translations": {
                                "ru": "Повисла <w>неловкая</w> тишина, которую никто не хотел прерывать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "głupia sytuacja",
                        "głupie uczucie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Затруднительный, неловкий, вызывающий чувство смущения (обычно о ситуации)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глупый",
                                "target_lang_sense_clarification": "нелепый"
                            },
                            {
                                "target_lang_word": "неловкий",
                                "target_lang_sense_clarification": "вызывающий смущение"
                            },
                            {
                                "target_lang_word": "дурацкий",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d135ebf4-9acb-321b-8ab9-7a2910621237",
                    "sense_definition": "Całkowity i niezasłużony, wynikający z czystego przypadku (używane w połączeniu ze słowem szczęście).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "idiom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miał <w>głupie</w> szczęście, że znalazł ten portfel na ulicy.",
                            "target_lang_translations": {
                                "ru": "Ему выпало <w>дурацкое</w> везение, что он нашел этот кошелек на улице."
                            }
                        },
                        {
                            "text": "To nie były jego umiejętności, tylko zwykłe <w>głupie</w> szczęście.",
                            "target_lang_translations": {
                                "ru": "Это были не его навыки, а просто обычное <w>дурацкое</w> везение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "głupie szczęście"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "idiomatic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полное и незаслуженное, возникшее по чистой случайности (используется в сочетании со словом счастье/везение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дурацкий",
                                "target_lang_sense_clarification": "случайный, незаслуженный"
                            },
                            {
                                "target_lang_word": "шальной",
                                "target_lang_sense_clarification": "случайный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

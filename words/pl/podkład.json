                        {
                            "text": "Ten film ma bardzo nastrojowy <w>podkład</w> muzyczny.",
                            "target_lang_translations": {
                                "ru": "У этого фильма очень атмосферное музыкальное <w>сопровождение</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "akompaniament",
                        "tło"
                    ],
                    "common_phrases": [
                        "podkład muzyczny",
                        "śpiewać do podkładu",
                        "gotowy podkład"
                    ],
                    "target_lang_definitions": {

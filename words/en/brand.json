    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e3f2982c-7044-3451-9537-71dfca05d877",
                    "sense_definition": "A specific product, service, or provider distinguished by a particular name or company.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "business_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I usually buy this <w>brand</w> of coffee because it tastes the best.",
                            "target_lang_translations": {
                                "ru": "Я обычно покупаю эту <w>ма́рку</w> кофе, потому что он самый вкусный."
                            }
                        },
                        {
                            "text": "Which <w>brand</w> of shoes do you prefer for running?",
                            "target_lang_translations": {
                                "ru": "Какую <w>ма́рку</w> обуви вы предпочитаете для бега?"
                            }
                        }
                    ],
                    "synonyms": [
                        "brand name",
                        "trademark",
                        "marque"
                    ],
                    "common_phrases": [
                        "favorite brand",
                        "popular brand",
                        "store brand",
                        "brand of car"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный продукт, услуга или поставщик, отличающиеся особым названием или принадлежностью к компании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ма́рка",
                                "target_lang_sense_clarification": "торговая марка, бренд товара"
                            },
                            {
                                "target_lang_word": "бренд",
                                "target_lang_sense_clarification": "популярная торговая марка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdfbb1d8-dea7-37d8-a879-68cbb92c2bd8",
                    "sense_definition": "The symbolic identity, represented by a name and/or a logo, which indicates a certain product or service to the public.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "business_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is working hard to improve its global <w>brand</w>.",
                            "target_lang_translations": {
                                "ru": "Компания усердно работает над улучшением своего глобального <w>бре́нда</w>."
                            }
                        },
                        {
                            "text": "Building <w>brand</w> loyalty is essential for long-term success in this market.",
                            "target_lang_translations": {
                                "ru": "Формирование лояльности к <w>бре́нду</w> необходимо для долгосрочного успеха на этом рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "logo",
                        "trademark",
                        "brand name"
                    ],
                    "common_phrases": [
                        "brand loyalty",
                        "brand awareness",
                        "luxury brand",
                        "global brand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Символический образ, представленный названием и/или логотипом, который ассоциируется у публики с определенным продуктом или услугой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бренд",
                                "target_lang_sense_clarification": "имидж компании или продукта"
                            },
                            {
                                "target_lang_word": "торго́вая ма́рка",
                                "target_lang_sense_clarification": "юридически зарегистрированный знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "681bc939-d878-31bf-9b83-2c3eaeada65a",
                    "sense_definition": "A particular type or kind of something, often referring to a style of humor, thinking, or behavior.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "type_style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His particular <w>brand</w> of humor is not appreciated by everyone.",
                            "target_lang_translations": {
                                "ru": "Его специфический <w>сорт</w> юмора нравится не всем."
                            }
                        },
                        {
                            "text": "The team played an aggressive <w>brand</w> of football that surprised their opponents.",
                            "target_lang_translations": {
                                "ru": "Команда играла в агрессивный <w>стиль</w> футбола, что удивило их соперников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brand of humor",
                        "unique brand",
                        "own brand of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особый тип или вид чего-либо, часто относящийся к стилю юмора, мышления или поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сорт",
                                "target_lang_sense_clarification": "разновидность, качество"
                            },
                            {
                                "target_lang_word": "тип",
                                "target_lang_sense_clarification": "категория, вид"
                            },
                            {
                                "target_lang_word": "мане́ра",
                                "target_lang_sense_clarification": "стиль поведения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d380ff25-36b6-3f04-8ae3-1515dceb6a0f",
                    "sense_definition": "The public image or reputation of an individual or group.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "business_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician worked hard to protect his personal <w>brand</w> during the campaign.",
                            "target_lang_translations": {
                                "ru": "Политик усердно работал над защитой своего личного <w>бре́нда</w> во время кампании."
                            }
                        },
                        {
                            "text": "Social media allows anyone to build their own <w>brand</w> online.",
                            "target_lang_translations": {
                                "ru": "Социальные сети позволяют любому создать свой собственный <w>бренд</w> в интернете."
                            }
                        }
                    ],
                    "synonyms": [
                        "repute",
                        "good name",
                        "name"
                    ],
                    "common_phrases": [
                        "personal brand",
                        "damage the brand",
                        "build a brand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичный образ или репутация отдельного человека или группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бренд",
                                "target_lang_sense_clarification": "личный бренд, репутация"
                            },
                            {
                                "target_lang_word": "и́мидж",
                                "target_lang_sense_clarification": "публичный образ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5137b08b-71e6-3dc3-8915-a4015c0901c0",
                    "sense_definition": "A permanent mark made on the skin of an animal or person by burning with a hot iron, used to show ownership or identity.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cattle bore the <w>brand</w> of the ranch on their flanks.",
                            "target_lang_translations": {
                                "ru": "Скот носил <w>клеймо́</w> ранчо на боках."
                            }
                        },
                        {
                            "text": "In history books, you can read about how a <w>brand</w> was used to mark criminals.",
                            "target_lang_translations": {
                                "ru": "В учебниках истории можно прочитать о том, как <w>клеймо́</w> использовалось для мечения преступников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cattle brand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянная метка, наносимая на кожу животного или человека раскаленным железом для обозначения принадлежности или идентификации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клеймо́",
                                "target_lang_sense_clarification": "выжженный знак"
                            },
                            {
                                "target_lang_word": "тавро́",
                                "target_lang_sense_clarification": "метка на скоте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "948459c3-e78d-3f06-881b-25fa619d969e",
                    "sense_definition": "A piece of burning wood or a glowing piece of coal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He pulled a burning <w>brand</w> from the fire to light his way in the dark.",
                            "target_lang_translations": {
                                "ru": "Он вытащил горящую <w>голове́шку</w> из костра, чтобы осветить себе путь в темноте."
                            }
                        },
                        {
                            "text": "The campfire died down until only a single glowing <w>brand</w> remained.",
                            "target_lang_translations": {
                                "ru": "Костер догорал, пока не осталась лишь одна тлеющая <w>головня́</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "burning brand",
                        "snatch a brand"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern usage typically prefers 'ember' or 'burning log'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок горящего дерева или тлеющий уголь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голове́шка",
                                "target_lang_sense_clarification": "тлеющее дерево"
                            },
                            {
                                "target_lang_word": "головня́",
                                "target_lang_sense_clarification": "обгоревшее полено"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7ce6a3b5-cd62-397c-96e8-2c2e3c6e5358",
                    "sense_definition": "To give a product or service a specific name, symbol, or design to distinguish it from others.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to <w>brand</w> the new soda as a healthy energy drink.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>позициони́ровать</w> новую газировку как полезный энергетический напиток."
                            }
                        },
                        {
                            "text": "The marketing team wants to <w>brand</w> the company as eco-friendly.",
                            "target_lang_translations": {
                                "ru": "Маркетинговая команда хочет <w>предста́вить</w> компанию как экологически чистую."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brand a product",
                        "fully branded"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать продукту или услуге особое название, символ или дизайн, чтобы выделить их среди других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бренди́ровать",
                                "target_lang_sense_clarification": "создавать бренд"
                            },
                            {
                                "target_lang_word": "позициони́ровать",
                                "target_lang_sense_clarification": "представлять на рынке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c562ccf0-4c4c-3d6b-a643-4331d29d82ed",
                    "sense_definition": "To describe or label someone in a negative way; to accuse someone of being something bad.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "type_style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the scandal, he was <w>branded</w> a liar by the press.",
                            "target_lang_translations": {
                                "ru": "После скандала пресса <w>заклейми́ла</w> его лжецом."
                            }
                        },
                        {
                            "text": "She refused to be <w>branded</w> a failure just because of one mistake.",
                            "target_lang_translations": {
                                "ru": "Она отказывалась позволить <w>наве́сить на себя ярлы́к</w> неудачницы только из-за одной ошибки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "branded a traitor",
                        "branded a criminal",
                        "branded a fool"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывать или называть кого-то в негативном ключе; обвинять кого-то в чем-то плохом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клейми́ть",
                                "target_lang_sense_clarification": "позорить, осуждать"
                            },
                            {
                                "target_lang_word": "ве́шать ярлы́к",
                                "target_lang_sense_clarification": "присваивать негативную характеристику"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9af9ef69-b313-352c-8933-afef699cc243",
                    "sense_definition": "To burn a mark onto the skin of an animal with a hot iron to show ownership.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ranchers <w>brand</w> the calves every spring to identify them.",
                            "target_lang_translations": {
                                "ru": "Фермеры <w>клеймя́т</w> телят каждую весну, чтобы идентифицировать их."
                            }
                        },
                        {
                            "text": "It is traditional to <w>brand</w> cattle with a unique symbol for each farm.",
                            "target_lang_translations": {
                                "ru": "Традиционно принято <w>ста́вить тавро́</w> на скот с уникальным символом для каждой фермы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brand cattle",
                        "brand the skin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выжигать метку на коже животного раскаленным железом для указания владельца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клейми́ть",
                                "target_lang_sense_clarification": "ставить клеймо"
                            },
                            {
                                "target_lang_word": "таври́ть",
                                "target_lang_sense_clarification": "ставить тавро"
                            }
                        ]
                    }
                }
            ]
        },
        {

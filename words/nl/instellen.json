{
    "word_family": [
        "de instelling"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "33c0e748-3cb6-37a4-a8e9-26aa3fcc0b9f",
                    "sense_definition": "Een apparaat, programma of systeem klaarmaken voor gebruik door de juiste opties of waarden te kiezen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "technical_adjustment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt de taal van je telefoon eenvoudig zelf <w>instellen</w>.",
                            "target_lang_translations": {
                                "ru": "Ты можешь легко сам <w>настроить</w> язык в своем телефоне.",
                                "en": "You can easily <w>set</w> the language of your phone yourself."
                            }
                        },
                        {
                            "text": "Heb jij de wekker al <w>ingesteld</w> voor morgenochtend?",
                            "target_lang_translations": {
                                "ru": "Ты уже <w>установил</w> будильник на завтрашнее утро?",
                                "en": "Have you <w>set</w> the alarm for tomorrow morning yet?"
                            }
                        }
                    ],
                    "synonyms": [
                        "afkondigen"
                    ],
                    "common_phrases": [
                        "een wachtwoord instellen",
                        "de temperatuur instellen",
                        "het volume instellen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подготавливать устройство, программу или систему к использованию, выбирая нужные опции или значения.",
                        "en": "To prepare a device, program, or system for use by choosing the right options or values."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настраивать",
                                "target_lang_sense_clarification": "параметры, технику"
                            },
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "значения, время"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "options, values, time"
                            },
                            {
                                "target_lang_word": "configure",
                                "target_lang_sense_clarification": "software, system"
                            },
                            {
                                "target_lang_word": "adjust",
                                "target_lang_sense_clarification": "settings, volume"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aca69288-ed6d-31a4-b517-42a7a5a78981",
                    "sense_definition": "Officieel laten beginnen of geldig maken, zoals een regel, wet, actie of onderzoek.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "official_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie gaat een groot onderzoek <w>instellen</w> naar de diefstal.",
                            "target_lang_translations": {
                                "ru": "Полиция собирается <w>начать</w> крупное расследование кражи.",
                                "en": "The police are going to <w>launch</w> a major investigation into the theft."
                            }
                        },
                        {
                            "text": "De regering heeft nieuwe regels <w>ingesteld</w> om het verkeer veiliger te maken.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>ввело</w> новые правила, чтобы сделать дорожное движение безопаснее.",
                                "en": "The government has <w>introduced</w> new rules to make traffic safer."
                            }
                        }
                    ],
                    "synonyms": [
                        "afkondigen",
                        "aanstellen"
                    ],
                    "common_phrases": [
                        "een onderzoek instellen",
                        "een verbod instellen",
                        "beroep instellen"

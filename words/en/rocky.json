{
    "word_family": [
        "rock",
        "rocker",
        "rockiness",
        "bedrock"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b684fa6f-8404-3fb6-b7bd-bf0559443693",
                    "sense_definition": "Abounding in or consisting of rocks.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boat approached the <w>rocky</w> shore very carefully.",
                            "target_lang_translations": {
                                "ru": "Лодка очень осторожно подошла к <w>каменистому</w> берегу."
                            }
                        },
                        {
                            "text": "We hiked up a steep and <w>rocky</w> path to reach the summit.",
                            "target_lang_translations": {
                                "ru": "Мы поднимались по крутой <w>каменистой</w> тропе, чтобы добраться до вершины."
                            }
                        }
                    ],
                    "synonyms": [
                        "stonelike",
                        "rocklike, rock-like",
                        "stonelike"
                    ],
                    "antonyms": [
                        "smooth"
                    ],
                    "common_phrases": [
                        "rocky road",
                        "rocky mountain",
                        "rocky shore",
                        "rocky terrain",
                        "rocky ground"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изобилующий камнями или состоящий из скал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каменистый",
                                "target_lang_sense_clarification": "покрытый камнями (о почве, береге)"
                            },
                            {
                                "target_lang_word": "скалистый",
                                "target_lang_sense_clarification": "состоящий из скал (о горах, местности)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9a8f709-4e3f-34fe-b35c-dd4130db7a80",
                    "sense_definition": "Full of difficulties, problems, or dangers; characterized by struggles.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_difficulty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They had a <w>rocky</w> start to their marriage, but now they are very happy.",
                            "target_lang_translations": {
                                "ru": "У них было <w>трудное</w> начало брака, но сейчас они очень счастливы."
                            }
                        },
                        {
                            "text": "The company faced a <w>rocky</w> financial year due to the sudden market crash.",
                            "target_lang_translations": {
                                "ru": "Для компании этот финансовый год выдался <w>непростым</w> из-за внезапного обвала рынка."
                            }
                        }
                    ],
                    "synonyms": [
                        "difficult",
                        "hard",
                        "tough",
                        "challenging",
                        "arduous",
                        "effortful"
                    ],
                    "antonyms": [
                        "smooth",
                        "easy",
                        "stable"
                    ],
                    "common_phrases": [
                        "rocky start",
                        "rocky relationship",
                        "rocky marriage",
                        "rocky period",
                        "rocky patch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный трудностей, проблем или опасностей; характеризующийся борьбой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трудный",
                                "target_lang_sense_clarification": "сложный, тяжелый (о периоде, отношениях)"
                            },
                            {
                                "target_lang_word": "непростой",
                                "target_lang_sense_clarification": "содержащий сложности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87c358a1-07c7-3638-8114-814937eabb5e",
                    "sense_definition": "Unstable or tending to rock and shake; not firm.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_stability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This old table is a bit <w>rocky</w>, so be careful with your drink.",
                            "target_lang_translations": {
                                "ru": "Этот старый стол немного <w>шаткий</w>, так что осторожнее с напитком."
                            }
                        },
                        {
                            "text": "The ladder felt <w>rocky</w> as he climbed up to fix the roof.",
                            "target_lang_translations": {
                                "ru": "Лестница казалась <w>неустойчивой</w>, когда он лез наверх чинить крышу."
                            }
                        }
                    ],
                    "synonyms": [
                        "wobbly",
                        "shaky",
                        "unstable",
                        "unsteady",
                        "tottering"
                    ],
                    "antonyms": [
                        "stable",
                        "steady",
                        "firm"
                    ],
                    "common_phrases": [
                        "rocky table",
                        "rocky chair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неустойчивый, склонный качаться или трястись; ненадежный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаткий",
                                "target_lang_sense_clarification": "качающийся, ненадежно стоящий"
                            },
                            {
                                "target_lang_word": "неустойчивый",
                                "target_lang_sense_clarification": "не имеющий твердой опоры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef120727-a18a-3f72-b7a9-096203d62332",
                    "sense_definition": "Resembling or characteristic of rock music.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "music_style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band's new song has a much more <w>rocky</w> sound than their previous ballads.",
                            "target_lang_translations": {
                                "ru": "У новой песни группы гораздо более <w>роковое</w> звучание, чем у их предыдущих баллад."
                            }
                        },
                        {
                            "text": "I prefer listening to <w>rocky</w> tunes when I am driving on the highway.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю слушать мелодии <w>в стиле рок</w>, когда еду по шоссе."
                            }
                        }
                    ],
                    "synonyms": [
                        "rocklike, rock-like",
                        "rockesque"
                    ],
                    "common_phrases": [
                        "rocky sound",
                        "rocky vibe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий рок-музыку или характерный для неё."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роковый",
                                "target_lang_sense_clarification": "относящийся к рок-музыке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95c58f42-ff5a-3bea-8046-b52b67e41258",
                    "sense_definition": "Feeling ill, dizzy, or unsteady, often as a result of a shock or illness.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I still feel a bit <w>rocky</w> after having the flu last week.",
                            "target_lang_translations": {
                                "ru": "Я всё ещё чувствую себя немного <w>разбитым</w> после перенесенного на прошлой неделе гриппа."
                            }
                        },
                        {
                            "text": "She looked pale and <w>rocky</w> when she stood up too quickly.",
                            "target_lang_translations": {
                                "ru": "Она выглядела бледной и <w>слабой</w>, когда слишком быстро встала."
                            }
                        }
                    ],
                    "synonyms": [
                        "shaky",
                        "unsteady",
                        "unnerved",
                        "weakened"
                    ],
                    "antonyms": [
                        "steady",
                        "well"
                    ],
                    "common_phrases": [
                        "feel rocky",
                        "look rocky"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий слабость, головокружение или неустойчивость (обычно из-за болезни или шока)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбитый",
                                "target_lang_sense_clarification": "плохо себя чувствующий, уставший"
                            },
                            {
                                "target_lang_word": "слабый",
                                "target_lang_sense_clarification": "физически немощный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "745b321e-9bfe-3056-af87-2e66222897b9",
                    "sense_definition": "Emotionally hard, unfeeling, or not easily affected.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "metaphorical_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>rocky</w> expression showed no sign of sympathy for their situation.",
                            "target_lang_translations": {
                                "ru": "Его <w>каменное</w> выражение лица не выказывало никакого сочувствия к их ситуации."
                            }
                        },
                        {
                            "text": "She maintained a <w>rocky</w> silence throughout the interrogation.",
                            "target_lang_translations": {
                                "ru": "Она хранила <w>каменное</w> молчание на протяжении всего допроса."
                            }
                        }
                    ],
                    "synonyms": [
                        "flinty",
                        "stonelike",
                        "hard",
                        "tough"
                    ],
                    "antonyms": [
                        "soft",
                        "sensitive"
                    ],
                    "common_phrases": [
                        "rocky heart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Эмоционально твердый, бесчувственный или не поддающийся влиянию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каменный",
                                "target_lang_sense_clarification": "метафорически: бесчувственный, неподвижный"
                            },
                            {
                                "target_lang_word": "суровый",
                                "target_lang_sense_clarification": "строгий, непреклонный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "11f54d21-ad45-37d9-ad93-abacea2414b5",
                    "sense_definition": "A diminutive of the male given names <w>Robert</w>, <w>Ricky</w>, <w>Rocco</w>, <w>Roch</w>, or <w>Rock</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My uncle <w>Rocky</w> is coming to visit us this weekend.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Рокки</w> приезжает навестить нас в эти выходные."
                            }
                        },
                        {
                            "text": "Everyone in the neighborhood knew <w>Rocky</w> as a friendly guy.",
                            "target_lang_translations": {
                                "ru": "Все в округе знали <w>Рокки</w> как дружелюбного парня."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма мужских имен Роберт, Рикки, Рокко и др."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Рокки",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "777cc36b-deb0-3e8c-9aa4-e1b7789d6d9a",
                    "sense_definition": "A nickname for the city of <w>Rockhampton</w> in Queensland, Australia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We drove up to <w>Rocky</w> for the weekend to see some friends.",
                            "target_lang_translations": {
                                "ru": "Мы поехали в <w>Рокки</w> на выходные, чтобы повидаться с друзьями."
                            }
                        },
                        {
                            "text": "Locals often refer to their city as <w>Rocky</w> instead of its full name.",
                            "target_lang_translations": {
                                "ru": "Местные часто называют свой город <w>Рокки</w> вместо полного названия."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Australia"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прозвище города Рокгемптон в Квинсленде, Австралия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Рокки",
                                "target_lang_sense_clarification": "название города"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
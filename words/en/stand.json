                },
                {
                    "sense_id": "e05d71f2-3e6f-30ab-a5ff-f5c4a337b009",
                    "sense_definition": "The tiered seating area for spectators at a sports stadium or arena.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fans in the <w>stands</w> cheered loudly for their team.",
                            "target_lang_translations": {
                                "ru": "Болельщики на <w>трибунах</w> громко поддерживали свою команду."
                            }
                        },
                        {
                            "text": "He hit the ball so hard it flew right into the <w>stands</w>.",
                            "target_lang_translations": {
                                "ru": "Он ударил по мячу так сильно, что тот улетел прямо на <w>трибуны</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bleachers",
                        "grandstand",
                        "terrace"
                    ],
                    "common_phrases": [
                        "in the stands"
                    ],

{
    "word_family": [
        "objęcie",
        "objętość",
        "obejma"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c2337bd3-45d2-3f37-8ce4-e0ce541252c3",
                    "sense_definition": "Otaczać kogoś lub coś ramionami, zazwyczaj w geście powitania, pożegnania lub czułości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczynka mocno <obejmowała> swojego ulubionego pluszowego misia.",
                            "target_lang_translations": {
                                "ru": "Девочка крепко <обнимала> своего любимого плюшевого мишку."
                            }
                        },
                        {
                            "text": "Na peronie ludzie płakali i <obejmowali> się na pożegnanie.",
                            "target_lang_translations": {
                                "ru": "На перроне люди плакали и <обнимались> на прощание."
                            }
                        }
                    ],
                    "synonyms": [
                        "przytulać",
                        "uściskać"
                    ],
                    "common_phrases": [
                        "obejmować ramieniem",
                        "czule obejmować",
                        "obejmować się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнимать кого-то или что-то руками, обычно в знак приветствия, прощания или нежности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнимать",
                                "target_lang_sense_clarification": "заключать в объятия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55df24bd-ca2e-3ca1-9e3b-29782f0986b2",
                    "sense_definition": "Zawierać w sobie, składać się z określonych elementów lub mieścić się w pewnych granicach.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cena wycieczki <obejmuje> pełne wyżywienie oraz ubezpieczenie turystyczne.",
                            "target_lang_translations": {
                                "ru": "Цена поездки <включает> полное питание и туристическую страховку."
                            }
                        },
                        {
                            "text": "Jego posiadłość <obejmuje> zabytkowy dworek i duży fragment lasu.",
                            "target_lang_translations": {
                                "ru": "Его поместье <охватывает> старинную усадьбу и большой участок леса."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawierać",
                        "mieścić",
                        "skupiać"
                    ],
                    "common_phrases": [
                        "obejmować zakresem",
                        "plan obejmuje",
                        "koszt obejmuje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержать в себе, состоять из определенных элементов или находиться в определенных границах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "содержать в себе"
                            },
                            {
                                "target_lang_word": "охватывать",
                                "target_lang_sense_clarification": "распространяться на определенный объем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f11c9268-5d29-371d-b135-0010f6d92259",
                    "sense_definition": "Rozpoczynać pełnienie ważnej funkcji, stanowiska lub urzędu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy prezydent uroczyście <obejmie> urząd w przyszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Новый президент торжественно <вступит> в должность в следующем месяце."
                            }
                        },
                        {
                            "text": "Po odejściu szefa, to ona <obejmuje> kierownicze stanowisko w dziale.",
                            "target_lang_translations": {
                                "ru": "После ухода начальника именно она <занимает> руководящую должность в отделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "przejmować"
                    ],
                    "common_phrases": [
                        "obejmować stanowisko",
                        "obejmować urząd",
                        "obejmować władzę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать выполнение важной функции, вступать в должность или занимать пост."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вступать",
                                "target_lang_sense_clarification": "в должность"
                            },
                            {
                                "target_lang_word": "занимать",
                                "target_lang_sense_clarification": "пост или должность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8be66b62-b79a-322f-81b6-43272442ba1c",
                    "sense_definition": "Odnosić się do kogoś lub czegoś, mieć zastosowanie w danym przypadku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowe przepisy podatkowe nie <obejmują> małych firm rodzinnych.",
                            "target_lang_translations": {
                                "ru": "Новые налоговые правила не <распространяются> на малые семейные фирмы."
                            }
                        },
                        {
                            "text": "Ogłoszona amnestia <obejmowała> tylko więźniów z niskimi wyrokami.",
                            "target_lang_translations": {
                                "ru": "Объявленная амнистия <касалась> только заключенных с небольшими сроками."
                            }
                        }
                    ],
                    "synonyms": [
                        "dotyczyć"
                    ],
                    "common_phrases": [
                        "zakaz obejmuje",
                        "ustawa obejmuje",
                        "obejmować wszystkich"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относиться к кому-то или чему-то, иметь применение в данном случае."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распространяться",
                                "target_lang_sense_clarification": "иметь действие на кого-то"
                            },
                            {
                                "target_lang_word": "касаться",
                                "target_lang_sense_clarification": "иметь отношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4444a4de-4ebc-3550-bea9-89f96c8b7955",
                    "sense_definition": "Rozprzestrzeniać się na jakimś obszarze lub opanowywać coś (często o ogniu, wojnie, zarazie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "coverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gwałtowny pożar szybko <obejmował> kolejne piętra wieżowca.",
                            "target_lang_translations": {
                                "ru": "Сильный пожар быстро <охватывал> следующие этажи небоскреба."
                            }
                        },
                        {
                            "text": "Epidemia grypy <obejmowała> coraz większe obszary kraju.",
                            "target_lang_translations": {
                                "ru": "Эпидемия гриппа <охватывала> всё большие территории страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogarniać",
                        "zajmować"
                    ],
                    "common_phrases": [
                        "ogień obejmuje",
                        "wojna obejmuje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространяться на какой-то территории или овладевать чем-то (часто об огне, войне, эпидемии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охватывать",
                                "target_lang_sense_clarification": "овладевать, распространяться повсюду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67f2f102-67f5-3c68-9ee1-465d6b38d934",
                    "sense_definition": "Widzieć całą przestrzeń lub rozumieć całość jakiegoś zagadnienia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "coverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stojąc na szczycie góry, <obejmował> wzrokiem całą malowniczą dolinę.",
                            "target_lang_translations": {
                                "ru": "Стоя на вершине горы, он <окидывал> взглядом всю живописную долину."
                            }
                        },
                        {
                            "text": "Trudno <obejmować> umysłem ogrom i skomplikowanie wszechświata.",
                            "target_lang_translations": {
                                "ru": "Трудно <охватить> умом огромность и сложность вселенной."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogarniać",
                        "pojmować"
                    ],
                    "common_phrases": [
                        "obejmować wzrokiem",
                        "obejmować umysłem",
                        "obejmować spojrzeniem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Видеть всё пространство или понимать суть какого-либо вопроса целиком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окидывать",
                                "target_lang_sense_clarification": "взглядом"
                            },
                            {
                                "target_lang_word": "охватывать",
                                "target_lang_sense_clarification": "умом или взглядом"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
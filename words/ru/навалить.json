    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f7eba76f-0298-363e-9b30-146a51b1b035",
                    "sense_definition": "Беспорядочно набросать или сложить что-либо в большом количестве в одно место.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Строители <навалили> гору мусора прямо у входа в подъезд.",
                            "target_lang_translations": {
                                "en": "The builders <dumped> a mountain of trash right at the entrance."
                            }
                        },
                        {
                            "text": "Не нужно было <навалить> кучу вещей на стул, теперь некуда сесть.",
                            "target_lang_translations": {
                                "en": "You shouldn't have <piled> a bunch of things on the chair, now there's nowhere to sit."
                            }
                        }
                    ],
                    "synonyms": [
                        "набросать",
                        "нагромоздить"
                    ],
                    "antonyms": [
                        "разобрать"
                    ],
                    "common_phrases": [
                        "навалить кучу",
                        "навалить гору",
                        "навалить вещей"
                    ],
                    "target_lang_definitions": {
                        "en": "To throw or pile something in a disorderly manner in large quantities in one place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pile up",
                                "target_lang_sense_clarification": "to accumulate in a pile"
                            },
                            {
                                "target_lang_word": "heap",
                                "target_lang_sense_clarification": "to put in a pile"
                            },
                            {
                                "target_lang_word": "dump",
                                "target_lang_sense_clarification": "to throw down carelessly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ce50a96-c2ae-3127-9059-6ea70746a1a6",
                    "sense_definition": "Выпасть толстым слоем в большом количестве (обычно о снеге).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За прошлую ночь <навалило> столько снега, что мы не смогли открыть дверь.",
                            "target_lang_translations": {
                                "en": "So much snow <piled up> last night that we couldn't open the door."
                            }
                        },
                        {
                            "text": "Зимой в лесу <навалило> сугробы по пояс.",
                            "target_lang_translations": {
                                "en": "In winter, snow <piled up> in waist-deep drifts in the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "намести",
                        "выпасть"
                    ],
                    "antonyms": [
                        "растаять"
                    ],
                    "common_phrases": [
                        "снегу навалило",
                        "за ночь навалило"
                    ],
                    "target_lang_definitions": {
                        "en": "To fall in a thick layer in large quantities (usually about snow)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pile up",
                                "target_lang_sense_clarification": "to accumulate (of snow)"
                            },
                            {
                                "target_lang_word": "fall heavily",
                                "target_lang_sense_clarification": "to snow intensely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eba26d8e-8f96-3f97-8336-adf131f512aa",
                    "sense_definition": "Поручить кому-либо выполнение тяжелой или многочисленной работы, возложить ответственность.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник решил <навалить> на меня отчёт за весь отдел.",
                            "target_lang_translations": {
                                "en": "The boss decided to <dump> the report for the whole department on me."
                            }
                        },
                        {
                            "text": "После увольнения коллеги на нас <навалили> новые обязанности.",
                            "target_lang_translations": {
                                "en": "After our colleague was fired, they <piled> new duties on us."
                            }
                        }
                    ],
                    "synonyms": [
                        "взвалить",
                        "нагрузить"
                    ],
                    "antonyms": [
                        "разгрузить",
                        "освободить"
                    ],
                    "common_phrases": [
                        "навалить работу",
                        "навалить обязанности",
                        "навалить ответственность"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To assign someone heavy or numerous tasks; to impose responsibility."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dump on",
                                "target_lang_sense_clarification": "to assign work unfairly"
                            },
                            {
                                "target_lang_word": "pile on",
                                "target_lang_sense_clarification": "to add more work or burden"
                            },
                            {
                                "target_lang_word": "load with",
                                "target_lang_sense_clarification": "to burden with tasks"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "855fbf79-12a4-315f-acd9-89ff6d17be5d",
                    "sense_definition": "Прийти или собраться где-либо в огромном количестве (о людях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На бесплатный концерт <навалило> столько народу, что яблоку негде упасть.",
                            "target_lang_translations": {
                                "en": "So many people <crowded> into the free concert that there was no room to swing a cat."
                            }
                        },
                        {
                            "text": "В магазин <навалило> много покупателей перед праздниками.",
                            "target_lang_translations": {
                                "en": "Many shoppers <flocked> to the store before the holidays."
                            }
                        }
                    ],
                    "synonyms": [
                        "набиться"
                    ],
                    "common_phrases": [
                        "народу навалило"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To arrive or gather somewhere in huge numbers (about people)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crowd into",
                                "target_lang_sense_clarification": "to gather in large numbers"
                            },
                            {
                                "target_lang_word": "flock to",
                                "target_lang_sense_clarification": "to go to a place in large numbers"
                            },
                            {
                                "target_lang_word": "pack into",
                                "target_lang_sense_clarification": "to fill a space tightly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3145d72-5ed6-3f3d-bd70-a0d7ed5dd141",
                    "sense_definition": "Опорожнить кишечник, испражниться.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Соседский кот опять <навалил> кучу на нашем коврике.",
                            "target_lang_translations": {
                                "en": "The neighbor's cat <took a dump> on our rug again."
                            }
                        },
                        {
                            "text": "Кто-то умудрился <навалить> прямо посреди тротуара.",
                            "target_lang_translations": {
                                "en": "Someone managed to <take a crap> right in the middle of the sidewalk."
                            }
                        }
                    ],
                    "synonyms": [
                        "нагадить",
                        "испражниться"
                    ],
                    "common_phrases": [
                        "навалить кучу",
                        "навалить в штаны"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To empty the bowels; to defecate."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take a dump",
                                "target_lang_sense_clarification": "slang for defecate"
                            },
                            {
                                "target_lang_word": "crap",
                                "target_lang_sense_clarification": "vulgar slang for defecate"
                            }
                        ]
                    }
                }
            ]
        }
    ]

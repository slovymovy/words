    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "106d0c7f-01ac-398a-876b-25e4ca427b55",
                    "sense_definition": "Iets of iemand over de grond of door het water trekken zonder het op te tillen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_drag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>sleepte</w> de zware koffer door de lange gang.",
                            "target_lang_translations": {
                                "ru": "Он <w>тащил</w> тяжелый чемодан по длинному коридору."
                            }
                        },
                        {
                            "text": "Het team wist de overwinning in de wacht te <w>slepen</w>.",
                            "target_lang_translations": {
                                "ru": "Команде удалось <w>вырвать</w> победу."
                            }
                        }
                    ],
                    "synonyms": [
                        "trekken",
                        "voorttrekken",
                        "sjorren"
                    ],
                    "antonyms": [
                        "duwen",
                        "dragen"
                    ],
                    "common_phrases": [
                        "in de wacht slepen",
                        "voor de rechter slepen",
                        "erbij slepen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тянуть что-то или кого-то по земле или воде, не поднимая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тащить",
                                "target_lang_sense_clarification": "перемещать волоком"
                            },
                            {
                                "target_lang_word": "волочить",
                                "target_lang_sense_clarification": "тянуть по поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afe7c664-ac74-3689-9dba-1bfdf2bdb3e6",
                    "sense_definition": "Zich langzaam en met veel moeite voortbewegen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_drag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gewonde man <w>sleepte</w> zich naar de kant van de weg.",
                            "target_lang_translations": {
                                "ru": "Раненый мужчина <w>дотащился</w> до обочины."
                            }
                        },
                        {
                            "text": "De saaie vergadering <w>sleepte</w> zich traag voort.",
                            "target_lang_translations": {
                                "ru": "Скучное собрание медленно <w>тянулось</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "voortsukkelen"
                    ],
                    "antonyms": [
                        "rennen",
                        "snellen"
                    ],
                    "common_phrases": [
                        "zich voort slepen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться медленно и с большим трудом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тащиться",
                                "target_lang_sense_clarification": "медленно передвигаться"
                            },
                            {
                                "target_lang_word": "тянуться",
                                "target_lang_sense_clarification": "длиться слишком долго"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0da03019-62a2-3b43-a5d8-a5c48efdcd30",
                    "sense_definition": "*verleden tijd* meervoud van <w>slijpen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De slagers <w>slepen</w> hun messen elke ochtend scherp.",
                            "target_lang_translations": {
                                "ru": "Мясники каждое утро <w>точили</w> свои ножи."
                            }
                        },
                        {
                            "text": "Vroeger <w>slepen</w> wij onze schaatsen altijd zelf.",
                            "target_lang_translations": {
                                "ru": "Раньше мы всегда сами <w>точили</w> свои коньки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время (множественное число) от глагола \"точить\" (slijpen)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точили",
                                "target_lang_sense_clarification": "делали острым"
                            }
                        ]
                    }
                }
            ]
        },

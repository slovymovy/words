            "senses": [
                {
                    "sense_id": "2e4969e2-4e06-3340-ba17-ec7652ba489b",
                    "sense_definition": "De vlezige rand aan de boven- en onderkant van de mondopening.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze stiftte haar <w>lippen</w> felrood voor het feestje.",
                            "target_lang_translations": {
                                "ru": "Она накрасила <w>губы</w> ярко-красной помадой перед вечеринкой."
                            }
                        },
                        {
                            "text": "Hij beet op zijn <w>lip</w> om niet hardop te hoeven lachen.",
                            "target_lang_translations": {
                                "ru": "Он прикусил <w>губу</w>, чтобы не рассмеяться вслух."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan iemands lippen hangen",
                        "op de lippen bijten",
                        "de lippen tuiten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясистый край, образующий верхнюю и нижнюю границы рта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "губа",
                                "target_lang_sense_clarification": "часть лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bcdeb10-f167-3297-a417-f7052dbe8cf9",
                    "sense_definition": "Een uitstekend stukje materiaal aan een voorwerp, vaak bedoeld om het te openen of te sluiten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trek voorzichtig aan het <w>lipje</w> om het blikje frisdrank te openen.",
                            "target_lang_translations": {
                                "ru": "Осторожно потяни за <w>язычок</w>, чтобы открыть банку с газировкой."
                            }
                        },
                        {
                            "text": "Het metalen <w>lipje</w> van de rits is helaas afgebroken.",
                            "target_lang_translations": {
                                "ru": "Металлический <w>язычок</w> молнии, к сожалению, отломился."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het lipje van een blikje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступающая деталь предмета, часто предназначенная для его открывания или закрывания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язычок",
                                "target_lang_sense_clarification": "деталь молнии, банки"
                            },
                            {
                                "target_lang_word": "ушко",
                                "target_lang_sense_clarification": "выступающая деталь с отверстием"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e30e58d3-70bc-3a82-866e-a357625789c0",
                    "sense_definition": "Vervoeging van <w>lippen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>lip</w> voorzichtig van de hete thee.",
                            "target_lang_translations": {
                                "ru": "Я осторожно <w>прихлёбываю</w> горячий чай."
                            }
                        },
                        {
                            "text": "<w>Lip</w> jij altijd zo luidruchtig als je soep eet?",
                            "target_lang_translations": {
                                "ru": "Ты всегда так громко <w>прихлёбываешь</w>, когда ешь суп?"
                            }
                        }

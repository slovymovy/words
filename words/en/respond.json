        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d53cf9ab-cd5e-38f0-8d26-953f5b5f0679",
                    "sense_definition": "To say or write something as a reply to a question or statement.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She did not <w>respond</w> to my text message yesterday.",
                            "target_lang_translations": {
                                "ru": "Она не <w>ответила</w> на мое текстовое сообщение вчера."
                            }
                        },
                        {
                            "text": "When the teacher asked a question, the student was eager to <w>respond</w>.",
                            "target_lang_translations": {
                                "ru": "Когда учитель задал вопрос, ученик очень хотел <w>ответить</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "answer",
                        "reply",
                        "retort",
                        "rejoin",
                        "return"
                    ],
                    "common_phrases": [
                        "respond to a question",
                        "respond immediately",
                        "respond to an email"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сказать или написать что-либо в ответ на вопрос или утверждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвечать",
                                "target_lang_sense_clarification": "давать ответ (несов. вид)"
                            },
                            {
                                "target_lang_word": "ответить",
                                "target_lang_sense_clarification": "дать ответ (сов. вид)"
                            }
                        ]
                    }
                },
                {

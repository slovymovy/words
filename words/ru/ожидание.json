    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d290c2aa-ed7b-3ad1-8f75-8a40f9756476",
                    "sense_definition": "Состояние, когда кто-то ждёт наступления какого-либо события или появления кого-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "waiting_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы провели несколько часов в зале <w>ожидания</w> аэропорта.",
                            "target_lang_translations": {
                                "en": "We spent several hours in the airport <w>waiting</w> room."
                            }
                        },
                        {
                            "text": "Томительное <w>ожидание</w> звонка заставило её сильно нервничать.",
                            "target_lang_translations": {
                                "en": "The tedious <w>wait</w> for the call made her very nervous."
                            }
                        }
                    ],
                    "synonyms": [
                        "выжидание"
                    ],
                    "common_phrases": [
                        "зал ожидания",
                        "в ожидании",
                        "режим ожидания",
                        "комната ожидания"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of waiting for an event to occur or for someone to appear."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "waiting",
                                "target_lang_sense_clarification": "The act or period of staying in expectation of something"
                            },
                            {
                                "target_lang_word": "wait",
                                "target_lang_sense_clarification": "A period of time spent waiting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c6c647e-6677-36fc-95de-e0ec910334a7",
                    "sense_definition": "Надежда на что-либо, предположение о том, каким должно быть будущее событие или результат.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "hope_assumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот новый фильм полностью оправдал <w>ожидания</w> зрителей.",
                            "target_lang_translations": {
                                "en": "This new film fully met the viewers' <w>expectations</w>."
                            }
                        },
                        {
                            "text": "Результат работы превзошёл все наши самые смелые <w>ожидания</w>.",
                            "target_lang_translations": {
                                "en": "The result of the work exceeded all our wildest <w>expectations</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "надежда",
                        "упование"
                    ],
                    "common_phrases": [
                        "оправдать ожидания",
                        "превзойти ожидания",
                        "обмануть ожидания",
                        "вопреки ожиданиям"
                    ],
                    "target_lang_definitions": {
                        "en": "A hope for something, or a belief about how a future event or result should turn out."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expectation",
                                "target_lang_sense_clarification": "A strong belief that something will happen or be the case"
                            },
                            {
                                "target_lang_word": "anticipation",
                                "target_lang_sense_clarification": "The feeling of excitement about something that is going to happen"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "30b9b22d-9dbd-30dc-8409-216ec9b17679",
                    "sense_definition": "mający barwę powstałą z połączenia żółci i czerwieni, taką jak skórka owocu pomarańczy",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłam sobie nowy <w>pomarańczowy</w> szalik, który pasuje do mojego płaszcza.",
                            "target_lang_translations": {
                                "ru": "Я купила себе новый <w>оранжевый</w> шарф, который подходит к моему пальто."
                            }
                        },
                        {
                            "text": "Te <w>pomarańczowe</w> ściany strasznie wyblakły i właściwie są już żółte.",
                            "target_lang_translations": {
                                "ru": "Эти <w>оранжевые</w> стены ужасно выцвели и, по сути, стали уже желтыми."
                            }
                        }
                    ],
                    "synonyms": [
                        "oranżowy"
                    ],
                    "common_phrases": [
                        "kolor pomarańczowy",
                        "pomarańczowe światło",
                        "pomarańczowa farba"
                    ],
                    "target_lang_definitions": {
                        "ru": "имеющий цвет, образованный смешением желтого и красного, похожий на кожуру апельсина"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оранжевый",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "апельсиновый",
                                "target_lang_sense_clarification": "оттенок цвета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfcb21fe-a3d8-3306-987a-2e4a9cecfbac",
                    "sense_definition": "zrobiony z owoców pomarańczy lub mający ich smak i zapach",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do śniadania zawsze piję świeży sok <w>pomarańczowy</w>.",
                            "target_lang_translations": {
                                "ru": "За завтраком я всегда пью свежий <w>апельсиновый</w> сок."
                            }
                        },
                        {
                            "text": "Na deser przygotowano galaretkę <w>pomarańczową</w> z owocami i bitą śmietaną.",
                            "target_lang_translations": {
                                "ru": "На десерт приготовили <w>апельсиновое</w> желе с фруктами и взбитыми сливками."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sok pomarańczowy",
                        "skórka pomarańczowa",
                        "dżem pomarańczowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "сделанный из апельсинов или имеющий их вкус и запах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "апельсиновый",
                                "target_lang_sense_clarification": "сделанный из фруктов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6086e12b-980f-327c-9f62-22f791fb8b49",
                    "sense_definition": "związany z drzewem lub krzewem, na którym rosną pomarańcze",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Henryk zasadził w swoim ogródku krzew <w>pomarańczowy</w>, ale nie wytrzymał mroźnej zimy.",
                            "target_lang_translations": {
                                "ru": "Генрик посадил в своем саду <w>апельсиновый</w> куст, но он не пережил морозную зиму."
                            }
                        },
                        {
                            "text": "Podczas wakacji spacerowaliśmy przez pachnący gaj <w>pomarańczowy</w>.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска мы гуляли по ароматной <w>апельсиновой</w> роще."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gaj pomarańczowy",
                        "drzewo pomarańczowe",
                        "sad pomarańczowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "связанный с деревом или кустарником, на котором растут апельсины"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "апельсиновый",
                                "target_lang_sense_clarification": "относящийся к растению"
                            }
                        ]
                    }
                }
            ]
        }
    ]

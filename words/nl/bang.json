{
    "word_family": [
        "bangheid",
        "bangerik",
        "bangelijk",
        "bangmakerij"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2a605e5a-1042-3256-a64f-f3ffb2df9073",
                    "sense_definition": "Gevoelens van angst ervaren voor gevaar, pijn of iets onbekends.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "fear_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn kleine zusje is <w>bang</w> voor grote honden.",
                            "target_lang_translations": {
                                "ru": "Моя младшая сестра <w>боится</w> больших собак.",
                                "en": "My little sister is <w>afraid</w> of big dogs."
                            }
                        },
                        {
                            "text": "Wees maar niet <w>bang</w>, ik zal je helpen.",
                            "target_lang_translations": {
                                "ru": "Не <w>бойся</w>, я тебе помогу.",
                                "en": "Don't be <w>afraid</w>, I will help you."
                            }
                        },
                        {
                            "text": "Hij was te <w>bang</w> om in het donker naar buiten te gaan.",
                            "target_lang_translations": {
                                "ru": "Он был слишком <w>напуган</w>, чтобы выходить на улицу в темноте.",
                                "en": "He was too <w>scared</w> to go outside in the dark."
                            }
                        }
                    ],
                    "synonyms": [
                        "angstig",
                        "bevreesd",
                        "benauwd"
                    ],
                    "antonyms": [
                        "dapper",
                        "moedig"
                    ],
                    "common_phrases": [
                        "bang zijn voor",
                        "niet bang zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий чувство страха перед опасностью, болью или чем-то неизвестным.",
                        "en": "Experiencing feelings of fear regarding danger, pain, or the unknown."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бояться",
                                "target_lang_sense_clarification": "глагол; наиболее частый перевод конструкции 'bang zijn'"
                            },
                            {
                                "target_lang_word": "испуганный",
                                "target_lang_sense_clarification": "прилагательное; описывает состояние страха"
                            },
                            {
                                "target_lang_word": "страшно",
                                "target_lang_sense_clarification": "наречие/категория состояния; используется в безличных предложениях (мне страшно)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "afraid",
                                "target_lang_sense_clarification": "adjective; the most common translation for 'bang zijn'"
                            },
                            {
                                "target_lang_word": "scared",
                                "target_lang_sense_clarification": "adjective; slightly less formal, implies a state of fear"
                            },
                            {
                                "target_lang_word": "frightened",
                                "target_lang_sense_clarification": "adjective; implies being suddenly or intensely affected by fear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc43c610-1d19-3d76-a05f-ae894131aea2",
                    "sense_definition": "Het vermoeden hebben dat er iets vervelends gaat gebeuren; vrezen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fear_apprehension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben <w>bang</w> dat het morgen de hele dag gaat regenen.",
                            "target_lang_translations": {
                                "ru": "Я <w>боюсь</w>, что завтра весь день будет идти дождь.",
                                "en": "I am <w>afraid</w> that it will rain all day tomorrow."
                            }
                        },
                        {
                            "text": "De dokter is <w>bang</w> dat de patiënt niet snel zal herstellen.",
                            "target_lang_translations": {
                                "ru": "Врач <w>опасается</w>, что пациент не скоро поправится.",
                                "en": "The doctor <w>fears</w> that the patient will not recover quickly."
                            }
                        }
                    ],
                    "synonyms": [
                        "beducht"
                    ],
                    "common_phrases": [
                        "ik ben bang dat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предчувствие, что произойдет что-то неприятное; опасение.",
                        "en": "Suspecting that something unpleasant is going to happen; fearing."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бояться",
                                "target_lang_sense_clarification": "выражение сожаления или опасения (боюсь, что...)"
                            },
                            {
                                "target_lang_word": "опасаться",
                                "target_lang_sense_clarification": "предполагать нежелательный исход"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "afraid",
                                "target_lang_sense_clarification": "adjective; used to express polite regret or apprehension (e.g., 'I'm afraid that...')"
                            },
                            {
                                "target_lang_word": "fear",
                                "target_lang_sense_clarification": "verb; formal, to anticipate a negative outcome"
                            },
                            {
                                "target_lang_word": "worried",
                                "target_lang_sense_clarification": "adjective; concerned about a potential outcome"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8aeb795-2fe9-3e79-8704-cb9d8b139428",
                    "sense_definition": "Van nature snel angstig of schrikachtig zijn als karaktereigenschap.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fear_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een <w>bange</w> jongen die nooit risico's durft te nemen.",
                            "target_lang_translations": {
                                "ru": "Он <w>боязливый</w> мальчик, который никогда не решается рисковать.",
                                "en": "He is a <w>timid</w> boy who never dares to take risks."
                            }
                        },
                        {
                            "text": "Doe niet zo <w>bang</w> en spring gewoon in het water.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>трусливым</w> и просто прыгай в воду.",
                                "en": "Don't be so <w>scared</w> and just jump in the water."
                            }
                        }
                    ],
                    "synonyms": [
                        "schrikachtig"
                    ],
                    "antonyms": [
                        "heldhaftig"
                    ],
                    "common_phrases": [
                        "een bange wezel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный к страху или испугу по своей природе (черта характера).",
                        "en": "Being naturally anxious or easily frightened as a character trait."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пугливый",
                                "target_lang_sense_clarification": "легко пугающийся"
                            },
                            {
                                "target_lang_word": "боязливый",
                                "target_lang_sense_clarification": "робкий, склонный к страху"
                            },
                            {
                                "target_lang_word": "трусливый",
                                "target_lang_sense_clarification": "недостаток смелости (негативный оттенок)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "timid",
                                "target_lang_sense_clarification": "adjective; showing a lack of courage or confidence"
                            },
                            {
                                "target_lang_word": "fearful",
                                "target_lang_sense_clarification": "adjective; inclined to be afraid by nature"
                            },
                            {
                                "target_lang_word": "cowardly",
                                "target_lang_sense_clarification": "adjective; lacking courage (negative connotation)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3905a76b-df88-3b06-a286-1bfcac1c5afa",
                    "sense_definition": "Angst oproepend of veroorzakend; vaak gebruikt om een zorgelijke tijd of situatie te beschrijven.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fear_causing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het waren <w>bange</w> dagen voor de inwoners tijdens de zware storm.",
                            "target_lang_translations": {
                                "ru": "Это были <w>тревожные</w> дни для жителей во время сильного шторма.",
                                "en": "They were <w>anxious</w> days for the residents during the heavy storm."
                            }
                        },
                        {
                            "text": "De soldaten beleefden <w>bange</w> momenten aan het front.",
                            "target_lang_translations": {
                                "ru": "Солдаты пережили <w>страшные</w> моменты на фронте.",
                                "en": "The soldiers experienced <w>fearful</w> moments at the front."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bange dagen",
                        "bange momenten"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "vooral in vaste uitdrukkingen over tijdsperiodes"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий страх или беспокойство; часто используется для описания тяжелых времен.",
                        "en": "Causing fear or anxiety; often used to describe a worrying time or situation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тревожный",
                                "target_lang_sense_clarification": "вызывающий беспокойство (о времени, днях)"
                            },
                            {
                                "target_lang_word": "страшный",
                                "target_lang_sense_clarification": "вызывающий сильный страх"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "anxious",
                                "target_lang_sense_clarification": "adjective; causing worry or unease (e.g., anxious days)"
                            },
                            {
                                "target_lang_word": "fearful",
                                "target_lang_sense_clarification": "adjective; causing fear or dread"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
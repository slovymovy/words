                {
                    "sense_id": "6a6cc7aa-4c14-30f4-9e48-3fee9a01ba8c",
                    "sense_definition": "De grootste afmeting van een voorwerp, gemeten van het ene uiteinde naar het andere.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "dimension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>lengte</w> van de tafel is precies twee meter.",
                            "target_lang_translations": {
                                "en": "The <w>length</w> of the table is exactly two meters.",
                                "ru": "<w>Длина</w> стола составляет ровно два метра."
                            }
                        },
                        {
                            "text": "We liepen over de volle <w>lengte</w> van het strand.",
                            "target_lang_translations": {
                                "en": "We walked the full <w>length</w> of the beach.",
                                "ru": "Мы прошли по пляжу во всю его <w>длину</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "afmeting"
                    ],
                    "antonyms": [
                        "breedte"
                    ],
                    "common_phrases": [
                        "in de lengte",
                        "over de hele lengte"
                    ],
                    "target_lang_definitions": {
                        "en": "The greatest dimension of an object, measured from one end to the other.",
                        "ru": "Наибольший размер предмета, измеряемый от одного конца до другого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "length",
                                "target_lang_sense_clarification": "physical dimension"
                            }

                    "sense_id": "c07d1c65-9110-388c-a0da-37330b4a5411",
                    "sense_definition": "De meervoudsvorm van <w>strand</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer liggen de Nederlandse <w>stranden</w> vol met toeristen.",
                            "target_lang_translations": {
                                "en": "In the summer, the Dutch <w>beaches</w> are full of tourists.",
                                "ru": "Летом голландские <w>пляжи</w> переполнены туристами."
                            }
                        },
                        {
                            "text": "Wij houden ervan om lange wandelingen te maken over rustige <w>stranden</w>.",
                            "target_lang_translations": {
                                "en": "We love taking long walks on quiet <w>beaches</w>.",
                                "ru": "Мы любим совершать долгие прогулки по тихим <w>пляжам</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "witte stranden",
                        "de Nederlandse stranden",
                        "schone stranden"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The plural form of 'beach'.",
                        "ru": "Множественное число слова \"пляж\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beaches",
                                "target_lang_sense_clarification": "plural of beach"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пляжи",
                                "target_lang_sense_clarification": "множественное число"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f69693ca-32c2-3347-aa04-21d0dfaeec85",
                    "sense_definition": "Niet meer verder kunnen gaan of mislukken (van plannen, onderhandelingen of reizigers).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "stuck_fail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de staking van het treinpersoneel zijn duizenden reizigers <w>gestrand</w> op het station.",
                            "target_lang_translations": {
                                "en": "Due to the train staff strike, thousands of travelers were <w>stranded</w> at the station.",
                                "ru": "Из-за забастовки персонала поездов тысячи пассажиров <w>застряли</w> на вокзале."
                            }
                        },
                        {
                            "text": "De onderhandelingen tussen de twee partijen zullen waarschijnlijk <w>stranden</w> op dit moeilijke punt.",
                            "target_lang_translations": {
                                "en": "The negotiations between the two parties will probably <w>fail</w> on this difficult point.",
                                "ru": "Переговоры между двумя сторонами, вероятно, <w>зайдут в тупик</w> на этом сложном моменте."
                            }
                        }
                    ],
                    "synonyms": [
                        "vastlopen",
                        "mislukken",
                        "stagneren"
                    ],
                    "antonyms": [
                        "slagen",
                        "voortgaan",
                        "lukken"
                    ],
                    "common_phrases": [
                        "in het zicht van de haven stranden",
                        "gestrande reizigers",
                        "de plannen zijn gestrand"
                    ],
                    "target_lang_definitions": {
                        "en": "To be unable to continue or to fail (of plans, negotiations, or travelers).",
                        "ru": "Не иметь возможности продолжать путь или потерпеть неудачу (о планах, переговорах или путешественниках)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be stranded",
                                "target_lang_sense_clarification": "unable to travel further"
                            },
                            {
                                "target_lang_word": "fail",
                                "target_lang_sense_clarification": "of plans or negotiations"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "застрять",
                                "target_lang_sense_clarification": "о путешественниках"
                            },
                            {
                                "target_lang_word": "потерпеть неудачу",
                                "target_lang_sense_clarification": "о планах или переговорах"
                            },
                            {
                                "target_lang_word": "сорваться",
                                "target_lang_sense_clarification": "о планах или сделках"
                            },
                            {
                                "target_lang_word": "зайти в тупик",
                                "target_lang_sense_clarification": "о переговорах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5769edc-8e45-30f8-a52d-2e2c697c8d37",
                    "sense_definition": "Vast komen te zitten op de grond of een ondiepte (meestal van een vaartuig of zeedier).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "stuck_fail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het grote vrachtschip <w>strandde</w> tijdens de storm op een zandbank.",
                            "target_lang_translations": {
                                "en": "The large cargo ship <w>ran aground</w> on a sandbank during the storm.",
                                "ru": "Большое грузовое судно во время шторма <w>село на мель</w> на песчаной отмели."
                            }
                        },
                        {
                            "text": "Vrijwilligers probeerden de walvis te redden die op de kust was <w>gestrand</w>.",
                            "target_lang_translations": {
                                "en": "Volunteers tried to save the whale that had <w>stranded</w> on the coast.",
                                "ru": "Волонтеры пытались спасти кита, который <w>выбросился</w> на берег."
                            }
                        }
                    ],
                    "synonyms": [
                        "vastlopen",
                        "aan de grond lopen"
                    ],
                    "antonyms": [
                        "vlotkomen",
                        "loskomen"
                    ],
                    "common_phrases": [
                        "op de kust stranden",
                        "een gestrand schip"
                    ],
                    "target_lang_definitions": {
                        "en": "To get stuck on the ground or in shallow water (usually of a vessel or sea animal).",
                        "ru": "Застрять на грунте или на мели (обычно о судне или морском животном)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run aground",
                                "target_lang_sense_clarification": "of a ship"
                            },
                            {
                                "target_lang_word": "strand",
                                "target_lang_sense_clarification": "of a sea animal or ship"
                            },
                            {
                                "target_lang_word": "run ashore",
                                "target_lang_sense_clarification": "onto the shore"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сесть на мель",
                                "target_lang_sense_clarification": "о корабле"
                            },
                            {
                                "target_lang_word": "выброситься на берег",
                                "target_lang_sense_clarification": "о морских животных"
                            }
                        ]
                    }
                }
            ]
        }
    ]

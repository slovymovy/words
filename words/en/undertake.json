{
    "word_family": [
        "undertaking",
        "undertaker"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "be1ac42e-d7ee-38f4-b509-fe455752b741",
                    "sense_definition": "To take upon oneself; to start or embark on a specific task or responsibility.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "start_task",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university decided to <w>undertake</w> a major research project.",
                            "target_lang_translations": {
                                "ru": "Университет решил <w>предпринять</w> крупный исследовательский проект."
                            }
                        },
                        {
                            "text": "She is willing to <w>undertake</w> the responsibility of leading the new team.",
                            "target_lang_translations": {
                                "ru": "Она готова <w>взять на себя</w> ответственность за руководство новой командой."
                            }
                        }
                    ],
                    "synonyms": [
                        "take on",
                        "tackle",
                        "set about"
                    ],
                    "common_phrases": [
                        "undertake a project",
                        "undertake a task",
                        "undertake research",
                        "undertake responsibility"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать на себя (ответственность, задачу) или начинать выполнение чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предпринимать",
                                "target_lang_sense_clarification": "начинать делать что-либо значительное"
                            },
                            {
                                "target_lang_word": "брать на себя",
                                "target_lang_sense_clarification": "принимать ответственность или задачу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f39a4a7-7e35-3265-9a2d-05ec6b3ae638",
                    "sense_definition": "To commit oneself to an obligation or promise to do something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "promise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer <w>undertook</w> to represent the client free of charge.",
                            "target_lang_translations": {
                                "ru": "Адвокат <w>обязался</w> представлять интересы клиента бесплатно."
                            }
                        },
                        {
                            "text": "He <w>undertook</w> to finish the construction work by the end of the month.",
                            "target_lang_translations": {
                                "ru": "Он <w>взял на себя обязательство</w> закончить строительные работы к концу месяца."
                            }
                        }
                    ],
                    "synonyms": [
                        "set about",
                        "take on",
                        "tackle"
                    ],
                    "common_phrases": [
                        "undertake to do something"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать на себя обязательство или давать обещание выполнить что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обязываться",
                                "target_lang_sense_clarification": "давать официальное обещание сделать что-то"
                            },
                            {
                                "target_lang_word": "ручаться",
                                "target_lang_sense_clarification": "гарантировать выполнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8480906-b20f-31e5-957e-fc341692bf6f",
                    "sense_definition": "To pass a slower moving vehicle on the side closest to the curb rather than the side closest to oncoming traffic.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "driving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is generally illegal to <w>undertake</w> another car on the motorway.",
                            "target_lang_translations": {
                                "ru": "Как правило, запрещено <w>обгонять слева</w> другую машину на автомагистрали."
                            }
                        },
                        {
                            "text": "The driver tried to <w>undertake</w> the lorry but realized it was too dangerous.",
                            "target_lang_translations": {
                                "ru": "Водитель попытался <w>обогнать</w> грузовик <w>слева</w>, но понял, что это слишком опасно."
                            }
                        }
                    ],
                    "antonyms": [
                        "overtake"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обгонять транспортное средство со стороны обочины (обычно слева в странах с левосторонним движением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обгонять слева",
                                "target_lang_sense_clarification": "со стороны обочины (в британском контексте)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "746b047f-2b65-3837-89cb-61e16d9e2a01",
                    "sense_definition": "The act of passing slower traffic on the curbside rather than on the side closest to oncoming traffic.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "driving_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer stopped him for a dangerous <w>undertake</w> on the highway.",
                            "target_lang_translations": {
                                "ru": "Полицейский остановил его за опасный <w>обгон слева</w> на шоссе."
                            }
                        },
                        {
                            "text": "Performing an <w>undertake</w> at high speed can lead to serious accidents.",
                            "target_lang_translations": {
                                "ru": "Выполнение <w>обгона слева</w> на высокой скорости может привести к серьезным авариям."
                            }
                        }
                    ],
                    "antonyms": [
                        "overtake"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Маневр обгона транспортного средства со стороны обочины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обгон слева",
                                "target_lang_sense_clarification": "маневр со стороны обочины"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
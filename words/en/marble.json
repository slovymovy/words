                },
                {
                    "sense_id": "7349314b-b460-30fd-a823-e7d7db1b8b2e",
                    "sense_definition": "A statutory town in Gunnison County, Colorado.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "names",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They planned a summer trip to visit <w>Marble</w>, Colorado.",
                            "target_lang_translations": {
                                "ru": "Они запланировали летнюю поездку, чтобы посетить <w>Марбл</w>, штат Колорадо."

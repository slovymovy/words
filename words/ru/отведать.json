    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "64c62aee-789e-38d6-9aa5-7355d4455d0a",
                    "sense_definition": "Съесть или выпить немного чего-либо, чтобы узнать вкус или насытиться.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "food_tasting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гости сели за стол, чтобы <w>отведать</w> праздничный пирог.",
                            "target_lang_translations": {
                                "en": "The guests sat at the table to <w>taste</w> the festive pie."
                            }
                        },
                        {
                            "text": "Вы обязательно должны <w>отведать</w> местного вина.",
                            "target_lang_translations": {
                                "en": "You absolutely must <w>try</w> the local wine."
                            }
                        }
                    ],
                    "synonyms": [
                        "попробовать"
                    ],
                    "common_phrases": [
                        "отведать блюдо",
                        "отведать угощение",
                        "отведать хлеба-соли"
                    ],
                    "target_lang_definitions": {
                        "en": "To eat or drink a small amount of something to assess its taste or enjoy it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "taste",
                                "target_lang_sense_clarification": "to perceive the flavor of something"
                            },
                            {
                                "target_lang_word": "sample",
                                "target_lang_sense_clarification": "to try a small amount of food or drink"
                            },
                            {
                                "target_lang_word": "try",
                                "target_lang_sense_clarification": "to eat or drink something to see if one likes it"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3eea2610-3f68-39e3-a635-106e52b7d8b0",
                    "sense_definition": "Испытать или узнать что-либо на собственном опыте (обычно что-то неприятное, тяжёлое).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "experience_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За свою долгую жизнь он успел <w>отведать</w> немало горя.",
                            "target_lang_translations": {
                                "en": "During his long life, he managed to <w>taste</w> plenty of sorrow."
                            }
                        },
                        {
                            "text": "Враг побоялся снова <w>отведать</w> силы нашего оружия.",
                            "target_lang_translations": {
                                "en": "The enemy was afraid to <w>taste</w> the power of our weapons again."
                            }
                        }
                    ],
                    "synonyms": [
                        "испытать",
                        "познать"
                    ],
                    "common_phrases": [
                        "отведать горя",
                        "отведать лиха",
                        "отведать кнута"
                    ],
                    "target_lang_definitions": {
                        "en": "To experience or learn something through personal experience (usually something unpleasant or difficult)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "experience",
                                "target_lang_sense_clarification": "to undergo or feel something"
                            },
                            {
                                "target_lang_word": "taste",
                                "target_lang_sense_clarification": "figurative: to have a brief experience of something"
                            },
                            {
                                "target_lang_word": "get a taste of",
                                "target_lang_sense_clarification": "idiomatic: to experience something briefly"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
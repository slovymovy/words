    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ce427bd6-9a57-32a8-b117-df65a4ace962",
                    "sense_definition": "Simple past and past participle of <w>tie</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>tied</w> her hair back with a red ribbon before starting work.",
                            "target_lang_translations": {
                                "ru": "Она <w>завязала</w> волосы сзади красной лентой перед началом работы."
                            }
                        },
                        {
                            "text": "He <w>tied</w> the heavy box securely to the roof of his car.",
                            "target_lang_translations": {
                                "ru": "Он надежно <w>привязал</w> тяжелую коробку к крыше своей машины."
                            }

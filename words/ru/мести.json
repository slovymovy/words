            "pos": "verb",
            "senses": [
                {
                    "sense_id": "993ae940-95f7-35e6-9e3e-4faf3405fa59",
                    "sense_definition": "Очищать поверхность от мусора, пыли или снега с помощью метлы или щётки.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дворник выходит рано утром, чтобы <w>мести</w> двор.",
                            "target_lang_translations": {
                                "en": "The janitor goes out early in the morning to <w>sweep</w> the yard."
                            }
                        },
                        {
                            "text": "Я не люблю <w>мести</w> пол, мне больше нравится пылесосить.",
                            "target_lang_translations": {
                                "en": "I don't like to <w>sweep</w> the floor; I prefer vacuuming."
                            }
                        }
                    ],
                    "synonyms": [
                        "подметать"
                    ],
                    "antonyms": [
                        "сорить"
                    ],
                    "common_phrases": [
                        "мести пол",
                        "мести двор",
                        "мести улицу"
                    ],
                    "target_lang_definitions": {
                        "en": "To clean a surface of debris, dust, or snow using a broom or brush."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sweep",
                                "target_lang_sense_clarification": "to clean with a broom"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0982b4e6-2369-3a56-b0f5-1828c899b883",
                    "sense_definition": "Сильно дуть, перенося снег или пыль (о ветре, вьюге).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой здесь часто <w>метёт</w>, и дороги становятся непроходимыми.",
                            "target_lang_translations": {
                                "en": "In winter, the snow often <w>sweeps</w> across here, and the roads become impassable."
                            }
                        },
                        {
                            "text": "Весь день на улице <w>мело</w>, поэтому мы остались дома.",
                            "target_lang_translations": {
                                "en": "The wind <w>swept</w> snow outside all day, so we stayed home."
                            }
                        }
                    ],
                    "common_phrases": [
                        "на улице метёт",
                        "снег метёт"
                    ],
                    "target_lang_definitions": {
                        "en": "To blow strongly, carrying snow or dust (of wind, blizzard)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sweep",
                                "target_lang_sense_clarification": "of wind or snow moving forcefully"
                            },
                            {
                                "target_lang_word": "blow",
                                "target_lang_sense_clarification": "of wind carrying snow"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33251715-6793-3ccd-aaa6-6a5045f623ce",
                    "sense_definition": "Волочить одеждой по поверхности, касаясь её.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её длинное вечернее платье <w>мело</w> по паркету.",
                            "target_lang_translations": {
                                "en": "Her long evening gown <w>swept</w> across the parquet."
                            }
                        },
                        {
                            "text": "Старайся не <w>мести</w> подолом по грязной земле.",
                            "target_lang_translations": {
                                "en": "Try not to <w>drag</w> the hem along the dirty ground."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To drag clothing along a surface, touching it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sweep",
                                "target_lang_sense_clarification": "to brush against or drag along"
                            },
                            {
                                "target_lang_word": "trail",
                                "target_lang_sense_clarification": "to drag behind"
                            }
                        ]
                    }
                }
            ]
        },

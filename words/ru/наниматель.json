    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4f975eae-d6d1-3a42-b32f-fbea345341c1",
                    "sense_definition": "Лицо или организация, которые нанимают кого-либо на работу и платят за неё деньги.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "employment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Добросовестный <w>наниматель</w> всегда вовремя выплачивает заработную плату своим сотрудникам.",
                            "target_lang_translations": {
                                "en": "A conscientious <w>employer</w> always pays salaries to their employees on time."
                            }
                        },
                        {
                            "text": "Мой потенциальный <w>наниматель</w> задавал много вопросов о моём прошлом опыте работы.",
                            "target_lang_translations": {
                                "en": "My potential <w>employer</w> asked many questions about my past work experience."
                            }
                        }
                    ],
                    "synonyms": [
                        "работодатель"
                    ],
                    "antonyms": [
                        "работник"
                    ],
                    "common_phrases": [
                        "потенциальный наниматель",
                        "требовательный наниматель",
                        "поиск нанимателя"
                    ],
                    "target_lang_definitions": {
                        "en": "A person or organization that hires someone for work and pays money for it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "employer",
                                "target_lang_sense_clarification": "person or company hiring workers"
                            },
                            {
                                "target_lang_word": "hirer",
                                "target_lang_sense_clarification": "one who hires"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e0e7555-f247-3df1-992c-396d76a84746",
                    "sense_definition": "Сторона в договоре, которая получает жильё или имущество во временное пользование за плату.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "renting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По условиям договора <w>наниматель</w> обязан оплачивать коммунальные услуги самостоятельно.",
                            "target_lang_translations": {
                                "en": "Under the terms of the contract, the <w>tenant</w> is obliged to pay utility bills independently."
                            }
                        },
                        {
                            "text": "Если <w>наниматель</w> нарушает правила проживания, собственник имеет право его выселить.",
                            "target_lang_translations": {
                                "en": "If the <w>tenant</w> violates the residency rules, the owner has the right to evict them."
                            }
                        }
                    ],
                    "synonyms": [
                        "арендатор",
                        "съёмщик"
                    ],
                    "antonyms": [
                        "владелец"
                    ],
                    "common_phrases": [
                        "наниматель жилого помещения",
                        "права нанимателя",
                        "ответственный наниматель"
                    ],
                    "target_lang_definitions": {
                        "en": "A party to a contract receiving housing or property for temporary use for a fee."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tenant",
                                "target_lang_sense_clarification": "person renting housing"
                            },
                            {
                                "target_lang_word": "lessee",
                                "target_lang_sense_clarification": "legal term for someone renting property"
                            }
                        ]
                    }
                }
            ]
        }
    ]

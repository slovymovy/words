    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6ddab68c-d8b4-3e3f-9d6b-9da0e7d9985c",
                    "sense_definition": "A participant in a competition, especially in athletics.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "participant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The youngest <w>competitor</w> in the race finished in third place.",
                            "target_lang_translations": {
                                "ru": "Самый юный <w>участник</w> гонки финишировал третьим."
                            }
                        },
                        {
                            "text": "She is a tough <w>competitor</w> who never gives up during a match.",
                            "target_lang_translations": {
                                "ru": "Она серьезный <w>соперник</w>, который никогда не сдается во время матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "contestant"
                    ],
                    "common_phrases": [
                        "fellow competitor",
                        "tough competitor",
                        "young competitor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участник соревнования, состязания (особенно спортивного)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участник",
                                "target_lang_sense_clarification": "тот, кто участвует в соревновании"
                            },
                            {
                                "target_lang_word": "соперник",
                                "target_lang_sense_clarification": "противник в спортивном состязании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ecb04f7-9232-3ad8-ae4d-b098aee46c6c",
                    "sense_definition": "A person or organization against whom one is competing.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "rival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our main <w>competitor</w> has just launched a similar product at a lower price.",
                            "target_lang_translations": {
                                "ru": "Наш главный <w>конкурент</w> только что выпустил похожий продукт по более низкой цене."
                            }
                        },
                        {
                            "text": "The company bought its largest <w>competitor</w> to dominate the market.",
                            "target_lang_translations": {
                                "ru": "Компания купила своего крупнейшего <w>конкурента</w>, чтобы доминировать на рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "adversary",
                        "opponent"
                    ],
                    "common_phrases": [
                        "direct competitor",
                        "main competitor",
                        "market competitor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или организация, соперничающие с кем-либо (обычно в бизнесе или борьбе за первенство)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конкурент",
                                "target_lang_sense_clarification": "в бизнесе, торговле"
                            },
                            {
                                "target_lang_word": "соперник",
                                "target_lang_sense_clarification": "противник в борьбе за что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b1f74b1-2fd1-3071-96cf-5721d70ea05c",
                    "sense_definition": "Partner, associate, one working with another toward a common goal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He acted as my loyal <w>competitor</w> in the governance of the realm.",
                            "target_lang_translations": {
                                "ru": "Он действовал как мой верный <w>соратник</w> в управлении королевством."
                            }
                        },
                        {
                            "text": "The king summoned his trusted <w>competitor</w> to discuss the treaty.",
                            "target_lang_translations": {
                                "ru": "Король призвал своего доверенного <w>сподвижника</w> для обсуждения договора."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical usage found in older texts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Партнер, соратник, человек, работающий вместе с другим ради общей цели (устаревшее)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соратник",
                                "target_lang_sense_clarification": "устаревшее значение"
                            },
                            {
                                "target_lang_word": "сподвижник",
                                "target_lang_sense_clarification": "устаревшее значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

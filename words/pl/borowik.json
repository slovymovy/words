            "senses": [
                {
                    "sense_id": "dd62c5d1-7d11-3c06-b207-55e569bbdb28",
                    "sense_definition": "Jadalny grzyb leśny o brązowym kapeluszu i grubym trzonie, bardzo ceniony w kuchni polskiej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj znalazłam w lesie dorodnego <w>borowika</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера я нашла в лесу крупный <w>боровик</w>."
                            }
                        },
                        {
                            "text": "Do świątecznego bigosu zawsze dodajemy suszone <w>borowiki</w>.",
                            "target_lang_translations": {
                                "ru": "В праздничный бигос мы всегда добавляем сушёные <w>белые грибы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "prawdziwek"
                    ],
                    "common_phrases": [
                        "suszone borowiki",
                        "sos z borowików",
                        "zbierać borowiki",
                        "zupa z borowików"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобный лесной гриб с коричневой шляпкой и толстой ножкой, очень ценимый в польской кухне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "белый гриб",
                                "target_lang_sense_clarification": "вид гриба"
                            },
                            {
                                "target_lang_word": "боровик",
                                "target_lang_sense_clarification": "вид гриба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec574ae1-2908-3075-9dff-eac008fa747a",
                    "sense_definition": "Funkcjonariusz Biura Ochrony Rządu (instytucji zajmującej się ochroną najważniejszych osób w państwie).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy <w>borowik</w> musi przejść bardzo trudne szkolenie fizyczne.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>сотрудник БОР</w> должен пройти очень сложную физическую подготовку."
                            }
                        },
                        {
                            "text": "Podczas wizyty prezydenta towarzyszyło mu kilku uzbrojonych <w>borowików</w>.",
                            "target_lang_translations": {
                                "ru": "Во время визита президента его сопровождали несколько вооружённых <w>сотрудников БОР</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "borowiec",
                        "BOR-owiec"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сотрудник Бюро охраны правительства (учреждения, занимающегося охраной высших должностных лиц)."
                    },

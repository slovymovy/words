    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d482cf50-15e5-3281-9cd9-2e679d1cfa7b",
                    "sense_definition": "A male given name from the Germanic languages, equivalent to English Charles.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My brother <w>Carl</w> is coming to visit us next week.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>Карл</w> приедет навестить нас на следующей неделе."
                            }
                        },
                        {
                            "text": "Please introduce yourself to <w>Carl</w> when you arrive at the office.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, представься <w>Карлу</w>, когда приедешь в офис."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Uncle Carl",
                        "Prince Carl"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя германского происхождения, эквивалент английского имени Чарльз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Карл",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4f78e4e0-fadd-3f51-b20f-8da2170f3f3d",
                    "sense_definition": "A rude, rustic man or a person of low social status.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "rustic_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wealthy merchant looked down upon the poor <w>carl</w> working in the fields.",
                            "target_lang_translations": {
                                "ru": "Богатый купец свысока смотрел на бедного <w>простолюдина</w>, работающего в поле."
                            }
                        },
                        {
                            "text": "He behaved like a <w>carl</w> at the dinner, ignoring all proper etiquette.",
                            "target_lang_translations": {
                                "ru": "За ужином он вел себя как <w>мужлан</w>, игнорируя этикет."
                            }
                        }
                    ],
                    "synonyms": [
                        "churl",
                        "boor",
                        "rustic",
                        "yokel",
                        "peasant",
                        "hick"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубый, неотесанный мужчина или человек низкого социального статуса (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мужлан",
                                "target_lang_sense_clarification": "грубый мужчина"
                            },
                            {
                                "target_lang_word": "простолюдин",
                                "target_lang_sense_clarification": "человек низкого происхождения"
                            },
                            {
                                "target_lang_word": "грубиян",
                                "target_lang_sense_clarification": "невежливый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e83fa90-46ba-3eb6-b657-44c01aa2f44a",
                    "sense_definition": "A stingy person who is unwilling to spend money.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "stingy_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't be such a <w>carl</w>; you can afford to buy a round of drinks.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>скрягой</w>, ты можешь позволить себе угостить всех выпивкой."
                            }
                        },
                        {
                            "text": "The old <w>carl</w> refused to donate even a penny to the local charity.",
                            "target_lang_translations": {
                                "ru": "Старый <w>скупердяй</w> отказался пожертвовать даже пенни местной благотворительной организации."
                            }
                        }
                    ],
                    "synonyms": [
                        "miser",
                        "skinflint",
                        "tightwad",
                        "cheapskate",
                        "scrooge"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Northern English or Scottish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скупой человек, не желающий тратить деньги (диалектное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скряга",
                                "target_lang_sense_clarification": "жадный человек"
                            },
                            {
                                "target_lang_word": "скупердяй",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "жмот",
                                "target_lang_sense_clarification": "грубое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9973f5c2-9819-3e69-a6e8-2bc3fe610fa9",
                    "sense_definition": "A student at Carleton College in Minnesota.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every <w>Carl</w> is expected to participate in the campus traditions during the first week.",
                            "target_lang_translations": {
                                "ru": "Ожидается, что каждый <w>студент Карлтонского колледжа</w> примет участие в кампусных традициях в течение первой недели."
                            }
                        },
                        {
                            "text": "She met another <w>Carl</w> while traveling abroad and they instantly bonded over their college days.",
                            "target_lang_translations": {
                                "ru": "Путешествуя за границей, она встретила другого <w>студента Карлтонского колледжа</w>, и они сразу же сдружились, вспоминая студенческие годы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "college slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Студент Карлтонского колледжа в Миннесоте (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "студент Карлтонского колледжа",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        },

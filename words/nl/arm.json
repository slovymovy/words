                    "sense_id": "894a3001-3c4a-3b42-a3e0-85bb1249a73a",
                    "sense_definition": "Lichaamsdeel dat van de schouder tot de hand loopt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb gisteren mijn <w>arm</w> gebroken tijdens het sporten.",
                            "target_lang_translations": {
                                "en": "I broke my <w>arm</w> yesterday while exercising.",
                                "ru": "Я вчера сломал <w>руку</w> во время занятий спортом."
                            }
                        },
                        {
                            "text": "Zij droeg het slapende kind voorzichtig in haar <w>armen</w>.",
                            "target_lang_translations": {
                                "en": "She carried the sleeping child carefully in her <w>arms</w>.",
                                "ru": "Она осторожно несла спящего ребенка на <w>руках</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de armen sluiten",
                        "een arm geven",
                        "met open armen"
                    ],
                    "target_lang_definitions": {
                        "en": "Body part running from the shoulder to the hand.",
                        "ru": "Часть тела от плеча до кисти."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "22e701e0-b34a-3f38-909d-9f8066243d7c",
                    "sense_definition": "Powszechne uznanie, wielki rozgłos i szacunek, jakimi cieszy się ktoś lub coś; bycie znanym przez wielu ludzi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody aktor szybko zdobył międzynarodową <w>sławę</w> dzięki swojej debiutanckiej roli.",
                            "target_lang_translations": {
                                "ru": "Молодой актёр быстро завоевал международную <w>славу</w> благодаря своей дебютной роли."
                            }
                        },
                        {
                            "text": "Jego <w>sława</w> jako wybitnego naukowca dotarła do najdalszych zakątków świata.",
                            "target_lang_translations": {
                                "ru": "Его <w>слава</w> как выдающегося учёного дошла до самых отдалённых уголков мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozgłos",
                        "popularność",
                        "fama"
                    ],
                    "antonyms": [
                        "zapomnienie",
                        "niesława"
                    ],
                    "common_phrases": [
                        "zyskać sławę",
                        "okryć się sławą",
                        "szczyt sławy",
                        "pragnienie sławy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Всеобщее признание, широкая известность и уважение, которыми пользуется кто-либо или что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слава",
                                "target_lang_sense_clarification": "почёт, всеобщее признание"
                            },
                            {
                                "target_lang_word": "известность",
                                "target_lang_sense_clarification": "популярность, состояние быть известным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ef3e20f-8425-3847-9034-13dc2893f8b1",
                    "sense_definition": "Osoba bardzo znana, ceniona i popularna w określonym środowisku lub dziedzinie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na uroczystym bankiecie pojawiły się największe <w>sławy</w> polskiego kina.",
                            "target_lang_translations": {
                                "ru": "На торжественном банкете появились крупнейшие <w>знаменитости</w> польского кино."
                            }
                        },
                        {
                            "text": "To miasto często odwiedzają sportowe <w>sławy</w> z całego świata.",
                            "target_lang_translations": {
                                "ru": "Этот город часто посещают спортивные <w>знаменитости</w> со всего мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "znakomitość",
                        "postać"
                    ],
                    "common_phrases": [
                        "światowe sławy",
                        "galeria sław",
                        "sławy sportu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень известный, уважаемый и популярный человек в определённой среде или области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знаменитость",
                                "target_lang_sense_clarification": "известная личность"
                            },
                            {
                                "target_lang_word": "звезда",
                                "target_lang_sense_clarification": "выдающийся деятель искусства или спорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2694a797-afd2-319c-b7f7-9dcc4ea3a46e",
                    "sense_definition": "Opinia, jaką ludzie mają o kimś lub o czymś; dobre lub złe imię.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta dzielnica ma niestety bardzo złą <w>sławę</w> wśród mieszkańców miasta.",
                            "target_lang_translations": {
                                "ru": "У этого района, к сожалению, очень дурная <w>слава</w> среди жителей города."
                            }
                        },
                        {
                            "text": "Przez lata pracował ciężko na swoją dobrą <w>sławę</w> jako uczciwy prawnik.",
                            "target_lang_translations": {
                                "ru": "Годами он тяжело работал на свою добрую <w>славу</w> честного юриста."
                            }
                        }
                    ],
                    "synonyms": [
                        "reputacja"
                    ],
                    "antonyms": [
                        "niesława",
                        "infamia"
                    ],
                    "common_phrases": [
                        "dobra sława",
                        "zła sława",
                        "cieszyć się sławą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мнение, которое люди имеют о ком-то или о чём-то; доброе или дурное имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слава",
                                "target_lang_sense_clarification": "репутация (часто в сочетании с прилагательными)"
                            },
                            {
                                "target_lang_word": "репутация",
                                "target_lang_sense_clarification": "общественное мнение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "761b129d-5dca-3880-bf94-4b6a382e83c8",
                    "sense_definition": "Żeńskie imię słowiańskie, funkcjonujące samodzielnie lub jako zdrobnienie imion zakończonych na -sława.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja ciocia <w>Sława</w> zawsze przyjeżdża do nas na święta Bożego Narodzenia.",
                            "target_lang_translations": {
                                "ru": "Моя тётя <w>Слава</w> всегда приезжает к нам на Рождество."
                            }
                        },
                        {
                            "text": "Imię <w>Sława</w> nie jest obecnie zbyt często nadawane małym dziewczynkom.",
                            "target_lang_translations": {
                                "ru": "Имя <w>Слава</w> сейчас не слишком часто дают маленьким девочкам."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское славянское имя, употребляемое самостоятельно или как уменьшительная форма имён, оканчивающихся на -слава."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Слава",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4935aaed-68d3-3198-804b-120108389c0f",
                    "sense_definition": "Miasto w zachodniej Polsce, w województwie lubuskim, znane z turystyki.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W te wakacje planujemy odwiedzić miasto <w>Sława</w> i odpocząć nad jeziorem.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы планируем посетить город <w>Слава</w> и отдохнуть на озере."
                            }
                        },
                        {
                            "text": "<w>Sława</w> przyciąga latem wielu turystów spragnionych wypoczynku nad wodą.",
                            "target_lang_translations": {
                                "ru": "<w>Слава</w> летом привлекает множество туристов, жаждущих отдыха у воды."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город на западе Польши, в Любушском воеводстве, известный туризмом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Слава",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        }
    ]

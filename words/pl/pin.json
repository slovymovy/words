    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6be90327-eb40-3a95-848f-de607cbd68a4",
                    "sense_definition": "Krótki kod cyfrowy służący do identyfikacji i autoryzacji dostępu, na przykład do karty płatniczej, telefonu lub usługi elektronicznej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "security_code",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz wpisać poprawny <w>pin</w>, aby wypłacić gotówkę z bankomatu.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно ввести правильный <w>ПИН-код</w>, чтобы снять наличные в банкомате."
                            }
                        },
                        {
                            "text": "Zablokowałem sobie telefon, ponieważ trzy razy podałem błędny <w>pin</w>.",
                            "target_lang_translations": {
                                "ru": "Я заблокировал свой телефон, так как трижды ввел неверный <w>ПИН-код</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kod",
                        "hasło"
                    ],
                    "common_phrases": [
                        "wpisać pin",
                        "zmienić pin",
                        "zapomnieć pinu",
                        "błędny pin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий цифровой код, используемый для идентификации и авторизации доступа, например, к банковской карте, телефону или электронной услуге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ПИН-код",
                                "target_lang_sense_clarification": "код доступа"
                            },
                            {
                                "target_lang_word": "пин",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c83380b-3a3e-3c22-98b7-0f241d02e8ad",
                    "sense_definition": "Metalowy styk, bolec lub nóżka w złączach elektronicznych i układach scalonych, służąca do przesyłania sygnałów elektrycznych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "electronics_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas montażu procesora trzeba bardzo uważać, aby nie wygiąć żadnego <w>pinu</w>.",
                            "target_lang_translations": {
                                "ru": "При установке процессора нужно быть очень осторожным, чтобы не погнуть ни одной <w>ножки</w>."
                            }
                        },
                        {
                            "text": "Wtyczka nie pasuje do gniazda, ponieważ ma inną liczbę <w>pinów</w>.",
                            "target_lang_translations": {
                                "ru": "Штекер не подходит к гнезду, так как у него другое количество <w>контактов</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "styk",
                        "nóżka",
                        "bolec",
                        "końcówka"
                    ],
                    "common_phrases": [
                        "wygięty pin",
                        "liczba pinów",
                        "złącze pinowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлический контакт, штырек или ножка в электронных разъемах и микросхемах, служащий для передачи электрических сигналов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контакт",
                                "target_lang_sense_clarification": "в электронике"
                            },
                            {
                                "target_lang_word": "ножка",
                                "target_lang_sense_clarification": "микросхемы"
                            },
                            {
                                "target_lang_word": "пин",
                                "target_lang_sense_clarification": "технический сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
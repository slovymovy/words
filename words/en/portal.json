    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b61c3424-9794-38ae-82ef-3847e37e248c",
                    "sense_definition": "A website or page that acts as a main entrance or starting point to other websites, services, or data on the Internet.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "digital_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Students must log in to the university <w>portal</w> to access their course materials and grades.",
                            "target_lang_translations": {
                                "ru": "Студенты должны войти в университетский <w>портал</w>, чтобы получить доступ к учебным материалам и оценкам."
                            }
                        },
                        {
                            "text": "The company launched a new customer <w>portal</w> to handle support requests more efficiently.",
                            "target_lang_translations": {
                                "ru": "Компания запустила новый клиентский <w>портал</w> для более эффективной обработки запросов в службу поддержки."
                            }
                        }
                    ],
                    "synonyms": [
                        "gateway",
                        "hub",
                        "site"
                    ],
                    "common_phrases": [
                        "web portal",
                        "online portal",
                        "customer portal",
                        "student portal"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Веб-сайт или страница, служащая главной точкой входа к другим ресурсам, сервисам или данным в Интернете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портал",
                                "target_lang_sense_clarification": "веб-сайт"
                            },
                            {
                                "target_lang_word": "интернет-портал",
                                "target_lang_sense_clarification": "веб-ресурс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00a8e83e-96b4-3dea-a048-63b3468e34e5",
                    "sense_definition": "An entrance, entry point, or means of entry, especially one that is grand, imposing, or architecturally significant.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_entrance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The massive stone <w>portal</w> of the cathedral was decorated with intricate carvings.",
                            "target_lang_translations": {
                                "ru": "Массивный каменный <w>портал</w> собора был украшен сложной резьбой."
                            }
                        },
                        {
                            "text": "We passed through the heavy oak <w>portal</w> and entered the great hall of the castle.",
                            "target_lang_translations": {
                                "ru": "Мы прошли через тяжелый дубовый <w>портал</w> и вошли в большой зал замка."
                            }
                        }
                    ],
                    "synonyms": [
                        "entrance",
                        "gateway",
                        "entryway",
                        "doorway"
                    ],
                    "antonyms": [
                        "exit"
                    ],
                    "common_phrases": [
                        "stone portal",
                        "main portal",
                        "arched portal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вход, особенно парадный, величественный или архитектурно значимый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портал",
                                "target_lang_sense_clarification": "архитектура"
                            },
                            {
                                "target_lang_word": "главный вход",
                                "target_lang_sense_clarification": "парадный вход"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a92b51f-d975-3f34-ad79-18781ef6d262",
                    "sense_definition": "A magical or technological aperture or doorway leading to another location, dimension, or period in time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wizard opened a glowing <w>portal</w> that allowed the heroes to escape to a different realm.",
                            "target_lang_translations": {
                                "ru": "Волшебник открыл светящийся <w>портал</w>, который позволил героям сбежать в другое царство."
                            }
                        },
                        {
                            "text": "In the movie, the scientists built a machine to generate a <w>portal</w> through time.",
                            "target_lang_translations": {
                                "ru": "В фильме ученые построили машину для создания <w>портала</w> сквозь время."
                            }
                        }
                    ],
                    "synonyms": [
                        "wormhole",
                        "vortex",
                        "gateway"
                    ],
                    "common_phrases": [
                        "time portal",
                        "open a portal",
                        "magic portal"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "fiction"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Магический или технологический проход, ведущий в другое место, измерение или время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портал",
                                "target_lang_sense_clarification": "фантастика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d798c06-028f-33dd-ab31-e7a4ab2e9de1",
                    "sense_definition": "A database and compliance tool used in US college sports to facilitate student-athletes who wish to transfer to a different school.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports_admin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Several key players entered the transfer <w>portal</w> after the head coach announced his resignation.",
                            "target_lang_translations": {
                                "ru": "Несколько ключевых игроков вышли на трансферный <w>портал</w> после того, как главный тренер объявил об отставке."
                            }
                        },
                        {
                            "text": "Coaches now spend much of the off-season recruiting talent from the <w>portal</w>.",
                            "target_lang_translations": {
                                "ru": "Тренеры теперь тратят большую часть межсезонья на набор талантов через <w>портал</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "transfer portal",
                        "enter the portal"
                    ],
                    "traits": [

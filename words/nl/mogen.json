{
    "word_family": [
        "vermogen",
        "mogelijk",
        "mogelijkheid",
        "mogendheid"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07fd183b-9175-35bd-9b29-d1ec6bd2d11c",
                    "sense_definition": "Toestemming hebben om iets te doen of te laten; toegestaan zijn.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je <mag> hier niet roken.",
                            "target_lang_translations": {
                                "ru": "Здесь <нельзя> курить.",
                                "en": "You <w>may</w> not smoke here."
                            }
                        },
                        {
                            "text": "Wij <mochten> gisteren laat opblijven.",
                            "target_lang_translations": {
                                "ru": "Вчера нам <разрешили> не ложиться спать допоздна.",
                                "en": "We <w>were allowed to</w> stay up late yesterday."
                            }
                        },
                        {
                            "text": "Dat <mag> niet van de politie.",
                            "target_lang_translations": {
                                "ru": "Полиция этого <не разрешает>.",
                                "en": "That <w>is not allowed</w> by the police."
                            }
                        }
                    ],
                    "synonyms": [
                        "toegestaan zijn"
                    ],
                    "common_phrases": [
                        "dat mag niet",
                        "mag ik even",
                        "niet mogen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь разрешение что-либо делать; быть дозволенным.",
                        "en": "To have permission to do something; to be allowed."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "можно",
                                "target_lang_sense_clarification": "разрешено"
                            },
                            {
                                "target_lang_word": "разрешаться",
                                "target_lang_sense_clarification": "быть дозволенным"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "be allowed to",
                                "target_lang_sense_clarification": "general permission"
                            },
                            {
                                "target_lang_word": "may",
                                "target_lang_sense_clarification": "formal permission"
                            },
                            {
                                "target_lang_word": "can",
                                "target_lang_sense_clarification": "informal permission"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3aefef4a-a89e-3766-80aa-8edab894a886",
                    "sense_definition": "Iets willen hebben of ergens naartoe willen (waarbij het hoofdwerkwoord 'hebben' of 'gaan' is weggelaten).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Mag> ik twee appels alstublieft?",
                            "target_lang_translations": {
                                "ru": "<Можно> мне два яблока, пожалуйста?",
                                "en": "<w>May</w> I have two apples, please?"
                            }
                        },
                        {
                            "text": "Je <mag> niet naar buiten, want het regent.",
                            "target_lang_translations": {
                                "ru": "Тебе <нельзя> на улицу, потому что идет дождь.",
                                "en": "You <w>may</w> not go outside, because it is raining."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mag ik een",
                        "mag ik wat vragen",
                        "mag ik naar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хотеть получить что-то или пойти куда-то (используется для вежливых просьб).",
                        "en": "To want to have something or go somewhere (used for polite requests)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "можно",
                                "target_lang_sense_clarification": "просьба получить или сделать что-то"
                            },
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "желать (в вежливой форме)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "may",
                                "target_lang_sense_clarification": "polite request (often with 'have' or 'go')"
                            },
                            {
                                "target_lang_word": "can",
                                "target_lang_sense_clarification": "informal request"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23a2c4ca-835a-353e-9e99-69e703c78bff",
                    "sense_definition": "Iemand of iets aardig of sympathiek vinden.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "liking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <mag> die jongen wel, hij is altijd vriendelijk.",
                            "target_lang_translations": {
                                "ru": "Мне <нравится> этот парень, он всегда дружелюбен.",
                                "en": "I <w>like</w> that boy, he is always friendly."
                            }
                        },
                        {
                            "text": "Zij <mochten> elkaar meteen toen ze elkaar ontmoetten.",
                            "target_lang_translations": {
                                "ru": "Они сразу <понравились> друг другу, когда встретились.",
                                "en": "They <w>liked</w> each other immediately when they met."
                            }
                        }
                    ],
                    "synonyms": [
                        "aardig vinden",
                        "leuk vinden"
                    ],
                    "common_phrases": [
                        "iemand graag mogen",
                        "iemand niet mogen",
                        "elkaar mogen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать симпатию к кому-либо или чему-либо; считать приятным.",
                        "en": "To feel affection for someone or something; to like."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нравиться",
                                "target_lang_sense_clarification": "быть симпатичным кому-то"
                            },
                            {
                                "target_lang_word": "любить",
                                "target_lang_sense_clarification": "испытывать привязанность"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "like",
                                "target_lang_sense_clarification": "to find pleasant"
                            },
                            {
                                "target_lang_word": "be fond of",
                                "target_lang_sense_clarification": "to have affection for"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5a5a1a2-6530-3cd2-b340-d27aadabd9a1",
                    "sense_definition": "Drukt uit dat iets een mogelijkheid is of als voorwaarde geldt (vaak aan het begin van een zin).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "modality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Mocht> het morgen regenen, dan blijven we thuis.",
                            "target_lang_translations": {
                                "ru": "<Если вдруг> завтра пойдет дождь, мы останемся дома.",
                                "en": "<w>Should</w> it rain tomorrow, we will stay home."
                            }
                        },
                        {
                            "text": "<Mocht> je hem zien, doe hem dan de groeten.",
                            "target_lang_translations": {
                                "ru": "<Если> увидишь его, передавай привет.",
                                "en": "<w>Should</w> you see him, give him my regards."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mocht het zo zijn",
                        "mocht het lukken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "alleen in de verleden tijd gebruikt voor deze betekenis"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает возможность или условие (часто в начале предложения: «если вдруг»).",
                        "en": "Expresses a possibility or condition (often used at the beginning of a sentence like 'should')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "если вдруг",
                                "target_lang_sense_clarification": "условие"
                            },
                            {
                                "target_lang_word": "случись",
                                "target_lang_sense_clarification": "вводное слово для условия"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "should",
                                "target_lang_sense_clarification": "conditional"
                            },
                            {
                                "target_lang_word": "happen to",
                                "target_lang_sense_clarification": "conditional possibility"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e7709ba-8462-3dd1-b0c6-7f8e9103f75a",
                    "sense_definition": "Waarschijnlijk zijn; aangenomen kunnen worden.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "modality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je <mag> aannemen dat het verhaal klopt.",
                            "target_lang_translations": {
                                "ru": "<Можно> предположить, что эта история правдива.",
                                "en": "You <w>may</w> assume that the story is true."
                            }
                        },
                        {
                            "text": "Al onze inspanningen <mochten> helaas niet baten.",
                            "target_lang_translations": {
                                "ru": "К сожалению, все наши усилия <не помогли>.",
                                "en": "Unfortunately, all our efforts <w>were</w> in vain."
                            }
                        }
                    ],
                    "synonyms": [
                        "kunnen"
                    ],
                    "common_phrases": [
                        "het mocht niet baten",
                        "je mag aannemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть вероятным; можно предположить.",
                        "en": "To be probable; to be safe to assume."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "можно",
                                "target_lang_sense_clarification": "вероятность или допущение"
                            },
                            {
                                "target_lang_word": "мочь",
                                "target_lang_sense_clarification": "быть в состоянии"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "may",
                                "target_lang_sense_clarification": "probability or assumption"
                            },
                            {
                                "target_lang_word": "can",
                                "target_lang_sense_clarification": "possibility"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c956c2ee-7f90-332f-b9be-3470110edbb7",
                    "sense_definition": "Drukt een wens of hoop uit.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "modality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Mogen> zij in vrede rusten.",
                            "target_lang_translations": {
                                "ru": "<Пусть> они покоятся с миром.",
                                "en": "<w>May</w> they rest in peace."
                            }
                        },
                        {
                            "text": "Het <moge> duidelijk zijn dat dit niet de bedoeling is.",
                            "target_lang_translations": {
                                "ru": "<Пусть> будет ясно, что это не входило в намерения.",
                                "en": "<w>Let</w> it be clear that this is not the intention."
                            }
                        }
                    ],
                    "common_phrases": [
                        "moge het zo zijn",
                        "het moge duidelijk zijn"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "vooral in formele of plechtige taal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает желание или надежду (торжественный или формальный стиль).",
                        "en": "Expresses a wish or hope (formal)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пусть",
                                "target_lang_sense_clarification": "пожелание"
                            },
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "частица для выражения пожелания"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "may",
                                "target_lang_sense_clarification": "expressing a wish"
                            },
                            {
                                "target_lang_word": "let",
                                "target_lang_sense_clarification": "imperative/wish"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
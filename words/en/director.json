{
    "word_family": [
        "direct",
        "direction",
        "directive",
        "directory",
        "directorship",
        "directorial",
        "indirect",
        "redirection"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "948fadb6-ad8e-3444-90ad-0f6b4bee8755",
                    "sense_definition": "A person who is in charge of an activity, department, or organization, or who supervises the creative and technical making of a film or play.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "management_leadership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The managing <w>director</w> presented the annual report to the staff.",
                            "target_lang_translations": {
                                "ru": "Генеральный <w>директор</w> представил сотрудникам годовой отчёт."
                            }
                        },
                        {
                            "text": "She is a famous film <w>director</w> who has won several awards for her movies.",
                            "target_lang_translations": {
                                "ru": "Она известный <w>режиссёр</w>, получившая несколько наград за свои фильмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "manager",
                        "head",
                        "chief",
                        "administrator",
                        "boss",
                        "filmmaker"
                    ],
                    "antonyms": [
                        "subordinate",
                        "employee",
                        "assistant"
                    ],
                    "common_phrases": [
                        "managing director",
                        "film director",
                        "executive director",
                        "creative director",
                        "funeral director"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, возглавляющий деятельность, отдел или организацию, либо руководящий творческим и техническим процессом создания фильма или спектакля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "директор",
                                "target_lang_sense_clarification": "руководитель организации или предприятия"
                            },
                            {
                                "target_lang_word": "режиссёр",
                                "target_lang_sense_clarification": "постановщик фильма или спектакля"
                            },
                            {
                                "target_lang_word": "руководитель",
                                "target_lang_sense_clarification": "начальник, управляющий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1df6ad7-be3a-3ce9-888b-3b3590018896",
                    "sense_definition": "A member of a governing board that manages the affairs of a business or organization.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "management_leadership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The board of <w>directors</w> voted to approve the merger with the rival company.",
                            "target_lang_translations": {
                                "ru": "Совет <w>директоров</w> проголосовал за одобрение слияния с компанией-конкурентом."
                            }
                        },
                        {
                            "text": "He was appointed as a non-executive <w>director</w> to provide independent advice.",
                            "target_lang_translations": {
                                "ru": "Его назначили неисполнительным <w>директором</w> для предоставления независимых консультаций."
                            }
                        }
                    ],
                    "synonyms": [
                        "board member",
                        "trustee"
                    ],
                    "common_phrases": [
                        "board of directors",
                        "associate director"
                    ],
                    "target_lang_definitions": {
                        "ru": "Член управляющего совета, который руководит делами компании или организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "директор",
                                "target_lang_sense_clarification": "член совета директоров"
                            },
                            {
                                "target_lang_word": "член правления",
                                "target_lang_sense_clarification": "участник руководящего органа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "785b7c3b-7f61-3674-8b90-51b1cc1dd312",
                    "sense_definition": "A person who provides guidance, advice, or counseling, especially in spiritual or personal matters.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "guidance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He meets with his spiritual <w>director</w> once a month to discuss his faith.",
                            "target_lang_translations": {
                                "ru": "Он встречается со своим духовным <w>наставником</w> раз в месяц, чтобы обсудить вопросы веры."
                            }
                        },
                        {
                            "text": "The camp <w>director</w> ensured that all the children felt safe and welcome.",
                            "target_lang_translations": {
                                "ru": "<w>Директор</w> лагеря позаботился о том, чтобы все дети чувствовали себя в безопасности и желанными гостями."
                            }
                        }
                    ],
                    "synonyms": [
                        "guide",
                        "mentor",
                        "counselor",
                        "advisor"
                    ],
                    "common_phrases": [
                        "spiritual director"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, дающий наставления, советы или консультации, особенно в духовных или личных вопросах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наставник",
                                "target_lang_sense_clarification": "духовный или личный советник"
                            },
                            {
                                "target_lang_word": "духовник",
                                "target_lang_sense_clarification": "священник, руководящий духовной жизнью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3c96970-7537-33c8-8cfa-9c64525dbf31",
                    "sense_definition": "A mechanical device or part that guides, aims, or orients the motion or flow of something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineer adjusted the flow <w>director</w> to improve the efficiency of the cooling system.",
                            "target_lang_translations": {
                                "ru": "Инженер отрегулировал <w>направляющую</w> потока, чтобы повысить эффективность системы охлаждения."
                            }
                        },
                        {
                            "text": "This small component acts as a <w>director</w> for the laser beam within the machine.",
                            "target_lang_translations": {
                                "ru": "Этот небольшой компонент действует как <w>направляющая</w> для лазерного луча внутри устройства."
                            }
                        }
                    ],
                    "synonyms": [
                        "guide"
                    ],
                    "target_lang_definitions": {
                        "ru": "Механическое устройство или деталь, которая направляет, нацеливает или ориентирует движение или поток чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "направляющая",
                                "target_lang_sense_clarification": "устройство для направления движения"
                            },
                            {
                                "target_lang_word": "директор",
                                "target_lang_sense_clarification": "элемент антенны"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "roczny",
        "rocznik",
        "rocznica",
        "coroczny",
        "roczek"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "64713c09-9bf2-3f5e-8e71-fb617d0fb217",
                    "sense_definition": "Jednostka czasu odpowiadająca okresowi obiegu Ziemi wokół Słońca, trwająca 12 miesięcy (zwykle 365 lub 366 dni).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym <w>roku</w> planujemy wyjazd za granicę.",
                            "target_lang_translations": {
                                "ru": "В следующем <w>году</w> мы планируем поездку за границу."
                            }
                        },
                        {
                            "text": "Szczęśliwego Nowego <w>Roku</w> życzę tobie i twojej rodzinie.",
                            "target_lang_translations": {
                                "ru": "Счастливого Нового <w>года</w> желаю тебе и твоей семье."
                            }
                        }
                    ],
                    "synonyms": [
                        "r."
                    ],
                    "common_phrases": [
                        "Nowy Rok",
                        "rok temu",
                        "w przyszłym roku",
                        "rok przestępny",
                        "pory roku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица времени, соответствующая периоду обращения Земли вокруг Солнца, длящаяся 12 месяцев (обычно 365 или 366 дней)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "год",
                                "target_lang_sense_clarification": "единица времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efd283db-efbe-3233-b1d6-1d5966c7e673",
                    "sense_definition": "Okres przeznaczony na realizację programu nauczania w szkole lub na uczelni, oddzielony wakacjami.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "education_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest już na trzecim <w>roku</w> studiów medycznych.",
                            "target_lang_translations": {
                                "ru": "Мой брат уже на третьем <w>курсе</w> медицинского института."
                            }
                        },
                        {
                            "text": "Nowy <w>rok</w> szkolny zaczyna się tradycyjnie pierwszego września.",
                            "target_lang_translations": {
                                "ru": "Новый учебный <w>год</w> начинается традиционно первого сентября."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rok szkolny",
                        "rok akademicki",
                        "pierwszy rok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период, отведенный на реализацию учебной программы в школе или вузе, отделенный каникулами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "год",
                                "target_lang_sense_clarification": "учебный период"
                            },
                            {
                                "target_lang_word": "курс",
                                "target_lang_sense_clarification": "год обучения в вузе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d072e519-d18f-35e4-9c2e-0c794eb01049",
                    "sense_definition": "Grupa studentów, którzy rozpoczęli studia w tym samym czasie i wspólnie realizują program nauczania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały nasz <w>rok</w> musi przygotować się do trudnego egzaminu z anatomii.",
                            "target_lang_translations": {
                                "ru": "Весь наш <w>курс</w> должен подготовиться к трудному экзамену по анатомии."
                            }
                        },
                        {
                            "text": "Marek został wybrany na starostę naszego <w>roku</w>.",
                            "target_lang_translations": {
                                "ru": "Марека выбрали старостой нашего <w>курса</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rocznik"
                    ],
                    "common_phrases": [
                        "starosta roku",
                        "spotkanie roku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа студентов, начавших обучение в одно и то же время и совместно проходящих учебную программу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курс",
                                "target_lang_sense_clarification": "группа студентов"
                            },
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "объединение учебных групп"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e75e6326-545b-3760-8043-77d9499007ff",
                    "sense_definition": "Czas, w jakim dowolne ciało niebieskie (np. planeta) wykonuje pełny obieg wokół swojej gwiazdy.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jowiszowy <w>rok</w> trwa prawie dwanaście lat ziemskich.",
                            "target_lang_translations": {
                                "ru": "Юпитерианский <w>год</w> длится почти двенадцать земных лет."
                            }
                        },
                        {
                            "text": "Astronomowie obliczyli, ile trwa <w>rok</w> na nowo odkrytej planecie.",
                            "target_lang_translations": {
                                "ru": "Астрономы вычислили, сколько длится <w>год</w> на недавно открытой планете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rok świetlny",
                        "rok marsjański"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время, за которое любое небесное тело (например, планета) совершает полный оборот вокруг своей звезды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "год",
                                "target_lang_sense_clarification": "астрономический период"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "867d9704-97cd-3041-a7bb-51bb888cc43f",
                    "sense_definition": "Dawne określenie na termin rozprawy sądowej lub samą sprawę sądową.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szlachcic nie stawił się na wyznaczony <w>rok</w> w trybunale.",
                            "target_lang_translations": {
                                "ru": "Дворянин не явился в назначенный <w>срок</w> в трибунал."
                            }
                        },
                        {
                            "text": "Stare dokumenty opisują <w>rok</w> sądowy, który odbył się w zamku.",
                            "target_lang_translations": {
                                "ru": "Старые документы описывают судебный <w>срок</w>, который состоялся в замке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historyczne znaczenie prawne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название срока судебного разбирательства или самого судебного дела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срок",
                                "target_lang_sense_clarification": "устаревшее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b560d24-c707-365d-872e-79a8cf738dfd",
                    "sense_definition": "Regionalne określenie na spódnicę, używane w gwarze śląskiej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_regional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia ubrała swój odświętny <w>rok</w> i poszła do kościoła.",
                            "target_lang_translations": {
                                "ru": "Бабушка надела свою праздничную <w>юбку</w> и пошла в церковь."
                            }
                        },
                        {
                            "text": "Na Śląsku dawniej każda kobieta nosiła szeroki <w>rok</w>.",
                            "target_lang_translations": {
                                "ru": "В Силезии раньше каждая женщина носила широкую <w>юбку</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "gwara śląska"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "Górny Śląsk"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное название юбки, используемое в силезском диалекте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юбка",
                                "target_lang_sense_clarification": "предмет одежды"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7dff9e46-1bbc-32f3-a27d-5edb64105734",
                    "sense_definition": "Polskie nazwisko (męskie lub żeńskie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Rok</w> jest znanym lekarzem w naszym mieście.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Рок</w> — известный врач в нашем городе."
                            }
                        },
                        {
                            "text": "Na liście obecności widnieje nazwisko Anna <w>Rok</w>.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих значится фамилия Анна <w>Рок</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия (мужская или женская)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Рок",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "target_lang_definitions": {
                        "ru": "Надетый на тело (об одежде или обуви).",
                        "en": "Worn on the body (regarding clothing or shoes)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надет",
                                "target_lang_sense_clarification": "об одежде"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "на ком-то"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "wearing/fitted"
                            }
                        ]
                    }
                }
            ]
        },

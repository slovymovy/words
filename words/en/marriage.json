        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6f1bb41b-8888-3333-a4e5-b34fb43ed3ba",
                    "sense_definition": "A legally or socially recognized union of two people as partners in a personal relationship.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "matrimonial_relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They have a happy and long-lasting <w>marriage</w>.",
                            "target_lang_translations": {
                                "ru": "У них счастливый и долгий <w>брак</w>."
                            }
                        },
                        {
                            "text": "Her parents arranged her <w>marriage</w> to a man from a neighboring village.",
                            "target_lang_translations": {
                                "ru": "Ее родители устроили ее <w>брак</w> с мужчиной из соседней деревни."
                            }
                        }
                    ],
                    "synonyms": [
                        "civil union",
                        "matrimony"
                    ],
                    "antonyms": [
                        "divorce"
                    ],
                    "common_phrases": [
                        "arranged marriage",
                        "same-sex marriage",
                        "open marriage",
                        "marriage proposal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридически или социально признанный союз двух людей в личных отношениях"
                    },
                    "translations": {

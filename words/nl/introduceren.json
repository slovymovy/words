{
    "word_family": [
        "introductie",
        "introducé",
        "herintroduceren"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f4b4c84a-fd89-3fb1-b43e-36bb9a063c72",
                    "sense_definition": "Iemand voor het eerst aan anderen voorstellen, zodat ze elkaar leren kennen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik mijn nieuwe collega aan jullie <w>introduceren</w>?",
                            "target_lang_translations": {
                                "en": "May I <w>introduce</w> my new colleague to you?"
                            }
                        },
                        {
                            "text": "Hij wilde zich <w>introduceren</w> bij de buren, maar ze waren niet thuis.",
                            "target_lang_translations": {
                                "en": "He wanted to <w>introduce</w> himself to the neighbors, but they were not home."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorstellen",
                        "presenteren"
                    ],
                    "common_phrases": [
                        "zich introduceren",
                        "iemand introduceren",
                        "gasten introduceren"
                    ],
                    "target_lang_definitions": {
                        "en": "To introduce someone to others for the first time, so they get to know each other."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "introduce",
                                "target_lang_sense_clarification": "make someone known"
                            },
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "introduce formally"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9dcd05b-b305-3a60-a78b-9cb59269687f",
                    "sense_definition": "Iets nieuws, zoals een plan, product of maatregel, in gebruik nemen of bekendmaken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "implementation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf wil volgende maand een nieuwe telefoon <w>introduceren</w>.",
                            "target_lang_translations": {
                                "en": "The company wants to <w>introduce</w> a new phone next month."
                            }
                        },
                        {
                            "text": "De overheid gaat strengere regels <w>introduceren</w> om het milieu te beschermen.",
                            "target_lang_translations": {
                                "en": "The government is going to <w>introduce</w> stricter rules to protect the environment."
                            }
                        }
                    ],
                    "synonyms": [
                        "invoeren",
                        "lanceren"
                    ],
                    "antonyms": [
                        "afschaffen"
                    ],
                    "common_phrases": [
                        "een product introduceren",
                        "maatregelen introduceren",
                        "veranderingen introduceren"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring something new, such as a plan, product, or measure, into use or make it known."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "introduce",
                                "target_lang_sense_clarification": "bring into use"
                            },
                            {
                                "target_lang_word": "launch",
                                "target_lang_sense_clarification": "release a product"
                            },
                            {
                                "target_lang_word": "implement",
                                "target_lang_sense_clarification": "put into effect"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35dd4c63-431c-3bd2-829b-eed3c96c27aa",
                    "sense_definition": "Een persoon meenemen en toegang geven tot een besloten groep of vereniging.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Leden mogen één gast per avond <w>introduceren</w> in de club.",
                            "target_lang_translations": {
                                "en": "Members may <w>introduce</w> one guest per evening into the club."
                            }
                        },
                        {
                            "text": "Je moet iemand kennen die je kan <w>introduceren</w> om lid te worden van deze vereniging.",
                            "target_lang_translations": {
                                "en": "You have to know someone who can <w>introduce</w> you to become a member of this association."
                            }
                        }
                    ],
                    "synonyms": [
                        "binnenleiden"
                    ],
                    "common_phrases": [
                        "een vriend introduceren",
                        "nieuwe leden introduceren"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring a person along and grant them access to a private group or association."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "introduce",
                                "target_lang_sense_clarification": "bring in as a guest or member"
                            },
                            {
                                "target_lang_word": "bring in",
                                "target_lang_sense_clarification": "grant entry to"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "61898cf9-3815-3ea2-afaf-0d898a00cf9c",
                    "sense_definition": "To be absent from school without a valid excuse.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "John said he was going to <w>mitch</w> the last lesson today.",
                            "target_lang_translations": {
                                "ru": "Джон сказал, что собирается <w>прогулять</w> последний урок сегодня."
                            }
                        },
                        {
                            "text": "The boys decided to <w>mitch</w> off and go to the park instead.",
                            "target_lang_translations": {
                                "ru": "Мальчики решили <w>сбежать с уроков</w> и пойти вместо этого в парк."
                            }
                        }
                    ],
                    "synonyms": [
                        "play truant",
                        "skive off"
                    ],
                    "common_phrases": [

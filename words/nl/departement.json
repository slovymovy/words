            "senses": [
                {
                    "sense_id": "4e60ef48-36dc-3857-92e8-b4858df025d6",
                    "sense_definition": "Een ministerie; een hoofdafdeling van de rijksoverheid onder leiding van een minister.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De minister is verantwoordelijk voor het beleid van zijn <w>departement</w>.",
                            "target_lang_translations": {
                                "ru": "Министр несет ответственность за политику своего <w>министерства</w>."
                            }
                        },
                        {
                            "text": "Er werken duizenden ambtenaren op het <w>departement</w> van Financiën.",
                            "target_lang_translations": {
                                "ru": "В <w>Министерстве</w> финансов работают тысячи государственных служащих."
                            }
                        }
                    ],
                    "synonyms": [
                        "ministerie"
                    ],
                    "common_phrases": [
                        "departement van Justitie",
                        "departement van Defensie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Министерство; главное подразделение центрального правительства под руководством министра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "министерство",
                                "target_lang_sense_clarification": "правительственное учреждение"
                            },
                            {
                                "target_lang_word": "ведомство",
                                "target_lang_sense_clarification": "государственный орган"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb9d8465-86c3-371f-ba82-c82222f2d08c",
                    "sense_definition": "Een bestuurlijk district of gewest in bepaalde landen, met name in Frankrijk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens onze vakantie bezochten we een prachtig <w>departement</w> in het zuiden van Frankrijk.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска мы посетили красивый <w>департамент</w> на юге Франции."
                            }
                        },
                        {
                            "text": "Het <w>departement</w> heeft te maken met zware regenval en overstromingen.",
                            "target_lang_translations": {
                                "ru": "Этот <w>департамент</w> столкнулся с сильными ливнями и наводнениями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Frans departement",
                        "overzees departement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Административный округ или регион в некоторых странах, особенно во Франции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "департамент",
                                "target_lang_sense_clarification": "административная единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4735dc09-7fd3-311a-9d29-32542b9985e3",
                    "sense_definition": "Een afdeling, vakgroep of subfaculteit binnen een universiteit of grote organisatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor welk <w>departement</w> werkt u binnen de universiteit?",
                            "target_lang_translations": {
                                "ru": "На какой <w>кафедре</w> вы работаете в университете?"
                            }
                        },
                        {
                            "text": "Het <w>departement</w> psychologie organiseert morgen een interessante lezing.",
                            "target_lang_translations": {
                                "ru": "<w>Кафедра</w> психологии организует завтра интересную лекцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "vakgroep",
                        "afdeling"
                    ],
                    "common_phrases": [
                        "hoofd van het departement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдел, кафедра или подразделение внутри университета или крупной организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кафедра",
                                "target_lang_sense_clarification": "в учебном заведении"
                            },
                            {
                                "target_lang_word": "отдел",
                                "target_lang_sense_clarification": "подразделение организации"
                            }
                        ]
                    }
                }

            "senses": [
                {
                    "sense_id": "ae2eeb29-11ad-3719-8dce-3fa3e5a4f85c",
                    "sense_definition": "A familiar or old-fashioned term of address for one's father.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "family_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>papa</w> taught me how to ride a bicycle in the park.",
                            "target_lang_translations": {
                                "ru": "Мой <w>папа</w> научил меня кататься на велосипеде в парке."
                            }
                        },
                        {
                            "text": "The little girl ran to her <w>papa</w> when she felt scared.",
                            "target_lang_translations": {
                                "ru": "Маленькая девочка побежала к своему <w>папе</w>, когда испугалась."
                            }
                        },
                        {
                            "text": "Everyone in the family knows that <w>papa</w> bear is protective of his cubs.",
                            "target_lang_translations": {
                                "ru": "Все в семье знают, что <w>папа</w>-медведь защищает своих медвежат."
                            }
                        }
                    ],
                    "synonyms": [
                        "dad",
                        "daddy",
                        "father",
                        "pa",
                        "pop"
                    ],
                    "antonyms": [
                        "mama",
                        "mom",
                        "mommy",
                        "mother"
                    ],
                    "common_phrases": [
                        "papa bear"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "familiar term"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "in some regions"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Знакомое или старомодное обращение к отцу"
                    },
                    "translations": {

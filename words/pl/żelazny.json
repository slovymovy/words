    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "76705d73-6d51-3a7b-849c-58766132cca8",
                    "sense_definition": "Wykonany z żelaza lub zawierający ten metal",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starym ogrodzie stała zardzewiała <w>żelazna</w> ławka, na której nikt nie siadał.",
                            "target_lang_translations": {
                                "ru": "В старом саду стояла ржавая <w>железная</w> скамейка, на которую никто не садился."
                            }
                        },
                        {
                            "text": "Ciężka <w>żelazna</w> brama zamknęła się za nami z głośnym hukiem.",
                            "target_lang_translations": {
                                "ru": "Тяжёлые <w>железные</w> ворота закрылись за нами с громким грохотом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "żelazna brama",
                        "żelazny pręt",
                        "żelazny most",
                        "żelazna krata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный из железа или содержащий этот металл"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железный",
                                "target_lang_sense_clarification": "прямое значение: сделанный из материала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eba9125e-f5fd-36af-ac53-aa3da44c881a",
                    "sense_definition": "Bardzo silny, wytrzymały i odporny na trudne warunki (zwykle o zdrowiu, nerwach lub kondycji)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_strength",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek ma <w>żelazne</w> zdrowie i mimo podeszłego wieku codziennie biega.",
                            "target_lang_translations": {
                                "ru": "У моего дедушки <w>железное</w> здоровье, и, несмотря на преклонный возраст, он бегает каждый день."
                            }
                        },
                        {
                            "text": "W pracy chirurga trzeba mieć <w>żelazne</w> nerwy, aby zachować spokój w trudnych sytuacjach.",
                            "target_lang_translations": {
                                "ru": "В работе хирурга нужно иметь <w>железные</w> нервы, чтобы сохранять спокойствие в трудных ситуациях."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytrzymały",
                        "odporny",
                        "twardy",
                        "zahartowany",
                        "niepodatny"
                    ],
                    "common_phrases": [
                        "żelazne zdrowie",
                        "żelazne nerwy",
                        "żelazna kondycja",
                        "żelazny uścisk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень крепкий, сильный и выносливый (обычно о здоровье или нервах)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железный",
                                "target_lang_sense_clarification": "переносное значение: крепкий, сильный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d23b1de4-d01c-3602-bbe0-5b5f44d8e9c9",
                    "sense_definition": "Ściśle przestrzegany, nienaruszalny i niepodlegający zmianom (zwykle o zasadach, logice lub dyscyplinie)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_strength",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej jednostce wojskowej panuje <w>żelazna</w> dyscyplina, której każdy żołnierz musi przestrzegać.",
                            "target_lang_translations": {
                                "ru": "В этой воинской части царит <w>железная</w> дисциплина, которую должен соблюдать каждый солдат."
                            }
                        },
                        {
                            "text": "Jego <w>żelazna</w> logika sprawiła, że nikt nie potrafił podważyć przedstawionych argumentów.",
                            "target_lang_translations": {
                                "ru": "Его <w>железная</w> логика привела к тому, что никто не смог опровергнуть представленные аргументы."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezwzględny",
                        "nienaruszalny",
                        "bezwarunkowy",
                        "nieugięty",
                        "niezłomny",
                        "trwały"
                    ],
                    "common_phrases": [
                        "żelazna dyscyplina",
                        "żelazna zasada",
                        "żelazna logika",
                        "żelazna konsekwencja",
                        "żelazny argument"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строго соблюдаемый, нерушимый и не подлежащий изменениям (обычно о правилах или логике)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железный",
                                "target_lang_sense_clarification": "переносное значение: непреложный, строгий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45881eba-7e0d-368c-9b00-ac123cc6c7b3",
                    "sense_definition": "Związany z żelazem jako surowcem, pierwiastkiem lub okresem historycznym",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Archeolodzy odkryli w tej okolicy osadę pochodzącą z okresu, który nazywamy wiekiem <w>żelaznym</w>.",
                            "target_lang_translations": {
                                "ru": "Археологи обнаружили в этой местности поселение, относящееся к периоду, который мы называем <w>железным</w> веком."
                            }
                        },
                        {
                            "text": "Rozwój przemysłu <w>żelaznego</w> w XIX wieku przyczynił się do szybkiej urbanizacji tego regionu.",
                            "target_lang_translations": {
                                "ru": "Развитие <w>железной</w> промышленности в XIX веке способствовало быстрой урбанизации этого региона."
                            }
                        }
                    ],
                    "common_phrases": [

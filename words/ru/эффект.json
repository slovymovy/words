    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "731a662c-0ffb-3a61-be71-44e44eee5485",
                    "sense_definition": "Результат или следствие каких-либо действий, мероприятий или причин.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "result_consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новое лекарство дало быстрый положительный <эффект>.",
                            "target_lang_translations": {
                                "en": "The new medicine produced a quick positive <effect>."
                            }
                        },
                        {
                            "text": "Мы получили неожиданный экономический <эффект> от внедрения этой технологии.",
                            "target_lang_translations": {
                                "en": "We received an unexpected economic <effect> from implementing this technology."
                            }
                        }
                    ],
                    "synonyms": [
                        "результа́т",
                        "сле́дствие",
                        "де́йствие"
                    ],
                    "common_phrases": [
                        "положительный эффект",
                        "побочный эффект",
                        "обратный эффект",
                        "экономический эффект"
                    ],
                    "target_lang_definitions": {
                        "en": "The result or consequence of some actions, measures, or causes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "effect",
                                "target_lang_sense_clarification": "outcome or consequence"
                            },
                            {
                                "target_lang_word": "result",
                                "target_lang_sense_clarification": "outcome"
                            },
                            {
                                "target_lang_word": "impact",
                                "target_lang_sense_clarification": "strong effect or influence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96b6819d-deb5-3c94-bba4-9efbcd70c1a8",
                    "sense_definition": "Технические средства (звук, свет, видео), используемые в кино, театре или играх для создания иллюзий.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом фантастическом фильме использованы потрясающие визуальные <эффекты>.",
                            "target_lang_translations": {
                                "en": "This sci-fi movie uses stunning visual <effects>."
                            }
                        },
                        {
                            "text": "Зрителям очень понравились световые <эффекты> на музыкальном концерте.",
                            "target_lang_translations": {
                                "en": "The audience really liked the lighting <effects> at the music concert."
                            }
                        }
                    ],
                    "common_phrases": [
                        "специальные эффекты",
                        "звуковые эффекты",
                        "световые эффекты",
                        "визуальные эффекты"
                    ],
                    "target_lang_definitions": {
                        "en": "Technical means (sound, light, video) used in cinema, theater, or games to create illusions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "effect",
                                "target_lang_sense_clarification": "technical illusion"
                            },
                            {
                                "target_lang_word": "special effect",
                                "target_lang_sense_clarification": "visual or sound illusion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b65cd25-1a2f-3f3e-bd88-a2cb553b1659",
                    "sense_definition": "Сильное впечатление, которое кто-либо или что-либо производит на окружающих.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её неожиданное появление произвело настоящий <эффект> на всех гостей.",
                            "target_lang_translations": {
                                "en": "Her unexpected appearance produced a real <effect> on all the guests."
                            }
                        },
                        {
                            "text": "Оратор говорил громко и пафосно, рассчитывая на внешний <эффект>.",
                            "target_lang_translations": {
                                "en": "The speaker spoke loudly and pompously, calculating for external <effect>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "произвести эффект",
                        "внешний эффект",
                        "ради эффекта"
                    ],
                    "target_lang_definitions": {
                        "en": "A strong impression that someone or something produces on others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "effect",
                                "target_lang_sense_clarification": "impression"
                            },
                            {
                                "target_lang_word": "impression",
                                "target_lang_sense_clarification": "impact on the mind or feelings"
                            },
                            {
                                "target_lang_word": "sensation",
                                "target_lang_sense_clarification": "widespread reaction or interest"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bd80d88-4b92-3ffb-8885-1bfef14586d5",
                    "sense_definition": "Физическое или научное явление, закономерность, проявляющаяся в определённых условиях.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учёные всего мира изучают парниковый <эффект> и его влияние на климат.",
                            "target_lang_translations": {
                                "en": "Scientists all over the world are studying the greenhouse <effect> and its influence on the climate."
                            }
                        },
                        {
                            "text": "На уроке физики преподаватель объяснил нам <эффект> Доплера.",
                            "target_lang_translations": {
                                "en": "In physics class, the teacher explained the Doppler <effect> to us."
                            }
                        }
                    ],
                    "common_phrases": [
                        "парниковый эффект",
                        "эффект плацебо",
                        "эффект Доплера"
                    ],
                    "target_lang_definitions": {
                        "en": "A physical or scientific phenomenon or regularity manifesting under certain conditions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "effect",
                                "target_lang_sense_clarification": "scientific phenomenon"
                            }
                        ]
                    }
                }
            ]
        }
    ]

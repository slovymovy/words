    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "61625924-c316-3e4c-82dc-c261f6eff1cb",
                    "sense_definition": "Staying in one place without moving; not active or in motion.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "lack_of_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The guard remained <w>static</w> at his post for several hours.",
                            "target_lang_translations": {
                                "ru": "Охранник оставался <w>неподвижным</w> на своем посту в течение нескольких часов."
                            }
                        },
                        {
                            "text": "The target was <w>static</w>, making it much easier for the archer to hit.",
                            "target_lang_translations": {
                                "ru": "Цель была <w>статичной</w>, поэтому лучнику было гораздо легче в нее попасть."
                            }
                        }
                    ],
                    "synonyms": [
                        "stationary",
                        "immobile",
                        "fixed",
                        "inert",
                        "stuck",
                        "unmovable"
                    ],
                    "antonyms": [
                        "mobile",
                        "moving"
                    ],
                    "common_phrases": [
                        "remain static",
                        "static object"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остающийся на одном месте без движения; не активный или не находящийся в движении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неподвижный",
                                "target_lang_sense_clarification": "не совершающий движений"
                            },
                            {
                                "target_lang_word": "статичный",
                                "target_lang_sense_clarification": "лишенный динамики"
                            },
                            {
                                "target_lang_word": "стационарный",
                                "target_lang_sense_clarification": "постоянный, непередвижной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3810f2c5-535b-3936-b939-de400e9fedbc",
                    "sense_definition": "Not changing, developing, or making progress over a period of time.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "lack_of_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "House prices have remained <w>static</w> for the last few months.",
                            "target_lang_translations": {
                                "ru": "Цены на жилье оставались <w>неизменными</w> последние несколько месяцев."
                            }
                        },
                        {
                            "text": "A <w>static</w> economy can lead to high unemployment rates.",
                            "target_lang_translations": {
                                "ru": "<w>Статичная</w> экономика может привести к высокому уровню безработицы."
                            }
                        }
                    ],
                    "synonyms": [
                        "constant",
                        "stable",
                        "unchanging",
                        "unvarying",
                        "changeless"
                    ],
                    "antonyms": [
                        "dynamic"
                    ],
                    "common_phrases": [
                        "static prices",
                        "static level"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не меняющийся, не развивающийся и не прогрессирующий в течение определенного периода времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "статичный",
                                "target_lang_sense_clarification": "застойный, без развития"
                            },
                            {
                                "target_lang_word": "неизменный",
                                "target_lang_sense_clarification": "постоянный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ef4af11-d855-34fe-b4e4-74aa1300b4a9",
                    "sense_definition": "Relating to computer memory or processes that are allocated before a program runs and do not change.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>static</w> variable retains its value between function calls.",
                            "target_lang_translations": {
                                "ru": "<w>Статическая</w> переменная сохраняет свое значение между вызовами функции."
                            }
                        },
                        {
                            "text": "Building a <w>static</w> website is often cheaper than creating a dynamic one.",
                            "target_lang_translations": {
                                "ru": "Создание <w>статического</w> сайта часто обходится дешевле, чем создание динамического."
                            }
                        }
                    ],
                    "antonyms": [
                        "dynamic"
                    ],
                    "common_phrases": [
                        "static variable",
                        "static memory",
                        "static website"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к компьютерной памяти или процессам, которые распределяются до запуска программы и не изменяются."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "статический",
                                "target_lang_sense_clarification": "в программировании"
                            }
                        ]
                    }
                }
            ]
        },
        {

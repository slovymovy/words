{
    "word_family": [
        "beschikbaar",
        "beschikbaarheid",
        "beschikking"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0b84c555-6e49-336e-a4c7-a65fd4ea956d",
                    "sense_definition": "Iets hebben waar je gebruik van kunt maken; in het bezit zijn van.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "bezit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het nieuwe hotel <w>beschikt</w> over een groot zwembad en een sauna.",
                            "target_lang_translations": {
                                "ru": "Новый отель <w>располагает</w> большим бассейном и сауной.",
                                "en": "The new hotel <w>features</w> a large swimming pool and a sauna."
                            }
                        },
                        {
                            "text": "De politie <w>beschikt</w> over voldoende bewijs om de verdachte aan te houden.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>располагает</w> достаточными доказательствами для задержания подозреваемого.",
                                "en": "The police <w>possess</w> sufficient evidence to arrest the suspect."
                            }
                        }
                    ],
                    "synonyms": [
                        "hebben",
                        "bezitten"
                    ],
                    "antonyms": [
                        "missen",
                        "ontberen"
                    ],
                    "common_phrases": [
                        "beschikken over",
                        "vrijelijk beschikken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь что-либо в своем распоряжении или владении, чем можно воспользоваться.",
                        "en": "To have something available for use; to be in possession of."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "располагать",
                                "target_lang_sense_clarification": "иметь в наличии"
                            },
                            {
                                "target_lang_word": "обладать",
                                "target_lang_sense_clarification": "владеть чем-либо"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "have at one's disposal",
                                "target_lang_sense_clarification": "to have available for use"
                            },
                            {
                                "target_lang_word": "possess",
                                "target_lang_sense_clarification": "to own or hold"
                            },
                            {
                                "target_lang_word": "feature",
                                "target_lang_sense_clarification": "to have as an amenity or characteristic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f1209ec-c3d6-3def-8bc7-8b250634084f",
                    "sense_definition": "Een besluit nemen of bepalen wat er moet gebeuren, vaak in een officiële of juridische context.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "besluitvorming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter <w>beschikte</w> dat de documenten openbaar gemaakt moesten worden.",
                            "target_lang_translations": {
                                "ru": "Судья <w>постановил</w>, что документы должны быть обнародованы.",
                                "en": "The judge <w>ruled</w> that the documents had to be made public."
                            }
                        },
                        {
                            "text": "In zijn testament <w>beschikte</w> de man dat al zijn geld naar een goed doel ging.",
                            "target_lang_translations": {
                                "ru": "В своем завещании мужчина <w>распорядился</w>, чтобы все его деньги пошли на благотворительность.",
                                "en": "In his will, the man <w>decreed</w> that all his money would go to charity."
                            }
                        }
                    ],
                    "synonyms": [
                        "beslissen",
                        "bepalen",
                        "verordenen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать официальное решение или определять ход действий, часто в юридическом контексте.",
                        "en": "To make a decision or determine a course of action, often in an official or legal context."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постановлять",
                                "target_lang_sense_clarification": "принимать судебное или официальное решение"
                            },
                            {
                                "target_lang_word": "распоряжаться",
                                "target_lang_sense_clarification": "давать указания"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "rule",
                                "target_lang_sense_clarification": "to make a judicial decision"
                            },
                            {
                                "target_lang_word": "decree",
                                "target_lang_sense_clarification": "to order formally"
                            },
                            {
                                "target_lang_word": "dispose",
                                "target_lang_sense_clarification": "to determine the fate of something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
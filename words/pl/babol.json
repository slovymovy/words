                            },
                            {
                                "target_lang_word": "карга",
                                "target_lang_sense_clarification": "бран. злая, безобразная старуха"
                            },
                            {
                                "target_lang_word": "бабища",
                                "target_lang_sense_clarification": "прост. крупная, грубая или неприятная женщина"
                            }
                        ]
                    }
                }
            ]

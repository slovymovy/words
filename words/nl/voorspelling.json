{
    "word_family": [
        "voorspellen",
        "voorspelbaar",
        "onvoorspelbaar",
        "voorspeller"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5806fa39-0356-3845-b68e-13e3c8d6d403",
                    "sense_definition": "Een uitspraak over wat er in de toekomst zal gebeuren, vaak gebaseerd op feiten of vermoedens.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>voorspelling</w> over de uitslag van de wedstrijd is toch nog uitgekomen.",
                            "target_lang_translations": {
                                "ru": "Его <w>предсказание</w> об исходе матча всё же сбылось."
                            }
                        },
                        {
                            "text": "De <w>voorspellingen</w> voor het weer van morgen zijn helaas niet zo goed.",
                            "target_lang_translations": {
                                "ru": "<w>Прогнозы</w> погоды на завтра, к сожалению, не очень хорошие."
                            }
                        }
                    ],
                    "synonyms": [
                        "prognose",
                        "verwachting"
                    ],
                    "common_phrases": [
                        "een voorspelling doen",
                        "de voorspelling komt uit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высказывание о том, что произойдет в будущем, часто основанное на фактах или догадках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предсказание",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "прогноз",
                                "target_lang_sense_clarification": "погода, экономика, спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b0e45e1-58eb-39c7-95d1-8605ef821724",
                    "sense_definition": "Een profetische uitspraak over de toekomst, vaak van religieuze of mystieke aard.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volgens de oude <w>voorspelling</w> zal het koninkrijk ooit weer in vrede leven.",
                            "target_lang_translations": {
                                "ru": "Согласно древнему <w>пророчеству</w>, королевство когда-нибудь снова будет жить в мире."
                            }
                        },
                        {
                            "text": "De heks deed een angstaanjagende <w>voorspelling</w> over zijn lot.",
                            "target_lang_translations": {
                                "ru": "Ведьма сделала пугающее <w>предсказание</w> о его судьбе."
                            }
                        }
                    ],
                    "synonyms": [
                        "profetie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пророческое высказывание о будущем, часто религиозного или мистического характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пророчество",
                                "target_lang_sense_clarification": "мистическое или религиозное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
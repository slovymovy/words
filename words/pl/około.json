            "senses": [
                {
                    "sense_id": "787abe5e-469c-3dae-a081-ec0142cd279a",
                    "sense_definition": "Wskazuje na przybliżony moment w czasie, w którym coś się dzieje.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "approximation_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Józek był tu <około> trzeciej i zostawił dla ciebie wiadomość.",
                            "target_lang_translations": {
                                "ru": "Юзек был здесь <около> трёх и оставил для тебя сообщение."
                            }
                        },
                        {
                            "text": "Wrócił do domu zmęczony <około> północy.",
                            "target_lang_translations": {
                                "ru": "Он вернулся домой уставшим <около> полуночи."
                            }

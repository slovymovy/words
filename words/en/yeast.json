                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mixture began to <w>yeast</w> and bubble in the warm kitchen.",
                            "target_lang_translations": {
                                "ru": "Смесь начала <w>бродить</w> и пузыриться в теплой кухне."
                            }
                        },
                        {
                            "text": "If you leave the fruit juice out too long, it might start to <w>yeast</w>.",
                            "target_lang_translations": {
                                "ru": "Если оставить фруктовый сок надолго, он может начать <w>бродить</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ferment",
                        "froth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бродить или выделять пену."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бродить",
                                "target_lang_sense_clarification": "о процессе ферментации"
                            },
                            {
                                "target_lang_word": "пениться",
                                "target_lang_sense_clarification": "образовывать пену"
                            }
                        ]
                    }
                }
            ]
        },
        {

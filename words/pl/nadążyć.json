{
    "word_family": [
        "nadążać",
        "dążyć",
        "zdążyć",
        "podążać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3ddde60f-67ed-3e06-b798-73f1b57f27ed",
                    "sense_definition": "Poruszać się wystarczająco szybko, aby utrzymać to samo tempo co ktoś inny i nie zostać w tyle.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_pace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz iść szybciej, jeśli chcesz za nami <w>nadążyć</w>.",
                            "target_lang_translations": {
                                "ru": "Ты должен идти быстрее, если хочешь за нами <w>угнаться</w>."
                            }
                        },
                        {
                            "text": "Małe dziecko biegło, próbując <w>nadążyć</w> za rodzicami.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок бежал, пытаясь <w>поспеть</w> за родителями."
                            }
                        }
                    ],
                    "synonyms": [
                        "dotrzymywać kroku"
                    ],
                    "antonyms": [
                        "zostawać w tyle"
                    ],
                    "common_phrases": [
                        "nadążyć za kimś",
                        "ledwo nadążyć",
                        "trudno nadążyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться достаточно быстро, чтобы поддерживать тот же темп, что и кто-то другой, и не отставать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угнаться",
                                "target_lang_sense_clarification": "двигаться с той же скоростью"
                            },
                            {
                                "target_lang_word": "поспеть",
                                "target_lang_sense_clarification": "не отстать в движении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "805da155-c3d4-3b57-98e0-a0b60468a564",
                    "sense_definition": "Dostosowywać się do zachodzących zmian, postępu lub panujących trendów, nie pozostając w tyle za innymi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_pace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dzisiejszych czasach trudno jest <w>nadążyć</w> za nowinkami technologicznymi.",
                            "target_lang_translations": {
                                "ru": "В наше время трудно <w>уследить</w> за технологическими новинками."
                            }
                        },
                        {
                            "text": "Firma musi się rozwijać, aby <w>nadążyć</w> za konkurencją na rynku.",
                            "target_lang_translations": {
                                "ru": "Компания должна развиваться, чтобы <w>не отставать</w> от конкуренции на рынке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nadążyć za modą",
                        "nadążyć za zmianami",
                        "nadążyć za światem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Адаптироваться к происходящим изменениям, прогрессу или тенденциям, не отставая от других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не отставать",
                                "target_lang_sense_clarification": "соответствовать уровню или темпу развития"
                            },
                            {
                                "target_lang_word": "поспевать",
                                "target_lang_sense_clarification": "следовать за изменениями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52d1c621-6432-338a-831b-870b86eebe2e",
                    "sense_definition": "Wykonywać bieżące zadania lub obowiązki na czas, radząc sobie z ich ilością lub tempem napływania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "work_pace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mamy tyle zamówień, że nie możemy z nimi <w>nadążyć</w>.",
                            "target_lang_translations": {
                                "ru": "У нас столько заказов, что мы не можем с ними <w>справиться</w>."
                            }
                        },
                        {
                            "text": "Pracownicy ledwo mogli <w>nadążyć</w> z obsługą klientów przed świętami.",
                            "target_lang_translations": {
                                "ru": "Сотрудники едва могли <w>справиться</w> с обслуживанием клиентов перед праздниками."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyrabiać się"
                    ],
                    "antonyms": [
                        "zalegać"
                    ],
                    "common_phrases": [
                        "nadążyć z pracą",
                        "nadążyć z płatnościami",
                        "nadążyć z terminami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять текущие задачи или обязанности вовремя, справляясь с их количеством или темпом поступления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "справляться",
                                "target_lang_sense_clarification": "успешно выполнять объем работы"
                            },
                            {
                                "target_lang_word": "успевать",
                                "target_lang_sense_clarification": "делать всё вовремя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db358ff8-5982-3b56-a303-3cdad94bd813",
                    "sense_definition": "Rozumieć na bieżąco to, co ktoś mówi, lub śledzić ze zrozumieniem szybki rozwój sytuacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_pace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Profesor mówił tak szybko, że studenci nie mogli za nim <w>nadążyć</w>.",
                            "target_lang_translations": {
                                "ru": "Профессор говорил так быстро, что студенты не могли за ним <w>уследить</w>."
                            }
                        },
                        {
                            "text": "Fabuła filmu była tak skomplikowana, że ciężko było za nią <w>nadążyć</w>.",
                            "target_lang_translations": {
                                "ru": "Сюжет фильма был таким сложным, что за ним было трудно <w>уследить</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozumieć",
                        "pojmować"
                    ],
                    "antonyms": [
                        "gubić się"
                    ],
                    "common_phrases": [
                        "nadążyć za myślą",
                        "nadążyć za tokiem rozumowania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать на ходу то, что кто-то говорит, или следить с пониманием за быстрым развитием ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уследить",
                                "target_lang_sense_clarification": "понимать ход мыслей или событий"
                            },
                            {
                                "target_lang_word": "поспевать",
                                "target_lang_sense_clarification": "успевать понимать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
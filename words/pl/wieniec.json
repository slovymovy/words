    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8d6e9c62-f7e8-3eed-85b7-458a79b3b1aa",
                    "sense_definition": "Wiązanka w kształcie kręgu upleciona z kwiatów, liści, gałązek lub kłosów, używana jako ozdoba lub symbol.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shape_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Delegacja złożyła uroczysty <w>wieniec</w> pod pomnikiem bohaterów.",
                            "target_lang_translations": {
                                "ru": "Делегация возложила торжественный <w>венок</w> к памятнику героям."
                            }
                        },
                        {
                            "text": "Na drzwiach wejściowych wisiał świąteczny <w>wieniec</w> z jedliny.",
                            "target_lang_translations": {
                                "ru": "На входной двери висел праздничный <w>венок</w> из еловых веток."
                            }
                        }
                    ],
                    "synonyms": [
                        "wianek",
                        "stroik",
                        "girlanda"
                    ],
                    "common_phrases": [
                        "wieniec laurowy",
                        "wieniec pogrzebowy",
                        "wieniec adwentowy",
                        "wieniec dożynkowy",
                        "składać wieniec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сплетение цветов, листьев или веток в форме кольца, используемое как украшение или символ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "венок",
                                "target_lang_sense_clarification": "украшение из цветов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f300b4f1-3768-3daa-bd80-3f614eef9ffe",
                    "sense_definition": "Poroże jelenia, stanowiące trofeum myśliwskie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biology_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary jeleń dumnie prezentował swój rozłożysty <w>wieniec</w>.",
                            "target_lang_translations": {
                                "ru": "Старый олень гордо демонстрировал свои раскидистые <w>рога</w>."
                            }
                        },
                        {
                            "text": "W leśniczówce na ścianie wisiał potężny <w>wieniec</w> byka.",
                            "target_lang_translations": {
                                "ru": "В доме лесника на стене висели мощные <w>рога</w> самца."
                            }
                        }
                    ],
                    "synonyms": [
                        "poroże",
                        "rogi"
                    ],
                    "common_phrases": [
                        "wieniec jelenia",
                        "zrzucić wieniec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рога оленя, являющиеся охотничьим трофеем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рога",
                                "target_lang_sense_clarification": "оленя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abe42faf-7f8e-3d96-8eaa-39bdf02f710b",
                    "sense_definition": "Kolisty układ przedmiotów, osób lub zwierząt otaczających coś lub kogoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wokół ogniska utworzył się ciasny <w>wieniec</w> harcerzy.",
                            "target_lang_translations": {
                                "ru": "Вокруг костра образовалось тесное <w>кольцо</w> скаутов."
                            }
                        },
                        {
                            "text": "Malowniczą dolinę otaczał <w>wieniec</w> wysokich gór.",
                            "target_lang_translations": {
                                "ru": "Живописную долину окружал <w>венок</w> высоких гор."
                            }
                        }
                    ],
                    "synonyms": [
                        "krąg"
                    ],
                    "common_phrases": [
                        "wieniec gór",
                        "wieniec widzów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Круговое расположение предметов или людей, окружающих что-либо или кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кольцо",
                                "target_lang_sense_clarification": "круг людей или предметов"
                            },
                            {
                                "target_lang_word": "венок",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cee515b-05b6-311e-9410-6b92f7761dac",
                    "sense_definition": "Wzmacniający element konstrukcji budowlanej, zwykle w formie żelbetowej belki obiegającej strop i spinającej ściany.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Murarze wylali betonowy <w>wieniec</w> na szczycie ścian nośnych.",
                            "target_lang_translations": {
                                "ru": "Каменщики залили бетонный <w>армопояс</w> поверх несущих стен."
                            }
                        },
                        {
                            "text": "Prawidłowo wykonany <w>wieniec</w> zapewnia stabilność całego budynku.",
                            "target_lang_translations": {
                                "ru": "Правильно выполненный <w>армопояс</w> обеспечивает устойчивость всего здания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wieniec stropowy",
                        "zbrojenie wieńca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Укрепляющий элемент строительной конструкции, обычно в виде железобетонной балки, опоясывающей перекрытие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "армопояс",
                                "target_lang_sense_clarification": "строительство"
                            },
                            {
                                "target_lang_word": "обвязочный пояс",
                                "target_lang_sense_clarification": "строительство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79cdafaf-d226-3c7a-8a57-e873b2c07001",
                    "sense_definition": "Zjawisko optyczne w postaci barwnej poświaty wokół Słońca lub Księżyca, powstające na skutek dyfrakcji światła.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "phenomenon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W mroźną noc wokół tarczy Księżyca pojawił się wyraźny <w>wieniec</w>.",
                            "target_lang_translations": {
                                "ru": "В морозную ночь вокруг диска Луны появился отчётливый <w>венец</w>."
                            }
                        },
                        {
                            "text": "To zjawisko atmosferyczne to <w>wieniec</w>, często mylony z halo.",
                            "target_lang_translations": {
                                "ru": "Это атмосферное явление — <w>венец</w>, который часто путают с гало."
                            }
                        }
                    ],
                    "synonyms": [
                        "lisia czapa"
                    ],
                    "common_phrases": [
                        "wieniec słoneczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оптическое явление в виде цветного свечения вокруг Солнца или Луны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "венец",
                                "target_lang_sense_clarification": "атмосферное явление"
                            }
                        ]
                    }
                }
            ]
        }
    ]

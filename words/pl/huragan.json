            "senses": [
                {
                    "sense_id": "e1560920-60fd-3cb7-a514-33ba865d3a4c",
                    "sense_definition": "Bardzo silny, porywisty wiatr o prędkości powyżej 33 m/s, zazwyczaj towarzyszący cyklonom tropikalnym, powodujący duże zniszczenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weather_phenomena",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj w nocy nadciągnął potężny <w>huragan</w> i połamał wszystkie drzewa w parku.",
                            "target_lang_translations": {
                                "ru": "Вчера ночью налетел мощный <w>ураган</w> и переломал все деревья в парке."
                            }
                        },
                        {
                            "text": "Meteorolodzy ostrzegają, że zbliżający się <w>huragan</w> może zniszczyć linie energetyczne.",
                            "target_lang_translations": {
                                "ru": "Метеорологи предупреждают, что приближающийся <w>ураган</w> может разрушить линии электропередачи."
                            }
                        }
                    ],
                    "synonyms": [
                        "orkan",
                        "tajfun",
                        "wichura",
                        "nawałnica"
                    ],
                    "common_phrases": [
                        "oko huraganu",
                        "niszczycielski huragan",
                        "przejście huraganu",
                        "siła huraganu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень сильный, порывистый ветер со скоростью более 33 м/с, обычно сопровождающий тропические циклоны и вызывающий большие разрушения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ураган",
                                "target_lang_sense_clarification": "атмосферное явление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96020468-5b55-3a5a-b0af-aee541e845c5",
                    "sense_definition": "Gwałtowny i nagły wybuch silnych emocji lub głośnej reakcji dużej grupy ludzi, na przykład śmiechu lub oklasków.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_outburst",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po zakończeniu spektaklu zachwycona widownia odpowiedziała <w>huraganem</w> braw.",
                            "target_lang_translations": {
                                "ru": "После окончания спектакля восхищённая публика ответила <w>шквалом</w> аплодисментов."
                            }
                        },
                        {
                            "text": "Jego niefortunne przejęzyczenie wywołało <w>huragan</w> śmiechu w całej sali wykładowej.",
                            "target_lang_translations": {
                                "ru": "Его неудачная оговорка вызвала <w>взрыв</w> смеха во всей лекционной аудитории."
                            }
                        }
                    ],
                    "synonyms": [
                        "wybuch",
                        "burza"
                    ],
                    "common_phrases": [
                        "huragan braw",
                        "huragan śmiechu",
                        "huragan oklasków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бурный и внезапный всплеск сильных эмоций или громкая реакция большой группы людей, например смех или аплодисменты."
                    },
                    "translations": {

{
    "word_family": [
        "rozbijać",
        "rozbicie",
        "rozbitek",
        "rozbiór"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "23dc5f02-fa32-3a7d-8870-54a91421b851",
                    "sense_definition": "Zniszczyć kruchy przedmiot (np. szkło, naczynie) na kawałki poprzez uderzenie lub upuszczenie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "destruction_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kot wskoczył na stół i <rozbił> mój ulubiony kubek.",
                            "target_lang_translations": {
                                "ru": "Кот запрыгнул на стол и <разбил> мою любимую кружку."
                            }
                        },
                        {
                            "text": "Uważaj, żeby nie <rozbić> tych jajek w torbie z zakupami.",
                            "target_lang_translations": {
                                "ru": "Осторожно, чтобы не <разбить> эти яйца в сумке с покупками."
                            }
                        }
                    ],
                    "synonyms": [
                        "stłuc",
                        "zbić"
                    ],
                    "antonyms": [
                        "skleić",
                        "naprawić"
                    ],
                    "common_phrases": [
                        "rozbić szklankę",
                        "rozbić talerz",
                        "rozbić lustro",
                        "rozbić jajko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушить хрупкий предмет (например, стекло, посуду) на куски ударом или падением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбить",
                                "target_lang_sense_clarification": "уничтожить хрупкий предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d4425d1-fffb-3d33-b5e5-6e6a15248e80",
                    "sense_definition": "O przedmiocie: rozpaść się na kawałki w wyniku upadku lub uderzenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "destruction_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wazon spadł z półki i <rozbił się> na drobne kawałki.",
                            "target_lang_translations": {
                                "ru": "Ваза упала с полки и <разбилась> на мелкие кусочки."
                            }
                        },
                        {
                            "text": "To szkło jest hartowane, więc nie powinno się łatwo <rozbić>.",
                            "target_lang_translations": {
                                "ru": "Это стекло закаленное, поэтому оно не должно легко <разбиться>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stłuc się",
                        "zbić się"
                    ],
                    "common_phrases": [
                        "szklanka się rozbiła",
                        "talerz się rozbił",
                        "rozbić się w drobny mak"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny (reflexive verb)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О предмете: разлететься на куски в результате падения или удара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбиться",
                                "target_lang_sense_clarification": "разлететься на осколки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5626595b-efed-382d-877e-98ac7d38ca6e",
                    "sense_definition": "Ustawić namiot lub przygotować obozowisko w wybranym miejscu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "camping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Postanowiliśmy <rozbić> namiot blisko jeziora, żeby mieć ładny widok.",
                            "target_lang_translations": {
                                "ru": "Мы решили <разбить> палатку у озера, чтобы был красивый вид."
                            }
                        },
                        {
                            "text": "Harcerze musieli szybko <rozbić> obóz przed nadejściem nocy.",
                            "target_lang_translations": {
                                "ru": "Скауты должны были быстро <разбить> лагерь до наступления ночи."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozłożyć",
                        "ustawić"
                    ],
                    "antonyms": [
                        "zwinąć",
                        "złożyć"
                    ],
                    "common_phrases": [
                        "rozbić namiot",
                        "rozbić obóz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установить палатку или подготовить лагерь в выбранном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбить",
                                "target_lang_sense_clarification": "установить (лагерь, палатку)"
                            },
                            {
                                "target_lang_word": "поставить",
                                "target_lang_sense_clarification": "разместить палатку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d52fcbdc-9bff-3f96-a43a-50305edf2217",
                    "sense_definition": "Zranić lub uszkodzić część ciała (zwykle głowę, kolano, nos) w wyniku uderzenia o twardą powierzchnię.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopiec przewrócił się na boisku i <rozbił> sobie kolano.",
                            "target_lang_translations": {
                                "ru": "Мальчик упал на площадке и <разбил> себе колено."
                            }
                        },
                        {
                            "text": "W ciemności uderzyłem o szafkę i <rozbiłem> łuk brwiowy.",
                            "target_lang_translations": {
                                "ru": "В темноте я ударился о шкафчик и <разбил> бровь."
                            }
                        }
                    ],
                    "synonyms": [
                        "zranić",
                        "poturbować"
                    ],
                    "common_phrases": [
                        "rozbić nos",
                        "rozbić kolano",
                        "rozbić głowę",
                        "rozbić wargę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно ушибить или поранить часть тела (обычно голову, колено, нос) при ударе о твердую поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбить",
                                "target_lang_sense_clarification": "поранить до крови"
                            },
                            {
                                "target_lang_word": "расшибить",
                                "target_lang_sense_clarification": "сильно ушибить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf1dbb0c-d48c-3171-aba3-2417cafeffaf",
                    "sense_definition": "O pojeździe lub kierowcy: ulec poważnemu wypadkowi, zderzyć się z czymś, powodując zniszczenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "accident",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samolot <rozbił się> podczas próby lądowania w gęstej mgle.",
                            "target_lang_translations": {
                                "ru": "Самолет <разбился> при попытке посадки в густом тумане."
                            }
                        },
                        {
                            "text": "Kierowca jechał zbyt szybko i <rozbił się> na drzewie.",
                            "target_lang_translations": {
                                "ru": "Водитель ехал слишком быстро и <разбился> о дерево."
                            }
                        }
                    ],
                    "synonyms": [
                        "ulec wypadkowi"
                    ],
                    "common_phrases": [
                        "rozbić się samochodem",
                        "rozbić się samolotem",
                        "rozbić się o drzewo"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny (reflexive verb)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О транспортном средстве или водителе: попасть в серьезную аварию, потерпеть крушение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбиться",
                                "target_lang_sense_clarification": "попасть в аварию, потерпеть крушение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff196359-ccd1-3753-bd3c-0a7e7ce1a212",
                    "sense_definition": "Doprowadzić do zniszczenia pojazdu w wyniku wypadku lub zderzenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "accident",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pożyczył samochód od ojca i niestety go <rozbił>.",
                            "target_lang_translations": {
                                "ru": "Он одолжил машину у отца и, к сожалению, <разбил> ее."
                            }
                        },
                        {
                            "text": "Stracił panowanie nad kierownicą i <rozbił> auto o barierki.",
                            "target_lang_translations": {
                                "ru": "Он потерял управление и <разбил> машину об ограждение."
                            }
                        }
                    ],
                    "synonyms": [
                        "zniszczyć",
                        "rozwalić"
                    ],
                    "common_phrases": [
                        "rozbić samochód",
                        "rozbić auto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести транспортное средство в негодность в результате аварии или столкновения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбить",
                                "target_lang_sense_clarification": "повредить машину в аварии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b388f52-a4cb-3b23-9ed4-4d5f55daf13a",
                    "sense_definition": "Uderzać mięso tłuczkiem, aby stało się cieńsze i bardziej miękkie przed smażeniem.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <rozbić> schab na kotlety, zanim zacznę je smażyć.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <отбить> свинину на отбивные, прежде чем начать их жарить."
                            }
                        },
                        {
                            "text": "Dobrze <rozbite> mięso będzie delikatniejsze po usmażeniu.",
                            "target_lang_translations": {
                                "ru": "Хорошо <отбитое> мясо будет нежнее после жарки."
                            }
                        }
                    ],
                    "synonyms": [
                        "stłuc",
                        "bić"
                    ],
                    "common_phrases": [
                        "rozbić kotlety",
                        "rozbić mięso"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отбивать мясо кухонным молотком, чтобы оно стало тоньше и мягче перед жаркой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отбить",
                                "target_lang_sense_clarification": "размягчить мясо ударами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cb29b2d-22c4-3b9d-a635-cf4f167b99c3",
                    "sense_definition": "Całkowicie pokonać siły wroga lub przeciwnika w walce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Generał zaplanował atak, który pozwolił ostatecznie <rozbić> armię nieprzyjaciela.",
                            "target_lang_translations": {
                                "ru": "Генерал спланировал атаку, которая позволила окончательно <разбить> армию неприятеля."
                            }
                        },
                        {
                            "text": "Nasza drużyna zdołała <rozbić> obronę przeciwnika w drugiej połowie meczu.",
                            "target_lang_translations": {
                                "ru": "Нашей команде удалось <взломать> оборону противника во второй половине матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokonać",
                        "rozgromić",
                        "zwyciężyć"
                    ],
                    "antonyms": [
                        "przegrać",
                        "ulec"
                    ],
                    "common_phrases": [
                        "rozbić armię",
                        "rozbić wroga",
                        "rozbić oddział"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью победить силы врага или противника в бою."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбить",
                                "target_lang_sense_clarification": "нанести поражение"
                            },
                            {
                                "target_lang_word": "разгромить",
                                "target_lang_sense_clarification": "полностью уничтожить силы врага"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
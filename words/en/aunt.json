            "senses": [
                {
                    "sense_id": "844c4b86-8090-341b-ac8d-9d0343dcac7a",
                    "sense_definition": "The sister or sister-in-law of one’s parent.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kinship_female",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>aunt</w> lives in a different city, so we visit her in the summer.",
                            "target_lang_translations": {
                                "ru": "Моя <w>тётя</w> живёт в другом городе, поэтому мы навещаем её летом."
                            }
                        },
                        {
                            "text": "I received a lovely birthday card from my <w>aunt</w> yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера я получил чудесную открытку на день рождения от моей <w>тёти</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "auntie",
                        "aunty"
                    ],
                    "antonyms": [
                        "uncle",
                        "nephew",
                        "niece"
                    ],
                    "common_phrases": [
                        "my aunt",
                        "great-aunt",
                        "maiden aunt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сестра отца или матери, или жена дяди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тётя",
                                "target_lang_sense_clarification": "нейтральное, общеупотребительное"
                            },
                            {
                                "target_lang_word": "тётка",
                                "target_lang_sense_clarification": "разговорное, иногда пренебрежительное"
                            },
                            {
                                "target_lang_word": "тётушка",
                                "target_lang_sense_clarification": "ласковое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d57d27b6-29a4-3755-b328-d3971a031af4",
                    "sense_definition": "A woman of an older generation, especially a close friend of one's parents, treated as family.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "kinship_female",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is not my real relative, but I have always called her <w>aunt</w>.",
                            "target_lang_translations": {
                                "ru": "Она мне не родная, но я всегда называл её <w>тётей</w>."
                            }
                        },
                        {
                            "text": "My mother's best friend acts like an <w>aunt</w> to all of us.",
                            "target_lang_translations": {
                                "ru": "Лучшая подруга моей мамы ведёт себя как <w>тётя</w> по отношению ко всем нам."
                            }
                        }
                    ],
                    "synonyms": [
                        "auntie",
                        "aunty"
                    ],
                    "common_phrases": [
                        "honorary aunt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина старшего поколения, особенно близкая подруга родителей, к которой относятся как к члену семьи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тётя",
                                "target_lang_sense_clarification": "обращение к старшей женщине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "796005e7-59e3-352b-972c-cdec11e20f55",
                    "sense_definition": "The female cousin or cousin-in-law of one’s parent.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "kinship_female",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Although she is technically my mother's cousin, we refer to her as <w>aunt</w>.",
                            "target_lang_translations": {
                                "ru": "Хотя формально она двоюродная сестра моей мамы, мы называем её <w>тётей</w>."
                            }
                        },
                        {
                            "text": "The family reunion included every distant <w>aunt</w> and uncle.",
                            "target_lang_translations": {
                                "ru": "На семейную встречу приехали все дальние <w>тёти</w> и дяди."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Двоюродная сестра отца или матери."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тётя",
                                "target_lang_sense_clarification": "общее название родственницы"
                            },
                            {
                                "target_lang_word": "двоюродная тётя",
                                "target_lang_sense_clarification": "точное определение родства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d14e488f-3dcd-32ea-9d9d-f9e444875ddd",
                    "sense_definition": "A procuress or bawd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the old play, the character was portrayed as a wicked <w>aunt</w>.",
                            "target_lang_translations": {
                                "ru": "В старой пьесе этот персонаж был изображён как злая <w>сводня</w>."
                            }
                        },
                        {
                            "text": "He warned the young man to avoid the house of the notorious <w>aunt</w>.",
                            "target_lang_translations": {
                                "ru": "Он предупредил юношу обходить стороной дом печально известной <w>сводни</w>."
                            }

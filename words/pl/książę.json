{
    "word_family": [
        "księżna",
        "księżniczka",
        "księstwo",
        "książęcy",
        "książątko"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c9b13c36-0c1c-3a34-8134-f371db73d0fa",
                    "sense_definition": "Syn króla, cesarza lub innego monarchy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "royal_family",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody <książę> zakochał się w pięknej dziewczynie z sąsiedniego królestwa.",
                            "target_lang_translations": {
                                "ru": "Молодой <принц> влюбился в прекрасную девушку из соседнего королевства."
                            }
                        },
                        {
                            "text": "Każda dziewczynka marzy, by przyjechał po nią <książę> z bajki na białym koniu.",
                            "target_lang_translations": {
                                "ru": "Каждая девочка мечтает, чтобы за ней приехал <принц> из сказки на белом коне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "książę z bajki",
                        "mały książę",
                        "książę krwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сын короля, императора или другого монарха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принц",
                                "target_lang_sense_clarification": "сын монарха, наследник"
                            },
                            {
                                "target_lang_word": "княжич",
                                "target_lang_sense_clarification": "сын князя (устар.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acdec203-eb08-3ad1-ad46-28f2055328a3",
                    "sense_definition": "Władca państwa lub terytorium nazywanego księstwem, nieposiadający tytułu króla.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ruler",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszko I nie był królem, a jedynie <księciem> Polan.",
                            "target_lang_translations": {
                                "ru": "Мешко I был не королём, а всего лишь <князем> полян."
                            }
                        },
                        {
                            "text": "Panujący <książę> wydał nowy dekret dotyczący podatków w swoim kraju.",
                            "target_lang_translations": {
                                "ru": "Правящий <князь> издал новый указ, касающийся налогов в своей стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "kniaź"
                    ],
                    "common_phrases": [
                        "wielki książę",
                        "książę małżonek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правитель государства или территории, называемой княжеством, не имеющий титула короля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "князь",
                                "target_lang_sense_clarification": "правитель феодального государства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71190e84-e69e-3a38-bf04-6be816eb3754",
                    "sense_definition": "Arystokrata noszący wysoki tytuł rodowy, zazwyczaj wyższy od hrabiego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nobility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Brytyjski następca tronu tradycyjnie nosi tytuł <Księcia> Walii.",
                            "target_lang_translations": {
                                "ru": "Британский наследник престола традиционно носит титул <принца> Уэльского."
                            }
                        },
                        {
                            "text": "Na balu pojawił się bogaty <książę>, który szukał żony wśród arystokracji.",
                            "target_lang_translations": {
                                "ru": "На балу появился богатый <князь>, который искал жену среди аристократии."
                            }
                        }
                    ],
                    "synonyms": [
                        "diuk"
                    ],
                    "common_phrases": [
                        "tytuł księcia",
                        "książę kościoła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аристократ, носящий высокий родовой титул, обычно выше графского."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герцог",
                                "target_lang_sense_clarification": "западноевропейский титул"
                            },
                            {
                                "target_lang_word": "князь",
                                "target_lang_sense_clarification": "дворянский титул"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
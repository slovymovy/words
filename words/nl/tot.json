                    ],
                    "common_phrases": [
                        "tot stand komen",
                        "in staat zijn tot",
                        "behoren tot",
                        "tot tranen toe"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates which function or capacity someone or something acquires.",

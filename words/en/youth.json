                {
                    "sense_id": "e774d6ee-2775-3731-be3f-2866382f8578",
                    "sense_definition": "The period of life between childhood and adulthood.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She traveled to many different countries in her <w>youth</w>.",
                            "target_lang_translations": {
                                "ru": "Она путешествовала по многим странам в <w>молодости</w>."
                            }
                        },
                        {
                            "text": "He regrets that he wasted his <w>youth</w> on unimportant things.",
                            "target_lang_translations": {
                                "ru": "Он жалеет, что потратил свою <w>молодость</w> на неважные вещи."
                            }
                        }
                    ],
                    "synonyms": [
                        "adolescents"
                    ],
                    "antonyms": [
                        "old age",
                        "age"
                    ],
                    "common_phrases": [

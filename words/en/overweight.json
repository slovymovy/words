    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cd59d8ef-76fb-37ab-8e10-c6f5257f5eaf",
                    "sense_definition": "Having a higher body weight, especially fat, than is generally considered healthy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "body_weight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor told him he was <w>overweight</w> and needed to change his diet.",
                            "target_lang_translations": {
                                "ru": "Врач сказал ему, что у него <w>избы́точный вес</w>, и ему нужно изменить диету."
                            }
                        },
                        {
                            "text": "Many <w>overweight</w> people find it difficult to lose the extra kilos.",
                            "target_lang_translations": {
                                "ru": "Многим людям <w>с ли́шним ве́сом</w> трудно сбросить лишние килограммы."
                            }
                        }
                    ],
                    "synonyms": [
                        "fat",
                        "obese",
                        "plump",
                        "chubby",
                        "plus-size",
                        "portly",
                        "tubby"
                    ],
                    "antonyms": [
                        "thin",
                        "slender",
                        "scrawny",
                        "underweight"
                    ],
                    "common_phrases": [
                        "slightly overweight",
                        "overweight person",
                        "overweight child"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий вес тела выше нормы, особенно из-за жировых отложений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с ли́шним ве́сом",
                                "target_lang_sense_clarification": "наиболее употребительный вариант"
                            },
                            {
                                "target_lang_word": "по́лный",
                                "target_lang_sense_clarification": "вежливое описание"
                            },
                            {
                                "target_lang_word": "ту́чный",
                                "target_lang_sense_clarification": "очень полный, грузный"
                            },
                            {
                                "target_lang_word": "страда́ющий ожире́нием",
                                "target_lang_sense_clarification": "медицинский контекст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b386d0bb-ddb9-3018-91f3-d08b6a1b81a0",
                    "sense_definition": "Weighing more than the allowed limit for safety or legal regulations.",

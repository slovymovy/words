            "senses": [
                {
                    "sense_id": "bd1781f3-95ca-3196-852b-5ae8e202b5e8",
                    "sense_definition": "Pozbawiony światła lub słabo oświetlony.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "light_and_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten pokój jest bardzo <w>ciemny</w>, więc musimy włączyć lampę.",
                            "target_lang_translations": {
                                "ru": "Эта комната очень <w>тёмная</w>, поэтому мы должны включить лампу."
                            }
                        },
                        {
                            "text": "Noc była tak <w>ciemna</w>, że nie widzieliśmy drogi do domu.",
                            "target_lang_translations": {
                                "ru": "Ночь была такой <w>тёмной</w>, что мы не видели дороги домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "mroczny"
                    ],
                    "antonyms": [
                        "jasny"
                    ],
                    "common_phrases": [
                        "ciemny pokój",
                        "ciemna noc",
                        "ciemna ulica",
                        "ciemny korytarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишённый света или слабо освещённый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тёмный",
                                "target_lang_sense_clarification": "о помещении, времени суток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f5b5842-c085-3148-baf5-169e3f1d85cc",
                    "sense_definition": "Mający kolor zbliżony do czarnego, brązowego lub granatowego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "light_and_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ona ma piękne, długie i <w>ciemne</w> włosy.",
                            "target_lang_translations": {
                                "ru": "У неё красивые длинные <w>тёмные</w> волосы."
                            }
                        },
                        {
                            "text": "Do tego garnituru pasują tylko <w>ciemne</w> buty.",
                            "target_lang_translations": {
                                "ru": "К этому костюму подходят только <w>тёмные</w> туфли."
                            }
                        }
                    ],
                    "synonyms": [
                        "czarny"
                    ],
                    "antonyms": [
                        "jasny",
                        "blady"
                    ],
                    "common_phrases": [
                        "ciemne włosy",
                        "ciemne oczy",
                        "ciemny chleb",
                        "ciemne piwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий цвет, близкий к чёрному, коричневому или тёмно-синему."

                    ],
                    "synonyms": [
                        "additionally",
                        "besides",
                        "further",
                        "moreover"
                    ],
                    "common_phrases": [
                        "yet another",
                        "yet again"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вдобавок; используется для подчеркивания увеличения количества или повторения действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ещё",
                                "target_lang_sense_clarification": "дополнительно"
                            },
                            {
                                "target_lang_word": "очередной",
                                "target_lang_sense_clarification": "ещё один"

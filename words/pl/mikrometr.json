{
    "word_family": [
        "mikrometryczny",
        "mikrometria"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "44d9d9df-00ff-3add-924c-79e2c85e2100",
                    "sense_definition": "Jednostka długości równa jednej milionowej części metra.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Włos ludzki ma grubość kilkudziesięciu <w>mikrometrów</w>.",
                            "target_lang_translations": {
                                "ru": "Толщина человеческого волоса составляет несколько десятков <w>микрометров</w>."
                            }
                        },
                        {
                            "text": "Jeden <w>mikrometr</w> to jedna tysięczna milimetra.",
                            "target_lang_translations": {
                                "ru": "Один <w>микрометр</w> — это одна тысячная миллиметра."
                            }
                        }
                    ],
                    "synonyms": [
                        "µm"
                    ],
                    "common_phrases": [
                        "jeden mikrometr",
                        "kilka mikrometrów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица длины, равная одной миллионной части метра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "микрометр",
                                "target_lang_sense_clarification": "единица измерения"
                            },
                            {
                                "target_lang_word": "микрон",
                                "target_lang_sense_clarification": "устаревшее, но часто используемое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d92840db-b55e-3548-892a-1c4b3525ff71",
                    "sense_definition": "Narzędzie pomiarowe służące do mierzenia bardzo małych długości lub grubości z dużą precyzją.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "measuring_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mechanik użył <w>mikrometru</w>, aby dokładnie zmierzyć średnicę śruby.",
                            "target_lang_translations": {
                                "ru": "Механик использовал <w>микрометр</w>, чтобы точно измерить диаметр винта."
                            }
                        },
                        {
                            "text": "W warsztacie niezbędny jest sprawny <w>mikrometr</w>.",
                            "target_lang_translations": {
                                "ru": "В мастерской необходим исправный <w>микрометр</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "drobnomierz",
                        "śruba mikrometryczna",
                        "leniwka"
                    ],
                    "common_phrases": [
                        "precyzyjny mikrometr",
                        "pomiar mikrometrem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Измерительный инструмент для точного измерения малых расстояний или толщины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "микрометр",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e59bb6ec-b771-3015-84c4-37343b994368",
                    "sense_definition": "Przyrząd astronomiczny do wyznaczania położenia ciał na niebie oraz odległości kątowej między nimi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measuring_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Astronom wykorzystał <w>mikrometr</w> do obserwacji gwiazd podwójnych.",
                            "target_lang_translations": {
                                "ru": "Астроном использовал <w>микрометр</w> для наблюдения за двойными звездами."
                            }
                        },
                        {
                            "text": "Ten teleskop jest wyposażony w specjalistyczny <w>mikrometr</w>.",
                            "target_lang_translations": {
                                "ru": "Этот телескоп оснащен специализированным <w>микрометром</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Астрономический прибор для измерения малых угловых расстояний между небесными телами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "микрометр",
                                "target_lang_sense_clarification": "астрономический прибор"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
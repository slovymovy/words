                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This is a matter of <w>great</w> importance to the community.",
                            "target_lang_translations": {
                                "ru": "Это дело <w>огромной</w> важности для общества."
                            }
                        },
                        {
                            "text": "He was considered a <w>great</w> man in the history of science.",
                            "target_lang_translations": {
                                "ru": "Он считался <w>великим</w> человеком в истории науки."
                            }
                        }
                    ],
                    "synonyms": [
                        "grand"
                    ],
                    "antonyms": [
                        "ordinary",
                        "mediocre"
                    ],
                    "common_phrases": [
                        "great importance",
                        "great man",
                        "great success"
                    ],

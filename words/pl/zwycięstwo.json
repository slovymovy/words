                    ],
                    "synonyms": [
                        "triumf",
                        "wygrana"
                    ],
                    "antonyms": [
                        "porażka",
                        "fiasko"
                    ],
                    "common_phrases": [
                        "zwycięstwo nad sobą",
                        "moralne zwycięstwo",
                        "osobiste zwycięstwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Успешное преодоление трудной ситуации, проблемы или собственной слабости"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "победа",
                                "target_lang_sense_clarification": "над трудностями или собой"

            "senses": [
                {
                    "sense_id": "aefd90c5-5f57-3f30-82e8-9e61bdeb647d",
                    "sense_definition": "Meervoud verleden tijd van <w>hellen</w>",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude bomen <w>helden</w> gevaarlijk over de weg na de zware storm.",
                            "target_lang_translations": {
                                "ru": "После сильного шторма старые деревья опасно <w>наклонились</w> над дорогой."
                            }
                        },
                        {
                            "text": "Wij <w>helden</w> allemaal een beetje naar voren om de spreker beter te kunnen horen.",
                            "target_lang_translations": {
                                "ru": "Мы все немного <w>наклонились</w> вперед, чтобы лучше слышать оратора."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время (множественное число) глагола \"hellen\" (наклоняться, иметь наклон)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наклонялись",
                                "target_lang_sense_clarification": "отклонялись от вертикали или сгибались"
                            },
                            {
                                "target_lang_word": "кренились",
                                "target_lang_sense_clarification": "наклонялись набок (часто о предметах)"
                            }
                        ]
                    }
                }
            ]
        },
        {

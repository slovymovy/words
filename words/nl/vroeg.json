        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c07d2bab-9f1c-32a0-a058-41e8811e8408",
                    "sense_definition": "Op een tijdstip dat dicht bij het begin van de dag of een periode ligt; niet laat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zon komt in de zomer altijd erg <w>vroeg</w> op.",
                            "target_lang_translations": {
                                "en": "The sun always rises very <w>early</w> in the summer.",
                                "ru": "Летом солнце всегда встает очень <w>рано</w>."
                            }
                        },
                        {
                            "text": "Ik moet morgen <w>vroeg</w> opstaan om de trein te halen.",
                            "target_lang_translations": {
                                "en": "I have to get up <w>early</w> tomorrow to catch the train.",
                                "ru": "Завтра мне нужно <w>рано</w> встать, чтобы успеть на поезд."
                            }
                        }
                    ],
                    "antonyms": [
                        "laat"
                    ],
                    "common_phrases": [
                        "vroeg of laat",
                        "vroeg in de ochtend",
                        "lekker vroeg",
                        "te vroeg"
                    ],
                    "target_lang_definitions": {
                        "en": "At a time near the beginning of the day or a period; not late.",
                        "ru": "Происходящий в начале дня или периода; не поздний."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "early",
                                "target_lang_sense_clarification": "near the start of the day"
                            }
                        ],
                        "ru": [

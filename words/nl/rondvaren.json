    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "239dc51a-7e14-32a8-9ced-5d09f5b2ee75",
                    "sense_definition": "Met een boot een tocht maken, vaak voor het plezier, en weer terugkeren naar het beginpunt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben de hele middag heerlijk op het meer <w>rondgevaren</w>.",
                            "target_lang_translations": {
                                "en": "We spent the whole afternoon delightfully <w>cruising around</w> on the lake.",
                                "ru": "Мы весь день чудесно <w>катались</w> по озеру."
                            }
                        },
                        {
                            "text": "In de zomer kun je hier met een klein bootje <w>rondvaren</w>.",
                            "target_lang_translations": {
                                "en": "In the summer, you can <w>sail around</w> here in a small boat.",
                                "ru": "Летом здесь можно <w>покататься</w> на маленькой лодке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lekker rondvaren",
                        "op de grachten rondvaren"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a trip by boat, often for pleasure, and return to the starting point.",
                        "ru": "Совершать поездку на лодке или судне, часто ради удовольствия, возвращаясь в исходную точку."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cruise around",
                                "target_lang_sense_clarification": "leisurely movement by boat"
                            },
                            {
                                "target_lang_word": "sail around",
                                "target_lang_sense_clarification": "general boating activity"
                            },
                            {
                                "target_lang_word": "boat around",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кататься на лодке",
                                "target_lang_sense_clarification": "развлекательная прогулка"
                            },
                            {
                                "target_lang_word": "совершать водную прогулку",
                                "target_lang_sense_clarification": "формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "890d1808-9061-3a1b-9a0d-e059e569321e",
                    "sense_definition": "Iemand meenemen op een boottocht en rondleiden over het water.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gids heeft de toeristen een uur lang door de stad <w>rondgevaren</w>.",
                            "target_lang_translations": {
                                "en": "The guide <w>took</w> the tourists <w>for a boat ride</w> through the city for an hour.",
                                "ru": "Гид целый час <w>возил</w> туристов по городу на лодке."
                            }
                        },
                        {
                            "text": "Hij wil zijn familie graag in zijn nieuwe boot <w>rondvaren</w>.",
                            "target_lang_translations": {
                                "en": "He would like to <w>take</w> his family <w>out</w> in his new boat.",
                                "ru": "Он хочет <w>покатать</w> свою семью на своей новой лодке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "toeristen rondvaren",
                        "gasten rondvaren"
                    ],
                    "target_lang_definitions": {
                        "en": "To take someone on a boat trip and show them around on the water.",
                        "ru": "Брать кого-либо на водную прогулку и показывать окрестности с воды."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take for a boat ride",
                                "target_lang_sense_clarification": "transporting passengers for leisure"
                            },
                            {
                                "target_lang_word": "show around by boat",
                                "target_lang_sense_clarification": "guiding someone on water"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "катать",
                                "target_lang_sense_clarification": "кого-либо на лодке"
                            },
                            {
                                "target_lang_word": "возить",
                                "target_lang_sense_clarification": "пассажиров по воде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82c60b12-dbaf-3d0b-a21f-264015eb8081",
                    "sense_definition": "Volledig om een landmassa, eiland of de aarde heen varen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zeilers hebben in twee jaar tijd de hele wereld <w>rondgevaren</w>.",
                            "target_lang_translations": {
                                "en": "The sailors <w>circumnavigated</w> the entire world in two years.",
                                "ru": "Моряки за два года <w>оплыли</w> весь мир."
                            }
                        },
                        {
                            "text": "Het schip moest het grote eiland <w>rondvaren</w> om de haven te bereiken.",
                            "target_lang_translations": {
                                "en": "The ship had to <w>sail around</w> the large island to reach the harbor.",
                                "ru": "Корабль должен был <w>обогнуть</w> большой остров, чтобы добраться до порта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de wereld rondvaren",
                        "een eiland rondvaren"
                    ],
                    "target_lang_definitions": {
                        "en": "To sail completely around a landmass, island, or the earth.",
                        "ru": "Полностью оплывать участок суши, остров или земной шар."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "circumnavigate",
                                "target_lang_sense_clarification": "formal/technical term"
                            },
                            {
                                "target_lang_word": "sail around",
                                "target_lang_sense_clarification": "encircling an object by boat"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оплывать",
                                "target_lang_sense_clarification": "вокруг объекта"
                            },
                            {
                                "target_lang_word": "огибать",
                                "target_lang_sense_clarification": "проходить мимо или вокруг"
                            }
                        ]
                    }
                }
            ]
        }
    ]

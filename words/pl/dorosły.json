    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ecc73444-1d21-317e-ad2b-6df9681e32aa",
                    "sense_definition": "O człowieku lub zwierzęciu: będący w wieku, w którym organizm jest w pełni ukształtowany fizycznie i zdolny do rozrodu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "biological_maturity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wielu krajach człowiek staje się <w>dorosły</w> po osiągnięciu osiemnastu lat.",
                            "target_lang_translations": {
                                "ru": "Во многих странах человек становится <w>взрослым</w> по достижении восемнадцати лет."
                            }
                        },
                        {
                            "text": "Czy w tym sklepie jest dostępna karma dla <w>dorosłych</w> psów?",
                            "target_lang_translations": {
                                "ru": "Есть ли в этом магазине корм для <w>взрослых</w> собак?"
                            }
                        }
                    ],
                    "synonyms": [
                        "dojrzały"
                    ],
                    "antonyms": [
                        "dziecko",
                        "dorosły"
                    ],
                    "common_phrases": [
                        "dorosły człowiek",
                        "dorosły pies",
                        "dorosły osobnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке или животном: находящийся в возрасте, когда организм полностью сформирован физически и способен к размножению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрослый",
                                "target_lang_sense_clarification": "достигший физической зрелости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78697e4f-f74a-34da-9468-f199cad3bc6e",
                    "sense_definition": "O człowieku: mający prawa i obowiązki osoby pełnoletniej; ponoszący pełną odpowiedzialność prawną za swoje czyny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legal_maturity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako <w>dorosły</w> obywatel masz prawo głosować w wyborach prezydenckich.",
                            "target_lang_translations": {
                                "ru": "Как <w>взрослый</w> гражданин, ты имеешь право голосовать на президентских выборах."
                            }
                        },
                        {
                            "text": "Kiedy będziesz <w>dorosły</w>, zrozumiesz, jak trudno jest zarządzać domowym budżetem.",
                            "target_lang_translations": {
                                "ru": "Когда ты станешь <w>взрослым</w>, ты поймешь, как трудно управлять семейным бюджетом."
                            }
                        }
                    ],
                    "synonyms": [
                        "dojrzały",
                        "dojrzały"
                    ],
                    "antonyms": [
                        "dziecko",
                        "dorosły"
                    ],
                    "common_phrases": [
                        "być dorosłym",
                        "stać się dorosłym",
                        "życie dorosłe"
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке: имеющий права и обязанности совершеннолетнего лица; несущий полную юридическую ответственность за свои поступки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрослый",
                                "target_lang_sense_clarification": "самостоятельный"
                            },
                            {
                                "target_lang_word": "совершеннолетний",
                                "target_lang_sense_clarification": "юридический статус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff8fbb40-e222-34a0-8a43-46fe066e5319",
                    "sense_definition": "Cechujący się powagą, rozsądkiem i odpowiedzialnością właściwą osobom dojrzałym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_maturity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To była bardzo <w>dorosła</w> decyzja, z której twoi rodzice mogą być dumni.",
                            "target_lang_translations": {
                                "ru": "Это было очень <w>взрослое</w> решение, которым твои родители могут гордиться."
                            }
                        },
                        {
                            "text": "Jego <w>dorosłe</w> podejście do problemów zaskoczyło wszystkich nauczycieli w szkole.",
                            "target_lang_translations": {
                                "ru": "Его <w>взрослый</w> подход к проблемам удивил всех учителей в школе."
                            }
                        }
                    ],
                    "synonyms": [
                        "dojrzały",
                        "dojrzały"
                    ],
                    "antonyms": [
                        "dziecko",
                        "dorosły"
                    ],
                    "common_phrases": [
                        "dorosłe zachowanie",
                        "dorosła decyzja",
                        "dorosłe podejście"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся серьезностью, рассудительностью и ответственностью, свойственной зрелым людям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрослый",
                                "target_lang_sense_clarification": "серьезный, ответственный"
                            },
                            {
                                "target_lang_word": "зрелый",
                                "target_lang_sense_clarification": "рассудительный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00e12ada-1ec7-3193-9ece-c27038124825",
                    "sense_definition": "Osoba, która osiągnęła dojrzałość fizyczną lub prawną; człowiek niebędący już dzieckiem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do kasyna mogą wejść tylko <w>dorośli</w>, więc musisz poczekać na zewnątrz.",
                            "target_lang_translations": {
                                "ru": "В казино могут входить только <w>взрослые</w>, поэтому тебе придется подождать снаружи."
                            }
                        },
                        {
                            "text": "Dzieci bawiły się w ogrodzie, a <w>dorośli</w> rozmawiali przy stole na tarasie.",
                            "target_lang_translations": {
                                "ru": "Дети играли в саду, а <w>взрослые</w> разговаривали за столом на террасе."
                            }
                        }
                    ],
                    "antonyms": [
                        "dziecko"
                    ],
                    "common_phrases": [
                        "bilet dla dorosłych",
                        "film dla dorosłych",
                        "świat dorosłych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, достигший физической или юридической зрелости; человек, который уже не является ребенком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрослый",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d5aec91d-69b5-3c30-afb8-05031aea7805",
                    "sense_definition": "Iemand die iets uitzonderlijk goed kan of schijnbaar onmogelijke prestaties levert.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De spits is een ware <w>duivelskunstenaar</w> aan de bal.",
                            "target_lang_translations": {
                                "ru": "Этот нападающий — настоящий <w>кудесник</w> мяча."
                            }
                        },
                        {
                            "text": "Als politiek <w>duivelskunstenaar</w> wist hij elke crisis te overleven.",
                            "target_lang_translations": {
                                "ru": "Будучи политическим <w>виртуозом</w>, он сумел пережить любой кризис."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een ware duivelskunstenaar",
                        "politiek duivelskunstenaar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто обладает исключительным мастерством или совершает, казалось бы, невозможное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виртуоз",
                                "target_lang_sense_clarification": "человек, достигший высшей степени мастерства"
                            },
                            {
                                "target_lang_word": "кудесник",
                                "target_lang_sense_clarification": "волшебник (в переносном смысле), мастер своего дела"
                            },
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "квалифицированный специалист"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68d1efa0-d0e9-3bf0-85f4-53cb6c43acac",
                    "sense_definition": "Een beoefenaar van zwarte magie of toverkunst.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de legende sloot de <w>duivelskunstenaar</w> een pact met het kwaad.",
                            "target_lang_translations": {
                                "ru": "Согласно легенде, <w>чернокнижник</w> заключил сделку со злом."
                            }
                        },
                        {
                            "text": "De oude <w>duivelskunstenaar</w> brouwde een geheimzinnig drankje.",
                            "target_lang_translations": {
                                "ru": "Старый <w>колдун</w> варил таинственное зелье."
                            }
                        }
                    ],
                    "synonyms": [
                        "tovenaar",
                        "heksenmeester"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто практикует черную магию или колдовство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чернокнижник",
                                "target_lang_sense_clarification": "колдун, занимающийся черной магией"
                            },
                            {
                                "target_lang_word": "колдун",
                                "target_lang_sense_clarification": "человек, занимающийся колдовством"
                            }
                        ]
                    }
                }
            ]
        }

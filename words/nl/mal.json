                            {
                                "target_lang_word": "mould",
                                "target_lang_sense_clarification": "hollow form for casting (UK spelling)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "форма",
                                "target_lang_sense_clarification": "для литья или формовки"
                            },
                            {
                                "target_lang_word": "матрица",
                                "target_lang_sense_clarification": "техническая форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e08d2051-af4f-3229-9913-ee80637e066a",

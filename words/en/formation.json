    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c90da8d-f551-3a78-abd3-cfa31153156c",
                    "sense_definition": "The act or process of creating something or the way in which something develops.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>formation</w> of the new government took several weeks of negotiation.",
                            "target_lang_translations": {
                                "ru": "<w>Формирование</w> нового правительства заняло несколько недель переговоров."
                            }
                        },
                        {
                            "text": "Scientists are observing the <w>formation</w> of stars in a distant galaxy.",
                            "target_lang_translations": {
                                "ru": "Ученые наблюдают за <w>образованием</w> звезд в далекой галактике."
                            }
                        }
                    ],
                    "synonyms": [
                        "creation",
                        "development",
                        "establishment",
                        "constitution"
                    ],
                    "antonyms": [
                        "destruction",
                        "dissolution"
                    ],
                    "common_phrases": [
                        "formation of",
                        "star formation",
                        "capital formation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс создания чего-либо, или то, как что-то развивается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формирование",
                                "target_lang_sense_clarification": "процесс придания формы или развития"
                            },
                            {
                                "target_lang_word": "образование",
                                "target_lang_sense_clarification": "создание или возникновение чего-либо"
                            },
                            {
                                "target_lang_word": "создание",
                                "target_lang_sense_clarification": "процесс творения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b4dfd84-3a6c-3b9c-a3f1-a3286f4b7eee",
                    "sense_definition": "A specific arrangement or pattern in which people or things are organized, especially soldiers, vehicles, or sports players.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fighter jets flew over the stadium in a tight <w>formation</w>.",
                            "target_lang_translations": {
                                "ru": "Истребители пролетели над стадионом в плотном <w>строю</w>."
                            }
                        },
                        {
                            "text": "The football team switched to a defensive <w>formation</w> to protect their lead.",
                            "target_lang_translations": {
                                "ru": "Футбольная команда перешла на оборонительное <w>построение</w>, чтобы удержать преимущество."
                            }
                        }
                    ],
                    "synonyms": [
                        "arrangement",
                        "configuration",
                        "layout",
                        "alignment"
                    ],
                    "common_phrases": [
                        "in formation",
                        "fly in formation",
                        "battle formation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особое расположение или порядок, в котором организованы люди или предметы, особенно солдаты, транспортные средства или спортсмены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строй",
                                "target_lang_sense_clarification": "военный или полетный порядок"
                            },
                            {
                                "target_lang_word": "построение",
                                "target_lang_sense_clarification": "расположение игроков или войск"
                            },
                            {
                                "target_lang_word": "боевой порядок",
                                "target_lang_sense_clarification": "военный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d231ff4-fd1d-33f1-aafc-518974b204d2",
                    "sense_definition": "A thing that has been formed, especially a natural structure or shape.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We stopped to take pictures of the unusual rock <w>formation</w> in the desert.",
                            "target_lang_translations": {
                                "ru": "Мы остановились, чтобы сфотографировать необычное скальное <w>образование</w> в пустыне."
                            }
                        },
                        {
                            "text": "The pilot navigated around a dangerous cloud <w>formation</w> during the flight.",
                            "target_lang_translations": {
                                "ru": "Пилот облетел опасное облачное <w>образование</w> во время полета."
                            }
                        }
                    ],
                    "synonyms": [
                        "structure",
                        "shape"
                    ],
                    "common_phrases": [
                        "rock formation",
                        "cloud formation"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что было сформировано, особенно природная структура или форма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "образование",
                                "target_lang_sense_clarification": "геологическое или природное явление"
                            },
                            {
                                "target_lang_word": "структура",
                                "target_lang_sense_clarification": "строение или форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "465ef83f-cda4-331a-9739-349832708574",
                    "sense_definition": "A distinct layer or body of rock that has a common origin and characteristics.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geologists identified a limestone <w>formation</w> that contains many ancient fossils.",
                            "target_lang_translations": {
                                "ru": "Геологи обнаружили известняковую <w>формацию</w>, содержащую множество древних окаменелостей."
                            }
                        },
                        {
                            "text": "Oil was discovered deep within the shale <w>formation</w> beneath the ocean floor.",
                            "target_lang_translations": {
                                "ru": "Нефть была обнаружена глубоко внутри сланцевого <w>пласта</w> под дном океана."
                            }
                        }
                    ],
                    "synonyms": [
                        "stratum",
                        "layer"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "geology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный слой или массив горной породы, имеющий общее происхождение и характеристики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формация",
                                "target_lang_sense_clarification": "геологический термин"
                            },
                            {
                                "target_lang_word": "пласт",
                                "target_lang_sense_clarification": "слой породы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e358941-2c71-321c-94b3-3a858be76acb",
                    "sense_definition": "An organized military unit or group of units under a specific command.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The enemy <w>formation</w> advanced slowly towards the border.",
                            "target_lang_translations": {
                                "ru": "Вражеское <w>формирование</w> медленно продвигалось к границе."
                            }
                        },
                        {
                            "text": "Several armored <w>formations</w> were deployed to support the infantry.",
                            "target_lang_translations": {
                                "ru": "Несколько бронетанковых <w>соединений</w> были развернуты для поддержки пехоты."
                            }
                        }
                    ],
                    "synonyms": [
                        "unit",
                        "force"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "military"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованная воинская часть или группа частей под определенным командованием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формирование",
                                "target_lang_sense_clarification": "военное подразделение"
                            },
                            {
                                "target_lang_word": "соединение",
                                "target_lang_sense_clarification": "крупная воинская часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "803b0b08-0f2e-3570-b512-94568f4d7c82",
                    "sense_definition": "The development of a person's character or spiritual life, especially in preparation for a religious vocation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The seminary focuses on the spiritual <w>formation</w> of young priests.",
                            "target_lang_translations": {
                                "ru": "Семинария сосредотачивается на духовном <w>становлении</w> молодых священников."
                            }
                        },
                        {
                            "text": "His moral <w>formation</w> was heavily influenced by his early teachers.",
                            "target_lang_translations": {
                                "ru": "На его нравственное <w>формирование</w> сильно повлияли его первые учителя."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "religious or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Развитие характера или духовной жизни человека, особенно при подготовке к религиозному служению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "становление",
                                "target_lang_sense_clarification": "развитие личности"
                            },
                            {
                                "target_lang_word": "формирование",
                                "target_lang_sense_clarification": "развитие качеств"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            },
                            {
                                "target_lang_word": "спросите",
                                "target_lang_sense_clarification": "обращение на «вы»"
                            },
                            {
                                "target_lang_word": "проси",
                                "target_lang_sense_clarification": "просьба"
                            }
                        ]
                    }
                }

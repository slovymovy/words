{
    "word_family": [
        "oszust",
        "oszustwo",
        "oszukańczy",
        "oszustka"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d873a1e5-d03e-314a-af2e-7c0e6842f350",
                    "sense_definition": "Mówić nieprawdę lub ukrywać fakty, aby wprowadzić kogoś w błąd.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinieneś <w>oszukiwać</w> swoich rodziców w tak ważnych sprawach.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <w>обманывать</w> своих родителей в таких важных делах."
                            }
                        },
                        {
                            "text": "Mój przyjaciel nigdy nie <w>oszukuje</w> i zawsze mówi to, co myśli.",
                            "target_lang_translations": {
                                "ru": "Мой друг никогда не <w>обманывает</w> и всегда говорит то, что думает."
                            }
                        }
                    ],
                    "synonyms": [
                        "kłamać",
                        "okłamywać",
                        "wprowadzać w błąd",
                        "zwodzić",
                        "łgać"
                    ],
                    "antonyms": [
                        "być szczerym",
                        "mówić prawdę"
                    ],
                    "common_phrases": [
                        "oszukiwać w żywe oczy",
                        "przestań mnie oszukiwać",
                        "oszukiwać dla korzyści"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить неправду или скрывать факты, чтобы ввести кого-то в заблуждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обманывать",
                                "target_lang_sense_clarification": "вводить в заблуждение (общее значение)"
                            },
                            {
                                "target_lang_word": "лгать",
                                "target_lang_sense_clarification": "говорить неправду"
                            },
                            {
                                "target_lang_word": "водить за нос",
                                "target_lang_sense_clarification": "фразеологизм: дурачить кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8821d642-fa7a-35b1-8303-b3fa8b1cf8f6",
                    "sense_definition": "Łamać zasady gry, sportu lub egzaminu, aby uzyskać lepszy wynik w nieuczciwy sposób.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel zauważył, że uczeń <w>oszukuje</w> na teście z matematyki.",
                            "target_lang_translations": {
                                "ru": "Учитель заметил, что ученик <w>списывает</w> на контрольной по математике."
                            }
                        },
                        {
                            "text": "Nikt nie lubi grać z kimś, kto ciągle <w>oszukuje</w> w karty.",
                            "target_lang_translations": {
                                "ru": "Никто не любит играть с тем, кто постоянно <w>жульничает</w> в карты."
                            }
                        }
                    ],
                    "synonyms": [
                        "kantować",
                        "szachrować",
                        "ściągać"
                    ],
                    "antonyms": [
                        "grać uczciwie"
                    ],
                    "common_phrases": [
                        "oszukiwać w karty",
                        "oszukiwać na egzaminie",
                        "oszukiwać w grze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушать правила игры, спорта или экзамена, чтобы нечестным путем получить лучший результат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жульничать",
                                "target_lang_sense_clarification": "в играх или спорте"
                            },
                            {
                                "target_lang_word": "списывать",
                                "target_lang_sense_clarification": "на экзамене или тесте"
                            },
                            {
                                "target_lang_word": "мухлевать",
                                "target_lang_sense_clarification": "разговорное: нечестно играть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10103d4a-5401-375e-8146-a0aa8a0abb83",
                    "sense_definition": "Pozbawiać kogoś pieniędzy lub majątku poprzez podstęp i manipulację.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nieuczciwy sprzedawca <w>oszukiwał</w> klientów, wydając im za mało reszty.",
                            "target_lang_translations": {
                                "ru": "Нечестный продавец <w>обсчитывал</w> клиентов, давая им слишком мало сдачи."
                            }
                        },
                        {
                            "text": "Ta firma od lat <w>oszukiwała</w> na podatkach, ukrywając swoje dochody.",
                            "target_lang_translations": {
                                "ru": "Эта компания годами <w>мошенничала</w> с налогами, скрывая свои доходы."
                            }
                        }
                    ],
                    "synonyms": [
                        "naciągać",
                        "okradać",
                        "wyłudzać",
                        "defraudować"
                    ],
                    "common_phrases": [
                        "oszukiwać na wadze",
                        "oszukiwać klientów",
                        "oszukiwać państwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишать кого-то денег или имущества путем обмана и манипуляций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мошенничать",
                                "target_lang_sense_clarification": "совершать финансовые махинации"
                            },
                            {
                                "target_lang_word": "обсчитывать",
                                "target_lang_sense_clarification": "неправильно давать сдачу"
                            },
                            {
                                "target_lang_word": "надувать",
                                "target_lang_sense_clarification": "разговорное: крупно обманывать на деньги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30b97e0a-a364-348b-b690-625ba2f53198",
                    "sense_definition": "Wmawiać sobie nieprawdę, nie chcąc zaakceptować rzeczywistości (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz przestać się <w>oszukiwać</w> i w końcu spojrzeć prawdzie w oczy.",
                            "target_lang_translations": {
                                "ru": "Ты должен перестать <w>обманывать себя</w> и наконец посмотреть правде в глаза."
                            }
                        },
                        {
                            "text": "Ona <w>oszukuje</w> samą siebie, wierząc, że on do niej wróci.",
                            "target_lang_translations": {
                                "ru": "Она <w>обманывает</w> саму себя, веря, что он к ней вернется."
                            }
                        }
                    ],
                    "synonyms": [
                        "łudzić się",
                        "okłamywać się",
                        "mamić się"
                    ],
                    "common_phrases": [
                        "nie ma co się oszukiwać",
                        "oszukiwać samego siebie",
                        "przestań się oszukiwać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убеждать себя в неправде, не желая принимать реальность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обманывать себя",
                                "target_lang_sense_clarification": "заниматься самообманом"
                            },
                            {
                                "target_lang_word": "тешить себя иллюзиями",
                                "target_lang_sense_clarification": "верить в желаемое, а не действительное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
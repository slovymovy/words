{
    "word_family": [
        "roundabout",
        "thereabout",
        "whereabout",
        "hereabout",
        "turnabout",
        "gadabout",
        "layabout",
        "runabout",
        "about-face"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "77888db8-f6f2-3f84-b05f-80dc5569310d",
                    "sense_definition": "On the subject of; concerning or regarding something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "topic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He talked a lot <w>about</w> his childhood.",
                            "target_lang_translations": {
                                "ru": "Он много говорил <w>о</w> своём детстве."
                            }
                        },
                        {
                            "text": "We must do something <w>about</w> this problem.",
                            "target_lang_translations": {
                                "ru": "Мы должны что-то предпринять <w>насчёт</w> этой проблемы."
                            }
                        },
                        {
                            "text": "I am very mad <w>about</w> all the pranks they played.",
                            "target_lang_translations": {
                                "ru": "Я очень зол <w>из-за</w> всех тех шалостей, что они устроили."
                            }
                        }
                    ],
                    "synonyms": [
                        "concerning",
                        "regarding",
                        "on",
                        "anent",
                        "re"
                    ],
                    "antonyms": [
                        "regardless of",
                        "irrespective of"
                    ],
                    "common_phrases": [
                        "talk about",
                        "think about",
                        "worry about",
                        "know about",
                        "mad about"
                    ],
                    "target_lang_definitions": {
                        "ru": "О предмете; касательно или относительно чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о",
                                "target_lang_sense_clarification": "предлог (с предложным падежом); наиболее нейтральный вариант"
                            },
                            {
                                "target_lang_word": "про",
                                "target_lang_sense_clarification": "предлог (с винительным падежом); разговорный вариант"
                            },
                            {
                                "target_lang_word": "насчёт",
                                "target_lang_sense_clarification": "предлог (с родительным падежом); в значении «относительно»"
                            },
                            {
                                "target_lang_word": "по поводу",
                                "target_lang_sense_clarification": "фраза; официальный или деловой стиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c431fdca-19ba-3270-bce8-055e6825f2de",
                    "sense_definition": "Surrounding or on all sides of something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_surrounding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The snake was coiled <w>about</w> his ankle.",
                            "target_lang_translations": {
                                "ru": "Змея обвилась <w>вокруг</w> его лодыжки."
                            }
                        },
                        {
                            "text": "She looked <w>about</w> her to see if anyone was watching.",
                            "target_lang_translations": {
                                "ru": "Она огляделась <w>вокруг</w>, чтобы проверить, не смотрит ли кто."
                            }
                        }
                    ],
                    "synonyms": [
                        "around",
                        "round"
                    ],
                    "common_phrases": [
                        "look about",
                        "gathered about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окружающий или находящийся со всех сторон чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вокруг",
                                "target_lang_sense_clarification": "предлог или наречие; указывает на нахождение со всех сторон"
                            },
                            {
                                "target_lang_word": "кругом",
                                "target_lang_sense_clarification": "наречие; охватывая всё пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33fa5e82-6fe4-33a8-8632-2e8c58f08578",
                    "sense_definition": "Intending to do something immediately; on the verge of (used with 'to').",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was <w>about</w> to leave when the door swung open.",
                            "target_lang_translations": {
                                "ru": "Я уже <w>собирался</w> уходить, когда дверь распахнулась."
                            }
                        },
                        {
                            "text": "The ceremony is <w>about</w> to begin.",
                            "target_lang_translations": {
                                "ru": "Церемония <w>вот-вот</w> начнётся."
                            }
                        }
                    ],
                    "synonyms": [
                        "going to",
                        "ready to"
                    ],
                    "common_phrases": [
                        "about to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеревающийся сделать что-то немедленно; на грани того, чтобы (используется с 'to')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "глагол; иметь намерение сделать что-то"
                            },
                            {
                                "target_lang_word": "вот-вот",
                                "target_lang_sense_clarification": "наречие; указывает на неминуемость действия"
                            },
                            {
                                "target_lang_word": "готов",
                                "target_lang_sense_clarification": "краткое прилагательное; состояние готовности к действию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ebdd2a9-6b57-3d8f-8905-3f040fb90c6e",
                    "sense_definition": "Here and there within a particular area; scattered or moving throughout.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location_dispersed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rubbish was strewn <w>about</w> the place.",
                            "target_lang_translations": {
                                "ru": "Мусор был разбросан <w>по</w> всему месту."
                            }
                        },
                        {
                            "text": "The children were running <w>about</w> the room.",
                            "target_lang_translations": {
                                "ru": "Дети бегали <w>по</w> комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "around",
                        "throughout",
                        "all over"
                    ],
                    "common_phrases": [
                        "strewn about",
                        "running about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здесь и там в пределах определенной области; разбросанный или движущийся повсюду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "предлог; указывает на движение или расположение в пределах места"
                            },
                            {
                                "target_lang_word": "повсюду",
                                "target_lang_sense_clarification": "наречие; везде, во всех местах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6396a213-5c92-3f09-8299-2093c4e2a0bf",
                    "sense_definition": "Located near or in the vicinity of a place.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot find my reading glasses, but they must be somewhere <w>about</w> the house.",
                            "target_lang_translations": {
                                "ru": "Я не могу найти очки для чтения, но они должны быть где-то <w>в</w> доме."
                            }
                        },
                        {
                            "text": "John is in the garden, probably somewhere <w>about</w> the woodshed.",
                            "target_lang_translations": {
                                "ru": "Джон в саду, наверное, где-то <w>у</w> сарая."
                            }
                        }
                    ],
                    "synonyms": [
                        "near",
                        "around",
                        "close to"
                    ],
                    "common_phrases": [
                        "somewhere about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный рядом или в окрестностях места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "где-то",
                                "target_lang_sense_clarification": "наречие; в неопределенном месте (часто с предлогом)"
                            },
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "предлог; возле, рядом"
                            },
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "предлог; в непосредственной близости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6be0699a-069b-30b0-bb57-1766e8bbb313",
                    "sense_definition": "Associated with a person or thing as a characteristic or quality.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "characteristic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was an air of confidence <w>about</w> the woman.",
                            "target_lang_translations": {
                                "ru": "<w>В</w> этой женщине чувствовалась уверенность."
                            }
                        },
                        {
                            "text": "He has his wits <w>about</w> him.",
                            "target_lang_translations": {
                                "ru": "Он не теряет головы (букв. разум <w>при</w> нём)."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wits about him",
                        "something about her"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с человеком или вещью как характеристика или качество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "предлог; указывает на наличие качества внутри человека/предмета"
                            },
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "предлог; принадлежность качества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5dd5d90-b69b-33cb-b034-9bafb3c260a8",
                    "sense_definition": "Carried on one's person.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I had no weapon <w>about</w> me but a stick.",
                            "target_lang_translations": {
                                "ru": "У меня не было <w>с</w> собой никакого оружия, кроме палки."
                            }
                        },
                        {
                            "text": "Do you have any money <w>about</w> you?",
                            "target_lang_translations": {
                                "ru": "У тебя есть <w>с</w> собой деньги?"
                            }
                        }
                    ],
                    "synonyms": [
                        "with"
                    ],
                    "common_phrases": [
                        "money about you"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющийся при себе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с собой",
                                "target_lang_sense_clarification": "фраза; наличие предмета у человека в данный момент"
                            },
                            {
                                "target_lang_word": "при себе",
                                "target_lang_sense_clarification": "фраза; официальный или уточняющий вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c380a12-0379-3a7f-b327-60c3b8296b19",
                    "sense_definition": "Engaged in or busy with a task.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They were just going <w>about</w> their business.",
                            "target_lang_translations": {
                                "ru": "Они просто <w>занимались</w> своими делами."
                            }
                        },
                        {
                            "text": "I really want to know what he is <w>about</w>.",
                            "target_lang_translations": {
                                "ru": "Я действительно хочу знать, чем он <w>занимается</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "engaged in",
                        "occupied with"
                    ],
                    "common_phrases": [
                        "about one's business"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занятый каким-либо делом или задачей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заниматься",
                                "target_lang_sense_clarification": "глагол; выполнять действие или работу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a24142bb-7e94-3a33-bfa2-d5ba9fd5fe36",
                    "sense_definition": "Approximately; roughly; nearly.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>about</w> as cold as it was last winter.",
                            "target_lang_translations": {
                                "ru": "Сейчас <w>примерно</w> так же холодно, как прошлой зимой."
                            }
                        },
                        {
                            "text": "He owes me <w>about</w> three hundred dollars.",
                            "target_lang_translations": {
                                "ru": "Он должен мне <w>около</w> трёхсот долларов."
                            }
                        },
                        {
                            "text": "I will see you <w>about</w> a year from now.",
                            "target_lang_translations": {
                                "ru": "Увидимся <w>примерно</w> через год."
                            }
                        }
                    ],
                    "synonyms": [
                        "approximately",
                        "roughly",
                        "around",
                        "almost",
                        "nearly"
                    ],
                    "common_phrases": [
                        "about right",
                        "about time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приблизительно; грубо; почти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "примерно",
                                "target_lang_sense_clarification": "наречие; ориентировочно"
                            },
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "предлог; используется с числами"
                            },
                            {
                                "target_lang_word": "почти",
                                "target_lang_sense_clarification": "наречие; близко к завершению или полному количеству"
                            },
                            {
                                "target_lang_word": "приблизительно",
                                "target_lang_sense_clarification": "наречие; не точно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e57a7280-42f1-310d-9a24-882b8fc1028e",
                    "sense_definition": "Moving from one place to another; in various directions.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was moving furniture <w>about</w> in the living room.",
                            "target_lang_translations": {
                                "ru": "Она передвигала мебель <w>по</w> гостиной."
                            }
                        },
                        {
                            "text": "He bustled <w>about</w> speaking to everyone at the party.",
                            "target_lang_translations": {
                                "ru": "Он суетился <w>вокруг</w>, разговаривая со всеми на вечеринке."
                            }
                        }
                    ],
                    "synonyms": [
                        "around",
                        "to and fro"
                    ],
                    "common_phrases": [
                        "walk about",
                        "move about",
                        "rush about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаясь с одного места на другое; в разных направлениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туда-сюда",
                                "target_lang_sense_clarification": "наречие; движение в разные стороны"
                            },
                            {
                                "target_lang_word": "взад-вперёд",
                                "target_lang_sense_clarification": "наречие; возвратно-поступательное движение"
                            },
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "предлог; движение в пределах пространства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "157928bf-0cff-3963-8687-1a0cb7b872a1",
                    "sense_definition": "In various places; here and there.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location_dispersed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bits of old machinery were lying <w>about</w>.",
                            "target_lang_translations": {
                                "ru": "Куски старого оборудования валялись <w>повсюду</w>."
                            }
                        },
                        {
                            "text": "Feathers were strewn <w>about</w> on the ground.",
                            "target_lang_translations": {
                                "ru": "Перья были разбросаны <w>по</w> земле."
                            }
                        }
                    ],
                    "synonyms": [
                        "around",
                        "here and there"
                    ],
                    "common_phrases": [
                        "lying about",
                        "scattered about"
                    ],
                    "target_lang_definitions": {
                        "ru": "В разных местах; здесь и там."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повсюду",
                                "target_lang_sense_clarification": "наречие; везде"
                            },
                            {
                                "target_lang_word": "разбросано",
                                "target_lang_sense_clarification": "наречие/причастие; хаотичное расположение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7426325a-b8c4-3f40-90a0-522d7cffddf7",
                    "sense_definition": "Used in phrasal verbs to indicate aimless, unproductive, or unstructured activity.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "activity_idle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stop messing <w>about</w> and get to work.",
                            "target_lang_translations": {
                                "ru": "Перестань валять <w>дурака</w> и принимайся за работу."
                            }
                        },
                        {
                            "text": "They spent the whole day loafing <w>about</w>.",
                            "target_lang_translations": {
                                "ru": "Они провели весь день, слоняясь <w>без дела</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "around"
                    ],
                    "common_phrases": [
                        "mess about",
                        "fool about",
                        "loaf about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется во фразовых глаголах для обозначения бесцельной, непродуктивной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "без дела",
                                "target_lang_sense_clarification": "фраза; праздно"
                            },
                            {
                                "target_lang_word": "дурака",
                                "target_lang_sense_clarification": "существительное (в идиомах типа «валять дурака»)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20b7743b-617e-3c91-b9ef-f3e82704110c",
                    "sense_definition": "In the opposite direction; to a reversed position.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "direction_reversal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He turned <w>about</w> and faced his niece.",
                            "target_lang_translations": {
                                "ru": "Он повернулся <w>кругом</w> и посмотрел на племянницу."
                            }
                        },
                        {
                            "text": "We went <w>about</w> and headed offshore.",
                            "target_lang_translations": {
                                "ru": "Мы развернулись <w>обратно</w> и направились от берега."
                            }
                        }
                    ],
                    "synonyms": [
                        "around",
                        "back"
                    ],
                    "common_phrases": [
                        "turn about",
                        "face about"
                    ],
                    "target_lang_definitions": {
                        "ru": "В противоположном направлении; в обратную сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кругом",
                                "target_lang_sense_clarification": "наречие; разворот на 180 градусов"
                            },
                            {
                                "target_lang_word": "назад",
                                "target_lang_sense_clarification": "наречие; в обратную сторону"
                            },
                            {
                                "target_lang_word": "обратно",
                                "target_lang_sense_clarification": "наречие; возвращение к исходной точке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a49fb765-8472-3851-b267-9b8010805b16",
                    "sense_definition": "Nearby; in the immediate area.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location_proximity",
                    "name_type": "no",
                    "examples": [
                        {

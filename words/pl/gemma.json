{
    "word_family": [
        "gemmologia",
        "gemmolog",
        "gemmologiczny"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a777c61b-7337-3add-b29c-f43cccae1685",
                    "sense_definition": "Kamień szlachetny lub półszlachetny, zazwyczaj o owalnym kształcie, ozdobiony rzeźbionym reliefem (wypukłym lub wklęsłym), używany jako element biżuterii.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum archeologicznym podziwialiśmy starożytną <w>gemmę</w> z wizerunkiem cesarza.",
                            "target_lang_translations": {
                                "ru": "В археологическом музее мы любовались древней <w>геммой</w> с изображением императора."
                            }
                        },
                        {
                            "text": "Ta misternie wykonana <w>gemma</w> została oprawiona w złoty pierścień.",
                            "target_lang_translations": {
                                "ru": "Эта искусно выполненная <w>гемма</w> была оправлена в золотое кольцо."
                            }
                        }
                    ],
                    "common_phrases": [
                        "antyczna gemma",
                        "kolekcja gemm",
                        "rzeźbiona gemma"
                    ],
                    "target_lang_definitions": {
                        "ru": "Драгоценный или полудрагоценный камень, обычно овальной формы, с вырезанным на нём рельефным изображением (выпуклым или углублённым), используемый как украшение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гемма",
                                "target_lang_sense_clarification": "резной камень"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "73b96507-a21c-3989-80df-022dbbe7b117",
                    "sense_definition": "Imię żeńskie pochodzenia łacińskiego, oznaczające pączek lub drogi kamień.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja przyjaciółka <w>Gemma</w> przyjechała do nas z Włoch na wakacje.",
                            "target_lang_translations": {
                                "ru": "Моя подруга <w>Гемма</w> приехала к нам из Италии на каникулы."
                            }
                        },
                        {
                            "text": "Po krótkim czasie <w>Gemma</w> została uzdrowiona.",
                            "target_lang_translations": {
                                "ru": "Спустя короткое время <w>Гемма</w> исцелилась."
                            }
                        }
                    ],
                    "common_phrases": [
                        "święta Gemma"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя латинского происхождения, означающее «почка» или «драгоценный камень»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гемма",
                                "target_lang_sense_clarification": "женское имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
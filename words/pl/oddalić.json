{
    "word_family": [
        "oddalać",
        "oddalenie",
        "daleki",
        "daleko",
        "dal"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f82c695d-a776-33ee-b25c-f904d579fac9",
                    "sense_definition": "Przemieścić się na większą odległość od kogoś lub czegoś; odejść (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodziej zdołał szybko <oddalić> się z miejsca wypadku.",
                            "target_lang_translations": {
                                "ru": "Вор сумел быстро <удалиться> с места происшествия."
                            }
                        },
                        {
                            "text": "Musimy <oddalić> się od ogniska, bo jest tu zbyt gorąco.",
                            "target_lang_translations": {
                                "ru": "Мы должны <отойти> от костра, потому что здесь слишком жарко."
                            }
                        }
                    ],
                    "synonyms": [
                        "odejść",
                        "odsunąć się",
                        "wycofać się"
                    ],
                    "antonyms": [
                        "przybliżyć się",
                        "podejść",
                        "zbliżyć się"
                    ],
                    "common_phrases": [
                        "oddalić się od brzegu",
                        "oddalić się z miejsca",
                        "znacznie się oddalić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместиться на большее расстояние от кого-либо или чего-либо; уйти (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удалиться",
                                "target_lang_sense_clarification": "переместиться вдаль, уйти"
                            },
                            {
                                "target_lang_word": "отойти",
                                "target_lang_sense_clarification": "сделать несколько шагов назад или в сторону"
                            },
                            {
                                "target_lang_word": "отдалиться",
                                "target_lang_sense_clarification": "стать более далеким (физически или переносно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8123e32e-1940-3f2f-a927-79888352b754",
                    "sense_definition": "Sprawić, że ktoś lub coś znajdzie się dalej od jakiegoś punktu; fizycznie coś odsunąć.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <oddalić> mikrofon od głośnika, żeby uniknąć sprzężenia.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <отодвиньте> микрофон от динамика, чтобы избежать обратной связи."
                            }
                        },
                        {
                            "text": "Musisz <oddalić> telefon od twarzy, aby zrobić wyraźne zdjęcie.",
                            "target_lang_translations": {
                                "ru": "Ты должен <отодвинуть> телефон от лица, чтобы сделать четкое фото."
                            }
                        }
                    ],
                    "synonyms": [
                        "odsunąć",
                        "przemieścić"
                    ],
                    "antonyms": [
                        "przybliżyć",
                        "przysunąć"
                    ],
                    "common_phrases": [
                        "oddalić rękę",
                        "oddalić przedmiot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместить кого-то или что-то дальше от какой-либо точки; физически отодвинуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отодвинуть",
                                "target_lang_sense_clarification": "переместить предмет в сторону или назад"
                            },
                            {
                                "target_lang_word": "отдалить",
                                "target_lang_sense_clarification": "увеличить расстояние между объектами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3f39de1-f0ff-30d7-8d99-1c71b76aeb7d",
                    "sense_definition": "Przestać o czymś myśleć, pozbyć się jakiegoś uczucia lub sprawić, że zagrożenie minie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Próbowała <oddalić> od siebie smutne wspomnienia z przeszłości.",
                            "target_lang_translations": {
                                "ru": "Она пыталась <отогнать> от себя грустные воспоминания из прошлого."
                            }
                        },
                        {
                            "text": "Udało mu się skutecznie <oddalić> wszelkie podejrzenia policji.",
                            "target_lang_translations": {
                                "ru": "Ему удалось успешно <отвести> от себя все подозрения полиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "odgonić",
                        "odrzucić"
                    ],
                    "antonyms": [
                        "przywołać"
                    ],
                    "common_phrases": [
                        "oddalić od siebie podejrzenia",
                        "oddalić czarne myśli",
                        "oddalić widmo bankructwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать думать о чем-то, избавиться от чувства или устранить угрозу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отогнать",
                                "target_lang_sense_clarification": "мысли, воспоминания, страх"
                            },
                            {
                                "target_lang_word": "отвести",
                                "target_lang_sense_clarification": "подозрения, угрозу, беду"
                            },
                            {
                                "target_lang_word": "отбросить",
                                "target_lang_sense_clarification": "сомнения, мысли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70fdc466-8899-3c26-aeb5-d347fd1830ad",
                    "sense_definition": "Uznać coś za bezzasadne i odrzucić w sposób oficjalny (często w kontekście sądowym).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_dismissal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd postanowił <oddalić> wniosek obrońcy o zwolnienie z aresztu.",
                            "target_lang_translations": {
                                "ru": "Суд постановил <отклонить> ходатайство защитника об освобождении из-под стражи."
                            }
                        },
                        {
                            "text": "Sędzia musiał <oddalić> pozew z powodu braku wystarczających dowodów.",
                            "target_lang_translations": {
                                "ru": "Судья был вынужден <отклонить> иск из-за недостатка доказательств."
                            }
                        }
                    ],
                    "synonyms": [
                        "odrzucić"
                    ],
                    "antonyms": [
                        "uwzględnić",
                        "przyjąć"
                    ],
                    "common_phrases": [
                        "oddalić wniosek",
                        "oddalić powództwo",
                        "oddalić apelację"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Признать необоснованным и официально отвергнуть (в юридическом контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отклонить",
                                "target_lang_sense_clarification": "иск, ходатайство, апелляцию"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
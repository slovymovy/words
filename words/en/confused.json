                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбивчивый",
                                "target_lang_sense_clarification": "непоследовательный, непонятный (о речи или объяснении)"
                            },
                            {
                                "target_lang_word": "путанный",
                                "target_lang_sense_clarification": "лишенный ясности и логики"
                            },
                            {

                {
                    "sense_id": "b8ace56e-fb5d-333a-bf76-ba97abfe9c14",
                    "sense_definition": "Een mechanisme waarmee een voertuig bestuurd wordt, zoals een stuurwiel in een auto of een fietsstuur.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "steering_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Houd je beide handen goed aan het <w>stuur</w>.",
                            "target_lang_translations": {
                                "en": "Keep both hands firmly on the <w>steering wheel</w>.",
                                "ru": "Держи обе руки крепко на <w>руле</w>."
                            }
                        },
                        {
                            "text": "Hij zit al de hele dag achter het <w>stuur</w> van de vrachtwagen.",
                            "target_lang_translations": {
                                "en": "He has been behind the <w>wheel</w> of the truck all day.",
                                "ru": "Он весь день сидит за <w>рулём</w> грузовика."
                            }
                        }
                    ],
                    "synonyms": [
                        "stuurwiel"
                    ],
                    "common_phrases": [
                        "achter het stuur",
                        "aan het stuur"
                    ],
                    "target_lang_definitions": {
                        "en": "A mechanism used to steer a vehicle, such as a steering wheel in a car or handlebars on a bicycle.",
                        "ru": "Механизм для управления транспортным средством, например, руль в автомобиле или на велосипеде."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steering wheel",
                                "target_lang_sense_clarification": "for cars and trucks"
                            },
                            {
                                "target_lang_word": "handlebars",
                                "target_lang_sense_clarification": "for bicycles and motorcycles"
                            },
                            {
                                "target_lang_word": "wheel",
                                "target_lang_sense_clarification": "informal/short for steering wheel"
                            }
                        ],
                        "ru": [

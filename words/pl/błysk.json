                },
                {
                    "sense_id": "c2269e78-7229-31f1-ac28-262338d05dc1",
                    "sense_definition": "Nagły, krótki przejaw jakiejś cechy, uczucia, myśli lub talentu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_manifestation",
                    "name_type": "no",
                    "examples": [

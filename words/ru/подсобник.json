            "senses": [
                {
                    "sense_id": "82afae55-c048-342a-9e24-83988d1107b3",
                    "sense_definition": "Разговорное название подсобного рабочего, выполняющего неквалифицированную, вспомогательную работу (обычно на стройке или производстве).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "worker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На стройку срочно требуется <w>подсобник</w>.",
                            "target_lang_translations": {
                                "en": "A <w>laborer</w> is urgently needed at the construction site."
                            }
                        },
                        {
                            "text": "Мой сосед устроился <w>подсобником</w> на склад, чтобы заработать денег.",
                            "target_lang_translations": {
                                "en": "My neighbor got a job as a <w>general worker</w> at a warehouse to earn some money."
                            }
                        }
                    ],
                    "synonyms": [
                        "разнорабочий",
                        "чернорабочий"
                    ],
                    "common_phrases": [
                        "работать подсобником",
                        "нанять подсобника",
                        "бригада подсобников"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial term for an auxiliary worker performing unskilled, supporting work (usually on a construction site or in production)."
                    },
                    "translations": {
                        "en": [
                            {

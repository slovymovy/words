                    "sense_id": "b6658397-f1f7-368b-84a3-5699dfbd405c",
                    "sense_definition": "Van haar; geeft aan dat iets toebehoort aan een vrouwelijke persoon.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possessive_fem",
                    "name_type": "no",

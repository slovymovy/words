    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "db2e8002-4faa-34a5-b408-4136dd3779f5",
                    "sense_definition": "Именовать, называть кого-либо по имени или давать кому-либо имя.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "naming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Скажите, пожалуйста, как вас <зовут>?",
                            "target_lang_translations": {
                                "en": "Could you please tell me what you are <called>?"
                            }
                        },
                        {
                            "text": "Моего лучшего друга <зовут> Александр, но мы зовём его Саша.",
                            "target_lang_translations": {
                                "en": "My best friend is <called> Alexander, but we call him Sasha."
                            }
                        }
                    ],
                    "synonyms": [
                        "называть",
                        "именовать"
                    ],
                    "common_phrases": [
                        "меня зовут",
                        "как тебя зовут"
                    ],
                    "target_lang_definitions": {
                        "en": "To give a name to someone or to address someone by a specific name."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "to name or address"
                            },
                            {
                                "target_lang_word": "name",
                                "target_lang_sense_clarification": "to give a name"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a396e6b0-05e0-3cbc-a094-3391847d88c2",
                    "sense_definition": "Громко произносить имя или издавать звуки, чтобы попросить кого-либо подойти или откликнуться.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "summoning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама громко <звала> детей домой, потому что начался дождь.",
                            "target_lang_translations": {
                                "en": "Mom was loudly <calling> the children home because it started to rain."
                            }
                        },
                        {
                            "text": "Если тебе станет страшно, просто <зови> на помощь.",
                            "target_lang_translations": {
                                "en": "If you get scared, just <call> for help."
                            }
                        }
                    ],
                    "synonyms": [
                        "окликать",
                        "подзывать"
                    ],
                    "common_phrases": [
                        "звать на помощь",
                        "звать врача"
                    ],
                    "target_lang_definitions": {
                        "en": "To speak loudly to get someone's attention or ask them to come."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "to shout or summon"
                            },
                            {
                                "target_lang_word": "call out",
                                "target_lang_sense_clarification": "to shout"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d817354e-9e87-366e-a69e-9c10bb3fa064",
                    "sense_definition": "Предлагать кому-либо прийти куда-то, посетить какое-либо место или событие.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "inviting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы часто <зовём> гостей на ужин по выходным.",
                            "target_lang_translations": {
                                "en": "We often <invite> guests for dinner on weekends."
                            }
                        },
                        {
                            "text": "Он <звал> её в кино уже три раза, но она отказывалась.",
                            "target_lang_translations": {
                                "en": "He <invited> her to the movies three times already, but she refused."
                            }
                        }
                    ],
                    "synonyms": [
                        "приглашать"
                    ],
                    "common_phrases": [
                        "звать в гости",
                        "звать замуж"
                    ],
                    "target_lang_definitions": {
                        "en": "To ask someone to go somewhere or do something with you."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "invite",
                                "target_lang_sense_clarification": "to ask to come"
                            },
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to request presence"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "garstka",
        "garściowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9e3ab76f-b3f9-3c78-8eca-953b40ae3934",
                    "sense_definition": "Dłoń z palcami zgiętymi do środka, ułożona tak, aby móc coś chwycić, trzymać lub zaczerpnąć.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "hand_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna zacisnął <garść> na klamce i szarpnął drzwi.",
                            "target_lang_translations": {
                                "ru": "Мужчина сжал <руку> на дверной ручке и дернул дверь."
                            }
                        },
                        {
                            "text": "Musisz wreszcie wziąć się w <garść> i zacząć działać.",
                            "target_lang_translations": {
                                "ru": "Ты должен наконец взять себя в <руки> и начать действовать."
                            }
                        }
                    ],
                    "synonyms": [
                        "furman"
                    ],
                    "common_phrases": [
                        "wziąć się w garść",
                        "trzymać w garści",
                        "mieć w garści"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ладонь с пальцами, согнутыми внутрь, сложенная так, чтобы можно было что-то схватить, удержать или зачерпнуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горсть",
                                "target_lang_sense_clarification": "сложенная ладонь"
                            },
                            {
                                "target_lang_word": "рука",
                                "target_lang_sense_clarification": "кисть руки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4fb729b-b0d1-35cb-97ab-b82109f363ff",
                    "sense_definition": "Ilość substancji lub przedmiotów, która mieści się w dłoni; często używane w odniesieniu do małej liczby czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do zupy wrzuciła <garść> posiekanej natki pietruszki.",
                            "target_lang_translations": {
                                "ru": "В суп она бросила <горсть> нарезанной петрушки."
                            }
                        },
                        {
                            "text": "Pożyczyłem od sąsiada <garść> soli, bo mi zabrakło.",
                            "target_lang_translations": {
                                "ru": "Я одолжил у соседа <горсть> соли, так как у меня закончилась."
                            }
                        }
                    ],
                    "synonyms": [
                        "furman"
                    ],
                    "common_phrases": [
                        "czerpać garściami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество вещества или предметов, которое помещается в ладони."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горсть",
                                "target_lang_sense_clarification": "мера объема"
                            },
                            {
                                "target_lang_word": "пригоршня",
                                "target_lang_sense_clarification": "полная ладонь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e69376fe-e632-37e0-a641-39a97c004e3d",
                    "sense_definition": "Pęk skoszonego zboża ułożony na polu, gotowy do związania w snop.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnicy układali <garście> żyta w równe rzędy na ściernisku.",
                            "target_lang_translations": {
                                "ru": "Фермеры укладывали <пучки> ржи в ровные ряды на стерне."
                            }
                        },
                        {
                            "text": "Każda <garść> zboża była cenna w czasach nieurodzaju.",
                            "target_lang_translations": {
                                "ru": "Каждая <горсть> зерна была ценна во времена неурожая."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "dotyczy tradycyjnego rolnictwa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пучок скошенного зерна, уложенный на поле и готовый к вязке в сноп."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пучок",
                                "target_lang_sense_clarification": "связка"
                            },
                            {
                                "target_lang_word": "горсть",
                                "target_lang_sense_clarification": "часть снопа"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e5dfe19d-383e-321f-b6ed-b0f532bc143c",
                    "sense_definition": "Стать сухим, полностью избавиться от влаги.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После сильного ливня нам нужно было срочно <w>просушиться</w>.",
                            "target_lang_translations": {
                                "en": "After the heavy downpour, we urgently needed to <w>get dry</w>."
                            }
                        },
                        {
                            "text": "Палатка должна хорошо <w>просушиться</w> на солнце перед тем, как мы её сложим.",
                            "target_lang_translations": {
                                "en": "The tent needs to <w>dry out</w> well in the sun before we pack it up."
                            }
                        }
                    ],
                    "synonyms": [
                        "просохнуть"
                    ],
                    "antonyms": [
                        "намокнуть",
                        "промокнуть"
                    ],
                    "common_phrases": [
                        "просушиться на солнце",
                        "просушиться на ветру",
                        "дать просушиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To become dry, to completely get rid of moisture."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dry out",
                                "target_lang_sense_clarification": "to become completely dry"
                            },
                            {
                                "target_lang_word": "get dry",
                                "target_lang_sense_clarification": "general state of becoming dry"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38256137-582e-3955-96cc-a8c80c49ee2c",
                    "sense_definition": "Высушить на себе намокшую одежду или обувь, обычно находясь рядом с источником тепла.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рыбаки развели костёр, чтобы согреться и <w>просушиться</w>.",
                            "target_lang_translations": {
                                "en": "The fishermen built a fire to warm up and <w>dry off</w>."
                            }
                        },
                        {
                            "text": "Мы зашли в ближайшее кафе, чтобы немного <w>просушиться</w> и выпить горячего чая.",
                            "target_lang_translations": {
                                "en": "We went into the nearest cafe to <w>dry off</w> a bit and drink some hot tea."
                            }
                        }
                    ],
                    "common_phrases": [
                        "просушиться у костра",
                        "зайти просушиться",
                        "просушиться у печки"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To dry one's wet clothes or shoes while wearing them, usually near a heat source."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dry off",
                                "target_lang_sense_clarification": "to remove moisture from oneself or one's clothes"
                            },
                            {
                                "target_lang_word": "dry oneself",
                                "target_lang_sense_clarification": "reflexive action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7de7fa91-b6cf-37d4-a272-6c609d73691b",
                    "sense_definition": "Быть высушенным (страдательный залог к глаголу <w>просушить</w>).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зерно недостаточно <w>просушилось</w> в элеваторе из-за сбоя оборудования.",
                            "target_lang_translations": {
                                "en": "The grain was not sufficiently <w>dried</w> in the elevator due to an equipment failure."
                            }
                        },
                        {
                            "text": "Бельё быстро <w>просушилось</w> благодаря тёплому ветру.",
                            "target_lang_translations": {
                                "en": "The laundry <w>dried</w> quickly thanks to the warm wind."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be dried (passive voice or reflexive usage indicating an object undergoing drying)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be dried",
                                "target_lang_sense_clarification": "passive state"
                            },
                            {
                                "target_lang_word": "dry",
                                "target_lang_sense_clarification": "intransitive verb for objects"
                            }
                        ]
                    }
                }
            ]
        }
    ]

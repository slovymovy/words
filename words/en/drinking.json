                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для питья",
                                "target_lang_sense_clarification": "предназначение предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "314d659b-3534-36ef-8e30-a10ac0f7e7e1",

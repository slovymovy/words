{
    "word_family": [
        "blower",
        "blowout",
        "blowup",
        "flyblown"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4bbab089-a821-3c7b-9dfb-c96427ad4126",
                    "sense_definition": "To produce a current of air, or to be moved by a current of air.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "air_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A cold wind began to <w>blow</w> from the north.",
                            "target_lang_translations": {
                                "ru": "С севера начал <w>дуть</w> холодный ветер."
                            }
                        },
                        {
                            "text": "The leaves <w>blew</w> across the empty street.",
                            "target_lang_translations": {
                                "ru": "Листья <w>несло</w> ветром по пустой улице."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wind blows",
                        "blow hard",
                        "blow away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать поток воздуха или перемещаться под воздействием потока воздуха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуть",
                                "target_lang_sense_clarification": "о ветре"
                            },
                            {
                                "target_lang_word": "веять",
                                "target_lang_sense_clarification": "слабо дуть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5507d5d-b543-365c-b705-c4f6d98ed3bc",
                    "sense_definition": "To send out a strong current of air from the mouth.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "air_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make a wish and <w>blow</w> out the candles on your cake.",
                            "target_lang_translations": {
                                "ru": "Загадай желание и <w>задуй</w> свечи на торте."
                            }
                        },
                        {
                            "text": "He <w>blew</w> on his hands to keep them warm.",
                            "target_lang_translations": {
                                "ru": "Он <w>подул</w> на руки, чтобы согреть их."
                            }
                        }
                    ],
                    "synonyms": [
                        "puff",
                        "exhale"
                    ],
                    "common_phrases": [
                        "blow out",
                        "blow bubbles",
                        "blow a kiss"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпускать сильную струю воздуха изо рта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуть",
                                "target_lang_sense_clarification": "ртом"
                            },
                            {
                                "target_lang_word": "подуть",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e8cb3aa-99d9-3e53-b9b5-4e1d91fc3935",
                    "sense_definition": "To clear the nose of mucus by forcing air through it into a tissue or handkerchief.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "air_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Excuse me, I need to <w>blow</w> my nose.",
                            "target_lang_translations": {
                                "ru": "Прошу прощения, мне нужно <w>высморкаться</w>."
                            }
                        },
                        {
                            "text": "She used a tissue to <w>blow</w> her nose loudly.",
                            "target_lang_translations": {
                                "ru": "Она использовала салфетку, чтобы громко <w>высморкаться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "clear"
                    ],
                    "common_phrases": [
                        "blow one's nose"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очищать нос от слизи, с силой выдыхая воздух через него в салфетку или платок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сморкаться",
                                "target_lang_sense_clarification": "очищать нос"
                            },
                            {
                                "target_lang_word": "высморкаться",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5b24745-54f1-3cf8-b0c1-92b81231450f",
                    "sense_definition": "To destroy something by an explosion, or to explode.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers threatened to <w>blow</w> up the bridge.",
                            "target_lang_translations": {
                                "ru": "Солдаты угрожали <w>взорвать</w> мост."
                            }
                        },
                        {
                            "text": "The gas tank might <w>blow</w> if the fire reaches it.",
                            "target_lang_translations": {
                                "ru": "Бензобак может <w>взорваться</w>, если огонь доберется до него."
                            }
                        }
                    ],
                    "synonyms": [
                        "explode",
                        "detonate",
                        "burst"
                    ],
                    "common_phrases": [
                        "blow up",
                        "blow apart",
                        "blow to pieces"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушать что-либо взрывом или взрываться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрывать",
                                "target_lang_sense_clarification": "разрушать"
                            },
                            {
                                "target_lang_word": "взрываться",
                                "target_lang_sense_clarification": "детонировать"
                            },
                            {
                                "target_lang_word": "взлететь на воздух",
                                "target_lang_sense_clarification": "идиома"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c5bec1b-01f4-3e2e-817b-eafae3425fb5",
                    "sense_definition": "To produce sound by forcing air into a musical instrument or horn.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The referee <w>blew</w> his whistle to stop the game.",
                            "target_lang_translations": {
                                "ru": "Судья <w>свистнул</w> в свисток, чтобы остановить игру."
                            }
                        },
                        {
                            "text": "He learned to <w>blow</w> the trumpet in the school band.",
                            "target_lang_translations": {
                                "ru": "Он учился <w>играть</w> на трубе в школьном оркестре."
                            }
                        }
                    ],
                    "synonyms": [
                        "sound",
                        "play"
                    ],
                    "common_phrases": [
                        "blow the whistle",
                        "blow a horn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Извлекать звук, вдувая воздух в музыкальный инструмент или рожок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуть",
                                "target_lang_sense_clarification": "в инструмент"
                            },
                            {
                                "target_lang_word": "играть",
                                "target_lang_sense_clarification": "на духовом инструменте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "129e851b-505c-3804-88b6-8a71ce54e91a",
                    "sense_definition": "To fail at something or miss a good opportunity by making a mistake.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I really want this job, so I hope I don't <w>blow</w> the interview.",
                            "target_lang_translations": {
                                "ru": "Я очень хочу эту работу, поэтому надеюсь, что не <w>завалю</w> собеседование."
                            }
                        },
                        {
                            "text": "You had a chance to win, but you <w>blew</w> it.",
                            "target_lang_translations": {
                                "ru": "У тебя был шанс выиграть, но ты его <w>упустил</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruin",
                        "spoil",
                        "mess up"
                    ],
                    "common_phrases": [
                        "blow it",
                        "blow a chance",
                        "blow an opportunity"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерпеть неудачу или упустить хорошую возможность из-за ошибки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продуть",
                                "target_lang_sense_clarification": "разг. проиграть"
                            },
                            {
                                "target_lang_word": "упустить",
                                "target_lang_sense_clarification": "шанс"
                            },
                            {
                                "target_lang_word": "завалить",
                                "target_lang_sense_clarification": "разг. провалить дело"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a92e283-a4f1-38a5-aca2-eb8d999ff2b3",
                    "sense_definition": "To stop working because of an electrical fault (specifically of a fuse or circuit).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old toaster caused the fuse to <w>blow</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за старого тостера <w>перегорел</w> предохранитель."
                            }
                        },
                        {
                            "text": "We sat in the dark after the main circuit <w>blew</w>.",
                            "target_lang_translations": {
                                "ru": "Мы сидели в темноте после того, как <w>выбило</w> пробки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "blow a fuse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать работать из-за электрической неисправности (обычно о предохранителе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перегореть",
                                "target_lang_sense_clarification": "о предохранителе"
                            },
                            {
                                "target_lang_word": "выбить",
                                "target_lang_sense_clarification": "о пробках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8ec07a9-c6e8-392e-992c-5f2134c36913",
                    "sense_definition": "To spend a large amount of money in a careless or wasteful way.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>blew</w> all his savings on a luxury vacation.",
                            "target_lang_translations": {
                                "ru": "Он <w>спустил</w> все свои сбережения на роскошный отпуск."
                            }
                        },
                        {
                            "text": "They <w>blew</w> a thousand dollars at the casino in one night.",
                            "target_lang_translations": {
                                "ru": "Они <w>продули</w> тысячу долларов в казино за одну ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "squander",
                        "waste"
                    ],
                    "antonyms": [
                        "save"
                    ],
                    "common_phrases": [
                        "blow money",
                        "blow savings"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тратить большую сумму денег беспечно или расточительно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спустить",
                                "target_lang_sense_clarification": "разг. потратить"
                            },
                            {
                                "target_lang_word": "продуть",
                                "target_lang_sense_clarification": "разг. проиграть деньги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9d57d5b-52ea-3c54-ac92-1b8b3dd4c838",
                    "sense_definition": "To be very disappointing, unpleasant, or of poor quality.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "slang_bad",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This movie really <w>blows</w>; let's leave.",
                            "target_lang_translations": {
                                "ru": "Этот фильм — полный <w>отстой</w>; давай уйдем."
                            }
                        },
                        {
                            "text": "It <w>blows</w> that we have to work on the weekend.",
                            "target_lang_translations": {
                                "ru": "<w>Отстойно</w>, что нам приходится работать в выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "suck"
                    ],
                    "common_phrases": [
                        "this blows"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть очень разочаровывающим, неприятным или низкого качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть отстоем",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "сосать",
                                "target_lang_sense_clarification": "груб. сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b18f5087-2606-3860-a8d7-8e761a039b36",
                    "sense_definition": "A hard hit with a hand, weapon, or object.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He received a painful <w>blow</w> to the head.",
                            "target_lang_translations": {
                                "ru": "Он получил болезненный <w>удар</w> по голове."
                            }
                        },
                        {
                            "text": "The boxers exchanged heavy <w>blows</w> in the final round.",
                            "target_lang_translations": {
                                "ru": "Боксеры обменялись тяжелыми <w>ударами</w> в финальном раунде."
                            }
                        }
                    ],
                    "synonyms": [
                        "hit",
                        "strike",
                        "punch",
                        "smack"
                    ],
                    "common_phrases": [
                        "deal a blow",
                        "strike a blow",
                        "heavy blow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный удар рукой, оружием или предметом."

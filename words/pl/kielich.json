                },
                {
                    "sense_id": "944ac9c6-f9c8-32ad-af5c-333226021a28",
                    "sense_definition": "Zewnętrzna część kwiatu, składająca się z listków (działek), która osłania pąk przed rozwinięciem, a później wspiera koronę kwiatu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kolorowy motyl usiadł delikatnie na brzegu <w>kielicha</w> róży."
                        },
                        {
                            "text": "Wieczorem <w>kielich</w> tego kwiatu zamyka się, chroniąc wnętrze przed chłodem."
                        }
                    ],
                    "common_phrases": [
                        "kielich kwiatu"
                    ]
                },
                {
                    "sense_id": "3d134933-a6f4-3726-8854-8c47efbc5ee3",
                    "sense_definition": "Element budowy nerki w kształcie lejka, który zbiera mocz i odprowadza go do miedniczki nerkowej.",

            "senses": [
                {
                    "sense_id": "36b202a8-9d9a-3f4b-92d8-2f3ccb3fee61",
                    "sense_definition": "Used to express anger, irritation, contempt, or disappointment.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I forgot to bring my wallet, <w>dammit</w>!",
                            "target_lang_translations": {
                                "ru": "Я забыл взять кошелёк, <w>чёрт побери</w>!"
                            }
                        },
                        {
                            "text": "God <w>dammit</w>, I cannot believe we missed the bus again.",
                            "target_lang_translations": {
                                "ru": "<w>Чёрт возьми</w>, не могу поверить, что мы снова опоздали на автобус."
                            }
                        }
                    ],
                    "synonyms": [
                        "damn",
                        "darn",
                        "blast",
                        "hell",
                        "shoot"
                    ],
                    "common_phrases": [
                        "god dammit",
                        "dammit all"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "mild profanity"
                        }
                    ],
                    "target_lang_definitions": {

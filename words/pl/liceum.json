    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "82bf56e8-5149-361b-a197-c0f3c08a23ac",
                    "sense_definition": "Szkoła średnia kończąca się egzaminem maturalnym, przygotowująca uczniów do podjęcia studiów wyższych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "school_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja starsza siostra chodzi do <w>liceum</w> w Warszawie.",
                            "target_lang_translations": {
                                "ru": "Моя старшая сестра ходит в <w>лицей</w> в Варшаве."
                            }
                        },
                        {
                            "text": "Po ukończeniu <w>liceum</w> zamierzam studiować informatykę na uniwersytecie.",
                            "target_lang_translations": {
                                "ru": "После окончания <w>лицея</w> я собираюсь изучать информатику в университете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "liceum ogólnokształcące",
                        "skończyć liceum",
                        "chodzić do liceum",
                        "uczeń liceum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Средняя школа, заканчивающаяся экзаменом на аттестат зрелости (матурой) и готовящая учеников к поступлению в вуз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лицей",
                                "target_lang_sense_clarification": "среднее учебное заведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "180f7a78-00c0-399c-b6d6-ee026f9eb02b",
                    "sense_definition": "Starożytna szkoła filozoficzna założona przez Arystotelesa w Atenach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ancient_schools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Arystoteles nauczał swoich uczniów w ateńskim <w>liceum</w>.",
                            "target_lang_translations": {
                                "ru": "Аристотель обучал своих учеников в афинском <w>Ликее</w>."
                            }
                        },
                        {
                            "text": "Starożytne <w>liceum</w> było ważnym ośrodkiem myśli filozoficznej i naukowej.",
                            "target_lang_translations": {
                                "ru": "Древний <w>Ликей</w> был важным центром философской и научной мысли."
                            }
                        }
                    ],
                    "synonyms": [
                        "lykeion",
                        "lykejon",
                        "likejon"
                    ],
                    "common_phrases": [
                        "Liceum Arystotelesa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древняя философская школа, основанная Аристотелем в Афинах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ликей",
                                "target_lang_sense_clarification": "школа Аристотеля"
                            },
                            {
                                "target_lang_word": "лицей",
                                "target_lang_sense_clarification": "в контексте античности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c99a4b7-8fce-3492-bb48-757abaa4856c",
                    "sense_definition": "Dwie ostatnie klasy szkoły średniej w systemie edukacji w Polsce w latach 1932–1948.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "school_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W systemie jędrzejewiczowskim po gimnazjum następowało dwuletnie <w>liceum</w>.",
                            "target_lang_translations": {
                                "ru": "В системе Енджеевича после гимназии следовал двухлетний <w>лицей</w>."
                            }
                        },
                        {
                            "text": "Mój dziadek zdawał maturę w przedwojennym <w>liceum</w>.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка сдавал экзамен на аттестат зрелости в довоенном <w>лицее</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical system 1932–1948"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Два последних класса средней школы в системе образования Польши в 1932–1948 годах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лицей",
                                "target_lang_sense_clarification": "историческая ступень образования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4fd2024-f126-3eaa-92bc-26bc81205e14",
                    "sense_definition": "Dawna szkoła średnia przygotowująca do studiów uniwersyteckich, funkcjonująca od renesansu do XIX wieku.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "school_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W XIX wieku <w>liceum</w> było szkołą elitarną dla młodzieży męskiej.",
                            "target_lang_translations": {
                                "ru": "В XIX веке <w>лицей</w> был элитной школой для юношей."
                            }
                        },
                        {
                            "text": "Wiele historycznych postaci uczęszczało do tego renomowanego <w>liceum</w>.",
                            "target_lang_translations": {
                                "ru": "Многие исторические личности посещали этот прославленный <w>лицей</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная средняя школа, готовившая к поступлению в университет (с эпохи Возрождения до XIX века)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лицей",
                                "target_lang_sense_clarification": "старинное учебное заведение"

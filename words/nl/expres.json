            "senses": [
                {
                    "sense_id": "945cd93e-c7ff-38cb-aa47-2df9629469fc",
                    "sense_definition": "met de bedoeling dat het gebeurt; niet per ongeluk",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "volition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft het raam <w>expres</w> gebroken omdat hij boos was.",
                            "target_lang_translations": {
                                "en": "He broke the window <w>on purpose</w> because he was angry.",
                                "ru": "Он <w>специально</w> разбил окно, потому что был зол."
                            }
                        },
                        {
                            "text": "Ik deed het niet <w>expres</w>, het spijt me echt.",
                            "target_lang_translations": {
                                "en": "I didn't do it <w>on purpose</w>, I'm really sorry.",
                                "ru": "Я сделал это не <w>специально</w>, мне правда жаль."
                            }
                        },
                        {
                            "text": "Hij deed <w>expres</w> niet zijn best tijdens de wedstrijd.",
                            "target_lang_translations": {
                                "en": "He <w>deliberately</w> didn't do his best during the match.",
                                "ru": "Он <w>нарочно</w> не старался во время матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "met opzet",
                        "opzettelijk"
                    ],
                    "antonyms": [
                        "per ongeluk",
                        "per abuis",
                        "abusievelijk"
                    ],
                    "common_phrases": [
                        "iets expres doen",
                        "niet expres"
                    ],
                    "target_lang_definitions": {
                        "en": "intentionally; not by accident",
                        "ru": "с намерением, чтобы это произошло; не случайно"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on purpose",
                                "target_lang_sense_clarification": "intentionally"
                            },
                            {
                                "target_lang_word": "deliberately",
                                "target_lang_sense_clarification": "consciously and intentionally"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "специально",
                                "target_lang_sense_clarification": "с целью, намеренно"
                            },
                            {
                                "target_lang_word": "нарочно",
                                "target_lang_sense_clarification": "с умыслом, часто назло"
                            },
                            {
                                "target_lang_word": "намеренно",
                                "target_lang_sense_clarification": "сознательно, официально"
                            }
                        ]
                    }

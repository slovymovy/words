    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5ae74122-6934-3eb5-a74f-02fe784c8a7b",
                    "sense_definition": "Вести устную беседу, обмениваться словами и мыслями с кем-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы любим <разговаривать> о музыке и кино.",
                            "target_lang_translations": {
                                "en": "We love to <talk> about music and movies."
                            }
                        },
                        {
                            "text": "Учитель попросил учеников не <разговаривать> во время урока.",
                            "target_lang_translations": {
                                "en": "The teacher asked the students not to <talk> during the lesson."
                            }
                        }
                    ],
                    "synonyms": [
                        "говорить",
                        "беседовать",
                        "общаться"
                    ],
                    "antonyms": [
                        "молчать"
                    ],
                    "common_phrases": [
                        "разговаривать по телефону",
                        "разговаривать с друзьями",
                        "громко разговаривать",
                        "разговаривать шёпотом"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a spoken conversation; to exchange words and thoughts with someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "talk",
                                "target_lang_sense_clarification": "communicate by speech"
                            },
                            {
                                "target_lang_word": "speak",
                                "target_lang_sense_clarification": "communicate orally"
                            },
                            {
                                "target_lang_word": "converse",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21bd372d-54cb-3920-909c-db1f18131b42",
                    "sense_definition": "Поддерживать общение, быть в дружеских отношениях (часто используется в отрицании при ссоре).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "communication_relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После той ссоры они не <разговаривают> уже целый месяц.",
                            "target_lang_translations": {
                                "en": "After that quarrel, they haven't been <talking> for a whole month."
                            }
                        },
                        {
                            "text": "Я обиделся и больше не буду с тобой <разговаривать>.",
                            "target_lang_translations": {
                                "en": "I'm offended and won't <talk> to you anymore."
                            }
                        }
                    ],
                    "synonyms": [
                        "общаться"
                    ],
                    "common_phrases": [
                        "не разговаривать друг с другом",
                        "перестать разговаривать"
                    ],
                    "target_lang_definitions": {
                        "en": "To maintain a relationship through communication; to be on friendly terms (often used in negative contexts)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be on speaking terms",
                                "target_lang_sense_clarification": "idiom, relationship status"
                            },
                            {
                                "target_lang_word": "talk",
                                "target_lang_sense_clarification": "maintain a relationship"
                            },
                            {
                                "target_lang_word": "speak",
                                "target_lang_sense_clarification": "maintain a relationship"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0be6fc9d-7ee5-3745-916a-fa36a5406b5d",
                    "sense_definition": "Убеждать кого-либо не делать чего-то; отговаривать.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я пытался <разговаривать> его от этой поездки, но он не послушал.",
                            "target_lang_translations": {
                                "en": "I tried to <talk> him <out of> this trip, but he didn't listen."
                            }
                        },
                        {
                            "text": "Бесполезно его <разговаривать>, он уже всё решил окончательно.",
                            "target_lang_translations": {
                                "en": "It's useless to <dissuade> him; he has already decided everything once and for all."
                            }
                        }
                    ],
                    "synonyms": [
                        "отговаривать",
                        "разубеждать"
                    ],
                    "antonyms": [
                        "уговаривать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "просторечное употребление"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To convince someone not to do something; to dissuade."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dissuade",
                                "target_lang_sense_clarification": "persuade against"
                            },
                            {
                                "target_lang_word": "talk out of",
                                "target_lang_sense_clarification": "persuade against"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f16e9697-d27b-3f49-80de-66b7c34b5f68",
                    "sense_definition": "Побуждать кого-либо к беседе, вызывать желание говорить.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "causative_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный журналист умеет <разговаривать> даже самого замкнутого собеседника.",
                            "target_lang_translations": {
                                "en": "An experienced journalist knows how to <draw out> even the most reserved interviewee."
                            }
                        },
                        {
                            "text": "Её нужно долго <разговаривать>, прежде чем она расскажет правду.",
                            "target_lang_translations": {
                                "en": "You have to <get> her <talking> for a long time before she tells the truth."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To induce someone to converse; to provoke a desire to speak."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get talking",
                                "target_lang_sense_clarification": "cause to start speaking"
                            },
                            {
                                "target_lang_word": "draw out",
                                "target_lang_sense_clarification": "encourage to speak"
                            }
                        ]
                    }
                }
            ]
        }
    ]

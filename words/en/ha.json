                {
                    "sense_id": "7a62ccc7-9928-3daf-9ad1-9bee7071749e",
                    "sense_definition": "A representation of laughter.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "laughter_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ha, ha! That is a very funny joke.",

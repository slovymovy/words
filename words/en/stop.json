{
    "word_family": [
        "stopper",
        "stoppage",
        "unstoppable",
        "nonstop"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "00b22547-531b-3fea-b9bd-0ae492d24107",
                    "sense_definition": "To come to a halt; to no longer move.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cease_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bus <w>stopped</w> to let the passengers off.",
                            "target_lang_translations": {
                                "ru": "Автобус <w>остановился</w>, чтобы высадить пассажиров."
                            }
                        },
                        {
                            "text": "You must <w>stop</w> when the traffic light turns red.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>остановиться</w>, когда загорится красный свет."
                            }
                        }
                    ],
                    "synonyms": [
                        "halt",
                        "pause",
                        "rest"
                    ],
                    "antonyms": [
                        "go",
                        "move",
                        "proceed"
                    ],
                    "common_phrases": [
                        "stop suddenly",
                        "stop dead",
                        "stop short"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать двигаться; остановиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "останавливаться",
                                "target_lang_sense_clarification": "несов. вид, прекращать движение"
                            },
                            {
                                "target_lang_word": "остановиться",
                                "target_lang_sense_clarification": "сов. вид, прекратить движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4abdb04f-63db-3c2f-b61e-04749f80e2c6",
                    "sense_definition": "To no longer continue an activity or action.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cease_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>stop</w> making that loud noise.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>перестань</w> так шуметь."
                            }
                        },
                        {
                            "text": "He <w>stopped</w> eating when he felt full.",
                            "target_lang_translations": {
                                "ru": "Он <w>перестал</w> есть, когда почувствовал сытость."
                            }
                        }
                    ],
                    "synonyms": [
                        "cease",
                        "discontinue",
                        "quit",
                        "desist",
                        "give up"
                    ],
                    "antonyms": [
                        "continue",
                        "keep"
                    ],
                    "common_phrases": [
                        "stop doing",
                        "stop talking",
                        "stop working"
                    ],
                    "target_lang_definitions": {
                        "ru": "Больше не продолжать действие или занятие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переставать",
                                "target_lang_sense_clarification": "несов. вид, прекращать делать что-либо"
                            },
                            {
                                "target_lang_word": "перестать",
                                "target_lang_sense_clarification": "сов. вид, прекратить делать что-либо"
                            },
                            {
                                "target_lang_word": "прекращать",
                                "target_lang_sense_clarification": "несов. вид, завершать действие"
                            },
                            {
                                "target_lang_word": "прекратить",
                                "target_lang_sense_clarification": "сов. вид, положить конец действию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "864531e9-5c2c-34f1-8a2e-8fa2690eeb10",
                    "sense_definition": "To cause something or someone to cease moving or progressing.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cease_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer <w>stopped</w> the car for speeding.",
                            "target_lang_translations": {
                                "ru": "Полицейский <w>остановил</w> машину за превышение скорости."
                            }
                        },
                        {
                            "text": "I tried to <w>stop</w> the ball, but it was too fast.",
                            "target_lang_translations": {
                                "ru": "Я пытался <w>остановить</w> мяч, но он летел слишком быстро."
                            }
                        }
                    ],
                    "synonyms": [
                        "halt",
                        "arrest",
                        "check"
                    ],
                    "antonyms": [
                        "set in motion",
                        "accelerate"
                    ],
                    "common_phrases": [
                        "stop the car",
                        "stop the flow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то или что-то прекратить движение или развитие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "останавливать",
                                "target_lang_sense_clarification": "несов. вид, задерживать движение"
                            },
                            {
                                "target_lang_word": "остановить",
                                "target_lang_sense_clarification": "сов. вид, задержать движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5520bb36-4d3f-3292-91a1-cf974522fc59",
                    "sense_definition": "To come to an end; to cease happening.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cease_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We can go outside once the rain <w>stops</w>.",
                            "target_lang_translations": {
                                "ru": "Мы сможем пойти на улицу, как только дождь <w>прекратится</w>."
                            }
                        },
                        {
                            "text": "The music suddenly <w>stopped</w> in the middle of the song.",
                            "target_lang_translations": {
                                "ru": "Музыка внезапно <w>прекратилась</w> на середине песни."
                            }
                        }
                    ],
                    "synonyms": [
                        "end",
                        "finish",
                        "cease",
                        "terminate"
                    ],
                    "antonyms": [
                        "continue",
                        "proceed"
                    ],
                    "common_phrases": [
                        "rain stopped",
                        "noise stopped"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подойти к концу; прекратиться (о событии или явлении)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прекращаться",
                                "target_lang_sense_clarification": "несов. вид, заканчиваться"
                            },
                            {
                                "target_lang_word": "прекратиться",
                                "target_lang_sense_clarification": "сов. вид, закончиться"
                            },
                            {
                                "target_lang_word": "заканчиваться",
                                "target_lang_sense_clarification": "подходить к концу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce5bdf24-190c-333b-a254-a923ba66d698",
                    "sense_definition": "To prevent someone from doing something or something from happening.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "prevent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't let a little rain <w>stop</w> you from enjoying the day.",
                            "target_lang_translations": {
                                "ru": "Не позволяй небольшому дождю <w>помешать</w> тебе насладиться днём."
                            }
                        },
                        {
                            "text": "We must take action to <w>stop</w> pollution in our city.",
                            "target_lang_translations": {
                                "ru": "Мы должны принять меры, чтобы <w>остановить</w> загрязнение в нашем городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "prevent",
                        "hinder",
                        "block",
                        "obstruct"
                    ],
                    "common_phrases": [
                        "stop someone from",
                        "can't stop me"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помешать кому-то сделать что-то или предотвратить событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остановить",
                                "target_lang_sense_clarification": "предотвратить действие"
                            },
                            {
                                "target_lang_word": "помешать",
                                "target_lang_sense_clarification": "создать препятствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74da263d-6b17-36d2-858b-0f6a030f7c6c",
                    "sense_definition": "To stay somewhere for a short time; to visit temporarily.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "stay",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>stopped</w> at a nice hotel for the night.",
                            "target_lang_translations": {
                                "ru": "Мы <w>остановились</w> в хорошем отеле на ночь."
                            }
                        },
                        {
                            "text": "Why don't you <w>stop</w> for dinner on your way home?",
                            "target_lang_translations": {
                                "ru": "Почему бы тебе не <w>остановиться</w> поужинать по дороге домой?"
                            }
                        }
                    ],
                    "synonyms": [
                        "stay",
                        "visit",
                        "lodge",
                        "tarry"
                    ],
                    "common_phrases": [
                        "stop by",
                        "stop over",
                        "stop with"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Остаться где-то на короткое время; временно посетить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "останавливаться",
                                "target_lang_sense_clarification": "временно селиться"
                            },
                            {
                                "target_lang_word": "остановиться",
                                "target_lang_sense_clarification": "сделать привал или поселиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77f9b43d-24c4-32c8-8859-dda0943a0fb0",
                    "sense_definition": "To close or block an opening or hole.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "block",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He used a piece of cloth to <w>stop</w> the leak.",
                            "target_lang_translations": {
                                "ru": "Он использовал кусок ткани, чтобы <w>заткнуть</w> течь."
                            }
                        },
                        {
                            "text": "The sink was <w>stopped</w> up with hair and soap.",
                            "target_lang_translations": {
                                "ru": "Раковина <w>засорилась</w> волосами и мылом."
                            }
                        }
                    ],
                    "synonyms": [
                        "plug",
                        "block",
                        "seal",
                        "clog"
                    ],
                    "common_phrases": [
                        "stop a gap",
                        "stop a leak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Закрыть или заблокировать отверстие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затыка́ть",
                                "target_lang_sense_clarification": "закрывать отверстие"
                            },
                            {
                                "target_lang_word": "заткну́ть",
                                "target_lang_sense_clarification": "плотно закрыть"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "34c9baa1-4e97-3c6d-b1a1-54f04d75fa90",
                    "sense_definition": "A designated place where public transport vehicles, such as buses or trains, halt to let passengers on and off.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will meet you at the bus <w>stop</w> in ten minutes.",
                            "target_lang_translations": {
                                "ru": "Я встречу тебя на автобусной <w>остановке</w> через десять минут."
                            }
                        },
                        {
                            "text": "This is the last <w>stop</w> before the train returns to the city.",
                            "target_lang_translations": {
                                "ru": "Это последняя <w>остановка</w> перед тем, как поезд вернется в город."
                            }
                        }
                    ],
                    "synonyms": [
                        "station",
                        "halt"
                    ],
                    "common_phrases": [
                        "bus stop",
                        "tram stop",
                        "next stop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специально отведенное место, где останавливается общественный транспорт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остановка",
                                "target_lang_sense_clarification": "место посадки пассажиров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1a6a9e3-a4b1-310c-9fb2-6c0a20176ed4",
                    "sense_definition": "The action of stopping or the state of being stopped.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car came to a sudden <w>stop</w> in the middle of the road.",
                            "target_lang_translations": {
                                "ru": "Машина совершила резкую <w>остановку</w> посреди дороги."
                            }
                        },
                        {
                            "text": "We need to put a <w>stop</w> to this bad behavior immediately.",
                            "target_lang_translations": {
                                "ru": "Мы должны немедленно положить <w>конец</w> этому плохому поведению."
                            }
                        }
                    ],
                    "synonyms": [
                        "halt",
                        "cessation",
                        "pause",
                        "standstill"
                    ],
                    "antonyms": [
                        "continue"
                    ],
                    "common_phrases": [
                        "come to a stop",
                        "put a stop to",
                        "full stop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие остановки или состояние покоя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остановка",
                                "target_lang_sense_clarification": "прекращение движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3d18854-4ee9-325f-8ca3-c1c09321ef68",
                    "sense_definition": "A punctuation mark (.) used to indicate the end of a sentence or an abbreviation.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "punctuation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't forget to put a full <w>stop</w> at the end of your sentence.",
                            "target_lang_translations": {
                                "ru": "Не забудь поставить <w>точку</w> в конце предложения."
                            }
                        },
                        {
                            "text": "His answer was a firm no, full <w>stop</w>.",
                            "target_lang_translations": {
                                "ru": "Его ответ был твердым «нет», и <w>точка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "period",
                        "full stop"
                    ],
                    "common_phrases": [
                        "full stop"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly British, Australian, etc."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак препинания (.), используемый для обозначения конца предложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точка",
                                "target_lang_sense_clarification": "знак препинания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74cf7f78-9c3c-3b98-9c1b-85540fc65435",
                    "sense_definition": "A device or object used to block a path or keep something in a fixed position.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bought a heavy door <w>stop</w> to keep the door open.",
                            "target_lang_translations": {
                                "ru": "Мы купили тяжелый дверной <w>упор</w>, чтобы держать дверь открытой."
                            }
                        },
                        {
                            "text": "The rubber <w>stop</w> prevents the handle from hitting the wall.",
                            "target_lang_translations": {
                                "ru": "Резиновый <w>ограничитель</w> предотвращает удар ручки о стену."
                            }
                        }
                    ],
                    "synonyms": [
                        "stopper",
                        "wedge"
                    ],
                    "common_phrases": [
                        "door stop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство или предмет, используемый для блокировки пути или фиксации чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ограничитель",
                                "target_lang_sense_clarification": "устройство для ограничения движения"
                            },
                            {
                                "target_lang_word": "стопор",
                                "target_lang_sense_clarification": "техническое приспособление"
                            },
                            {
                                "target_lang_word": "упор",
                                "target_lang_sense_clarification": "предмет для фиксации (напр. двери)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9114eb89-bb96-37b8-b792-45242e57bdba",
                    "sense_definition": "An act of preventing the opposition from scoring a goal in sports.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The goalkeeper made a brilliant <w>stop</w> to save the match.",
                            "target_lang_translations": {
                                "ru": "Вратарь сделал блестящий <w>сейв</w>, чтобы спасти матч."
                            }
                        },
                        {
                            "text": "That was a crucial <w>stop</w> in the final minutes of the game.",
                            "target_lang_translations": {
                                "ru": "Это было решающее <w>спасение</w> на последних минутах игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "save",
                        "block"
                    ],
                    "common_phrases": [
                        "make a stop",
                        "great stop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по предотвращению гола соперника в спорте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сейв",
                                "target_lang_sense_clarification": "спорт. отражение мяча"
                            },
                            {
                                "target_lang_word": "спасение",
                                "target_lang_sense_clarification": "спорт. защита ворот"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "b45afd0d-d7f4-37c7-9179-6545e92f8d29",
                    "sense_definition": "Een plotselinge, hevige angst of ontsteltenis, veroorzaakt door iets onverwachts of gevaarlijks.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tot mijn grote <w>schrik</w> zag ik dat de voordeur wijd openstond.",
                            "target_lang_translations": {
                                "en": "To my great <w>shock</w> I saw that the front door was wide open."
                            }
                        },
                        {
                            "text": "De kinderen kregen de <w>schrik</w> van hun leven toen het licht plotseling uitviel.",
                            "target_lang_translations": {
                                "en": "The children got the <w>fright</w> of their lives when the light suddenly went out."
                            }
                        }
                    ],
                    "synonyms": [
                        "angst",
                        "ontsteltenis",
                        "vrees"
                    ],
                    "antonyms": [
                        "kalmte",
                        "geruststelling"
                    ],
                    "common_phrases": [
                        "met de schrik vrijkomen",
                        "de schrik van je leven",
                        "schrik hebben",
                        "de schrik zit erin"
                    ],
                    "target_lang_definitions": {
                        "en": "A sudden, intense fear or shock caused by something unexpected or dangerous."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fright",
                                "target_lang_sense_clarification": "sudden intense fear"
                            },
                            {
                                "target_lang_word": "scare",
                                "target_lang_sense_clarification": "a sudden feeling of fear or alarm"
                            },
                            {
                                "target_lang_word": "shock",
                                "target_lang_sense_clarification": "a sudden upsetting or surprising event"
                            }
                        ]
                    }
                }
            ]
        },

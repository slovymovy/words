                },
                {
                    "sense_id": "42a30bb1-f4fb-357f-b72a-031a29ddbc1b",
                    "sense_definition": "Een lijst met opties of commando's op een beeldscherm waaruit een gebruiker kan kiezen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet in het <w>menu</w> op de knop 'Exporteren' klikken.",
                            "target_lang_translations": {
                                "en": "You have to click the 'Export' button in the <w>menu</w>.",
                                "ru": "Вам нужно нажать кнопку «Экспорт» в <w>меню</w>."
                            }
                        },
                        {
                            "text": "Via het <w>menu</w> kun je de instellingen van het programma wijzigen.",
                            "target_lang_translations": {
                                "en": "You can change the program settings via the <w>menu</w>.",
                                "ru": "Через <w>меню</w> можно изменить настройки программы."
                            }
                        }
                    ],

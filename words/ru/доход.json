            "senses": [
                {
                    "sense_id": "c08c6e9a-75a2-3f0a-92dd-e23c7ac70953",
                    "sense_definition": "Деньги или материальные ценности, получаемые человеком или семьей в результате работы или какой-либо деятельности.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За последний год <w>доходы</w> нашей семьи выросли незначительно.",
                            "target_lang_translations": {
                                "en": "Over the last year, our family <w>income</w> increased slightly."
                            }
                        },
                        {
                            "text": "Студенты часто ищут дополнительный источник <w>дохода</w>.",
                            "target_lang_translations": {
                                "en": "Students often look for an additional source of <w>income</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "приход",
                        "выгода"
                    ],
                    "antonyms": [
                        "расход"
                    ],
                    "common_phrases": [
                        "источник дохода",
                        "высокий доход",
                        "получать доход",
                        "стабильный доход"
                    ],
                    "target_lang_definitions": {
                        "en": "Money or material assets received by a person or family through work or investments."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "income",
                                "target_lang_sense_clarification": "money received on a regular basis"
                            },
                            {
                                "target_lang_word": "earnings",
                                "target_lang_sense_clarification": "money obtained in return for labor or services"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a704e3d4-ace6-3742-a70f-81984ddba009",
                    "sense_definition": "Общая сумма денег, заработанная предприятием, организацией или государством.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Медстраховка нового работника уменьшает <w>доходы</w> фирмы.",
                            "target_lang_translations": {
                                "en": "A new employee's health insurance reduces the firm's <w>income</w>."
                            }
                        },
                        {
                            "text": "Компания скрывала свои реальные <w>доходы</w> от налоговой службы.",
                            "target_lang_translations": {
                                "en": "The company hid its real <w>revenues</w> from the tax authorities."
                            }
                        }
                    ],
                    "synonyms": [
                        "выручка",
                        "приход"
                    ],
                    "antonyms": [
                        "расход",
                        "убыток"
                    ],
                    "common_phrases": [
                        "чистый доход",
                        "налог на доход",
                        "распределение доходов"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "economics/finance"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The total amount of money earned by a business, organization, or government."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "revenue",

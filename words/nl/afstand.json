    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c5251dc0-7d35-3253-a1d5-42f1ca23ab63",
                    "sense_definition": "De fysieke ruimte of tijd tussen twee plaatsen, punten of objecten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Van hier naar de noordpool is een behoorlijke <w>afstand</w>.",
                            "target_lang_translations": {
                                "ru": "Отсюда до Северного полюса приличное <w>расстояние</w>.",
                                "en": "It is a considerable <w>distance</w> from here to the North Pole."
                            }
                        },
                        {
                            "text": "Wel moesten de mensen 1,5 meter <w>afstand</w> van elkaar houden op straat.",
                            "target_lang_translations": {
                                "ru": "Однако людям приходилось соблюдать <w>дистанцию</w> в 1,5 метра друг от друга на улице.",
                                "en": "However, people had to keep a <w>distance</w> of 1.5 meters from each other on the street."
                            }
                        }
                    ],
                    "synonyms": [
                        "verte"
                    ],
                    "antonyms": [
                        "nabijheid"
                    ],
                    "common_phrases": [
                        "op afstand",
                        "afstand houden",
                        "korte afstand",
                        "lange afstand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое пространство или время между двумя местами, точками или объектами.",
                        "en": "The physical space or time between two places, points, or objects."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расстояние",
                                "target_lang_sense_clarification": "пространство между двумя пунктами"
                            },
                            {
                                "target_lang_word": "дистанция",
                                "target_lang_sense_clarification": "интервал, который необходимо соблюдать"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "distance",
                                "target_lang_sense_clarification": "physical space between points"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9519146b-5cee-3d75-8dfa-24f4f954c07a",
                    "sense_definition": "Een gebrek aan emotionele betrokkenheid of intimiteit; terughoudendheid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij probeerde wat <w>afstand</w> te nemen van de stressvolle situatie.",
                            "target_lang_translations": {
                                "ru": "Он пытался немного <w>дистанцироваться</w> от стрессовой ситуации.",
                                "en": "He tried to create some <w>distance</w> from the stressful situation."
                            }
                        },
                        {
                            "text": "Er was een duidelijke <w>afstand</w> voelbaar tussen de twee collega's.",
                            "target_lang_translations": {
                                "ru": "Между двумя коллегами чувствовалась явная <w>дистанция</w>.",
                                "en": "A clear <w>distance</w> was palpable between the two colleagues."
                            }
                        }
                    ],
                    "common_phrases": [
                        "afstand nemen",
                        "afstand bewaren",
                        "van een afstand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствие эмоциональной близости или вовлеченности; сдержанность.",
                        "en": "A lack of emotional involvement or intimacy; reserve."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дистанция",
                                "target_lang_sense_clarification": "эмоциональная удаленность"
                            },
                            {
                                "target_lang_word": "отстранённость",
                                "target_lang_sense_clarification": "отсутствие участия или интереса"
                            },
                            {
                                "target_lang_word": "отчуждённость",
                                "target_lang_sense_clarification": "холодность в отношениях"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "distance",
                                "target_lang_sense_clarification": "emotional remoteness"
                            },
                            {
                                "target_lang_word": "aloofness",
                                "target_lang_sense_clarification": "social coldness or reserve"
                            },
                            {
                                "target_lang_word": "detachment",
                                "target_lang_sense_clarification": "lack of involvement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a922c01-9356-339e-ac17-c0114361cef1",
                    "sense_definition": "Het vrijwillig opgeven van een recht, bezit of aanspraak.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het meisje deed met moeite <w>afstand</w> van haar lievelingsbeer.",
                            "target_lang_translations": {
                                "ru": "Девочка с трудом <w>рассталась</w> со своим любимым плюшевым мишкой.",
                                "en": "The girl <w>parted</w> with her favorite bear with difficulty."
                            }
                        },
                        {
                            "text": "De koning deed officieel <w>afstand</w> van de troon ten gunste van zijn zoon.",
                            "target_lang_translations": {
                                "ru": "Король официально <w>отрекся</w> от престола в пользу своего сына.",
                                "en": "The king officially <w>abdicated</w> the throne in favor of his son."
                            }
                        }
                    ],
                    "common_phrases": [
                        "afstand doen van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добровольный отказ от права, владения или притязания.",
                        "en": "The voluntary giving up of a right, possession, or claim."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказ",
                                "target_lang_sense_clarification": "от прав или владения"
                            },
                            {
                                "target_lang_word": "отречение",
                                "target_lang_sense_clarification": "от престола или высокого статуса"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "renunciation",
                                "target_lang_sense_clarification": "act of rejecting a right or claim"
                            },
                            {
                                "target_lang_word": "waiver",
                                "target_lang_sense_clarification": "voluntary relinquishment"
                            },
                            {
                                "target_lang_word": "abdication",
                                "target_lang_sense_clarification": "giving up a throne or high office"
                            }
                        ]
                    }
                }
            ]

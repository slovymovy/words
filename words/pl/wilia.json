                    "frequency": "Low",
                    "semantic_group_id": "rivers",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Wilno jest malowniczo położone nad rzeką <w>Wilią</w>.",
                            "target_lang_translations": {
                                "ru": "Вильнюс живописно расположен на реке <w>Вилии</w>."
                            }
                        },
                        {
                            "text": "Woda w <w>Wilii</w> była zimna, ale bardzo czysta.",
                            "target_lang_translations": {
                                "ru": "Вода в <w>Вилии</w> была холодной, но очень чистой."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Река, протекающая по территории Беларуси и Литвы, правый приток Немана, протекающая в том числе через Вильнюс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Вилия",
                                "target_lang_sense_clarification": "река в Беларуси и Литве"
                            }

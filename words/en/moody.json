    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cadfdebd-e3ad-316a-bdde-4197519ebdd6",
                    "sense_definition": "Feeling or showing a state of sadness, sulkiness, or depression.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She sat in a <w>moody</w> silence after losing the game.",
                            "target_lang_translations": {
                                "ru": "Она сидела в <w>мрачном</w> молчании после проигрыша в игре."
                            }
                        },
                        {
                            "text": "Don't be so <w>moody</w> just because you didn't get your way.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>угрюмым</w> только из-за того, что всё вышло не по-твоему."
                            }
                        }
                    ],
                    "synonyms": [
                        "sulky",
                        "sullen",
                        "glum",
                        "morose",
                        "depressed",
                        "miserable",
                        "down",
                        "unhappy"
                    ],
                    "antonyms": [
                        "cheerful",
                        "happy",
                        "joyful"
                    ],
                    "common_phrases": [
                        "moody silence",
                        "moody look",
                        "moody expression"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий или выражающий грусть, угрюмость или подавленность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угрюмый",
                                "target_lang_sense_clarification": "о человеке или выражении лица"
                            },
                            {
                                "target_lang_word": "мрачный",
                                "target_lang_sense_clarification": "о настроении или виде"
                            },
                            {
                                "target_lang_word": "не в духе",
                                "target_lang_sense_clarification": "в плохом настроении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08f63006-2afc-302e-8795-607215c4b8c7",
                    "sense_definition": "Given to sudden or frequent changes of mind or mood; temperamental.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>moody</w> teenager refused to come out of his room all day.",
                            "target_lang_translations": {
                                "ru": "<w>Капризный</w> подросток отказывался выходить из своей комнаты весь день."
                            }
                        },
                        {
                            "text": "He is a <w>moody</w> player who performs brilliantly one day and poorly the next.",
                            "target_lang_translations": {
                                "ru": "Он <w>нестабильный</w> игрок, который один день выступает блестяще, а на следующий — плохо."
                            }
                        }
                    ],
                    "synonyms": [
                        "temperamental",
                        "capricious",
                        "fickle",
                        "volatile"
                    ],
                    "antonyms": [
                        "stable",
                        "steady",
                        "even-tempered"
                    ],
                    "common_phrases": [
                        "moody teenager",
                        "moody behavior"
                    ],
                    "target_lang_definitions": {

                    ],
                    "common_phrases": [
                        "bezpośrednio przed",
                        "bezpośrednio po",
                        "bezpośrednio do"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без участия третьих лиц или посредников; также: сразу, без промежутка во времени или пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напрямую",
                                "target_lang_sense_clarification": "без посредников"
                            },
                            {
                                "target_lang_word": "непосредственно",
                                "target_lang_sense_clarification": "без промежуточных звеньев или времени"
                            },
                            {
                                "target_lang_word": "прямо",

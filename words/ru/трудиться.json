    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4fdbd879-0654-3301-83e2-570743119abb",
                    "sense_definition": "Заниматься полезной деятельностью, выполнять работу, затрачивая физические или умственные силы.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "work_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой отец всю жизнь честно <трудился> на заводе."
                        },
                        {
                            "text": "В этой семье все много <трудятся>, чтобы обеспечить себе достойную жизнь."
                        }
                    ],
                    "synonyms": [
                        "работать"
                    ],
                    "antonyms": [
                        "лениться",
                        "отлынивать"
                    ],
                    "common_phrases": [
                        "усердно трудиться",
                        "честно трудиться",
                        "трудиться на благо",
                        "трудиться не покладая рук"
                    ]
                },
                {
                    "sense_id": "b96da698-bdde-36ce-8534-71b03c116fa4",
                    "sense_definition": "Прилагать усилия, упорно работать над созданием или изучением чего-либо (обычно с предлогом «над»).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "work_specific",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Писатель долго <трудился> над своим новым романом."
                        },
                        {
                            "text": "Сейчас я <тружусь> над интересной проблемой, которая требует много времени."
                        }
                    ],
                    "synonyms": [
                        "работать",
                        "стараться",
                        "силиться"
                    ],
                    "antonyms": [
                        "лениться"
                    ],
                    "common_phrases": [
                        "трудиться над проектом",
                        "трудиться над собой",
                        "трудиться над задачей"
                    ]
                },
                {
                    "sense_id": "e797b251-105c-385c-9919-dbbc51c9487f",
                    "sense_definition": "Утруждать себя, делать лишние усилия (часто используется в повелительном наклонении с отрицанием).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bother",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не <трудись> объяснять мне причины, я и так всё понимаю."
                        },
                        {
                            "text": "Можешь не <трудиться> искать оправдания своему поступку."
                        }
                    ],
                    "synonyms": [
                        "стараться",
                        "затрудняться"
                    ],
                    "common_phrases": [
                        "не трудись объяснять",
                        "не трудись отвечать"
                    ]
                }
            ]
        }
    ]

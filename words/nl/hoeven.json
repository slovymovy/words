{
    "word_family": [
        "behoeven",
        "behoefte"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17cf9b2f-2216-329b-a593-b882c34b6dd8",
                    "sense_definition": "Niet verplicht of noodzakelijk zijn om iets te doen (meestal gebruikt met een ontkenning).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "auxiliary_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je <w>hoeft</w> vandaag niet te werken als je ziek bent.",
                            "target_lang_translations": {
                                "ru": "Тебе сегодня не <w>нужно</w> работать, если ты болен.",
                                "en": "You don't <w>have</w> to work today if you are sick."
                            }
                        },
                        {
                            "text": "Wij <w>hoefden</w> gelukkig niet lang op de trein te wachten.",
                            "target_lang_translations": {
                                "ru": "К счастью, нам не <w>пришлось</w> долго ждать поезда.",
                                "en": "Fortunately, we didn't <w>have</w> to wait long for the train."
                            }
                        }
                    ],
                    "synonyms": [
                        "behoeven"
                    ],
                    "antonyms": [
                        "moeten"
                    ],
                    "common_phrases": [
                        "niet hoeven te",
                        "nergens voor hoeven"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "negative polarity item"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не быть обязанным или вынужденным что-то делать (обычно используется с отрицанием).",
                        "en": "To not be obliged or necessary to do something (usually used with a negation)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "в сочетании с отрицанием (не нужно)"
                            },
                            {
                                "target_lang_word": "приходиться",
                                "target_lang_sense_clarification": "в сочетании с отрицанием (не приходится/не пришлось)"
                            },
                            {
                                "target_lang_word": "обязан",
                                "target_lang_sense_clarification": "в сочетании с отрицанием (не обязан)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "have to",
                                "target_lang_sense_clarification": "used with negation (don't have to)"
                            },
                            {
                                "target_lang_word": "need",
                                "target_lang_sense_clarification": "used with negation (need not)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15158e63-6632-3a96-8d81-7ae6d516dcae",
                    "sense_definition": "Willen hebben of nodig hebben (meestal gebruikt in ontkennende zinnen).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transitive_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>hoef</w> geen koffie, dank je wel.",
                            "target_lang_translations": {
                                "ru": "Я не <w>хочу</w> кофе, спасибо.",
                                "en": "I don't <w>want</w> any coffee, thank you."
                            }
                        },
                        {
                            "text": "Hij zei dat hij dat geld niet <w>hoefde</w>.",
                            "target_lang_translations": {
                                "ru": "Он сказал, что эти деньги ему не <w>нужны</w>.",
                                "en": "He said he didn't <w>want</w> that money."
                            }
                        }
                    ],
                    "synonyms": [
                        "nodig hebben"
                    ],
                    "common_phrases": [
                        "niet hoeven"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "negative polarity item"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нуждаться в чем-то или хотеть чего-то (обычно используется в отрицательных предложениях).",
                        "en": "To want to have or need something (usually used in negative sentences)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нуждаться",
                                "target_lang_sense_clarification": "в сочетании с отрицанием (не нужен)"
                            },
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "в сочетании с отрицанием (не хочу)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "want",
                                "target_lang_sense_clarification": "used with negation (don't want)"
                            },
                            {
                                "target_lang_word": "need",
                                "target_lang_sense_clarification": "used with negation (don't need)"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5c1e00bb-1837-3286-819e-ec87e1d00396",
                    "sense_definition": "Een bromfiets of motorfiets, vaak informeel zo genoemd vanwege het ploffende geluid van de motor.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buurman rijdt elke ochtend op zijn oude <w>ploffer</w> naar het werk.",
                            "target_lang_translations": {
                                "en": "My neighbor rides his old <w>moped</w> to work every morning."
                            }
                        },
                        {
                            "text": "We hoorden de <w>ploffer</w> al van ver aankomen door het lawaai dat hij maakte.",
                            "target_lang_translations": {
                                "en": "We could hear the <w>motorbike</w> coming from afar because of the noise it made."
                            }
                        }
                    ],
                    "synonyms": [
                        "brommer",
                        "motor"
                    ],
                    "common_phrases": [
                        "oude ploffer"
                    ],
                    "target_lang_definitions": {
                        "en": "A moped or motorcycle, often informally called this because of the popping sound of the engine."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "moped",
                                "target_lang_sense_clarification": "informal term for a light motorcycle"
                            },
                            {
                                "target_lang_word": "motorbike",
                                "target_lang_sense_clarification": "informal reference to the vehicle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20916989-f462-3763-811f-653f0bd77ef9",
                    "sense_definition": "Een handvuurwapen zoals een pistool of revolver.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de oude detectivefilm trok de gangster plotseling een <w>ploffer</w> uit zijn jas.",
                            "target_lang_translations": {
                                "en": "In the old detective movie, the gangster suddenly pulled a <w>gun</w> from his coat."
                            }
                        },
                        {
                            "text": "De politie vond een verborgen <w>ploffer</w> tijdens de huiszoeking.",
                            "target_lang_translations": {
                                "en": "The police found a hidden <w>piece</w> during the search."
                            }
                        }
                    ],
                    "synonyms": [
                        "pistool",
                        "revolver",
                        "schietijzer"
                    ],
                    "target_lang_definitions": {
                        "en": "A firearm such as a pistol or revolver."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gun",
                                "target_lang_sense_clarification": "slang term for a firearm"
                            },
                            {
                                "target_lang_word": "piece",
                                "target_lang_sense_clarification": "slang for a weapon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd44e0cd-9c6e-3fe3-aa34-636e67522a4f",
                    "sense_definition": "Een medeklinker die wordt gevormd door de luchtstroom in de mond volledig te blokkeren en daarna plotseling los te laten.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De letters 'p', 't' en 'k' zijn voorbeelden van een stemloze <w>ploffer</w>.",
                            "target_lang_translations": {
                                "en": "The letters 'p', 't', and 'k' are examples of a voiceless <w>plosive</w>."
                            }
                        },
                        {
                            "text": "Bij de uitspraak van een <w>ploffer</w> hoor je een korte explosie van lucht.",
                            "target_lang_translations": {
                                "en": "When pronouncing a <w>plosive</w>, you hear a short explosion of air."
                            }
                        }
                    ],
                    "synonyms": [
                        "plofklank",
                        "occlusief"
                    ],
                    "common_phrases": [
                        "stemloze ploffer",
                        "stemhebbende ploffer"
                    ],
                    "target_lang_definitions": {
                        "en": "A consonant formed by completely blocking the airflow in the mouth and then suddenly releasing it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plosive",
                                "target_lang_sense_clarification": "linguistics term"
                            },
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "linguistics term"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
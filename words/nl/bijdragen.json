    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e5c1df25-a9a7-38fe-a911-b756e527af99",
                    "sense_definition": "Een aandeel leveren aan iets; helpen om een bepaald resultaat te bereiken of iets te veroorzaken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "contribute_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gezond eten en voldoende beweging <w>dragen bij</w> aan een fit lichaam.",
                            "target_lang_translations": {
                                "en": "Healthy eating and sufficient exercise <w>contribute</w> to a fit body.",
                                "ru": "Здоровое питание и достаточная физическая активность <w>способствуют</w> хорошей физической форме."
                            }
                        },
                        {
                            "text": "De nieuwe maatregelen zullen zeker <w>bijdragen</w> aan de veiligheid op straat.",
                            "target_lang_translations": {
                                "en": "The new measures will certainly <w>contribute</w> to safety on the street.",
                                "ru": "Новые меры, безусловно, <w>будут способствовать</w> безопасности на улицах."
                            }
                        },
                        {
                            "text": "De mensheid <w>draagt</w> met aan zekerheid grenzende waarschijnlijkheid <w>bij</w> aan de klimaatsverandering.",
                            "target_lang_translations": {
                                "en": "Humanity <w>contributes</w> to climate change with near certainty.",
                                "ru": "Человечество с вероятностью, граничащей с достоверностью, <w>вносит вклад</w> в изменение климата."
                            }
                        }
                    ],
                    "synonyms": [
                        "contribueren",
                        "bijdrages",
                        "bijdrages"
                    ],
                    "antonyms": [
                        "tegenwerken"
                    ],
                    "common_phrases": [
                        "bijdragen aan",
                        "actief bijdragen",
                        "ertoe bijdragen dat"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide a share in something; to help achieve a result or cause something.",
                        "ru": "Вносить вклад во что-либо; помогать достичь определенного результата или вызвать что-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "contribute",
                                "target_lang_sense_clarification": "to help cause a result"
                            },
                            {
                                "target_lang_word": "add to",
                                "target_lang_sense_clarification": "to increase or enhance a situation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "способствовать",
                                "target_lang_sense_clarification": "помогать развитию или осуществлению чего-либо"
                            },
                            {
                                "target_lang_word": "вносить вклад",
                                "target_lang_sense_clarification": "делать что-либо полезное для общего дела"
                            },
                            {
                                "target_lang_word": "содействовать",
                                "target_lang_sense_clarification": "оказывать помощь в какой-либо деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54fc0408-db80-39da-b586-63d60361038b",
                    "sense_definition": "Geld of middelen geven als aandeel in de totale kosten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "contribute_financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Alle ouders moesten financieel <w>bijdragen</w> aan het schoolreisje.",
                            "target_lang_translations": {
                                "en": "All parents had to <w>contribute</w> financially to the school trip.",
                                "ru": "Все родители должны были финансово <w>поучаствовать</w> в школьной поездке."
                            }
                        },
                        {
                            "text": "Hij <w>droeg</w> gul <w>bij</w> aan de inzameling voor het goede doel.",
                            "target_lang_translations": {
                                "en": "He <w>contributed</w> generously to the collection for the charity.",
                                "ru": "Он щедро <w>пожертвовал</w> на сбор средств для благотворительности."
                            }
                        }
                    ],
                    "synonyms": [
                        "contribueren",
                        "bijdrages",
                        "bijdrages"
                    ],
                    "common_phrases": [
                        "financieel bijdragen",
                        "bijdragen in de kosten"
                    ],
                    "target_lang_definitions": {

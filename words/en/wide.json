                    "target_lang_definitions": {
                        "ru": "В крикете: мяч, поданный слишком далеко от бэтсмена, за что начисляется дополнительное очко."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уайд",
                                "target_lang_sense_clarification": "термин в крикете"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
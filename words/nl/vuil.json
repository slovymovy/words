                },
                {
                    "sense_id": "8711774a-7f3a-33a2-8051-57f7985ffa47",
                    "sense_definition": "Viezigheid; materie die iets onrein maakt, zoals stof of modder.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "dirt_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zit een beetje <vuil> in mijn oog dat erg irriteert.",
                            "target_lang_translations": {

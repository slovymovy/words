                },
                {
                    "sense_id": "8e6ca342-30c6-3761-994a-a1c067e32dd5",
                    "sense_definition": "Środki potrzebne do życia, utrzymanie lub praca zarobkowa pozwalająca na zaspokojenie potrzeb.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "livelihood",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8fa37a9e-4134-3268-adbb-eea341ba2f35",
                    "sense_definition": "Mający kształt figury o czterech równych bokach i kątach prostych lub kształt do niej zbliżony; kanciasty.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "shape_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W kuchni stoi duży, <w>kwadratowy</w> stół dla czterech osób.",
                            "target_lang_translations": {
                                "ru": "На кухне стоит большой <w>квадратный</w> стол на четырех человек."
                            }
                        },
                        {
                            "text": "Jego <w>kwadratowa</w> twarz o ostrych rysach wyglądała bardzo poważnie.",
                            "target_lang_translations": {
                                "ru": "Его <w>квадратное</w> лицо с острыми чертами выглядело очень серьезным."
                            }
                        }
                    ],
                    "synonyms": [
                        "kanciasty"
                    ],
                    "antonyms": [
                        "okrągły",
                        "owalny"
                    ],
                    "common_phrases": [
                        "kwadratowy stół",
                        "kwadratowa twarz",
                        "kwadratowy nawias",
                        "kwadratowy kształt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий форму фигуры с четырьмя равными сторонами и прямыми углами или форму, близкую к ней; угловатый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квадратный",
                                "target_lang_sense_clarification": "о форме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3aa132e-64bc-3430-850a-d13fdae4dc20",
                    "sense_definition": "Służący do określania miary powierzchni w połączeniu z nazwą jednostki długości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "math_units",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wynajmuję kawalerkę, która ma trzydzieści metrów <w>kwadratowych</w>.",
                            "target_lang_translations": {
                                "ru": "Я снимаю однокомнатную квартиру, в которой тридцать <w>квадратных</w> метров."
                            }
                        },
                        {
                            "text": "Jeden kilometr <w>kwadratowy</w> to sto hektarów.",
                            "target_lang_translations": {
                                "ru": "Один <w>квадратный</w> километр — это сто гектаров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "metr kwadratowy",
                        "kilometr kwadratowy",
                        "centymetr kwadratowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используемый для обозначения меры площади в сочетании с названием единицы длины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квадратный",
                                "target_lang_sense_clarification": "мера площади"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95d879df-9811-38b8-a82b-3ae2936e4e82",
                    "sense_definition": "W matematyce: dotyczący drugiej potęgi liczby, wyrażenia algebraicznego lub pierwiastka drugiego stopnia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "math_algebra",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na dzisiejszej lekcji matematyki omawialiśmy funkcję <w>kwadratową</w>.",
                            "target_lang_translations": {
                                "ru": "На сегодняшнем уроке математики мы обсуждали <w>квадратичную</w> функцию."
                            }
                        },
                        {
                            "text": "Musisz obliczyć pierwiastek <w>kwadratowy</w> z tej liczby, aby otrzymać wynik.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно вычислить <w>квадратный</w> корень из этого числа, чтобы получить результат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pierwiastek kwadratowy",
                        "funkcja kwadratowa",
                        "równanie kwadratowe",
                        "trójmian kwadratowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "В математике: относящийся ко второй степени числа, алгебраического выражения или корня второй степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квадратный",
                                "target_lang_sense_clarification": "корень, уравнение"
                            },
                            {
                                "target_lang_word": "квадратичный",
                                "target_lang_sense_clarification": "функция"
                            }
                        ]
                    }
                }
            ]
        }
    ]

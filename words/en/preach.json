    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "72714ef0-7494-3e6b-b520-3891b25a3211",
                    "sense_definition": "To deliver a religious speech or sermon, usually during a church service.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_discourse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The priest <w>preaches</w> to the congregation every Sunday morning."
                        },
                        {
                            "text": "He traveled around the country to <w>preach</w> the gospel to anyone who would listen."
                        }
                    ],
                    "synonyms": [
                        "church"
                    ],
                    "common_phrases": [
                        "preach the gospel",
                        "preach a sermon",
                        "preach to the converted"
                    ]
                },
                {
                    "sense_id": "a739ab90-b1ff-3e62-b0e0-3335a243fafb",
                    "sense_definition": "To earnestly advocate for or recommend a particular behavior, belief, or lifestyle.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "advocacy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She always <w>preaches</w> the benefits of a healthy diet and regular exercise."
                        },
                        {
                            "text": "It is important to practice what you <w>preach</w> before criticizing others."
                        }
                    ],
                    "synonyms": [
                        "church"
                    ],
                    "common_phrases": [
                        "practice what you preach",
                        "preach tolerance",
                        "preach patience"
                    ]
                },
                {
                    "sense_id": "e061af96-1a62-393c-9d73-63cf16097fc9",
                    "sense_definition": "To give moral advice or instruction in an annoying, self-righteous, or unwanted manner.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "advocacy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not <w>preach</w> at me about how to spend my money."
                        },
                        {
                            "text": "I hate it when he starts to <w>preach</w> about my life choices."
                        }
                    ],
                    "common_phrases": [
                        "stop preaching",
                        "preach at someone"
                    ]
                }
            ]
        },

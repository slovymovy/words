            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e5a0ee2-7abf-3fce-b831-14932e0ff626",
                    "sense_definition": "akt opuszczenia kogoś lub czegoś na zawsze, często wiążący się z pozostawieniem bez opieki",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abandonment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagłe <w>porzucenie</w> rodziny przez ojca było dla dzieci ogromnym szokiem.",
                            "target_lang_translations": {
                                "ru": "Внезапный <w>уход</w> отца из семьи стал для детей огромным шоком."
                            }
                        },
                        {
                            "text": "Za <w>porzucenie</w> psa w lesie grozi wysoka grzywna lub kara więzienia.",
                            "target_lang_translations": {
                                "ru": "За <w>оставление</w> собаки в лесу грозит высокий штраф или тюремное заключение."
                            }
                        }
                    ],
                    "synonyms": [
                        "opuszczenie",
                        "pozostawienie"
                    ],
                    "antonyms": [
                        "przygarnięcie",
                        "zaopiekowanie się"
                    ],
                    "common_phrases": [
                        "porzucenie dziecka",
                        "porzucenie rodziny",
                        "porzucenie psa",
                        "porzucenie mienia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт оставления кого-либо или чего-либо навсегда, часто связанный с отсутствием заботы или присмотра"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставление",
                                "target_lang_sense_clarification": "формальное; например, оставление в опасности или оставление семьи"
                            },
                            {
                                "target_lang_word": "бросание",
                                "target_lang_sense_clarification": "разговорное; действие по значению глагола бросить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9733ed9c-1fd6-359f-937f-16028406e61f",
                    "sense_definition": "ostateczne zrezygnowanie z wykonywania jakiejś czynności, realizacji planu lub wyznawania idei",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "quitting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej niespodziewane <w>porzucenie</w> studiów medycznych na ostatnim roku zdziwiło wszystkich znajomych.",
                            "target_lang_translations": {
                                "ru": "Её неожиданный <w>уход</w> с медицинского факультета на последнем курсе удивил всех знакомых."
                            }
                        },
                        {
                            "text": "Całkowite <w>porzucenie</w> palenia papierosów znacznie poprawiło jego stan zdrowia i kondycję.",
                            "target_lang_translations": {
                                "ru": "Полный <w>отказ</w> от курения сигарет значительно улучшил его здоровье и физическую форму."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaniechanie",
                        "rezygnacja",
                        "zaprzestanie"
                    ],
                    "antonyms": [
                        "kontynuacja",
                        "podtrzymanie"
                    ],
                    "common_phrases": [
                        "porzucenie pracy",
                        "porzucenie studiów",
                        "porzucenie nadziei",
                        "porzucenie nałogu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окончательный отказ от выполнения какого-либо действия, реализации плана или следования идее"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказ",
                                "target_lang_sense_clarification": "от привычки, планов или убеждений"
                            },
                            {
                                "target_lang_word": "уход",
                                "target_lang_sense_clarification": "с работы, из университета"
                            },
                            {
                                "target_lang_word": "прекращение",
                                "target_lang_sense_clarification": "действия или процесса"
                            }
                        ]
                    }
                }
            ]
        }
    ]

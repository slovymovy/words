                    "common_phrases": [
                        "coat of arms"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Геральдические символы или знаки отличия, используемые семьей или организацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герб",
                                "target_lang_sense_clarification": "геральдический символ"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d7790972-479a-3466-9e40-d76fc2b3ab42",
                    "sense_definition": "To supply with weapons or armor.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government <w>armed</w> the troops with new rifles before the mission.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>вооружило</w> войска новыми винтовками перед миссией."
                            }
                        },
                        {
                            "text": "The villagers <w>armed</w> themselves to defend their homes against bandits.",
                            "target_lang_translations": {
                                "ru": "Жители деревни <w>вооружились</w>, чтобы защитить свои дома от бандитов."
                            }
                        }
                    ],
                    "synonyms": [
                        "beweapon"
                    ],
                    "common_phrases": [
                        "arm oneself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжать оружием или доспехами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вооружать",
                                "target_lang_sense_clarification": "снабжать оружием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ad93d6a-2604-3f3e-8c75-12c1a1a1887a",
                    "sense_definition": "To prepare a device, such as a bomb or alarm, for operation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please remember to <w>arm</w> the security system before you leave the house.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не забудьте <w>включить</w> охранную систему перед выходом из дома."
                            }
                        },
                        {
                            "text": "The pilot <w>armed</w> the missiles as he approached the target.",
                            "target_lang_translations": {
                                "ru": "Пилот <w>привел в боевую готовность</w> ракеты при приближении к цели."
                            }
                        }
                    ],
                    "common_phrases": [
                        "arm the alarm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подготавливать устройство, например бомбу или сигнализацию, к работе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "сигнализацию"
                            },
                            {
                                "target_lang_word": "взводить",
                                "target_lang_sense_clarification": "механизм"
                            },
                            {
                                "target_lang_word": "приводить в боевую готовность",
                                "target_lang_sense_clarification": "оружие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d73dbc6-f123-3c8b-a1d1-28c639250d14",
                    "sense_definition": "To provide with knowledge, skills, or authority needed for a task.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "military_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She went into the meeting <w>armed</w> with the latest statistics and reports.",
                            "target_lang_translations": {
                                "ru": "Она пришла на встречу, <w>вооруженная</w> последней статистикой и отчетами."
                            }
                        },
                        {
                            "text": "The training course <w>arms</w> students with essential skills for their future careers.",
                            "target_lang_translations": {
                                "ru": "Учебный курс <w>вооружает</w> студентов необходимыми навыками для их будущей карьеры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "armed with information",
                        "armed with patience"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставлять знания, навыки или полномочия, необходимые для выполнения задачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вооружать",
                                "target_lang_sense_clarification": "знаниями или навыками"
                            }
                        ]
                    }
                }
            ]
        },

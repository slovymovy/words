    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "928509c9-dd57-39f9-b3dc-02319f613d39",
                    "sense_definition": "The third-person singular simple present indicative form of the verb <need>, indicating that something is required or essential.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <needs> to finish her homework before she can go out.",
                            "target_lang_translations": {
                                "ru": "Ей <w>нужно</w> закончить домашнюю работу, прежде чем она сможет пойти гулять."
                            }
                        },
                        {
                            "text": "The car <needs> a new battery to start properly.",
                            "target_lang_translations": {
                                "ru": "Машине <w>нужен</w> новый аккумулятор, чтобы нормально заводиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "requires",
                        "demands"
                    ],
                    "common_phrases": [
                        "needs to",
                        "needs help",
                        "needs time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма третьего лица единственного числа глагола \"need\", указывающая на то, что что-то требуется или является необходимым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нуждается",
                                "target_lang_sense_clarification": "испытывать потребность в чем-либо"
                            },
                            {
                                "target_lang_word": "требует",
                                "target_lang_sense_clarification": "быть необходимым (о вещах или действиях)"
                            },
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "необходимо (используется как сказуемое)"
                            }
                        ]
                    }
                }
            ]
        },
        {

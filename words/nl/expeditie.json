    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a66d2a60-77d7-37f1-9c57-812a7b5aa356",
                    "sense_definition": "Een georganiseerde tocht of reis, vaak naar een afgelegen gebied voor wetenschappelijk onderzoek of ontdekking.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_journey",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De biologen gingen op <w>expeditie</w> naar de Amazone om nieuwe diersoorten te vinden.",
                            "target_lang_translations": {
                                "en": "The biologists went on an <w>expedition</w> to the Amazon to find new animal species.",
                                "ru": "Биологи отправились в <w>экспедицию</w> на Амазонку, чтобы найти новые виды животных."
                            }
                        },
                        {
                            "text": "Tijdens de <w>expeditie</w> naar de Noordpool moesten de onderzoekers extreme kou trotseren.",
                            "target_lang_translations": {
                                "en": "During the <w>expedition</w> to the North Pole, the researchers had to brave extreme cold.",
                                "ru": "Во время <w>экспедиции</w> на Северный полюс исследователям пришлось противостоять экстремальному холоду."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontdekkingsreis",
                        "onderzoekingstocht"
                    ],
                    "common_phrases": [
                        "op expeditie gaan",
                        "wetenschappelijke expeditie",
                        "pool-expeditie"
                    ],
                    "target_lang_definitions": {
                        "en": "An organized journey or trip, often to a remote area for scientific research or discovery.",
                        "ru": "Организованное путешествие или поход, часто в отдаленный район для научных исследований или открытий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expedition",
                                "target_lang_sense_clarification": "journey for a specific purpose"
                            },
                            {
                                "target_lang_word": "exploration",
                                "target_lang_sense_clarification": "act of exploring"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "экспедиция",
                                "target_lang_sense_clarification": "научная или исследовательская"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "641db72c-33b4-330f-80c3-ba7657b07c0d",
                    "sense_definition": "Het proces van het verzenden en vervoeren van goederen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_logistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>expeditie</w> van de bestelling duurt doorgaans twee tot drie werkdagen.",
                            "target_lang_translations": {
                                "en": "The <w>shipping</w> of the order usually takes two to three working days.",
                                "ru": "<w>Отправка</w> заказа обычно занимает от двух до трех рабочих дней."
                            }
                        },
                        {
                            "text": "Hij werkt in de logistiek en is verantwoordelijk voor de dagelijkse <w>expeditie</w> van vracht.",
                            "target_lang_translations": {
                                "en": "He works in logistics and is responsible for the daily <w>dispatch</w> of freight.",
                                "ru": "Он работает в логистике и отвечает за ежедневную <w>отправку</w> грузов."
                            }
                        }
                    ],
                    "synonyms": [
                        "verzending",
                        "transport",
                        "goederenvervoer"
                    ],
                    "common_phrases": [
                        "expeditie van goederen"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of sending off and transporting goods.",
                        "ru": "Процесс отправки и перевозки грузов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shipping",
                                "target_lang_sense_clarification": "transport of goods"
                            },
                            {
                                "target_lang_word": "dispatch",
                                "target_lang_sense_clarification": "act of sending off"
                            },
                            {
                                "target_lang_word": "forwarding",
                                "target_lang_sense_clarification": "logistics process"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отправка",
                                "target_lang_sense_clarification": "процесс посылки груза"
                            },
                            {
                                "target_lang_word": "экспедирование",
                                "target_lang_sense_clarification": "сопровождение грузов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd2271ac-c096-358c-9f98-701459bb27a9",
                    "sense_definition": "De afdeling binnen een bedrijf die verantwoordelijk is voor het verzendklaar maken en versturen van goederen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_logistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt de vrachtbrief ophalen bij de balie van de <w>expeditie</w>.",
                            "target_lang_translations": {
                                "en": "You can pick up the waybill at the <w>shipping department</w> desk.",
                                "ru": "Вы можете забрать накладную на стойке <w>отдела отправки</w>."
                            }
                        },
                        {
                            "text": "De vrachtwagens staan te wachten bij de <w>expeditie</w> om geladen te worden.",
                            "target_lang_translations": {
                                "en": "The trucks are waiting at the <w>dispatch area</w> to be loaded.",
                                "ru": "Грузовики ждут у <w>экспедиции</w>, чтобы загрузиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "verzendafdeling"
                    ],
                    "common_phrases": [
                        "hoofd expeditie",
                        "werken op de expeditie"
                    ],
                    "target_lang_definitions": {
                        "en": "The department within a company responsible for preparing and sending out goods.",
                        "ru": "Отдел в компании, отвечающий за подготовку и отправку товаров."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shipping department",

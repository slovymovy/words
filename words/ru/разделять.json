        "поделять"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f307010d-fda5-3a48-91af-6760b88b8516",
                    "sense_definition": "Делить целое на части или служить границей между чем-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_division",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Широкая река <разделяет> город на две половины.",
                            "target_lang_translations": {
                                "en": "A wide river <w>divides</w> the city into two halves."
                            }
                        },
                        {
                            "text": "Вам нужно <разделять> текст на абзацы для удобства чтения.",
                            "target_lang_translations": {
                                "en": "You need to <w>divide</w> the text into paragraphs for easier reading."
                            }
                        }
                    ],
                    "synonyms": [
                        "делить",
                        "разъединять",
                        "расчленять"
                    ],
                    "antonyms": [
                        "соединять",
                        "объединять"
                    ],
                    "common_phrases": [
                        "разделять на части",
                        "разделять на группы",
                        "разделять границей"
                    ],
                    "target_lang_definitions": {
                        "en": "To divide a whole into parts or serve as a boundary between things."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "divide",
                                "target_lang_sense_clarification": "separate into parts"
                            },
                            {
                                "target_lang_word": "separate",
                                "target_lang_sense_clarification": "keep apart"
                            },
                            {
                                "target_lang_word": "split",
                                "target_lang_sense_clarification": "break into parts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c369dbd-ab80-3382-9ce0-581e048d7249",
                    "sense_definition": "Быть согласным с чьим-либо мнением или испытывать те же чувства, что и другой человек.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я полностью <разделяю> ваше мнение по этому важному вопросу.",
                            "target_lang_translations": {
                                "en": "I fully <w>share</w> your opinion on this important matter."
                            }
                        },
                        {
                            "text": "Друзья должны <разделять> не только радость, но и горе.",
                            "target_lang_translations": {
                                "en": "Friends should <w>share</w> not only joy, but also grief."
                            }
                        }
                    ],
                    "synonyms": [
                        "делить",
                        "распределять"
                    ],
                    "common_phrases": [
                        "разделять мнение",
                        "разделять точку зрения",
                        "разделять убеждения",
                        "разделять чувства"
                    ],
                    "target_lang_definitions": {
                        "en": "To agree with someone's opinion or experience the same feelings."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "share",
                                "target_lang_sense_clarification": "feelings or opinions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe637148-3ba8-3d51-bc91-99da7065e192",
                    "sense_definition": "Пользоваться чем-либо совместно с другими или распределять что-то между кем-то.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sharing_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студентам приходится <разделять> одну маленькую комнату в общежитии.",
                            "target_lang_translations": {
                                "en": "Students have to <w>share</w> one small room in the dormitory."
                            }
                        },
                        {
                            "text": "Мы договорились <разделять> все расходы на поездку поровну.",
                            "target_lang_translations": {
                                "en": "We agreed to <w>split</w> all trip expenses equally."
                            }
                        }
                    ],
                    "synonyms": [
                        "распределять",
                        "делить"
                    ],
                    "common_phrases": [
                        "разделять ответственность",
                        "разделять обязанности",
                        "разделять трапезу"
                    ],
                    "target_lang_definitions": {
                        "en": "To use something jointly with others or distribute something among people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "share",
                                "target_lang_sense_clarification": "use jointly"
                            },
                            {
                                "target_lang_word": "split",
                                "target_lang_sense_clarification": "distribute or divide up"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "92fb9df2-a4b8-3617-a4b9-db436edabd10",
                    "sense_definition": "Современный динамичный танец с акробатическими элементами; сокращённое название брейк-данса.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Парень вначале станцевал на скользкой дороге некое подобие верхнего <w>брейка</w>, а потом потерял равновесие.",
                            "target_lang_translations": {
                                "en": "At first, the guy danced a semblance of upper <w>breakdancing</w> on the slippery road, and then lost his balance."
                            }
                        },
                        {
                            "text": "На школьной дискотеке все с интересом смотрели, как он танцует <w>брейк</w>.",
                            "target_lang_translations": {
                                "en": "At the school disco, everyone watched with interest as he danced <w>breakdance</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "брейк-данс"
                    ],
                    "common_phrases": [
                        "танцевать брейк",
                        "верхний брейк",
                        "нижний брейк"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "сокращение от брейк-данс"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A modern dynamic dance style characterized by acrobatic movements; short for breakdancing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breakdancing",
                                "target_lang_sense_clarification": "dance style"
                            },
                            {
                                "target_lang_word": "breaking",
                                "target_lang_sense_clarification": "hip-hop culture term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "993bcd48-1254-32e1-b359-c7ed3f19eba5",
                    "sense_definition": "Короткий перерыв, временная пауза в работе или другом занятии.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы работали три часа подряд, поэтому решили сделать небольшой <w>брейк</w>.",
                            "target_lang_translations": {
                                "en": "We worked for three hours straight, so we decided to take a short <w>break</w>."
                            }
                        },
                        {
                            "text": "Во время конференции был объявлен <w>брейк</w> на кофе и свободное общение.",
                            "target_lang_translations": {
                                "en": "During the conference, a <w>break</w> for coffee and socializing was announced."
                            }
                        }
                    ],
                    "synonyms": [
                        "перерыв",
                        "прерывание"
                    ],
                    "common_phrases": [
                        "сделать брейк",
                        "кофе-брейк"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A short interval or pause in work or an activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "rest period"
                            },
                            {
                                "target_lang_word": "pause",
                                "target_lang_sense_clarification": "temporary stop"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "c112bb8c-741e-32d1-96f1-31a944d5a462",
                    "sense_definition": "Команда рефери в боксе, требующая от спортсменов прекратить ближний бой и разойтись.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рефери громко крикнул «<w>брейк</w>», но боксёры не сразу услышали команду.",
                            "target_lang_translations": {
                                "en": "The referee shouted \"<w>break</w>\" loudly, but the boxers didn't hear the command immediately."
                            }
                        },
                        {
                            "text": "Если бойцы входят в клинч, судья вмешивается и объявляет <w>брейк</w>.",
                            "target_lang_translations": {
                                "en": "If the fighters enter a clinch, the referee intervenes and calls <w>break</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "команда брейк"
                    ],
                    "target_lang_definitions": {
                        "en": "A command given by a boxing referee to separate fighters."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "boxing command"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "5dbfdb9b-4e8a-308e-b6ff-3a1297cb56e4",
                    "sense_definition": "Een persoon die boodschappen van een godheid aan de mensen doorgeeft",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moslims geloven dat Mohammed de laatste <w>profeet</w> is.",
                            "target_lang_translations": {
                                "en": "Muslims believe that Muhammad is the last <w>prophet</w>."
                            }
                        },
                        {
                            "text": "De <w>profeet</w> waarschuwde het volk om hun leven te beteren.",
                            "target_lang_translations": {
                                "en": "The <w>prophet</w> warned the people to mend their ways."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "79b8e4fd-97d6-3407-b7d1-ae5640403da5",
                    "sense_definition": "Действие по глаголу «изгнать»; принудительное удаление кого-либо из страны, города, жилища или сообщества.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Король подписал указ об <w>изгнании</w> всех заговорщиков из столицы.",
                            "target_lang_translations": {
                                "en": "The king signed a decree on the <w>banishment</w> of all conspirators from the capital."
                            }
                        },
                        {
                            "text": "Обряд <w>изгнания</w> злых духов продолжался всю ночь.",
                            "target_lang_translations": {
                                "en": "The ritual of the <w>casting out</w> of evil spirits lasted all night."
                            }
                        }
                    ],
                    "synonyms": [
                        "выдворение",
                        "высылка"
                    ],
                    "antonyms": [
                        "приглашение",
                        "водворение"
                    ],
                    "common_phrases": [
                        "изгнание из страны",
                        "изгнание из рая",
                        "изгнание бесов",
                        "угроза изгнания"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of expelling; the forced removal of someone from a country, city, dwelling, or community."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expulsion",
                                "target_lang_sense_clarification": "general act of forcing someone out"
                            },
                            {
                                "target_lang_word": "banishment",
                                "target_lang_sense_clarification": "punitive removal from a country or place"
                            },
                            {
                                "target_lang_word": "eviction",
                                "target_lang_sense_clarification": "removal from a dwelling or property"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab94f481-2acd-3987-8f3e-0a8943caeb8b",
                    "sense_definition": "Пребывание в чужой стране или отдалённой местности в результате принудительного выселения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Знаменитый писатель долгие годы жил в <w>изгнании</w>, тоскуя по родине.",
                            "target_lang_translations": {
                                "en": "The famous writer lived in <w>exile</w> for many years, longing for his homeland."
                            }
                        },
                        {
                            "text": "Даже в <w>изгнании</w> он не переставал думать о судьбе своего народа.",
                            "target_lang_translations": {
                                "en": "Even in <w>exile</w>, he did not stop thinking about the fate of his people."
                            }
                        }
                    ],
                    "synonyms": [
                        "ссылка"
                    ],
                    "common_phrases": [
                        "жить в изгнании",
                        "годы изгнания",
                        "умереть в изгнании",
                        "добровольное изгнание"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of living in a foreign country or remote area as a result of forced removal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exile",
                                "target_lang_sense_clarification": "state of being barred from one's native country"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
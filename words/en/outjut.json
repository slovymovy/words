{
    "word_family": [
        "jut"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2ef0442d-5a8d-306a-8759-0f8c85fad69d",
                    "sense_definition": "To extend or stick out beyond the surrounding surface or line.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The jagged rocks <w>outjut</w> from the cliff face, making the climb difficult.",
                            "target_lang_translations": {
                                "ru": "Острые камни <w>выступают</w> из поверхности скалы, затрудняя подъем."
                            }
                        },
                        {
                            "text": "Several old balconies <w>outjut</w> over the narrow street below.",
                            "target_lang_translations": {
                                "ru": "Несколько старых балконов <w>выдаются</w> над узкой улицей внизу."
                            }
                        }
                    ],
                    "synonyms": [
                        "protrude",
                        "project",
                        "stick out",
                        "extend",
                        "overhang"
                    ],
                    "antonyms": [
                        "recede"
                    ],
                    "common_phrases": [
                        "outjut from the wall",
                        "outjut over the water"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступать или выдаваться вперед за пределы основной линии или поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выступать",
                                "target_lang_sense_clarification": "выходить за пределы поверхности"
                            },
                            {
                                "target_lang_word": "выдаваться",
                                "target_lang_sense_clarification": "заметно выступать вперед"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4bc17246-0733-3b2d-90b5-2858b6b1cac3",
                    "sense_definition": "A part of an object or structure that extends outward; a protrusion.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He stood on a small <w>outjut</w> of rock to get a better view of the valley.",
                            "target_lang_translations": {
                                "ru": "Он стоял на небольшом скалистом <w>выступе</w>, чтобы лучше рассмотреть долину."
                            }
                        },
                        {
                            "text": "The building features a decorative <w>outjut</w> above the main entrance.",
                            "target_lang_translations": {
                                "ru": "Здание имеет декоративный <w>выступ</w> над главным входом."
                            }
                        }
                    ],
                    "synonyms": [
                        "protrusion",
                        "projection",
                        "prominence",
                        "overhang"
                    ],
                    "antonyms": [
                        "indentation",
                        "recess",
                        "depression"
                    ],
                    "common_phrases": [
                        "a rocky outjut",
                        "a sharp outjut"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть объекта или сооружения, выступающая наружу; выступ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выступ",
                                "target_lang_sense_clarification": "выступающая часть чего-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
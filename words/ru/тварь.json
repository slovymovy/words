    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4be15c40-8bb8-327e-87cf-012c5a1940b6",
                    "sense_definition": "Мерзкий, подлый, недостойный человек (используется как грубое ругательство).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта <w>тварь</w> украла все наши сбережения и сбежала.",
                            "target_lang_translations": {
                                "en": "That <w>scum</w> stole all our savings and ran away."
                            }
                        },
                        {
                            "text": "Я не хочу больше видеть эту неблагодарную <w>тварь</w> в своём доме.",
                            "target_lang_translations": {
                                "en": "I don't want to see that ungrateful <w>wretch</w> in my house anymore."
                            }
                        }
                    ],
                    "synonyms": [
                        "мразь",
                        "скотина",
                        "гад",
                        "тварина",
                        "тварюка"
                    ],
                    "antonyms": [
                        "ангел",
                        "джентльмен"
                    ],
                    "common_phrases": [
                        "продажная тварь",
                        "неблагодарная тварь",
                        "редкостная тварь",
                        "подлая тварь"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "бранное, грубое"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A vile, mean, or unworthy person (used as a coarse insult)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scum",
                                "target_lang_sense_clarification": "highly offensive, referring to a despicable person"
                            },
                            {
                                "target_lang_word": "wretch",
                                "target_lang_sense_clarification": "a despicable or contemptible person"
                            },
                            {
                                "target_lang_word": "creature",
                                "target_lang_sense_clarification": "derogatory, implying subhuman nature"
                            },
                            {
                                "target_lang_word": "beast",
                                "target_lang_sense_clarification": "cruel or repulsive person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bad3c184-3c63-3b92-bf77-f48a44e8027b",
                    "sense_definition": "Любое живое существо, животное или человек (часто в религиозном или книжном контексте).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "creature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждая божья <w>тварь</w> имеет право на жизнь под солнцем.",
                            "target_lang_translations": {
                                "en": "Every God's <w>creature</w> has the right to life under the sun."
                            }
                        },
                        {
                            "text": "В тёмном лесу затаилась какая-то неведомая и страшная <w>тварь</w>.",
                            "target_lang_translations": {
                                "en": "Some unknown and terrible <w>creature</w> was lurking in the dark forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "создание",
                        "животное",
                        "животина"
                    ],
                    "antonyms": [
                        "бог"
                    ],
                    "common_phrases": [
                        "божья тварь",
                        "всякой твари по паре",
                        "бессловесная тварь",
                        "тварь дрожащая"
                    ],
                    "target_lang_definitions": {
                        "en": "Any living being, animal, or person (often in a religious or literary context)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "creature",
                                "target_lang_sense_clarification": "a living being, especially an animal"
                            },
                            {
                                "target_lang_word": "beast",
                                "target_lang_sense_clarification": "a large or dangerous animal"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1ced5d88-098e-3502-8d06-75268205f1f3",
                    "sense_definition": "Przypisywać komuś odpowiedzialność za coś złego lub uważać kogoś za sprawcę niepowodzenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "blame_accusation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek niesłusznie <winił> sąsiada za zniszczenie płotu.",
                            "target_lang_translations": {
                                "ru": "Марек несправедливо <винил> соседа за сломанный забор."
                            }
                        },
                        {
                            "text": "Nie możesz <winić> mnie za wszystkie swoje problemy w pracy.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь <винить> меня за все свои проблемы на работе."
                            }
                        },
                        {
                            "text": "Ona zawsze <wini> pogodę, gdy spóźnia się na spotkanie.",
                            "target_lang_translations": {
                                "ru": "Она всегда <винит> погоду, когда опаздывает на встречу."
                            }
                        }
                    ],
                    "synonyms": [
                        "obwiniać",
                        "oskarżać"
                    ],
                    "antonyms": [
                        "usprawiedliwiać",
                        "bronić"
                    ],
                    "common_phrases": [
                        "winić kogoś za coś",
                        "niesłusznie winić",
                        "winić los",
                        "winić innych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приписывать кому-либо ответственность за что-то плохое или считать кого-то виновником неудачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "винить",
                                "target_lang_sense_clarification": "считать виноватым"
                            },
                            {
                                "target_lang_word": "обвинять",
                                "target_lang_sense_clarification": "возлагать вину"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e54e687e-b383-3516-a8a1-62b8adb40365",
                    "sense_definition": "Uważać samego siebie za winnego; mieć do siebie żal o popełnione błędy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "blame_accusation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestań się <winić>, to był tylko nieszczęśliwy wypadek.",
                            "target_lang_translations": {
                                "ru": "Перестань <винить> себя, это был просто несчастный случай."
                            }
                        },
                        {
                            "text": "Chłopiec <winił> się za to, że zgubił ulubioną zabawkę siostry.",
                            "target_lang_translations": {
                                "ru": "Мальчик <винил> себя за то, что потерял любимую игрушку сестры."
                            }
                        }
                    ],
                    "synonyms": [
                        "obwiniać się"
                    ],
                    "antonyms": [
                        "usprawiedliwiać się"
                    ],
                    "common_phrases": [
                        "winić się za błędy",
                        "niepotrzebnie się winić",
                        "winić się w duchu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать самого себя виновным; сожалеть о совершенных ошибках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "винить",
                                "target_lang_sense_clarification": "себя"
                            },
                            {
                                "target_lang_word": "корить",
                                "target_lang_sense_clarification": "упрекать себя"
                            }
                        ]
                    }
                }
            ]
        }
    ]

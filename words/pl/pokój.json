            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d93d75b3-ae22-3c56-aad8-89c3a25c176a",
                    "sense_definition": "Wydzielone ścianami pomieszczenie w mieszkaniu, domu lub innym budynku, przeznaczone do celów mieszkalnych lub użytkowych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "space_room",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat ma bardzo duży i jasny <w>pokój</w> na pierwszym piętrze.",
                            "target_lang_translations": {
                                "ru": "У моего брата очень большая и светлая <w>комната</w> на втором этаже."
                            }
                        },
                        {
                            "text": "Muszę posprzątać swój <w>pokój</w>, zanim przyjdą do nas goście.",
                            "target_lang_translations": {
                                "ru": "Я должен убрать свою <w>комнату</w>, прежде чем к нам придут гости."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomieszczenie",
                        "izba",
                        "kąt"
                    ],
                    "common_phrases": [
                        "duży pokój",
                        "wynająć pokój",
                        "sprzątać pokój",
                        "pokój gościnny",
                        "numer pokoju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отделенное стенами помещение в квартире, доме или другом здании, предназначенное для жилых или хозяйственных целей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комната",
                                "target_lang_sense_clarification": "помещение в жилом доме"
                            },
                            {
                                "target_lang_word": "помещение",
                                "target_lang_sense_clarification": "пространство внутри здания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02dbf646-e01b-388d-b51d-613dfa24b808",
                    "sense_definition": "Stan stosunków między państwami lub narodami, w którym nie toczą się działania wojenne; brak wojny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "peace_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy ludzie na świecie marzą o trwałym i bezpiecznym <w>pokoju</w>.",
                            "target_lang_translations": {
                                "ru": "Все люди на свете мечтают о прочном и безопасном <w>мире</w>."
                            }
                        },
                        {
                            "text": "Po wielu latach krwawej wojny wreszcie zapanował upragniony <w>pokój</w>.",
                            "target_lang_translations": {
                                "ru": "После многих лет кровавой войны наконец воцарился долгожданный <w>мир</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozejm",
                        "zawieszenie broni",
                        "zgoda",
                        "mir"
                    ],
                    "antonyms": [
                        "wojna"
                    ],
                    "common_phrases": [
                        "pokój na świecie",
                        "żyć w pokoju",
                        "utrzymać pokój",
                        "nagroda pokojowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние отношений между государствами или народами, при котором не ведутся военные действия; отсутствие войны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мир",
                                "target_lang_sense_clarification": "отсутствие войны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "703db16e-22de-3ef0-a8b8-ada598eda925",
                    "sense_definition": "Stan wewnętrznej równowagi psychicznej, brak trosk, lęku lub negatywnych emocji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "peace_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Medytacja pozwala wielu ludziom odzyskać wewnętrzny <w>pokój</w> i harmonię.",
                            "target_lang_translations": {
                                "ru": "Медитация позволяет многим людям обрести внутренний <w>покой</w> и гармонию."
                            }
                        },
                        {
                            "text": "Życzę ci <w>pokoju</w> ducha w tych trudnych dla ciebie chwilach.",
                            "target_lang_translations": {
                                "ru": "Желаю тебе душевного <w>покоя</w> в эти трудные для тебя минуты."
                            }
                        }
                    ],
                    "synonyms": [
                        "spokój",
                        "harmonia",
                        "cisza"
                    ],
                    "antonyms": [
                        "niepokój",
                        "dysharmonia"
                    ],
                    "common_phrases": [
                        "pokój ducha",
                        "wewnętrzny pokój",
                        "pokój serca"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often literary or religious context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние внутреннего психического равновесия, отсутствие тревог, страха или негативных эмоций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покой",
                                "target_lang_sense_clarification": "душевное равновесие"
                            },
                            {
                                "target_lang_word": "спокойствие",
                                "target_lang_sense_clarification": "отсутствие волнения"
                            },
                            {
                                "target_lang_word": "умиротворение",
                                "target_lang_sense_clarification": "глубокое спокойствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96c9ab00-8eae-3f46-981c-f6f1a9bc3b42",
                    "sense_definition": "Oficjalne porozumienie lub traktat kończący wojnę między walczącymi stronami.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "peace_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prezydenci obu krajów spotkali się, aby uroczyście podpisać <w>pokój</w>.",
                            "target_lang_translations": {
                                "ru": "Президенты обеих стран встретились, чтобы торжественно подписать <w>мир</w>."
                            }
                        },
                        {
                            "text": "Ten historyczny <w>pokój</w> zmienił granice państw w Europie Środkowej.",
                            "target_lang_translations": {
                                "ru": "Этот исторический <w>мир</w> изменил границы государств в Центральной Европе."
                            }
                        }
                    ],
                    "synonyms": [
                        "traktat pokojowy",
                        "pakt pokojowy"
                    ],
                    "common_phrases": [
                        "podpisać pokój",
                        "zawrzeć pokój",
                        "warunki pokoju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное соглашение или договор, завершающий войну между воюющими сторонами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мир",
                                "target_lang_sense_clarification": "мирный договор"
                            },
                            {
                                "target_lang_word": "мирный договор",
                                "target_lang_sense_clarification": "документ о мире"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7feb8b4-a3d6-323a-9609-391a1fadfc51",
                    "sense_definition": "W kontekście religijnym: stan wiecznego szczęścia i zbawienia po śmierci.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "peace_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzina modliła się o wieczny <w>pokój</w> dla duszy zmarłego dziadka.",
                            "target_lang_translations": {
                                "ru": "Семья молилась о вечном <w>покое</w> для души умершего дедушки."
                            }
                        },
                        {
                            "text": "Napis na nagrobku głosił: spoczywaj w <w>pokoju</w>.",
                            "target_lang_translations": {
                                "ru": "Надпись на надгробии гласила: покойся с <w>миром</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpoczynek"
                    ],
                    "common_phrases": [
                        "wieczny pokój",
                        "spoczywaj w pokoju"
                    ],
                    "target_lang_definitions": {
                        "ru": "В религиозном контексте: состояние вечного счастья и спасения после смерти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покой",
                                "target_lang_sense_clarification": "вечный отдых"
                            },
                            {
                                "target_lang_word": "упокоение",
                                "target_lang_sense_clarification": "церковный термин"
                            }
                        ]
                    }
                }
            ]

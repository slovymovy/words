            "pos": "verb",
            "senses": [
                {
                    "sense_id": "66539744-3f95-3c6e-9ce6-145cb3c3805a",
                    "sense_definition": "Внезапно появиться, охватить с силой (о чувствах, мыслях, физическом состоянии).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "feelings_states",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вечером на него <накатила> странная грусть, и он не захотел никуда идти.",
                            "target_lang_translations": {
                                "en": "In the evening, a strange sadness <came over> him, and he didn't want to go anywhere."
                            }
                        },
                        {
                            "text": "Внезапно на меня <накатила> усталость, и глаза начали слипаться.",
                            "target_lang_translations": {
                                "en": "Suddenly, fatigue <washed over> me, and my eyes began to close."
                            }
                        }
                    ],
                    "synonyms": [
                        "нахлынуть",
                        "овладеть",
                        "подступить"
                    ],
                    "antonyms": [
                        "отпустить",
                        "пройти"
                    ],
                    "common_phrases": [
                        "накатила грусть",
                        "накатила усталость",
                        "слёзы накатили",
                        "накатило вдохновение"
                    ],
                    "target_lang_definitions": {
                        "en": "To suddenly overwhelm or come over someone (of feelings, thoughts, or physical states)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come over",
                                "target_lang_sense_clarification": "general term for feelings affecting someone"
                            },
                            {
                                "target_lang_word": "wash over",
                                "target_lang_sense_clarification": "implies a wave-like feeling covering someone"
                            },
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "sudden and forceful impact of a feeling"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f3bb21c-d2fb-33e9-93df-b684411b9c30",
                    "sense_definition": "Приблизиться, надвинуться массой (о волнах, тумане, облаках).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_motion_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Огромная волна <накатила> на берег и смыла наши вещи.",
                            "target_lang_translations": {
                                "en": "A huge wave <rolled onto> the shore and washed away our things."
                            }
                        },
                        {
                            "text": "С моря <накатил> густой туман, скрыв очертания кораблей.",
                            "target_lang_translations": {
                                "en": "A thick fog <rolled in> from the sea, hiding the outlines of the ships."
                            }
                        }
                    ],
                    "synonyms": [
                        "набежать",
                        "надвинуться"
                    ],
                    "antonyms": [
                        "отхлынуть",
                        "отступить"
                    ],
                    "common_phrases": [
                        "волна накатила",
                        "туман накатил"
                    ],
                    "target_lang_definitions": {
                        "en": "To move forward or onto something in a mass (of waves, fog, clouds)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "roll in",
                                "target_lang_sense_clarification": "for fog, clouds, or weather"
                            },
                            {
                                "target_lang_word": "roll onto",
                                "target_lang_sense_clarification": "for waves moving onto a shore"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23a4276e-ee88-36a8-aaba-9c44670abffa",
                    "sense_definition": "Выпить спиртного (обычно водки или другого крепкого напитка).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "drinking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После тяжёлой рабочей недели они решили <накатить> по сто грамм.",
                            "target_lang_translations": {
                                "en": "After a hard work week, they decided to <knock back> a hundred grams."
                            }
                        },
                        {
                            "text": "Давай <накатим> за встречу, мы так давно не виделись!",
                            "target_lang_translations": {
                                "en": "Let's <have a drink> to our meeting, we haven't seen each other for so long!"
                            }
                        }
                    ],
                    "synonyms": [
                        "выпить",
                        "дёрнуть"
                    ],
                    "common_phrases": [
                        "накатить по сто",
                        "накатить за встречу",
                        "накатить рюмку"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To drink alcohol, usually a shot of strong liquor (slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "knock back",
                                "target_lang_sense_clarification": "idiomatic, implies drinking quickly"
                            },
                            {
                                "target_lang_word": "down",
                                "target_lang_sense_clarification": "to drink the contents of a glass"
                            },
                            {
                                "target_lang_word": "have a drink",
                                "target_lang_sense_clarification": "general context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afb69a3f-33a6-3d7b-9107-3bfaf0fc68f0",
                    "sense_definition": "Переместить что-либо качением на какое-либо место или поверхность.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие помогли <накатить> тяжёлую бочку на платформу грузовика.",
                            "target_lang_translations": {
                                "en": "The workers helped <roll> the heavy barrel onto the truck platform."
                            }
                        },
                        {
                            "text": "Нужно аккуратно <накатить> колесо на ось, чтобы закрепить его.",
                            "target_lang_translations": {
                                "en": "You need to carefully <roll> the wheel onto the axle to secure it."
                            }
                        }
                    ],
                    "synonyms": [
                        "закатить",
                        "надвинуть"
                    ],
                    "antonyms": [
                        "скатить",
                        "снять"
                    ],
                    "common_phrases": [
                        "накатить бочку",
                        "накатить колесо"
                    ],
                    "target_lang_definitions": {
                        "en": "To move an object by rolling it onto a specific place or surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "roll onto",
                                "target_lang_sense_clarification": "moving an object onto a surface"
                            },
                            {
                                "target_lang_word": "roll",
                                "target_lang_sense_clarification": "action of moving by turning"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a40425a4-0b51-3609-bd7e-53f638ccea53",
                    "sense_definition": "Установить обновление, патч или новую версию программного обеспечения поверх существующей.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Системный администратор планирует <накатить> обновления безопасности сегодня ночью.",
                            "target_lang_translations": {
                                "en": "The system administrator plans to <roll out> security updates tonight."
                            }
                        },
                        {
                            "text": "Если программа работает с ошибками, попробуй <накатить> последнюю версию драйверов.",
                            "target_lang_translations": {
                                "en": "If the program is buggy, try to <install> the latest version of the drivers."
                            }
                        }
                    ],
                    "synonyms": [
                        "установить",
                        "инсталлировать"
                    ],
                    "antonyms": [
                        "снести",
                        "удалить"
                    ],
                    "common_phrases": [
                        "накатить обновление",
                        "накатить патч",
                        "накатить винду"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "IT professional slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To install or apply a software update or patch (IT slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "roll out",
                                "target_lang_sense_clarification": "to deploy an update"
                            },
                            {
                                "target_lang_word": "install",
                                "target_lang_sense_clarification": "standard term"
                            },
                            {
                                "target_lang_word": "apply",
                                "target_lang_sense_clarification": "often used with patches"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e50a69a-32da-33eb-9a27-b8a4d0d4ed8d",
                    "sense_definition": "Неожиданно приехать или прийти куда-либо (часто группой).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "movement_arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы не ждали гостей, но друзья решили <накатить> к нам на дачу без звонка.",
                            "target_lang_translations": {
                                "en": "We weren't expecting guests, but friends decided to <roll up> to our dacha without calling."
                            }
                        },
                        {
                            "text": "В любой момент может <накатить> проверка, поэтому документы должны быть в порядке.",
                            "target_lang_translations": {
                                "en": "An inspection might <descend upon> us at any moment, so the documents must be in order."
                            }
                        }
                    ],
                    "synonyms": [

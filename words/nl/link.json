{
    "word_family": [
        "linken",
        "linkheid"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e3430ce-9030-3853-a287-1c951fa49351",
                    "sense_definition": "Een verwijzing op een computer of internetpagina die de gebruiker direct naar een ander document of een andere plaats leidt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "connection_digital",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klik op de <w>link</w> in de e-mail om je wachtwoord te wijzigen.",
                            "target_lang_translations": {
                                "en": "Click on the <w>link</w> in the email to change your password.",
                                "ru": "Нажмите на <w>ссылку</w> в электронном письме, чтобы изменить пароль."
                            }
                        },
                        {
                            "text": "De leraar stuurde een <w>link</w> door naar het interessante artikel.",
                            "target_lang_translations": {
                                "en": "The teacher forwarded a <w>link</w> to the interesting article.",
                                "ru": "Учитель переслал <w>ссылку</w> на интересную статью."
                            }
                        }
                    ],
                    "synonyms": [
                        "koppeling",
                        "verwijzing"
                    ],
                    "common_phrases": [
                        "op een link klikken",
                        "een link delen",
                        "de link volgen"
                    ],
                    "target_lang_definitions": {
                        "en": "A reference on a computer or webpage that leads the user directly to another document or place.",
                        "ru": "Ссылка на компьютере или веб-странице, которая направляет пользователя непосредственно к другому документу или месту."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "link",
                                "target_lang_sense_clarification": "digital reference"
                            },
                            {
                                "target_lang_word": "hyperlink",
                                "target_lang_sense_clarification": "technical term"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ссылка",
                                "target_lang_sense_clarification": "в интернете или документе"
                            },
                            {
                                "target_lang_word": "линк",
                                "target_lang_sense_clarification": "компьютерный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ab70579-81c6-32d1-aafd-cd1f59745632",
                    "sense_definition": "Een logisch verband of relatie tussen twee zaken, gebeurtenissen of personen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "connection_logical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie onderzoekt de <w>link</w> tussen de twee inbraken in de buurt.",
                            "target_lang_translations": {
                                "en": "The police are investigating the <w>link</w> between the two burglaries in the neighborhood.",
                                "ru": "Полиция расследует <w>связь</w> между двумя кражами со взломом в этом районе."
                            }
                        },
                        {
                            "text": "Kun jij de <w>link</w> leggen tussen deze twee historische gebeurtenissen?",
                            "target_lang_translations": {
                                "en": "Can you make the <w>connection</w> between these two historical events?",
                                "ru": "Ты можешь найти <w>связь</w> между этими двумя историческими событиями?"
                            }
                        }
                    ],
                    "synonyms": [
                        "verband"
                    ],
                    "common_phrases": [
                        "de link leggen",
                        "een link hebben met",
                        "de link zien"
                    ],
                    "target_lang_definitions": {
                        "en": "A logical connection or relationship between two things, events, or people.",
                        "ru": "Логическая связь или отношение между двумя вещами, событиями или людьми."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "link",
                                "target_lang_sense_clarification": "connection/relation"
                            },
                            {
                                "target_lang_word": "connection",
                                "target_lang_sense_clarification": "relationship"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "логическая или причинно-следственная"
                            },
                            {
                                "target_lang_word": "взаимосвязь",
                                "target_lang_sense_clarification": "взаимное отношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c2ea222-34e9-37a6-89a4-4f83207bdc6c",
                    "sense_definition": "Een fysieke verbinding, bijvoorbeeld met een kabel tussen apparaten.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "connection_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>link</w> tussen de computer en de printer is verbroken.",
                            "target_lang_translations": {
                                "en": "The <w>connection</w> between the computer and the printer is lost.",
                                "ru": "<w>Соединение</w> между компьютером и принтером прервано."
                            }
                        },
                        {
                            "text": "Controleer of de kabel voor de <w>link</w> goed is aangesloten.",
                            "target_lang_translations": {
                                "en": "Check if the cable for the <w>link</w> is connected properly.",
                                "ru": "Проверьте, хорошо ли подключен кабель для <w>соединения</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbinding"
                    ],
                    "target_lang_definitions": {
                        "en": "A physical connection, for example with a cable between devices.",
                        "ru": "Физическое соединение, например, кабелем между устройствами."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "connection",
                                "target_lang_sense_clarification": "physical bond"
                            },
                            {
                                "target_lang_word": "link",
                                "target_lang_sense_clarification": "system connection"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "соединение",
                                "target_lang_sense_clarification": "физическое подключение"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "канал передачи данных"
                            }
                        ]
                    }
                }
            ]
        },
        {

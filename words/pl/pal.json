                    "sense_id": "9fa07395-54be-3ffe-bd06-58379eb637c3",
                    "sense_definition": "Forma trybu rozkazującego (2. os. lp) czasownika <w>palić</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <pal> papierosów w tym pomieszczeniu.",
                            "target_lang_translations": {
                                "ru": "Не <кури> сигареты в этом помещении."
                            }
                        },
                        {
                            "text": "<Pal> w piecu, bo robi się coraz zimniej.",
                            "target_lang_translations": {
                                "ru": "<Топи> печь, потому что становится всё холоднее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pal licho",
                        "pal sześć"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма повелительного наклонения (2-е лицо, ед. ч.) глагола 'palić' (курить, топить, жечь)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кури",
                                "target_lang_sense_clarification": "о сигаретах"
                            },
                            {
                                "target_lang_word": "топи",
                                "target_lang_sense_clarification": "о печи"
                            },
                            {

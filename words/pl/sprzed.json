    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "96da5899-0a98-3a6e-99f4-8114fa506592",
                    "sense_definition": "Wskazuje, jak dawno temu coś miało miejsce lub skąd pochodzi, określając ilość minionego czasu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znalazłem w szufladzie stary rachunek <w>sprzed</w> roku.",
                            "target_lang_translations": {
                                "ru": "Я нашел в ящике старый счет <w>от</w> прошлого года."
                            }
                        },
                        {
                            "text": "To jest bardzo ważna wiadomość <w>sprzed</w> chwili.",
                            "target_lang_translations": {
                                "ru": "Это очень важная новость, полученная минуту <w>назад</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sprzed chwili",
                        "sprzed roku",
                        "sprzed tygodnia",
                        "sprzed lat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на давность события или время происхождения чего-либо (как давно это было)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "указывает на дату или время происхождения (напр. письмо от вчера)"
                            },
                            {
                                "target_lang_word": "... давности",
                                "target_lang_sense_clarification": "в конструкции, указывающей возраст (напр. годичной давности)"
                            },
                            {
                                "target_lang_word": "назад",
                                "target_lang_sense_clarification": "при указании времени, прошедшего с момента события"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36416d1e-8740-3878-9ad4-d7da66464248",
                    "sense_definition": "Oznacza ruch z miejsca znajdującego się przed czymś lub usunięcie czegoś z tego miejsca.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Taksówka właśnie odjechała <w>sprzed</w> wejścia do hotelu.",
                            "target_lang_translations": {
                                "ru": "Такси только что отъехало <w>от</w> входа в отель."
                            }
                        },
                        {
                            "text": "Sprzątnij te brudne buty <w>sprzed</w> drzwi.",
                            "target_lang_translations": {
                                "ru": "Убери эти грязные ботинки <w>от</w> двери."
                            }
                        }
                    ],
                    "antonyms": [
                        "zza"
                    ],
                    "common_phrases": [
                        "sprzed domu",
                        "sprzed nosa",
                        "sprzed oczu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает движение от места, находящегося перед чем-либо, или удаление объекта с этого места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "движение прочь от объекта"
                            },
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "удаление с поверхности или в устойчивых выражениях (напр. с глаз)"
                            },
                            {
                                "target_lang_word": "из-под",
                                "target_lang_sense_clarification": "в идиомах (напр. из-под носа)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58ef2e5e-0690-3dfe-ab2f-f90dac322fc4",
                    "sense_definition": "Wskazuje na pochodzenie z okresu wcześniejszego niż określone wydarzenie historyczne lub moment w życiu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek wciąż przechowuje pamiątki <w>sprzed</w> wojny.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка все еще хранит памятные вещи <w>довоенных</w> времен."
                            }
                        },
                        {
                            "text": "To są stare tradycje <w>sprzed</w> rewolucji przemysłowej.",
                            "target_lang_translations": {
                                "ru": "Это старые традиции <w>времен до</w> промышленной революции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sprzed wojny",
                        "sprzed wieków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на происхождение из периода, предшествующего определенному историческому событию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "времен до",
                                "target_lang_sense_clarification": "относящийся к периоду до чего-либо"
                            },
                            {
                                "target_lang_word": "до...",
                                "target_lang_sense_clarification": "как приставка в прилагательных (напр. довоенный)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

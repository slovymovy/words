    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "56e1828f-cb93-3c52-aa53-b4ff28c2b743",
                    "sense_definition": "Duży, często obwisły lub wystający brzuch.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "belly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po sytym obiedzie wujek rozpiął pasek i poklepał się po <w>bebechu</w>.",
                            "target_lang_translations": {
                                "ru": "После сытного обеда дядя расстегнул ремень и похлопал себя по <w>пузу</w>."
                            }
                        },
                        {
                            "text": "Muszę zacząć ćwiczyć, bo urósł mi straszny <w>bebech</w> od siedzenia przed telewizorem.",
                            "target_lang_translations": {
                                "ru": "Мне нужно начать тренироваться, а то у меня выросло ужасное <w>брюхо</w> от сидения перед телевизором."
                            }
                        }
                    ],
                    "synonyms": [
                        "brzuch",
                        "bandzioch",
                        "brzuszek"
                    ],
                    "common_phrases": [
                        "wywalić bebech",
                        "wciągnąć bebech"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой, часто отвисший или выступающий живот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пузо",
                                "target_lang_sense_clarification": "разговорное, большой живот"
                            },
                            {
                                "target_lang_word": "брюхо",
                                "target_lang_sense_clarification": "разговорное, часто грубое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65a59af8-7422-36a7-8677-98903c6c834e",
                    "sense_definition": "Narządy wewnętrzne organizmu, wnętrzności (zazwyczaj używane w liczbie mnogiej).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "insides",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na widok krwi i <w>bebechów</w> w horrorze musiałem zamknąć oczy.",
                            "target_lang_translations": {
                                "ru": "При виде крови и <w>кишок</w> в фильме ужасов мне пришлось закрыть глаза."
                            }
                        },
                        {
                            "text": "Wilki rozszarpały ofiarę i wyjadły jej <w>bebechy</w>.",
                            "target_lang_translations": {
                                "ru": "Волки растерзали жертву и съели её <w>потроха</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wnętrzność",
                        "trzewia",
                        "flak"
                    ],
                    "common_phrases": [
                        "wypruwać sobie bebechy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренние органы, внутренности (обычно во множественном числе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потроха",
                                "target_lang_sense_clarification": "внутренности животного или человека"
                            },
                            {
                                "target_lang_word": "кишки",
                                "target_lang_sense_clarification": "кишечник, внутренности"
                            },
                            {
                                "target_lang_word": "внутренности",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e8fa28e-0dd6-3b33-b94b-761da53c67ce",
                    "sense_definition": "Wewnętrzne części urządzenia, mechanizmu lub komputera.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "insides",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Informatyk rozkręcił obudowę laptopa, żeby wyczyścić jego zakurzone <w>bebechy</w>.",
                            "target_lang_translations": {
                                "ru": "Компьютерщик раскрутил корпус ноутбука, чтобы почистить его запылившуюся <w>начинку</w>."
                            }
                        },
                        {
                            "text": "Ten samochód wygląda staro, ale ma nowoczesne <w>bebechy</w> pod maską.",
                            "target_lang_translations": {
                                "ru": "Эта машина выглядит старой, но у неё современная <w>начинка</w> под капотом."
                            }
                        }
                    ],
                    "synonyms": [
                        "wnętrzność"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренние детали устройства, механизма или компьютера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начинка",
                                "target_lang_sense_clarification": "внутреннее устройство техники"
                            },
                            {
                                "target_lang_word": "внутренности",
                                "target_lang_sense_clarification": "детали внутри корпуса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3620b19-6f58-337d-bf76-4ed0ac80b29f",
                    "sense_definition": "Stara, zniszczona pościel, pierzyna lub to, czym jest wypchana.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "bedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia kazała wyrzucić te stare <w>bebechy</w> na strych i kupić nowe kołdry.",
                            "target_lang_translations": {
                                "ru": "Бабушка велела выбросить эти старые <w>перины</w> на чердак и купить новые одеяла."
                            }
                        },
                        {
                            "text": "Spaliśmy pod ciężkimi <w>bebechami</w>, które pachniały kurzem.",
                            "target_lang_translations": {
                                "ru": "Мы спали под тяжелыми <w>перинами</w>, которые пахли пылью."
                            }
                        }
                    ],

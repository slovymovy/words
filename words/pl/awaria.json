                    "sense_definition": "Niespodziewane uszkodzenie maszyny, urządzenia lub systemu technicznego, które zakłóca lub uniemożliwia ich poprawne działanie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z powodu poważnej <w>awarii</w> silnika musieliśmy wezwać lawetę na autostradę.",
                            "target_lang_translations": {
                                "ru": "Из-за серьезной <w>поломки</w> двигателя нам пришлось вызвать эвакуатор на автостраду."
                            }
                        },
                        {
                            "text": "Technicy pracują całą noc, aby jak najszybciej usunąć <w>awarię</w> sieci energetycznej.",
                            "target_lang_translations": {
                                "ru": "Техники работают всю ночь, чтобы как можно скорее устранить <w>аварию</w> в энергосети."
                            }
                        }
                    ],
                    "synonyms": [
                        "usterka",
                        "uszkodzenie",
                        "zepsucie"
                    ],
                    "common_phrases": [
                        "awaria prądu",
                        "awaria samochodu",
                        "usunąć awarię",
                        "zgłosić awarię"
                    ],
                    "target_lang_definitions": {

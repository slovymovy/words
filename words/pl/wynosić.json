    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dad0a167-6be3-3b16-b607-0da454110970",
                    "sense_definition": "Brać przedmiot i przemieszczać go na zewnątrz jakiegoś pomieszczenia, budynku lub obszaru.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat pomógł mi <wynieść> ciężką kanapę przed dom.",
                            "target_lang_translations": {
                                "ru": "Мой брат помог мне <вынести> тяжелый диван на улицу."
                            }
                        },
                        {
                            "text": "Pamiętaj, żeby wieczorem <wynieść> śmieci do kontenera.",
                            "target_lang_translations": {
                                "ru": "Помни, что вечером нужно <вынести> мусор в контейнер."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyprowadzać",
                        "usuwać"
                    ],
                    "antonyms": [
                        "wnosić"
                    ],
                    "common_phrases": [
                        "wynosić śmieci",
                        "wynosić meble",
                        "wynosić na zewnątrz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать предмет и перемещать его наружу из помещения или здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выноси́ть",
                                "target_lang_sense_clarification": "перемещать наружу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14a8cb8a-b358-31c4-833e-ffc80e33387b",
                    "sense_definition": "Osiągać określoną wartość liczbową, sumę lub wielkość (często o kosztach, wymiarach lub czasie).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "value_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Całkowity koszt naprawy samochodu <wyniósł> ponad dwa tysiące złotych.",
                            "target_lang_translations": {
                                "ru": "Общая стоимость ремонта автомобиля <составила> более двух тысяч злотых."
                            }
                        },
                        {
                            "text": "Temperatura w nocy będzie <wynosić> minus pięć stopni Celsjusza.",
                            "target_lang_translations": {
                                "ru": "Температура ночью будет <составлять> минус пять градусов Цельсия."
                            }
                        }
                    ],
                    "synonyms": [
                        "równać się",
                        "sięgać"
                    ],
                    "common_phrases": [
                        "wynosić łącznie",
                        "koszt wynosi",
                        "suma wynosi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достигать определенного числового значения, суммы или величины (часто о расходах, размерах или времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составля́ть",
                                "target_lang_sense_clarification": "о сумме или количестве"
                            },
                            {
                                "target_lang_word": "равня́ться",
                                "target_lang_sense_clarification": "быть равным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e9d2420-1b2e-3f63-b98c-3b2d58b9752e",
                    "sense_definition": "Zdobywać wiedzę, doświadczenie, nawyki lub wartości i zachowywać je w pamięci po opuszczeniu jakiegoś miejsca (np. szkoły, domu rodzinnego).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z lekcji historii uczniowie powinni <wynieść> wiedzę o błędach przeszłości.",
                            "target_lang_translations": {
                                "ru": "С уроков истории ученики должны <вынести> знания об ошибках прошлого."
                            }
                        },
                        {
                            "text": "Wszystkie dobre maniery <wyniosła> z domu rodzinnego.",
                            "target_lang_translations": {
                                "ru": "Все хорошие манеры она <вынесла> из родительского дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "czerpać",
                        "zdobywać",
                        "uzyskiwać"
                    ],
                    "common_phrases": [
                        "wynosić z domu",
                        "wynosić ze szkoły",
                        "wynosić naukę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Усваивать знания, опыт или привычки, покидая какое-либо место (например, школу или родительский дом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выноси́ть",
                                "target_lang_sense_clarification": "извлекать опыт или знания"
                            },
                            {
                                "target_lang_word": "извлека́ть",
                                "target_lang_sense_clarification": "получать пользу или урок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d3a12dd-a0ec-392e-bdfa-10b75135dfc9",
                    "sense_definition": "Opuszczać jakieś miejsce, wyprowadzać się lub odchodzić (używane z zaimkiem zwrotnym „się”); w trybie rozkazującym często oznacza żądanie, aby ktoś natychmiast wyszedł.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_leaving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiedzi postanowili <wynieść> się z głośnego centrum miasta na wieś.",
                            "target_lang_translations": {
                                "ru": "Соседи решили <переехать> из шумного центра города в деревню."
                            }
                        },
                        {
                            "text": "Jeśli nie przestaniesz krzyczeć, natychmiast <wynoś> się z mojego pokoju!",
                            "target_lang_translations": {
                                "ru": "Если не перестанешь кричать, немедленно <убирайся> из моей комнаты!"
                            }
                        }
                    ],
                    "synonyms": [
                        "wyprowadzać się",
                        "odchodzić",
                        "zmywać się"
                    ],
                    "antonyms": [
                        "wprowadzać się",
                        "zostawać"
                    ],
                    "common_phrases": [
                        "wynosić się z domu",
                        "wynoś się"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "especially when used as an imperative command to leave"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покидать место, переезжать или уходить (обычно с возвратным местоимением); в повелительном наклонении — требование уйти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убира́ться",
                                "target_lang_sense_clarification": "разг. уходить вон"
                            },
                            {
                                "target_lang_word": "переезжа́ть",
                                "target_lang_sense_clarification": "менять место жительства"
                            },
                            {
                                "target_lang_word": "выселя́ться",
                                "target_lang_sense_clarification": "покидать жилище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25c88e9b-37fa-3a09-a3ca-3d5873c390f5",
                    "sense_definition": "Kraść coś, zabierać cudzą własność ukradkiem (często z miejsca pracy).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stealing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zwolniono go dyscyplinarnie, ponieważ próbował <wynieść> narzędzia z zakładu pracy.",
                            "target_lang_translations": {
                                "ru": "Его уволили по статье, так как он пытался <вынести> инструменты с завода."
                            }
                        },
                        {
                            "text": "Nieuczciwi pracownicy potrafią <wynosić> towar małymi partiami przez wiele miesięcy.",
                            "target_lang_translations": {
                                "ru": "Нечестные сотрудники умудряются <выносить> товар небольшими партиями в течение многих месяцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "kraść",
                        "podkradać",
                        "zwijać"
                    ],
                    "common_phrases": [
                        "wynosić z zakładu",
                        "wynosić towar"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "euphemism for stealing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Красть что-либо, тайно вынося чужую собственность (часто с места работы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выноси́ть",
                                "target_lang_sense_clarification": "красть, вынося с предприятия"
                            },
                            {
                                "target_lang_word": "тащи́ть",
                                "target_lang_sense_clarification": "разг. воровать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8665eb6-82d2-310a-9dc9-0c698cdbdebf",
                    "sense_definition": "Transportować obiekt (np. rakietę, satelitę) w górę, w przestrzeń kosmiczną.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "space_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowoczesna rakieta nośna z powodzeniem <wyniosła> na orbitę satelitę telekomunikacyjnego.",
                            "target_lang_translations": {
                                "ru": "Современная ракета-носитель успешно <вывела> на орбиту телекоммуникационный спутник."
                            }
                        },
                        {
                            "text": "Silniki muszą mieć ogromną moc, aby <wynieść> statek kosmiczny poza atmosferę.",
                            "target_lang_translations": {
                                "ru": "Двигатели должны обладать огромной мощностью, чтобы <вывести> космический корабль за пределы атмосферы."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystrzeliwać"
                    ],
                    "common_phrases": [
                        "wynosić na orbitę",
                        "wynosić satelitę"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical/scientific context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортировать объект (например, ракету, спутник) вверх, в космическое пространство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выводи́ть",
                                "target_lang_sense_clarification": "на орбиту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bb3e9a6-29fa-3072-a4b5-de4ddb597918",
                    "sense_definition": "Chwalić kogoś, stawiać na wyższej pozycji, nadawać komuś lub czemuś wielkie znaczenie i godność.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "exaltation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krytycy w swoich recenzjach <wynosili> talent młodego pianisty pod niebiosa.",
                            "target_lang_translations": {
                                "ru": "Критики в своих рецензиях <возносили> талант молодого пианиста до небес."
                            }
                        },
                        {
                            "text": "Jego zasługi dla kraju <wyniosły> go na szczyty władzy politycznej.",
                            "target_lang_translations": {
                                "ru": "Его заслуги перед страной <вознесли> его на вершины политической власти."
                            }
                        }
                    ],
                    "synonyms": [
                        "wywyższać",
                        "wychwalać",
                        "awansować"
                    ],
                    "antonyms": [
                        "poniżać",
                        "degradować"
                    ],
                    "common_phrases": [
                        "wynosić na ołtarze",
                        "wynosić pod niebiosa",
                        "wynosić na tron"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "somewhat literary or formal style"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хвалить кого-либо, возвеличивать, придавать кому-либо или чему-либо большое значение и достоинство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возноси́ть",
                                "target_lang_sense_clarification": "восхвалять или возвышать"
                            },
                            {
                                "target_lang_word": "превозноси́ть",
                                "target_lang_sense_clarification": "очень сильно хвалить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
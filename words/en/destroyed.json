            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f294c79f-c977-34a4-a03e-e7fa4f2f58c2",
                    "sense_definition": "Damaged beyond repair; ruined; wrecked; obliterated.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "damage_ruin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>destroyed</w> city was slowly rebuilt over many years.",
                            "target_lang_translations": {
                                "ru": "<w>Разрушенный</w> город медленно восстанавливали в течение многих лет."
                            }
                        },
                        {
                            "text": "We looked at the <w>destroyed</w> car and wondered how anyone survived.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели на <w>разбитую</w> машину и удивлялись, как кто-то мог выжить."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruined",
                        "wrecked",
                        "demolished"
                    ],
                    "antonyms": [
                        "intact",
                        "preserved"
                    ],
                    "common_phrases": [
                        "totally destroyed",
                        "badly destroyed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поврежденный так, что не подлежит восстановлению; разрушенный; уничтоженный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрушенный",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "уничтоженный",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "разбитый",
                                "target_lang_sense_clarification": "о технике или транспорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b0eb4c4-9a3e-39b8-995f-f94aeee1a54b",

                },
                {
                    "sense_id": "74287c04-01f4-3ad8-83eb-b9ebb045e706",
                    "sense_definition": "The human genitals, usually referring to the male organ.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "anatomy_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The comedian made a crude joke about his <w>weapon</w> during the show.",
                            "target_lang_translations": {
                                "ru": "Комик отпустил грубую шутку про свой <w>ствол</w> во время шоу."
                            }
                        },
                        {
                            "text": "He was arrested for exposing his <w>weapon</w> in a public park.",
                            "target_lang_translations": {
                                "ru": "Его арестовали за то, что он оголил свое <w>хозяйство</w> в общественном парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "genitals",
                        "private parts",

                        "wybrać wewnętrzny"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane jako rzeczownik w mowie potocznej (skrót myślowy)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Телефонный номер, действующий внутри одной организации (сокращение от: внутренний номер)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добавочный номер",
                                "target_lang_sense_clarification": "телефон"
                            },
                            {
                                "target_lang_word": "внутренний номер",
                                "target_lang_sense_clarification": "телефон"
                            }
                        ]
                    }
                }
            ]
        }
    ]

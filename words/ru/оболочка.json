                            {
                                "target_lang_word": "shell",
                                "target_lang_sense_clarification": "hard outer layer or general structure"
                            },
                            {
                                "target_lang_word": "membrane",
                                "target_lang_sense_clarification": "thin pliable sheet, especially in biology"
                            },
                            {
                                "target_lang_word": "coating",
                                "target_lang_sense_clarification": "thin layer applied to a surface"
                            },
                            {
                                "target_lang_word": "envelope",
                                "target_lang_sense_clarification": "surrounding layer, often gaseous or abstract"

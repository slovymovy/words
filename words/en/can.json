                },
                {
                    "sense_id": "344eccc4-ed90-3aa4-a44a-862d88471f9e",
                    "sense_definition": "A prison or jail.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "prison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The thief spent five years in the <w>can</w> for robbery.",
                            "target_lang_translations": {
                                "ru": "Вор провел пять лет в <w>тюряге</w> за ограбление."

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f6e75a15-0f5a-3e69-b22a-902a96ae4988",
                    "sense_definition": "Having dark skin as a result of exposure to the sun.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "skin_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You look very <w>tan</w> after your holiday in Spain.",
                            "target_lang_translations": {
                                "ru": "Ты выглядишь очень <w>загорелым</w> после отпуска в Испании."
                            }
                        },
                        {
                            "text": "He is tall and <w>tan</w> from working outdoors all summer.",
                            "target_lang_translations": {
                                "ru": "Он высокий и <w>загорелый</w> от работы на улице всё лето."
                            }
                        }
                    ],
                    "synonyms": [
                        "tanned",
                        "suntanned"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий темную кожу в результате воздействия солнца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загорелый",
                                "target_lang_sense_clarification": "о коже или человеке"
                            },
                            {
                                "target_lang_word": "смуглый",
                                "target_lang_sense_clarification": "темный от загара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "059413fd-0d31-3979-ab62-cf846bf130cf",
                    "sense_definition": "Yellowish-brown in colour.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "color_brown",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a pair of <w>tan</w> boots that matched her bag.",
                            "target_lang_translations": {
                                "ru": "Она носила пару <w>светло-коричневых</w> ботинок, которые подходили к её сумке."
                            }
                        },
                        {
                            "text": "The walls of the living room were painted a soft <w>tan</w>.",
                            "target_lang_translations": {
                                "ru": "Стены гостиной были покрашены в мягкий <w>светло-коричневый цвет</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Желтовато-коричневого цвета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светло-коричневый",
                                "target_lang_sense_clarification": "о цвете"
                            },
                            {
                                "target_lang_word": "рыжевато-коричневый",
                                "target_lang_sense_clarification": "оттенок загара"
                            },
                            {
                                "target_lang_word": "желтовато-коричневый",
                                "target_lang_sense_clarification": "о цвете"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "13920b65-7317-362d-98f1-799d72369278",
                    "sense_definition": "A darkening of the skin resulting from exposure to sunlight or similar light sources.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "skin_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wants to get a <w>tan</w> before the summer ends.",
                            "target_lang_translations": {
                                "ru": "Она хочет получить <w>загар</w> до конца лета."
                            }
                        },
                        {
                            "text": "He came back from the beach with a deep <w>tan</w>.",
                            "target_lang_translations": {
                                "ru": "Он вернулся с пляжа с сильным <w>загаром</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get a tan",
                        "fake tan",
                        "suntan lotion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потемнение кожи в результате воздействия солнечного света или подобных источников света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загар",
                                "target_lang_sense_clarification": "результат пребывания на солнце"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32b32ade-6d90-362c-89ef-1795e1d98c4b",
                    "sense_definition": "A light, yellowish-brown colour.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "color_brown",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist used <w>tan</w> for the background of the portrait.",
                            "target_lang_translations": {
                                "ru": "Художник использовал <w>желтовато-коричневый цвет</w> для фона портрета."
                            }
                        },
                        {
                            "text": "Do you have these shoes in <w>tan</w> or black?",
                            "target_lang_translations": {
                                "ru": "У вас есть эти туфли в <w>рыжевато-коричневом цвете</w> или черном?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "in tan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Светлый, желтовато-коричневый цвет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "желтовато-коричневый цвет",
                                "target_lang_sense_clarification": "оттенок"
                            },
                            {
                                "target_lang_word": "рыжевато-коричневый цвет",
                                "target_lang_sense_clarification": "оттенок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b315d54d-a60b-386d-a4d7-11ef6d74cd8d",
                    "sense_definition": "The bark of an oak or other tree from which tannic acid is obtained for leather making.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "leather_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tannery ordered a fresh supply of oak <w>tan</w>.",
                            "target_lang_translations": {
                                "ru": "Кожевенный завод заказал свежий запас дубового <w>корья</w>."
                            }
                        },
                        {
                            "text": "Workers used the <w>tan</w> to process the raw hides.",
                            "target_lang_translations": {
                                "ru": "Рабочие использовали <w>дубильную кору</w> для обработки сырых шкур."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кора дуба или другого дерева, из которой получают дубильную кислоту для выделки кожи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дубильная кора",
                                "target_lang_sense_clarification": "материал"
                            },
                            {
                                "target_lang_word": "корьё",
                                "target_lang_sense_clarification": "профессиональный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2563bf12-1a1f-3289-b378-02f4ccd1aefe",
                    "sense_definition": "An Armenian drink made of yoghurt and water.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We enjoyed a cold glass of <w>tan</w> with our lunch.",
                            "target_lang_translations": {
                                "ru": "Мы насладились холодным стаканом <w>тана</w> за обедом."
                            }
                        },
                        {
                            "text": "She prepared fresh <w>tan</w> by mixing yoghurt, water, and salt.",
                            "target_lang_translations": {
                                "ru": "Она приготовила свежий <w>тан</w>, смешав йогурт, воду и соль."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Armenian"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Армянский напиток из йогурта (мацони) и воды."

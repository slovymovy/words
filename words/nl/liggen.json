{
    "word_family": [
        "ligging",
        "ligplaats",
        "ligstoel",
        "liggeld"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d14e859-9c6d-34b5-ac3c-a3a7e5c9a060",
                    "sense_definition": "Zich in een horizontale positie bevinden, meestal om te rusten of te slapen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "position_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben moe en ga even op de bank <w>liggen</w>.",
                            "target_lang_translations": {
                                "ru": "Я устал и пойду немного <w>полежу</w> на диване.",
                                "en": "I am tired and am going to <w>lie down</w> on the couch for a bit."
                            }
                        },
                        {
                            "text": "De hond <w>ligt</w> heerlijk te slapen in zijn mand.",
                            "target_lang_translations": {
                                "ru": "Собака <w>лежит</w> в своей корзине и сладко спит.",
                                "en": "The dog <w>is lying</w> sound asleep in his basket."
                            }
                        }
                    ],
                    "antonyms": [
                        "staan",
                        "zitten"
                    ],
                    "common_phrases": [
                        "in bed liggen",
                        "op de grond liggen",
                        "wakker liggen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в горизонтальном положении, обычно для отдыха или сна.",
                        "en": "To be in a horizontal position, usually to rest or sleep."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лежать",
                                "target_lang_sense_clarification": "находиться в горизонтальном положении"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "physical position"
                            },
                            {
                                "target_lang_word": "lie down",
                                "target_lang_sense_clarification": "action of reclining"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c0543c7-210e-39b1-9df8-eb5ec09388a8",
                    "sense_definition": "Zich op een bepaalde plaats bevinden (gebruikt voor geografische locaties en objecten).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "position_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn sleutels <w>liggen</w> nog op de keukentafel.",
                            "target_lang_translations": {
                                "ru": "Мои ключи все еще <w>лежат</w> на кухонном столе.",
                                "en": "My keys are still <w>lying</w> on the kitchen table."
                            }
                        },
                        {
                            "text": "Amsterdam <w>ligt</w> in het westen van Nederland.",
                            "target_lang_translations": {
                                "ru": "Амстердам <w>находится</w> на западе Нидерландов.",
                                "en": "Amsterdam <w>lies</w> in the west of the Netherlands."
                            }
                        }
                    ],
                    "synonyms": [
                        "zijn",
                        "zitten"
                    ],
                    "common_phrases": [
                        "waar ligt dat",
                        "ver weg liggen",
                        "naast elkaar liggen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в определенном месте (используется для географических объектов и предметов).",
                        "en": "To be situated in a certain place (used for geographical locations and objects)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лежать",
                                "target_lang_sense_clarification": "о предметах"
                            },
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "о географических объектах"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "location of objects"
                            },
                            {
                                "target_lang_word": "be situated",
                                "target_lang_sense_clarification": "geographical location"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "961732c2-98bd-3689-b1ce-a69bd5a03a32",
                    "sense_definition": "Hulpwerkwoord dat aangeeft dat iemand langdurig met iets bezig is (vaak in een liggende houding).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "grammar_aspect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>lag</w> de hele middag een boek te lezen.",
                            "target_lang_translations": {
                                "ru": "Он <w>лежал</w> и читал книгу весь день.",
                                "en": "He <w>lay</w> reading a book all afternoon."
                            }
                        },
                        {
                            "text": "Je biefstuk <w>ligt</w> daar koud te worden.",
                            "target_lang_translations": {
                                "ru": "Твой стейк <w>лежит</w> там и остывает.",
                                "en": "Your steak <w>is lying</w> there getting cold."
                            }
                        }
                    ],
                    "common_phrases": [
                        "liggen te slapen",
                        "liggen te dromen",
                        "liggen te wachten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вспомогательный глагол, указывающий на продолжительность действия (часто в лежачем положении).",
                        "en": "Auxiliary verb indicating that someone is busy with something for a long time (often in a lying position)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лежать",
                                "target_lang_sense_clarification": "в конструкции с другим действием"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "continuous action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46306f07-1e86-3ba0-84ed-2aa757c6bd93",
                    "sense_definition": "Bij iemand passen of door iemand leuk gevonden worden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_suit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat nieuwe werk <w>ligt</w> hem eigenlijk wel goed.",
                            "target_lang_translations": {
                                "ru": "Эта новая работа ему на самом деле <w>подходит</w>.",
                                "en": "That new job actually <w>suits</w> him quite well."
                            }
                        },
                        {
                            "text": "Piano spelen <w>ligt</w> me niet zo, ik zing liever.",
                            "target_lang_translations": {
                                "ru": "Игра на пианино мне не очень <w>подходит</w>, я предпочитаю петь.",
                                "en": "Playing the piano doesn't really <w>suit</w> me, I prefer singing."
                            }
                        }
                    ],
                    "synonyms": [
                        "bevallen"
                    ],
                    "common_phrases": [
                        "dat ligt mij wel",
                        "het ligt me niet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходить кому-либо или нравиться кому-либо.",
                        "en": "To suit someone or be liked by someone."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "соответствовать"
                            },
                            {
                                "target_lang_word": "быть по душе",
                                "target_lang_sense_clarification": "нравиться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "suit",
                                "target_lang_sense_clarification": "be suitable"
                            },
                            {
                                "target_lang_word": "appeal to",
                                "target_lang_sense_clarification": "be liked by"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
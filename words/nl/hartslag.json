    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8fc40e51-3107-39f3-bbf9-ace4382fad3b",
                    "sense_definition": "Het kloppen van het hart; een enkele samentrekking van de hartspier.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dokter luisterde met een stethoscoop naar mijn <w>hartslag</w>.",
                            "target_lang_translations": {
                                "en": "The doctor listened to my <w>heartbeat</w> with a stethoscope.",
                                "ru": "Врач слушал мое <w>сердцебиение</w> через стетоскоп."
                            }
                        },
                        {
                            "text": "Door de schrik sloeg zijn <w>hartslag</w> even over.",
                            "target_lang_translations": {
                                "en": "Due to the fright, his <w>heartbeat</w> skipped for a moment.",
                                "ru": "От испуга его <w>сердцебиение</w> на мгновение сбилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "polsslag"
                    ],
                    "common_phrases": [
                        "onregelmatige hartslag",
                        "snelle hartslag",
                        "hartslag voelen"
                    ],
                    "target_lang_definitions": {
                        "en": "The beating of the heart; a single contraction of the heart muscle.",
                        "ru": "Биение сердца; отдельное сокращение сердечной мышцы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heartbeat",
                                "target_lang_sense_clarification": "single pulsation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сердцебиение",
                                "target_lang_sense_clarification": "процесс биения сердца"
                            },
                            {
                                "target_lang_word": "стук сердца",
                                "target_lang_sense_clarification": "звук или ощущение удара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bb3afa6-ffc5-36a5-a610-44125055bc16",
                    "sense_definition": "Het ritme of de frequentie waarmee het hart klopt, vaak uitgedrukt in slagen per minuut.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oscar voelde zijn <w>hartslag</w> toenemen, net alsof hij op de uitkijk zat.",
                            "target_lang_translations": {
                                "en": "Oscar felt his <w>heartbeat</w> increase, just as if he were on the lookout.",
                                "ru": "Оскар почувствовал, как участился его <w>пульс</w>, словно он сидел в засаде."
                            }
                        },
                        {
                            "text": "Tijdens intensieve sportoefeningen gaat je <w>hartslag</w> flink omhoog.",
                            "target_lang_translations": {
                                "en": "During intensive sports exercises, your <w>heart rate</w> goes up significantly.",
                                "ru": "Во время интенсивных тренировок твой <w>пульс</w> сильно повышается."
                            }
                        }
                    ],
                    "synonyms": [
                        "hartfrequentie",
                        "polsslag"
                    ],
                    "common_phrases": [
                        "hartslag in rust",
                        "maximale hartslag",
                        "hartslag meten"
                    ],
                    "target_lang_definitions": {
                        "en": "The rhythm or frequency at which the heart beats, often expressed in beats per minute.",
                        "ru": "Ритм или частота, с которой бьется сердце (часто выражается в ударах в минуту)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heart rate",
                                "target_lang_sense_clarification": "frequency/speed"
                            },
                            {
                                "target_lang_word": "heartbeat",
                                "target_lang_sense_clarification": "rhythm"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пульс",
                                "target_lang_sense_clarification": "частота сердечных сокращений"
                            },
                            {
                                "target_lang_word": "частота пульса",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4f4a516-59bb-3a78-83f1-ffafcb8ebe7b",
                    "sense_definition": "Een drijvende kracht of het levendige centrum van iets.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Innovatie vormt de <w>hartslag</w> van dit moderne technologiebedrijf.",
                            "target_lang_translations": {
                                "en": "Innovation forms the <w>heartbeat</w> of this modern technology company.",
                                "ru": "Инновации — это <w>пульс</w> этой современной технологической компании."
                            }
                        },
                        {
                            "text": "De drukke haven is de economische <w>hartslag</w> van de hele regio.",
                            "target_lang_translations": {
                                "en": "The busy port is the economic <w>heartbeat</w> of the entire region.",
                                "ru": "Оживленный порт — это экономический <w>пульс</w> всего региона."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A driving force or the lively center of something.",
                        "ru": "Движущая сила или жизненный центр чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heartbeat",
                                "target_lang_sense_clarification": "vital center"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пульс",
                                "target_lang_sense_clarification": "переносное значение: ритм жизни"
                            },
                            {
                                "target_lang_word": "сердцебиение",
                                "target_lang_sense_clarification": "переносное значение: источник жизни"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4fa0c4bf-b8c5-3a09-9316-146cf7ca1533",
                    "sense_definition": "Разделять что-либо на части или делать надрезы с помощью острого инструмента (ножа, ножниц и т.п.).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама попросила меня <резать> хлеб к обеду аккуратными ломтиками.",
                            "target_lang_translations": {
                                "en": "Mom asked me to <slice> the bread for dinner into neat slices."
                            }
                        },
                        {
                            "text": "Этими тупыми ножницами очень трудно <резать> плотную бумагу.",
                            "target_lang_translations": {
                                "en": "It is very difficult to <cut> thick paper with these blunt scissors."
                            }
                        }
                    ],
                    "antonyms": [
                        "соединять"
                    ],
                    "common_phrases": [
                        "резать хлеб",
                        "резать ножом",
                        "резать на куски",
                        "резать бумагу"
                    ],
                    "target_lang_definitions": {
                        "en": "To divide something into parts or make incisions using a sharp tool (such as a knife or scissors)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cut",
                                "target_lang_sense_clarification": "general action"
                            },
                            {
                                "target_lang_word": "slice",
                                "target_lang_sense_clarification": "into thin pieces"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52371000-8452-37d6-8555-2cd2298a9566",
                    "sense_definition": "Убивать или ранить кого-либо (обычно животное) с помощью острого предмета.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "killing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В деревне принято <резать> скот поздней осенью.",
                            "target_lang_translations": {
                                "en": "In the village, it is customary to <slaughter> livestock in late autumn."
                            }
                        },
                        {
                            "text": "Преступники угрожали <резать> всех, кто попытается им помешать.",
                            "target_lang_translations": {
                                "en": "The criminals threatened to <stab> everyone who tried to stop them."
                            }
                        }
                    ],
                    "synonyms": [
                        "зарезывать"
                    ],
                    "common_phrases": [
                        "резать скот",
                        "резать кур",
                        "резать без ножа"
                    ],
                    "target_lang_definitions": {
                        "en": "To kill or wound someone (usually an animal) using a sharp object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slaughter",
                                "target_lang_sense_clarification": "livestock"
                            },
                            {
                                "target_lang_word": "stab",
                                "target_lang_sense_clarification": "a person"
                            },
                            {
                                "target_lang_word": "cut",
                                "target_lang_sense_clarification": "wound with a knife"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e0f484d-093c-341c-8ac1-8569c5df7355",
                    "sense_definition": "Причинять физическую боль, сильно сдавливая тело или врезаясь в него (об одежде, веревках).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тяжёлый рюкзак начал больно <резать> мне плечи узкими лямками.",
                            "target_lang_translations": {
                                "en": "The heavy backpack started to painfully <dig into> my shoulders with its narrow straps."
                            }
                        },
                        {
                            "text": "Тугой воротник новой рубашки продолжает <резать> шею весь день.",
                            "target_lang_translations": {
                                "en": "The tight collar of the new shirt continues to <cut into> my neck all day."
                            }
                        }
                    ],
                    "common_phrases": [
                        "резать плечо",
                        "резать кожу",
                        "ремень режет"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause physical pain by compressing the body or digging into it (referring to clothes, ropes, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dig into",
                                "target_lang_sense_clarification": "press painfully"
                            },
                            {
                                "target_lang_word": "cut into",
                                "target_lang_sense_clarification": "leave a mark on skin"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfe8a9f7-d34d-3ad1-b0a4-c76c1e23b1a1",
                    "sense_definition": "Вызывать неприятное, болезненное ощущение в органах чувств (в глазах от света или в ушах от звука).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "irritation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Яркий солнечный свет начал <резать> глаза, когда мы вышли из тёмной комнаты.",
                            "target_lang_translations": {
                                "en": "The bright sunlight began to <hurt> my eyes when we came out of the dark room."
                            }
                        },
                        {
                            "text": "Этот скрипучий звук скрипки просто <режет> слух.",
                            "target_lang_translations": {
                                "en": "This scratching sound of the violin simply <grates on> the ears."
                            }
                        }
                    ],
                    "synonyms": [
                        "раздражать",
                        "коробить"
                    ],
                    "common_phrases": [
                        "резать слух",
                        "резать глаза",
                        "свет режет"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause an unpleasant, painful sensation in the sensory organs (eyes from light or ears from sound)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hurt",
                                "target_lang_sense_clarification": "cause pain"
                            },
                            {
                                "target_lang_word": "sting",
                                "target_lang_sense_clarification": "eyes"
                            },
                            {
                                "target_lang_word": "grate on",
                                "target_lang_sense_clarification": "ears/hearing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "391b4be6-bc6b-36e9-bd01-0b028619e600",
                    "sense_definition": "Проводить хирургическую операцию.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач сказал, что таблетки не помогут и придётся <резать>.",
                            "target_lang_translations": {
                                "en": "The doctor said that pills wouldn't help and he would have to <operate>."
                            }
                        },
                        {
                            "text": "Известный хирург будет <резать> пациента завтра утром.",
                            "target_lang_translations": {
                                "en": "A famous surgeon will <operate on> the patient tomorrow morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "оперировать"
                    ],
                    "common_phrases": [
                        "придётся резать",
                        "резать по живому"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used instead of the formal 'оперировать'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To perform a surgical operation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "operate",
                                "target_lang_sense_clarification": "medical"
                            },
                            {
                                "target_lang_word": "cut",
                                "target_lang_sense_clarification": "colloquial"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86f8105a-4e46-3d9e-be85-79a68d118ca5",
                    "sense_definition": "Создавать художественные изделия или узоры из дерева, кости или камня.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой дедушка любил <резать> красивые фигурки из дерева.",
                            "target_lang_translations": {
                                "en": "My grandfather loved to <carve> beautiful figures out of wood."
                            }
                        },
                        {
                            "text": "Мастер начал <резать> сложный узор на поверхности шкатулки.",
                            "target_lang_translations": {
                                "en": "The master began to <carve> a complex pattern on the surface of the box."
                            }
                        }
                    ],
                    "common_phrases": [
                        "резать по дереву",
                        "резать по кости"
                    ],
                    "target_lang_definitions": {
                        "en": "To create artistic objects or patterns from wood, bone, or stone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carve",
                                "target_lang_sense_clarification": "wood or stone"
                            },
                            {
                                "target_lang_word": "whittle",
                                "target_lang_sense_clarification": "wood"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2066151-46b6-3c9d-97e4-f6cf0aa756d7",
                    "sense_definition": "Говорить правду прямо, резко и откровенно.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он не боялся <резать> правду в глаза даже начальству.",
                            "target_lang_translations": {
                                "en": "He was not afraid to <tell> the truth to people's faces, even to the management."
                            }
                        },
                        {
                            "text": "Иногда лучше промолчать, чем <резать> правду-матку.",
                            "target_lang_translations": {
                                "en": "Sometimes it is better to remain silent than to <tell> the home truth."
                            }
                        }
                    ],
                    "common_phrases": [
                        "резать правду",
                        "резать правду-матку",
                        "резать в глаза"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in fixed phrases"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To speak the truth directly, bluntly, and frankly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "the truth"
                            },
                            {
                                "target_lang_word": "speak",
                                "target_lang_sense_clarification": "bluntly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47ac5144-125b-324c-be1e-8d6e2142f028",
                    "sense_definition": "Ставить неудовлетворительную оценку на экзамене, намеренно «заваливать» студента.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Профессор обещал не <резать> студентов на пересдаче.",
                            "target_lang_translations": {
                                "en": "The professor promised not to <fail> students on the retake."
                            }
                        },
                        {
                            "text": "Говорят, что на вступительных экзаменах любят <резать> абитуриентов.",
                            "target_lang_translations": {
                                "en": "They say that at entrance exams they love to <flunk> applicants."
                            }
                        }
                    ],
                    "common_phrases": [
                        "резать на экзамене"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "student slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To give a failing grade on an exam, intentionally failing a student."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fail",
                                "target_lang_sense_clarification": "a student"
                            },
                            {
                                "target_lang_word": "flunk",
                                "target_lang_sense_clarification": "slang"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "998f96d9-dbd1-3473-ae2f-dbf065731ef6",
                    "sense_definition": "Искусственно ограничивать скорость передачи данных или доступ к ресурсам.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Провайдер начал <резать> скорость интернета по вечерам.",
                            "target_lang_translations": {
                                "en": "The provider started to <throttle> internet speed in the evenings."
                            }
                        },
                        {
                            "text": "Системный администратор решил <резать> трафик для социальных сетей.",
                            "target_lang_translations": {
                                "en": "The system administrator decided to <limit> traffic for social networks."
                            }
                        }
                    ],
                    "common_phrases": [
                        "резать скорость",
                        "резать трафик"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "computer/internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To artificially limit data transfer speed or access to resources."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throttle",
                                "target_lang_sense_clarification": "bandwidth/speed"
                            },
                            {
                                "target_lang_word": "cut",
                                "target_lang_sense_clarification": "reduce"
                            },
                            {
                                "target_lang_word": "limit",
                                "target_lang_sense_clarification": "access"
                            }
                        ]
                    }
                }
            ]
        }
    ]

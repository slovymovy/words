    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bfa08214-6369-38f7-ba27-deb83d7f1aa7",
                    "sense_definition": "Опустить вниз что-либо, закреплённое сверху или удерживаемое на весу (часть тела, предмет).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "hanging_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мальчик сел на высокий подоконник и <свесил> ноги на улицу.",
                            "target_lang_translations": {
                                "en": "The boy sat on the high windowsill and <dangled> his legs outside."
                            }
                        },
                        {
                            "text": "Чтобы поднять ведро с водой, нам пришлось <свесить> верёвку в колодец.",
                            "target_lang_translations": {
                                "en": "To pull up the bucket of water, we had to <lower> the rope into the well."
                            }
                        }
                    ],
                    "synonyms": [
                        "спустить",
                        "опустить"
                    ],
                    "antonyms": [
                        "поднять"
                    ],
                    "common_phrases": [
                        "свесить ноги",
                        "свесить руки",
                        "свесить верёвку"
                    ],
                    "target_lang_definitions": {
                        "en": "To lower something so that it hangs freely (e.g., legs, a rope)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dangle",
                                "target_lang_sense_clarification": "legs or arms swinging freely"
                            },
                            {
                                "target_lang_word": "hang down",
                                "target_lang_sense_clarification": "general action of letting something hang"
                            },
                            {
                                "target_lang_word": "let down",
                                "target_lang_sense_clarification": "to lower something like a rope"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4678e0c7-d437-3c10-a9a9-9d916405f2a0",
                    "sense_definition": "Склонить или низко опустить (обычно голову) в знак усталости, уныния или покорности.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "hanging_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Уставший путник сел на камень и бессильно <свесил> голову на грудь.",
                            "target_lang_translations": {
                                "en": "The tired traveler sat on a stone and powerlessly <hung> his head on his chest."
                            }
                        },
                        {
                            "text": "Собака виновато <свесила> уши, понимая, что хозяин недоволен.",
                            "target_lang_translations": {
                                "en": "The dog guiltily <drooped> its ears, realizing the owner was displeased."
                            }
                        }
                    ],
                    "synonyms": [
                        "повесить",
                        "склонить",
                        "опустить"
                    ],
                    "antonyms": [
                        "поднять",
                        "вскинуть"
                    ],
                    "common_phrases": [
                        "свесить голову"
                    ],
                    "target_lang_definitions": {
                        "en": "To lower or bow (usually the head) as a sign of fatigue, sadness, or submission."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hang",
                                "target_lang_sense_clarification": "specifically one's head"
                            },
                            {
                                "target_lang_word": "droop",
                                "target_lang_sense_clarification": "head or ears, implying lack of energy"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "36022351-e55a-324f-bef1-45860c4180e3",
                    "sense_definition": "Weakened, damaged, or functioning less effectively than usual.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school provides special books for the visually <w>impaired</w>.",
                            "target_lang_translations": {
                                "ru": "Школа предоставляет специальные книги для <w>слабовидящих</w>."
                            }
                        },
                        {
                            "text": "His <w>impaired</w> memory made it difficult to recall the events of that day.",
                            "target_lang_translations": {
                                "ru": "Его <w>ослабленная</w> память мешала вспомнить события того дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "challenged"
                    ],
                    "common_phrases": [
                        "visually impaired",
                        "hearing impaired",
                        "physically impaired"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ослабленный, поврежденный или функционирующий менее эффективно, чем обычно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ослабленный",
                                "target_lang_sense_clarification": "о здоровье, памяти или функциях"
                            },
                            {
                                "target_lang_word": "нарушенный",

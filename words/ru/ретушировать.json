    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2e8aec1f-7112-3626-a8b5-93f62db280f7",
                    "sense_definition": "Обрабатывать изображения или фотографии с целью устранения дефектов, исправления деталей или общего улучшения качества.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Фотограф обещал качественно <w>ретушировать</w> все свадебные снимки.",
                            "target_lang_translations": {
                                "en": "The photographer promised to <w>retouch</w> all the wedding shots to a high standard."
                            }
                        },
                        {
                            "text": "В графическом редакторе можно быстро <w>ретушировать</w> старую фотографию.",
                            "target_lang_translations": {
                                "en": "You can quickly <w>retouch</w> an old photo in a graphics editor."
                            }
                        }
                    ],
                    "synonyms": [
                        "залакировывать"
                    ],
                    "common_phrases": [
                        "ретушировать фотографию",
                        "ретушировать снимок",
                        "ретушировать портрет",
                        "ретушировать изображение"
                    ],
                    "target_lang_definitions": {
                        "en": "To process images or photographs to remove defects, correct details, or generally improve quality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "retouch",
                                "target_lang_sense_clarification": "images or photos"
                            },
                            {
                                "target_lang_word": "airbrush",
                                "target_lang_sense_clarification": "photos to remove imperfections"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b188d97-d02d-312d-bd19-eb207943c0b0",
                    "sense_definition": "Намеренно изменять факты или информацию, чтобы представить их в более выгодном свете, скрывая недостатки.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Журналист решил не <w>ретушировать</w> действительность и написал всю правду.",
                            "target_lang_translations": {
                                "en": "The journalist decided not to <w>gloss over</w> reality and wrote the whole truth."
                            }
                        },
                        {
                            "text": "В своих мемуарах политик пытался <w>ретушировать</w> ошибки прошлого.",
                            "target_lang_translations": {
                                "en": "In his memoirs, the politician tried to <w>whitewash</w> the mistakes of the past."
                            }
                        }
                    ],
                    "synonyms": [
                        "лакировать",
                        "залакировывать",
                        "подлакировывать"
                    ],
                    "common_phrases": [
                        "ретушировать действительность",
                        "ретушировать факты",
                        "ретушировать историю",
                        "ретушировать правду"
                    ],
                    "target_lang_definitions": {
                        "en": "To intentionally alter facts or information to present them in a more favorable light, concealing flaws."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gloss over",
                                "target_lang_sense_clarification": "facts or reality"
                            },
                            {
                                "target_lang_word": "whitewash",
                                "target_lang_sense_clarification": "mistakes or history"
                            },
                            {
                                "target_lang_word": "sugarcoat",
                                "target_lang_sense_clarification": "unpleasant truths"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
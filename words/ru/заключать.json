    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cd670f1e-1770-3c39-a3c0-2cd77b82fb96",
                    "sense_definition": "Официально оформлять договорённость, сделку, союз или соглашение.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компании планируют <заключать> выгодную сделку в конце года.",
                            "target_lang_translations": {
                                "en": "The companies plan to <close> a profitable deal at the end of the year."
                            }
                        },
                        {
                            "text": "В древности короли предпочитали <заключать> мир через династические браки.",
                            "target_lang_translations": {
                                "en": "In ancient times, kings preferred to <make> peace through dynastic marriages."
                            }
                        },
                        {
                            "text": "Молодые люди решили официально <заключать> брак в следующем месяце.",
                            "target_lang_translations": {
                                "en": "The young couple decided to officially <enter into> marriage next month."
                            }
                        }
                    ],
                    "synonyms": [
                        "подписывать",
                        "оформлять"
                    ],
                    "antonyms": [
                        "расторгать"
                    ],
                    "common_phrases": [
                        "заключать договор",
                        "заключать сделку",
                        "заключать брак",
                        "заключать мир",
                        "заключать пари"
                    ],
                    "target_lang_definitions": {
                        "en": "To officially finalize an agreement, deal, union, or treaty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conclude",
                                "target_lang_sense_clarification": "formal agreements, treaties, or pacts"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "deals, bargains, or peace"
                            },
                            {
                                "target_lang_word": "enter into",
                                "target_lang_sense_clarification": "contracts, marriages, or alliances"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "933a9f66-d6ed-3230-9c44-901493daae4d",
                    "sense_definition": "Делать вывод или приходить к мнению на основе рассуждений и фактов.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из его молчания можно <заключать>, что он не согласен с нами.",
                            "target_lang_translations": {
                                "en": "From his silence, one can <conclude> that he disagrees with us."
                            }
                        },
                        {
                            "text": "Мы начали <заключать>, что эксперимент прошёл успешно.",
                            "target_lang_translations": {
                                "en": "We began to <conclude> that the experiment was successful."
                            }
                        }
                    ],
                    "synonyms": [
                        "выводить"
                    ],
                    "target_lang_definitions": {
                        "en": "To draw a conclusion or arrive at an opinion based on reasoning and facts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conclude",
                                "target_lang_sense_clarification": "to decide after reasoning"
                            },
                            {
                                "target_lang_word": "infer",
                                "target_lang_sense_clarification": "to deduce from evidence"
                            }

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3f3fb9bd-d579-3e55-9d37-4ff50af4c35c",
                    "sense_definition": "Lopen voor het plezier of ter ontspanning, meestal in een rustig tempo en zonder specifiek doel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben even een ommetje <w>gewandeld</w> na het eten.",
                            "target_lang_translations": {
                                "en": "We went for a short <w>walk</w> after dinner.",
                                "ru": "Мы немного <w>прогулялись</w> после ужина."
                            }
                        },
                        {
                            "text": "In het weekend <w>wandelen</w> wij graag samen door het park.",
                            "target_lang_translations": {
                                "en": "On weekends we like to <w>stroll</w> through the park together.",
                                "ru": "По выходным мы любим вместе <w>гулять</w> по парку."
                            }
                        }
                    ],
                    "synonyms": [
                        "hiken"
                    ],
                    "antonyms": [
                        "rennen"
                    ],
                    "common_phrases": [
                        "gaan wandelen",
                        "lekker wandelen",
                        "buiten wandelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To walk for pleasure or relaxation, usually at a leisurely pace and without a specific destination.",
                        "ru": "Ходить пешком ради удовольствия или отдыха, обычно в спокойном темпе и без конкретной цели."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stroll",
                                "target_lang_sense_clarification": "leisurely"
                            },
                            {
                                "target_lang_word": "walk",
                                "target_lang_sense_clarification": "general"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гулять",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "прогуливаться",
                                "target_lang_sense_clarification": "неспешно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fdac34d-8aac-39fd-8a1c-f90b4914c943",
                    "sense_definition": "Een lange, stevige tocht te voet maken, vaak door de natuur of heuvelachtig gebied.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb met mijn tent twee weken lang door de heuvels <w>gewandeld</w>.",
                            "target_lang_translations": {
                                "en": "I <w>hiked</w> through the hills with my tent for two weeks.",
                                "ru": "Я две недели <w>путешествовал пешком</w> с палаткой по холмам."
                            }
                        },
                        {
                            "text": "Tijdens onze vakantie in Oostenrijk <w>wandelen</w> we veel in de bergen.",
                            "target_lang_translations": {
                                "en": "During our holiday in Austria, we <w>hike</w> a lot in the mountains.",
                                "ru": "Во время нашего отпуска в Австрии мы много <w>ходим в походы</w> в горах."
                            }
                        }
                    ],
                    "synonyms": [
                        "hiken"
                    ],
                    "common_phrases": [
                        "in de bergen wandelen",
                        "een route wandelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To go on a long, vigorous journey on foot, often through nature or hilly terrain.",
                        "ru": "Совершать длительную, энергичную прогулку пешком, часто на природе или по пересеченной местности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hike",
                                "target_lang_sense_clarification": "in nature"
                            },
                            {
                                "target_lang_word": "walk",
                                "target_lang_sense_clarification": "long distance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ходить в поход",
                                "target_lang_sense_clarification": "активный отдых"
                            },
                            {
                                "target_lang_word": "путешествовать пешком",
                                "target_lang_sense_clarification": "на дальние расстояния"
                            }
                        ]
                    }
                }
            ]
        }
    ]

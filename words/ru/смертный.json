    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "770b2f30-0b21-3fad-a1d7-bdef3b239ae1",
                    "sense_definition": "Подверженный смерти; такой, который не может жить вечно (в противоположность богам или вечным сущностям).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "biological_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все люди <смертны>, и это естественный закон природы.",
                            "target_lang_translations": {
                                "en": "All humans are <w>mortal</w>, and this is a natural law of nature."
                            }
                        },
                        {
                            "text": "Философы часто размышляют о том, что тело <смертно>, а душа вечна.",
                            "target_lang_translations": {
                                "en": "Philosophers often reflect on the fact that the body is <w>mortal</w>, while the soul is eternal."
                            }
                        }
                    ],
                    "synonyms": [
                        "смертельный",
                        "смертельный"
                    ],
                    "antonyms": [
                        "бессмертный",
                        "вечный"
                    ],
                    "common_phrases": [
                        "все люди смертны",
                        "смертная природа"
                    ],
                    "target_lang_definitions": {
                        "en": "Subject to death; unable to live forever (as opposed to gods or eternal entities)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mortal",
                                "target_lang_sense_clarification": "subject to death"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4060d828-9d23-3b1b-a1f7-c2e5994ef0d7",
                    "sense_definition": "Влекущий за собой смерть или связанный с наказанием в виде лишения жизни (преимущественно в устойчивых сочетаниях).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legal_religious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Судья зачитал преступнику <смертный> приговор в полной тишине.",
                            "target_lang_translations": {
                                "en": "The judge read the <w>death</w> sentence to the criminal in complete silence."
                            }
                        },
                        {
                            "text": "Во многих странах <смертная> казнь была отменена в прошлом веке.",
                            "target_lang_translations": {
                                "en": "In many countries, the <w>death</w> penalty was abolished in the last century."
                            }
                        },
                        {
                            "text": "Гордыня считается одним из семи <смертных> грехов.",
                            "target_lang_translations": {
                                "en": "Pride is considered one of the seven <w>deadly</w> sins."
                            }
                        }
                    ],
                    "common_phrases": [
                        "смертная казнь",
                        "смертный приговор",
                        "смертный грех",
                        "смертный бой"
                    ],
                    "target_lang_definitions": {
                        "en": "Entailing death or related to punishment by loss of life (mainly in set phrases)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "death",
                                "target_lang_sense_clarification": "in 'death penalty' or 'death sentence'"
                            },
                            {
                                "target_lang_word": "capital",
                                "target_lang_sense_clarification": "in 'capital punishment'"
                            },
                            {
                                "target_lang_word": "mortal",
                                "target_lang_sense_clarification": "in 'mortal sin'"
                            },
                            {
                                "target_lang_word": "deadly",
                                "target_lang_sense_clarification": "in 'deadly sin'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbb1632e-a365-3170-82a7-62e0e31de691",
                    "sense_definition": "Относящийся к моменту умирания, к похоронам или к умершему.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "related_to_dying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В свой <смертный> час старик позвал детей, чтобы проститься.",
                            "target_lang_translations": {
                                "en": "In his <w>dying</w> hour, the old man called his children to say goodbye."
                            }
                        },
                        {
                            "text": "Солдатам выдавали специальный <смертный> медальон с личными данными.",
                            "target_lang_translations": {
                                "en": "Soldiers were issued a special <w>death</w> medallion with personal data."
                            }
                        }
                    ],
                    "synonyms": [
                        "смертельный",
                        "смертельный"
                    ],
                    "antonyms": [
                        "пожизненный"
                    ],
                    "common_phrases": [
                        "смертный час",
                        "смертное ложе",
                        "смертная рубаха"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the moment of dying, funerals, or the deceased."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dying",
                                "target_lang_sense_clarification": "related to the time of death"
                            },
                            {
                                "target_lang_word": "death",
                                "target_lang_sense_clarification": "attributive use, e.g. deathbed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b25c476e-e273-3167-921e-e02365f2d17b",
                    "sense_definition": "Очень сильный, крайний в своём проявлении (обычно о чём-то неприятном: скуке, усталости, страхе).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_extreme",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На этом собрании царила <смертная> скука, и никто не слушал докладчика.",
                            "target_lang_translations": {
                                "en": "<w>Deadly</w> boredom reigned at this meeting, and no one listened to the speaker."
                            }
                        },
                        {
                            "text": "Её лицо покрыла <смертная> бледность, когда она услышала плохие новости.",
                            "target_lang_translations": {
                                "en": "A <w>deathly</w> pallor covered her face when she heard the bad news."
                            }
                        }
                    ],
                    "synonyms": [
                        "смертельный",
                        "смертельный"
                    ],
                    "common_phrases": [
                        "скука смертная",
                        "смертная бледность",
                        "смертная тоска"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Very strong, extreme in manifestation (usually regarding something unpleasant: boredom, fatigue, fear)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deadly",
                                "target_lang_sense_clarification": "extreme (boredom)"
                            },
                            {
                                "target_lang_word": "deathly",
                                "target_lang_sense_clarification": "resembling death (pallor)"
                            },
                            {
                                "target_lang_word": "mortal",
                                "target_lang_sense_clarification": "extreme (fear/danger)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9c36f4c9-d937-3b12-8a65-2c6954dc94bc",
                    "sense_definition": "Человек как существо, жизнь которого неизбежно заканчивается смертью (часто с оттенком иронии или противопоставления высшим силам).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Простым <смертным> вход на это закрытое мероприятие был запрещён.",
                            "target_lang_translations": {
                                "en": "Entry to this closed event was forbidden to mere <w>mortals</w>."
                            }
                        },
                        {
                            "text": "Древние мифы рассказывают о любви богов к обычным <смертным>.",
                            "target_lang_translations": {
                                "en": "Ancient myths tell of the love of gods for ordinary <w>mortals</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "смертельный",
                        "смертельный"
                    ],
                    "antonyms": [
                        "бессмертный",
                        "бог"
                    ],
                    "common_phrases": [
                        "простой смертный",
                        "обычный смертный"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "substantivized adjective"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A human being whose life inevitably ends in death (often ironic or contrasted with higher powers)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mortal",
                                "target_lang_sense_clarification": "human being"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "172fa68e-0e42-38b5-8b11-171bc52a4911",
                    "sense_definition": "Aan de voorkant van iets of iemand; dichterbij dan iets anders.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die auto staat altijd <w>voor</w> ons huis geparkeerd.",
                            "target_lang_translations": {
                                "en": "That car is always parked <w>in front of</w> our house."
                            }
                        },
                        {
                            "text": "Er hangen donkere wolken <w>voor</w> de zon.",
                            "target_lang_translations": {
                                "en": "Dark clouds are hanging <w>in front of</w> the sun."
                            }
                        }

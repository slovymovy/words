            "senses": [
                {
                    "sense_id": "090f6e2d-4ed4-38df-9f67-0c65d1a4eb34",
                    "sense_definition": "Een grote ruimte die speciaal is ingericht voor het geven van lezingen, colleges of concerten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "public_rooms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De studenten zochten een plaats in de grote <w>gehoorzaal</w> voordat het college begon.",
                            "target_lang_translations": {
                                "ru": "Студенты искали место в большой <w>аудитории</w> перед началом лекции."
                            }
                        },
                        {
                            "text": "De professor sprak in een volle <w>gehoorzaal</w> over de geschiedenis van de kunst.",
                            "target_lang_translations": {
                                "ru": "Профессор рассказывал об истории искусства в переполненной <w>аудитории</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "auditorium"
                    ],
                    "common_phrases": [
                        "volle gehoorzaal",
                        "in de gehoorzaal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое помещение, специально оборудованное для проведения лекций, занятий или концертов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аудитория",
                                "target_lang_sense_clarification": "учебное помещение"
                            },
                            {
                                "target_lang_word": "лекционный зал",
                                "target_lang_sense_clarification": "зал для лекций"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5b274a3-f831-397f-8ace-9edd0b0351af",
                    "sense_definition": "Een zaal waar een vorst of hoogwaardigheidsbekleder bezoekers officieel ontvangt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "formal_rooms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koning wachtte in de <w>gehoorzaal</w> op de komst van de buitenlandse diplomaten.",
                            "target_lang_translations": {
                                "ru": "Король ожидал прибытия иностранных дипломатов в <w>зале для аудиенций</w>."
                            }
                        },
                        {
                            "text": "Het paleis beschikt over een prachtige <w>gehoorzaal</w> met antieke wandtapijten.",
                            "target_lang_translations": {
                                "ru": "Во дворце есть великолепный <w>зал для аудиенций</w> со старинными гобеленами."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Зал, где монарх или высокопоставленное лицо официально принимает посетителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зал для аудиенций",

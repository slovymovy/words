    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8901c74a-55c8-3f25-aadf-e0c27dddb663",
                    "sense_definition": "Iemand formeel of beleefd vragen om iets te doen",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>verzoek</w> u vriendelijk om even te wachten.",
                            "target_lang_translations": {
                                "en": "I kindly <w>request</w> you to wait a moment."
                            }
                        },
                        {
                            "text": "De passagiers werd <w>verzocht</w> hun veiligheidsgordels aan te doen.",
                            "target_lang_translations": {
                                "en": "Passengers were <w>requested</w> to fasten their seatbelts."
                            }
                        },
                        {
                            "text": "Wij <w>verzoeken</w> iedereen om stil te zijn tijdens de voorstelling.",
                            "target_lang_translations": {
                                "en": "We <w>request</w> everyone to be quiet during the performance."
                            }
                        }
                    ],
                    "synonyms": [
                        "vragen"
                    ],
                    "antonyms": [
                        "bevelen",
                        "verbieden"
                    ],
                    "common_phrases": [
                        "vriendelijk verzoeken",
                        "dringend verzoeken",
                        "iemand verzoeken te"
                    ],
                    "target_lang_definitions": {
                        "en": "To ask someone formally or politely to do something"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "request",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "general"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8664bdf-717e-3607-941e-7d8bdac7dc96",
                    "sense_definition": "Iemand in verleiding brengen of op de proef stellen",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "tempt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het oude verhaal werd de held <w>verzocht</w> door rijkdom en macht.",
                            "target_lang_translations": {
                                "en": "In the old story, the hero was <w>tempted</w> by wealth and power."
                            }
                        },
                        {
                            "text": "Hij liet zich niet <w>verzoeken</w> om zijn principes opzij te zetten.",
                            "target_lang_translations": {
                                "en": "He did not let himself be <w>tempted</w> to set aside his principles."
                            }

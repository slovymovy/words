            "pos": "verb",
            "senses": [
                {
                    "sense_id": "16e2f711-657e-37d9-93c8-f58ce857a06e",
                    "sense_definition": "Przyjąć lub wziąć coś, co zostało przekazane, wysłane lub przygotowane do wzięcia (np. list, paczkę, nagrodę).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "receive_collect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść dzisiaj na pocztę, żeby <odebrać> polecony list.",
                            "target_lang_translations": {
                                "ru": "Мне нужно сегодня пойти на почту, чтобы <w>забрать</w> заказное письмо."
                            }
                        },
                        {
                            "text": "Czy możesz <odebrać> dzieci z przedszkola po południu?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>забрать</w> детей из детского сада после обеда?"
                            }
                        }
                    ],
                    "synonyms": [
                        "przyjąć",
                        "wziąć",
                        "otrzymać"
                    ],
                    "antonyms": [
                        "rozłączyć",
                        "rozłączyć"
                    ],
                    "common_phrases": [
                        "odebrać paczkę",
                        "odebrać list",
                        "odebrać nagrodę",
                        "odebrać dyplom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять или взять что-то, что было передано, отправлено или подготовлено (например, письмо, посылку, награду)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забрать",
                                "target_lang_sense_clarification": "взять подготовленное или кого-то"
                            },
                            {
                                "target_lang_word": "получить",
                                "target_lang_sense_clarification": "принять отправленное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ede45315-ac4f-3bb8-914b-6d2bb46514fc",
                    "sense_definition": "Podnieść słuchawkę lub nacisnąć przycisk, aby rozpocząć rozmowę telefoniczną.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "phone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzwoniłem do ciebie trzy razy, ale nie chciałeś <odebrać>.",
                            "target_lang_translations": {
                                "ru": "Я звонил тебе три раза, но ты не хотел <w>брать трубку</w>."
                            }
                        },
                        {
                            "text": "Nie mogę teraz <odebrać> telefonu, bo prowadzę samochód.",
                            "target_lang_translations": {
                                "ru": "Я не могу сейчас <w>ответить</w>, так как веду машину."
                            }
                        }
                    ],
                    "antonyms": [
                        "rozłączyć"
                    ],
                    "common_phrases": [
                        "odebrać telefon",
                        "odebrać połączenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поднять трубку или нажать кнопку, чтобы начать телефонный разговор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответить",
                                "target_lang_sense_clarification": "на звонок"
                            },
                            {
                                "target_lang_word": "взять трубку",
                                "target_lang_sense_clarification": "начать разговор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b90a49e-3a54-39ce-8804-b023d3ceeafc",
                    "sense_definition": "Zabrać coś komuś lub pozbawić kogoś czegoś (np. prawa, nadziei, zdrowia).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "deprive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd zdecydował <odebrać> mu prawa rodzicielskie z powodu zaniedbań.",
                            "target_lang_translations": {
                                "ru": "Суд решил <w>лишить</w> его родительских прав из-за неисполнения обязанностей."
                            }
                        },
                        {
                            "text": "Nie otworzył nawet ust, bo blada wściekłość <odebrała> mu mowę.",
                            "target_lang_translations": {
                                "ru": "Он даже не открыл рот, потому что дикая ярость <w>отняла</w> у него дар речи."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabrać",
                        "pozbawić"
                    ],
                    "antonyms": [
                        "rozłączyć"
                    ],
                    "common_phrases": [
                        "odebrać głos",
                        "odebrać nadzieję",
                        "odebrać życie",
                        "odebrać prawo jazdy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отнять что-то у кого-то или лишить кого-то чего-то (например, прав, надежды, здоровья)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лишить",
                                "target_lang_sense_clarification": "прав, надежды"
                            },
                            {
                                "target_lang_word": "отнять",
                                "target_lang_sense_clarification": "силой или против воли"
                            },
                            {
                                "target_lang_word": "забрать",
                                "target_lang_sense_clarification": "присвоить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34f179c8-1a63-3b32-a72c-5fc8af922ca5",
                    "sense_definition": "Odzyskać swoją własność, która była u kogoś innego lub została wcześniej utracona.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "regain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pożyczyłem mu książkę miesiąc temu i wciąż nie mogę jej <odebrać>.",
                            "target_lang_translations": {
                                "ru": "Я одолжил ему книгу месяц назад и всё ещё не могу её <w>забрать</w>."
                            }
                        },
                        {
                            "text": "Jutro pojadę do warsztatu, żeby <odebrać> samochód po naprawie.",
                            "target_lang_translations": {
                                "ru": "Завтра я поеду в мастерскую, чтобы <w>забрать</w> машину после ремонта."
                            }
                        }
                    ],
                    "synonyms": [
                        "odzyskać"
                    ],
                    "antonyms": [
                        "rozłączyć"
                    ],
                    "common_phrases": [
                        "odebrać dług",
                        "odebrać własność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вернуть свою собственность, которая была у кого-то другого или была ранее утрачена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забрать",
                                "target_lang_sense_clarification": "вернуть себе"
                            },
                            {
                                "target_lang_word": "получить обратно",
                                "target_lang_sense_clarification": "вернуть вещь"
                            }
                        ]
                    }
                }
            ]
        }

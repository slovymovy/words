                {
                    "sense_id": "f21a61a7-406b-3881-8a80-ae1daedece16",
                    "sense_definition": "Część tuszy wołowej z odcinka grzbietowego, znajdująca się za łopatką.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "meat_cuts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sklepie mięsnym poprosiłem o świeży <w>rozbratel</w>, aby przygotować wywar na zupę.",
                            "target_lang_translations": {
                                "ru": "В мясном магазине я попросил свежий <w>толстый край</w>, чтобы приготовить бульон для супа."
                            }
                        },
                        {
                            "text": "Ten kawałek mięsa to <w>rozbratel</w>, który doskonale nadaje się do powolnego pieczenia.",
                            "target_lang_translations": {
                                "ru": "Этот кусок мяса — <w>толстый край</w>, который отлично подходит для медленного запекания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rozbratel wołowy",
                        "rozbratel z kością",
                        "świeży rozbratel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть говяжьей туши из спинного отдела, находящаяся за лопаткой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толстый край",
                                "target_lang_sense_clarification": "часть туши"
                            },
                            {
                                "target_lang_word": "ростбиф",
                                "target_lang_sense_clarification": "отруб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64a3345c-9115-35bb-b938-50b512473b2b",
                    "sense_definition": "Kotlet lub potrawa mięsna przyrządzona z odpowiedniej części wołowiny, zazwyczaj smażona lub duszona.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "meat_dishes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na niedzielny obiad mama przygotowała pyszny <w>rozbratel</w> w sosie własnym z cebulą.",
                            "target_lang_translations": {
                                "ru": "На воскресный обед мама приготовила вкусный <w>антрекот</w> в собственном соку с луком."
                            }
                        },
                        {
                            "text": "W restauracji zamówiłem soczysty <w>rozbratel</w> podany z ziemniakami i zestawem surówek.",
                            "target_lang_translations": {
                                "ru": "В ресторане я заказал сочный <w>антрекот</w>, поданный с картофелем и набором салатов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "duszony rozbratel",
                        "smażony rozbratel",
                        "rozbratel z cebulą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Котлета или мясное блюдо, приготовленное из соответствующей части говядины, обычно жареное или тушеное."
                    },

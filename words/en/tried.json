{
    "word_family": [
        "try",
        "trial",
        "trier",
        "trying"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "297154b6-132a-3d90-9e3d-1ae7da6f06ed",
                    "sense_definition": "Simple past and past participle of <w>try</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>tried</w> to open the door, but it was locked tight.",
                            "target_lang_translations": {
                                "ru": "Она <w>попыталась</w> открыть дверь, но та была плотно заперта."
                            }
                        },
                        {
                            "text": "I <w>tried</w> my best to finish the homework before dinner.",
                            "target_lang_translations": {
                                "ru": "Я <w>старался</w> изо всех сил закончить домашнюю работу до ужина."
                            }
                        }
                    ],
                    "synonyms": [
                        "attempted",
                        "endeavored"
                    ],
                    "common_phrases": [
                        "tried to",
                        "tried hard",
                        "tried everything",
                        "tried my best"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола try (пытаться, пробовать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пытался",
                                "target_lang_sense_clarification": "совершал попытку"
                            },
                            {
                                "target_lang_word": "пробовал",
                                "target_lang_sense_clarification": "экспериментировал или тестировал"
                            },
                            {
                                "target_lang_word": "старался",
                                "target_lang_sense_clarification": "прилагал усилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "eb8edf33-5792-317b-906e-cb06a1964cc3",
                    "sense_definition": "Proven to be reliable, effective, or trustworthy through experience or testing.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The grandmother shared her <w>tried</w> and true recipe for apple pie.",
                            "target_lang_translations": {
                                "ru": "Бабушка поделилась своим <w>проверенным</w> рецептом яблочного пирога."
                            }
                        },
                        {
                            "text": "We prefer using <w>tried</w> methods rather than experimenting with new ones.",
                            "target_lang_translations": {
                                "ru": "Мы предпочитаем использовать <w>испытанные</w> методы, а не экспериментировать с новыми."
                            }
                        }
                    ],
                    "synonyms": [
                        "tested",
                        "proven",
                        "reliable",
                        "trustworthy"
                    ],
                    "antonyms": [
                        "untried",
                        "untested"
                    ],
                    "common_phrases": [
                        "tried and true",
                        "tried and tested"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доказавший свою надежность, эффективность или достоверность на опыте или в ходе испытаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверенный",
                                "target_lang_sense_clarification": "подтвержденный опытом"
                            },
                            {
                                "target_lang_word": "испытанный",
                                "target_lang_sense_clarification": "прошедший проверку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ef7088d-121f-3b81-8df6-f162280357ba",
                    "sense_definition": "Having been subjected to a legal trial in a court of law.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>tried</w> prisoners were escorted back to their cells after the hearing.",
                            "target_lang_translations": {
                                "ru": "<w>Судимых</w> заключенных отвели обратно в камеры после слушания."
                            }
                        },
                        {
                            "text": "Once <w>tried</w>, a person cannot usually be prosecuted again for the same crime.",
                            "target_lang_translations": {
                                "ru": "После того как человека <w>судили</w>, его обычно нельзя преследовать повторно за то же преступление."
                            }
                        }
                    ],
                    "synonyms": [
                        "judged",
                        "prosecuted"
                    ],
                    "antonyms": [
                        "untried"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергнутый судебному разбирательству в суде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судимый",
                                "target_lang_sense_clarification": "подвергшийся суду"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
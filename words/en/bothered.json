    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c09eaf3e-be0b-36df-bbc5-a40d4469fc8d",
                    "sense_definition": "Feeling annoyed, irritated, or uncomfortable due to a disturbance or worry.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_annoyance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was clearly <w>bothered</w> by the loud music coming from next door.",
                            "target_lang_translations": {
                                "ru": "Она была явно <w>раздражена</w> громкой музыкой, доносившейся из соседней квартиры."
                            }
                        },
                        {
                            "text": "He gets <w>bothered</w> when people interrupt him while he is working.",
                            "target_lang_translations": {
                                "ru": "Он <w>раздражается</w>, когда люди прерывают его во время работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "annoyed",
                        "irritated",
                        "vexed",
                        "agitated"
                    ],
                    "antonyms": [
                        "calm",
                        "unbothered",
                        "pleased"
                    ],
                    "common_phrases": [
                        "bothered by",
                        "hot and bothered"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий раздражение, досаду или дискомфорт из-за беспокойства или переживаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздражённый",
                                "target_lang_sense_clarification": "annoyed"
                            },
                            {
                                "target_lang_word": "обеспокоенный",
                                "target_lang_sense_clarification": "worried, disturbed"
                            },
                            {
                                "target_lang_word": "взволнованный",
                                "target_lang_sense_clarification": "agitated"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0e26dab-bf78-341e-94a8-4f6cbebece82",
                    "sense_definition": "Concerned, interested, or willing to make an effort to do something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attitude_willingness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am not <w>bothered</w> about where we go for dinner, so you can choose.",
                            "target_lang_translations": {
                                "ru": "Мне <w>безразлично</w>, куда мы пойдем ужинать, так что выбирай ты."
                            }
                        },
                        {
                            "text": "He couldn't be <w>bothered</w> to clean his room this weekend.",
                            "target_lang_translations": {
                                "ru": "Ему было <w>лень</w> убирать свою комнату в эти выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "concerned",
                        "interested"
                    ],
                    "antonyms": [
                        "indifferent",
                        "unconcerned"
                    ],
                    "common_phrases": [
                        "can't be bothered",
                        "not bothered"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заинтересованный или готовый приложить усилия (часто используется в отрицательных конструкциях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "озабоченный",
                                "target_lang_sense_clarification": "concerned"
                            },
                            {
                                "target_lang_word": "заинтересованный",
                                "target_lang_sense_clarification": "interested"
                            }
                        ]
                    }
                }
            ]
        },

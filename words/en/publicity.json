{
    "word_family": [
        "public",
        "publicize",
        "publicist",
        "publication"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e6c22a06-b989-3951-8011-ed013f9d5fb6",
                    "sense_definition": "Public interest or attention that someone or something receives.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "media_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scandal caused a lot of bad <w>publicity</w> for the company.",
                            "target_lang_translations": {
                                "ru": "Скандал обернулся для компании широкой негативной <w>оглаской</w>."
                            }
                        },
                        {
                            "text": "She tries to avoid <w>publicity</w> to protect her family's privacy.",
                            "target_lang_translations": {
                                "ru": "Она старается избегать <w>публичности</w>, чтобы защитить частную жизнь своей семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "attention",
                        "exposure",
                        "fame",
                        "spotlight",
                        "notoriety"
                    ],
                    "antonyms": [
                        "privacy",
                        "obscurity"
                    ],
                    "common_phrases": [
                        "bad publicity",
                        "good publicity",
                        "negative publicity",
                        "seek publicity",
                        "adverse publicity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общественный интерес или внимание, которое кто-то или что-то получает."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внимание",
                                "target_lang_sense_clarification": "интерес со стороны общества или СМИ"
                            },
                            {
                                "target_lang_word": "огласка",
                                "target_lang_sense_clarification": "публичное обсуждение, часто в негативном контексте"
                            },
                            {
                                "target_lang_word": "известность",
                                "target_lang_sense_clarification": "популярность, узнаваемость"
                            },
                            {
                                "target_lang_word": "слава",
                                "target_lang_sense_clarification": "широкая известность, репутация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1437c5b-e5a4-3cb3-8371-0f4d5776c5a0",
                    "sense_definition": "The activity or business of creating public interest in a person, product, or event.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "media_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band is doing a lot of <w>publicity</w> for their new album.",
                            "target_lang_translations": {
                                "ru": "Группа делает большую <w>рекламу</w> своему новому альбому."
                            }
                        },
                        {
                            "text": "It was just a <w>publicity</w> stunt to get more followers.",
                            "target_lang_translations": {
                                "ru": "Это был просто трюк ради <w>паблисити</w>, чтобы набрать больше подписчиков."
                            }
                        }
                    ],
                    "synonyms": [
                        "promotion",
                        "advertising",
                        "marketing",
                        "hype"
                    ],
                    "common_phrases": [
                        "publicity stunt",
                        "publicity campaign",
                        "publicity material",
                        "publicity department"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность по созданию общественного интереса к человеку, товару или событию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реклама",
                                "target_lang_sense_clarification": "распространение информации для привлечения интереса"
                            },
                            {
                                "target_lang_word": "пиар",
                                "target_lang_sense_clarification": "связи с общественностью, формирование имиджа"
                            },
                            {
                                "target_lang_word": "паблисити",
                                "target_lang_sense_clarification": "публичность, достигаемая через СМИ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d74c47d2-d1a3-3102-99b0-b9167281c397",
                    "sense_definition": "The quality or state of being open to public observation or knowledge.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "state_of_openness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>publicity</w> of court proceedings is essential for justice.",
                            "target_lang_translations": {
                                "ru": "<w>Гласность</w> судебного разбирательства необходима для правосудия."
                            }
                        },
                        {
                            "text": "He was uncomfortable with the <w>publicity</w> of the location.",
                            "target_lang_translations": {
                                "ru": "Ему было не по себе от <w>публичности</w> этого места."
                            }
                        }
                    ],
                    "synonyms": [
                        "openness",
                        "visibility"
                    ],
                    "antonyms": [
                        "privacy",
                        "secrecy",
                        "confidentiality"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество или состояние открытости для общественного наблюдения или знания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гласность",
                                "target_lang_sense_clarification": "открытость и доступность информации для общества"
                            },
                            {
                                "target_lang_word": "публичность",
                                "target_lang_sense_clarification": "открытость для посторонних глаз"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
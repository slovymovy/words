                {
                    "sense_id": "c1230969-c9e9-3cd2-bd4e-dbd4630c4096",
                    "sense_definition": "Ogół zwykłych obywateli, często w odniesieniu do niższych warstw społecznych, pracowników fizycznych lub mieszkańców wsi, w przeciwieństwie do elit rządzących lub arystokracji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Politycy często powołują się na wolę <ludu>, aby uzasadnić swoje kontrowersyjne decyzje.",
                            "target_lang_translations": {
                                "ru": "Политики часто ссылаются на волю <w>народа</w>, чтобы оправдать свои спорные решения."
                            }
                        },
                        {
                            "text": "Prosty <lud> wiejski przez wieki kultywował tradycje, które dzisiaj powoli zanikają.",
                            "target_lang_translations": {
                                "ru": "Простой деревенский <w>люд</w> веками хранил традиции, которые сегодня постепенно исчезают."
                            }
                        }
                    ],
                    "synonyms": [
                        "pospólstwo",
                        "masy",
                        "społeczeństwo"
                    ],
                    "antonyms": [
                        "elita",
                        "arystokracja",
                        "szlachta"
                    ],
                    "common_phrases": [
                        "głos ludu",
                        "wola ludu",
                        "prosty lud"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность простых граждан, часто подразумевающая низшие социальные слои или сельских жителей, в противовес правящей элите."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "народ",
                                "target_lang_sense_clarification": "население, простые люди"
                            },
                            {
                                "target_lang_word": "люд",
                                "target_lang_sense_clarification": "собирательное; люди определенной среды или профессии"
                            },
                            {
                                "target_lang_word": "простонародье",
                                "target_lang_sense_clarification": "низшие слои общества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27c88b5e-3355-39ce-b5c6-3b0c335578f3",
                    "sense_definition": "Zbiorowość ludzi połączona wspólnym pochodzeniem, językiem, kulturą i terytorium; grupa etniczna lub plemię.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starożytności wiele <ludów> wędrowało przez Europę w poszukiwaniu nowych ziem do osiedlenia.",
                            "target_lang_translations": {
                                "ru": "В древности многие <w>народы</w> кочевали по Европе в поисках новых земель для поселения."
                            }
                        },
                        {
                            "text": "Badacze odkryli nieznane dotąd zwyczaje <ludów> zamieszkujących dorzecze Amazonki.",
                            "target_lang_translations": {
                                "ru": "Исследователи открыли неизвестные ранее обычаи <w>народов</w>, населяющих бассейн Амазонки."
                            }
                        }
                    ],

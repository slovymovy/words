    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "0413dc8a-ac43-3583-93fb-3b51b79957fa",
                    "sense_definition": "Vanaf het begin tot het einde van een bepaalde tijdsperiode; zolang iets duurt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De reservespeler zat <w>gedurende</w> de eerste helft op de bank.",
                            "target_lang_translations": {
                                "en": "The substitute player sat on the bench <w>throughout</w> the first half.",
                                "ru": "Запасной игрок сидел на скамейке <w>на протяжении</w> первого тайма."
                            }
                        },
                        {
                            "text": "<w>Gedurende</w> een lange tijd bleef hij naar het mooie schilderij kijken.",
                            "target_lang_translations": {
                                "en": "<w>For</w> a long time, he kept looking at the beautiful painting.",
                                "ru": "<w>В течение</w> долгого времени он продолжал смотреть на красивую картину."
                            }
                        }
                    ],
                    "synonyms": [
                        "tijdens"
                    ],
                    "common_phrases": [
                        "gedurende de dag",
                        "gedurende lange tijd",
                        "gedurende de hele periode"
                    ],
                    "target_lang_definitions": {
                        "en": "From the beginning to the end of a specific time period; as long as something lasts.",
                        "ru": "С начала до конца определенного периода времени; пока что-то длится."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throughout",
                                "target_lang_sense_clarification": "from beginning to end"
                            },
                            {
                                "target_lang_word": "during",
                                "target_lang_sense_clarification": "for the entire duration"
                            },
                            {
                                "target_lang_word": "for",
                                "target_lang_sense_clarification": "indicating duration"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в течение",
                                "target_lang_sense_clarification": "на всем протяжении времени"
                            },
                            {
                                "target_lang_word": "на протяжении",
                                "target_lang_sense_clarification": "указывает на длительность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c30461c-31c8-3e45-8060-35bc61eb0bb0",
                    "sense_definition": "Op een bepaald moment binnen het verloop van een periode.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij viel <w>gedurende</w> de saaie film in slaap.",
                            "target_lang_translations": {
                                "en": "He fell asleep <w>during</w> the boring movie.",
                                "ru": "Он заснул <w>во время</w> скучного фильма."
                            }
                        },
                        {
                            "text": "Ze stelden veel vragen <w>gedurende</w> de presentatie.",
                            "target_lang_translations": {
                                "en": "They asked many questions <w>during</w> the presentation.",
                                "ru": "Они задавали много вопросов <w>во время</w> презентации."
                            }
                        }
                    ],
                    "synonyms": [
                        "tijdens",
                        "onder"
                    ],
                    "common_phrases": [
                        "gedurende de vergadering",
                        "gedurende het jaar",
                        "gedurende de rit"
                    ],
                    "target_lang_definitions": {
                        "en": "At a specific moment within the course of a period.",
                        "ru": "В определенный момент в течение периода."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "during",
                                "target_lang_sense_clarification": "at a point in time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "во время",
                                "target_lang_sense_clarification": "в момент события"
                            },
                            {
                                "target_lang_word": "в ходе",
                                "target_lang_sense_clarification": "в процессе"
                            }
                        ]
                    }
                }
            ]
        }
    ]

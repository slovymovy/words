            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c9d18c62-6a37-3226-9c5c-b5f633a287fd",
                    "sense_definition": "Przemieszczać się z jednego miejsca do drugiego, zazwyczaj na dużą odległość, w celach turystycznych lub służbowych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "travel_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice uwielbiają <w>podróżować</w> pociągiem po całej Europie.",
                            "target_lang_translations": {
                                "ru": "Мои родители обожают <w>путешествовать</w> на поезде по всей Европе."
                            }
                        },
                        {
                            "text": "Muszę często <w>podróżować</w> służbowo do innych miast.",
                            "target_lang_translations": {
                                "ru": "Мне приходится часто <w>ездить</w> в командировки в другие города."
                            }
                        }
                    ],
                    "synonyms": [
                        "jeździć",
                        "wędrować",
                        "wojażować"
                    ],
                    "common_phrases": [
                        "podróżować po świecie",
                        "podróżować pociągiem",
                        "podróżować samolotem",
                        "bezpiecznie podróżować",
                        "dużo podróżować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться из одного места в другое, обычно на большое расстояние, с туристической или деловой целью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путешествовать",
                                "target_lang_sense_clarification": "совершать путешествие, бывать в разных местах"
                            },
                            {
                                "target_lang_word": "ездить",
                                "target_lang_sense_clarification": "перемещаться с помощью транспорта, часто регулярно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a8add01-e40a-3b89-a968-ece9edf4c972",
                    "sense_definition": "Pomalować coś lekko na kolor różowy lub nałożyć odrobinę różu kosmetycznego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "color_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Malarz postanowił delikatnie <w>podróżować</w> tło obrazu, aby nadać mu ciepła.",
                            "target_lang_translations": {
                                "ru": "Художник решил слегка <w>подрозовить</w> фон картины, чтобы придать ему теплоты."
                            }
                        },
                        {
                            "text": "Wizażystka zaczęła <w>podróżować</w> policzki modelki przed wyjściem na wybieg.",
                            "target_lang_translations": {
                                "ru": "Визажист начала <w>подрумянивать</w> щеки модели перед выходом на подиум."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rzadkie słowotwórstwo od słowa różowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слегка окрасить в розовый цвет или нанести немного румян."
                    },
                    "translations": {

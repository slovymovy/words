                    "frequency": "Middle",
                    "semantic_group_id": "help_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители продолжают <помогать> студентам деньгами во время учёбы."
                        },
                        {
                            "text": "Наш благотворительный фонд <помогает> нуждающимся семьям продуктами и одеждой."
                        }
                    ],
                    "synonyms": [
                        "оказывать помощь"
                    ],
                    "common_phrases": [
                        "помогать деньгами",
                        "помогать материально",
                        "помогать семье"
                    ]
                }
            ]
        }
    ]

                {
                    "sense_id": "84bb2214-8dd1-370d-9a97-da0938ff3a28",
                    "sense_definition": "Ręczne narzędzie rolnicze o półkolistym, zakrzywionym ostrzu, służące dawniej do ścinania zboża lub trawy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik naostrzył stary <w>sierp</w>, aby ściąć wysoką trawę za stodołą.",
                            "target_lang_translations": {
                                "ru": "Фермер наточил старый <w>серп</w>, чтобы срезать высокую траву за сараем."
                            }
                        },
                        {
                            "text": "Symbol przedstawiający <w>sierp</w> i młot był powszechnie używany w czasach komunizmu.",
                            "target_lang_translations": {
                                "ru": "Символ, изображающий <w>серп</w> и молот, широко использовался во времена коммунизма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sierp i młot",
                        "ostry sierp",
                        "ciąć sierpem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ручное сельскохозяйственное орудие с полукруглым изогнутым лезвием, использовавшееся ранее для срезания зерновых или травы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серп",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db6fde4b-cae2-3f93-a220-dc12406144dc",
                    "sense_definition": "Rodzaj ciosu w boksie zadanego z boku ręką ugiętą w łokciu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bokser zaskoczył przeciwnika szybkim lewym <w>sierpem</w> w szczękę.",
                            "target_lang_translations": {
                                "ru": "Боксер застиг противника врасплох быстрым левым <w>хуком</w> в челюсть."
                            }
                        },
                        {
                            "text": "Trener pokazał zawodnikowi, jak prawidłowo wyprowadzić prawy <w>sierp</w>.",
                            "target_lang_translations": {
                                "ru": "Тренер показал спортсмену, как правильно нанести правый <w>хук</w>."
                            }
                        }
                    ],
                    "synonyms": [

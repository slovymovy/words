    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "c18fb4c9-d13a-369c-b823-101fbe704a1f",
                    "sense_definition": "Op voorwaarde dat; alleen in het geval dat.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je mag vanavond uitgaan, <w>mits</w> je eerst je huiswerk afmaakt.",
                            "target_lang_translations": {
                                "en": "You may go out tonight, <w>provided that</w> you finish your homework first.",
                                "ru": "Ты можешь пойти гулять сегодня вечером, <w>при условии, что</w> сначала сделаешь уроки."
                            }
                        },
                        {
                            "text": "De verzekering keert het bedrag uit, <w>mits</w> de schade binnen 24 uur is gemeld.",
                            "target_lang_translations": {
                                "en": "The insurance pays out the amount, <w>provided that</w> the damage is reported within 24 hours.",
                                "ru": "Страховая компания выплатит сумму <w>при условии, что</w> об ущербе будет заявлено в течение 24 часов."
                            }
                        }
                    ],
                    "antonyms": [
                        "tenzij"
                    ],
                    "target_lang_definitions": {
                        "en": "On the condition that; provided that.",
                        "ru": "При условии, что; только в том случае, если."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "provided that",
                                "target_lang_sense_clarification": "formal condition"
                            },
                            {
                                "target_lang_word": "on condition that",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "if",
                                "target_lang_sense_clarification": "general condition"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "при условии, что",
                                "target_lang_sense_clarification": "формальное условие"
                            },
                            {
                                "target_lang_word": "если",
                                "target_lang_sense_clarification": "общее условие"
                            },
                            {
                                "target_lang_word": "только если",
                                "target_lang_sense_clarification": "ограничительное условие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e389f923-e4ac-3497-8f6c-95ed8e75605b",
                    "sense_definition": "Aangezien; omdat.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kon niet aanwezig zijn, <w>mits</w> hij ziek was.",
                            "target_lang_translations": {
                                "en": "He could not be present, <w>since</w> he was ill.",
                                "ru": "Он не мог присутствовать, <w>поскольку</w> был болен."
                            }
                        },
                        {
                            "text": "De vergadering werd uitgesteld, <w>mits</w> er te weinig deelnemers waren.",
                            "target_lang_translations": {
                                "en": "The meeting was postponed, <w>because</w> there were too few participants.",
                                "ru": "Собрание перенесли, <w>так как</w> было слишком мало участников."
                            }
                        }
                    ],
                    "synonyms": [
                        "vermits"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "wordt in modern Nederlands vaak als onjuist beschouwd"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Since; because (archaic or non-standard usage).",
                        "ru": "Поскольку; так как (устаревшее или нестандартное употребление)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "since",
                                "target_lang_sense_clarification": "reason"
                            },
                            {
                                "target_lang_word": "because",
                                "target_lang_sense_clarification": "reason"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поскольку",
                                "target_lang_sense_clarification": "причина"
                            },
                            {
                                "target_lang_word": "так как",
                                "target_lang_sense_clarification": "причина"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "97d29db7-f60c-38cb-923a-e527fa13cdbf",
                    "sense_definition": "Op voorwaarde van; met als vereiste.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toegang tot het evenement is mogelijk <w>mits</w> betaling aan de deur.",
                            "target_lang_translations": {
                                "en": "Access to the event is possible <w>subject to</w> payment at the door.",
                                "ru": "Вход на мероприятие возможен <w>при условии</w> оплаты на входе."
                            }
                        },
                        {
                            "text": "Het artikel mag worden overgenomen <w>mits</w> duidelijke bronvermelding.",
                            "target_lang_translations": {
                                "en": "The article may be reproduced <w>subject to</w> clear attribution.",
                                "ru": "Статью можно перепечатывать <w>при условии</w> четкого указания источника."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mits betaling",
                        "mits toestemming"
                    ],
                    "target_lang_definitions": {
                        "en": "Subject to; on condition of (followed by a noun).",
                        "ru": "При условии (чего-либо); при наличии (требования)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "subject to",
                                "target_lang_sense_clarification": "followed by a noun"
                            },
                            {
                                "target_lang_word": "upon",
                                "target_lang_sense_clarification": "formal, followed by a noun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "при условии",
                                "target_lang_sense_clarification": "с родительным падежом"
                            },
                            {
                                "target_lang_word": "при",
                                "target_lang_sense_clarification": "с предложным падежом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5ceb87a2-5ce1-31cd-b4f8-85a6083fc837",
                    "sense_definition": "Een voorwaarde, beperking of bezwaar.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het voorstel werd zonder <w>mitsen</w> en maren aangenomen.",
                            "target_lang_translations": {
                                "en": "The proposal was accepted without <w>ifs</w> and buts.",
                                "ru": "Предложение было принято без <w>оговорок</w> и возражений."
                            }
                        },
                        {
                            "text": "Er zitten nogal wat <w>mitsen</w> en maren aan dit ingewikkelde plan.",
                            "target_lang_translations": {
                                "en": "There are quite a few <w>ifs</w> and buts to this complicated plan.",
                                "ru": "В этом сложном плане есть немало <w>оговорок</w> и нюансов."
                            }
                        }
                    ],
                    "common_phrases": [

                    "frequency": "Low",
                    "semantic_group_id": "abuse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник начал <сволочить> подчинённых за малейшую ошибку в отчёте.",
                            "target_lang_translations": {
                                "en": "The boss started <cussing out> the subordinates for the slightest mistake in the report."
                            }
                        },
                        {
                            "text": "Не стоит так <сволочить> человека, не разобравшись в ситуации.",
                            "target_lang_translations": {
                                "en": "You shouldn't <slag off> a person like that without understanding the situation."
                            }
                        }
                    ],
                    "synonyms": [
                        "костерить",
                        "поносить",
                        "ругать",
                        "бранить"
                    ],
                    "antonyms": [
                        "хвалить",
                        "одобрять"
                    ],
                    "common_phrases": [
                        "сволочить на чём свет стоит",
                        "сволочить почём зря"

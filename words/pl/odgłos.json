                {
                    "sense_id": "a7a19438-5317-38e7-af6d-a511e34ff345",
                    "sense_definition": "Dźwięk towarzyszący jakiejś czynności, zjawisku lub wydawany przez żywą istotę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sound_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z oddali dobiegał cichy <w>odgłos</w> jadącego pociągu.",
                            "target_lang_translations": {
                                "ru": "Издалека доносился тихий <w>шум</w> едущего поезда."
                            }
                        },
                        {
                            "text": "W nocy słyszałem niepokojące <w>odgłosy</w> dochodzące z piwnicy.",
                            "target_lang_translations": {
                                "ru": "Ночью я слышал тревожные <w>звуки</w>, доносящиеся из подвала."
                            }
                        },
                        {
                            "text": "Każdy <w>odgłos</w> kroków na korytarzu budził w nim lęk.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>звук</w> шагов в коридоре вызывал у него страх."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odgłos kroków",
                        "odgłosy natury",
                        "dziwny odgłos",
                        "głośny odgłos",
                        "odgłosy walki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, сопровождающий какое-либо действие, явление или издаваемый живым существом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звук",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "шум",
                                "target_lang_sense_clarification": "неопределенный или громкий звук"
                            },
                            {
                                "target_lang_word": "отзвук",
                                "target_lang_sense_clarification": "звук, доносящийся издалека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b776d3c-b062-3669-99fb-1d30c9362d56",
                    "sense_definition": "Reakcja otoczenia na jakieś wydarzenie lub czyjeś postępowanie; oddźwięk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reaction_metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Publikacja tej kontrowersyjnej książki wywołała szeroki <w>odgłos</w> w mediach.",
                            "target_lang_translations": {
                                "ru": "Публикация этой скандальной книги вызвала широкий <w>резонанс</w> в СМИ."
                            }
                        },
                        {
                            "text": "Jego apel o pomoc znalazł żywy <w>odgłos</w> w społeczeństwie.",
                            "target_lang_translations": {
                                "ru": "Его призыв о помощи нашел живой <w>отклик</w> в обществе."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "2c58a147-0546-3be7-a938-c3f28e514129",
                    "sense_definition": "Met als gevolg dat; waardoor een bepaalde situatie ontstaat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "gevolg",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het heeft de hele nacht geregend <zodat> de straten nu nat zijn.",
                            "target_lang_translations": {
                                "en": "It rained all night <w>so that</w> the streets are now wet.",
                                "ru": "Всю ночь шёл дождь, <w>так что</w> улицы теперь мокрые."
                            }
                        },
                        {
                            "text": "De trein had vertraging <zodat> ik te laat op mijn werk kwam.",
                            "target_lang_translations": {
                                "en": "The train was delayed <w>so that</w> I arrived late for work.",
                                "ru": "Поезд задержался, <w>так что</w> я опоздал на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "waardoor"
                    ],
                    "common_phrases": [
                        "zodat het",
                        "zodat er"
                    ],
                    "target_lang_definitions": {
                        "en": "With the result that; causing a specific situation to arise.",
                        "ru": "Следствие; в результате чего возникает определенная ситуация."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "so that",
                                "target_lang_sense_clarification": "indicating consequence"
                            },
                            {
                                "target_lang_word": "so",
                                "target_lang_sense_clarification": "informal, indicating result"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "так что",
                                "target_lang_sense_clarification": "следствие"
                            },
                            {
                                "target_lang_word": "вследствие чего",
                                "target_lang_sense_clarification": "формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d364df7c-45ce-3182-b2ee-2f82680091a0",
                    "sense_definition": "Met de bedoeling dat; om te zorgen dat iets gebeurt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "doel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik spaar elke maand geld <zodat> ik later een huis kan kopen.",
                            "target_lang_translations": {
                                "en": "I save money every month <w>so that</w> I can buy a house later.",
                                "ru": "Я откладываю деньги каждый месяц, <w>чтобы</w> потом купить дом."
                            }
                        },
                        {
                            "text": "Zij vertrok vroeg van huis <zodat> ze de eerste bus kon halen.",
                            "target_lang_translations": {
                                "en": "She left home early <w>so that</w> she could catch the first bus.",
                                "ru": "Она вышла из дома рано, <w>чтобы</w> успеть на первый автобус."
                            }
                        }
                    ],
                    "synonyms": [
                        "opdat"
                    ],
                    "common_phrases": [
                        "zodat ik kan",
                        "zodat je niet",
                        "zodat we"
                    ],
                    "target_lang_definitions": {
                        "en": "With the intention that; to ensure that something happens.",
                        "ru": "С целью; для того, чтобы что-то произошло."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "so that",
                                "target_lang_sense_clarification": "indicating purpose"
                            },
                            {
                                "target_lang_word": "in order that",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чтобы",
                                "target_lang_sense_clarification": "цель"
                            },
                            {
                                "target_lang_word": "для того чтобы",
                                "target_lang_sense_clarification": "цель (усиление)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
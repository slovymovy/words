    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "004bb567-f5d0-3ceb-bdd6-2ed992c33953",
                    "sense_definition": "A short form of <w>champion</w>, referring to a winner of a competition or a highly skilled person.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "champion_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The reigning <w>champ</w> waved to the crowd after winning the match.",
                            "target_lang_translations": {
                                "ru": "Действующий <w>чемпион</w> помахал толпе после победы в матче."
                            }
                        },
                        {
                            "text": "He is the undisputed world <w>champ</w> in heavyweight boxing.",
                            "target_lang_translations": {
                                "ru": "Он бесспорный <w>чемпион</w> мира по боксу в тяжелом весе."
                            }
                        }
                    ],
                    "synonyms": [
                        "poundies"
                    ],
                    "antonyms": [
                        "loser"
                    ],
                    "common_phrases": [
                        "world champ",
                        "defending champ"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "clipping of champion"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от слова \"champion\" (чемпион), обозначающее победителя соревнования или высококвалифицированного человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чемпион",
                                "target_lang_sense_clarification": "победитель"
                            },
                            {
                                "target_lang_word": "чемп",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0824442c-197e-35d6-acaf-899962bb4604",
                    "sense_definition": "A friendly or sometimes patronizing term of address for a man or boy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "champion_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hey <w>champ</w>, are you ready for your first day of school?",
                            "target_lang_translations": {
                                "ru": "Эй, <w>чемпион</w>, ты готов к первому дню в школе?"
                            }
                        },
                        {
                            "text": "Don't worry about the mistake, <w>champ</w>, you will do better next time.",
                            "target_lang_translations": {
                                "ru": "Не переживай из-за ошибки, <w>чемпион</w>, в следующий раз получится лучше."
                            }
                        }
                    ],
                    "synonyms": [
                        "poundies"
                    ],
                    "common_phrases": [
                        "hey champ"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дружеское или иногда снисходительное обращение к мужчине или мальчику."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чемпион",
                                "target_lang_sense_clarification": "обращение"

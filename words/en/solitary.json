    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "79d8ce07-cec1-30a5-81dd-118d7c41c554",
                    "sense_definition": "Living or being by oneself; having no companion present.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "alone_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>solitary</w> figure stood at the edge of the cliff, watching the sunset.",
                            "target_lang_translations": {
                                "ru": "<w>Одинокая</w> фигура стояла на краю обрыва, наблюдая за закатом."
                            }
                        },
                        {
                            "text": "She prefers a <w>solitary</w> existence, far away from the noise of the city.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает <w>уединённый</w> образ жизни, вдали от городского шума."
                            }
                        }
                    ],
                    "synonyms": [
                        "alone",
                        "solo",
                        "friendless",
                        "companionless"
                    ],
                    "antonyms": [
                        "accompanied",
                        "sociable",
                        "gregarious"
                    ],
                    "common_phrases": [
                        "solitary life",
                        "solitary figure",
                        "solitary existence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живущий или находящийся в одиночестве; без спутников или компаньонов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одинокий",
                                "target_lang_sense_clarification": "находящийся один, без других людей (о человеке или фигуре)"
                            },
                            {
                                "target_lang_word": "уединённый",
                                "target_lang_sense_clarification": "скрытый от посторонних, изолированный (об образе жизни)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9bb0487-e494-3f73-b4f6-66fa94626767",
                    "sense_definition": "Performed, passed, or endured without companions.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "alone_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Writing is often a <w>solitary</w> pursuit that requires deep concentration.",
                            "target_lang_translations": {
                                "ru": "Писательство — это часто <w>уединённое</w> занятие, требующее глубокой концентрации."
                            }
                        },
                        {
                            "text": "He went for a <w>solitary</w> walk to clear his head after the argument.",
                            "target_lang_translations": {
                                "ru": "Он отправился на <w>одиночную</w> прогулку, чтобы проветрить голову после ссоры."
                            }
                        }
                    ],
                    "synonyms": [
                        "solo",
                        "lone"
                    ],
                    "antonyms": [
                        "joint",
                        "collective",
                        "shared"
                    ],
                    "common_phrases": [
                        "solitary walk",
                        "solitary pursuit",
                        "solitary journey",
                        "solitary confinement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершаемый или проводимый в одиночестве, без участия других людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одиночный",
                                "target_lang_sense_clarification": "совершаемый силами одного человека"
                            },
                            {
                                "target_lang_word": "уединённый",
                                "target_lang_sense_clarification": "проходящий в тишине и покое, вдали от людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98997efe-7919-3e4f-a00b-d85a50b0a414",
                    "sense_definition": "Single; individual; being the only one.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "single_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was a <w>solitary</w> cloud in the otherwise clear blue sky.",
                            "target_lang_translations": {
                                "ru": "В остальном чистом голубом небе было лишь <w>одинокое</w> облако."
                            }
                        },
                        {
                            "text": "The team managed to score only a <w>solitary</w> goal during the entire match.",
                            "target_lang_translations": {
                                "ru": "Команде удалось забить лишь <w>единственный</w> гол за весь матч."
                            }
                        }
                    ],
                    "synonyms": [
                        "single",
                        "sole",
                        "lone",
                        "only",
                        "individual"
                    ],
                    "antonyms": [
                        "multiple",
                        "numerous",
                        "many"
                    ],
                    "common_phrases": [
                        "solitary example",
                        "solitary instance",
                        "not a solitary word"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единственный; отдельный; существующий в одном экземпляре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "единственный",
                                "target_lang_sense_clarification": "только один"
                            },
                            {
                                "target_lang_word": "одинокий",
                                "target_lang_sense_clarification": "стоящий отдельно от других подобных объектов"
                            },
                            {
                                "target_lang_word": "единичный",
                                "target_lang_sense_clarification": "редкий, встречающийся как отдельный случай"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34663fa9-788d-3320-aa9d-b132157ce5c3",
                    "sense_definition": "Not inhabited or occupied; remote from society; desolate.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "remote_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They built a cabin in a <w>solitary</w> spot near the mountains.",
                            "target_lang_translations": {
                                "ru": "Они построили хижину в <w>уединённом</w> месте недалеко от гор."
                            }
                        },
                        {
                            "text": "The travelers crossed a <w>solitary</w> desert where no one had lived for centuries.",
                            "target_lang_translations": {
                                "ru": "Путешественники пересекли <w>безлюдную</w> пустыню, где никто не жил веками."
                            }
                        }
                    ],
                    "synonyms": [
                        "remote",
                        "isolated",
                        "desolate",
                        "deserted",
                        "uninhabited",
                        "unfrequented"
                    ],
                    "antonyms": [
                        "populous",
                        "crowded",
                        "busy"
                    ],
                    "common_phrases": [
                        "solitary place",
                        "solitary spot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Необитаемый или удалённый от общества; пустынный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уединённый",
                                "target_lang_sense_clarification": "находящийся вдали от людных мест"
                            },
                            {
                                "target_lang_word": "безлюдный",
                                "target_lang_sense_clarification": "необитаемый, пустой"
                            },
                            {
                                "target_lang_word": "пустынный",
                                "target_lang_sense_clarification": "дикий, заброшенный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cbc54858-29f9-33b5-8efc-ab102a8d5697",
                    "sense_definition": "A form of imprisonment where the inmate is isolated from any human contact (short for solitary confinement).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "confinement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prisoner was sent to <w>solitary</w> after starting a fight in the cafeteria.",
                            "target_lang_translations": {
                                "ru": "Заключённого отправили в <w>одиночку</w> после того, как он затеял драку в столовой."
                            }
                        },
                        {
                            "text": "Spending weeks in <w>solitary</w> can have a severe impact on a person's mental health.",
                            "target_lang_translations": {
                                "ru": "Пребывание неделями в <w>одиночной камере</w> может серьёзно сказаться на психическом здоровье человека."
                            }
                        }
                    ],
                    "synonyms": [
                        "solitary confinement",
                        "isolation"
                    ],
                    "common_phrases": [
                        "in solitary",
                        "sent to solitary",
                        "released from solitary"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "ellipsis of solitary confinement"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид тюремного заключения, при котором заключённый полностью изолирован от контактов с людьми (разг. от solitary confinement)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одиночка",
                                "target_lang_sense_clarification": "разговорное название одиночной камеры или заключения"
                            },
                            {
                                "target_lang_word": "карцер",
                                "target_lang_sense_clarification": "изолированная камера для наказаний"
                            },
                            {
                                "target_lang_word": "одиночная камера",
                                "target_lang_sense_clarification": "помещение для содержания одного заключённого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61f18f27-5b32-3bf9-89c8-ad79055cf645",
                    "sense_definition": "One who lives alone or avoids the company of others; a recluse.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was a natural <w>solitary</w> who found peace only when he was away from crowds.",
                            "target_lang_translations": {
                                "ru": "Он был прирождённым <w>отшельником</w>, который обретал покой только вдали от толпы."
                            }
                        },
                        {
                            "text": "The old woman was known as a <w>solitary</w>, rarely seen outside her garden walls.",
                            "target_lang_translations": {
                                "ru": "Старуха была известна как <w>затворница</w>, которую редко видели за стенами её сада."
                            }
                        }
                    ],
                    "synonyms": [
                        "recluse",
                        "hermit",
                        "loner",
                        "introvert"
                    ],
                    "antonyms": [
                        "socialite",
                        "extrovert"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который живёт один или избегает общества других; затворник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отшельник",
                                "target_lang_sense_clarification": "человек, живущий в уединении, часто по религиозным причинам"
                            },
                            {
                                "target_lang_word": "затворник",
                                "target_lang_sense_clarification": "тот, кто не выходит из дома и избегает общения"
                            },
                            {
                                "target_lang_word": "нелюдим",
                                "target_lang_sense_clarification": "замкнутый человек, избегающий общества"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "transport_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The emergency supplies were <w>airplaned</w> to the disaster zone immediately.",
                            "target_lang_translations": {
                                "ru": "Предметы первой необходимости были немедленно <w>доставлены самолётом</w> в зону бедствия."
                            }
                        },
                        {
                            "text": "The company <w>airplaned</w> the fresh seafood to ensure it arrived quickly.",
                            "target_lang_translations": {
                                "ru": "Компания <w>отправила самолётом</w> свежие морепродукты, чтобы обеспечить их быстрое прибытие."
                            }
                        }
                    ],
                    "synonyms": [
                        "airlift",
                        "fly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перевозить что-либо на самолёте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доставлять самолётом",
                                "target_lang_sense_clarification": "транспортировка груза"
                            },
                            {
                                "target_lang_word": "перевозить по воздуху",
                                "target_lang_sense_clarification": "способ доставки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

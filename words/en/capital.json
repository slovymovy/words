                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличный, превосходный или первоклассный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "превосхо́дный",
                                "target_lang_sense_clarification": "отличный"
                            },
                            {
                                "target_lang_word": "отли́чный",
                                "target_lang_sense_clarification": "высшего качества"
                            }
                        ]
                    }
                }
            ]
        },

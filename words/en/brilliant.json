                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tropical bird had <w>brilliant</w> blue feathers.",
                            "target_lang_translations": {
                                "ru": "У тропической птицы были <w>ярко</w>-синие перья."
                            }
                        },
                        {
                            "text": "She wore a dress of <w>brilliant</w> red to the party.",
                            "target_lang_translations": {
                                "ru": "Она надела на вечеринку платье <w>сочного</w> красного цвета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brilliant blue",
                        "brilliant color"
                    ],
                    "target_lang_definitions": {
                        "ru": "Яркий и насыщенный (о цвете)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сочный",
                                "target_lang_sense_clarification": "о насыщенном цвете"
                            },
                            {
                                "target_lang_word": "яркий",
                                "target_lang_sense_clarification": "броский, заметный"
                            }

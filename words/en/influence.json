            "senses": [
                {
                    "sense_id": "98ae6d42-9d68-3a82-8ca1-c2dd5aee3541",
                    "sense_definition": "The power or ability to have an effect on people or things, or to change how something develops.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "power_to_affect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have absolutely no <w>influence</w> over his decisions."
                        },
                        {
                            "text": "The weather had a significant <w>influence</w> on our travel plans."
                        }
                    ],
                    "synonyms": [
                        "impact",
                        "effect",
                        "control",
                        "sway",
                        "authority"
                    ],
                    "antonyms": [
                        "powerlessness"
                    ],
                    "common_phrases": [
                        "have influence over",
                        "under the influence",
                        "sphere of influence",
                        "exercise influence"
                    ]
                },
                {
                    "sense_id": "9d7dceb4-f0f7-3836-aa8b-84113c43c6ec",
                    "sense_definition": "A person or thing that affects someone or something else.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "agent_of_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My older brother was a good <w>influence</w> on me when I was growing up."
                        },
                        {
                            "text": "He has been a great <w>influence</w> on the voters during the recent elections."
                        }
                    ],
                    "synonyms": [
                        "inspiration",
                        "example",
                        "model"
                    ],
                    "common_phrases": [
                        "bad influence",
                        "good influence",
                        "major influence",
                        "positive influence"
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d8997b6-aa00-388e-b5d7-fc9e99c000ec",
                    "sense_definition": "To have an effect on the character, development, or behavior of someone or something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "to_affect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician wants to <w>influence</w> the public with his new speech."
                        },
                        {
                            "text": "I must admit that this book <w>influenced</w> my outlook on life significantly."

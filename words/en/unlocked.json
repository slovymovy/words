{
    "word_family": [
        "lock",
        "unlock",
        "locker",
        "locksmith",
        "lockable",
        "interlock"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ff2e31af-a3f8-3150-bb5b-d512074f4737",
                    "sense_definition": "Not secured or protected by a lock.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_of_security",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She realized she had left the front door <w>unlocked</w> all night.",
                            "target_lang_translations": {
                                "ru": "Она поняла, что оставила входную дверь <w>незапертой</w> на всю ночь."
                            }
                        },
                        {
                            "text": "The safe was found <w>unlocked</w> and empty.",
                            "target_lang_translations": {
                                "ru": "Сейф нашли <w>незапертым</w> и пустым."
                            }
                        }
                    ],
                    "synonyms": [
                        "unsecured",
                        "open"
                    ],
                    "antonyms": [
                        "locked"
                    ],
                    "common_phrases": [
                        "left unlocked",
                        "unlocked door",
                        "remain unlocked"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не закрытый на замок или засов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "незапертый",
                                "target_lang_sense_clarification": "о двери или замке"
                            },
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "доступный для входа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fcf5ed6-8ca5-37c6-a6b1-d5339c609bdc",
                    "sense_definition": "Not restricted to a specific network carrier.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tech_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I bought an <w>unlocked</w> phone so I can use local SIM cards when traveling.",
                            "target_lang_translations": {
                                "ru": "Я купил <w>разблокированный</w> телефон, чтобы использовать местные сим-карты в путешествиях."
                            }
                        },
                        {
                            "text": "Is this device <w>unlocked</w> or tied to a contract?",
                            "target_lang_translations": {
                                "ru": "Это устройство <w>разблокировано</w> или привязано к контракту?"
                            }
                        }
                    ],
                    "synonyms": [
                        "unrestricted"
                    ],
                    "antonyms": [
                        "locked"
                    ],
                    "common_phrases": [
                        "unlocked phone",
                        "factory unlocked",
                        "buy unlocked"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не привязанный к конкретному оператору сотовой связи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разблокированный",
                                "target_lang_sense_clarification": "о телефоне или электронике"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8ed7a7be-8ada-30dc-a551-afaa89bfe85b",
                    "sense_definition": "Simple past and past participle of <w>unlock</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>unlocked</w> the car and got in.",
                            "target_lang_translations": {
                                "ru": "Он <w>открыл</w> машину и сел внутрь."
                            }
                        },
                        {
                            "text": "The mystery was finally <w>unlocked</w> by the detective.",
                            "target_lang_translations": {
                                "ru": "Тайна была наконец <w>раскрыта</w> детективом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "unlocked the door",
                        "unlocked the secrets"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола unlock (отпирать, открывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открыл",
                                "target_lang_sense_clarification": "снял замок"
                            },
                            {
                                "target_lang_word": "разблокировал",
                                "target_lang_sense_clarification": "снял блокировку"
                            },
                            {
                                "target_lang_word": "раскрыл",
                                "target_lang_sense_clarification": "тайну или секрет"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
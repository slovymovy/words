            "senses": [
                {
                    "sense_id": "f6e6cb5a-e1c4-303e-93c7-f46140ba8682",
                    "sense_definition": "Vrouw die in een klooster woont en haar leven aan de godsdienst heeft gewijd.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_roles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude <w>non</w> schudt haar hoofd.",
                            "target_lang_translations": {
                                "ru": "Старая <w>монахиня</w> качает головой.",
                                "en": "The old <w>nun</w> shakes her head."
                            }
                        },
                        {
                            "text": "<w>Nonnen</w> kan men makkelijk herkennen aan hun typische zwart-witte kledij.",
                            "target_lang_translations": {
                                "ru": "<w>Монахинь</w> легко узнать по их типичному черно-белому одеянию.",
                                "en": "<w>Nuns</w> are easily recognized by their typical black and white attire."
                            }
                        }
                    ],
                    "synonyms": [
                        "zuster",
                        "kloosterzuster"
                    ],
                    "antonyms": [
                        "monnik"
                    ],
                    "common_phrases": [
                        "katholieke non",
                        "leven als non"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, живущая в монастыре и посвятившая свою жизнь служению Богу.",
                        "en": "A woman who lives in a convent and has dedicated her life to religion."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монахиня",
                                "target_lang_sense_clarification": "член религиозной общины"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "nun",
                                "target_lang_sense_clarification": "member of a religious community"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a093bb2-79fb-3792-8a99-7f5ae6e3dc1d",
                    "sense_definition": "Dier, zoals een vlinder of vogel, met een uiterlijk dat aan de kleding van een kloosterzuster doet denken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animals_resembling_humans",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>non</w> is een nachtvlinder die vooral in naalbossen voorkomt.",
                            "target_lang_translations": {
                                "ru": "<w>Монашенка</w> — это ночная бабочка, которая встречается преимущественно в хвойных лесах.",
                                "en": "The <w>nun moth</w> is a moth that occurs mainly in coniferous forests."
                            }
                        },
                        {
                            "text": "Vanwege het zwart-witte verenkleed wordt deze eend ook wel <w>non</w> genoemd.",
                            "target_lang_translations": {
                                "ru": "Из-за черно-белого оперения эту утку также называют <w>монашенкой</w>.",
                                "en": "Because of the black and white plumage, this duck is also called a <w>nun</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Животное (бабочка или птица), чья окраска напоминает одеяние монахини.",
                        "en": "An animal, such as a butterfly or bird, with an appearance that resembles a nun's habit."
                    },
                    "translations": {
                        "ru": [
                            {

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d205347b-f296-39e1-aeab-9d492ab75119",
                    "sense_definition": "A unit of distance equal to 1,760 yards or approximately 1.6 kilometers.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "unit_of_length",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nearest gas station is about five <w>miles</w> down the road.",
                            "target_lang_translations": {
                                "ru": "Ближайшая заправка находится примерно в пяти <w>милях</w> по дороге."
                            }
                        },
                        {
                            "text": "She runs three <w>miles</w> every morning to stay fit.",
                            "target_lang_translations": {
                                "ru": "Она пробегает три <w>мили</w> каждое утро, чтобы поддерживать форму."
                            }
                        }
                    ],
                    "common_phrases": [
                        "miles per hour",
                        "miles away",
                        "square mile",
                        "nautical mile"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения расстояния, равная 1760 ярдам или примерно 1,6 километра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миля",
                                "target_lang_sense_clarification": "мера длины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c7a5061-b96a-34a9-9efe-f3501251f163",
                    "sense_definition": "A point earned in a frequent flyer program that can be used to pay for flights or upgrades.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have collected enough <w>miles</w> to get a free ticket to Paris.",
                            "target_lang_translations": {
                                "ru": "Я накопил достаточно <w>миль</w>, чтобы получить бесплатный билет в Париж."
                            }
                        },
                        {
                            "text": "You can earn extra <w>miles</w> by using this credit card.",
                            "target_lang_translations": {
                                "ru": "Вы можете заработать дополнительные <w>мили</w>, используя эту кредитную карту."
                            }
                        }
                    ],
                    "synonyms": [
                        "point"
                    ],
                    "common_phrases": [
                        "air miles",
                        "frequent flyer miles"
                    ],
                    "target_lang_definitions": {
                        "ru": "Балл, начисляемый в программах лояльности авиакомпаний, который можно использовать для оплаты перелетов."

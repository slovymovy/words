    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6a5cfb0c-a12b-3d38-89e8-9936392f4a81",
                    "sense_definition": "Związany z żółwiem, dotyczący tego zwierzęcia lub do niego należący.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na plaży znaleźliśmy pustą <żółwią> skorupę wyrzuconą przez morze.",
                            "target_lang_translations": {
                                "ru": "На пляже мы нашли пустой <черепаший> панцирь, выброшенный морем."
                            }
                        },
                        {
                            "text": "W menu tej egzotycznej restauracji znajduje się zupa <żółwiowa>.",
                            "target_lang_translations": {
                                "ru": "В меню этого экзотического ресторана есть <черепаховый> суп."
                            }
                        }
                    ],
                    "common_phrases": [
                        "żółwia skorupa",
                        "zupa żółwiowa",
                        "żółwie jaja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с черепахой, относящийся к этому животному или принадлежащий ему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "черепаший",
                                "target_lang_sense_clarification": "принадлежащий черепахе"
                            },
                            {
                                "target_lang_word": "черепаховый",
                                "target_lang_sense_clarification": "сделанный из черепахи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b49614a-0512-3a75-9ebc-c5ec321d6534",
                    "sense_definition": "Bardzo powolny, odbywający się w wolnym tempie; cecha kojarzona z powolnością żółwia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prace nad budową nowego mostu posuwają się w <żółwim> tempie.",
                            "target_lang_translations": {
                                "ru": "Работы по строительству нового моста продвигаются в <черепашьем> темпе."
                            }
                        },
                        {
                            "text": "Samochód przed nami poruszał się z <żółwią> prędkością, blokując ruch.",
                            "target_lang_translations": {
                                "ru": "Машина перед нами двигалась с <черепашьей> скоростью, блокируя движение."
                            }
                        }
                    ],
                    "synonyms": [
                        "powolny",
                        "ślamazarny",
                        "ospały"
                    ],
                    "antonyms": [
                        "szybki",
                        "błyskawiczny",
                        "ekspresowy"
                    ],
                    "common_phrases": [
                        "żółwie tempo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень медленный, происходящий в медленном темпе; свойство, ассоциируемое с медлительностью черепахи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "черепаший",
                                "target_lang_sense_clarification": "медленный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "941cd768-6cb7-36dc-96ef-8ac7bee9ba25",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>żółw</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym rezerwacie przyrody żyje wiele gatunków rzadkich <żółwi>.",
                            "target_lang_translations": {
                                "ru": "В этом природном заповеднике обитает много видов редких <черепах>."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie widziałem tak ogromnych <żółwi> morskich.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не видел таких огромных морских <черепах>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного «черепаха»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "черепах",
                                "target_lang_sense_clarification": "форма мн. ч. слова «черепаха»"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1acc39f1-8d79-3b75-a920-dc31d20a0caf",
                    "sense_definition": "The plural form of the noun <w>up</w>, referring to positive events, successes, or high points in life.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_up_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Life is full of <w>ups</w> and downs, but we have to keep moving forward.",
                            "target_lang_translations": {
                                "ru": "Жизнь полна <w>взлётов</w> и падений, но мы должны двигаться вперед."
                            }
                        },
                        {
                            "text": "The team had its <w>ups</w> during the season, winning several important matches.",
                            "target_lang_translations": {
                                "ru": "У команды были свои <w>взлёты</w> в течение сезона, когда она выиграла несколько важных матчей."
                            }
                        }
                    ],
                    "synonyms": [
                        "highs",
                        "peaks",
                        "successes"
                    ],
                    "antonyms": [
                        "downs",
                        "lows",
                        "failures"
                    ],
                    "common_phrases": [
                        "ups and downs"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural of up"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число существительного \"up\", обозначающее положительные события, успехи или лучшие моменты жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взлёты",
                                "target_lang_sense_clarification": "успехи, удачные моменты"
                            },
                            {
                                "target_lang_word": "подъёмы",
                                "target_lang_sense_clarification": "периоды улучшения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98b1c817-8c14-34c7-b279-93da1a996308",
                    "sense_definition": "An abbreviation for Uninterruptible Power Supply, a device that provides emergency power to electronics when the main power source fails.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We installed a <w>UPS</w> to prevent the server from crashing during a blackout.",
                            "target_lang_translations": {
                                "ru": "Мы установили <w>ИБП</w>, чтобы предотвратить сбой сервера во время отключения электричества."
                            }
                        },
                        {
                            "text": "The computer kept running thanks to the <w>UPS</w> even after the lights went out.",
                            "target_lang_translations": {
                                "ru": "Компьютер продолжал работать благодаря <w>бесперебойнику</w> даже после того, как погас свет."
                            }
                        }
                    ],
                    "synonyms": [
                        "battery backup"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура для Uninterruptible Power Supply (источник бесперебойного питания) — устройство для аварийного питания электроники."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ИБП",
                                "target_lang_sense_clarification": "аббревиатура: источник бесперебойного питания"
                            },
                            {
                                "target_lang_word": "бесперебойник",
                                "target_lang_sense_clarification": "разговорное название устройства"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17e0833d-16aa-3ede-8d26-423b93163a69",
                    "sense_definition": "The third-person singular simple present indicative form of the verb <w>up</w>, meaning to increase, raise, or improve something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_up_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>ups</w> the volume whenever his favorite song comes on the radio.",
                            "target_lang_translations": {
                                "ru": "Он <w>прибавляет</w> громкость всякий раз, когда по радио звучит его любимая песня."
                            }
                        },
                        {
                            "text": "The company <w>ups</w> the price of its products every year to match inflation.",
                            "target_lang_translations": {
                                "ru": "Компания <w>повышает</w> цены на свою продукцию каждый год, чтобы соответствовать инфляции."
                            }
                        }
                    ],
                    "synonyms": [
                        "raises",
                        "increases",
                        "boosts"
                    ],
                    "antonyms": [
                        "lowers",
                        "decreases",
                        "reduces"
                    ],
                    "common_phrases": [
                        "ups the ante",
                        "ups the stakes"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "third-person singular of up"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма третьего лица единственного числа глагола \"up\", означающая увеличивать, повышать или улучшать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повышает",
                                "target_lang_sense_clarification": "увеличивает уровень, цену или ставки"
                            },
                            {
                                "target_lang_word": "поднимает",
                                "target_lang_sense_clarification": "делает выше"
                            },
                            {
                                "target_lang_word": "увеличивает",
                                "target_lang_sense_clarification": "делает больше по количеству или размеру"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "93aaa66f-d13b-3e30-ae82-696696867284",

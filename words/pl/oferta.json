    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "90ecbb35-ac76-3031-9aca-adcfee12bd08",
                    "sense_definition": "To, co się komuś proponuje; propozycja zrobienia czegoś, kupna lub sprzedaży.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dostałem wczoraj bardzo ciekawą <ofertę> pracy w dużej firmie.",
                            "target_lang_translations": {
                                "ru": "Вчера я получил очень интересное <w>предложение</w> работы в крупной компании."
                            }
                        },
                        {
                            "text": "To jest <oferta> specjalna, która obowiązuje tylko do końca tygodnia.",
                            "target_lang_translations": {
                                "ru": "Это специальное <w>предложение</w>, которое действует только до конца недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "propozycja"
                    ],
                    "common_phrases": [
                        "oferta pracy",
                        "oferta specjalna",
                        "złożyć ofertę",
                        "przyjąć ofertę",
                        "odrzucić ofertę"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что предлагается кому-либо; предложение сделать что-либо, купить или продать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предложение",
                                "target_lang_sense_clarification": "пропозиция, вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89e59766-9004-308e-b31e-dca205b9d068",
                    "sense_definition": "Zestaw towarów lub usług, które firma lub sklep proponuje klientom do kupienia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sklep ma w swojej <ofercie> szeroki wybór świeżych owoców i warzyw.",
                            "target_lang_translations": {
                                "ru": "В <w>ассортименте</w> магазина представлен широкий выбор свежих фруктов и овощей."
                            }
                        },
                        {
                            "text": "Nasza <oferta> handlowa jest skierowana głównie do małych przedsiębiorstw.",
                            "target_lang_translations": {
                                "ru": "Наше коммерческое <w>предложение</w> ориентировано в основном на малые предприятия."
                            }
                        }
                    ],
                    "synonyms": [
                        "asortyment",
                        "wybór"
                    ],
                    "common_phrases": [
                        "bogata oferta",
                        "w ofercie",
                        "oferta handlowa",
                        "zapoznać się z ofertą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор товаров или услуг, которые компания или магазин предлагает клиентам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ассортимент",
                                "target_lang_sense_clarification": "выбор товаров"
                            },
                            {
                                "target_lang_word": "предложение",
                                "target_lang_sense_clarification": "совокупность услуг на рынке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "369c9e42-9344-3366-8dea-47d4fb680078",
                    "sense_definition": "Oficjalna propozycja zawarcia umowy o ściśle określonych warunkach prawnych i finansowych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma budowlana przygotowała szczegółową <ofertę> przetargową na budowę mostu.",
                            "target_lang_translations": {
                                "ru": "Строительная компания подготовила подробное тендерное <w>предложение</w> на строительство моста."
                            }
                        },
                        {
                            "text": "Ta <oferta> jest wiążąca i wygasa po upływie trzydziestu dni.",
                            "target_lang_translations": {
                                "ru": "Эта <w>оферта</w> является обязательной и истекает через тридцать дней."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oferta przetargowa",
                        "ważność oferty",
                        "oferta wiążąca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное предложение заключить договор на строго определенных юридических и финансовых условиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оферта",
                                "target_lang_sense_clarification": "юридический термин"
                            },
                            {
                                "target_lang_word": "коммерческое предложение",
                                "target_lang_sense_clarification": "деловое предложение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

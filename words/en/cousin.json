        "cousinhood"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0bcc5d8f-9fe7-3778-a9fb-2fd5cff44250",
                    "sense_definition": "The child of your aunt or uncle.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "relation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>cousin</w> and I like to play video games together on weekends.",
                            "target_lang_translations": {
                                "ru": "Мы с моим <w>двоюродным братом</w> любим играть в видеоигры по выходным."
                            }
                        },
                        {
                            "text": "She is my first <w>cousin</w> because her father is my mother's brother.",
                            "target_lang_translations": {
                                "ru": "Она моя <w>двоюродная сестра</w>, потому что её отец — брат моей матери."
                            }
                        }
                    ],
                    "synonyms": [
                        "relative",
                        "relation"
                    ],
                    "common_phrases": [
                        "first cousin",
                        "second cousin",
                        "distant cousin",
                        "cousin by marriage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ребенок вашей тети или вашего дяди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двоюродный брат",
                                "target_lang_sense_clarification": "мужской пол"
                            },
                            {
                                "target_lang_word": "двоюродная сестра",
                                "target_lang_sense_clarification": "женский пол"
                            },
                            {
                                "target_lang_word": "кузен",
                                "target_lang_sense_clarification": "мужской пол, книжное"
                            },
                            {
                                "target_lang_word": "кузина",
                                "target_lang_sense_clarification": "женский пол, книжное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f723e84-8510-37b8-bb67-383069b3618c",
                    "sense_definition": "A person belonging to a group or country that is similar or related to your own.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "relation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We welcomed our Canadian <w>cousins</w> to the conference in London.",
                            "target_lang_translations": {
                                "ru": "Мы приветствовали наших канадских <w>собратьев</w> на конференции в Лондоне."
                            }
                        },
                        {
                            "text": "The festival celebrates the shared history between us and our European <w>cousins</w>.",
                            "target_lang_translations": {
                                "ru": "Фестиваль прославляет общую историю, связывающую нас с нашими европейскими <w>собратьями</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "country cousin",
                        "American cousins"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, принадлежащий к группе или стране, родственной или похожей на вашу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собрат",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "родственник",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c7c893f-e122-3d35-b99a-82f28f00e620",
                    "sense_definition": "Something that is related or very similar to something else.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wolf is a wild <w>cousin</w> of the domestic dog.",
                            "target_lang_translations": {
                                "ru": "Волк — дикий <w>сородич</w> домашней собаки."
                            }
                        },
                        {
                            "text": "This expensive sports car is a faster <w>cousin</w> to the standard model.",
                            "target_lang_translations": {
                                "ru": "Этот дорогой спорткар — более быстрый <w>собрат</w> стандартной модели."
                            }
                        }
                    ],
                    "common_phrases": [
                        "close cousin",
                        "smaller cousin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Животное, растение или предмет, родственные или очень похожие на другие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сородич",
                                "target_lang_sense_clarification": "о животных или растениях"
                            },
                            {
                                "target_lang_word": "собрат",
                                "target_lang_sense_clarification": "о предметах или явлениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72f94fff-d5c2-3e75-a611-8756193cbfdb",
                    "sense_definition": "A formal term used by a monarch to address another monarch or a high-ranking noble.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "relation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The King sent a letter addressing the Duke as his trusty and well-beloved <w>cousin</w>.",
                            "target_lang_translations": {
                                "ru": "Король отправил письмо, обращаясь к герцогу как к своему верному и горячо любимому <w>кузену</w>."
                            }
                        },
                        {
                            "text": "In the play, the Queen greets her rival as her noble <w>cousin</w> despite their conflict.",
                            "target_lang_translations": {
                                "ru": "В пьесе королева приветствует свою соперницу как благородную <w>кузину</w>, несмотря на их конфликт."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical or formal usage by royalty"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Формальное обращение монарха к другому монарху или высокопоставленному дворянину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кузен",
                                "target_lang_sense_clarification": "историческое, формальное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a6214052-be6f-344d-8d20-9325e3981d5a",
                    "sense_definition": "To address someone as cousin or treat them as a relative.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>cousined</w> everyone in the village to make them feel welcome.",
                            "target_lang_translations": {
                                "ru": "Она <w>называла кузенами</w> всех в деревне, чтобы они чувствовали себя желанными гостями."
                            }
                        },
                        {
                            "text": "He <w>cousined</w> his way into the family gathering despite being a stranger.",
                            "target_lang_translations": {
                                "ru": "Он <w>напросился в кузены</w> и попал на семейное собрание, несмотря на то, что был чужаком."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Называть кого-либо кузеном или обращаться с кем-либо как с родственником."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называть кузеном",
                                "target_lang_sense_clarification": "фразовый перевод"
                            },
                            {
                                "target_lang_word": "родниться",
                                "target_lang_sense_clarification": "вести себя как родственник"
                            }
                        ]
                    }
                }
            ]
        },

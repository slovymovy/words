                },
                {
                    "sense_id": "9d6c0a4c-3fb0-3ed4-84c0-69e617f9c64b",
                    "sense_definition": "Produkt wybrakowany, uszkodzony lub niespełniający norm jakościowych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "defect_product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie produkcyjne <w>braki</w> zostały wyrzucone do kontenera.",
                            "target_lang_translations": {
                                "ru": "Весь производственный <w>брак</w> был выброшен в контейнер."
                            }
                        },
                        {
                            "text": "Kontrola jakości odrzuciła ten egzemplarz jako <w>brak</w>.",
                            "target_lang_translations": {
                                "ru": "Контроль качества отбраковал этот экземпляр как <w>брак</w>."
                            }
                        }

{
    "word_family": [
        "fatsoen",
        "fatsoeneren",
        "onfatsoenlijk"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6a30e14b-ced3-394b-9f7c-205bb6fdbb70",
                    "sense_definition": "Van voldoende kwaliteit of zoals men mag verwachten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een lange reis hadden we eindelijk een <w>fatsoenlijke</w> maaltijd.",
                            "target_lang_translations": {
                                "en": "After a long journey, we finally had a <w>decent</w> meal.",
                                "ru": "После долгого путешествия у нас наконец-то была <w>приличная</w> еда."
                            }
                        },
                        {
                            "text": "Het is moeilijk om voor die prijs een <w>fatsoenlijk</w> apparaat te vinden.",
                            "target_lang_translations": {
                                "en": "It is difficult to find a <w>decent</w> device for that price.",
                                "ru": "Трудно найти <w>достойный</w> прибор за эту цену."
                            }
                        }
                    ],
                    "synonyms": [
                        "degelijk",
                        "behoorlijk"
                    ],
                    "antonyms": [
                        "onfatsoenlijk"
                    ],
                    "common_phrases": [
                        "een fatsoenlijke maaltijd",
                        "een fatsoenlijk salaris"
                    ],
                    "target_lang_definitions": {
                        "en": "Of sufficient quality or as one might expect.",
                        "ru": "Достаточного качества или такой, как ожидается; приемлемый."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decent",
                                "target_lang_sense_clarification": "of acceptable quality"
                            },
                            {
                                "target_lang_word": "proper",
                                "target_lang_sense_clarification": "real, genuine, sufficient"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приличный",
                                "target_lang_sense_clarification": "хорошего качества, достойный"
                            },
                            {
                                "target_lang_word": "достойный",
                                "target_lang_sense_clarification": "заслуживающий уважения или внимания"
                            },
                            {
                                "target_lang_word": "сносный",
                                "target_lang_sense_clarification": "терпимый, удовлетворительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b47023d-436c-3710-a059-a061ea918e1c",
                    "sense_definition": "Met goede manieren en normen; eerbaar en netjes.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "moral",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ieder <w>fatsoenlijk</w> mens zou in deze situatie helpen.",
                            "target_lang_translations": {
                                "en": "Any <w>decent</w> person would help in this situation.",
                                "ru": "Любой <w>порядочный</w> человек помог бы в этой ситуации."
                            }
                        },
                        {
                            "text": "Zij komt uit een zeer <w>fatsoenlijke</w> familie.",
                            "target_lang_translations": {
                                "en": "She comes from a very <w>respectable</w> family.",
                                "ru": "Она из очень <w>приличной</w> семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "behoorlijk"
                    ],
                    "antonyms": [
                        "onfatsoenlijk"
                    ],
                    "common_phrases": [
                        "een fatsoenlijk mens",
                        "fatsoenlijke mensen"
                    ],
                    "target_lang_definitions": {
                        "en": "Having good manners and standards; honorable and neat.",
                        "ru": "Обладающий хорошими манерами и моральными принципами; честный и благопристойный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decent",
                                "target_lang_sense_clarification": "morally correct"
                            },
                            {
                                "target_lang_word": "respectable",
                                "target_lang_sense_clarification": "regarded by society as good"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "порядочный",
                                "target_lang_sense_clarification": "честный, морально чистоплотный"
                            },
                            {
                                "target_lang_word": "приличный",
                                "target_lang_sense_clarification": "благовоспитанный, из хорошей среды"
                            },
                            {
                                "target_lang_word": "благовоспитанный",
                                "target_lang_sense_clarification": "имеющий хорошее воспитание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18ade3af-2733-3931-aa76-623dab210cfc",
                    "sense_definition": "Op een nette manier, volgens de etiquette.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet leren om <w>fatsoenlijk</w> te eten als we uitgaan.",
                            "target_lang_translations": {
                                "en": "You must learn to eat <w>properly</w> when we go out.",
                                "ru": "Тебе нужно научиться <w>прилично</w> есть, когда мы выходим в свет."
                            }
                        },
                        {
                            "text": "Hij kan niet eens <w>fatsoenlijk</w> antwoord geven op mijn vraag.",
                            "target_lang_translations": {
                                "en": "He cannot even give a <w>proper</w> answer to my question.",
                                "ru": "Он даже не может дать <w>нормальный</w> ответ на мой вопрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "behoorlijk"
                    ],
                    "antonyms": [
                        "onfatsoenlijk"
                    ],
                    "common_phrases": [
                        "fatsoenlijk eten",
                        "fatsoenlijk praten"
                    ],
                    "target_lang_definitions": {
                        "en": "In a polite way, according to etiquette.",
                        "ru": "Надлежащим образом, вежливо, в соответствии с этикетом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "properly",
                                "target_lang_sense_clarification": "correctly, politely"
                            },
                            {
                                "target_lang_word": "decently",
                                "target_lang_sense_clarification": "in a fitting manner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прилично",
                                "target_lang_sense_clarification": "вежливо, как подобает"
                            },
                            {
                                "target_lang_word": "как следует",
                                "target_lang_sense_clarification": "должным образом"
                            },
                            {
                                "target_lang_word": "по-человечески",
                                "target_lang_sense_clarification": "нормально, как принято у людей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
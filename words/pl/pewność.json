{
    "word_family": [
        "pewny",
        "pewnie",
        "upewnić",
        "zapewnić",
        "zapewnienie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6d603aaf-349a-33ad-a34c-d609d1426dbf",
                    "sense_definition": "Stan psychiczny polegający na całkowitym przekonaniu, że coś jest prawdą; brak wątpliwości.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam stuprocentową <w>pewność</w>, że zamknąłem drzwi przed wyjściem.",
                            "target_lang_translations": {
                                "ru": "У меня стопроцентная <w>уверенность</w> в том, что я закрыл дверь перед уходом."
                            }
                        },
                        {
                            "text": "Musisz mieć absolutną <w>pewność</w>, zanim podejmiesz tę trudną decyzję.",
                            "target_lang_translations": {
                                "ru": "Тебе нужна абсолютная <w>уверенность</w>, прежде чем принять это трудное решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "wiara"
                    ],
                    "antonyms": [
                        "niepewność",
                        "wątpliwość"
                    ],
                    "common_phrases": [
                        "mieć pewność",
                        "z całą pewnością",
                        "nabrać pewności",
                        "dla pewności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психическое состояние, заключающееся в полной убежденности в истинности чего-либо; отсутствие сомнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уверенность",
                                "target_lang_sense_clarification": "отсутствие сомнений"
                            },
                            {
                                "target_lang_word": "убеждённость",
                                "target_lang_sense_clarification": "твердая вера во что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "331250be-aff5-34c1-9d18-1664083777f6",
                    "sense_definition": "Swoboda i zdecydowanie w działaniu, poruszaniu się lub mówieniu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przemawiał z taką <w>pewnością</w>, że wszyscy na sali mu uwierzyli.",
                            "target_lang_translations": {
                                "ru": "Он говорил с такой <w>уверенностью</w>, что все в зале ему поверили."
                            }
                        },
                        {
                            "text": "Jej ruchy na korcie tenisowym cechowała duża siła i <w>pewność</w>.",
                            "target_lang_translations": {
                                "ru": "Её движения на теннисном корте отличались большой силой и <w>уверенностью</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "swoboda",
                        "zdecydowanie"
                    ],
                    "antonyms": [
                        "niepewność"
                    ],
                    "common_phrases": [
                        "pewność siebie",
                        "z dużą pewnością"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свобода и решительность в действиях, движениях или речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уверенность",
                                "target_lang_sense_clarification": "в себе или своих действиях"
                            },
                            {
                                "target_lang_word": "решительность",
                                "target_lang_sense_clarification": "смелость в принятии решений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "661fcfa5-4560-3b7a-94df-8d20e2ed2456",
                    "sense_definition": "Stopień prawdopodobieństwa lub dokładności, z jaką coś jest zgodne z prawdą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "accuracy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Algorytm rozpoznaje twarze z <w>pewnością</w> wynoszącą ponad dziewięćdziesiąt procent.",
                            "target_lang_translations": {
                                "ru": "Алгоритм распознает лица с <w>уверенностью</w> более девяноста процентов."
                            }
                        },
                        {
                            "text": "Te badania nie dają nam wystarczającej <w>pewności</w> statystycznej.",
                            "target_lang_translations": {
                                "ru": "Эти исследования не дают нам достаточной статистической <w>достоверности</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "granice pewności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень вероятности или точности, с которой что-либо соответствует истине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достоверность",
                                "target_lang_sense_clarification": "статистическая или научная"
                            },
                            {
                                "target_lang_word": "уверенность",
                                "target_lang_sense_clarification": "степень вероятности (в алгоритмах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4ae76943-2fdf-3240-ae30-69b440cf08a5",
                    "sense_definition": "Лишённый зрения, неспособный видеть.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_blindness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот <w>слепой</w> музыкант играет на пианино лучше многих зрячих людей.",
                            "target_lang_translations": {
                                "en": "This <w>blind</w> musician plays the piano better than many sighted people."
                            }
                        },
                        {
                            "text": "После аварии он стал полностью <w>слепым</w> и долго учился жить в темноте.",
                            "target_lang_translations": {
                                "en": "After the accident, he became completely <w>blind</w> and spent a long time learning to live in darkness."
                            }
                        }
                    ],
                    "synonyms": [
                        "незрячий"
                    ],
                    "antonyms": [
                        "зрячий"
                    ],
                    "common_phrases": [
                        "слепой от рождения",
                        "слепой на один глаз",
                        "слепой старик"
                    ],
                    "target_lang_definitions": {
                        "en": "Deprived of sight, unable to see."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blind",
                                "target_lang_sense_clarification": "unable to see"
                            },
                            {
                                "target_lang_word": "sightless",
                                "target_lang_sense_clarification": "lacking sight"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "159e5b83-577d-38e6-840a-6542873e90ba",
                    "sense_definition": "Неразумный, безрассудный; не контролируемый разумом (о сильных чувствах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_unreasonable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <w>слепая</w> ревность разрушила их счастливый брак всего за один год.",
                            "target_lang_translations": {
                                "en": "His <w>blind</w> jealousy destroyed their happy marriage in just one year."
                            }
                        },
                        {
                            "text": "Материнская <w>слепая</w> любовь иногда мешает ребёнку стать самостоятельным.",
                            "target_lang_translations": {
                                "en": "Maternal <w>blind</w> love sometimes prevents a child from becoming independent."
                            }
                        }
                    ],
                    "synonyms": [
                        "неразумный",
                        "безрассудный"
                    ],
                    "common_phrases": [
                        "слепая любовь",
                        "слепая ярость",
                        "слепая вера",
                        "слепая ревность"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Unreasonable, reckless; not controlled by reason (regarding strong feelings)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blind",
                                "target_lang_sense_clarification": "unreasonable or unconditional"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85b9c215-7543-3496-8418-53fa93ba3d8c",
                    "sense_definition": "Происходящий без участия зрения (о действиях, методах или проверках).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_method",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Секретарь освоила <w>слепой</w> метод печати на клавиатуре всего за две недели.",
                            "target_lang_translations": {
                                "en": "The secretary mastered <w>touch</w> typing on the keyboard in just two weeks."
                            }
                        },
                        {
                            "text": "Пилоты тренировались выполнять <w>слепой</w> полёт, полагаясь только на приборы.",
                            "target_lang_translations": {
                                "en": "The pilots trained to perform <w>blind</w> flying, relying only on instruments."
                            }
                        }
                    ],
                    "antonyms": [
                        "визуальный"
                    ],
                    "common_phrases": [
                        "слепой метод",
                        "слепая печать",
                        "слепой полёт",
                        "слепой тест"
                    ],
                    "target_lang_definitions": {
                        "en": "Occurring without the use of sight (regarding actions, methods, or checks)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blind",
                                "target_lang_sense_clarification": "done without seeing or prior knowledge"
                            },
                            {
                                "target_lang_word": "touch",
                                "target_lang_sense_clarification": "in 'touch typing'"
                            },
                            {
                                "target_lang_word": "instrument",
                                "target_lang_sense_clarification": "in 'instrument flight'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a015cac-daf0-3dc0-bfcb-9d9c28b80549",
                    "sense_definition": "Не имеющий сквозного прохода или выхода.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "structural_closed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы случайно зашли в <w>слепой</w> переулок и нам пришлось возвращаться назад.",
                            "target_lang_translations": {
                                "en": "We accidentally walked into a <w>blind</w> alley and had to turn back."
                            }
                        },
                        {
                            "text": "Эта агрессивная политика заведёт мирные переговоры в <w>слепой</w> тупик.",
                            "target_lang_translations": {
                                "en": "This aggressive policy will lead the peace negotiations into a <w>dead</w> end."
                            }
                        }
                    ],
                    "common_phrases": [
                        "слепой переулок",
                        "слепая шахта",
                        "слепая кишка"
                    ],
                    "target_lang_definitions": {
                        "en": "Having no through passage or exit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blind",
                                "target_lang_sense_clarification": "closed at one end"
                            },
                            {
                                "target_lang_word": "dead-end",
                                "target_lang_sense_clarification": "leading nowhere"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5e919e3-f26f-3c84-853c-664ae71cd6bc",
                    "sense_definition": "Плохо различимый, нечёткий, с трудом читаемый (о тексте или шрифте).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "visual_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старой книге был очень мелкий и <w>слепой</w> шрифт, от которого болели глаза.",
                            "target_lang_translations": {
                                "en": "The old book had very small and <w>faint</w> print that made my eyes hurt."
                            }
                        },
                        {
                            "text": "Из-за плохой краски документ получился <w>слепым</w> и почти нечитаемым.",
                            "target_lang_translations": {
                                "en": "Due to bad ink, the document turned out <w>illegible</w> and almost unreadable."
                            }
                        }
                    ],
                    "antonyms": [
                        "чёткий",
                        "ясный"
                    ],
                    "common_phrases": [
                        "слепой шрифт",
                        "слепой текст",
                        "слепая копия"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Hard to distinguish, unclear, difficult to read (regarding text or font)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "faint",
                                "target_lang_sense_clarification": "lacking contrast or clarity"
                            },
                            {
                                "target_lang_word": "illegible",
                                "target_lang_sense_clarification": "impossible to read"
                            },
                            {
                                "target_lang_word": "dim",
                                "target_lang_sense_clarification": "not bright or clear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3d4fb1e-ef35-3ffe-bb21-265fcf1344e1",
                    "sense_definition": "Случайный, стихийный, не зависящий от воли и расчёта человека.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_random",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Только <w>слепой</w> случай помог нам избежать этой страшной катастрофы.",
                            "target_lang_translations": {
                                "en": "Only <w>blind</w> chance helped us avoid this terrible catastrophe."
                            }
                        },
                        {
                            "text": "Мы доверились <w>слепой</w> удаче и купили лотерейный билет на последние деньги.",
                            "target_lang_translations": {
                                "en": "We trusted in <w>blind</w> luck and bought a lottery ticket with our last money."
                            }
                        }
                    ],
                    "synonyms": [
                        "случайный",
                        "стихийный"
                    ],
                    "common_phrases": [
                        "слепой случай",
                        "слепая удача",
                        "слепой рок",
                        "слепой жребий"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Accidental, elemental, independent of human will and calculation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blind",
                                "target_lang_sense_clarification": "random or accidental"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "098855ec-1d40-32c4-a041-e69eebc9ce1b",
                    "sense_definition": "Человек, лишённый зрения.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_blindness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Собака-поводырь помогает <w>слепому</w> безопасно переходить через оживлённую улицу.",
                            "target_lang_translations": {
                                "en": "A guide dog helps a <w>blind person</w> cross a busy street safely."
                            }
                        },
                        {
                            "text": "В этом интернате обучают <w>слепых</w> чтению по специальной системе Брайля.",
                            "target_lang_translations": {
                                "en": "In this boarding school, they teach <w>the blind</w> to read using the special Braille system."
                            }
                        }
                    ],
                    "synonyms": [
                        "незрячий",
                        "слепец"
                    ],
                    "antonyms": [
                        "зрячий"
                    ],
                    "common_phrases": [
                        "помощь слепым",
                        "школа для слепых",
                        "общество слепых",
                        "собака для слепого"
                    ],
                    "target_lang_definitions": {
                        "en": "A person deprived of sight."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blind person",
                                "target_lang_sense_clarification": "individual unable to see"
                            },
                            {
                                "target_lang_word": "the blind",
                                "target_lang_sense_clarification": "collective noun"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2193d8da-949f-3792-acd0-d7e96abe9e3e",
                    "sense_definition": "Drobna plamka lub punkt o nieregularnym kształcie, często stanowiąca element wzoru na powierzchni.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_pattern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłam nową sukienkę w kolorowe <w>ciapki</w>.",
                            "target_lang_translations": {
                                "ru": "Я купила новое платье в цветные <w>пятнышки</w>."
                            }
                        },
                        {
                            "text": "Sierść psa była pokryta ciemnymi <w>ciapkami</w>.",
                            "target_lang_translations": {
                                "ru": "Шерсть собаки была покрыта темными <w>пятнышками</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kropka"
                    ],
                    "common_phrases": [
                        "w ciapki",
                        "kolorowe ciapki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкое пятнышко или точка неправильной формы, часто являющаяся элементом узора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пятнышко",
                                "target_lang_sense_clarification": "маленькое пятно"
                            },
                            {
                                "target_lang_word": "крапинка",
                                "target_lang_sense_clarification": "точка, элемент узора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "289bb571-ad45-312c-b33f-b06c59d042fd",
                    "sense_definition": "Niewielka ilość gęstej, lepkiej lub brudzącej substancji; zdrobnienie od słowa <w>ciapa</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na czystym obrusie została tłusta <w>ciapka</w> sosu.",
                            "target_lang_translations": {
                                "ru": "На чистой скатерти осталось жирное <w>пятнышко</w> соуса."
                            }
                        },
                        {
                            "text": "Zetrzyj tę brudną <w>ciapkę</w> z podłogi, zanim zaschnie.",
                            "target_lang_translations": {
                                "ru": "Вытри этот грязный <w>ляп</w> с пола, пока он не засох."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ciapka błota"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое количество густой, липкой или пачкающей субстанции; уменьшительное от слова \"ciapa\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пятнышко",
                                "target_lang_sense_clarification": "небольшое загрязнение"
                            },
                            {
                                "target_lang_word": "ляп",
                                "target_lang_sense_clarification": "разг. комок грязи или густой жидкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5c3a314-4898-35b3-823e-6dad5a0a80cf",
                    "sense_definition": "Osoba mało zaradna, niezdarna lub powolna; zdrobnienie od słowa <w>ciapa</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź taka <w>ciapka</w>, spróbuj to naprawić samodzielnie.",
                            "target_lang_translations": {
                                "ru": "Не будь такой <w>растяпой</w>, попробуй починить это самостоятельно."
                            }
                        },
                        {
                            "text": "Mój brat to straszna <w>ciapka</w>, ciągle o czymś zapomina.",
                            "target_lang_translations": {
                                "ru": "Мой брат — страшная <w>растяпа</w>, он постоянно о чем-то забывает."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ty ciapko"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нерасторопный, неуклюжий или медлительный человек; уменьшительное от слова \"ciapa\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растяпа",
                                "target_lang_sense_clarification": "неуклюжий человек"
                            },
                            {
                                "target_lang_word": "недотёпа",
                                "target_lang_sense_clarification": "нерасторопный человек"
                            }
                        ]
                    }
                }
            ]
        }
    ]

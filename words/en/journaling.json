    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "66757083-c812-3cb5-b3a5-c1681dbb42f0",
                    "sense_definition": "The activity of writing in a diary or journal regularly to record thoughts, experiences, or feelings.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She finds <w>journaling</w> to be a helpful way to process her emotions after a long day.",
                            "target_lang_translations": {
                                "ru": "Она находит <w>ведение дневника</w> полезным способом переработать свои эмоции после долгого дня."
                            }
                        },
                        {
                            "text": "Daily <w>journaling</w> has improved his writing skills and memory.",
                            "target_lang_translations": {
                                "ru": "Ежедневное <w>ведение дневника</w> улучшило его навыки письма и память."
                            }
                        }
                    ],
                    "synonyms": [
                        "diary-keeping"
                    ],
                    "common_phrases": [
                        "daily journaling",
                        "bullet journaling",
                        "gratitude journaling",
                        "journaling habit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность по регулярному ведению дневника для записи мыслей, опыта или чувств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ведение дневника",
                                "target_lang_sense_clarification": "процесс записи мыслей"
                            },
                            {
                                "target_lang_word": "журналинг",
                                "target_lang_sense_clarification": "психологическая практика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32a45ddf-7ac7-3843-972a-2c4619317a18",
                    "sense_definition": "A technique used in computing where a system records changes to a log before writing them to the main file system to ensure data integrity.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "computing_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The database relies on <w>journaling</w> to prevent corruption in case of a system crash.",
                            "target_lang_translations": {
                                "ru": "База данных полагается на <w>журналирование</w> для предотвращения повреждения данных в случае сбоя системы."
                            }
                        },
                        {
                            "text": "File system <w>journaling</w> allows for faster recovery after an unexpected shutdown.",
                            "target_lang_translations": {
                                "ru": "<w>Журналирование</w> файловой системы позволяет быстрее восстановиться после неожиданного выключения."
                            }
                        }
                    ],
                    "synonyms": [
                        "logging"
                    ],
                    "common_phrases": [
                        "database journaling",
                        "file system journaling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Технология в информатике, при которой система записывает изменения в журнал перед их внесением в основную файловую систему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "журналирование",
                                "target_lang_sense_clarification": "в информатике"
                            }
                        ]
                    }
                }
            ]
        },
        {

                            {
                                "target_lang_word": "more genuine",
                                "target_lang_sense_clarification": "comparative of genuine; more authentic or sincere"
                            },
                            {
                                "target_lang_word": "truer",
                                "target_lang_sense_clarification": "comparative of true; more in accordance with fact or reality"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "более настоящий",
                                "target_lang_sense_clarification": "сравнение"
                            },
                            {
                                "target_lang_word": "искреннее",
                                "target_lang_sense_clarification": "о чувствах или поведении"

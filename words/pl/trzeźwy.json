                {
                    "sense_id": "22a544e7-0c06-3862-b9bf-9857d3654aca",
                    "sense_definition": "Taki, który nie pił alkoholu lub nie znajduje się pod jego wpływem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "alcohol_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat obiecał, że wróci dzisiaj do domu całkowicie <w>trzeźwy</w>.",
                            "target_lang_translations": {
                                "ru": "Мой брат пообещал, что вернется сегодня домой совершенно <w>трезвым</w>."
                            }
                        },
                        {
                            "text": "Policjant sprawdził, czy kierowca samochodu jest na pewno <w>trzeźwy</w>.",
                            "target_lang_translations": {
                                "ru": "Полицейский проверил, действительно ли водитель машины <w>трезв</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "pijany",
                        "nietrzeźwy",
                        "wstawiony",
                        "spity",
                        "na bani"
                    ],
                    "common_phrases": [
                        "być całkowicie trzeźwym",
                        "wrócić trzeźwy",
                        "trzeźwy kierowca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто не пил алкоголь или не находится под его влиянием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трезвый",
                                "target_lang_sense_clarification": "не пьяный"

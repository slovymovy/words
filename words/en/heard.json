            "senses": [
                {
                    "sense_id": "4db2ee91-1cac-3069-8ed3-2a40d03c24be",
                    "sense_definition": "That has been perceived aurally or by the ear.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>heard</w> melody was much sweeter than the one I had imagined.",
                            "target_lang_translations": {
                                "ru": "<w>Услышанная</w> мелодия была гораздо приятнее той, что я себе представлял."
                            }
                        },
                        {
                            "text": "She tried to distinguish the <w>heard</w> sounds from the background noise.",
                            "target_lang_translations": {
                                "ru": "Она пыталась отличить <w>услышанные</w> звуки от фонового шума."
                            }
                        }
                    ],
                    "synonyms": [
                        "audible",
                        "perceived"
                    ],
                    "antonyms": [
                        "unheard"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что было воспринято на слух; услышанный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "услышанный",
                                "target_lang_sense_clarification": "воспринятый на слух"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "97b63a3c-7b80-3674-ae65-c7a1ece750f8",
                    "sense_definition": "Used to indicate that a message or instruction has been understood.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please make sure the report is on my desk by noon. — <w>Heard</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, убедитесь, что отчет будет у меня на столе к полудню. — <w>Принято</w>."
                            }
                        },
                        {
                            "text": "We need to evacuate the building immediately. — <w>Heard</w>, we are moving now.",
                            "target_lang_translations": {
                                "ru": "Нам нужно немедленно эвакуировать здание. — <w>Принято</w>, выдвигаемся."
                            }
                        }
                    ],
                    "synonyms": [
                        "understood",
                        "roger",
                        "copy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для подтверждения того, что сообщение или инструкция поняты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принято",

                            },
                            {
                                "target_lang_word": "enclose",
                                "target_lang_sense_clarification": "to surround or close off on all sides"
                            },
                            {
                                "target_lang_word": "hedge in",
                                "target_lang_sense_clarification": "to enclose with a hedge"
                            }

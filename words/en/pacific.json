                {
                    "sense_id": "794dc7e4-f5b9-3916-a090-b9d72c98f652",
                    "sense_definition": "The Pacific Ocean, the largest ocean on Earth, separating Asia and Australia from the Americas.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ocean_name",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "We flew across the <w>Pacific</w> to reach Japan.",
                            "target_lang_translations": {
                                "ru": "Мы перелетели через <w>Тихий океан</w>, чтобы добраться до Японии."
                            }
                        },
                        {
                            "text": "Many unique fish species live deep in the <w>Pacific</w>.",
                            "target_lang_translations": {
                                "ru": "Многие уникальные виды рыб обитают в глубинах <w>Тихого океана</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the Pacific",
                        "across the Pacific",
                        "South Pacific"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тихий океан, самый большой океан на Земле, отделяющий Азию и Австралию от Америки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тихий океан",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                },

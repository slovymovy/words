{
    "word_family": [
        "expansion",
        "expansive",
        "expandable",
        "expander",
        "expansively"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9bf00538-bc87-3df2-bdd0-65841f54d983",
                    "sense_definition": "To become larger in size, volume, or quantity, or to make something larger.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "increase_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most metals <w>expand</w> when they get hot.",
                            "target_lang_translations": {
                                "ru": "Большинство металлов <w>расширяются</w> при нагревании."
                            }
                        },
                        {
                            "text": "The bird <w>expanded</w> its wings to dry them in the sun.",
                            "target_lang_translations": {
                                "ru": "Птица <w>расправила</w> крылья, чтобы просушить их на солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "enlarge",
                        "spread out",
                        "open out",
                        "unfold",
                        "spread"
                    ],
                    "antonyms": [
                        "contract",
                        "collapse"
                    ],
                    "common_phrases": [
                        "expand rapidly",
                        "expand and contract"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться или делать больше по размеру, объему или количеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расширяться",
                                "target_lang_sense_clarification": "становиться шире или больше (о предметах, веществах)"
                            },
                            {
                                "target_lang_word": "расширять",
                                "target_lang_sense_clarification": "делать шире или больше"
                            },
                            {
                                "target_lang_word": "увеличиваться",
                                "target_lang_sense_clarification": "расти в размере или объеме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b87f93ba-4616-3209-a398-94997796e0b5",
                    "sense_definition": "To increase the scope, extent, or importance of something, such as a business or activity.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "increase_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company plans to <w>expand</w> its operations into Europe next year.",
                            "target_lang_translations": {
                                "ru": "Компания планирует <w>расширить</w> свою деятельность в Европе в следующем году."
                            }
                        },
                        {
                            "text": "Reading books helps to <w>expand</w> your vocabulary.",
                            "target_lang_translations": {
                                "ru": "Чтение книг помогает <w>расширить</w> ваш словарный запас."
                            }
                        }
                    ],
                    "synonyms": [
                        "enlarge"
                    ],
                    "antonyms": [
                        "limit"
                    ],
                    "common_phrases": [
                        "expand the business",
                        "expand the range",
                        "expand operations"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличивать масштаб, охват или значимость чего-либо (например, бизнеса или деятельности)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расширять",
                                "target_lang_sense_clarification": "увеличивать охват (бизнеса, знаний)"
                            },
                            {
                                "target_lang_word": "развивать",
                                "target_lang_sense_clarification": "улучшать и увеличивать масштаб деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20f0169b-87f3-3c70-9977-1dcf86f5882a",
                    "sense_definition": "To show the hidden subentries of a list or folder in a graphical user interface.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Click the arrow to <w>expand</w> the menu and see more options.",
                            "target_lang_translations": {
                                "ru": "Нажмите на стрелку, чтобы <w>развернуть</w> меню и увидеть больше опций."
                            }
                        },
                        {
                            "text": "You need to <w>expand</w> the file tree to find the document.",
                            "target_lang_translations": {
                                "ru": "Вам нужно <w>раскрыть</w> дерево файлов, чтобы найти документ."
                            }
                        }
                    ],
                    "antonyms": [
                        "collapse"
                    ],
                    "common_phrases": [
                        "click to expand",
                        "expand the folder"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывать скрытые элементы списка или папки в компьютерном интерфейсе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развернуть",
                                "target_lang_sense_clarification": "открыть содержимое папки или меню"
                            },
                            {
                                "target_lang_word": "раскрыть",
                                "target_lang_sense_clarification": "сделать видимым скрытый список"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "741e6027-7538-3d9a-92f9-2613eaf5677f",
                    "sense_definition": "To speak or write about something in greater detail.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Could you please <w>expand</w> on your earlier statement?",
                            "target_lang_translations": {
                                "ru": "Не могли бы вы <w>раскрыть</w> подробнее ваше предыдущее утверждение?"
                            }
                        },
                        {
                            "text": "The author <w>expands</w> the simple plot into a complex novel.",
                            "target_lang_translations": {
                                "ru": "Автор <w>развивает</w> простой сюжет в сложный роман."
                            }
                        }
                    ],
                    "synonyms": [
                        "elaborate",
                        "expand on"
                    ],
                    "common_phrases": [
                        "expand on the idea",
                        "expand on a topic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить или писать о чем-либо более подробно и обстоятельно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрывать",
                                "target_lang_sense_clarification": "подробно описывать тему или мысль"
                            },
                            {
                                "target_lang_word": "развивать",
                                "target_lang_sense_clarification": "дополнять сказанное новыми деталями"
                            },
                            {
                                "target_lang_word": "подробно излагать",
                                "target_lang_sense_clarification": "описывать в деталях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8146b222-dc74-3d50-9c12-35a2d9a2f874",
                    "sense_definition": "To rewrite a mathematical expression as a longer sum of terms.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Students learned how to <w>expand</w> binomial expressions in algebra class.",
                            "target_lang_translations": {
                                "ru": "Ученики учились <w>раскрывать</w> биномиальные выражения на уроке алгебры."
                            }
                        },
                        {
                            "text": "If you <w>expand</w> this bracket, you get a quadratic equation.",
                            "target_lang_translations": {
                                "ru": "Если <w>раскрыть</w> эту скобку, получится квадратное уравнение."
                            }
                        }
                    ],
                    "antonyms": [
                        "factor"
                    ],
                    "common_phrases": [
                        "expand the equation",
                        "expand the brackets"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Преобразовывать математическое выражение, раскрывая скобки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрывать",
                                "target_lang_sense_clarification": "математическое действие со скобками"
                            },
                            {
                                "target_lang_word": "раскладывать",
                                "target_lang_sense_clarification": "представлять в виде суммы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
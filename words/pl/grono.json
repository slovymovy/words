    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e757d9b4-4d88-3e7b-a9e4-c05a62630676",
                    "sense_definition": "Grupa osób połączonych wspólnymi zainteresowaniami, celem, pracą lub więzami towarzyskimi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Święta Bożego Narodzenia spędziliśmy w ścisłym rodzinnym <w>gronie</w>.",
                            "target_lang_translations": {
                                "ru": "Мы провели Рождество в тесном семейном <w>кругу</w>."
                            }
                        },
                        {
                            "text": "Do projektu zaproszono szerokie <w>grono</w> specjalistów z całej Europy.",
                            "target_lang_translations": {
                                "ru": "К проекту был приглашен широкий <w>круг</w> специалистов со всей Европы."
                            }
                        }
                    ],
                    "synonyms": [
                        "pęczek",
                        "wiązka"
                    ],
                    "common_phrases": [
                        "w gronie rodzinnym",
                        "grono pedagogiczne",
                        "szerokie grono",
                        "wąskie grono",
                        "w gronie przyjaciół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, объединенных общими интересами, целью, работой или дружескими связями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "группа людей, общество"
                            },
                            {
                                "target_lang_word": "коллектив",
                                "target_lang_sense_clarification": "группа сотрудников"
                            },
                            {
                                "target_lang_word": "группа",
                                "target_lang_sense_clarification": "совокупность лиц"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a112be9a-3d8e-356f-aed9-39e437c99c6a",
                    "sense_definition": "Skupisko owoców, zwykle drobnych, rosnących blisko siebie na jednej wspólnej szypułce.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fruit_cluster",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na talerzu leżało słodkie <w>grono</w> winogron, po które wszyscy chętnie sięgali.",
                            "target_lang_translations": {
                                "ru": "На тарелке лежала сладкая <w>гроздь</w> винограда, за которой все охотно тянулись."
                            }
                        },
                        {
                            "text": "Z krzaka zwisało ciężkie, dojrzałe <w>grono</w> czerwonych porzeczek.",
                            "target_lang_translations": {
                                "ru": "С куста свисала тяжелая, спелая <w>гроздь</w> красной смородины."
                            }
                        }
                    ],
                    "synonyms": [
                        "kiść"
                    ],
                    "common_phrases": [
                        "grono winogron",
                        "zerwać grono"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скопление плодов, обычно мелких, растущих близко друг к другу на одной общей плодоножке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гроздь",
                                "target_lang_sense_clarification": "плодов или ягод"
                            },
                            {
                                "target_lang_word": "кисть",
                                "target_lang_sense_clarification": "ягод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a88be17-11ac-3d3f-8145-21221d16c93f",
                    "sense_definition": "Typ kwiatostanu, w którym pojedyncze kwiaty osadzone są na szypułkach wzdłuż osi głównej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Konwalia majowa jest przykładem rośliny, która wytwarza kwiatostan w formie <w>grona</w>.",
                            "target_lang_translations": {
                                "ru": "Ландыш майский — пример растения, образующего соцветие в форме <w>кисти</w>."
                            }
                        },
                        {
                            "text": "Na lekcji biologii uczniowie rysowali schemat budowy <w>grona</w> u różnych gatunków.",
                            "target_lang_translations": {
                                "ru": "На уроке биологии ученики рисовали схему строения <w>кисти</w> у различных видов."
                            }
                        }
                    ],
                    "synonyms": [
                        "kiść"
                    ],
                    "common_phrases": [
                        "grono proste",
                        "grono złożone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип соцветия, в котором отдельные цветки расположены на цветоножках вдоль главной оси."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кисть",
                                "target_lang_sense_clarification": "тип соцветия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9e60712-02ce-3379-89cb-ef2f8691a908",
                    "sense_definition": "Kuliste lub zaoblone zakończenie tylnej części lufy w dawnych działach artyleryjskich.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military_history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum wojskowym przewodnik pokazał nam armatę z charakterystycznym <w>gronem</w>.",
                            "target_lang_translations": {
                                "ru": "В военном музее гид показал нам пушку с характерным <w>винградом</w>."
                            }
                        },
                        {
                            "text": "Dawni artylerzyści wykorzystywali <w>grono</w> do zaczepiania lin podczas manewrowania działem.",
                            "target_lang_translations": {
                                "ru": "Артиллеристы прошлого использовали <w>винград</w> для закрепления канатов при маневрировании орудием."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "termin historyczny dotyczący uzbrojenia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шаровидное или закругленное окончание казенной части ствола старинных артиллерийских орудий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "винград",
                                "target_lang_sense_clarification": "часть пушки"
                            }
                        ]
                    }
                }
            ]
        }

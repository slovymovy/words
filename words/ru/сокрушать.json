    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "37e9d4c8-6d6c-3f38-9b07-3a9eb15101f8",
                    "sense_definition": "Полностью уничтожать, побеждать или лишать силы (врага, государство, надежды, аргументы).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "destruction_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Армия готовилась <w>сокрушать</w> любого противника на своём пути.",
                            "target_lang_translations": {
                                "en": "The army was preparing to <w>crush</w> any adversary on its path."
                            }
                        },
                        {
                            "text": "Эта новость может окончательно <w>сокрушать</w> наши надежды на успех.",
                            "target_lang_translations": {
                                "en": "This news could finally <w>shatter</w> our hopes for success."
                            }
                        },
                        {
                            "text": "Он умел <w>сокрушать</w> доводы оппонентов одной меткой фразой.",
                            "target_lang_translations": {
                                "en": "He knew how to <w>smash</w> opponents' arguments with a single apt phrase."
                            }
                        }
                    ],
                    "synonyms": [
                        "уничтожать",
                        "ниспровергать",
                        "разрушать"
                    ],
                    "antonyms": [
                        "созидать",
                        "упрочивать",
                        "создавать"
                    ],
                    "common_phrases": [
                        "сокрушать врага",
                        "сокрушать надежды",
                        "сокрушать основы",
                        "сокрушать препятствия"
                    ],
                    "target_lang_definitions": {
                        "en": "To completely destroy, defeat, or deprive of power (an enemy, a state, hopes, arguments)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crush",
                                "target_lang_sense_clarification": "to defeat an enemy or destroy hopes/resistance completely"
                            },
                            {
                                "target_lang_word": "shatter",
                                "target_lang_sense_clarification": "to destroy hopes, illusions, or peace"
                            },
                            {
                                "target_lang_word": "smash",
                                "target_lang_sense_clarification": "to defeat overwhelmingly or destroy arguments"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "498b2adf-ffb7-3798-9b87-634b4afbd187",
                    "sense_definition": "Сильно ломать, разбивать на части или разрушать что-либо твёрдое и прочное.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "destruction_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ураган начал безжалостно <w>сокрушать</w> старые деревянные постройки.",
                            "target_lang_translations": {
                                "en": "The hurricane began to ruthlessly <w>smash</w> the old wooden buildings."
                            }
                        },
                        {
                            "text": "Солдаты ворвались в город и принялись <w>сокрушать</w> дома мирных жителей.",
                            "target_lang_translations": {
                                "en": "Soldiers burst into the city and began to <w>destroy</w> the homes of civilians."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрушать",
                        "разрушать",
                        "разрушать"
                    ],
                    "antonyms": [
                        "создавать",
                        "созидать"
                    ],
                    "common_phrases": [
                        "сокрушать стены",
                        "сокрушать скалы",
                        "сокрушать идолов"
                    ],
                    "target_lang_definitions": {
                        "en": "To violently break, smash into pieces, or destroy something hard and solid."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smash",
                                "target_lang_sense_clarification": "to break something violently"
                            },
                            {
                                "target_lang_word": "shatter",
                                "target_lang_sense_clarification": "to break into many pieces"
                            },
                            {
                                "target_lang_word": "crush",
                                "target_lang_sense_clarification": "to destroy by pressure or impact"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1986dc4-fa7b-3939-8fab-26d8acd20687",
                    "sense_definition": "Причинять глубокую скорбь, сильно печалить или огорчать.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зачем вы плачете и <w>сокрушаете</w> сердце моё своими жалобами?",
                            "target_lang_translations": {
                                "en": "Why do you weep and <w>break</w> my heart with your complaints?"
                            }
                        },
                        {
                            "text": "Его необдуманные поступки продолжают <w>сокрушать</w> родителей.",
                            "target_lang_translations": {
                                "en": "His thoughtless actions continue to <w>grieve</w> his parents."
                            }
                        }
                    ],
                    "synonyms": [
                        "уничтожать"
                    ],
                    "common_phrases": [

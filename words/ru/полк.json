    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c78bcca-cffb-32bb-a348-8c444a50efac",
                    "sense_definition": "Воинская часть, состоящая из нескольких батальонов или дивизионов, обычно входящая в состав дивизии или бригады.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "military_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир <w>полка</w> отдал приказ о наступлении на рассвете.",
                            "target_lang_translations": {
                                "en": "The commander of the <w>regiment</w> gave the order to attack at dawn."
                            }
                        },
                        {
                            "text": "В нашем <w>полку</w> служат солдаты из самых разных регионов страны.",
                            "target_lang_translations": {
                                "en": "Soldiers from very different regions of the country serve in our <w>regiment</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "воинская часть"
                    ],
                    "common_phrases": [
                        "командир полка",
                        "штаб полка",
                        "служить в полку"
                    ],
                    "target_lang_definitions": {
                        "en": "A military unit consisting of several battalions or divisions, usually forming part of a division or brigade."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "regiment",
                                "target_lang_sense_clarification": "military unit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7d074d9-058d-3c4c-bff5-90ef207bb017",
                    "sense_definition": "Большое количество, множество людей или предметов (переносное значение).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "multitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На кухне собрался целый <w>полк</w> голодных гостей, ожидающих ужина.",
                            "target_lang_translations": {
                                "en": "A whole <w>regiment</w> of hungry guests gathered in the kitchen, waiting for dinner."
                            }
                        },
                        {
                            "text": "У неё в шкафу стоит <w>полк</w> старых туфель, которые она никогда не носит.",
                            "target_lang_translations": {
                                "en": "She has a <w>regiment</w> of old shoes in her closet that she never wears."
                            }
                        }
                    ],
                    "common_phrases": [
                        "целый полк"
                    ],
                    "target_lang_definitions": {
                        "en": "A large quantity, a multitude of people or objects (figurative meaning)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "regiment",
                                "target_lang_sense_clarification": "large number (figurative)"
                            },
                            {
                                "target_lang_word": "army",
                                "target_lang_sense_clarification": "large number (figurative)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a96a6b1b-9437-3b9c-a360-0bfb6620ae1c",
                    "sense_definition": "Военный поход, бой или сражение (в древнерусском языке).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "campaign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "«Слово о <w>полку</w> Игореве» — известнейший памятник древнерусской литературы.",
                            "target_lang_translations": {
                                "en": "\"The Tale of Igor's <w>Campaign</w>\" is the most famous monument of Old Russian literature."
                            }
                        },
                        {
                            "text": "Князь собрал дружину и выступил в <w>полк</w> против кочевников.",
                            "target_lang_translations": {
                                "en": "The prince gathered his retinue and set out on a <w>campaign</w> against the nomads."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Слово о полку Игореве"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A military campaign, battle, or engagement (in Old Russian)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "campaign",
                                "target_lang_sense_clarification": "military expedition (archaic)"
                            },
                            {
                                "target_lang_word": "host",

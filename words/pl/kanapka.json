    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "04daad97-1e9e-31fb-b031-b826d346c181",
                    "sense_definition": "Kromka pieczywa posmarowana masłem lub innym tłuszczem, z dodatkiem wędliny, sera, warzyw itp., albo dwie kromki złożone z nadzieniem w środku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano jem na śniadanie <kanapkę> z żółtym serem i pomidorem.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я ем на завтрак <бутерброд> с сыром и помидором."
                            }
                        },
                        {
                            "text": "Mama przygotowała mi pyszną <kanapkę> z szynką do szkoły.",
                            "target_lang_translations": {
                                "ru": "Мама приготовила мне в школу вкусный <бутерброд> с ветчиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "sandwicz (składany)",
                        "tartinka (pojedyncza)",
                        "gw. (Poznań) sznytka",
                        "gw. (Górny Śląsk) klapsznita"
                    ],
                    "common_phrases": [
                        "kanapka z serem",
                        "kanapka z szynką",
                        "robić kanapki",
                        "kanapka do szkoły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ломтик хлеба, намазанный маслом или другим жиром, с добавлением ветчины, сыра, овощей и т.п., либо два ломтика с начинкой посередине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бутерброд",
                                "target_lang_sense_clarification": "ломтик хлеба с закуской"
                            },
                            {
                                "target_lang_word": "сэндвич",
                                "target_lang_sense_clarification": "закрытый бутерброд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce941a71-fb89-31dd-abe3-2263399e5d69",
                    "sense_definition": "Zdrobnienie od: <w>kanapa</w>; mały, często ozdobny mebel do siedzenia.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W rogu salonu stoi mała, wygodna <kanapka>, na której lubi spać nasz kot.",
                            "target_lang_translations": {
                                "ru": "В углу гостиной стоит маленький удобный <диванчик>, на котором любит спать наш кот."
                            }
                        },
                        {
                            "text": "Po obiedzie miło jest poleżeć na <kanapce> przed telewizorkiem.",
                            "target_lang_translations": {
                                "ru": "После обеда приятно полежать на <диванчике> перед телевизором."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова kanapa; маленький, часто декоративный предмет мебели для сидения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диванчик",
                                "target_lang_sense_clarification": "маленький диван"

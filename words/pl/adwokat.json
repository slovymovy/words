    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "92682d0f-bceb-371a-b791-86c3a07f772c",
                    "sense_definition": "Prawnik uprawniony do udzielania porad prawnych oraz reprezentowania klientów przed sądem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legal_professional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <adwokat> poradził mi, abym nie odpowiadał na żadne pytania bez jego obecności.",
                            "target_lang_translations": {
                                "ru": "Мой <адвокат> посоветовал мне не отвечать ни на какие вопросы без его присутствия."
                            }
                        },
                        {
                            "text": "Sąd przyznał oskarżonemu <adwokata> z urzędu, ponieważ nie było go stać na prywatną obronę.",
                            "target_lang_translations": {
                                "ru": "Суд назначил обвиняемому государственного <адвоката>, так как он не мог позволить себе частную защиту."
                            }
                        }
                    ],
                    "synonyms": [
                        "mecenas",
                        "obrońca",
                        "prawnik"
                    ],
                    "antonyms": [
                        "prokurator"
                    ],
                    "common_phrases": [
                        "adwokat z urzędu",
                        "wynająć adwokata",
                        "dobry adwokat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юрист, имеющий право давать юридические консультации и представлять клиентов в суде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адвокат",
                                "target_lang_sense_clarification": "юрист"
                            },
                            {
                                "target_lang_word": "защитник",
                                "target_lang_sense_clarification": "в суде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a3e29e6-1aad-307e-b769-e4dde96eca3a",
                    "sense_definition": "Osoba, która popiera jakąś sprawę, ideę lub broni kogoś w dyskusji, nie pełniąc przy tym funkcji prawnej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "supporter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie prosiłem cię, żebyś był moim <adwokatem> w sporze z rodzicami.",
                            "target_lang_translations": {
                                "ru": "Я не просил тебя быть моим <адвокатом> в споре с родителями."
                            }
                        },
                        {
                            "text": "W tej dyskusji postanowiłem wystąpić jako <adwokat> diabła, aby sprawdzić wasze argumenty.",
                            "target_lang_translations": {
                                "ru": "В этой дискуссии я решил выступить <адвокатом> дьявола, чтобы проверить ваши аргументы."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzecznik",
                        "obrońca"
                    ],
                    "common_phrases": [
                        "adwokat diabła",
                        "adwokat sprawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который поддерживает какое-либо дело, идею или защищает кого-то в споре, не выполняя при этом юридических функций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адвокат",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "защитник",
                                "target_lang_sense_clarification": "сторонник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ca1178c-9e87-311a-bd0f-4cb0f74f6678",
                    "sense_definition": "Gęsty, słodki likier jajeczny o żółtej barwie, zazwyczaj na bazie spirytusu lub winiaku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "alcohol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do popołudniowej kawy babcia podała nam po kieliszku domowego <adwokata>.",
                            "target_lang_translations": {
                                "ru": "К послеобеденному кофе бабушка подала нам по рюмке домашнего <яичного ликера>."
                            }
                        },
                        {
                            "text": "Ten tort jest przełożony kremem z dodatkiem gęstego <adwokata>.",
                            "target_lang_translations": {
                                "ru": "Этот торт прослоен кремом с добавлением густого <яичного ликера>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ajerkoniak"
                    ],
                    "common_phrases": [
                        "kieliszek adwokata",
                        "domowy adwokat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Густой сладкий яичный ликер желтого цвета, обычно на основе спирта или бренди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яичный ликер",
                                "target_lang_sense_clarification": "напиток"
                            },
                            {
                                "target_lang_word": "адвокат",
                                "target_lang_sense_clarification": "название ликера"
                            }
                        ]
                    }
                }
            ]
        }
    ]

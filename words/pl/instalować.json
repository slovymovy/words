    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07f1fc92-38ef-3c21-a60b-fa7371772d22",
                    "sense_definition": "Umieszczać i konfigurować program komputerowy lub aplikację na urządzeniu elektronicznym, aby były gotowe do użycia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tech_software",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <w>zainstalować</w> nowy program antywirusowy, ponieważ stary już wygasł.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>установить</w> новую антивирусную программу, так как срок действия старой уже истек."
                            }
                        },
                        {
                            "text": "Nie powinieneś <w>instalować</w> gier na służbowym komputerze bez zgody szefa.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <w>устанавливать</w> игры на рабочем компьютере без разрешения начальника."
                            }
                        }
                    ],
                    "synonyms": [
                        "wgrywać"
                    ],
                    "antonyms": [
                        "odinstalowywać",
                        "dezinstalować",
                        "odinstalować"
                    ],
                    "common_phrases": [
                        "instalować system operacyjny",
                        "instalować aplikację",
                        "instalować sterowniki",
                        "instalować aktualizację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Размещать и настраивать компьютерную программу или приложение на электронном устройстве, чтобы они были готовы к использованию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "программы, приложения"
                            },
                            {
                                "target_lang_word": "инсталлировать",
                                "target_lang_sense_clarification": "ПО (технический термин)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08b5a5dd-79bd-3692-b431-a3dd865e9892",
                    "sense_definition": "Montować urządzenie techniczne, maszynę lub instalację w określonym miejscu, podłączając je tak, aby działało.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tech_hardware",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro przyjdą fachowcy, aby <w>zainstalować</w> nam szybki internet światłowodowy.",
                            "target_lang_translations": {
                                "ru": "Завтра придут специалисты, чтобы <w>провести</w> нам быстрый оптоволоконный интернет."
                            }
                        },
                        {
                            "text": "Ojciec uczył mnie, jak poprawnie <w>instalować</w> gniazdka elektryczne w ścianie.",
                            "target_lang_translations": {
                                "ru": "Отец учил меня, как правильно <w>устанавливать</w> электрические розетки в стене."
                            }
                        }
                    ],
                    "synonyms": [
                        "montować"
                    ],
                    "antonyms": [
                        "dezinstalować"
                    ],
                    "common_phrases": [
                        "instalować oświetlenie",
                        "instalować klimatyzację",
                        "instalować alarm",
                        "instalować sieć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Монтировать техническое устройство, механизм или систему в определенном месте, подключая их для работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "оборудование, технику"
                            },
                            {
                                "target_lang_word": "проводить",
                                "target_lang_sense_clarification": "сети, коммуникации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "144e5bab-db49-3aef-8095-22335ebc9c59",
                    "sense_definition": "Zajmować miejsce, rozgaszczać się lub wprowadzać do nowego pomieszczenia (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "reflexive_settle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Goście zaczęli powoli <w>instalować się</w> w swoich pokojach hotelowych po długiej podróży.",
                            "target_lang_translations": {
                                "ru": "Гости начали потихоньку <w>располагаться</w> в своих гостиничных номерах после долгого путешествия."
                            }
                        },
                        {
                            "text": "Mój kot lubi <w>instalować się</w> na fotelu, gdy tylko z niego wstanę.",
                            "target_lang_translations": {
                                "ru": "Мой кот любит <w>устраиваться</w> на кресле, как только я с него встану."
                            }
                        }
                    ],
                    "common_phrases": [
                        "instalować się w fotelu",
                        "instalować się w hotelu",
                        "instalować się wygodnie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used humorously or casually"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Занимать место, располагаться или устраиваться в новом помещении (часто с оттенком иронии или о комфорте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "располагаться",
                                "target_lang_sense_clarification": "занимать место"
                            },
                            {
                                "target_lang_word": "устраиваться",
                                "target_lang_sense_clarification": "обживаться, усаживаться"
                            }
                        ]
                    }
                }
            ]
        }
    ]

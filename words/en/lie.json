    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0bc4b833-695d-3f43-ad1b-3dfebd4ce1d1",
                    "sense_definition": "To rest in a horizontal position on a surface.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sem_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>lie</w> down on the bed for a few minutes.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>прилечь</w> на кровать на несколько минут."
                            }
                        },
                        {
                            "text": "The cat likes to <w>lie</w> in the sun near the window.",
                            "target_lang_translations": {
                                "ru": "Кошка любит <w>лежать</w> на солнце у окна."
                            }
                        }
                    ],
                    "synonyms": [
                        "recline",
                        "rest"
                    ],
                    "common_phrases": [
                        "lie down",
                        "lie still",
                        "lie back"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в горизонтальном положении на поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лежать",
                                "target_lang_sense_clarification": "находиться в горизонтальном положении"
                            },
                            {
                                "target_lang_word": "лечь",
                                "target_lang_sense_clarification": "принять горизонтальное положение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f0630d5-11db-3350-bc4e-b3391287e68c",
                    "sense_definition": "To say something that is not true intentionally to deceive someone.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sem_falsehood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should never <w>lie</w> to your parents about where you are.",
                            "target_lang_translations": {
                                "ru": "Тебе никогда не следует <w>лгать</w> родителям о том, где ты находишься."
                            }
                        },
                        {
                            "text": "He <w>lied</w> about his age to get the job.",
                            "target_lang_translations": {
                                "ru": "Он <w>соврал</w> о своем возрасте, чтобы получить работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "fib",
                        "prevaricate"
                    ],
                    "common_phrases": [
                        "lie to someone",
                        "lie about something",
                        "don't lie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить неправду намеренно, с целью обмана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лгать",
                                "target_lang_sense_clarification": "говорить неправду"
                            },
                            {
                                "target_lang_word": "врать",
                                "target_lang_sense_clarification": "разг. говорить неправду, выдумывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c13e0d5-254f-3348-a1f3-346d94ea952d",
                    "sense_definition": "To be situated or placed in a specific location.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sem_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small village <w>lies</w> in a valley between two high mountains.",
                            "target_lang_translations": {
                                "ru": "Маленькая деревня <w>лежит</w> в долине между двумя высокими горами."
                            }
                        },
                        {
                            "text": "Many challenges <w>lie</w> ahead of us on this long journey.",
                            "target_lang_translations": {
                                "ru": "Многие испытания <w>ждут</w> нас впереди в этом долгом путешествии."
                            }
                        }
                    ],
                    "synonyms": [
                        "be situated",
                        "be located"
                    ],
                    "common_phrases": [
                        "lie ahead",
                        "lie to the north",
                        "lie near"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться или быть расположенным в определенном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "быть где-либо"
                            },
                            {
                                "target_lang_word": "располагаться",
                                "target_lang_sense_clarification": "занимать место"
                            },
                            {
                                "target_lang_word": "лежать",
                                "target_lang_sense_clarification": "геогр. простираться, находиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f527859-b5ba-365b-a9ff-a523d0290cbc",
                    "sense_definition": "To remain in a certain state or condition for a period of time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sem_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She used to <w>lie</w> awake at night worrying about the future.",
                            "target_lang_translations": {
                                "ru": "Раньше она часто <w>лежала</w> без сна по ночам, беспокоясь о будущем."
                            }
                        },
                        {
                            "text": "The old fields <w>lie</w> fallow during the winter months.",
                            "target_lang_translations": {
                                "ru": "Старые поля <w>лежат</w> под паром в зимние месяцы."
                            }
                        }
                    ],
                    "synonyms": [
                        "remain",
                        "stay"
                    ],
                    "common_phrases": [
                        "lie awake",
                        "lie dormant",
                        "lie empty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставаться в определенном состоянии или условии в течение некоторого времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставаться",
                                "target_lang_sense_clarification": "пребывать в состоянии"
                            },
                            {
                                "target_lang_word": "лежать",
                                "target_lang_sense_clarification": "находиться без движения или использования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa6dde2f-0f7b-3896-ac72-677ccd9bd84d",
                    "sense_definition": "To exist or be found within something; to belong to.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sem_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The solution to the problem <w>lies</w> in better communication between the teams.",
                            "target_lang_translations": {
                                "ru": "Решение проблемы <w>заключается</w> в лучшей коммуникации между командами."
                            }
                        },
                        {
                            "text": "The responsibility for the project <w>lies</w> with the manager.",
                            "target_lang_translations": {
                                "ru": "Ответственность за проект <w>лежит</w> на менеджере."
                            }
                        }
                    ],
                    "synonyms": [
                        "exist",
                        "reside"
                    ],
                    "common_phrases": [
                        "lie in the fact",
                        "lie with someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существовать или заключаться в чем-либо; принадлежать кому-либо (об ответственности)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заключаться",
                                "target_lang_sense_clarification": "состоять в чем-либо"
                            },
                            {
                                "target_lang_word": "лежать",
                                "target_lang_sense_clarification": "об обязанности или ответственности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e3039d8f-2375-3f04-b8c7-c64a29527d76",
                    "sense_definition": "An intentionally false statement made to deceive someone.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sem_falsehood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She told a <w>lie</w> because she was afraid of getting into trouble.",
                            "target_lang_translations": {
                                "ru": "Она сказала <w>неправду</w>, потому что боялась попасть в беду."
                            }
                        },
                        {
                            "text": "I knew he was telling a <w>lie</w> by the look on his face.",
                            "target_lang_translations": {
                                "ru": "Я понял, что он говорит <w>неправду</w>, по выражению его лица."
                            }
                        }
                    ],
                    "synonyms": [
                        "falsehood",
                        "fib",
                        "fabrication",
                        "untruth"
                    ],
                    "antonyms": [
                        "truth",
                        "fact"
                    ],
                    "common_phrases": [
                        "tell a lie",
                        "white lie",
                        "pack of lies"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренно ложное утверждение, сделанное с целью обмана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ложь",
                                "target_lang_sense_clarification": "неправда, обман"
                            },
                            {
                                "target_lang_word": "неправда",
                                "target_lang_sense_clarification": "то, что противоречит истине"
                            },
                            {
                                "target_lang_word": "враньё",
                                "target_lang_sense_clarification": "разг. ложь, выдумка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "074649f2-8388-3b77-b498-d278b83b83c8",
                    "sense_definition": "The position or way in which something is situated or arranged.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sem_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The golfer had a difficult <w>lie</w> because the ball was in the tall grass.",
                            "target_lang_translations": {
                                "ru": "У гольфиста было сложное <w>положение</w> мяча, так как он попал в высокую траву."
                            }
                        },
                        {
                            "text": "He studied the <w>lie</w> of the land before deciding where to build.",
                            "target_lang_translations": {
                                "ru": "Он изучил <w>рельеф</w> местности, прежде чем решать, где строить."
                            }
                        }
                    ],
                    "synonyms": [
                        "position",
                        "location"
                    ],
                    "common_phrases": [
                        "lie of the land",
                        "good lie",
                        "bad lie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положение или способ, которым что-то расположено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положение",
                                "target_lang_sense_clarification": "расположение объекта"
                            },
                            {
                                "target_lang_word": "рельеф",
                                "target_lang_sense_clarification": "очертания поверхности земли"
                            }
                        ]
                    }
                }
            ]
        },

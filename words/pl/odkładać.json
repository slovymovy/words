    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f2370e0a-f93e-34a2-b2d2-e90feaf4a2f1",
                    "sense_definition": "Kłaść trzymany przedmiot na bok, w inne miejsce lub z powrotem na jego stałe miejsce.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze <w>odkładam</w> klucze do tej samej szuflady, żeby ich nie zgubić.",
                            "target_lang_translations": {
                                "ru": "Я всегда <w>кладу</w> ключи в один и тот же ящик, чтобы не потерять их."
                            }
                        },
                        {
                            "text": "Proszę <w>odkładać</w> przeczytane książki na półkę.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>кладите</w> прочитанные книги на полку."
                            }
                        }
                    ],
                    "synonyms": [
                        "odraczać",
                        "odciągać się",
                        "odwlekać się",
                        "opóźniać się",
                        "spóźniać się",
                        "przekładać",
                        "przenosić",
                        "przesuwać",
                        "dawać",
                        "zdawać się",
                        "odciskać się"
                    ],
                    "antonyms": [
                        "brać",
                        "podnosić"
                    ],
                    "common_phrases": [
                        "odkładać na miejsce",
                        "odkładać na bok",
                        "odkładać na półkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Класть предмет в сторону, в другое место или возвращать на его обычное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класть",
                                "target_lang_sense_clarification": "помещать предмет куда-либо"
                            },
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "убирать в сторону"
                            },
                            {
                                "target_lang_word": "убирать",
                                "target_lang_sense_clarification": "класть на место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2973f76a-db4f-3b75-b158-7c703ddee24f",
                    "sense_definition": "Przesuwać termin wykonania czegoś na późniejszy czas; zwlekać z czymś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_management",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie lubię <w>odkładać</w> trudnych zadań na później.",
                            "target_lang_translations": {
                                "ru": "Я не люблю <w>откладывать</w> трудные задачи на потом."
                            }
                        },
                        {
                            "text": "Ciągle <w>odkłada</w> wizytę u dentysty, mimo że boli go ząb.",
                            "target_lang_translations": {
                                "ru": "Он постоянно <w>откладывает</w> визит к стоматологу, хотя у него болит зуб."
                            }
                        }
                    ],
                    "synonyms": [
                        "przesuwać",
                        "przekładać",
                        "odraczać",
                        "odwlekać się",
                        "opóźniać się"
                    ],
                    "antonyms": [
                        "przyspieszać"
                    ],
                    "common_phrases": [
                        "odkładać na później",
                        "odkładać na jutro",
                        "odkładać decyzję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переносить срок выполнения чего-либо на более позднее время; медлить с чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "переносить на потом"
                            },
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "менять время события"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78f95bff-ea73-3fb8-9750-da4247712225",
                    "sense_definition": "Gromadzić pieniądze lub inne zasoby z myślą o przyszłym wykorzystaniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regularnie <w>odkładamy</w> pieniądze na wymarzone wakacje w Grecji.",
                            "target_lang_translations": {
                                "ru": "Мы регулярно <w>откладываем</w> деньги на отпуск мечты в Греции."
                            }
                        },
                        {
                            "text": "Moi rodzice co miesiąc <w>odkładają</w> niewielką sumę na czarną godzinę.",
                            "target_lang_translations": {
                                "ru": "Мои родители каждый месяц <w>откладывают</w> небольшую сумму на черный день."
                            }
                        }
                    ],
                    "synonyms": [
                        "dawać",

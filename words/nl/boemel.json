{
    "word_family": [
        "boemelen",
        "boemeltrein"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e65efc8d-6256-3e61-bf20-7a2f36279ad2",
                    "sense_definition": "Een trein die op alle tussengelegen stations stopt en daardoor vaak langzaam is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>boemel</w> stopt in elk klein dorpje tussen de twee steden.",
                            "target_lang_translations": {
                                "ru": "<w>Электричка</w> останавливается в каждой маленькой деревушке между двумя городами."
                            }
                        },
                        {
                            "text": "Omdat we geen haast hadden, gingen we met de <w>boemel</w> naar het strand.",
                            "target_lang_translations": {
                                "ru": "Так как мы не спешили, мы поехали на пляж на <w>пригородном поезде</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stoptrein",
                        "boemeltrein"
                    ],
                    "antonyms": [
                        "sneltrein",
                        "intercity"
                    ],
                    "common_phrases": [
                        "met de boemel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поезд, который останавливается на всех промежуточных станциях и поэтому часто едет медленно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "электричка",
                                "target_lang_sense_clarification": "пригородный поезд с частыми остановками"
                            },
                            {
                                "target_lang_word": "пригородный поезд",
                                "target_lang_sense_clarification": "поезд местного сообщения"
                            },
                            {
                                "target_lang_word": "тихоход",
                                "target_lang_sense_clarification": "медленно движущийся транспорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55e1e6ce-f4bb-3a3a-a005-088702f4ac22",
                    "sense_definition": "Het uitgaan en langs verschillende cafés trekken om te drinken en plezier te maken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het behalen van hun diploma gingen de studenten flink aan de <w>boemel</w>.",
                            "target_lang_translations": {
                                "ru": "После получения дипломов студенты ушли в настоящий <w>загул</w>."
                            }
                        },
                        {
                            "text": "Hij is gisteravond met zijn vrienden op de <w>boemel</w> geweest.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером он с друзьями был в <w>загуле</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kroegentocht"
                    ],
                    "common_phrases": [
                        "aan de boemel",
                        "op de boemel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Посещение различных кафе и баров ради выпивки и развлечений; кутёж."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кутёж",
                                "target_lang_sense_clarification": "шумная попойка, разгул"
                            },
                            {
                                "target_lang_word": "загул",
                                "target_lang_sense_clarification": "продолжительное пьянство или гулянка"
                            },
                            {
                                "target_lang_word": "гулянка",
                                "target_lang_sense_clarification": "веселое времяпрепровождение с алкоголем"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7cd882b6-ae35-390c-b39c-80df7a2066f7",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>boemelen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>boemel</w> vandaag rustig door de stad zonder specifiek doel.",
                            "target_lang_translations": {
                                "ru": "Я сегодня спокойно <w>брожу</w> по городу без особой цели."
                            }
                        },
                        {
                            "text": "Soms <w>boemel</w> ik met de trein naar mijn werk om een boek te kunnen lezen.",
                            "target_lang_translations": {
                                "ru": "Иногда я <w>еду на электричке</w> на работу, чтобы почитать книгу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени от глагола boemelen (ехать на медленном поезде или бесцельно бродить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брожу",
                                "target_lang_sense_clarification": "гулять без определенной цели"
                            },
                            {
                                "target_lang_word": "еду на электричке",
                                "target_lang_sense_clarification": "передвигаться на медленном поезде"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
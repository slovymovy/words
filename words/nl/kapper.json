            "senses": [
                {
                    "sense_id": "68884018-78d4-3e61-8d73-2b9dbbc86056",
                    "sense_definition": "Iemand die voor zijn beroep het haar van mensen knipt en verzorgt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet morgen naar de <w>kapper</w> om mijn haar te laten knippen.",
                            "target_lang_translations": {
                                "en": "I have to go to the <w>hairdresser</w> tomorrow to get my hair cut.",
                                "ru": "Мне нужно завтра к <w>парикмахеру</w>, чтобы подстричься."
                            }
                        },
                        {
                            "text": "De <w>kapper</w> vroeg of ik mijn haar kort of lang wilde houden.",
                            "target_lang_translations": {
                                "en": "The <w>hairdresser</w> asked if I wanted to keep my hair short or long.",
                                "ru": "<w>Парикмахер</w> спросил, хочу ли я оставить волосы короткими или длинными."
                            }
                        }
                    ],
                    "synonyms": [
                        "coiffeur",
                        "friseur"
                    ],
                    "common_phrases": [
                        "naar de kapper",
                        "bij de kapper"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who cuts and styles people's hair as a profession.",
                        "ru": "Тот, кто профессионально стрижет волосы и ухаживает за ними."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hairdresser",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "barber",
                                "target_lang_sense_clarification": "usually for men"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "парикмахер",
                                "target_lang_sense_clarification": "специалист по стрижке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2ee2a3f-203f-3a19-8bd4-23aabe2a0e6d",
                    "sense_definition": "Een struik uit het Middellandse Zeegebied waarvan de bloemknoppen (kappertjes) worden gegeten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>kapper</w> groeit het best in een warm en droog klimaat.",
                            "target_lang_translations": {
                                "en": "The <w>caper bush</w> grows best in a warm and dry climate.",
                                "ru": "<w>Каперсник</w> лучше всего растет в теплом и сухом климате."
                            }
                        },
                        {
                            "text": "In de botanische tuin staat een prachtige bloeiende <w>kapper</w>.",
                            "target_lang_translations": {
                                "en": "There is a beautiful blooming <w>caper bush</w> in the botanical garden.",
                                "ru": "В ботаническом саду стоит прекрасный цветущий <w>каперсник</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kapperboom"
                    ],
                    "target_lang_definitions": {
                        "en": "A shrub from the Mediterranean region whose flower buds (capers) are eaten.",
                        "ru": "Кустарник из Средиземноморья, цветочные почки которого (каперсы) употребляют в пищу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "caper bush",
                                "target_lang_sense_clarification": "plant"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "каперсник",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93f3b9d7-e90f-361f-9c89-a71fad9fb47d",
                    "sense_definition": "Iemand die bomen omhakt of hout hakt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>kapper</w> werkte de hele dag in het bos om bomen te vellen.",
                            "target_lang_translations": {
                                "en": "The <w>woodcutter</w> worked in the forest all day felling trees.",
                                "ru": "<w>Лесоруб</w> работал весь день в лесу, валя деревья."
                            }
                        },
                        {
                            "text": "Als ervaren <w>kapper</w> wist hij precies hoe de boom zou vallen.",
                            "target_lang_translations": {
                                "en": "As an experienced <w>woodcutter</w>, he knew exactly how the tree would fall.",
                                "ru": "Как опытный <w>лесоруб</w>, он точно знал, как упадет дерево."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who chops down trees or chops wood.",
                        "ru": "Тот, кто рубит деревья или колет дрова."
                    },
                    "translations": {

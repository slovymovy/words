    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "046da53d-27a0-3b3b-9de1-570d53cefbf3",
                    "sense_definition": "Wykonać otwór przechodzący przez coś na wylot, używając siły lub ostrego narzędzia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_penetration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść do kosmetyczki, aby <w>przebić</w> uszy i nosić kolczyki.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти к косметологу, чтобы <w>проколоть</w> уши и носить серьги."
                            }
                        },
                        {
                            "text": "Ten gwóźdź jest wystarczająco długi, by <w>przebić</w> grubą deskę na wylot.",
                            "target_lang_translations": {
                                "ru": "Этот гвоздь достаточно длинный, чтобы <w>пробить</w> толстую доску насквозь."
                            }
                        }
                    ],
                    "synonyms": [
                        "przekłuć",
                        "przedziurawić"
                    ],
                    "antonyms": [
                        "załatać",
                        "zakleić"
                    ],
                    "common_phrases": [
                        "przebić na wylot",
                        "przebić uszy",
                        "przebić murem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать сквозное отверстие в чем-либо, применяя силу или острый инструмент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробить",
                                "target_lang_sense_clarification": "сделать отверстие силой"
                            },
                            {
                                "target_lang_word": "проколоть",
                                "target_lang_sense_clarification": "сделать отверстие острым предметом (например, уши)"
                            },
                            {
                                "target_lang_word": "проткнуть",
                                "target_lang_sense_clarification": "вонзить острый предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "993c74bd-5708-3116-a6b6-a88eaefc23b6",
                    "sense_definition": "Zniszczyć szczelność nadmuchanego przedmiotu, powodując gwałtowną ucieczkę powietrza.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_penetration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca nie zauważył szkła na drodze i przypadkowo <w>przebił</w> oponę.",
                            "target_lang_translations": {
                                "ru": "Водитель не заметил стекла на дороге и случайно <w>пробил</w> колесо."
                            }
                        },
                        {
                            "text": "Dziecko bawiło się ostrym widelcem i bałem się, że może <w>przebić</w> balon.",
                            "target_lang_translations": {
                                "ru": "Ребенок играл острой вилкой, и я боялся, что он может <w>проткнуть</w> воздушный шар."
                            }
                        }
                    ],
                    "synonyms": [
                        "przekłuć"
                    ],
                    "antonyms": [
                        "napompować",
                        "załatać"
                    ],
                    "common_phrases": [
                        "przebić oponę",
                        "przebić dętkę",
                        "przebić balon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушить герметичность надутого предмета, вызвав выход воздуха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проколоть",
                                "target_lang_sense_clarification": "шину, мяч, воздушный шар"
                            },
                            {
                                "target_lang_word": "пробить",
                                "target_lang_sense_clarification": "колесо (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28cbe1d4-e7ad-32de-9dbb-bb2bf22e42e8",
                    "sense_definition": "Zaoferować lepszą stawkę w licytacji lub użyć silniejszej karty w grze, aby pokonać przeciwnika.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "competition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza firma postanowiła <w>przebić</w> ofertę konkurencji, proponując niższą cenę za usługi.",
                            "target_lang_translations": {
                                "ru": "Наша компания решила <w>перебить</w> предложение конкурентов, предложив более низкую цену за услуги."
                            }
                        },
                        {
                            "text": "Nie mam wystarczająco mocnych kart, żeby <w>przebić</w> twojego asa pik.",
                            "target_lang_translations": {
                                "ru": "У меня нет достаточно сильных карт, чтобы <w>побить</w> твоего туза пик."
                            }
                        }
                    ],
                    "synonyms": [
                        "przelicytować",
                        "przewyższyć",
                        "pokonać"
                    ],
                    "antonyms": [
                        "spasować"
                    ],
                    "common_phrases": [
                        "przebić ofertę",
                        "przebić stawkę",
                        "przebić kartę",
                        "przebić atutem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предложить лучшую ставку на торгах или использовать более сильную карту в игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перебить",
                                "target_lang_sense_clarification": "цену, ставку, предложение"
                            },
                            {
                                "target_lang_word": "побить",
                                "target_lang_sense_clarification": "карту"
                            }
                        ]
                    }
                }
            ]
        }
    ]

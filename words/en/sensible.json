    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d27ad2d0-9893-3b15-bd36-9b60bc55718c",
                    "sense_definition": "Acting with or showing good sense and sound judgment; reasonable and practical.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "judgment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a <w>sensible</w> decision to stay home during the storm.",
                            "target_lang_translations": {
                                "ru": "Остаться дома во время шторма было <w>разумным</w> решением."
                            }
                        },
                        {
                            "text": "She gave me some very <w>sensible</w> advice about how to save money.",
                            "target_lang_translations": {
                                "ru": "Она дала мне очень <w>дельный</w> совет о том, как сэкономить деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "reasonable"
                    ],
                    "common_phrases": [
                        "sensible decision",
                        "sensible advice",
                        "sensible approach",
                        "sensible person"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действующий с умом и здравым смыслом; разумный и практичный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разу́мный",
                                "target_lang_sense_clarification": "логичный, обоснованный"
                            },
                            {
                                "target_lang_word": "благоразу́мный",
                                "target_lang_sense_clarification": "рассудительный, осмотрительный"
                            },
                            {
                                "target_lang_word": "здравомы́слящий",
                                "target_lang_sense_clarification": "о человеке: обладающий здравым умом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "862e3f85-8825-3434-8ede-fa9bd4834404",
                    "sense_definition": "Practical and useful rather than fashionable or attractive (especially regarding clothing or objects).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "practicality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should wear <w>sensible</w> shoes if you plan to walk all day.",
                            "target_lang_translations": {
                                "ru": "Тебе следует надеть <w>практичную</w> обувь, если планируешь ходить весь день."
                            }
                        },
                        {
                            "text": "He bought a <w>sensible</w> family car instead of a sports car.",
                            "target_lang_translations": {
                                "ru": "Вместо спортивной машины он купил <w>практичный</w> семейный автомобиль."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sensible shoes",
                        "sensible clothes",
                        "sensible diet",
                        "sensible car"
                    ],
                    "target_lang_definitions": {
                        "ru": "Практичный и удобный, а не модный или привлекательный (особенно об одежде или предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "практи́чный",
                                "target_lang_sense_clarification": "удобный в использовании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9795c6c-636b-3406-b822-906b5896e77e",
                    "sense_definition": "Able to be perceived by the senses; noticeable or appreciable.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was a <w>sensible</w> drop in temperature as the sun went down.",
                            "target_lang_translations": {
                                "ru": "С заходом солнца произошло <w>ощутимое</w> понижение температуры."
                            }
                        },
                        {
                            "text": "The new policy had no <w>sensible</w> effect on the economy.",
                            "target_lang_translations": {
                                "ru": "Новая политика не оказала <w>заметного</w> влияния на экономику."
                            }
                        }
                    ],
                    "synonyms": [
                        "perceptible",
                        "appreciable",
                        "detectable"
                    ],
                    "antonyms": [
                        "imperceptible",
                        "undetectable"
                    ],
                    "common_phrases": [
                        "sensible difference",
                        "sensible change"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "common in older literature"
                        },
                        {
                            "trait_type": "form",
                            "comment": "literary or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддающийся восприятию органами чувств; заметный или существенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ощути́мый",
                                "target_lang_sense_clarification": "воспринимаемый чувствами, существенный"
                            },
                            {
                                "target_lang_word": "заме́тный",
                                "target_lang_sense_clarification": "явный, очевидный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1cf5ff8-894a-3cf2-be73-8f11ac2c95bd",
                    "sense_definition": "Fully aware or cognizant of something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "awareness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am fully <w>sensible</w> of the honor you have bestowed upon me.",
                            "target_lang_translations": {
                                "ru": "Я в полной мере <w>осознаю</w> честь, которую вы мне оказали."
                            }
                        },
                        {
                            "text": "He was <w>sensible</w> of the risks involved in the mission.",
                            "target_lang_translations": {
                                "ru": "Он <w>осознавал</w> риски, связанные с этой миссией."
                            }
                        }
                    ],
                    "antonyms": [
                        "insensible"
                    ],
                    "common_phrases": [
                        "sensible of"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly found in formal or older texts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью осознающий что-либо или осведомленный о чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сознаю́щий",
                                "target_lang_sense_clarification": "понимающий, отдающий себе отчет"
                            },
                            {
                                "target_lang_word": "осведомлённый",
                                "target_lang_sense_clarification": "знающий о чем-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e8c5e646-9861-3ca5-b41d-2d860bd0158c",
                    "sense_definition": "An object or thing that can be perceived by the senses.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "philosophy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In his lecture, the professor distinguished between abstract ideas and concrete <w>sensibles</w>.",
                            "target_lang_translations": {
                                "ru": "В своей лекции профессор проводил различие между абстрактными идеями и конкретными <w>чувственно воспринимаемыми объектами</w>."
                            }
                        },
                        {
                            "text": "Plato argued that <w>sensibles</w> are always changing and therefore cannot be the source of true knowledge.",
                            "target_lang_translations": {
                                "ru": "Платон утверждал, что <w>чувственные вещи</w> постоянно меняются и поэтому не могут быть источником истинного знания."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical philosophy context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Объект или явление, которые могут быть восприняты органами чувств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувственно воспринимаемый объект",
                                "target_lang_sense_clarification": "философский термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

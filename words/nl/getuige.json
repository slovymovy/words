                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Singular present subjunctive of 'getuigen' (to testify).",
                        "ru": "Форма сослагательного наклонения глагола 'getuigen' (свидетельствовать), выражающая пожелание или призыв."
                    },
                    "translations": {

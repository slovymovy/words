            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c0783c69-1ec6-31e7-b097-a225e742126e",
                    "sense_definition": "een gedomesticeerde vogel die eieren legt en vaak op boerderijen wordt gehouden",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>kip</w> legt elke ochtend een vers ei in het hok.",
                            "target_lang_translations": {
                                "en": "The <w>chicken</w> lays a fresh egg in the coop every morning.",
                                "ru": "<w>Курица</w> каждое утро откладывает свежее яйцо в курятнике."
                            }
                        },
                        {
                            "text": "Op de kinderboerderij loopt een bruine <w>kip</w> tussen de geiten.",
                            "target_lang_translations": {
                                "en": "At the petting zoo, a brown <w>chicken</w> walks among the goats.",
                                "ru": "В контактном зоопарке коричневая <w>курица</w> ходит среди коз."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoen",
                        "hen",
                        "kieken"
                    ],
                    "common_phrases": [
                        "als een kip zonder kop",
                        "met de kippen op stok",
                        "geen kip"
                    ],
                    "target_lang_definitions": {
                        "en": "a domesticated bird that lays eggs and is often kept on farms",
                        "ru": "домашняя птица, которая несет яйца и часто содержится на фермах"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chicken",
                                "target_lang_sense_clarification": "animal"
                            },
                            {
                                "target_lang_word": "hen",
                                "target_lang_sense_clarification": "female chicken"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "курица",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2405308-9824-3521-bf25-dd340bcac951",
                    "sense_definition": "het vlees van een kip dat als voedsel wordt gegeten",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_meat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vanavond eten we rijst met groenten en <w>kip</w>.",
                            "target_lang_translations": {

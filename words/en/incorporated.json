    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7fb10011-1cbc-3db8-9d43-b7c06d598123",
                    "sense_definition": "Simple past and past participle of <w>incorporate</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The architect <w>incorporated</w> solar panels into the design of the roof.",
                            "target_lang_translations": {
                                "ru": "Архитектор <w>включил</w> солнечные панели в проект крыши."
                            }
                        },
                        {
                            "text": "All the latest safety features have been <w>incorporated</w> into the new car model.",
                            "target_lang_translations": {
                                "ru": "Все новейшие функции безопасности были <w>внедрены</w> в новую модель автомобиля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "incorporated into",
                        "fully incorporated"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past and past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола incorporate (включать, объединять)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "в состав чего-либо"
                            },
                            {
                                "target_lang_word": "внедрять",
                                "target_lang_sense_clarification": "вводить в употребление или конструкцию"
                            },
                            {
                                "target_lang_word": "объединять",
                                "target_lang_sense_clarification": "соединять в одно целое"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "15767e67-84b3-312f-ad86-fde991f93d55",
                    "sense_definition": "Formed into a legal business entity where ownership is divided into shares and liability is limited.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "An <w>incorporated</w> business protects its owners from personal liability for company debts.",
                            "target_lang_translations": {
                                "ru": "<w>Зарегистрированный как корпорация</w> бизнес защищает своих владельцев от личной ответственности по долгам компании."
                            }
                        },
                        {
                            "text": "The local sports club decided to become an <w>incorporated</w> association.",
                            "target_lang_translations": {
                                "ru": "Местный спортивный клуб решил стать <w>зарегистрированной</w> ассоциацией."
                            }
                        }
                    ],
                    "antonyms": [
                        "unincorporated"
                    ],
                    "common_phrases": [
                        "incorporated body",
                        "incorporated association",
                        "incorporated company"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оформленный как юридическое лицо (корпорация), где владение разделено на акции, а ответственность ограничена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарегистрированный",
                                "target_lang_sense_clarification": "как юридическое лицо"
                            },
                            {
                                "target_lang_word": "акционерный",
                                "target_lang_sense_clarification": "о типе компании или общества"
                            },
                            {
                                "target_lang_word": "инкорпорированный",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
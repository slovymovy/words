                    "sense_id": "341289ff-bfe0-3560-9a04-c9c61c7eb611",
                    "sense_definition": "Stop speaking! Hush!",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "silence_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Mum</w>! Someone is coming down the hall.",
                            "target_lang_translations": {
                                "ru": "<w>Тсс</w>! Кто-то идёт по коридору."
                            }
                        },
                        {
                            "text": "You must be quiet now, so <w>mum</w>!",
                            "target_lang_translations": {
                                "ru": "Ты должен вести себя тихо, так что <w>молчок</w>!"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестань говорить! Тсс!"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тсс",
                                "target_lang_sense_clarification": "призыв к тишине"
                            },
                            {
                                "target_lang_word": "молчок",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]

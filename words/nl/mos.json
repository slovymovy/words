{
    "word_family": [
        "mossig",
        "mosgroen",
        "ontmossen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "89a5dc49-ad91-3d21-9bb0-0d7148f065f3",
                    "sense_definition": "Een kleine, groene plant zonder bloemen die in dichte kussens groeit op vochtige plekken zoals bomen, stenen en de grond.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het dak van het oude schuurtje was helemaal bedekt met groen <w>mos</w>.",
                            "target_lang_translations": {
                                "ru": "Крыша старого сарая была полностью покрыта зеленым <w>мхом</w>."
                            }
                        },
                        {
                            "text": "Tijdens de wandeling in het bos zagen we zacht <w>mos</w> op de boomstammen groeien.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки по лесу мы видели, как на стволах деревьев растет мягкий <w>мох</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zacht als mos",
                        "mos op het dak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкое зеленое растение без цветов, которое растет плотными подушками на влажных местах, таких как деревья, камни и земля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мох",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f85d76ca-e320-3a94-86c3-bc4c0085082a",
                    "sense_definition": "Een organisme dat bestaat uit een samenleving van een schimmel en een alg.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de rotsen aan de kust groeide een geel soort <w>mos</w> dat goed tegen zout water kan.",
                            "target_lang_translations": {
                                "ru": "На скалах у побережья рос желтый вид <w>мха</w>, который хорошо переносит соленую воду."
                            }
                        },
                        {
                            "text": "Biologen leggen uit dat dit <w>mos</w> eigenlijk een symbiose is tussen twee organismen.",
                            "target_lang_translations": {
                                "ru": "Биологи объясняют, что этот <w>мох</w> на самом деле является симбиозом двух организмов."
                            }
                        }
                    ],
                    "synonyms": [
                        "korstmos"
                    ],
                    "common_phrases": [
                        "IJslands mos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организм, представляющий собой симбиоз гриба и водоросли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лишайник",
                                "target_lang_sense_clarification": "биологический термин"
                            },
                            {
                                "target_lang_word": "мох",
                                "target_lang_sense_clarification": "в народных названиях (напр. исландский мох)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fbcd152-b146-3cf7-8e0b-f250786fd24e",
                    "sense_definition": "Een drassig stuk land of moeras.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude verhalen vertellen over reizigers die verdwaalden in het verraderlijke <w>mos</w>.",
                            "target_lang_translations": {
                                "ru": "Старые предания рассказывают о путниках, сбившихся с пути в коварном <w>болоте</w>."
                            }
                        },
                        {
                            "text": "Het vee kon niet grazen op het natte <w>mos</w> aan de rand van het dorp.",
                            "target_lang_translations": {
                                "ru": "Скот не мог пастись на мокром <w>болоте</w> на окраине деревни."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "wordt zelden nog in deze betekenis gebruikt in modern Nederlands"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заболоченный участок земли или топь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болото",
                                "target_lang_sense_clarification": "местность"
                            },
                            {
                                "target_lang_word": "топь",
                                "target_lang_sense_clarification": "трясина"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2c21d5f2-9465-317c-8a0c-0748a951785f",
                    "sense_definition": "Наводить порядок в помещении, раскладывать вещи по местам, удалять грязь и пыль.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждую субботу мы всей семьёй начинаем <w>убираться</w> в нашей большой квартире."
                        },
                        {
                            "text": "Мама попросила детей <w>убраться</w> в своей комнате, прежде чем идти гулять."
                        }
                    ],
                    "synonyms": [
                        "наводить порядок",
                        "прибираться",
                        "производить уборку"
                    ],
                    "antonyms": [
                        "мусорить",
                        "сорить"
                    ],
                    "common_phrases": [
                        "убираться в комнате",
                        "убираться дома",
                        "убираться в квартире"
                    ]
                },
                {
                    "sense_id": "6c87cf92-2893-39f0-ab0d-e56207f8e9a3",
                    "sense_definition": "Уходить, удаляться откуда-либо (часто используется как грубое требование покинуть место).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "leaving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охранник грубо сказал нам <w>убираться</w> отсюда, иначе он вызовет полицию."
                        },
                        {
                            "text": "Я понял, что мне пора <w>убираться</w> домой, пока не начался сильный дождь."
                        }
                    ],
                    "synonyms": [
                        "уходить",
                        "удаляться",
                        "проваливать",
                        "уматывать"
                    ],
                    "antonyms": [
                        "приходить",
                        "прибывать"
                    ],
                    "common_phrases": [
                        "убирайся отсюда",
                        "убираться восвояси",
                        "пора убираться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в разговорной речи"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "может звучать грубо"
                        }
                    ]
                },
                {
                    "sense_id": "511a8ebb-cd8b-3f9e-a1d0-096897916785",
                    "sense_definition": "Помещаться внутри чего-либо, влезать по размеру в ограниченное пространство.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fitting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все мои вещи никак не хотели <w>убираться</w> в этот маленький дорожный чемодан."
                        },
                        {
                            "text": "Эта огромная коробка просто не <w>уберётся</w> на заднее сиденье твоей машины."
                        }
                    ],
                    "synonyms": [
                        "влазить"
                    ],
                    "common_phrases": [
                        "убираться в сумку",
                        "не убираться в коробку"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "в некоторых регионах или разговорной речи"

    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4b5e2b91-d31b-3884-8051-464f8c25f039",
                    "sense_definition": "Of surpassing excellence; magnificent.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "excellence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That is a <w>brilliant</w> idea for the new project.",
                            "target_lang_translations": {
                                "ru": "Это <w>блестящая</w> идея для нового проекта."
                            }
                        },
                        {
                            "text": "The team did a <w>brilliant</w> job organizing the event.",
                            "target_lang_translations": {
                                "ru": "Команда проделала <w>блестящую</w> работу по организации мероприятия."
                            }
                        }
                    ],
                    "synonyms": [
                        "excellent",
                        "magnificent",
                        "wonderful",
                        "splendid",
                        "superb",
                        "marvelous",
                        "exceptional",
                        "glorious",
                        "striking"
                    ],
                    "common_phrases": [
                        "brilliant idea",
                        "brilliant job",
                        "absolutely brilliant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превосходный, великолепный; выдающегося качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блестящий",
                                "target_lang_sense_clarification": "превосходный, замечательный"
                            },
                            {
                                "target_lang_word": "великолепный",
                                "target_lang_sense_clarification": "роскошный, очень хороший"
                            },
                            {
                                "target_lang_word": "блистательный",
                                "target_lang_sense_clarification": "имеющий большой успех"
                            }
                        ]

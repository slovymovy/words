                {
                    "sense_id": "147df99b-b628-3fcc-8567-8d092b2bac46",
                    "sense_definition": "Het geluid van een harde klap, botsing of explosie",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De deur viel met een harde <w>bam</w> in het slot.",
                            "target_lang_translations": {
                                "ru": "Дверь захлопнулась с громким <w>бам</w>."
                            }
                        },
                        {
                            "text": "Hij gooide het zware boek op tafel: <w>bam</w>!",
                            "target_lang_translations": {
                                "ru": "Он бросил тяжелую книгу на стол: <w>бам</w>!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "en toen bam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук сильного удара, столкновения или взрыва"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бам",
                                "target_lang_sense_clarification": "звукоподражание удару"
                            },
                            {
                                "target_lang_word": "бац",
                                "target_lang_sense_clarification": "резкий звук удара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c867cc38-b563-32a3-9172-644cfc3351c7",
                    "sense_definition": "Een uitroep om te benadrukken dat er plotseling iets opvallends gebeurt",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We liepen rustig op straat en <w>bam</w>, het begon keihard te regenen.",
                            "target_lang_translations": {
                                "ru": "Мы спокойно шли по улице, и <w>бац</w>, начался ливень."
                            }
                        },
                        {
                            "text": "Je denkt dat je klaar bent met werken en <w>bam</w>, er komt nog een opdracht bij.",
                            "target_lang_translations": {
                                "ru": "Думаешь, что закончил работу, и <w>бац</w>, прилетает еще одно задание."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, подчеркивающий внезапность происходящего события"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бац",
                                "target_lang_sense_clarification": "о неожиданном событии"
                            },
                            {
                                "target_lang_word": "бам",
                                "target_lang_sense_clarification": "внезапно"
                            }
                        ]
                    }
                }
            ]
        },
        {

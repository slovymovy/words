                    "sense_id": "a7ce9f31-7b0c-3f6e-a40c-eb8f8c394b04",
                    "sense_definition": "Zbudowany z cegły, kamienia lub betonu; wzniesiony techniką murarską.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice kupili stary, <w>murowany</w> dom na wsi.",
                            "target_lang_translations": {
                                "ru": "Мои родители купили старый <w>каменный</w> дом в деревне."
                            }
                        },
                        {
                            "text": "W centrum miasta stoi wysoki, <w>murowany</w> kościół z czerwoną dachówką.",
                            "target_lang_translations": {
                                "ru": "В центре города стоит высокая <w>каменная</w> церковь с красной черепицей."
                            }
                        }
                    ],
                    "antonyms": [
                        "drewniany"
                    ],
                    "common_phrases": [
                        "dom murowany",
                        "garaż murowany",
                        "płot murowany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Построенный из кирпича, камня или бетона; возведенный методом каменной кладки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каменный",
                                "target_lang_sense_clarification": "сделанный из камня или кирпича (в противоположность деревянному)"
                            },
                            {
                                "target_lang_word": "кирпичный",
                                "target_lang_sense_clarification": "сделанный из кирпича"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ac18f7c-d59e-3227-b5f9-c9c47c996b42",
                    "sense_definition": "Taki, który jest całkowicie pewny, nieunikniony lub zagwarantowany.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli nie założysz czapki na ten mróz, katar masz <w>murowany</w>.",
                            "target_lang_translations": {
                                "ru": "Если не наденешь шапку в такой мороз, насморк тебе <w>обеспечен</w>."
                            }
                        },
                        {
                            "text": "Z takim talentem i pracowitością jego sukces jest <w>murowany</w>.",
                            "target_lang_translations": {
                                "ru": "С таким талантом и трудолюбием его успех <w>гарантирован</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pewny",
                        "stuprocentowy"
                    ],
                    "antonyms": [
                        "niepewny",
                        "wątpliwy"
                    ],
                    "common_phrases": [
                        "sukces murowany",
                        "kłopoty murowane",
                        "efekt murowany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который является абсолютно точным, неизбежным или гарантированным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гарантированный",
                                "target_lang_sense_clarification": "несомненный, стопроцентный"
                            },
                            {
                                "target_lang_word": "обеспеченный",
                                "target_lang_sense_clarification": "неизбежный (часто о неприятностях)"
                            },
                            {
                                "target_lang_word": "верный",
                                "target_lang_sense_clarification": "надежный, неминуемый"
                            }
                        ]
                    }

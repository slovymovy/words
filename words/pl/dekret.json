                },
                {
                    "sense_id": "246a2f2e-89f1-3330-8dcb-520610dea972",
                    "sense_definition": "Adnotacja na dokumencie księgowym określająca sposób jego ujęcia w księgach rachunkowych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "accounting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Główna księgowa sprawdziła <w>dekret</w> na fakturze przed jej zaksięgowaniem.",
                            "target_lang_translations": {
                                "ru": "Главный бухгалтер проверила <w>контировку</w> на фактуре перед её проводкой."
                            }
                        },
                        {
                            "text": "Błędny <w>dekret</w> może spowodować poważne niezgodności w bilansie firmy.",
                            "target_lang_translations": {
                                "ru": "Ошибочная <w>контировка</w> может привести к серьезным расхождениям в балансе фирмы."
                            }
                        }
                    ],
                    "synonyms": [

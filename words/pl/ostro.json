                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круто",
                                "target_lang_sense_clarification": "о подъеме или спуске"
                            },
                            {
                                "target_lang_word": "резко",
                                "target_lang_sense_clarification": "о повороте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "239f5912-ea0c-361a-baf8-64ba1c1413d2",
                    "sense_definition": "Wyraźnie, z dobrze widocznymi konturami i szczegółami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W nowych okularach widzę wszystko bardzo <w>ostro</w>.",
                            "target_lang_translations": {
                                "ru": "В новых очках я вижу всё очень <w>четко</w>."
                            }
                        },
                        {
                            "text": "Ten aparat nie łapie <w>ostro</w> obiektów w ruchu.",
                            "target_lang_translations": {
                                "ru": "Этот фотоаппарат не снимает <w>резко</w> движущиеся объекты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "widzieć ostro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отчетливо, с хорошо видимыми контурами и деталями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "четко",
                                "target_lang_sense_clarification": "о зрении"
                            },
                            {
                                "target_lang_word": "резко",
                                "target_lang_sense_clarification": "о фокусе изображения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b50cf172-0bd9-343c-b1af-07d3c2e34ecc",
                    "sense_definition": "Wysokim, przenikliwym lub drażniącym tonem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzwonek telefonu zadzwonił <w>ostro</w> w ciszy nocnej.",
                            "target_lang_translations": {
                                "ru": "Телефонный звонок <w>резко</w> прозвенел в ночной тишине."
                            }
                        },
                        {
                            "text": "Jej głos brzmiał <w>ostro</w> i nieprzyjemnie.",
                            "target_lang_translations": {
                                "ru": "Ее голос звучал <w>резко</w> и неприятно."
                            }
                        }
                    ],
                    "synonyms": [
                        "wysoko"
                    ],
                    "antonyms": [
                        "niski",
                        "gruby"
                    ],
                    "common_phrases": [
                        "brzmieć ostro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высоким, пронзительным или раздражающим тоном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резко",
                                "target_lang_sense_clarification": "о звуке"
                            },
                            {
                                "target_lang_word": "пронзительно",
                                "target_lang_sense_clarification": "о голосе или сигнале"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1096e3d2-cf6f-311b-abe0-f1e3b586ea91",
                    "sense_definition": "Ryzykownie, niebezpiecznie lub z wielkimi emocjami (potocznie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wczorajszym koncercie było naprawdę <w>ostro</w>.",
                            "target_lang_translations": {
                                "ru": "На вчерашнем концерте было реально <w>круто</w>."
                            }
                        },
                        {
                            "text": "Ale <w>ostro</w> pojechałeś z tym żartem, chyba się obraził.",
                            "target_lang_translations": {
                                "ru": "Ну ты и <w>жестко</w> пошутил, он, кажется, обиделся."
                            }
                        }
                    ],
                    "common_phrases": [
                        "było ostro",
                        "pojechać ostro"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej do opisania intensywnych sytuacji"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рискованно, опасно или очень эмоционально (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жестко",
                                "target_lang_sense_clarification": "сленг, экстремально"
                            },
                            {
                                "target_lang_word": "круто",
                                "target_lang_sense_clarification": "сленг, интенсивно"
                            }
                        ]
                    }
                }

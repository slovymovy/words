            "senses": [
                {
                    "sense_id": "113e82ca-ca3f-3111-8ee4-976f31187456",
                    "sense_definition": "dotyczący Japonii, jej mieszkańców lub kultury",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "country_rel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra uwielbia <w>japońską</w> kuchnię i często robi sushi.",
                            "target_lang_translations": {
                                "ru": "Моя сестра обожает <w>японскую</w> кухню и часто делает суши."
                            }
                        },
                        {
                            "text": "<w>Japońskie</w> samochody słyną z niezawodności na całym świecie.",
                            "target_lang_translations": {
                                "ru": "<w>Японские</w> автомобили славятся своей надёжностью во всём мире."
                            }
                        }
                    ],
                    "antonyms": [
                        "antyjapoński"
                    ],
                    "common_phrases": [
                        "kuchnia japońska",
                        "japoński ogród",
                        "japońska wiśnia",
                        "sprzęt japoński"
                    ],
                    "target_lang_definitions": {
                        "ru": "относящийся к Японии, её жителям или культуре"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "японский",
                                "target_lang_sense_clarification": "относящийся к стране"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41d37d45-d8d7-38b5-abe1-ce3a65308b95",
                    "sense_definition": "odnoszący się do języka japońskiego",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "lang_rel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem nowy słownik polsko-<w>japoński</w> do nauki słówek.",
                            "target_lang_translations": {
                                "ru": "Я купил новый польско-<w>японский</w> словарь для изучения слов."
                            }
                        },
                        {
                            "text": "<w>Japońska</w> gramatyka różni się znacznie od polskiej.",
                            "target_lang_translations": {
                                "ru": "<w>Японская</w> грамматика значительно отличается от польской."
                            }
                        }
                    ],
                    "common_phrases": [
                        "alfabet japoński",
                        "pismo japońskie",
                        "napis japoński"
                    ],
                    "target_lang_definitions": {

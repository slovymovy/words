{
    "word_family": [
        "annoyance",
        "annoyingly"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ec0e62f7-ccc9-37c0-8bd8-f43e7c7aae68",
                    "sense_definition": "To make someone slightly angry, impatient, or irritated, especially by repeated actions.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "irritate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It really begins to <w>annoy</w> me when people talk loudly during a movie.",
                            "target_lang_translations": {
                                "ru": "Меня начинает сильно <w>раздражать</w>, когда люди громко разговаривают во время фильма."
                            }
                        },
                        {
                            "text": "My little brother likes to <w>annoy</w> the cat by pulling its tail.",
                            "target_lang_translations": {
                                "ru": "Мой младший брат любит <w>донимать</w> кота, дергая его за хвост."
                            }
                        },
                        {
                            "text": "Please do not <w>annoy</w> the driver while the bus is moving.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>отвлекайте</w> водителя во время движения автобуса."
                            }
                        }
                    ],
                    "synonyms": [
                        "irritate",
                        "bother",
                        "bug",
                        "irk",
                        "vex",
                        "disturb",
                        "upset"
                    ],
                    "antonyms": [
                        "please",
                        "soothe",
                        "comfort"
                    ],
                    "common_phrases": [
                        "annoy someone",
                        "really annoy",
                        "easily annoy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать у кого-либо легкий гнев, нетерпение или раздражение, особенно повторяющимися действиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздражать",
                                "target_lang_sense_clarification": "вызывать недовольство или злость"
                            },
                            {
                                "target_lang_word": "надоедать",
                                "target_lang_sense_clarification": "становиться неприятным из-за навязчивости или повторения"
                            },
                            {
                                "target_lang_word": "досаждать",
                                "target_lang_sense_clarification": "причинять неудобство или неприятности"
                            },
                            {
                                "target_lang_word": "беспокоить",
                                "target_lang_sense_clarification": "нарушать покой, отвлекать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ef74349-87eb-3b2c-9af4-c785e0590c73",
                    "sense_definition": "To harm, attack, or harass physically, often used in a military context.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "harm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general sent light cavalry to <w>annoy</w> the enemy's rear guard.",
                            "target_lang_translations": {
                                "ru": "Генерал послал легкую кавалерию <w>тревожить</w> арьергард противника."
                            }
                        },
                        {
                            "text": "They built a shelter so the harsh wind would not <w>annoy</w> them.",
                            "target_lang_translations": {
                                "ru": "Они построили укрытие, чтобы резкий ветер не <w>досаждал</w> им."
                            }
                        }
                    ],
                    "synonyms": [
                        "harm",
                        "injure",
                        "harass",
                        "molest"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'harass' or 'harm'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить вред, атаковать или физически изматывать (часто в военном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тревожить",
                                "target_lang_sense_clarification": "воен. беспокоить противника частыми мелкими атаками"
                            },
                            {
                                "target_lang_word": "изматывать",
                                "target_lang_sense_clarification": "лишать сил постоянным воздействием"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7343847e-ee91-301d-b21d-5338ac2dbf47",
                    "sense_definition": "A feeling of discomfort, trouble, or vexation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He wished to live a quiet life free from worldly <w>annoy</w>.",
                            "target_lang_translations": {
                                "ru": "Он хотел прожить спокойную жизнь, свободную от мирских <w>треволнений</w>."
                            }
                        },
                        {
                            "text": "To her great <w>annoy</w>, the carriage arrived two hours late.",
                            "target_lang_translations": {
                                "ru": "К её великой <w>досаде</w>, карета прибыла на два часа позже."
                            }
                        }
                    ],
                    "synonyms": [
                        "annoyance",
                        "vexation",
                        "trouble"
                    ],
                    "antonyms": [
                        "comfort",
                        "please"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern English uses 'annoyance'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство дискомфорта, беспокойства или досады."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "досада",
                                "target_lang_sense_clarification": "чувство раздражения и неудовольствия"
                            },
                            {
                                "target_lang_word": "неприятность",
                                "target_lang_sense_clarification": "то, что доставляет беспокойство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
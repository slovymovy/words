    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "14e3bc67-8156-3831-9585-4731b1600622",
                    "sense_definition": "The situation, consequences, or aftereffects that follow a significant, usually destructive or negative, event.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The country faced a severe economic crisis in the <w>aftermath</w> of the war.",
                            "target_lang_translations": {
                                "ru": "Страна столкнулась с тяжелым экономическим кризисом <w>вследствие</w> войны."
                            }
                        },
                        {
                            "text": "In the immediate <w>aftermath</w> of the scandal, the company's stock price dropped significantly.",
                            "target_lang_translations": {
                                "ru": "Сразу <w>после</w> скандала цена акций компании значительно упала."
                            }
                        }
                    ],
                    "synonyms": [
                        "consequence",
                        "repercussion",
                        "wake",
                        "aftereffect",
                        "outcome"
                    ],
                    "common_phrases": [
                        "in the aftermath",
                        "immediate aftermath",
                        "aftermath of the disaster"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, последствия или отголоски, возникающие после значительного, обычно разрушительного или негативного события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последствия",
                                "target_lang_sense_clarification": "результат или состояние после события"
                            },
                            {
                                "target_lang_word": "отголоски",
                                "target_lang_sense_clarification": "остаточные явления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14dc0021-019d-368f-9f18-b67b979ea994",
                    "sense_definition": "A second crop of grass that grows after the first harvest of the season.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmers returned to the fields to cut the <w>aftermath</w> for winter feed.",
                            "target_lang_translations": {
                                "ru": "Фермеры вернулись на поля, чтобы скосить <w>отаву</w> на зимний корм."
                            }
                        },
                        {
                            "text": "Cattle were allowed to graze on the lush <w>aftermath</w> late in the autumn.",
                            "target_lang_translations": {
                                "ru": "Скоту позволили пастись на сочной <w>отаве</w> поздней осенью."
                            }
                        }
                    ],
                    "synonyms": [
                        "lattermath"
                    ],
                    "target_lang_definitions": {
                        "ru": "Второй урожай травы, вырастающий после первого покоса в сезоне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отава",
                                "target_lang_sense_clarification": "трава, выросшая на месте скошенной"
                            },
                            {
                                "target_lang_word": "второй покос",
                                "target_lang_sense_clarification": "сбор травы во второй раз за сезон"
                            }
                        ]
                    }
                }
            ]
        }
    ]

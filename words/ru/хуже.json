    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a0211024-436c-3d37-b739-9b12a63f6ca7",
                    "sense_definition": "Сравнительная степень прилагательного <w>плохой</w>; обозначает более низкое качество или свойства по сравнению с чем-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот фильм оказался <w>хуже</w>, чем я думал.",
                            "target_lang_translations": {
                                "en": "This movie turned out to be <w>worse</w> than I thought."
                            }
                        },
                        {
                            "text": "Погода сегодня <w>хуже</w> некуда, поэтому мы остались дома.",
                            "target_lang_translations": {
                                "en": "The weather today couldn't be <w>worse</w>, so we stayed home."
                            }
                        }
                    ],
                    "antonyms": [
                        "лучше"
                    ],
                    "common_phrases": [
                        "хуже некуда",
                        "тем хуже",
                        "не хуже других"
                    ],
                    "target_lang_definitions": {
                        "en": "Comparative degree of the adjective 'bad'; indicates lower quality or properties compared to something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "worse",
                                "target_lang_sense_clarification": "lower quality"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "449b1ccd-79f5-3470-822c-8ffc0f1e9042",
                    "sense_definition": "О ситуации, когда физическое самочувствие или положение дел становится более тяжёлым или неприятным (используется как сказуемое).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Больному стало <w>хуже</w> к вечеру.",
                            "target_lang_translations": {
                                "en": "The patient got <w>worse</w> by evening."
                            }
                        },
                        {
                            "text": "Мне <w>хуже</w> не стало, но голова всё ещё болит.",
                            "target_lang_translations": {
                                "en": "I didn't get <w>worse</w>, but my head still hurts."
                            }
                        }
                    ],
                    "antonyms": [
                        "лучше"
                    ],
                    "common_phrases": [
                        "стало хуже",
                        "бывает хуже"
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to a situation where physical well-being or the state of affairs becomes more difficult or unpleasant."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "worse",
                                "target_lang_sense_clarification": "health or condition"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c038c627-0b50-37c6-a9fe-485c897c1b56",
                    "sense_definition": "Сравнительная степень наречия <w>плохо</w>; выполнение действия с меньшим успехом или качеством.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он видит <w>хуже</w> без своих очков.",
                            "target_lang_translations": {
                                "en": "He sees <w>worse</w> without his glasses."
                            }
                        },
                        {
                            "text": "Дела в компании идут всё <w>хуже</w> и <w>хуже</w>.",
                            "target_lang_translations": {
                                "en": "Business in the company is going <w>worse</w> and <w>worse</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "лучше"
                    ],
                    "common_phrases": [
                        "всё хуже и хуже",
                        "значительно хуже",
                        "куда хуже"
                    ],
                    "target_lang_definitions": {
                        "en": "Comparative degree of the adverb 'badly'; execution of an action with less success or quality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "worse",
                                "target_lang_sense_clarification": "less effectively"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "being"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2e3b75f4-a64f-396d-bccd-837c88022cbe",
                    "sense_definition": "The first-person singular present form of the verb <w>be</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>am</w> very happy to see you today.",
                            "target_lang_translations": {
                                "ru": "Я очень <w>рад</w> видеть вас сегодня."
                            }
                        },
                        {
                            "text": "I <w>am</w> sorry for the mistake I made.",
                            "target_lang_translations": {
                                "ru": "Мне <w>жаль</w> из-за сделанной ошибки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "I am sure",
                        "I am sorry",
                        "I am ready",
                        "here I am"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола <w>be</w> (быть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "в значении существования (часто опускается)"
                            },
                            {
                                "target_lang_word": "являюсь",
                                "target_lang_sense_clarification": "формальная связка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7177af87-669c-310d-a823-297a20e2b263",
                    "sense_definition": "Used to indicate time between midnight and noon (ante meridiem).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train departs at 10 <w>am</w> sharp.",
                            "target_lang_translations": {
                                "ru": "Поезд отправляется ровно в 10 <w>утра</w>."
                            }
                        },
                        {
                            "text": "I usually wake up at 7 <w>am</w> on weekdays.",
                            "target_lang_translations": {
                                "ru": "Я обычно просыпаюсь в 7 <w>утра</w> по будням."
                            }
                        }
                    ],
                    "antonyms": [
                        "pm"
                    ],
                    "common_phrases": [
                        "9 am",
                        "11 am"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения времени с полуночи до полудня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утра",
                                "target_lang_sense_clarification": "при указании времени (напр. 10 утра)"
                            },
                            {
                                "target_lang_word": "до полудня",
                                "target_lang_sense_clarification": "формальное обозначение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b14ea4d9-cc6e-3acb-9cde-10c53f7592f2",
                    "sense_definition": "A system of radio broadcasting using amplitude modulation.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "radio_technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We listened to the news on <w>AM</w> radio.",
                            "target_lang_translations": {
                                "ru": "Мы слушали новости по <w>AM</w>-радио."
                            }
                        },
                        {
                            "text": "The <w>AM</w> signal reaches further than FM in this area.",
                            "target_lang_translations": {
                                "ru": "Сигнал <w>AM</w> в этой местности распространяется дальше, чем FM."
                            }
                        }
                    ],
                    "antonyms": [
                        "FM"
                    ],
                    "common_phrases": [
                        "AM radio",
                        "AM band"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система радиовещания, использующая амплитудную модуляцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "АМ",
                                "target_lang_sense_clarification": "диапазон радиовещания"
                            },
                            {
                                "target_lang_word": "амплитудная модуляция",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a3e0229-17df-3359-b389-3310c02e7420",
                    "sense_definition": "A master's degree in the arts or humanities (Master of Arts).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "academic_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He received his <w>AM</w> in History from the university.",
                            "target_lang_translations": {
                                "ru": "Он получил степень <w>магистра искусств</w> по истории в университете."
                            }
                        },
                        {
                            "text": "She is studying for an <w>AM</w> degree.",
                            "target_lang_translations": {
                                "ru": "Она учится на степень <w>магистра искусств</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "MA"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень магистра гуманитарных наук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магистр искусств",
                                "target_lang_sense_clarification": "ученая степень"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
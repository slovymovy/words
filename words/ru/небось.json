    "entries": [
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "500d2766-6f31-3144-88db-778aeb0d7c8e",
                    "sense_definition": "Вводное слово, выражающее предположение, догадку или неуверенность говорящего; соответствует по значению словам «наверное», «вероятно», «должно быть».",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "assumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты, <w>небось</w>, проголодался после такой долгой прогулки по лесу?",
                            "target_lang_translations": {
                                "en": "You <w>must be</w> hungry after such a long walk in the forest?"
                            }
                        },
                        {
                            "text": "У вас там <w>небось</w> уже настоящая зима наступила.",
                            "target_lang_translations": {
                                "en": "<w>I suppose</w> real winter has already set in over there."
                            }
                        }
                    ],
                    "synonyms": [
                        "наверное",
                        "вероятно",
                        "должно быть",
                        "пожалуй",
                        "поди"
                    ],
                    "common_phrases": [
                        "небось устал",
                        "небось забыл",
                        "небось холодно"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An introductory word expressing a guess, assumption, or uncertainty; corresponds to \"probably\", \"likely\", or \"must be\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "probably",
                                "target_lang_sense_clarification": "general assumption"
                            },
                            {
                                "target_lang_word": "I suppose",
                                "target_lang_sense_clarification": "expressing a guess"
                            },
                            {
                                "target_lang_word": "I bet",
                                "target_lang_sense_clarification": "colloquial, strong assumption"
                            },
                            {
                                "target_lang_word": "must be",
                                "target_lang_sense_clarification": "logical deduction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed130ee1-3842-3bf0-9892-e58fc0fe74e7",
                    "sense_definition": "Употребляется для выражения уверенности, успокоения или ободрения собеседника; близко по значению к «не бойся», «конечно», «безусловно».",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reassurance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Да ты не дрожи так, <w>небось</w>, не обидят тебя здесь.",
                            "target_lang_translations": {
                                "en": "Don't shiver like that, <w>don't worry</w>, they won't hurt you here."
                            }
                        },
                        {
                            "text": "<w>Небось</w>, справишься с этой задачей, она не такая уж сложная.",
                            "target_lang_translations": {
                                "en": "<w>Surely</w> you'll manage this task, it's not that complicated."
                            }
                        }
                    ],
                    "synonyms": [
                        "поди",
                        "чай"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express confidence, reassurance, or encouragement; similar to \"don't be afraid\", \"surely\", or \"no doubt\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "don't worry",
                                "target_lang_sense_clarification": "reassurance"
                            },
                            {
                                "target_lang_word": "surely",
                                "target_lang_sense_clarification": "expressing confidence"
                            },
                            {
                                "target_lang_word": "don't be afraid",
                                "target_lang_sense_clarification": "literal meaning (etymological)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
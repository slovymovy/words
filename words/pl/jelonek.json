            "senses": [
                {
                    "sense_id": "645ea52b-41d6-37b5-b0ee-453d03df732d",
                    "sense_definition": "Młody osobnik z gatunku jeleni; zdrobnienie od słowa <w>jeleń</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "young_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na łące widzieliśmy sarnę z małym <w>jelonkiem</w>.",
                            "target_lang_translations": {
                                "ru": "На лугу мы видели косулю с маленьким <w>оленёнком</w>."
                            }
                        },
                        {
                            "text": "Ten <w>jelonek</w> zgubił swoją mamę i błąkał się po lesie.",
                            "target_lang_translations": {
                                "ru": "Этот <w>оленёнок</w> потерял свою маму и бродил по лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "jelenię"
                    ],
                    "common_phrases": [
                        "mały jelonek",
                        "stado jelonków"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Детёныш оленя; уменьшительная форма от слова \"олень\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оленёнок",
                                "target_lang_sense_clarification": "детёныш"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91d8d4ae-0583-3b9c-92ed-65256f1a7943",
                    "sense_definition": "Gatunek dużego chrząszcza, którego samce posiadają żuwaczki przypominające poroże jelenia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "insect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jelonek</w> rogacz jest w Polsce gatunkiem chronionym.",
                            "target_lang_translations": {
                                "ru": "<w>Жук-олень</w> в Польше является охраняемым видом."
                            }
                        },
                        {
                            "text": "W starym lesie dębowym udało nam się znaleźć rzadkiego <w>jelonka</w>.",
                            "target_lang_translations": {
                                "ru": "В старом дубовом лесу нам удалось найти редкого <w>жука-оленя</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jelonek rogacz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид крупного жука, самцы которого имеют жвалы, напоминающие рога оленя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жук-олень",
                                "target_lang_sense_clarification": "насекомое"
                            }
                        ]
                    }
                }
            ]
        },
        {

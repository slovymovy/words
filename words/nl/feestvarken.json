    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "693c4387-cde0-3575-85bb-5d175396b95b",
                    "sense_definition": "De persoon voor wie een feest wordt gevierd, vaak omdat hij of zij jarig is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_celebrated",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vandaag is mijn kleine zusje het <w>feestvarken</w>.",
                            "target_lang_translations": {
                                "ru": "Сегодня моя младшая сестренка — <w>именинница</w>."
                            }
                        },
                        {
                            "text": "Alle gasten zongen luidkeels een lied voor het <w>feestvarken</w>.",
                            "target_lang_translations": {
                                "ru": "Все гости громко пели песню для <w>виновника торжества</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "eregast",
                        "jarige"
                    ],
                    "common_phrases": [
                        "het feestvarken zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, в честь которого устроен праздник, часто по случаю дня рождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "именинник",
                                "target_lang_sense_clarification": "человек, празднующий день рождения"
                            },
                            {
                                "target_lang_word": "виновник торжества",
                                "target_lang_sense_clarification": "главное лицо праздника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "316ee25f-f1c5-3a4a-aa9a-6d6e080603aa",
                    "sense_definition": "Een varken dat speciaal voor een feestelijke gelegenheid wordt geslacht en gegeten.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "animal_food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het middeleeuwse banket werd een groot <w>feestvarken</w> aan het spit geregen.",
                            "target_lang_translations": {
                                "ru": "Во время средневекового пира на вертел насадили большого <w>праздничного поросенка</w>."
                            }
                        },
                        {
                            "text": "De slager bereidde het <w>feestvarken</w> voor op het grote dorpsfeest.",
                            "target_lang_translations": {
                                "ru": "Мясник готовил <w>праздничную свинью</w> к большому деревенскому празднику."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Свинья, специально откормленная и приготовленная для праздничного застолья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "праздничный поросенок",
                                "target_lang_sense_clarification": "животное или блюдо для праздника"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
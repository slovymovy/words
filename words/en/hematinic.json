    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "763fadf6-1aa3-3927-b578-8f8509c6d761",
                    "sense_definition": "Relating to a substance or treatment that improves the quality of blood by increasing hemoglobin or red blood cells.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "blood_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor prescribed a <w>hematinic</w> supplement to help treat the patient's anemia.",
                            "target_lang_translations": {
                                "ru": "Врач назначил <w>гематиническую</w> добавку для лечения анемии у пациента."
                            }
                        },
                        {
                            "text": "Spinach is often praised for its <w>hematinic</w> properties due to its iron content.",
                            "target_lang_translations": {
                                "ru": "Шпинат часто хвалят за его <w>гематинические</w> свойства из-за содержания железа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hematinic agent",
                        "hematinic effect",
                        "hematinic activity"
                    ],
                    "target_lang_definitions": {

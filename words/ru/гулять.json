            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4eed703f-f0bf-3b54-9337-fc2dc31daf0b",
                    "sense_definition": "Ходить пешком на свежем воздухе ради отдыха или удовольствия.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В хорошую погоду мы любим <гулять> в городском парке.",
                            "target_lang_translations": {
                                "en": "In good weather, we like to <walk> in the city park."
                            }
                        },
                        {
                            "text": "По вечерам соседи выходят <гулять> со своими собаками.",
                            "target_lang_translations": {
                                "en": "In the evenings, neighbors go out to <walk> their dogs."
                            }
                        }
                    ],
                    "synonyms": [
                        "сойтись",
                        "веселиться",
                        "развлекаться",
                        "кутить"
                    ],
                    "antonyms": [
                        "стоять",
                        "лежать"
                    ],
                    "common_phrases": [
                        "пойти гулять",
                        "гулять с собакой",
                        "гулять в парке",
                        "гулять пешком"
                    ],
                    "target_lang_definitions": {
                        "en": "To walk outdoors for leisure or pleasure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "walk",
                                "target_lang_sense_clarification": "general movement on foot"
                            },
                            {
                                "target_lang_word": "stroll",
                                "target_lang_sense_clarification": "walking in a leisurely way"
                            },
                            {
                                "target_lang_word": "go for a walk",
                                "target_lang_sense_clarification": "common phrase for the activity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b9c4e97-47ae-33ce-ac50-6cbca41504e0",
                    "sense_definition": "Веселиться, развлекаться, отмечать праздник (часто с употреблением алкоголя).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "leisure_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вся деревня три дня <гуляла> на шумной свадьбе.",
                            "target_lang_translations": {
                                "en": "The whole village <partied> at the noisy wedding for three days."
                            }
                        },
                        {
                            "text": "Студенты сдали последнюю сессию и <гуляли> до самого утра.",
                            "target_lang_translations": {
                                "en": "The students passed their last session and <partied> until the very morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "веселиться",
                        "развлекаться",
                        "кутить"
                    ],
                    "common_phrases": [
                        "гулять на свадьбе",
                        "гулять всю ночь",
                        "гулять на все"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To party, celebrate, or have a good time (often involving alcohol)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "party",
                                "target_lang_sense_clarification": "to celebrate socially, often noisily"
                            },
                            {
                                "target_lang_word": "celebrate",
                                "target_lang_sense_clarification": "to observe a notable occasion"
                            },
                            {
                                "target_lang_word": "carouse",
                                "target_lang_sense_clarification": "to drink and enjoy oneself with others"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79635a60-cc3a-3a1d-91b5-d961a77128de",
                    "sense_definition": "Быть свободным от работы или учёбы, иметь выходной день.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leisure_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Праздник пришёлся на четверг, так что мы <гуляем> четыре дня подряд.",
                            "target_lang_translations": {
                                "en": "The holiday fell on a Thursday, so we <are off> for four days in a row."
                            }
                        },
                        {
                            "text": "Начальник подписал отпуск, и теперь я <гуляю> целый месяц.",
                            "target_lang_translations": {
                                "en": "The boss signed the vacation request, and now I <am off> for a whole month."
                            }
                        }
                    ],
                    "common_phrases": [
                        "завтра гуляем",
                        "гулять два дня"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be free from work or study; to have time off."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be off",
                                "target_lang_sense_clarification": "to not be at work"
                            },
                            {
                                "target_lang_word": "have time off",
                                "target_lang_sense_clarification": "to have a break from duties"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d88c1efe-f719-349d-a1ce-228c7cdba7c9",
                    "sense_definition": "Встречаться с кем-либо, иметь романтические отношения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relationships_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она <гуляла> с этим парнем ещё в старших классах школы.",
                            "target_lang_translations": {
                                "en": "She <dated> this guy back in high school."
                            }
                        },
                        {
                            "text": "Родители строго запрещали дочери <гулять> допоздна.",
                            "target_lang_translations": {
                                "en": "Parents strictly forbade their daughter to <go out> late."
                            }
                        }
                    ],
                    "common_phrases": [
                        "гулять с парнем",
                        "гулять с девушкой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To meet with someone romantically; to be in a relationship."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "date",
                                "target_lang_sense_clarification": "to go on dates or be in a relationship"
                            },
                            {
                                "target_lang_word": "go out",
                                "target_lang_sense_clarification": "to have a romantic relationship with someone"
                            },
                            {
                                "target_lang_word": "see",
                                "target_lang_sense_clarification": "to be dating someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "654c9e57-dfe7-38b9-8e15-d02578725961",
                    "sense_definition": "Изменять супругу или постоянному партнёру, иметь связи на стороне.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relationships_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все соседи знали, что её муж давно <гуляет>.",
                            "target_lang_translations": {
                                "en": "All the neighbors knew that her husband had been <cheating> for a long time."
                            }
                        },
                        {
                            "text": "Он был примерным семьянином и никогда не <гулял> от жены.",
                            "target_lang_translations": {
                                "en": "He was a model family man and never <cheated> on his wife."
                            }
                        }
                    ],
                    "common_phrases": [
                        "муж гуляет",
                        "гулять от жены",
                        "гулять налево"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cheat on a spouse or partner; to have an affair."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cheat",
                                "target_lang_sense_clarification": "to be sexually unfaithful"
                            },
                            {
                                "target_lang_word": "have an affair",
                                "target_lang_sense_clarification": "to have a sexual relationship outside marriage"
                            },
                            {
                                "target_lang_word": "sleep around",
                                "target_lang_sense_clarification": "to have many sexual partners"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fac5b09e-2fce-3247-ab16-112f4c875d21",
                    "sense_definition": "Свободно перемещаться в разных направлениях (о ветре, сквозняке, волнах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старом деревянном доме по полу постоянно <гуляли> сквозняки.",
                            "target_lang_translations": {
                                "en": "Drafts constantly <blew> along the floor in the old wooden house."
                            }
                        },
                        {
                            "text": "Холодный ветер свободно <гулял> по пустому полю.",
                            "target_lang_translations": {
                                "en": "A cold wind <roamed> freely across the empty field."
                            }
                        }
                    ],
                    "antonyms": [
                        "застыть"
                    ],
                    "common_phrases": [
                        "ветер гуляет",
                        "сквозняк гуляет",
                        "волны гуляют"
                    ],
                    "target_lang_definitions": {
                        "en": "To move freely in different directions (of wind, drafts, or waves)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blow",
                                "target_lang_sense_clarification": "movement of air"
                            },
                            {
                                "target_lang_word": "roam",
                                "target_lang_sense_clarification": "to move about without a fixed destination"
                            },
                            {
                                "target_lang_word": "wander",
                                "target_lang_sense_clarification": "to move casually through a space"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55c2efc9-315d-32cd-bfd4-86f44a8525c7",
                    "sense_definition": "Находиться в бездействии, не использоваться (о механизмах, земле или помещении).",

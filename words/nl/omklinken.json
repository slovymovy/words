{
    "word_family": [
        "klinken",
        "klinknagel",
        "klink",
        "verklinken"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7b3d82a4-4bb8-3076-8831-b20fbdba20a2",
                    "sense_definition": "De uitstekende punt van een spijker of bout ombuigen en platslaan zodat deze stevig vastzit.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "bevestigen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De timmerman moest de spijker aan de achterkant <w>omklinken</w>.",
                            "target_lang_translations": {
                                "ru": "Плотнику нужно было <w>загнуть</w> гвоздь с обратной стороны.",
                                "en": "The carpenter had to <w>clinch</w> the nail at the back."
                            }
                        },
                        {
                            "text": "Hij <w>klonk</w> de nagels <w>om</w> om de verbinding extra sterk te maken.",
                            "target_lang_translations": {
                                "ru": "Он <w>загнул</w> гвозди, чтобы сделать соединение особенно прочным.",
                                "en": "He <w>clinched</w> the nails to make the connection extra strong."
                            }
                        }
                    ],
                    "synonyms": [
                        "ombuigen"
                    ],
                    "common_phrases": [
                        "een spijker omklinken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "scheidbaar werkwoord"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Загнуть и расплющить выступающий конец гвоздя или болта для прочного закрепления.",
                        "en": "To bend over and flatten the protruding point of a nail or bolt to secure it."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загибать",
                                "target_lang_sense_clarification": "гвоздь (загнуть выступающий конец)"
                            },
                            {
                                "target_lang_word": "расклепывать",
                                "target_lang_sense_clarification": "расплющивать конец болта или заклепки"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "clinch",
                                "target_lang_sense_clarification": "to secure a nail by bending the tip"
                            },
                            {
                                "target_lang_word": "rivet",
                                "target_lang_sense_clarification": "to fasten with a metal pin"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67d315a7-853e-307d-80e7-6a10240a2743",
                    "sense_definition": "Iets of iemand volledig omsluiten of omringen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "omsluiten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hoge, oude muren <w>omklonken</w> de verborgen binnentuin.",
                            "target_lang_translations": {
                                "ru": "Высокие старые стены <w>окружали</w> скрытый внутренний сад.",
                                "en": "High, old walls <w>enclosed</w> the hidden courtyard."
                            }
                        },
                        {
                            "text": "Een diepe stilte <w>omklonk</w> het verlaten landhuis in het bos.",
                            "target_lang_translations": {
                                "ru": "Глубокая тишина <w>окружала</w> заброшенную усадьбу в лесу.",
                                "en": "A deep silence <w>surrounded</w> the abandoned country house in the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "omsluiten",
                        "omvatten",
                        "omringen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "onscheidbaar werkwoord"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью окружать или обступать что-либо или кого-либо.",
                        "en": "To completely enclose or surround something or someone."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окружать",
                                "target_lang_sense_clarification": "находиться вокруг"
                            },
                            {
                                "target_lang_word": "опоясывать",
                                "target_lang_sense_clarification": "окаймлять (о стенах, оградах)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "surround",
                                "target_lang_sense_clarification": "to be all around"
                            },
                            {
                                "target_lang_word": "enclose",
                                "target_lang_sense_clarification": "to shut in on all sides"
                            },
                            {
                                "target_lang_word": "encircle",
                                "target_lang_sense_clarification": "to form a circle around"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hung",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A large wooden platform was <w>suspended</w> from the ceiling by thick chains.",
                            "target_lang_translations": {
                                "ru": "Большая деревянная платформа была <w>подвешена</w> к потолку на толстых цепях."
                            }
                        },
                        {
                            "text": "The bridge is <w>suspended</w> over the canyon by steel cables.",
                            "target_lang_translations": {
                                "ru": "Мост <w>подвешен</w> над каньоном на стальных тросах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "suspended ceiling",
                        "suspended load"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвешенный сверху; висящий в пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвесной",
                                "target_lang_sense_clarification": "о конструкции (потолок, мост)"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1bf18c9d-1251-335a-b330-f12664431a27",
                    "sense_definition": "Крупный пассажирский самолёт, совершающий регулярные рейсы.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transport_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В аэропорту приземлился огромный пассажирский <w>лайнер</w> из Москвы.",
                            "target_lang_translations": {
                                "en": "A huge passenger <w>airliner</w> from Moscow landed at the airport."
                            }
                        },
                        {
                            "text": "Современный реактивный <w>лайнер</w> способен преодолеть это расстояние за несколько часов.",
                            "target_lang_translations": {
                                "en": "A modern <w>jetliner</w> is capable of covering this distance in a few hours."
                            }
                        }
                    ],
                    "synonyms": [
                        "самолёт",
                        "авиалайнер",
                        "аэробус",
                        "борт"
                    ],
                    "common_phrases": [
                        "пассажирский лайнер",
                        "реактивный лайнер",
                        "комфортабельный лайнер"
                    ],
                    "target_lang_definitions": {
                        "en": "A large passenger aircraft making regular flights."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "airliner",
                                "target_lang_sense_clarification": "passenger aircraft"
                            },
                            {
                                "target_lang_word": "jetliner",
                                "target_lang_sense_clarification": "jet-powered aircraft"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0605268-f573-3aa5-9ad9-0b0d82d6f567",
                    "sense_definition": "Крупное морское судно (обычно пассажирское), совершающее рейсы по определённому маршруту.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Белоснежный круизный <w>лайнер</w> медленно отходил от причала.",
                            "target_lang_translations": {
                                "en": "The snow-white cruise <w>liner</w> slowly departed from the pier."
                            }
                        },
                        {
                            "text": "Путешествие через океан на таком судне напоминает отдых в плавучем отеле, ведь океанский <w>лайнер</w> оборудован всем необходимым.",
                            "target_lang_translations": {
                                "en": "A journey across the ocean on such a vessel resembles a vacation in a floating hotel, as an ocean <w>liner</w> is equipped with everything necessary."
                            }
                        }
                    ],
                    "synonyms": [
                        "судно",
                        "теплоход",
                        "корабль"
                    ],
                    "common_phrases": [
                        "океанский лайнер",
                        "круизный лайнер",
                        "морской лайнер"
                    ],
                    "target_lang_definitions": {
                        "en": "A large sea vessel (usually passenger) making voyages on a specific route."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "liner",
                                "target_lang_sense_clarification": "passenger ship"
                            },
                            {
                                "target_lang_word": "ocean liner",
                                "target_lang_sense_clarification": "type of ship"
                            },
                            {
                                "target_lang_word": "cruise ship",
                                "target_lang_sense_clarification": "modern equivalent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1519f10f-5c70-3137-bd39-c84022e5453a",
                    "sense_definition": "Капиллярная ручка с тонким стержнем, используемая для черчения и рисования.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Художник взял чёрный <w>лайнер</w>, чтобы прорисовать мелкие детали на эскизе.",
                            "target_lang_translations": {
                                "en": "The artist took a black <w>fineliner</w> to draw small details on the sketch."
                            }
                        },
                        {
                            "text": "Для создания ровных контуров лучше всего использовать качественный <w>лайнер</w>.",
                            "target_lang_translations": {
                                "en": "To create smooth contours, it is best to use a high-quality <w>fineliner</w>."

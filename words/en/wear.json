    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6aef0a98-d0b5-3298-98ff-ef6852f8dcaa",
                    "sense_definition": "To have clothing, accessories, or equipment on one's body.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>wears</w> a uniform to work every day.",
                            "target_lang_translations": {
                                "ru": "Она <w>носит</w> форму на работу каждый день."
                            }
                        },
                        {
                            "text": "He <w>wore</w> a heavy coat because it was snowing outside.",
                            "target_lang_translations": {
                                "ru": "Он <w>был одет</w> в теплое пальто, так как на улице шел снег."
                            }
                        }
                    ],
                    "synonyms": [
                        "gybe"
                    ],
                    "common_phrases": [
                        "wear clothes",
                        "wear a hat",
                        "wear shoes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь одежду, аксессуары или снаряжение на своем теле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "носить",
                                "target_lang_sense_clarification": "иметь на себе (об одежде)"
                            },
                            {
                                "target_lang_word": "быть одетым в",
                                "target_lang_sense_clarification": "состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac9cacf4-f68b-389e-b05c-2056016b6afb",
                    "sense_definition": "To have or carry on one's person habitually, such as a hairstyle, glasses, or beard.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandfather <w>wears</w> glasses for reading.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка <w>носит</w> очки для чтения."
                            }
                        },
                        {
                            "text": "She <w>wears</w> her hair in a long ponytail.",
                            "target_lang_translations": {
                                "ru": "Она <w>носит</w> волосы собранными в длинный хвост."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wear glasses",
                        "wear one's hair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянно иметь при себе или на себе (например, прическу, очки или бороду)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "носить",
                                "target_lang_sense_clarification": "очки, прическу, бороду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4eee415-d6b6-3437-b8e9-9187d104cbe8",
                    "sense_definition": "To display a particular facial expression or manner.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>wore</w> a smile despite the bad news.",
                            "target_lang_translations": {
                                "ru": "Он <w>улыбался</w>, несмотря на плохие новости."
                            }
                        },
                        {
                            "text": "The old house <w>wore</w> an air of sadness and neglect.",
                            "target_lang_translations": {
                                "ru": "Старый дом <w>имел</w> печальный и заброшенный вид."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wear a smile",
                        "wear a look"
                    ],
                    "target_lang_definitions": {
                        "ru": "Демонстрировать определенное выражение лица или манеру держаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь вид",
                                "target_lang_sense_clarification": "выглядеть определенным образом"
                            },
                            {
                                "target_lang_word": "выражать",
                                "target_lang_sense_clarification": "о лице, эмоциях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9413e9b3-2baa-312a-9a7d-2ae41e1233fc",
                    "sense_definition": "To damage, erode, or consume something gradually through friction or constant use.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_erosion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The flowing water has <w>worn</w> a channel through the rock.",
                            "target_lang_translations": {
                                "ru": "Текущая вода <w>проточила</w> канал в скале."
                            }
                        },
                        {
                            "text": "Walking so much will <w>wear</w> holes in your socks.",
                            "target_lang_translations": {
                                "ru": "От такой долгой ходьбы в твоих носках <w>протрутся</w> дырки."
                            }
                        }
                    ],
                    "synonyms": [
                        "gybe"
                    ],
                    "common_phrases": [
                        "wear a hole",
                        "wear away",
                        "wear down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постепенно повреждать, разрушать или истирать что-либо трением или постоянным использованием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "истирать",
                                "target_lang_sense_clarification": "разрушать трением"
                            },
                            {
                                "target_lang_word": "протирать",
                                "target_lang_sense_clarification": "делать дыры трением"
                            },
                            {
                                "target_lang_word": "протачивать",
                                "target_lang_sense_clarification": "о воде/камне"
                            }
                        ]

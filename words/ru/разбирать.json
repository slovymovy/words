                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seize",
                                "target_lang_sense_clarification": "of a sudden strong emotion"
                            },
                            {
                                "target_lang_word": "overcome",
                                "target_lang_sense_clarification": "to overpower or overwhelm"
                            },
                            {
                                "target_lang_word": "take hold of",
                                "target_lang_sense_clarification": "to begin to affect strongly"
                            }
                        ]
                    }
                }

                            },
                            {
                                "target_lang_word": "достаточный",
                                "target_lang_sense_clarification": "вполне хватающий для нужд"
                            },
                            {
                                "target_lang_word": "уверенный",
                                "target_lang_sense_clarification": "о преимуществе или победе в спорте"
                            },
                            {
                                "target_lang_word": "безбедный",
                                "target_lang_sense_clarification": "о жизни или существовании"
                            }
                        ]
                    }

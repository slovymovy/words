    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3bab0e31-909c-34d5-840d-0cba8d0f4b5a",
                    "sense_definition": "Having a special quality, character, or appearance that is different and easy to recognize.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "characteristic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The male bird has <w>distinctive</w> blue markings on its wings.",
                            "target_lang_translations": {
                                "ru": "У самца этой птицы есть <w>отличи́тельные</w> синие отметины на крыльях."
                            }
                        },
                        {
                            "text": "She has a very <w>distinctive</w> way of walking that makes her easy to spot.",
                            "target_lang_translations": {
                                "ru": "У неё очень <w>характе́рная</w> походка, благодаря которой её легко узнать."
                            }
                        },
                        {
                            "text": "The restaurant is famous for the <w>distinctive</w> flavor of its sauces.",
                            "target_lang_translations": {
                                "ru": "Этот ресторан славится <w>осо́бым</w> вкусом своих соусов."
                            }
                        }
                    ],
                    "synonyms": [
                        "characteristic",
                        "unique",
                        "distinguishing",
                        "peculiar",
                        "typical"
                    ],
                    "antonyms": [
                        "ordinary",
                        "common",
                        "nondescript"
                    ],
                    "common_phrases": [
                        "distinctive feature",
                        "distinctive style",
                        "distinctive voice",
                        "distinctive look"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий особое качество, характер или внешний вид, который отличается от других и легко узнаваем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "характе́рный",
                                "target_lang_sense_clarification": "свойственный определенному типу или лицу"
                            },
                            {
                                "target_lang_word": "отличи́тельный",
                                "target_lang_sense_clarification": "служащий признаком отличия"
                            },
                            {
                                "target_lang_word": "осо́бый",
                                "target_lang_sense_clarification": "не похожий на других, своеобразный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "174d044a-ea20-3705-a794-bbe81b92f78a",
                    "sense_definition": "Serving to distinguish between meanings or sounds in a language.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In English, voicing is a <w>distinctive</w> feature that separates 'p' from 'b'.",
                            "target_lang_translations": {
                                "ru": "В английском языке звонкость — это <w>различи́тельный</w> признак, который отличает звук «p» от «b»."
                            }
                        },
                        {
                            "text": "Linguists analyze the <w>distinctive</w> elements of phonemes to understand speech patterns.",
                            "target_lang_translations": {
                                "ru": "Лингвисты анализируют <w>различи́тельные</w> элементы фонем, чтобы понять речевые модели."
                            }
                        }
                    ],
                    "synonyms": [
                        "distinguishing",
                        "diacritic"
                    ],
                    "common_phrases": [
                        "distinctive feature"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "linguistics"
                        }
                    ],
                    "target_lang_definitions": {

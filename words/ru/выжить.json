    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e5b918c5-008f-3bee-b911-636e5bcb7dd0",
                    "sense_definition": "Остаться в живых, продолжить существование, несмотря на смертельную опасность, болезнь или тяжёлые условия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "survival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи сделали всё возможное, чтобы пациент смог <w>выжить</w> после тяжёлой операции.",
                            "target_lang_translations": {
                                "en": "The doctors did everything possible so the patient could <w>survive</w> after the major surgery."
                            }
                        },
                        {
                            "text": "В суровом климате севера без тёплой одежды и укрытия невозможно <w>выжить</w>.",
                            "target_lang_translations": {
                                "en": "In the harsh climate of the north, it is impossible to <w>survive</w> without warm clothes and shelter."
                            }
                        }
                    ],
                    "antonyms": [
                        "погибнуть",
                        "вымереть"
                    ],
                    "common_phrases": [
                        "чудом выжить",
                        "шанс выжить",
                        "выжить любой ценой",
                        "выжить в катастрофе"
                    ],
                    "target_lang_definitions": {
                        "en": "To remain alive or continue to exist despite deadly danger, illness, or harsh conditions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "survive",
                                "target_lang_sense_clarification": "remain alive"
                            },
                            {
                                "target_lang_word": "pull through",
                                "target_lang_sense_clarification": "recover from serious illness or danger"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c753d7f-61ec-3712-a0a0-a1cbf3af1312",
                    "sense_definition": "Заставить кого-либо покинуть помещение, место жительства или работы, намеренно создавая невыносимые условия.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "force_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новые соседи вели себя так шумно и нагло, словно хотели <w>выжить</w> нас из собственной квартиры.",
                            "target_lang_translations": {
                                "en": "The new neighbors behaved so noisily and brazenly, as if they wanted to <w>drive</w> us <w>out</w> of our own apartment."
                            }
                        },
                        {
                            "text": "Завистливые коллеги плели интриги, стараясь <w>выжить</w> успешного сотрудника из отдела.",
                            "target_lang_translations": {
                                "en": "Envious colleagues schemed, trying to <w>force</w> the successful employee <w>out</w> of the department."
                            }
                        }
                    ],
                    "synonyms": [
                        "изгнать",
                        "выдворить",
                        "вытурить"
                    ],
                    "common_phrases": [
                        "выжить из дома",
                        "выжить с работы",
                        "выжить из квартиры"
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone to leave a place of residence or work by intentionally creating unbearable conditions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "force out",
                                "target_lang_sense_clarification": "compel to leave"
                            },
                            {
                                "target_lang_word": "drive out",
                                "target_lang_sense_clarification": "expel by pressure"
                            },
                            {
                                "target_lang_word": "squeeze out",
                                "target_lang_sense_clarification": "pressure to leave gradually"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
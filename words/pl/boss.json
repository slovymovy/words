{
    "word_family": [
        "bossowski"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a9cc35aa-a744-3387-bff2-a795ab342018",
                    "sense_definition": "Szef firmy lub organizacji; osoba zarządzająca i podejmująca decyzje.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz nowy <w>boss</w> wprowadził w biurze bardzo surowe zasady.",
                            "target_lang_translations": {
                                "ru": "Наш новый <w>босс</w> ввел в офисе очень строгие правила."
                            }
                        },
                        {
                            "text": "Muszę zapytać o zgodę, zanim <w>boss</w> wyjdzie na ważne spotkanie.",
                            "target_lang_translations": {
                                "ru": "Мне нужно спросить разрешения, прежде чем <w>босс</w> уйдет на важную встречу."
                            }
                        }
                    ],
                    "synonyms": [
                        "szef",
                        "zwierzchnik"
                    ],
                    "common_phrases": [
                        "wielki boss",
                        "mój boss"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo często używane w kontekście biznesowym, ale mniej formalne niż prezes czy dyrektor"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Руководитель компании или организации; лицо, принимающее решения (часто разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "босс",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "шеф",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "начальник",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "439f90dd-3349-3958-9db5-e0f0b5f19bb0",
                    "sense_definition": "Przywódca zorganizowanej grupy przestępczej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja aresztowała groźnego <w>bossa</w> mafii sycylijskiej podczas nalotu.",
                            "target_lang_translations": {
                                "ru": "Полиция арестовала опасного <w>босса</w> сицилийской мафии во время облавы."
                            }
                        },
                        {
                            "text": "Młody chłopak szybko zyskał zaufanie i został przedstawiony <w>bossowi</w>.",
                            "target_lang_translations": {
                                "ru": "Молодой парень быстро завоевал доверие и был представлен <w>боссу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "szef"
                    ],
                    "common_phrases": [
                        "boss mafii",
                        "boss narkotykowy",
                        "boss gangu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лидер организованной преступной группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "босс",
                                "target_lang_sense_clarification": "мафии"
                            },
                            {
                                "target_lang_word": "главарь",
                                "target_lang_sense_clarification": "банды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfad3d85-41e7-3ee0-977e-dec788ba196f",
                    "sense_definition": "Przeciwnik w grze komputerowej, znacznie większy i silniejszy niż inni, występujący zazwyczaj na końcu poziomu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mogę pokonać ostatniego <w>bossa</w>, mimo że próbowałem już dziesięć razy.",
                            "target_lang_translations": {
                                "ru": "Я не могу победить последнего <w>босса</w>, хотя пытался уже десять раз."
                            }
                        },
                        {
                            "text": "Walka z <w>bossem</w> wymagała od gracza dużej zręczności i dobrej strategii.",
                            "target_lang_translations": {
                                "ru": "Битва с <w>боссом</w> требовала от игрока большой ловкости и хорошей стратегии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "walka z bossem",
                        "finałowy boss",
                        "pokonać bossa"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "terminologia gier komputerowych"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Противник в компьютерной игре, значительно сильнее других, обычно встречающийся в конце уровня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "босс",
                                "target_lang_sense_clarification": "компьютерные игры"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
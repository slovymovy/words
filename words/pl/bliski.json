{
    "word_family": [
        "bliskość",
        "blisko",
        "zbliżyć",
        "przybliżyć",
        "zbliżenie"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "64faa597-0b46-3a35-b7f9-90d95058e65d",
                    "sense_definition": "Znajdujący się w małej odległości przestrzennej; taki, do którego jest niedaleko.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "proximity_physical_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szukamy sklepu spożywczego w <w>bliskiej</w> okolicy naszego hotelu.",
                            "target_lang_translations": {
                                "ru": "Мы ищем продуктовый магазин в <w>близкой</w> окрестности нашего отеля."
                            }
                        },
                        {
                            "text": "Mój dom rodzinny jest bardzo <w>bliski</w> rzeki.",
                            "target_lang_translations": {
                                "ru": "Мой родной дом очень <w>близок</w> к реке."
                            }
                        }
                    ],
                    "synonyms": [
                        "niedaleki",
                        "pobliski"
                    ],
                    "antonyms": [
                        "daleki",
                        "odległy"
                    ],
                    "common_phrases": [
                        "bliski wschód",
                        "bliski zasięg",
                        "bliskie sąsiedztwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся на небольшом расстоянии; расположенный неподалеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "близкий",
                                "target_lang_sense_clarification": "о расстоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad8b5957-b3c4-3f35-84e6-8c0494fd3942",
                    "sense_definition": "Mający nastąpić w niedługim czasie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "proximity_physical_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam nadzieję, że spotkamy się w <w>bliskiej</w> przyszłości.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, мы встретимся в <w>близком</w> будущем."
                            }
                        },
                        {
                            "text": "Koniec roku szkolnego jest już bardzo <w>bliski</w>.",
                            "target_lang_translations": {
                                "ru": "Конец учебного года уже очень <w>близок</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nadchodzący",
                        "niedaleki"
                    ],
                    "antonyms": [
                        "odległy",
                        "daleki"
                    ],
                    "common_phrases": [
                        "w bliskiej przyszłości",
                        "bliski koniec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Который должен наступить в скором времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "близкий",
                                "target_lang_sense_clarification": "о времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4a66c81-1cd0-35ca-b4a0-962a686d232a",
                    "sense_definition": "Łączący się z kimś silną więzią uczuciową, wzajemnym zrozumieniem i zaufaniem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "human_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest mój <w>bliski</w> przyjaciel, którego znam od dziecka.",
                            "target_lang_translations": {
                                "ru": "Это мой <w>близкий</w> друг, которого я знаю с детства."
                            }
                        },
                        {
                            "text": "Mimo odległości, utrzymują ze sobą bardzo <w>bliskie</w> relacje.",
                            "target_lang_translations": {
                                "ru": "Несмотря на расстояние, они поддерживают очень <w>близкие</w> отношения."
                            }
                        }
                    ],
                    "synonyms": [
                        "pobliski",
                        "nadchodzący",
                        "nieuchronny"
                    ],
                    "antonyms": [
                        "daleki",
                        "odległy"
                    ],
                    "common_phrases": [
                        "bliski przyjaciel",
                        "bliska osoba",
                        "bliskie relacje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с кем-либо тесными отношениями, любовью или дружбой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "близкий",
                                "target_lang_sense_clarification": "о друге, отношениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5148abf3-58ee-34bb-a424-adca99c16b4f",
                    "sense_definition": "Będący w bezpośrednim lub bliskim stopniu pokrewieństwa.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "human_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wesele zaprosiliśmy tylko <w>bliską</w> rodzinę.",
                            "target_lang_translations": {
                                "ru": "На свадьбу мы пригласили только <w>близкую</w> родню."
                            }
                        },
                        {
                            "text": "Ciocia Ania jest moją <w>bliską</w> krewną ze strony matki.",
                            "target_lang_translations": {
                                "ru": "Тетя Аня — моя <w>близкая</w> родственница со стороны матери."
                            }
                        }
                    ],
                    "antonyms": [
                        "daleki"
                    ],
                    "common_phrases": [
                        "bliska rodzina",
                        "bliski krewny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий в прямом или тесном родстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "близкий",
                                "target_lang_sense_clarification": "о родственниках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10edab9c-feb8-3d74-9839-9b91b7da63c6",
                    "sense_definition": "Taki, któremu niewiele brakuje do osiągnięcia jakiegoś stanu lub zrobienia czegoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Byłem <w>bliski</w> rezygnacji z pracy, ale zmieniłem zdanie.",
                            "target_lang_translations": {
                                "ru": "Я был <w>близок</w> к увольнению с работы, но передумал."
                            }
                        },
                        {
                            "text": "Dziewczynka była <w>bliska</w> płaczu, gdy zgubiła swoją ulubioną zabawkę.",
                            "target_lang_translations": {
                                "ru": "Девочка была <w>близка</w> к слезам, когда потеряла свою любимую игрушку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bliski płaczu",
                        "bliski obłędu",
                        "bliski prawdy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся на грани какого-либо состояния; почти готовый что-то сделать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "близкий",
                                "target_lang_sense_clarification": "о состоянии (близок к чему-то)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9247f31-6bbf-3d57-b5aa-4b0c013989e1",
                    "sense_definition": "Mający cechy prawie takie same, jak ktoś lub coś innego.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten odcień niebieskiego jest bardzo <w>bliski</w> granatowemu.",
                            "target_lang_translations": {
                                "ru": "Этот оттенок синего очень <w>близок</w> к темно-синему."
                            }
                        },
                        {
                            "text": "Jego poglądy polityczne są <w>bliskie</w> moim przekonaniom.",
                            "target_lang_translations": {
                                "ru": "Его политические взгляды <w>близки</w> моим убеждениям."
                            }
                        }
                    ],
                    "synonyms": [
                        "podobny"
                    ],
                    "common_phrases": [
                        "bliski ideału",
                        "bliski sercu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большое сходство с чем-либо; похожий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "близкий",
                                "target_lang_sense_clarification": "похожий, сходный"
                            },
                            {
                                "target_lang_word": "сродни",
                                "target_lang_sense_clarification": "подобный (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "202dbff5-bcec-38cd-b5d7-1a2aa7c092b5",
                    "sense_definition": "Bardziej dokładny, szczegółowy i sprecyzowany (używane głównie w stopniu wyższym).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "information_detail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę o podanie <w>bliższych</w> informacji na temat tego projektu.",
                            "target_lang_translations": {
                                "ru": "Прошу предоставить более <w>подробную</w> информацию об этом проекте."
                            }
                        },
                        {
                            "text": "Policja bada <w>bliższe</w> okoliczności tego tajemniczego wypadku.",
                            "target_lang_translations": {
                                "ru": "Полиция расследует <w>подробные</w> обстоятельства этого загадочного происшествия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bliższe dane",
                        "bliższe informacje",
                        "bliższe szczegóły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Более подробный, детальный и точный (обычно в сравнительной степени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подробный",
                                "target_lang_sense_clarification": "детальный (о сведениях)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f11d213e-1118-31a1-85b4-a87edb1812a8",
                    "sense_definition": "Osoba z rodziny lub kręgu przyjaciół, z którą jest się silnie emocjonalnie związanym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "human_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Święta Bożego Narodzenia to czas spędzany w gronie <w>bliskich</w>.",
                            "target_lang_translations": {
                                "ru": "Рождество — это время, проводимое в кругу <w>близких</w>."
                            }
                        },
                        {
                            "text": "W trudnych chwilach zawsze mogę liczyć na pomoc moich <w>bliskich</w>.",
                            "target_lang_translations": {
                                "ru": "В трудные моменты я всегда могу рассчитывать на помощь моих <w>близких</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strata bliskiego",
                        "troska o bliskich",
                        "grono bliskich"
                    ],
                    "target_lang_definitions": {
                        "ru": "Родственник или друг, с которым существует тесная эмоциональная связь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "близкий",
                                "target_lang_sense_clarification": "родной человек"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
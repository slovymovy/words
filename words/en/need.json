{
    "word_family": [
        "needy",
        "needless",
        "needfully",
        "neediness"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "28ec3c09-dd17-3610-96e4-fc9fe445c7e7",
                    "sense_definition": "To require something because it is essential or very important.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_require",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Living things <w>need</w> water and food to survive.",
                            "target_lang_translations": {
                                "ru": "Живым организмам <w>нужна</w> вода и еда, чтобы выжить."
                            }
                        },
                        {
                            "text": "We <w>need</w> to leave now if we want to catch the bus.",
                            "target_lang_translations": {
                                "ru": "Нам <w>нужно</w> выходить сейчас, если мы хотим успеть на автобус."
                            }
                        }
                    ],
                    "synonyms": [
                        "require",
                        "be in need of"
                    ],
                    "common_phrases": [
                        "need to",
                        "need help",
                        "really need"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требовать чего-либо, так как это важно или необходимо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нуждаться",
                                "target_lang_sense_clarification": "испытывать потребность в чем-либо"
                            },
                            {
                                "target_lang_word": "быть нужным",
                                "target_lang_sense_clarification": "требоваться кому-либо (обычно используется в формах: нужно, нужен, нужна)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "071c5582-71e6-365c-a3c8-2c6bbedcc762",
                    "sense_definition": "To want something very much.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_want",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I really <w>need</w> a holiday after working so hard.",
                            "target_lang_translations": {
                                "ru": "Мне действительно <w>нужен</w> отпуск после такой тяжелой работы."
                            }
                        },
                        {
                            "text": "She <w>needed</w> a shower after the long hike.",
                            "target_lang_translations": {
                                "ru": "Ей <w>нужен был</w> душ после долгого похода."
                            }
                        }
                    ],
                    "synonyms": [
                        "want",
                        "desire",
                        "would like",
                        "wish for"
                    ],
                    "common_phrases": [
                        "need a break",
                        "need a drink"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень сильно хотеть чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть нужным",
                                "target_lang_sense_clarification": "быть необходимым для удовлетворения желания (мне нужно)"
                            },
                            {
                                "target_lang_word": "нуждаться",
                                "target_lang_sense_clarification": "чувствовать острую нехватку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cba69a1-e4e5-37bc-b58b-ab5e36ad3ed3",
                    "sense_definition": "To require a particular action or treatment to be done to it.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_require",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The house <w>needs</w> cleaning before the guests arrive.",
                            "target_lang_translations": {
                                "ru": "Дом <w>нуждается</w> в уборке до приезда гостей."
                            }
                        },
                        {
                            "text": "Something <w>needs</w> to be done about this problem immediately.",
                            "target_lang_translations": {
                                "ru": "С этой проблемой <w>нужно</w> что-то делать немедленно."
                            }
                        }
                    ],
                    "synonyms": [
                        "require"
                    ],
                    "common_phrases": [
                        "needs doing",
                        "needs to be"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требовать определенного действия или обращения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "требоваться",
                                "target_lang_sense_clarification": "быть обязательным для выполнения"
                            },
                            {
                                "target_lang_word": "нуждаться",
                                "target_lang_sense_clarification": "требовать ухода или действия (нуждаться в ремонте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e047c122-86df-3655-a25e-e40f0157a930",
                    "sense_definition": "To be obliged or required to do something (used as a modal verb, mostly in negative sentences or questions).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_modal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You <w>need</w> not worry about the details right now.",
                            "target_lang_translations": {
                                "ru": "Тебе <w>не нужно</w> беспокоиться о деталях прямо сейчас."
                            }
                        },
                        {
                            "text": "I <w>need</w> not tell you how important this meeting is.",
                            "target_lang_translations": {
                                "ru": "Мне <w>нет нужды</w> говорить тебе, насколько важна эта встреча."
                            }
                        }
                    ],
                    "common_phrases": [
                        "need not",
                        "need I"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "modal auxiliary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть обязанным или должным сделать что-либо (используется как модальный глагол)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть нужным",
                                "target_lang_sense_clarification": "в отрицании: нет необходимости (не нужно)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f2b2393-ebbc-365f-813d-615f1984295d",
                    "sense_definition": "A situation in which something is necessary or must be done.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_requirement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is no <w>need</w> to be afraid of the dark.",
                            "target_lang_translations": {
                                "ru": "Нет <w>необходимости</w> бояться темноты."
                            }
                        },
                        {
                            "text": "The school has a <w>need</w> for more experienced teachers.",
                            "target_lang_translations": {
                                "ru": "Школа испытывает <w>потребность</w> в более опытных учителях."
                            }
                        }
                    ],
                    "synonyms": [
                        "use",
                        "occasion"
                    ],
                    "common_phrases": [
                        "no need to",
                        "in need of",
                        "need for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, в которой что-то необходимо или должно быть сделано."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "необходимость",
                                "target_lang_sense_clarification": "обязательность, неизбежность"
                            },
                            {
                                "target_lang_word": "нужда",
                                "target_lang_sense_clarification": "надобность"
                            },
                            {
                                "target_lang_word": "потребность",
                                "target_lang_sense_clarification": "внутренняя необходимость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4758e6c9-dfa1-3622-b6a4-e76269adaf70",
                    "sense_definition": "The state of not having enough money, food, or other necessary things.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_poverty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The charity helps families living in great <w>need</w>.",
                            "target_lang_translations": {
                                "ru": "Благотворительная организация помогает семьям, живущим в большой <w>нужде</w>."
                            }
                        },
                        {
                            "text": "He promised to help his friend in his hour of <w>need</w>.",
                            "target_lang_translations": {
                                "ru": "Он обещал помочь другу в трудный час <w>нужды</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lack"
                    ],
                    "common_phrases": [
                        "in dire need",
                        "times of need"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние нехватки денег, еды или других необходимых вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нужда",
                                "target_lang_sense_clarification": "бедность, нищета"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "176a01a8-60dd-309e-a0b9-d9253f358497",
                    "sense_definition": "Słodycz o elastycznej konsystencji, którą się żuje, a nie połyka.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_candy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił ucznia, aby wyrzucił <gumę> do kosza.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил ученика выбросить <жвачку> в мусорное ведро."
                            }
                        },
                        {
                            "text": "Moja ulubiona <guma> do żucia ma smak miętowy.",
                            "target_lang_translations": {
                                "ru": "Моя любимая <жевательная резинка> — мятная."
                            }
                        }
                    ],
                    "common_phrases": [
                        "guma do żucia",
                        "guma balonowa",
                        "żuć gumę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкое эластичное изделие, которое жуют, а не глотают."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жевательная резинка",
                                "target_lang_sense_clarification": "полное название"
                            },
                            {
                                "target_lang_word": "жвачка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ab6c099-a734-3d50-9c48-bf3195e32f4d",
                    "sense_definition": "Elastyczne tworzywo otrzymywane z kauczuku lub produkowane sztucznie, używane do wyrobu opon, uszczelek i innych przedmiotów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podeszwa tych butów jest wykonana z trwałej <gumy>.",
                            "target_lang_translations": {
                                "ru": "Подошва этих ботинок сделана из прочной <резины>."
                            }
                        },
                        {
                            "text": "W powietrzu unosił się nieprzyjemny zapach palonej <gumy>.",
                            "target_lang_translations": {
                                "ru": "В воздухе стоял неприятный запах жжёной <резины>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "guma syntetyczna",
                        "palona guma"
                    ],
                    "target_lang_definitions": {
                        "ru": "Эластичный материал, получаемый из каучука или производимый искусственно, используемый для изготовления шин, уплотнителей и других предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резина",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9400f57d-c3c0-3a0a-9898-ff8e7592f570",
                    "sense_definition": "Elastyczna tasiemka używana w krawiectwie lub dziecięca zabawa polegająca na skakaniu przez taką tasiemkę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "game_clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczynki spędziły całe popołudnie na podwórku, grając w <gumę>.",
                            "target_lang_translations": {
                                "ru": "Девочки провели весь день во дворе, играя в <резиночку>."
                            }
                        },
                        {
                            "text": "Muszę wymienić <gumę> w pasie spodni, bo jest zbyt luźna.",
                            "target_lang_translations": {
                                "ru": "Мне нужно заменить <резинку> в поясе брюк, потому что она слишком свободная."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grać w gumę",
                        "skakać w gumę",
                        "guma w pasie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Эластичная тесьма, используемая в шитье, или детская игра, заключающаяся в прыжках через такую тесьму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резинка",
                                "target_lang_sense_clarification": "швейная фурнитура"
                            },
                            {
                                "target_lang_word": "резиночка",
                                "target_lang_sense_clarification": "детская игра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f823cbc5-38e2-3a45-976a-6b42e5a9ffe4",
                    "sense_definition": "Awaria koła w pojeździe polegająca na przebiciu opony i ucieczce powietrza.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vehicle_issue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spóźniłem się do pracy, ponieważ rano złapałem <gumę>.",
                            "target_lang_translations": {
                                "ru": "Я опоздал на работу, так как утром проколол <шину>."
                            }
                        },
                        {
                            "text": "Kierowca musiał zjechać na pobocze, żeby zmienić koło po złapaniu <gumy>.",
                            "target_lang_translations": {
                                "ru": "Водитель был вынужден съехать на обочину, чтобы поменять колесо после прокола <шины>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kapeć",
                        "flak"
                    ],
                    "common_phrases": [
                        "złapać gumę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повреждение колеса транспортного средства, заключающееся в проколе шины и утечке воздуха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шина",
                                "target_lang_sense_clarification": "в контексте прокола"
                            },
                            {
                                "target_lang_word": "прокол",
                                "target_lang_sense_clarification": "событие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b64cc6e-9c2c-3b2d-b2b0-88e620c6af5a",
                    "sense_definition": "Kosmetyk do stylizacji włosów o gęstej i ciągnącej konsystencji.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cosmetic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fryzjer użył odrobiny <gumy>, aby utrwalić moją fryzurę.",
                            "target_lang_translations": {
                                "ru": "Парикмахер использовал немного <пасты>, чтобы зафиксировать мою прическу."
                            }
                        },
                        {
                            "text": "Codziennie rano nakładam <gumę> na włosy, żeby się nie puszyły.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я наношу <пасту> на волосы, чтобы они не пушились."
                            }
                        }
                    ],
                    "common_phrases": [
                        "guma do włosów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Косметическое средство для укладки волос с густой и тягучей консистенцией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моделирующая паста",
                                "target_lang_sense_clarification": "косметика"
                            },
                            {
                                "target_lang_word": "жвачка для волос",
                                "target_lang_sense_clarification": "сленг стилистов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39150f75-4b12-3104-bba4-15750b12b648",
                    "sense_definition": "Potoczne określenie prezerwatywy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "contraception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Odpowiedzialny partner zawsze pamięta, żeby kupić <gumy>.",
                            "target_lang_translations": {
                                "ru": "Ответственный партнер всегда помнит, что нужно купить <резинки>."
                            }
                        },
                        {
                            "text": "W tym automacie w toalecie można kupić <gumy>.",
                            "target_lang_translations": {
                                "ru": "В этом автомате в туалете можно купить <презервативы>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zob. prezerwatywa"
                    ],
                    "common_phrases": [
                        "paczka gum"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "very informal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название презерватива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резинка",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "презерватив",
                                "target_lang_sense_clarification": "нейтральное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

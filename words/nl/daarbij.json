            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3e343bab-043e-3be7-af32-4f55a03425b2",
                    "sense_definition": "Bij datgene wat eerder genoemd is of aangewezen wordt; bij die zaak of dat voorwerp.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kocht een nieuw pak en het overhemd dat <w>daarbij</w> past.",
                            "target_lang_translations": {
                                "en": "He bought a new suit and the shirt that goes <w>with it</w>.",
                                "ru": "Он купил новый костюм и рубашку, которая <w>к нему</w> подходит."
                            }
                        },
                        {
                            "text": "Kijk naar de foto en lees de tekst die <w>daarbij</w> staat.",
                            "target_lang_translations": {
                                "en": "Look at the photo and read the text that goes <w>with it</w>.",
                                "ru": "Посмотри на фотографию и прочитай текст, который <w>к ней</w> относится."
                            }
                        }
                    ],
                    "synonyms": [
                        "bij dat",
                        "bij die"
                    ],
                    "target_lang_definitions": {
                        "en": "With that thing mentioned earlier or pointed at; accompanying that object.",
                        "ru": "При том, что было упомянуто ранее или на что указывают; при этом предмете или деле."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "with that",
                                "target_lang_sense_clarification": "referring to a specific object or matter"
                            },
                            {
                                "target_lang_word": "with it",
                                "target_lang_sense_clarification": "accompanying something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "к этому",
                                "target_lang_sense_clarification": "указывает на соответствие или принадлежность"
                            },
                            {
                                "target_lang_word": "при этом",
                                "target_lang_sense_clarification": "указывает на нахождение рядом или наличие"
                            },
                            {
                                "target_lang_word": "с этим",
                                "target_lang_sense_clarification": "указывает на сопровождение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bea1363-dfbc-3920-a65e-77eb23a2d5bb",
                    "sense_definition": "Gebruikt om een extra reden, argument of omstandigheid toe te voegen; bovendien.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is erg koud buiten en <w>daarbij</w> regent het hard.",
                            "target_lang_translations": {
                                "en": "It is very cold outside and <w>moreover</w> it is raining hard.",
                                "ru": "На улице очень холодно, и <w>к тому же</w> идет сильный дождь."
                            }
                        },
                        {
                            "text": "Ik heb geen tijd om te gaan en <w>daarbij</w> heb ik geen geld.",
                            "target_lang_translations": {
                                "en": "I don't have time to go and <w>besides</w> I don't have any money.",
                                "ru": "У меня нет времени идти, и <w>кроме того</w> у меня нет денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "bovendien",
                        "daarnaast",
                        "ook",
                        "verder"
                    ],
                    "common_phrases": [
                        "daarbij komt",
                        "en daarbij"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to add an extra reason, argument, or circumstance; moreover.",
                        "ru": "Используется для добавления дополнительной причины, аргумента или обстоятельства; кроме того."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "moreover",
                                "target_lang_sense_clarification": "formal addition of a reason"
                            },
                            {

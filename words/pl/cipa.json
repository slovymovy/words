{
    "word_family": [
        "cipka",
        "cipcia"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "533a3c10-8ed7-3359-b100-0f2473657e27",
                    "sense_definition": "Wulgarne określenie zewnętrznych żeńskich narządów płciowych.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z oburzeniem zwierzyła się przyjaciółce, że jej ginekolog używa słowa <w>cipa</w>.",
                            "target_lang_translations": {
                                "ru": "С возмущением она призналась подруге, что её гинеколог употребляет слово <w>пизда</w>."
                            }
                        },
                        {
                            "text": "Używanie słowa <w>cipa</w> w towarzystwie jest uważane za bardzo niekulturalne.",
                            "target_lang_translations": {
                                "ru": "Использование слова <w>пизда</w> в обществе считается очень бескультурным."
                            }
                        }
                    ],
                    "synonyms": [
                        "pizda",
                        "pipa",
                        "kuciapa",
                        "psita",
                        "piczka"
                    ],
                    "antonyms": [
                        "chuj"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "wulgarnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное обозначение женских наружных половых органов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пизда",
                                "target_lang_sense_clarification": "вульгарно"
                            },
                            {
                                "target_lang_word": "манда",
                                "target_lang_sense_clarification": "вульгарно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8422250-d0b8-3daa-8e7c-c48f62d78b46",
                    "sense_definition": "Pospolite, obraźliwe określenie osoby (często mężczyzny) niezaradnej, mało energicznej lub tchórzliwej.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ależ z niego <w>cipa</w> – niczego załatwić nie potrafi.",
                            "target_lang_translations": {
                                "ru": "Ну и <w>размазня</w> же он — ничего не может уладить."
                            }
                        },
                        {
                            "text": "Nie bądź taką <w>cipą</w> i powiedz w końcu szefowi o podwyżce.",
                            "target_lang_translations": {
                                "ru": "Не будь такой <w>тряпкой</w> и поговори наконец с начальником о повышении."
                            }
                        }
                    ],
                    "synonyms": [
                        "pipa"
                    ],
                    "common_phrases": [
                        "być cipą",
                        "straszna cipa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "obraźliwie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Просторечное, оскорбительное обозначение человека (часто мужчины), который беспомощен, неэнергичен или труслив."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тряпка",
                                "target_lang_sense_clarification": "о слабохарактерном человеке"
                            },
                            {
                                "target_lang_word": "размазня",
                                "target_lang_sense_clarification": "о нерешительном, вялом человеке"
                            },
                            {
                                "target_lang_word": "слабак",
                                "target_lang_sense_clarification": "о слабом человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c9b60f9-873f-3717-97b5-ca21868e93a7",
                    "sense_definition": "Wulgarne i pogardliwe określenie kobiety, zazwyczaj takiej, której się nie lubi lub uważa za głupią.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "„Ty głupia <w>cipo</w>!” – krzyknęła Iwona ze złością.",
                            "target_lang_translations": {
                                "ru": "«Ты глупая <w>пизда</w>!» — злобно крикнула Ивона."
                            }
                        },
                        {
                            "text": "Jakaś wredna <w>cipa</w> zajechała mi drogę na parkingu.",
                            "target_lang_translations": {
                                "ru": "Какая-то вредная <w>пизда</w> подрезала меня на парковке."
                            }
                        }
                    ],
                    "synonyms": [
                        "pizda"
                    ],
                    "common_phrases": [
                        "głupia cipa",
                        "wredna cipa"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "wulgarnie, obraźliwie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное и презрительное обозначение женщины, обычно такой, которая не нравится или считается глупой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пизда",
                                "target_lang_sense_clarification": "вульгарно, оскорбительно"
                            },
                            {
                                "target_lang_word": "сука",
                                "target_lang_sense_clarification": "бранное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96af928a-f43c-30e6-9c0f-47b04a949443",
                    "sense_definition": "Regionalne lub dawne określenie kury; zgrubienie od słowa cipka.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodyni wyszła na podwórko i wołała głośno: <w>cipa</w>, cipa, chodźcie tu!",
                            "target_lang_translations": {
                                "ru": "Хозяйка вышла во двор и громко позвала: <w>цыпа</w>, цыпа, идите сюда!"
                            }
                        },
                        {
                            "text": "Ta stara <w>cipa</w> przestała już nieść jajka i chodzi tylko po podwórku.",
                            "target_lang_translations": {
                                "ru": "Эта старая <w>курица</w> перестала нести яйца и только ходит по двору."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное или устаревшее название курицы; увеличительная форма от слова cipka."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курица",
                                "target_lang_sense_clarification": "домашняя птица"
                            },
                            {
                                "target_lang_word": "цыпа",
                                "target_lang_sense_clarification": "разговорное, при подзывании"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "56a6a7c4-a2f7-3ec2-82ed-7d0cee0b1248",
                    "sense_definition": "Nazwisko używane w Polsce.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Cipa</w> został poproszony o podejście do okienka w urzędzie.",
                            "target_lang_translations": {
                                "ru": "Господина <w>Ципу</w> попросили подойти к окошку в учреждении."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie spotkałem nikogo o nazwisku <w>Cipa</w>.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не встречал никого с фамилией <w>Ципа</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, встречающаяся в Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ципа",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
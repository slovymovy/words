                {
                    "sense_id": "00fd447f-b3c2-3e08-938b-69dda059799b",
                    "sense_definition": "Cecha kogoś lub czegoś, co jest inne, specyficzne i wyróżnia się na tle innych; posiadanie własnego, indywidualnego charakteru.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "quality_distinctness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy naród stara się pielęgnować swoją kulturową <w>odrębność</w> i tradycje.",
                            "target_lang_translations": {
                                "ru": "Каждый народ старается беречь свою культурную <w>самобытность</w> и традиции."
                            }
                        },
                        {
                            "text": "Jej styl ubierania podkreślał jej <w>odrębność</w> w grupie rówieśników.",
                            "target_lang_translations": {
                                "ru": "Её стиль одежды подчёркивал её <w>индивидуальность</w> в группе сверстников."
                            }
                        }
                    ],
                    "synonyms": [
                        "odmienność",
                        "inność",
                        "różność"
                    ],
                    "common_phrases": [

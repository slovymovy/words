{
    "word_family": [
        "viezigheid",
        "viezerik"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f381f372-3878-3287-94d6-b974de1a6cd9",
                    "sense_definition": "Niet schoon; bevuild met stof, modder of vlekken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet je <w>vieze</w> handen wassen voordat je aan tafel gaat.",
                            "target_lang_translations": {
                                "ru": "Ты должен помыть свои <w>грязные</w> руки перед тем, как садиться за стол."
                            }
                        },
                        {
                            "text": "Na het spelen in de tuin waren zijn kleren erg <w>vies</w> geworden.",
                            "target_lang_translations": {
                                "ru": "После игры в саду его одежда стала очень <w>грязной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vuil",
                        "smerig"
                    ],
                    "antonyms": [
                        "schoon",
                        "rein"
                    ],
                    "common_phrases": [
                        "vieze handen",
                        "vieze kleren",
                        "vies worden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечистый; испачканный пылью, грязью или пятнами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грязный",
                                "target_lang_sense_clarification": "физически нечистый"
                            },
                            {
                                "target_lang_word": "испачканный",
                                "target_lang_sense_clarification": "покрытый грязью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c5f45e4-91ce-302a-9446-d9ec16960ea7",
                    "sense_definition": "Met een onaangename smaak of geur; niet lekker.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sensory_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De melk is bedorven en smaakt heel <w>vies</w>.",
                            "target_lang_translations": {
                                "ru": "Молоко испортилось и на вкус очень <w>противное</w>."
                            }
                        },
                        {
                            "text": "Er hangt een <w>vieze</w> lucht in de keuken door de verbrande taart.",
                            "target_lang_translations": {
                                "ru": "На кухне стоит <w>неприятный</w> запах из-за подгоревшего пирога."
                            }
                        }
                    ],
                    "synonyms": [
                        "onsmakelijk",
                        "goor"
                    ],
                    "antonyms": [
                        "lekker",
                        "heerlijk"
                    ],
                    "common_phrases": [
                        "vies smaken",
                        "een vieze geur",
                        "bah wat vies"
                    ],
                    "target_lang_definitions": {
                        "ru": "С неприятным вкусом или запахом; невкусный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противный",
                                "target_lang_sense_clarification": "вызывающий отвращение"
                            },
                            {
                                "target_lang_word": "невкусный",
                                "target_lang_sense_clarification": "о еде или напитках"
                            },
                            {
                                "target_lang_word": "неприятный",
                                "target_lang_sense_clarification": "о запахе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f58431b7-a38c-3498-8297-1da4ce0e0a29",
                    "sense_definition": "Moreel verwerpelijk, onfatsoenlijk of seksueel getint.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "moral_judgment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij maakte de hele avond <w>vieze</w> grappen waar niemand om moest lachen.",
                            "target_lang_translations": {
                                "ru": "Весь вечер он отпускал <w>пошлые</w> шутки, над которыми никто не смеялся."
                            }
                        },
                        {
                            "text": "De politicus speelde een <w>vies</w> spelletje om de verkiezingen te winnen.",
                            "target_lang_translations": {
                                "ru": "Политик вел <w>грязную</w> игру, чтобы выиграть выборы."
                            }
                        }
                    ],
                    "synonyms": [
                        "smerig",
                        "goor",
                        "onsmakelijk"
                    ],
                    "antonyms": [
                        "fatsoenlijk",
                        "netjes"
                    ],
                    "common_phrases": [
                        "een vieze man",
                        "vieze praatjes",
                        "een vies spelletje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Морально предосудительный, неприличный или с сексуальным подтекстом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пошлый",
                                "target_lang_sense_clarification": "вульгарный, о шутках"
                            },
                            {
                                "target_lang_word": "грязный",
                                "target_lang_sense_clarification": "бесчестный, аморальный"
                            },
                            {
                                "target_lang_word": "неприличный",
                                "target_lang_sense_clarification": "недостойный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc971a63-53ea-3a76-aa10-1a4a387b9088",
                    "sense_definition": "Een afkeer hebbend van iets; iets niet willen aanraken of gebruiken.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broer is niet <w>vies</w> van hard werken om zijn doelen te bereiken.",
                            "target_lang_translations": {
                                "ru": "Мой брат не <w>брезгует</w> тяжелой работой, чтобы достичь своих целей."
                            }
                        },
                        {
                            "text": "Zij is erg <w>vies</w> van spinnen en andere insecten in huis.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>брезглива</w> к паукам и другим насекомым в доме."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niet vies zijn van",
                        "ergens vies van zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий отвращение к чему-либо; нежелание прикасаться или использовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брезгливый",
                                "target_lang_sense_clarification": "испытывающий отвращение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
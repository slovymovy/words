            "senses": [
                {
                    "sense_id": "f3f95973-f276-3f70-9916-43bb6dde5509",
                    "sense_definition": "Разновидность домашних животных, обладающих общими признаками, передающимися по наследству.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У моего друга есть собака очень редкой <w>породы</w>.",
                            "target_lang_translations": {
                                "en": "My friend has a dog of a very rare <w>breed</w>."
                            }
                        },
                        {
                            "text": "Эта <w>порода</w> кошек известна своим спокойным и дружелюбным характером.",
                            "target_lang_translations": {
                                "en": "This <w>breed</w> of cat is known for its calm and friendly character."
                            }
                        }
                    ],
                    "synonyms": [
                        "раса",
                        "раса"
                    ],
                    "common_phrases": [

{
    "word_family": [
        "uniformity",
        "uniformly",
        "nonuniform"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a5f11c00-9687-31e0-ab23-5edde31115f8",
                    "sense_definition": "A distinctive outfit worn by members of the same group, such as soldiers, police officers, students, or employees, to identify them.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "All the students must wear a blue <w>uniform</w> to school.",
                            "target_lang_translations": {
                                "ru": "Все ученики должны носить в школу синюю <w>форму</w>."
                            }
                        },
                        {
                            "text": "The soldier looked very proud in his military <w>uniform</w>.",
                            "target_lang_translations": {
                                "ru": "Солдат выглядел очень гордым в своей военной <w>форме</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "school uniform",
                        "military uniform",
                        "wear a uniform",
                        "in uniform"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличительная одежда, которую носят члены одной группы, например солдаты, полицейские, студенты или сотрудники, для их идентификации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фо́рма",
                                "target_lang_sense_clarification": "одежда установленного образца (школьная, военная)"
                            },
                            {
                                "target_lang_word": "унифо́рма",
                                "target_lang_sense_clarification": "форменная одежда, часто специальная"
                            },
                            {
                                "target_lang_word": "мунди́р",
                                "target_lang_sense_clarification": "военная или парадная официальная одежда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa3bc305-988d-3e6f-b4d8-aee0589fc6f7",
                    "sense_definition": "A police officer who wears a uniform, as distinguished from a detective or plainclothes officer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The detective arrived first, followed quickly by two <w>uniforms</w>.",
                            "target_lang_translations": {
                                "ru": "Детектив прибыл первым, за ним быстро последовали двое <w>полицейских в форме</w>."
                            }
                        },
                        {
                            "text": "We need more <w>uniforms</w> on the street to reassure the public.",
                            "target_lang_translations": {
                                "ru": "Нам нужно больше <w>полицейских в форме</w> на улицах, чтобы успокоить общественность."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in police jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полицейский в форме, в отличие от детектива или сотрудника в штатском."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полицейский в форме",
                                "target_lang_sense_clarification": "разговорное обозначение сотрудника полиции при исполнении"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7e00c2e6-c03c-3ae4-b296-b3f0b9ae8bb6",
                    "sense_definition": "Remaining the same in all cases and at all times; unvarying and consistent.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consistency_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cookies should be of <w>uniform</w> size so they bake evenly.",
                            "target_lang_translations": {
                                "ru": "Печенье должно быть <w>одинакового</w> размера, чтобы оно пропеклось равномерно."
                            }
                        },
                        {
                            "text": "The company tries to maintain a <w>uniform</w> standard of service across all branches.",
                            "target_lang_translations": {
                                "ru": "Компания старается поддерживать <w>единый</w> стандарт обслуживания во всех филиалах."
                            }
                        }
                    ],
                    "synonyms": [
                        "regular",
                        "steady",
                        "invariable",
                        "homogeneous",
                        "stable"
                    ],
                    "antonyms": [
                        "nonuniform",
                        "diverse"
                    ],
                    "common_phrases": [
                        "uniform size",
                        "uniform color",
                        "uniform distribution",
                        "uniform manner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остающийся неизменным во всех случаях и в любое время; постоянный и последовательный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "единообра́зный",
                                "target_lang_sense_clarification": "приведённый к единству, постоянный"
                            },
                            {
                                "target_lang_word": "равноме́рный",
                                "target_lang_sense_clarification": "одинаковый во всех частях, распределённый без перепадов"
                            },
                            {
                                "target_lang_word": "одинаковый",
                                "target_lang_sense_clarification": "ничем не отличающийся от других"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0d8e8585-3288-321d-81ad-110ab3bafba7",
                    "sense_definition": "To dress or clothe someone in a uniform.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new recruits were <w>uniformed</w> and sent to the training camp.",
                            "target_lang_translations": {
                                "ru": "Новых новобранцев <w>обмундировали</w> и отправили в тренировочный лагерь."
                            }
                        },
                        {
                            "text": "The hotel staff were <w>uniformed</w> in smart black suits.",
                            "target_lang_translations": {
                                "ru": "Персонал отеля был <w>одет</w> в элегантные черные костюмы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одевать или облачать кого-либо в униформу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмундировывать",
                                "target_lang_sense_clarification": "снабжать форменной одеждой"
                            },
                            {
                                "target_lang_word": "одевать в форму",
                                "target_lang_sense_clarification": "облачать в униформу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
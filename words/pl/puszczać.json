            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bb3789ec-5e8e-3c0c-9075-ba01a5afd18a",
                    "sense_definition": "przestawać kogoś lub coś trzymać; pozwalać, aby coś wypadło z ręki",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "release_hold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy <w>puszczam</w> smycz, mój pies zaczyna biec.",
                            "target_lang_translations": {
                                "ru": "Когда я <w>отпускаю</w> поводок, моя собака начинает бежать."
                            }
                        },
                        {
                            "text": "Nigdy nie <w>puszczaj</w> kierownicy podczas jazdy samochodem.",
                            "target_lang_translations": {
                                "ru": "Никогда не <w>отпускай</w> руль во время вождения автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "uwalniać",
                        "wypuszczać"
                    ],
                    "antonyms": [
                        "chwytać się",
                        "łapać się",
                        "chwytać się",
                        "łapać się"
                    ],
                    "common_phrases": [
                        "puszczać latawiec",
                        "puszczać wodze fantazji"
                    ],
                    "target_lang_definitions": {
                        "ru": "переставать держать кого-то или что-то; позволять чему-то выпасть из рук"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отпускать",
                                "target_lang_sense_clarification": "прекращать удерживать"
                            },
                            {
                                "target_lang_word": "выпускать",
                                "target_lang_sense_clarification": "давать выпасть или выйти из рук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b9937a0-ac3d-340d-a717-702a72e03ca0",
                    "sense_definition": "pozwalać komuś lub czemuś wejść, wyjść lub przejść",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "allow_passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ochroniarz nie <w>puszczał</w> nikogo do klubu bez biletu.",
                            "target_lang_translations": {
                                "ru": "Охранник никого не <w>пускал</w> в клуб без билета."
                            }
                        },
                        {
                            "text": "Zawsze <w>puszczam</w> starsze osoby przodem w drzwiach.",
                            "target_lang_translations": {
                                "ru": "Я всегда <w>пропускаю</w> пожилых людей вперед в дверях."
                            }
                        }
                    ],
                    "synonyms": [
                        "przepuszczać",
                        "wypuszczać"
                    ],
                    "common_phrases": [
                        "puszczać kogoś przodem",
                        "puszczać wolno"
                    ],
                    "target_lang_definitions": {

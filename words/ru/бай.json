        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "262dff26-03df-3f44-99d4-5f8219ad5c38",
                    "sense_definition": "Форма повелительного наклонения глагола <w>баять</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не <бай> мне сказки, а говори правду.",
                            "target_lang_translations": {
                                "en": "Don't <tell> me fairy tales, speak the truth."
                            }
                        },
                        {
                            "text": "Ты <бай>, да не заговаривайся.",
                            "target_lang_translations": {
                                "en": "You may <speak>, but don't get carried away."
                            }
                        }
                    ],

{
    "word_family": [
        "puin",
        "puinruimen",
        "puinruimer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f34e4d97-768a-3a80-b4ee-a398369745fb",
                    "sense_definition": "Een toestand van grote wanorde, chaos of een situatie die volledig misgelopen is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na deze periode van wanbeleid was de economie een grote <w>puinhoop</w>.",
                            "target_lang_translations": {
                                "en": "After this period of mismanagement, the economy was a big <w>mess</w>.",
                                "ru": "После этого периода бесхозяйственности экономика находилась в полной <w>разрухе</w>."
                            }
                        },
                        {
                            "text": "Hij heeft er een enorme <w>puinhoop</w> van gemaakt door zijn afspraken niet na te komen.",
                            "target_lang_translations": {
                                "en": "He made a huge <w>mess</w> of it by not fulfilling his agreements.",
                                "ru": "Он устроил настоящий <w>хаос</w>, не выполнив свои обещания."
                            }
                        }
                    ],
                    "synonyms": [
                        "rommel",
                        "rotzooi",
                        "troep",
                        "zooi"
                    ],
                    "common_phrases": [
                        "er een puinhoop van maken",
                        "een grote puinhoop"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of great disorder, chaos, or a situation that has gone completely wrong.",
                        "ru": "Состояние сильного беспорядка, хаоса или ситуация, которая полностью вышла из-под контроля."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mess",
                                "target_lang_sense_clarification": "a situation that is confused and full of problems"
                            },
                            {
                                "target_lang_word": "shambles",
                                "target_lang_sense_clarification": "a state of total disorder"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хаос",
                                "target_lang_sense_clarification": "полная неразбериха"
                            },
                            {
                                "target_lang_word": "беспорядок",
                                "target_lang_sense_clarification": "отсутствие порядка"
                            },
                            {
                                "target_lang_word": "разруха",
                                "target_lang_sense_clarification": "состояние упадка (часто об экономике или управлении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b62440ec-a85e-3a46-af57-ff59b40df605",
                    "sense_definition": "Een stapel brokstukken, meestal ontstaan door de verwoesting, instorting of sloop van gebouwen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rubble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bombardement liet van deze stad niet meer dan <w>puinhopen</w> over.",
                            "target_lang_translations": {
                                "en": "The bombardment left nothing of this city but <w>ruins</w>.",
                                "ru": "После бомбардировки от города остались одни <w>руины</w>."
                            }
                        },
                        {
                            "text": "De reddingswerkers zochten urenlang in de <w>puinhoop</w> naar overlevenden.",
                            "target_lang_translations": {
                                "en": "The rescue workers searched the <w>rubble</w> for survivors for hours.",
                                "ru": "Спасатели часами искали выживших под <w>завалами</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een rokende puinhoop",
                        "in puin liggen"
                    ],
                    "target_lang_definitions": {
                        "en": "A pile of debris, usually resulting from the destruction, collapse, or demolition of buildings.",
                        "ru": "Груда обломков, обычно образовавшаяся в результате разрушения, обрушения или сноса зданий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rubble",
                                "target_lang_sense_clarification": "broken stones or bricks from a building"
                            },
                            {
                                "target_lang_word": "ruins",
                                "target_lang_sense_clarification": "the remains of a destroyed building or city"
                            },
                            {
                                "target_lang_word": "heap of rubble",
                                "target_lang_sense_clarification": "a pile of debris"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "руины",
                                "target_lang_sense_clarification": "остатки разрушенного здания или города"
                            },
                            {
                                "target_lang_word": "развалины",
                                "target_lang_sense_clarification": "разрушенные строения"
                            },
                            {
                                "target_lang_word": "завал",
                                "target_lang_sense_clarification": "нагромождение обломков"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "saying",
        "hearsay",
        "naysay",
        "gainsay",
        "soothsayer",
        "unsayable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e8f8254e-800c-3f26-b489-d8c4a7ee9731",
                    "sense_definition": "To express a thought, opinion, or fact in words, either spoken or written.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>said</w> he would be here tomorrow.",
                            "target_lang_translations": {
                                "ru": "Он <w>сказал</w>, что будет здесь завтра."
                            }
                        },
                        {
                            "text": "Please <w>say</w> yes to the invitation.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>скажи</w> «да» на это приглашение."
                            }
                        }
                    ],
                    "synonyms": [
                        "speak",
                        "tell",
                        "state",
                        "utter",
                        "voice",
                        "verbalize"
                    ],
                    "antonyms": [
                        "write"
                    ],
                    "common_phrases": [
                        "say yes",
                        "say no",
                        "say goodbye",
                        "say sorry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать мысли, мнение или факты словами, устно или письменно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говорить",
                                "target_lang_sense_clarification": "выражать мысли вслух"
                            },
                            {
                                "target_lang_word": "сказать",
                                "target_lang_sense_clarification": "сообщить что-либо (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "307581a6-46e1-34e4-8a9c-39a4f9cbbae0",
                    "sense_definition": "To indicate or provide information in a written form.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sign <w>says</w> it is fifty kilometres to Paris.",
                            "target_lang_translations": {
                                "ru": "Знак <w>гласит</w>, что до Парижа пятьдесят километров."
                            }
                        },
                        {
                            "text": "What does the letter <w>say</w>?",
                            "target_lang_translations": {
                                "ru": "О чем <w>говорится</w> в письме?"
                            }
                        }
                    ],
                    "synonyms": [
                        "state"
                    ],
                    "common_phrases": [
                        "the sign says",
                        "the clock says"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщать или предоставлять информацию в письменном виде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гласить",
                                "target_lang_sense_clarification": "о надписях, законах"
                            },
                            {
                                "target_lang_word": "говориться",
                                "target_lang_sense_clarification": "содержаться в тексте"
                            },
                            {
                                "target_lang_word": "сообщать",
                                "target_lang_sense_clarification": "передавать информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d02ed40-3a9f-3435-ab6d-96e7c3b0b69f",
                    "sense_definition": "To state as a common opinion, rumor, or well-known fact; usually used in the passive voice or with 'they'.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>say</w> that breakfast is the most important meal.",
                            "target_lang_translations": {
                                "ru": "<w>Говорят</w>, что завтрак — самый важный прием пищи."
                            }
                        },
                        {
                            "text": "It is <w>said</w> that the castle is haunted.",
                            "target_lang_translations": {
                                "ru": "<w>Говорят</w>, что в замке обитают привидения."
                            }
                        }
                    ],
                    "synonyms": [
                        "report"
                    ],
                    "common_phrases": [
                        "they say",
                        "it is said"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утверждать как общее мнение, слух или общеизвестный факт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говорить",
                                "target_lang_sense_clarification": "обычно во мн. ч.: говорят"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f27be395-0aab-343e-a49b-43ea91209780",
                    "sense_definition": "To pronounce a word or sound.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>say</w> your name slowly and clearly.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>произнесите</w> свое имя медленно и четко."
                            }
                        },
                        {
                            "text": "The child is learning to <w>say</w> new words.",
                            "target_lang_translations": {
                                "ru": "Ребенок учится <w>говорить</w> новые слова."
                            }
                        }
                    ],
                    "synonyms": [
                        "utter",
                        "verbalize"
                    ],
                    "common_phrases": [
                        "say a word",
                        "say it loud"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произносить слово или звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "произносить",
                                "target_lang_sense_clarification": "выговаривать звуки"
                            },
                            {
                                "target_lang_word": "сказать",
                                "target_lang_sense_clarification": "вымолвить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "640e5b8e-84d0-3e88-9486-0867b935282e",
                    "sense_definition": "To recite something from memory, such as a prayer or a poem.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children stood up to <w>say</w> the pledge.",
                            "target_lang_translations": {
                                "ru": "Дети встали, чтобы <w>произнести</w> клятву."
                            }
                        },
                        {
                            "text": "We always <w>say</w> grace before we eat dinner.",
                            "target_lang_translations": {
                                "ru": "Мы всегда <w>читаем</w> молитву перед ужином."
                            }
                        }
                    ],
                    "synonyms": [
                        "speak",
                        "utter"
                    ],
                    "common_phrases": [
                        "say grace",
                        "say a prayer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Читать наизусть, например, молитву или стихотворение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "читать",
                                "target_lang_sense_clarification": "молитву, стихи"
                            },
                            {
                                "target_lang_word": "произносить",
                                "target_lang_sense_clarification": "торжественно говорить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51f6cecc-682e-3f5f-a0c6-297e3bffe444",
                    "sense_definition": "To suppose or assume something as a hypothesis or example.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "assumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Say</w> you win the lottery, what would you do?",
                            "target_lang_translations": {
                                "ru": "<w>Скажем</w>, ты выиграешь в лотерею, что ты будешь делать?"
                            }
                        },
                        {
                            "text": "Let's <w>say</w> we meet at the station at five.",
                            "target_lang_translations": {
                                "ru": "<w>Допустим</w>, мы встретимся на вокзале в пять."
                            }
                        }
                    ],
                    "common_phrases": [
                        "let's say"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предполагать или допускать что-либо в качестве гипотезы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "допустим",
                                "target_lang_sense_clarification": "предположим"
                            },
                            {
                                "target_lang_word": "скажем",
                                "target_lang_sense_clarification": "вводное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fbc5040-b97c-3cca-9874-db7da6db6b77",
                    "sense_definition": "To express strong belief in an outcome, often used in the context of betting.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My money <w>says</w> that our team will win the game.",
                            "target_lang_translations": {
                                "ru": "Готов <w>поспорить</w>, что наша команда выиграет."
                            }
                        },
                        {
                            "text": "Experience <w>says</w> that this plan will not work.",
                            "target_lang_translations": {
                                "ru": "Опыт <w>говорит</w>, что этот план не сработает."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "idiomatic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать твердую уверенность в исходе, часто в контексте пари."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говорить",
                                "target_lang_sense_clarification": "свидетельствовать"
                            },
                            {
                                "target_lang_word": "утверждать",
                                "target_lang_sense_clarification": "быть уверенным"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b834c2f9-d32d-3f8e-84e1-5a47744b289e",
                    "sense_definition": "The right or power to influence a decision or express an opinion.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I want to have a <w>say</w> in choosing the holiday destination.",
                            "target_lang_translations": {
                                "ru": "Я хочу иметь <w>право голоса</w> при выборе места отдыха."
                            }
                        },
                        {
                            "text": "The manager has the final <w>say</w> on hiring new staff.",
                            "target_lang_translations": {
                                "ru": "За менеджером последнее <w>слово</w> в найме новых сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "voice"
                    ],
                    "common_phrases": [
                        "have a say",
                        "final say"
                    ],
                    "target_lang_definitions": {
                        "ru": "Право или возможность влиять на решение или выражать мнение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "право голоса",
                                "target_lang_sense_clarification": "возможность влиять"
                            },
                            {
                                "target_lang_word": "слово",
                                "target_lang_sense_clarification": "в выражении «последнее слово»"
                            },
                            {
                                "target_lang_word": "мнение",
                                "target_lang_sense_clarification": "суждение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a0514233-5977-3653-9ef3-b775eaf328cd",
                    "sense_definition": "Used to introduce an example, a possibility, or an approximate amount.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "assumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pick a color you like, <w>say</w>, blue or green.",
                            "target_lang_translations": {
                                "ru": "Выберите цвет, который вам нравится, <w>скажем</w>, синий или зеленый."
                            }
                        },
                        {
                            "text": "He was driving fast, <w>say</w>, sixty miles per hour.",
                            "target_lang_translations": {
                                "ru": "Он ехал быстро, <w>скажем</w>, шестьдесят миль в час."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения примера, возможности или приблизительного количества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скажем",
                                "target_lang_sense_clarification": "например"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "db271d7d-9ce2-3ce3-8118-612ff6393b3d",
                    "sense_definition": "Used to gain someone's attention before making an inquiry or suggestion, or to express surprise.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Say</w>, what did you think about that movie?",
                            "target_lang_translations": {
                                "ru": "<w>Послушай</w>, что ты думаешь об этом фильме?"
                            }
                        },
                        {
                            "text": "<w>Say</w>, that is a beautiful painting on the wall.",
                            "target_lang_translations": {
                                "ru": "<w>Смотри</w>, какая красивая картина на стене."
                            }
                        }
                    ],
                    "synonyms": [
                        "hey"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often dated"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы привлечь чье-то внимание или выразить удивление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "послушай",
                                "target_lang_sense_clarification": "обращение"
                            },
                            {
                                "target_lang_word": "скажи",
                                "target_lang_sense_clarification": "обращение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "85e95f79-ff06-3252-b382-e5aadf439182",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Say</w> is a well-known author in this region.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Сэй</w> — известный автор в этом регионе."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <w>Say</w> yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера я встретил женщину по имени Сара <w>Сэй</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сэй",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
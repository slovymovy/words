{
    "word_family": [
        "свобода",
        "свободный",
        "освобождение",
        "освободитель",
        "освобождаться"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "68cd0236-ff32-3757-95c9-8ab2e6f8e265",
                    "sense_definition": "Сделать пространство или ёмкость пустыми, убрав оттуда людей или предметы.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, помогите мне <освободить> стол от лишних бумаг перед обедом.",
                            "target_lang_translations": {
                                "en": "Please help me <clear> the table of unnecessary papers before lunch."
                            }
                        },
                        {
                            "text": "Пассажиры должны <освободить> вагоны поезда на конечной станции.",
                            "target_lang_translations": {
                                "en": "Passengers must <vacate> the train cars at the terminal station."
                            }
                        }
                    ],
                    "synonyms": [
                        "очистить",
                        "опустошить"
                    ],
                    "common_phrases": [
                        "освободить место",
                        "освободить помещение",
                        "освободить дорогу",
                        "освободить проход"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a space or container empty by removing people or objects."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "To remove items from a surface or space"
                            },
                            {
                                "target_lang_word": "empty",
                                "target_lang_sense_clarification": "To remove contents from a container"
                            },
                            {
                                "target_lang_word": "vacate",
                                "target_lang_sense_clarification": "To leave a place, making it empty (usually of people)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca509258-883f-39b8-a6d8-0f5d2f476ed3",
                    "sense_definition": "Сделать свободным, выпустить на волю из заключения, плена или рабства.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Армия смогла <освободить> город от захватчиков всего за три дня.",
                            "target_lang_translations": {
                                "en": "The army was able to <liberate> the city from invaders in just three days."
                            }
                        },
                        {
                            "text": "Мы решили <освободить> птицу из клетки и отпустить её в лес.",
                            "target_lang_translations": {
                                "en": "We decided to <free> the bird from the cage and release it into the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "спасти"
                    ],
                    "antonyms": [
                        "поработить",
                        "пленить",
                        "закабалить",
                        "сковать",
                        "связать"
                    ],
                    "common_phrases": [
                        "освободить заложников",
                        "освободить пленных",
                        "освободить рабов",
                        "освободить город"
                    ],
                    "target_lang_definitions": {
                        "en": "To make free, let out to liberty from imprisonment, captivity, or slavery."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "liberate",
                                "target_lang_sense_clarification": "To set free from oppression, occupation, or slavery"
                            },
                            {
                                "target_lang_word": "free",
                                "target_lang_sense_clarification": "To release from confinement or captivity"
                            },
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "To let go or set free"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "576b7588-bb4e-3cea-9add-4ead81f6470c",
                    "sense_definition": "Избавить от выполнения каких-либо обязанностей, забот или чего-то нежелательного.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач решил <освободить> ученика от уроков физкультуры из-за болезни.",
                            "target_lang_translations": {
                                "en": "The doctor decided to <excuse> the student from PE classes due to illness."
                            }
                        },
                        {
                            "text": "Суд может <освободить> человека от штрафа, если он докажет свою невиновность.",
                            "target_lang_translations": {
                                "en": "The court can <exempt> a person from a fine if they prove their innocence."
                            }
                        }
                    ],
                    "synonyms": [
                        "избавить"
                    ],
                    "common_phrases": [
                        "освободить от ответственности",
                        "освободить от налогов",
                        "освободить от уплаты",
                        "освободить от работы"
                    ],
                    "target_lang_definitions": {
                        "en": "To relieve from the performance of duties, cares, or something undesirable."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exempt",
                                "target_lang_sense_clarification": "To free from an obligation, rule, or payment"
                            },
                            {
                                "target_lang_word": "excuse",
                                "target_lang_sense_clarification": "To grant permission not to do something"
                            },
                            {
                                "target_lang_word": "relieve",
                                "target_lang_sense_clarification": "To free from a burden or responsibility"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8258783f-4501-35c4-b38d-0cdb18e300a4",
                    "sense_definition": "Лишить занимаемой должности или уволить с работы.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Совет директоров проголосовал за то, чтобы <освободить> генерального директора от должности.",
                            "target_lang_translations": {
                                "en": "The board of directors voted to <dismiss> the CEO from his position."
                            }
                        },
                        {
                            "text": "Министра могут <освободить> от поста в связи с переходом на другую работу.",
                            "target_lang_translations": {
                                "en": "The minister may be <relieved> of his post in connection with a transfer to another job."
                            }
                        }
                    ],
                    "common_phrases": [
                        "освободить от должности",
                        "освободить от поста"
                    ],
                    "target_lang_definitions": {
                        "en": "To deprive of a held position or fire from a job."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dismiss",
                                "target_lang_sense_clarification": "To officially end someone's employment"
                            },
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "To take someone out of a position or office"
                            },
                            {
                                "target_lang_word": "relieve",
                                "target_lang_sense_clarification": "To take a position or duty away from someone (formal)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            "target_lang_translations": {
                                "en": "I just need to <w>create</w> a new account for this website."
                            }
                        },
                        {
                            "text": "De systeembeheerder heeft gisteren een profiel voor de nieuwe werknemer <w>aangemaakt</w>.",
                            "target_lang_translations": {
                                "en": "The system administrator <w>created</w> a profile for the new employee yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "creëren",
                        "openen",
                        "maken"
                    ],
                    "antonyms": [
                        "verwijderen",
                        "wissen"
                    ],
                    "common_phrases": [

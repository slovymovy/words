    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9008264d-d49e-3c4e-8ae0-3a352091f72e",
                    "sense_definition": "Дать возможность увидеть что-либо, продемонстрировать зрительно.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_demonstration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <показать> тебе свою новую машину.",
                            "target_lang_translations": {
                                "en": "I want to <show> you my new car."
                            }
                        },
                        {
                            "text": "Гид обещал <показать> город за один день.",
                            "target_lang_translations": {
                                "en": "The guide promised to <show> us the city in one day."
                            }
                        }
                    ],
                    "synonyms": [
                        "продемонстрировать"
                    ],
                    "antonyms": [
                        "скрыть",
                        "спрятать"
                    ],
                    "common_phrases": [
                        "показать город",
                        "показать дорогу",
                        "показать на карте",
                        "показать фильм"
                    ],
                    "target_lang_definitions": {
                        "en": "To allow someone to see something; to display visually."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "general visual demonstration"
                            },
                            {
                                "target_lang_word": "demonstrate",
                                "target_lang_sense_clarification": "to show how something works or looks"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "458f2cbe-105b-3947-a673-c27b3eabc26a",
                    "sense_definition": "Предъявить документ или предмет для проверки и официального осмотра.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_demonstration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При входе в музей нужно <показать> билет.",
                            "target_lang_translations": {
                                "en": "At the entrance to the museum, you need to <show> your ticket."
                            }
                        },
                        {
                            "text": "Полицейский попросил водителя <показать> документы.",
                            "target_lang_translations": {
                                "en": "The police officer asked the driver to <show> his documents."
                            }
                        }
                    ],
                    "synonyms": [
                        "предъявить"
                    ],
                    "antonyms": [
                        "скрыть"
                    ],
                    "common_phrases": [
                        "показать паспорт",
                        "показать билет",
                        "показать пропуск",
                        "показать документы"
                    ],
                    "target_lang_definitions": {
                        "en": "To present a document or object for official inspection or verification."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "to present for checking"
                            },
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "formal presentation of documents"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c313f08-876e-3947-86c1-227b915fbe83",
                    "sense_definition": "Жестом (обычно рукой или пальцем) обратить чьё-либо внимание на кого-либо или что-либо.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_demonstration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мальчик решил <показать> пальцем на витрину магазина.",
                            "target_lang_translations": {
                                "en": "The boy decided to <point> his finger at the shop window."
                            }
                        },
                        {
                            "text": "Прохожий любезно согласился <показать> направление к вокзалу.",
                            "target_lang_translations": {
                                "en": "The passerby kindly agreed to <show> the direction to the station."
                            }
                        }
                    ],
                    "synonyms": [
                        "указать"
                    ],
                    "common_phrases": [
                        "показать пальцем",
                        "показать рукой",
                        "показать направление"
                    ],
                    "target_lang_definitions": {
                        "en": "To direct attention to someone or something using a gesture, usually with a finger or hand."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "point",
                                "target_lang_sense_clarification": "to indicate direction or object with a finger"
                            },
                            {
                                "target_lang_word": "indicate",
                                "target_lang_sense_clarification": "to show direction"
                            },
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "to guide or direct attention"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "340b74d2-f9dd-3de8-b7d1-a321b1491996",
                    "sense_definition": "Обнаружить, сделать явным какое-либо свойство, качество или достижение.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_demonstration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спортсмен смог <показать> отличный результат на соревнованиях.",
                            "target_lang_translations": {
                                "en": "The athlete was able to <show> an excellent result at the competition."
                            }
                        },
                        {
                            "text": "В трудной ситуации он сумел <показать> себя настоящим другом.",
                            "target_lang_translations": {
                                "en": "In a difficult situation, he managed to <show> himself as a true friend."
                            }
                        }
                    ],
                    "synonyms": [
                        "проявить",
                        "обнаружить",
                        "продемонстрировать"
                    ],
                    "antonyms": [
                        "скрыть",
                        "утаить"
                    ],
                    "common_phrases": [
                        "показать характер",
                        "показать результат",
                        "показать себя",
                        "показать пример"
                    ],
                    "target_lang_definitions": {
                        "en": "To reveal, demonstrate, or make evident a quality, skill, or achievement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "to reveal a quality or result"
                            },
                            {
                                "target_lang_word": "demonstrate",
                                "target_lang_sense_clarification": "to prove or make evident"
                            },
                            {
                                "target_lang_word": "display",
                                "target_lang_sense_clarification": "to exhibit a quality or emotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3063dacf-5244-34cb-bbb7-366fea498f9e",
                    "sense_definition": "Обозначить какую-либо величину или время (об измерительных приборах).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вчера уличный термометр <показал> минус десять градусов.",
                            "target_lang_translations": {
                                "en": "Yesterday the street thermometer <showed> minus ten degrees."
                            }
                        },
                        {
                            "text": "Старинные часы на башне <показали> ровно полдень.",
                            "target_lang_translations": {
                                "en": "The old clock on the tower <showed> exactly noon."
                            }
                        }
                    ],
                    "common_phrases": [
                        "часы показали",
                        "термометр показал",
                        "приборы показали"
                    ],
                    "target_lang_definitions": {
                        "en": "To indicate a specific value, measurement, or time (regarding instruments)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "to display a reading"
                            },
                            {
                                "target_lang_word": "read",
                                "target_lang_sense_clarification": "to indicate a specific measurement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae64512e-d749-32a4-a1df-595e27f8b44f",
                    "sense_definition": "Выразить угрозу наказать или проучить кого-либо (обычно в эмоциональной речи).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "idiomatic_threat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если ты снова опоздаешь, я тебе <покажу>!",
                            "target_lang_translations": {
                                "en": "If you are late again, I'll <show> you!"
                            }
                        },
                        {
                            "text": "Рассерженный сосед кричал, что <покажет> хулиганам, как шуметь ночью.",
                            "target_lang_translations": {
                                "en": "The angry neighbor shouted that he would <show> the hooligans what it means to make noise at night."
                            }
                        }
                    ],
                    "synonyms": [
                        "припугнуть",
                        "проучить"
                    ],
                    "common_phrases": [
                        "я тебе покажу",
                        "я вам покажу",
                        "я ей покажу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in spoken language to express a threat"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To threaten to punish or teach someone a lesson (used in idiomatic expressions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "in threats like 'I'll show you'"
                            },
                            {
                                "target_lang_word": "teach a lesson",
                                "target_lang_sense_clarification": "to punish or reprimand"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b080f8a-3567-385f-ba7f-798ad2ef9b65",
                    "sense_definition": "Дать официальные свидетельские показания на допросе или в суде.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Свидетель согласился <показать> правду о событиях той ночи.",
                            "target_lang_translations": {
                                "en": "The witness agreed to <testify> to the truth about the events of that night."
                            }
                        },
                        {
                            "text": "Обвиняемый решил <показать>, что действовал в целях самообороны.",
                            "target_lang_translations": {
                                "en": "The accused decided to <testify> that he acted in self-defense."
                            }
                        }
                    ],
                    "synonyms": [
                        "дать показания",
                        "заявить"
                    ],
                    "antonyms": [
                        "утаить"
                    ],
                    "common_phrases": [
                        "показать против",
                        "показать на суде"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "legal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To give official testimony or evidence in a legal setting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "testify",
                                "target_lang_sense_clarification": "to give evidence as a witness"
                            },
                            {
                                "target_lang_word": "give evidence",
                                "target_lang_sense_clarification": "legal context"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
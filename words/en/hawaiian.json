            "senses": [
                {
                    "sense_id": "467158ef-3131-326d-8b4e-d8eb7886c7c7",
                    "sense_definition": "Relating to the US state of Hawaii or its culture.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "place_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are planning a <w>Hawaiian</w> vacation for next summer.",
                            "target_lang_translations": {
                                "ru": "Мы планируем <w>гавайский</w> отпуск на следующее лето."
                            }
                        },
                        {
                            "text": "He bought a colorful <w>Hawaiian</w> shirt to wear to the beach party.",
                            "target_lang_translations": {
                                "ru": "Он купил цветастую <w>гавайскую</w> рубашку, чтобы пойти на пляжную вечеринку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Hawaiian shirt",
                        "Hawaiian Islands",
                        "Hawaiian style"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к американскому штату Гавайи или его культуре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гава́йский",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69239883-802b-37e7-ad18-4bd01c35411d",
                    "sense_definition": "Of or pertaining to the indigenous people, culture, or language of Hawaii.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "culture_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dancers performed a traditional <w>Hawaiian</w> hula for the tourists.",
                            "target_lang_translations": {
                                "ru": "Танцоры исполнили для туристов традиционный <w>гавайский</w> танец хула."
                            }
                        },
                        {
                            "text": "She is very interested in <w>Hawaiian</w> history and legends.",
                            "target_lang_translations": {
                                "ru": "Она очень интересуется <w>гавайской</w> историей и легендами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Hawaiian culture",
                        "Hawaiian music",
                        "Hawaiian history"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к коренному народу, культуре или языку Гавайев."
                    },
                    "translations": {
                        "ru": [
                            {

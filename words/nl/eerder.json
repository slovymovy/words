            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "daa8a005-bf77-3589-bdaf-69da29526b4f",
                    "sense_definition": "Op een vroeger tijdstip; in het verleden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_ref",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb dit nog nooit <w>eerder</w> gezien.",
                            "target_lang_translations": {
                                "en": "I have never seen this <w>before</w>.",
                                "ru": "Я никогда не видел этого <w>раньше</w>."
                            }
                        },
                        {
                            "text": "Zoals <w>eerder</w> gezegd, gaan we morgen op vakantie.",
                            "target_lang_translations": {
                                "en": "As said <w>earlier</w>, we are going on vacation tomorrow.",
                                "ru": "Как было сказано <w>ранее</w>, мы едем в отпуск завтра."
                            }
                        }
                    ],
                    "synonyms": [
                        "vroeger"
                    ],
                    "antonyms": [
                        "later"
                    ],
                    "common_phrases": [
                        "niet eerder",
                        "eerder dan",
                        "zoals eerder gezegd"
                    ],
                    "target_lang_definitions": {
                        "en": "At an earlier time; in the past.",
                        "ru": "В более раннее время; в прошлом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earlier",
                                "target_lang_sense_clarification": "comparative of early; previously"
                            },
                            {
                                "target_lang_word": "before",
                                "target_lang_sense_clarification": "at a previous time"
                            },
                            {
                                "target_lang_word": "previously",
                                "target_lang_sense_clarification": "formal; before the present time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раньше",
                                "target_lang_sense_clarification": "наречие времени"
                            },
                            {
                                "target_lang_word": "ранее",
                                "target_lang_sense_clarification": "более формальный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72c84faf-4c0b-39ca-ae1d-92ddaf58c3db",
                    "sense_definition": "Om juister te zijn; in grotere mate; liever.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "preference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <w>eerder</w> een misverstand dan een fout.",
                            "target_lang_translations": {
                                "en": "It is <w>rather</w> a misunderstanding than a mistake.",
                                "ru": "Это <w>скорее</w> недоразумение, чем ошибка."
                            }
                        },
                        {
                            "text": "Ik zou <w>eerder</w> voor de rode auto kiezen.",
                            "target_lang_translations": {
                                "en": "I would <w>sooner</w> choose the red car.",
                                "ru": "Я бы <w>скорее</w> выбрал красную машину."
                            }
                        }
                    ],
                    "synonyms": [
                        "meer"
                    ],
                    "antonyms": [
                        "minder"
                    ],
                    "common_phrases": [
                        "of eerder",
                        "eerder dit dan dat"
                    ],
                    "target_lang_definitions": {
                        "en": "To be more precise; to a greater extent; preferably.",
                        "ru": "Точнее говоря; в большей степени; скорее."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rather",
                                "target_lang_sense_clarification": "used to correct something or indicate preference"
                            },
                            {
                                "target_lang_word": "sooner",
                                "target_lang_sense_clarification": "preferably; willingly"
                            },
                            {
                                "target_lang_word": "more",
                                "target_lang_sense_clarification": "to a greater degree"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "скорее",
                                "target_lang_sense_clarification": "предпочтение или уточнение"
                            },
                            {
                                "target_lang_word": "вернее",
                                "target_lang_sense_clarification": "уточнение сказанного"
                            }
                        ]
                    }
                }
            ]
        },
        {

                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical/waterbouwkunde"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитное покрытие или конструкция из плетеных материалов, используемая в гидротехнике для укрепления дна и берегов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюфяк",

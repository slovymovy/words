    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "25ccf725-c5cc-3bcf-8dec-06c453ec7cee",
                    "sense_definition": "Currently holding power, authority, or control over a country or group.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>ruling</w> party promised to improve the economy before the next election.",
                            "target_lang_translations": {
                                "ru": "<w>Правящая</w> партия пообещала улучшить экономику перед следующими выборами."
                            }
                        },
                        {
                            "text": "Despite the protests, the <w>ruling</w> monarch refused to step down from the throne.",
                            "target_lang_translations": {
                                "ru": "Несмотря на протесты, <w>правящий</w> монарх отказался отречься от престола."
                            }
                        }
                    ],
                    "synonyms": [
                        "reigning",
                        "in power",
                        "regnant"
                    ],
                    "common_phrases": [
                        "ruling party",
                        "ruling class",
                        "ruling monarch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся у власти, обладающий полномочиями или контролем над страной или группой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пра́вящий",
                                "target_lang_sense_clarification": "находящийся у власти (о партии, монархе)"
                            },
                            {
                                "target_lang_word": "госпо́дствующий",
                                "target_lang_sense_clarification": "доминирующий, преобладающий (о классе, мнении)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2ff2118c-7299-308f-b7ca-11b60fe70b8f",
                    "sense_definition": "An official decision or order made by a judge or an authority figure on a specific matter.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The court issued a <w>ruling</w> that banned the company from selling the product.",
                            "target_lang_translations": {
                                "ru": "Суд вынес <w>постановление</w>, запрещающее компании продавать этот продукт."
                            }
                        },
                        {
                            "text": "We are waiting for the official <w>ruling</w> before we can proceed with the construction.",
                            "target_lang_translations": {
                                "ru": "Мы ждем официального <w>решения</w>, прежде чем сможем приступить к строительству."
                            }
                        }
                    ],
                    "synonyms": [
                        "order",
                        "edict"
                    ],
                    "common_phrases": [
                        "court ruling",
                        "official ruling",
                        "make a ruling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное решение или приказ, вынесенные судьей или представителем власти по конкретному вопросу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постановле́ние",
                                "target_lang_sense_clarification": "официальное распоряжение или акт"
                            },
                            {
                                "target_lang_word": "реше́ние",
                                "target_lang_sense_clarification": "судебный вердикт или заключение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dfcc11b8-5c80-3f68-b3e3-017316d7199d",

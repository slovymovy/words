            "senses": [
                {
                    "sense_id": "546b57d2-e568-347b-8944-88d64ea5e6de",
                    "sense_definition": "Adolf Hitler (1889–1945), the leader of the Nazi Party and dictator of Germany from 1933 to 1945.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "historical_figure",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "The Second World War began after <w>Hitler</w> ordered the invasion of Poland.",
                            "target_lang_translations": {
                                "ru": "Вторая мировая война началась после того, как <w>Гитлер</w> отдал приказ о вторжении в Польшу."
                            }
                        },
                        {
                            "text": "Many historians have analyzed the speeches of Adolf <w>Hitler</w> to understand his rise to power.",
                            "target_lang_translations": {
                                "ru": "Многие историки анализировали речи Адольфа <w>Гитлера</w>, чтобы понять его приход к власти."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Adolf Hitler",
                        "Hitler's regime",
                        "rise of Hitler"
                    ],
                    "target_lang_definitions": {
                        "ru": "Адольф Гитлер (1889–1945), лидер нацистской партии и диктатор Германии с 1933 по 1945 год."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гитлер",
                                "target_lang_sense_clarification": "историческая личность"

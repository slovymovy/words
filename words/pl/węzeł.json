            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0188ea37-c1db-3ba7-b7cf-24bc2950dd09",
                    "sense_definition": "Miejsce, w którym sznurek, lina lub inny materiał został zaciśnięty po uprzednim przepleceniu, służące do łączenia elementów lub ich zabezpieczania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_knot",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę mocno zawiązać <w>węzeł</w> na butach, żeby się nie rozwiązały.",
                            "target_lang_translations": {
                                "ru": "Я должен крепко завязать <w>узел</w> на ботинках, чтобы они не развязались."
                            }
                        },
                        {
                            "text": "Żeglarz pokazał nam, jak zrobić prawidłowy <w>węzeł</w> ratowniczy.",
                            "target_lang_translations": {
                                "ru": "Моряк показал нам, как сделать правильный спасательный <w>узел</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "supeł",
                        "wiązanie"
                    ],
                    "common_phrases": [
                        "zawiązać węzeł",
                        "rozwiązać węzeł",
                        "mocny węzeł",
                        "węzeł płaski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где затянута веревка, шнур или другой материал для соединения или закрепления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узел",
                                "target_lang_sense_clarification": "из веревки, шнура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "790d5ed1-91eb-3854-a206-aae296241cc8",
                    "sense_definition": "Miejsce, w którym krzyżują się lub łączą drogi, szlaki komunikacyjne, rurociągi lub linie energetyczne.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "infrastructure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy <w>węzeł</w> autostradowy znacznie ułatwił wyjazd z miasta.",
                            "target_lang_translations": {
                                "ru": "Новая транспортная <w>развязка</w> значительно облегчила выезд из города."
                            }
                        },
                        {
                            "text": "To miasto stanowi ważny <w>węzeł</w> kolejowy w tej części kraju.",
                            "target_lang_translations": {
                                "ru": "Этот город является важным железнодорожным <w>узлом</w> в этой части страны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "węzeł komunikacyjny",
                        "węzeł drogowy",
                        "węzeł kolejowy",
                        "węzeł przesiadkowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место пересечения или соединения дорог, путей сообщения или линий коммуникаций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узел",
                                "target_lang_sense_clarification": "транспортный, железнодорожный"
                            },
                            {
                                "target_lang_word": "развязка",
                                "target_lang_sense_clarification": "дорожная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e362e5c-1f87-3f9b-a8c5-d900623921b5",
                    "sense_definition": "Silna więź, relacja lub zobowiązanie łączące ludzi.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda para uroczyście zawarła <w>węzeł</w> małżeński w obecności rodziny.",
                            "target_lang_translations": {
                                "ru": "Молодая пара торжественно скрепила брачные <w>узы</w> в присутствии семьи."
                            }
                        },
                        {
                            "text": "Mimo upływu lat, <w>węzeł</w> przyjaźni między nimi nigdy nie osłabł.",
                            "target_lang_translations": {
                                "ru": "Несмотря на прошедшие годы, <w>узы</w> дружбы между ними никогда не ослабевали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "węzeł małżeński",
                        "święty węzeł"
                    ],
                    "traits": [

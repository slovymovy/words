            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a6fe6f55-60a0-3a36-91e2-5d3bcc43abca",
                    "sense_definition": "Een van de vier windstreken, de richting waar de zon opkomt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zon komt elke ochtend op in het <w>oosten</w>.",
                            "target_lang_translations": {
                                "en": "The sun rises every morning in the <w>east</w>.",
                                "ru": "Солнце каждое утро восходит на <w>востоке</w>."
                            }
                        },
                        {
                            "text": "Duitsland ligt ten <w>oosten</w> van Nederland.",
                            "target_lang_translations": {
                                "en": "Germany lies to the <w>east</w> of the Netherlands.",
                                "ru": "Германия находится к <w>востоку</w> от Нидерландов."
                            }
                        }
                    ],
                    "synonyms": [
                        "oost"
                    ],
                    "antonyms": [
                        "westen"
                    ],
                    "common_phrases": [
                        "in het oosten",
                        "ten oosten van",
                        "naar het oosten"
                    ],
                    "target_lang_definitions": {
                        "en": "One of the four cardinal directions, the direction where the sun rises.",
                        "ru": "Одна из четырех сторон света, направление, где восходит солнце."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "east",
                                "target_lang_sense_clarification": "direction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "восток",
                                "target_lang_sense_clarification": "сторона света"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45565c04-9bd1-3d68-9192-39dc9b206160",
                    "sense_definition": "De landen en culturen in Azië en het oostelijke deel van de wereld, gezien vanuit Europa.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "region",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Veel specerijen werden vroeger uit het <w>Oosten</w> gehaald.",
                            "target_lang_translations": {
                                "en": "Many spices were formerly brought from the <w>East</w>.",
                                "ru": "Раньше многие специи привозили с <w>Востока</w>."
                            }
                        },
                        {
                            "text": "De cultuur in het <w>Oosten</w> verschilt van die in het Westen.",
                            "target_lang_translations": {
                                "en": "The culture in the <w>East</w> differs from that in the West.",
                                "ru": "Культура <w>Востока</w> отличается от культуры Запада."
                            }
                        }
                    ],
                    "synonyms": [
                        "Oriënt"
                    ],
                    "antonyms": [
                        "Westen"
                    ],
                    "common_phrases": [
                        "het Midden-Oosten",
                        "het Verre Oosten"
                    ],
                    "target_lang_definitions": {
                        "en": "The countries and cultures in Asia and the eastern part of the world, viewed from Europe.",
                        "ru": "Страны и культуры Азии и восточной части мира, часто противопоставляемые Западу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the East",
                                "target_lang_sense_clarification": "region/Asia"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Восток",
                                "target_lang_sense_clarification": "регион, страны Азии"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            {
                                "target_lang_word": "плоды",
                                "target_lang_sense_clarification": "результат усилий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d48c99ab-b5fd-38f6-ad60-22e1615153ec",

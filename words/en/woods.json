    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "13088882-8d72-347b-be7b-392571c75af1",
                    "sense_definition": "A dense collection of trees covering a relatively small area; a small forest.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "nature_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went for a long walk in the <w>woods</w> behind our house.",
                            "target_lang_translations": {
                                "ru": "Мы отправились на долгую прогулку в <w>лес</w> за нашим домом."
                            }
                        },
                        {
                            "text": "The cabin is hidden deep in the <w>woods</w>, far from the main road.",
                            "target_lang_translations": {
                                "ru": "Хижина спрятана глубоко в <w>лесу</w>, вдалеке от главной дороги."
                            }
                        },
                        {
                            "text": "Although the situation improved, we are not out of the <w>woods</w> yet.",
                            "target_lang_translations": {
                                "ru": "Хотя ситуация улучшилась, мы еще не вне <w>опасности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "forest",
                        "grove",
                        "woodland",
                        "copse"
                    ],
                    "antonyms": [
                        "clearing",
                        "meadow",
                        "plain",
                        "field"
                    ],
                    "common_phrases": [
                        "in the woods",
                        "out of the woods",
                        "neck of the woods",
                        "walk in the woods"
                    ],
                    "target_lang_definitions": {
                        "ru": "Густое скопление деревьев, покрывающее относительно небольшую территорию; небольшой лес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лес",
                                "target_lang_sense_clarification": "пространство, поросшее деревьями"
                            },
                            {
                                "target_lang_word": "роща",
                                "target_lang_sense_clarification": "небольшой, обособленный лес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "341bcb92-6fbd-37b5-8e5e-8577bf5694c5",
                    "sense_definition": "Plural form of <w>wood</w> (the hard fibrous material of trees), referring to different types or varieties.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "material_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter used a combination of dark and light <w>woods</w> for the table.",
                            "target_lang_translations": {
                                "ru": "Плотник использовал сочетание темных и светлых <w>пород дерева</w> для стола."
                            }
                        },
                        {
                            "text": "Exotic <w>woods</w> like mahogany and teak are often used in luxury furniture.",
                            "target_lang_translations": {
                                "ru": "Экзотические <w>сорта древесины</w>, такие как красное дерево и тик, часто используются в элитной мебели."
                            }
                        }
                    ],
                    "synonyms": [
                        "timbers",
                        "lumbers"
                    ],
                    "common_phrases": [
                        "hard woods",
                        "soft woods",
                        "exotic woods"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова wood (древесина), обозначающее различные её виды или сорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "породы дерева",
                                "target_lang_sense_clarification": "различные виды древесных растений как материал"
                            },
                            {
                                "target_lang_word": "сорта древесины",
                                "target_lang_sense_clarification": "разновидности древесного материала"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d30d5fe4-a160-335e-b1f3-d611e8679c55",
                    "sense_definition": "A topographic surname derived from Middle English.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "surname_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Woods</w> has been our neighbor for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Вудс</w> был нашим соседом более двадцати лет."
                            }
                        },
                        {
                            "text": "Tiger <w>Woods</w> is one of the most famous golfers in history.",
                            "target_lang_translations": {
                                "ru": "Тайгер <w>Вудс</w> — один из самых известных гольфистов в истории."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия топонимического происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Вудс",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]

{
    "word_family": [
        "teleskopowy",
        "teleskopowo"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "359ecd31-b94b-3bd5-9176-6919258b717b",
                    "sense_definition": "Przyrząd optyczny służący do obserwacji odległych obiektów, zwłaszcza ciał niebieskich.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "optical_instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody astronom obserwował księżyc przez swój pierwszy <w>teleskop</w>.",
                            "target_lang_translations": {
                                "ru": "Молодой астроном наблюдал за луной через свой первый <w>телескоп</w>."
                            }
                        },
                        {
                            "text": "Największy <w>teleskop</w> kosmiczny przesyła niesamowite zdjęcia galaktyk.",
                            "target_lang_translations": {
                                "ru": "Крупнейший космический <w>телескоп</w> передает невероятные снимки галактик."
                            }
                        }
                    ],
                    "synonyms": [
                        "luneta"
                    ],
                    "common_phrases": [
                        "teleskop kosmiczny",
                        "teleskop zwierciadlany",
                        "patrzeć przez teleskop",
                        "teleskop Hubble'a"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оптический прибор, используемый для наблюдения за удаленными объектами, особенно небесными телами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телескоп",
                                "target_lang_sense_clarification": "астрономический прибор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b077144d-5930-3c44-be80-2dcb7e1d758e",
                    "sense_definition": "Element konstrukcyjny składający się z rur wsuwanych jedna w drugą, co pozwala na regulację długości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wędka posiada składany <w>teleskop</w>, dzięki czemu łatwo ją transportować.",
                            "target_lang_translations": {
                                "ru": "У удочки есть складной <w>телескоп</w>, благодаря чему ее легко транспортировать."
                            }
                        },
                        {
                            "text": "Operator dźwigu wysunął <w>teleskop</w> na maksymalną długość.",
                            "target_lang_translations": {
                                "ru": "Оператор крана выдвинул <w>телескоп</w> на максимальную длину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wysięgnik teleskopu",
                        "ramię teleskopu",
                        "teleskop hydrauliczny"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конструктивный элемент, состоящий из труб, вставляемых одна в другую, что позволяет регулировать длину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телескопическая труба",
                                "target_lang_sense_clarification": "механизм"
                            },
                            {
                                "target_lang_word": "телескоп",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47444c07-c8ab-3121-8f5e-3fc5c5204a5f",
                    "sense_definition": "Odmiana złotej rybki charakteryzująca się dużymi, mocno wypukłymi oczami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sklepie zoologicznym kupiłem czarnego <w>teleskopa</w> do mojego akwarium.",
                            "target_lang_translations": {
                                "ru": "В зоомагазине я купил черного <w>телескопа</w> для своего аквариума."
                            }
                        },
                        {
                            "text": "Hodowla <w>teleskopów</w> wymaga dbania o wysoką jakość wody.",
                            "target_lang_translations": {
                                "ru": "Разведение <w>телескопов</w> требует поддержания высокого качества воды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czarny teleskop",
                        "teleskop czarny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность золотой рыбки, характеризующаяся большими, сильно выпуклыми глазами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телескоп",
                                "target_lang_sense_clarification": "рыба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18294acb-60cf-3057-a08a-bf6bf2614068",
                    "sense_definition": "Amortyzator w pojeździe zbudowany z rur wchodzących jedna w drugą.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mechanik zalecił wymianę przedniego <w>teleskopu</w> w samochodzie.",
                            "target_lang_translations": {
                                "ru": "Механик рекомендовал замену передней <w>телескопической стойки</w> в автомобиле."
                            }
                        },
                        {
                            "text": "Stare <w>teleskopy</w> przestały tłumić drgania na nierównej drodze.",
                            "target_lang_translations": {
                                "ru": "Старые <w>амортизаторы</w> перестали гасить вибрации на неровной дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "dalowid"
                    ],
                    "common_phrases": [
                        "wymiana teleskopów",
                        "cieknący teleskop"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "automotive jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Амортизатор в транспортном средстве, состоящий из входящих друг в друга труб."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телескопическая стойка",
                                "target_lang_sense_clarification": "деталь автомобиля"
                            },
                            {
                                "target_lang_word": "амортизатор",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
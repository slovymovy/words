    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ee51c0b8-ec50-33e7-98c1-e633fabcf78d",
                    "sense_definition": "Положительное качество, преимущество кого-либо или чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этого плана есть свои <w>достоинства</w> и недостатки.",
                            "target_lang_translations": {
                                "en": "This plan has its <w>merits</w> and flaws."
                            }
                        },
                        {
                            "text": "Главное <w>достоинство</w> этой квартиры — её удобное расположение.",
                            "target_lang_translations": {
                                "en": "The main <w>advantage</w> of this apartment is its convenient location."
                            }
                        }
                    ],
                    "synonyms": [
                        "преимущество",
                        "плюс"
                    ],
                    "antonyms": [
                        "недостаток",
                        "дефект"
                    ],
                    "common_phrases": [
                        "оценить по достоинству",
                        "главное достоинство",
                        "скрытые достоинства"
                    ],
                    "target_lang_definitions": {
                        "en": "A positive quality or advantage of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "merit",
                                "target_lang_sense_clarification": "positive quality"
                            },
                            {
                                "target_lang_word": "virtue",
                                "target_lang_sense_clarification": "good quality"
                            },
                            {
                                "target_lang_word": "advantage",
                                "target_lang_sense_clarification": "benefit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d146d39-8dea-37f6-a7c4-23562cc0bacd",
                    "sense_definition": "Осознание своей моральной ценности и уважение к самому себе.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "self_respect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Даже в трудной ситуации нужно сохранять чувство собственного <w>достоинства</w>.",
                            "target_lang_translations": {
                                "en": "Even in a difficult situation, one must maintain a sense of self-<w>dignity</w>."
                            }
                        },
                        {
                            "text": "Это предложение унижает моё человеческое <w>достоинство</w>.",
                            "target_lang_translations": {
                                "en": "This offer humiliates my human <w>dignity</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "благородство"
                    ],
                    "common_phrases": [
                        "чувство собственного достоинства",
                        "человеческое достоинство",
                        "уронить достоинство"
                    ],
                    "target_lang_definitions": {
                        "en": "Awareness of one's own moral worth and self-respect."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dignity",
                                "target_lang_sense_clarification": "self-worth"
                            },
                            {
                                "target_lang_word": "self-respect",
                                "target_lang_sense_clarification": "regard for oneself"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fd9bcd1-bd04-3104-87e8-038daec2966f",
                    "sense_definition": "Внешнее проявление самоуважения, сдержанность и благородство в поведении.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "self_respect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она приняла эту неприятную новость с удивительным <w>достоинством</w>.",
                            "target_lang_translations": {
                                "en": "She accepted this unpleasant news with surprising <w>dignity</w>."
                            }
                        },
                        {
                            "text": "Он вышел из комнаты медленно и с <w>достоинством</w>.",
                            "target_lang_translations": {
                                "en": "He left the room slowly and with <w>dignity</w>."
                            }

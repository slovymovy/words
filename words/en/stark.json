{
    "word_family": [
        "starkly",
        "starkness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6e831fcc-7ec0-3a4b-b738-1e90b38caacb",
                    "sense_definition": "Empty, simple, or plain in appearance, often in a way that seems cold, desolate, or unpleasant.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "stark_adj_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>stark</w> landscape of the desert was both beautiful and intimidating.",
                            "target_lang_translations": {
                                "ru": "<w>Суровый</w> пустынный пейзаж был одновременно прекрасен и пугающ."
                            }
                        },
                        {
                            "text": "She sat in a <w>stark</w> white room with no furniture or decoration.",
                            "target_lang_translations": {
                                "ru": "Она сидела в <w>пустой</w> белой комнате без мебели и украшений."
                            }
                        }
                    ],
                    "synonyms": [
                        "desolate",
                        "barren",
                        "bleak",
                        "bare",
                        "austere"
                    ],
                    "antonyms": [
                        "lush",
                        "ornate",
                        "decorated"
                    ],
                    "common_phrases": [
                        "stark landscape",
                        "stark beauty",
                        "stark white",
                        "stark room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пустой, простой или неприукрашенный на вид, часто производящий впечатление холода, запустения или неприятности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пустынный",
                                "target_lang_sense_clarification": "о местности или пейзаже"
                            },
                            {
                                "target_lang_word": "голый",
                                "target_lang_sense_clarification": "о стенах, комнатах или деревьях"
                            },
                            {
                                "target_lang_word": "суровый",
                                "target_lang_sense_clarification": "о внешнем виде или стиле"
                            },
                            {
                                "target_lang_word": "строгий",
                                "target_lang_sense_clarification": "простой, без излишеств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89fc47ed-ff4e-3c1d-b825-05ca15ff50c0",
                    "sense_definition": "Very obvious, clear, or absolute; often used to describe a sharp difference or an undeniable fact.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "stark_adj_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a <w>stark</w> contrast between the lives of the rich and the poor in this city.",
                            "target_lang_translations": {
                                "ru": "В этом городе существует <w>разительный</w> контраст между жизнью богатых и бедных."
                            }
                        },
                        {
                            "text": "The report issued a <w>stark</w> warning about the dangers of climate change.",
                            "target_lang_translations": {
                                "ru": "В докладе содержалось <w>суровое</w> предупреждение об опасностях изменения климата."
                            }
                        }
                    ],
                    "synonyms": [
                        "sharp",
                        "distinct",
                        "clear",
                        "absolute",
                        "unmistakable"
                    ],
                    "antonyms": [
                        "subtle",
                        "fuzzy",
                        "slight",
                        "indistinct"
                    ],
                    "common_phrases": [
                        "stark contrast",
                        "stark difference",
                        "stark warning",
                        "stark relief"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень очевидный, ясный или абсолютный; часто используется для описания резкого различия или неоспоримого факта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разительный",
                                "target_lang_sense_clarification": "о контрасте или перемене"
                            },
                            {
                                "target_lang_word": "резкий",
                                "target_lang_sense_clarification": "о различии или границе"
                            },
                            {
                                "target_lang_word": "явный",
                                "target_lang_sense_clarification": "очевидный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d539467-acc4-3ce1-985a-41f69eed1da2",
                    "sense_definition": "Unpleasantly clear, harsh, or grim; impossible to avoid or ignore.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "stark_adj_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The refugees face a <w>stark</w> choice between staying in a war zone or risking their lives at sea.",
                            "target_lang_translations": {
                                "ru": "Беженцы стоят перед <w>суровым</w> выбором: остаться в зоне военных действий или рисковать жизнью в море."
                            }
                        },
                        {
                            "text": "The documentary provided a <w>stark</w> reminder of the brutality of the conflict.",
                            "target_lang_translations": {
                                "ru": "Документальный фильм послужил <w>суровым</w> напоминанием о жестокости конфликта."
                            }
                        }
                    ],
                    "synonyms": [
                        "grim",
                        "harsh",
                        "severe",
                        "blunt"
                    ],
                    "antonyms": [
                        "pleasant",
                        "comforting",
                        "mild"
                    ],
                    "common_phrases": [
                        "stark choice",
                        "stark reality",
                        "stark truth",
                        "stark reminder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприятно ясный, суровый или мрачный; то, что невозможно избежать или игнорировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суровый",
                                "target_lang_sense_clarification": "о выборе, реальности или напоминании"
                            },
                            {
                                "target_lang_word": "жестокий",
                                "target_lang_sense_clarification": "о правде или действительности"
                            },
                            {
                                "target_lang_word": "неприглядный",
                                "target_lang_sense_clarification": "о фактах или истине"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "737b3f31-7e1e-3fd1-b984-5147b0c0772c",
                    "sense_definition": "Completely or extremely; used to emphasize specific adjectives indicating madness or nakedness.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "stark_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The toddler ran out of the bathroom <w>stark</w> naked before his mother could catch him.",

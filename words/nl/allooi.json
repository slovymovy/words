{
    "word_family": [
        "allieren",
        "alliage"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c994a8b2-f847-3517-bd1b-edd9d27954e8",
                    "sense_definition": "Kwaliteit, innerlijke waarde of soort, vaak gebruikt om het niveau van personen of zaken aan te duiden.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij gingen liever niet om met mensen van dat <w>allooi</w>.",
                            "target_lang_translations": {
                                "ru": "Они предпочитали не общаться с людьми такого <w>пошиба</w>."
                            }
                        },
                        {
                            "text": "De aangeboden goederen waren helaas van bedenkelijk <w>allooi</w>.",
                            "target_lang_translations": {
                                "ru": "Предлагаемые товары были, к сожалению, сомнительного <w>качества</w>."
                            }
                        },
                        {
                            "text": "Dit is dichtkunst van <w>allooi</w>, die nog jaren gelezen zal worden.",
                            "target_lang_translations": {
                                "ru": "Это поэзия высокой <w>пробы</w>, которую будут читать еще долгие годы."
                            }
                        }
                    ],
                    "synonyms": [
                        "alliage",
                        "legering"
                    ],
                    "common_phrases": [
                        "van bedenkelijk allooi",
                        "van verdacht allooi",
                        "van goed allooi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество, внутренняя ценность или род; часто используется для характеристики уровня людей или вещей (нередко с негативным оттенком)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пошиб",
                                "target_lang_sense_clarification": "разряд или сорт, часто с пренебрежительным оттенком"
                            },
                            {
                                "target_lang_word": "сорт",
                                "target_lang_sense_clarification": "категория или разновидность"
                            },
                            {
                                "target_lang_word": "проба",
                                "target_lang_sense_clarification": "перен. качество, достоинство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bd464a9-b948-3d91-bbaf-e55c4ee2165e",
                    "sense_definition": "Een mengsel van samengesmolten metalen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metal_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Elektrum is een natuurlijk voorkomend <w>allooi</w> van zilver en goud.",
                            "target_lang_translations": {
                                "ru": "Электрум — это встречающийся в природе <w>сплав</w> серебра и золота."
                            }
                        },
                        {
                            "text": "Voor dit specifieke onderdeel is een hard <w>allooi</w> vereist.",
                            "target_lang_translations": {
                                "ru": "Для этой конкретной детали требуется твердый <w>сплав</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "legering",
                        "alliage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смесь расплавленных металлов; сплав."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сплав",
                                "target_lang_sense_clarification": "соединение металлов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7375494-1706-369a-9190-1534c5f42cf1",
                    "sense_definition": "Het gehalte aan edelmetaal in een legering, uitgedrukt in een verhouding.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "metal_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De muntmeester controleerde nauwkeurig het <w>allooi</w> van de gouden munten.",
                            "target_lang_translations": {
                                "ru": "Монетный мастер тщательно проверял <w>пробу</w> золотых монет."
                            }
                        },
                        {
                            "text": "Munten met een te laag <w>allooi</w> werden uit de roulatie genomen.",
                            "target_lang_translations": {
                                "ru": "Монеты со слишком низкой <w>пробой</w> изымались из обращения."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержание драгоценного металла в сплаве, выраженное в пропорции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проба",
                                "target_lang_sense_clarification": "содержание благородного металла"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
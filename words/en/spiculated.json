    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "89b37387-ac6d-336c-b288-ccb11d549a0d",
                    "sense_definition": "Having small, needle-like spikes or sharp points on the surface.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "shape_texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor was concerned about the <w>spiculated</w> appearance of the mass on the scan.",
                            "target_lang_translations": {
                                "ru": "Врач был обеспокоен <w>спикулообразным</w> видом образования на снимке."
                            }
                        },
                        {
                            "text": "Under the microscope, the cell membrane looked <w>spiculated</w> rather than smooth.",
                            "target_lang_translations": {
                                "ru": "Под микроскопом клеточная мембрана выглядела <w>шиповидной</w>, а не гладкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "spiky",
                        "barbed",
                        "prickly"
                    ],
                    "antonyms": [
                        "smooth",
                        "rounded"
                    ],
                    "common_phrases": [
                        "spiculated margin",
                        "spiculated mass",
                        "spiculated cells"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий небольшие игольчатые шипы или острые выступы на поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спикулообразный",
                                "target_lang_sense_clarification": "имеющий форму спикул; часто используется в медицине"
                            },
                            {
                                "target_lang_word": "шиповидный",
                                "target_lang_sense_clarification": "покрытый шипами (например, о клетках)"
                            },
                            {
                                "target_lang_word": "лучистый",
                                "target_lang_sense_clarification": "термин радиологии (о контурах образования)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "72e7f836-3055-3f8f-82a7-e35aea28620c",
                    "sense_definition": "Simple past and past participle of <w>spiculate</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The frost <w>spiculated</w> across the windowpane in intricate patterns.",
                            "target_lang_translations": {
                                "ru": "Иней <w>разросся иглами</w> по оконному стеклу, образуя замысловатые узоры."
                            }
                        },
                        {
                            "text": "As the solution dried, the crystals <w>spiculated</w> into sharp, needle-like forms.",
                            "target_lang_translations": {
                                "ru": "По мере высыхания раствора кристаллы <w>вытянулись</w> в острые, игольчатые формы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола spiculate (образовывать иглы/шипы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "образовал спикулы",
                                "target_lang_sense_clarification": "сформировал игольчатые структуры"
                            },
                            {
                                "target_lang_word": "принял игольчатую форму",
                                "target_lang_sense_clarification": "стал похожим на иглу или шип"

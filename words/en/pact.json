                {
                    "sense_id": "fca7398c-3e89-37a9-a0b3-ca357d5ded9a",
                    "sense_definition": "A formal agreement between individuals or groups to do something or to help each other.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "agreement_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We made a <w>pact</w> to stay friends no matter what happens.",
                            "target_lang_translations": {
                                "ru": "Мы заключили <w>договор</w> остаться друзьями, что бы ни случилось."
                            }
                        },
                        {
                            "text": "The siblings formed a secret <w>pact</w> to hide the broken vase from their parents.",
                            "target_lang_translations": {
                                "ru": "Брат с сестрой заключили тайный <w>уговор</w>, чтобы скрыть разбитую вазу от родителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "agreement",
                        "compact",
                        "covenant",
                        "bond"
                    ],
                    "common_phrases": [

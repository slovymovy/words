{
    "word_family": [
        "hoed",
        "hoeder",
        "behoeden"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "05d49673-e222-3963-b5e6-9c610cb1e87a",
                    "sense_definition": "Meervoud van <w>hoed</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dames droegen prachtige <w>hoeden</w> tijdens de bruiloft.",
                            "target_lang_translations": {
                                "en": "The ladies wore beautiful <w>hats</w> during the wedding.",
                                "ru": "Дамы были в прекрасных <w>шляпах</w> во время свадьбы."
                            }
                        },
                        {
                            "text": "Bij de ingang hangen de jassen en <w>hoeden</w> aan de kapstok.",
                            "target_lang_translations": {
                                "en": "At the entrance, the coats and <w>hats</w> hang on the coat rack.",
                                "ru": "У входа на вешалке висят пальто и <w>шляпы</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of <w>hat</w>.",
                        "ru": "Множественное число от слова \"шляпа\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hats",
                                "target_lang_sense_clarification": "plural form of headwear"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шляпы",
                                "target_lang_sense_clarification": "головные уборы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "18e0d277-344a-369f-a852-a80a9379596b",
                    "sense_definition": "Een kudde dieren (zoals schapen of geiten) bewaken en verzorgen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De herder <w>hoedde</w> zijn schapen op de uitgestrekte heide.",
                            "target_lang_translations": {
                                "en": "The shepherd <w>tended</w> his sheep on the vast heath.",
                                "ru": "Пастух <w>пас</w> своих овец на обширной пустоши."
                            }
                        },
                        {
                            "text": "Vroeger was het heel gewoon dat kinderen de ganzen moesten <w>hoeden</w>.",
                            "target_lang_translations": {
                                "en": "In the past, it was quite common for children to have to <w>tend</w> the geese.",
                                "ru": "Раньше было обычным делом, что дети должны были <w>пасти</w> гусей."
                            }
                        }
                    ],
                    "synonyms": [
                        "weiden",
                        "bewaken"
                    ],
                    "common_phrases": [
                        "schapen hoeden",
                        "vee hoeden"
                    ],
                    "target_lang_definitions": {
                        "en": "To guard and care for a herd of animals (such as sheep or goats).",
                        "ru": "Охранять и пасти стадо животных (например, овец или коз)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tend",
                                "target_lang_sense_clarification": "to care for or look after animals"
                            },
                            {
                                "target_lang_word": "herd",
                                "target_lang_sense_clarification": "to keep or look after livestock"
                            },
                            {
                                "target_lang_word": "watch over",
                                "target_lang_sense_clarification": "to guard and protect"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пасти",
                                "target_lang_sense_clarification": "животных"
                            },
                            {
                                "target_lang_word": "стеречь",
                                "target_lang_sense_clarification": "охранять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "523de703-920a-399a-8153-a4136ab85e35",
                    "sense_definition": "Zich in acht nemen voor gevaar of fouten; oppassen dat iets negatiefs niet gebeurt.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet je <w>hoeden</w> voor mensen die te mooie beloftes maken.",
                            "target_lang_translations": {
                                "en": "You must <w>guard</w> against people who make promises that are too good to be true.",
                                "ru": "Тебе следует <w>остерегаться</w> людей, которые дают слишком красивые обещания."
                            }
                        },
                        {
                            "text": "Het bestuur <w>hoedde</w> zich voor overhaaste beslissingen tijdens de crisis.",
                            "target_lang_translations": {
                                "en": "The board <w>guarded</w> against hasty decisions during the crisis.",
                                "ru": "Правление <w>остерегалось</w> поспешных решений во время кризиса."
                            }
                        }
                    ],
                    "synonyms": [
                        "oppassen",
                        "waken"
                    ],
                    "common_phrases": [
                        "zich hoeden voor"
                    ],
                    "target_lang_definitions": {
                        "en": "To be wary of danger or mistakes; to watch out that something negative does not happen.",
                        "ru": "Остерегаться опасности или ошибок; быть осторожным, чтобы не случилось чего-то плохого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "guard against",
                                "target_lang_sense_clarification": "to take precautions against"
                            },
                            {
                                "target_lang_word": "beware of",
                                "target_lang_sense_clarification": "to be cautious of"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "остерегаться",
                                "target_lang_sense_clarification": "быть осторожным"
                            },
                            {
                                "target_lang_word": "беречься",
                                "target_lang_sense_clarification": "избегать опасности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
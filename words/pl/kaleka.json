    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0ce724a-cd78-3b2b-b15e-452573be9810",
                    "sense_definition": "Osoba, która utraciła pełną sprawność fizyczną z powodu trwałego uszkodzenia ciała, choroby lub wady wrodzonej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disability_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ciężkim wypadku samochodowym mój sąsiad został <w>kaleką</w>.",
                            "target_lang_translations": {
                                "ru": "После тяжелой автомобильной аварии мой сосед остался <w>калекой</w>."
                            }
                        },
                        {
                            "text": "Fundacja zbiera pieniądze na wózki inwalidzkie, aby pomóc biednym <w>kalekom</w>.",
                            "target_lang_translations": {
                                "ru": "Фонд собирает деньги на инвалидные коляски, чтобы помочь бедным <w>калекам</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "inwalida"
                    ],
                    "common_phrases": [
                        "zostać kaleką",
                        "biedny kaleka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, утративший полную физическую дееспособность из-за стойкого повреждения тела, болезни или врожденного порока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "калека",
                                "target_lang_sense_clarification": "человек с физическими увечьями"
                            },
                            {
                                "target_lang_word": "инвалид",
                                "target_lang_sense_clarification": "медицинский/официальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dccb579f-913e-3637-8549-93c68cfbc867",
                    "sense_definition": "Osoba bardzo niezdarna, niezręczna lub niezaradna życiowo.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clumsiness_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie proś go o naprawę kranu, to straszna <w>kaleka</w> techniczna.",
                            "target_lang_translations": {
                                "ru": "Не проси его чинить кран, он в технике полная <w>бестолочь</w>."
                            }
                        },
                        {
                            "text": "Mój brat to życiowa <w>kaleka</w>, nie potrafi nawet opłacić rachunków.",
                            "target_lang_translations": {
                                "ru": "Мой брат — <w>недотёпа</w> по жизни, не может даже оплатить счета."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezdara",
                        "oferma",
                        "ciamajda",
                        "fajtłapa",
                        "łamaga",
                        "niedołęga"
                    ],
                    "common_phrases": [
                        "życiowa kaleka",
                        "ale kaleka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe clumsy people"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень неуклюжий, неловкий или неприспособленный к жизни человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недотёпа",
                                "target_lang_sense_clarification": "неловкий, неуклюжий человек"
                            },
                            {
                                "target_lang_word": "неумеха",
                                "target_lang_sense_clarification": "человек, который ничего не умеет делать"
                            },
                            {
                                "target_lang_word": "инвалид",
                                "target_lang_sense_clarification": "переносное значение: неприспособленный человек"
                            }
                        ]
                    }
                }
            ]
        },
        {

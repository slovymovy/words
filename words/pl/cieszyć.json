                    "sense_id": "be2b2203-d34f-3ecb-8f8b-24fb060b93e7",
                    "sense_definition": "Odczuwać radość, zadowolenie lub satysfakcję z jakiegoś powodu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "feeling_joy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo się <w>cieszę</w>, że w końcu cię widzę.",
                            "target_lang_translations": {
                                "ru": "Я очень <w>рад</w>, что наконец вижу тебя."
                            }
                        },
                        {
                            "text": "Dzieci <w>cieszyły</w> się z otrzymanych pod choinkę prezentów.",
                            "target_lang_translations": {
                                "ru": "Дети <w>радовались</w> подаркам, полученным на Рождество."
                            }
                        }
                    ],
                    "synonyms": [
                        "radować się",
                        "weselić się"
                    ],
                    "antonyms": [
                        "smucić się",
                        "martwić się"
                    ],
                    "common_phrases": [
                        "cieszyć się życiem",
                        "cieszyć się z prezentu",
                        "cieszyć się na spotkanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать радость, удовлетворение или удовольствие по какой-либо причине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радоваться",
                                "target_lang_sense_clarification": "испытывать чувство радости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "210f2b1f-37a4-3d88-bf0c-b732e225a977",
                    "sense_definition": "Sprawiać komuś przyjemność, wywoływać u kogoś uczucie radości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "causing_joy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój sukces zawodowy ogromnie mnie <w>cieszy</w>.",
                            "target_lang_translations": {
                                "ru": "Твой профессиональный успех меня очень <w>радует</w>."
                            }
                        },
                        {
                            "text": "Widok kwitnących wiosną kwiatów <w>cieszył</w> nasze oczy.",
                            "target_lang_translations": {
                                "ru": "Вид цветущих весной цветов <w>радовал</w> наш глаз."
                            }
                        }
                    ],
                    "synonyms": [
                        "radować",
                        "weselić"
                    ],
                    "antonyms": [
                        "smucić",
                        "martwić"
                    ],
                    "common_phrases": [
                        "to mnie cieszy",
                        "cieszyć oko",
                        "cieszyć rodziców"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доставлять кому-то удовольствие, вызывать у кого-то чувство радости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радовать",
                                "target_lang_sense_clarification": "доставлять радость"
                            },
                            {
                                "target_lang_word": "веселить",
                                "target_lang_sense_clarification": "развлекать, забавлять"
                            },
                            {
                                "target_lang_word": "тешить",
                                "target_lang_sense_clarification": "доставлять удовольствие, часто в контексте самолюбия или надежд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d2fef6c-2e89-349f-97ec-43ae44f3fe1f",
                    "sense_definition": "Posiadać coś wartościowego (np. zdrowie, sławę) lub korzystać z czyjegoś uznania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "possessing_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten młody aktor <w>cieszy</w> się dużą popularnością w Polsce.",
                            "target_lang_translations": {
                                "ru": "Этот молодой актер <w>пользуется</w> большой популярностью в Польше."
                            }
                        },
                        {
                            "text": "Mimo podeszłego wieku dziadek wciąż <w>cieszy</w> się dobrym zdrowiem.",
                            "target_lang_translations": {
                                "ru": "Несмотря на преклонный возраст, дедушка всё ещё <w>обладает</w> хорошим здоровьем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cieszyć się zdrowiem",
                        "cieszyć się uznaniem",
                        "cieszyć się złą sławą",
                        "cieszyć się popularnością"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладать чем-то ценным (например, здоровьем, славой) или пользоваться чьим-то признанием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пользоваться",
                                "target_lang_sense_clarification": "иметь (успех, популярность, уважение)"
                            },
                            {
                                "target_lang_word": "обладать",
                                "target_lang_sense_clarification": "иметь (здоровье, права)"
                            }
                        ]
                    }
                }
            ]
        }

                            {
                                "target_lang_word": "виртуоз",
                                "target_lang_sense_clarification": "человек, достигший совершенства в деле"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "master",
                                "target_lang_sense_clarification": "expert or skilled artist"
                            },
                            {
                                "target_lang_word": "virtuoso",
                                "target_lang_sense_clarification": "highly skilled person"
                            }
                        ]

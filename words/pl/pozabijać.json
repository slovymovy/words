                        "zamknąć"
                    ],
                    "antonyms": [
                        "otworzyć"
                    ],
                    "common_phrases": [
                        "pozabijać okna deskami",
                        "pozabijać otwory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заколотить досками или гвоздями много отверстий (например, окон или дверей)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заколотить",
                                "target_lang_sense_clarification": "плотно закрыть, забив гвоздями или досками"

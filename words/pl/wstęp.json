    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f1e0e47-2332-3dc2-8337-68e2b1311da6",
                    "sense_definition": "Prawo lub możliwość wejścia na dany teren, do budynku lub uczestniczenia w wydarzeniu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "admission_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci do lat siedmiu mają darmowy <w>wstęp</w> do muzeum.",
                            "target_lang_translations": {
                                "ru": "Детям до семи лет <w>вход</w> в музей бесплатный."
                            }
                        },
                        {
                            "text": "Ochroniarz powiedział, że <w>wstęp</w> na imprezę jest tylko dla zaproszonych gości.",
                            "target_lang_translations": {
                                "ru": "Охранник сказал, что <w>вход</w> на вечеринку только для приглашенных гостей."
                            }
                        },
                        {
                            "text": "Na drzwiach wisiała tabliczka z napisem: <w>wstęp</w> wzbroniony.",
                            "target_lang_translations": {
                                "ru": "На двери висела табличка с надписью: <w>вход</w> воспрещен."
                            }
                        }
                    ],
                    "synonyms": [
                        "wejście",
                        "prawo wejścia"
                    ],
                    "common_phrases": [
                        "wstęp wolny",
                        "bilet wstępu",
                        "wstęp wzbroniony",
                        "mieć wstęp"
                    ],
                    "target_lang_definitions": {
                        "ru": "Право или возможность входа на территорию, в здание или участия в мероприятии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вход",
                                "target_lang_sense_clarification": "действие или право входа"
                            },
                            {
                                "target_lang_word": "доступ",
                                "target_lang_sense_clarification": "возможность попасть куда-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71fabcaa-d222-35d6-b84b-a91d2efcf702",
                    "sense_definition": "Wstępna, wyodrębniona część tekstu, książki lub przemówienia, wprowadzająca w temat lub wyjaśniająca okoliczności powstania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intro_text_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autor napisał bardzo osobisty <w>wstęp</w> do swojej nowej powieści.",
                            "target_lang_translations": {
                                "ru": "Автор написал очень личное <w>предисловие</w> к своему новому роману."
                            }
                        },
                        {
                            "text": "We <w>wstępie</w> wyjaśniono główne tezy zawarte w artykule.",
                            "target_lang_translations": {
                                "ru": "Во <w>введении</w> объяснены основные тезисы, содержащиеся в статье."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedmowa",
                        "wprowadzenie",
                        "prolog",
                        "przedsłowie"
                    ],
                    "antonyms": [

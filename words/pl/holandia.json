            "pos": "name",
            "senses": [
                {
                    "sense_id": "1bfd3741-4163-398d-a55f-e9d68a37bcdf",
                    "sense_definition": "Państwo w Europie Zachodniej, którego stolicą jest Amsterdam; potocznie nazywane Niderlandami.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geo_country",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W te wakacje planujemy pojechać samochodem do <w>Holandii</w>.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы планируем поехать на машине в <w>Голландию</w>."
                            }
                        },
                        {
                            "text": "<w>Holandia</w> słynie z pięknych pól tulipanów i zabytkowych wiatraków.",
                            "target_lang_translations": {
                                "ru": "<w>Голландия</w> славится красивыми тюльпановыми полями и старинными ветряными мельницами."
                            }
                        }
                    ],
                    "synonyms": [
                        "Niderlandy",
                        "Królestwo Niderlandów"
                    ],
                    "common_phrases": [
                        "król Holandii",
                        "wycieczka do Holandii",
                        "mapa Holandii",
                        "stolica Holandii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государство в Западной Европе со столицей в Амстердаме; официально называется Нидерланды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Голландия",
                                "target_lang_sense_clarification": "общеупотребительное название"
                            },
                            {
                                "target_lang_word": "Нидерланды",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a46edfdf-d407-35c5-9f67-a53a42008818",
                    "sense_definition": "Reprezentacja sportowa tego kraju, szczególnie w piłce nożnej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_team",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "Wczoraj <w>Holandia</w> wygrała ważny mecz z Niemcami.",
                            "target_lang_translations": {
                                "ru": "Вчера <w>Голландия</w> выиграла важный матч у Германии."
                            }
                        },
                        {
                            "text": "Wszyscy kibice liczyli na to, że <w>Holandia</w> zdobędzie mistrzostwo.",
                            "target_lang_translations": {
                                "ru": "Все болельщики рассчитывали на то, что <w>Голландия</w> выиграет чемпионат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mecz z Holandią",
                        "Holandia gra",
                        "kibicować Holandii"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w kontekście sportowym jako skrót myślowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивная сборная этой страны, особенно по футболу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Голландия",
                                "target_lang_sense_clarification": "о спортивной команде"
                            },
                            {
                                "target_lang_word": "сборная Нидерландов",
                                "target_lang_sense_clarification": "национальная команда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0135f03f-c83c-3c28-bd14-70c31508ca40",
                    "sense_definition": "Historyczna kraina w zachodniej części Niderlandów, obejmująca dwie współczesne prowincje.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geo_region",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Historyczna <w>Holandia</w> była najważniejszym regionem gospodarczym republiki.",
                            "target_lang_translations": {
                                "ru": "Историческая <w>Голландия</w> была важнейшим экономическим регионом республики."
                            }
                        },
                        {
                            "text": "Wiele osób myli ten region z całym krajem, ale właściwa <w>Holandia</w> to tylko dwie prowincje.",
                            "target_lang_translations": {
                                "ru": "Многие путают этот регион со всей страной, но собственно <w>Голландия</w> — это только две провинции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hrabstwo Holandii",
                        "prowincja Holandia"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "dotyczy konkretnego regionu historycznego"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Историческая область на западе Нидерландов, включающая две современные провинции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Голландия",
                                "target_lang_sense_clarification": "историческая область"
                            }
                        ]
                    }
                }
            ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c7053a26-14d5-3962-88f2-4c464879666f",
                    "sense_definition": "A long, organized journey undertaken by a group of people for a specific purpose, such as exploration, research, or war.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "journey_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientists are planning a scientific <w>expedition</w> to the Amazon rainforest.",
                            "target_lang_translations": {
                                "ru": "Ученые планируют научную <w>экспедицию</w> в тропические леса Амазонки."
                            }
                        },
                        {
                            "text": "They decided to go on an <w>expedition</w> to find the lost city.",
                            "target_lang_translations": {
                                "ru": "Они решили отправиться в <w>экспедицию</w>, чтобы найти затерянный город."
                            }
                        },
                        {
                            "text": "The general led a military <w>expedition</w> across the mountains.",
                            "target_lang_translations": {
                                "ru": "Генерал возглавил военную <w>экспедицию</w> через горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "journey",
                        "voyage",
                        "trek",
                        "mission",
                        "excursion"
                    ],
                    "common_phrases": [
                        "go on an expedition",
                        "scientific expedition",
                        "military expedition",
                        "mount an expedition",
                        "polar expedition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длительное организованное путешествие, предпринимаемое группой людей с определенной целью, например, для исследования, научных изысканий или военных действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспедиция",
                                "target_lang_sense_clarification": "путешествие"
                            },
                            {
                                "target_lang_word": "поход",
                                "target_lang_sense_clarification": "военный или туристический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b791982-16d7-3344-89b1-cb5cdb252087",
                    "sense_definition": "The group of people, vehicles, and equipment involved in an organized journey.",

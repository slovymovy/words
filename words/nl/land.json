            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3ce2ae0-c624-3300-bc94-ba5fd6cff0fd",
                    "sense_definition": "Een geografisch afgebakend gebied dat een eigen staat vormt met een eigen regering.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geo_political",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nederland is een klein <w>land</w> met veel water en bruggen.",
                            "target_lang_translations": {
                                "ru": "Нидерланды — маленькая <w>страна</w> с большим количеством воды и мостов."
                            }
                        },
                        {
                            "text": "De koning en koningin bezoeken elke provincie in ons <w>land</w>.",
                            "target_lang_translations": {
                                "ru": "Король и королева посещают каждую провинцию в нашей <w>стране</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "staat",
                        "rijk"
                    ],
                    "common_phrases": [
                        "in ons land",
                        "het hele land",
                        "vreemd land"
                    ],
                    "target_lang_definitions": {
                        "ru": "Географически определённая территория, образующая собственное государство с собственным правительством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страна",
                                "target_lang_sense_clarification": "государство, территория"
                            },
                            {
                                "target_lang_word": "государство",
                                "target_lang_sense_clarification": "политическая организация общества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57f8e93a-41b6-351f-8bf0-285df908168d",
                    "sense_definition": "Het deel van het aardoppervlak dat niet met water bedekt is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_terrain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na weken op zee kwamen de zeelieden eindelijk weer aan <w>land</w>.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3175e5d0-6675-3f85-8b9c-379052a0d180",
                    "sense_definition": "A small piece, portion, or quantity of something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "amount_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There were <w>bits</w> of broken glass scattered all over the floor.",

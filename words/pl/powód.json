    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e9a509c-81f5-3db3-bb9c-1bbbb750a42a",
                    "sense_definition": "Coś, co uzasadnia podjęcie jakiegoś działania, wyjaśnia zdarzenie lub jest przyczyną określonego stanu rzeczy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reason_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie miał żadnego <powodu>, żeby kłamać w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "У него не было никакой <w>причины</w> лгать по этому делу."
                            }
                        },
                        {
                            "text": "Lekarz zapewniał pacjenta, że nie ma <powodu> do niepokoju.",
                            "target_lang_translations": {
                                "ru": "Врач уверял пациента, что нет <w>повода</w> для беспокойства."
                            }
                        },
                        {
                            "text": "Zagrożenie powodzią jest głównym <powodem> budowania wałów wzdłuż rzeki.",
                            "target_lang_translations": {
                                "ru": "Угроза наводнения является главной <w>причиной</w> строительства дамб вдоль реки."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyczyna",
                        "pobudka",
                        "przesłanka"
                    ],
                    "antonyms": [
                        "pozwany"
                    ],
                    "common_phrases": [
                        "z powodu",
                        "bez powodu",
                        "dać powód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обстоятельство, служащее основанием для какого-либо действия или причиной чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "причина",
                                "target_lang_sense_clarification": "основание, источник"
                            },
                            {
                                "target_lang_word": "повод",
                                "target_lang_sense_clarification": "предлог, случай"
                            },
                            {
                                "target_lang_word": "основание",
                                "target_lang_sense_clarification": "обоснование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f45b7afc-c929-3f54-8689-4a03bc29acbb",
                    "sense_definition": "Osoba, która wnosi pozew do sądu, rozpoczynając sprawę cywilną.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_person_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ponieważ <powód> przedstawił zaświadczenie lekarskie, rozprawa została przełożona.",
                            "target_lang_translations": {
                                "ru": "Так как <w>истец</w> предоставил медицинскую справку, слушание было отложено."
                            }
                        },
                        {
                            "text": "Sąd orzekł, że <powód> ma rację i przyznał mu odszkodowanie.",
                            "target_lang_translations": {
                                "ru": "Суд постановил, что <w>истец</w> прав, и присудил ему компенсацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "pozywający"
                    ],
                    "antonyms": [
                        "pozwany"
                    ],
                    "common_phrases": [
                        "strona powodowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, которое подает иск в суд, начиная гражданское дело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "истец",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9979bedb-df45-3f27-86b2-47fd7f49d90c",
                    "sense_definition": "Rzemień lub lina przymocowana do ogłowia, służąca do prowadzenia konia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "equestrian_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stajenny wyprowadził spokojnego konia na długim <powodzie>.",
                            "target_lang_translations": {
                                "ru": "Конюх вывел спокойную лошадь на длинном <w>поводе</w>."
                            }
                        },
                        {
                            "text": "Zwierzę szarpnęło głową i zerwało się z <powodu>.",
                            "target_lang_translations": {
                                "ru": "Животное дернуло головой и сорвалось с <w>повода</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wodza"
                    ],
                    "common_phrases": [
                        "prowadzić na powodzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремень или веревка, прикрепленная к уздечке, используемая для ведения лошади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повод",
                                "target_lang_sense_clarification": "ремень уздечки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "99dfe83d-ab5e-311a-b7f1-d4446ef07dc7",
                    "sense_definition": "A friendly term of address used to express familiarity, especially towards a young man.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Come on, <w>jong</w>, we are going to be late for the movie.",
                            "target_lang_translations": {
                                "ru": "Пошли, <w>дружище</w>, мы опоздаем на фильм."
                            }
                        },
                        {
                            "text": "I have not seen you in a long time, <w>jong</w>.",
                            "target_lang_translations": {
                                "ru": "Давно не виделись, <w>приятель</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "buddy",
                        "mate",
                        "pal"
                    ],
                    "common_phrases": [
                        "hey jong",
                        "listen jong"

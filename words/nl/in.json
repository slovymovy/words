                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "collect",
                                "target_lang_sense_clarification": "receive money"
                            },
                            {
                                "target_lang_word": "cash",
                                "target_lang_sense_clarification": "convert to money"
                            }
                        ]
                    }
                }
            ]

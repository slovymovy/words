    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2cd71b7e-d13f-368d-aad4-fe2123582008",
                    "sense_definition": "Денежное взыскание, налагаемое в качестве наказания за нарушение закона, правил или договора.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель заплатил крупный <штраф> за превышение скорости.",
                            "target_lang_translations": {
                                "en": "The driver paid a large <fine> for speeding."
                            }
                        },
                        {
                            "text": "За курение в общественном месте полагается административный <штраф>.",
                            "target_lang_translations": {
                                "en": "Smoking in a public place carries an administrative <fine>."
                            }
                        }
                    ],
                    "synonyms": [
                        "пеня"
                    ],
                    "antonyms": [
                        "поощрение",
                        "премия"
                    ],
                    "common_phrases": [
                        "заплатить штраф",
                        "административный штраф",
                        "выписать штраф",
                        "штраф за парковку"
                    ],
                    "target_lang_definitions": {
                        "en": "A sum of money imposed as a punishment for breaking a law, rule, or contract."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fine",
                                "target_lang_sense_clarification": "monetary punishment"
                            },
                            {
                                "target_lang_word": "penalty",
                                "target_lang_sense_clarification": "punishment for violation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14d069ea-2592-3878-a057-857b3c7f32d0",
                    "sense_definition": "Наказание за нарушение правил в спорте или игре.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Судья назначил <штраф> нашей команде за грубую игру.",
                            "target_lang_translations": {
                                "en": "The referee awarded a <penalty> against our team for rough play."
                            }
                        },
                        {
                            "text": "Игрок получил <штраф> и был вынужден покинуть поле.",
                            "target_lang_translations": {
                                "en": "The player received a <penalty> and was forced to leave the field."
                            }
                        }
                    ],
                    "common_phrases": [
                        "получить штраф"
                    ],
                    "target_lang_definitions": {
                        "en": "A punishment for violating rules in sports or games."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "penalty",
                                "target_lang_sense_clarification": "in sports"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18443cec-1cd5-39c6-bfde-473bb4c78cf2",
                    "sense_definition": "Штрафной батальон (военное подразделение для провинившихся).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его дед воевал в <штрафе> в самые тяжёлые годы войны.",
                            "target_lang_translations": {
                                "en": "His grandfather fought in a <penal battalion> during the hardest years of the war."
                            }
                        },
                        {
                            "text": "Многие солдаты искупили свою вину в <штрафе>.",
                            "target_lang_translations": {
                                "en": "Many soldiers redeemed their guilt in a <penal battalion>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "сокращение от «штрафной батальон»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A penal battalion (a military unit for soldiers who have committed offenses)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "penal battalion",
                                "target_lang_sense_clarification": "military unit"
                            },
                            {
                                "target_lang_word": "penal unit",
                                "target_lang_sense_clarification": "military"
                            }
                        ]
                    }
                }
            ]
        }
    ]

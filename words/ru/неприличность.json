    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e60d2ca8-8c25-3008-8055-a5594bf56961",
                    "sense_definition": "Свойство по значению прилагательного <w>неприличный</w>; отсутствие приличия, нарушение общепринятых норм поведения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его поведение граничило с откровенной <w>неприличностью</w>.",
                            "target_lang_translations": {
                                "en": "His behavior bordered on outright <w>indecency</w>."
                            }
                        },
                        {
                            "text": "Меня поразила <w>неприличность</w> этого вопроса в данной ситуации.",
                            "target_lang_translations": {
                                "en": "I was struck by the <w>impropriety</w> of this question in the given situation."
                            }
                        }
                    ],
                    "synonyms": [
                        "непристойность",
                        "бестактность",
                        "некорректность"
                    ],
                    "antonyms": [
                        "приличность",
                        "приличие"
                    ],
                    "common_phrases": [
                        "до неприличности",
                        "граничить с неприличностью"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of being indecent; lack of decency or violation of accepted norms of behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "indecency",
                                "target_lang_sense_clarification": "quality of being indecent"
                            },
                            {
                                "target_lang_word": "impropriety",
                                "target_lang_sense_clarification": "failure to observe standards or modesty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d055d61c-17d0-382d-89d3-414ee6329510",
                    "sense_definition": "Неприличный поступок, грубое высказывание или действие.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он позволил себе говорить разные <w>неприличности</w> в присутствии дам.",
                            "target_lang_translations": {
                                "en": "He allowed himself to speak various <w>obscenities</w> in the presence of ladies."
                            }
                        },
                        {
                            "text": "За такие <w>неприличности</w> его выгнали из класса.",
                            "target_lang_translations": {
                                "en": "He was kicked out of class for such <w>indecencies</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "непристойность",
                        "пошлость",
                        "грубость",
                        "скабрезность"
                    ],
                    "common_phrases": [
                        "говорить неприличности",
                        "писать неприличности"
                    ],
                    "target_lang_definitions": {
                        "en": "An indecent act, rude remark, or offensive action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "obscenity",
                                "target_lang_sense_clarification": "offensive word or action"
                            },
                            {
                                "target_lang_word": "vulgarity",
                                "target_lang_sense_clarification": "rude or offensive remark"
                            }
                        ]
                    }
                }
            ]
        }
    ]

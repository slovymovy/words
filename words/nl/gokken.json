            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7968bc46-7c3f-35e7-b87a-000261397066",
                    "sense_definition": "Een antwoord geven of een keuze maken zonder zeker te weten of het juist is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je het antwoord op de vraag niet weet, mag je gewoon <w>gokken</w>.",
                            "target_lang_translations": {
                                "en": "If you don't know the answer to the question, you can just <w>guess</w>.",
                                "ru": "Если ты не знаешь ответа на вопрос, можешь просто <w>попытаться угадать</w>."
                            }
                        },
                        {
                            "text": "Ik <w>gok</w> dat we er over ongeveer een half uur zullen zijn.",
                            "target_lang_translations": {
                                "en": "I <w>guess</w> that we will be there in about half an hour.",
                                "ru": "Я <w>предполагаю</w>, что мы будем там примерно через полчаса."
                            }
                        }
                    ],
                    "synonyms": [
                        "raden"
                    ],
                    "antonyms": [
                        "weten"
                    ],
                    "common_phrases": [
                        "verkeerd gokken",
                        "maar wat gokken",
                        "goed gokken"
                    ],
                    "target_lang_definitions": {
                        "en": "To give an answer or make a choice without knowing for sure if it is correct.",
                        "ru": "Давать ответ или делать выбор, не зная наверняка, правильно ли это."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "guess",
                                "target_lang_sense_clarification": "to estimate or suppose without certainty"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "угадывать",
                                "target_lang_sense_clarification": "пытаться найти правильный ответ"
                            },
                            {
                                "target_lang_word": "предполагать",
                                "target_lang_sense_clarification": "высказывать догадку или мнение"
                            },
                            {
                                "target_lang_word": "гадать",
                                "target_lang_sense_clarification": "называть варианты наугад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "911f2fd1-9981-394d-8d9e-902897cd2524",
                    "sense_definition": "Geld of iets van waarde inzetten op de uitkomst van een spel of gebeurtenis waarvan de uitslag onzeker is.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij verloor veel geld door te <w>gokken</w> in het casino.",
                            "target_lang_translations": {
                                "en": "He lost a lot of money by <w>gambling</w> in the casino.",
                                "ru": "Он потерял много денег, <w>играя в азартные игры</w> в казино."
                            }
                        },
                        {
                            "text": "Mensen <w>gokken</w> vaak op de uitslag van belangrijke voetbalwedstrijden.",
                            "target_lang_translations": {
                                "en": "People often <w>bet</w> on the result of important football matches.",
                                "ru": "Люди часто <w>делают ставки</w> на результаты важных футбольных матчей."
                            }
                        }
                    ],
                    "synonyms": [
                        "wedden",
                        "spelen"
                    ],
                    "common_phrases": [
                        "online gokken",
                        "gokken op paarden",
                        "gokken om geld"
                    ],
                    "target_lang_definitions": {
                        "en": "To stake money or something of value on the outcome of a game or event where the result is uncertain.",
                        "ru": "Ставить деньги или ценности на исход игры или события с неопределенным результатом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gamble",
                                "target_lang_sense_clarification": "to play games of chance for money"
                            },
                            {
                                "target_lang_word": "bet",
                                "target_lang_sense_clarification": "to wager money on an outcome"
                            },
                            {
                                "target_lang_word": "wager",

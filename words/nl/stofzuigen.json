    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "472fe7d7-e815-32cf-9166-62b49a2291c5",
                    "sense_definition": "Schoonmaken door stof en vuil op te zuigen met een elektrisch apparaat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat kleed is zo vies dat je het nu meteen moet <w>stofzuigen</w>.",
                            "target_lang_translations": {
                                "ru": "Этот ковер такой грязный, что его нужно немедленно <w>пропылесосить</w>."
                            }
                        },
                        {
                            "text": "Ik heb gisteren de hele benedenverdieping <w>gestofzuigd</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера я <w>пропылесосил</w> весь нижний этаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "zuigen"
                    ],
                    "common_phrases": [
                        "de kamer stofzuigen",
                        "het tapijt stofzuigen",
                        "even stofzuigen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убирать пыль и грязь, всасывая их с помощью электрического устройства (пылесоса)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пылесосить",
                                "target_lang_sense_clarification": "процесс уборки"
                            },
                            {
                                "target_lang_word": "пропылесосить",
                                "target_lang_sense_clarification": "завершить уборку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c77cde2e-67f5-3ba2-aa6e-45f72e4fc751",
                    "sense_definition": "Een terrein grondig schoonmaken door met een groep mensen op een rij te lopen en al het afval handmatig op te rapen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het zomerkamp moesten we met zijn allen het veld <w>stofzuigen</w>.",
                            "target_lang_translations": {
                                "ru": "После летнего лагеря нам всем пришлось <w>прочесывать</w> поле, собирая мусор."
                            }
                        },
                        {
                            "text": "De vrijwilligers gingen het festivalterrein <w>stofzuigen</w> om de laatste restjes afval te verwijderen.",
                            "target_lang_translations": {
                                "ru": "Волонтеры отправились <w>зачищать</w> территорию фестиваля, чтобы убрать последние остатки мусора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het terrein stofzuigen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщательно убирать территорию, выстроившись группой в ряд и собирая весь мусор вручную."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прочесывать",
                                "target_lang_sense_clarification": "местность в поисках мусора"
                            },
                            {
                                "target_lang_word": "зачищать",
                                "target_lang_sense_clarification": "территорию от мусора"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
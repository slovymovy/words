            "pos": "noun",
            "senses": [
                {
                    "sense_id": "77b4d29c-09f6-376a-8c74-35d5417f701f",
                    "sense_definition": "Sztuczny nasyp z ziemi, budowany zazwyczaj wzdłuż rzeki lub brzegu morza w celu ochrony terenów przed powodzią.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "earthwork",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkańcy układali worki z piaskiem, aby wzmocnić <wały> przeciwpowodziowe.",
                            "target_lang_translations": {
                                "ru": "Жители укладывали мешки с песком, чтобы укрепить противопаводковые <дамбы>."
                            }
                        },
                        {
                            "text": "Wysoka woda przerwała <wał> i zalała pobliskie wioski.",
                            "target_lang_translations": {
                                "ru": "Высокая вода прорвала <дамбу> и затопила близлежащие деревни."
                            }
                        }
                    ],
                    "synonyms": [
                        "nasyp"
                    ],
                    "common_phrases": [
                        "wał przeciwpowodziowy",
                        "wały ochronne",
                        "umacniać wały"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственная земляная насыпь, обычно вдоль реки или берега моря, для защиты от наводнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вал",
                                "target_lang_sense_clarification": "земляная насыпь"
                            },
                            {
                                "target_lang_word": "дамба",
                                "target_lang_sense_clarification": "гидротехническое сооружение"
                            },
                            {
                                "target_lang_word": "насыпь",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "892a8255-33c9-39e5-99d3-138e84ea05b7",
                    "sense_definition": "Element maszyny w kształcie walca, który obraca się i służy do przenoszenia momentu obrotowego lub zamiany ruchu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mechanical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mechanik stwierdził, że <wał> korbowy w silniku wymaga wymiany.",
                            "target_lang_translations": {
                                "ru": "Механик заявил, что коленчатый <вал> в двигателе требует замены."
                            }
                        },
                        {
                            "text": "Ten <wał> napędowy przekazuje moc z silnika na koła pojazdu.",
                            "target_lang_translations": {
                                "ru": "Этот приводной <вал> передает мощность от двигателя на колеса автомобиля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wał korbowy",
                        "wał napędowy",
                        "wał rozrządu"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деталь машины в форме цилиндра, которая вращается и служит для передачи крутящего момента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вал",
                                "target_lang_sense_clarification": "деталь механизма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7bbd88f-570d-3a55-be55-287f4f2a9d10",
                    "sense_definition": "Ciężka maszyna lub narzędzie w kształcie walca służące do ugniatania i wyrównywania nawierzchni, na przykład asfaltu lub ziemi.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drogowcy użyli ciężkiego <wału> do wyrównania nowej warstwy asfaltu.",
                            "target_lang_translations": {
                                "ru": "Дорожники использовали тяжелый <каток> для выравнивания нового слоя асфальта."
                            }
                        },
                        {
                            "text": "Rolnik przejechał <wałem> po polu, aby ubić ziemię po siewie.",
                            "target_lang_translations": {
                                "ru": "Фермер проехал <катком> по полю, чтобы утрамбовать землю после посева."
                            }
                        }
                    ],
                    "synonyms": [
                        "walec"
                    ],
                    "common_phrases": [
                        "wał drogowy",
                        "wał strunowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжелая машина или инструмент цилиндрической формы для уплотнения и выравнивания поверхности (асфальта, почвы)."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ce77b98e-ee1a-3859-a27f-4f3d530e6edb",
                    "sense_definition": "Meervoud van <w>maan</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "moon_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jupiter en Saturnus hebben tientallen <w>manen</w> die om hen heen draaien.",
                            "target_lang_translations": {
                                "en": "Jupiter and Saturn have dozens of <w>moons</w> orbiting them.",
                                "ru": "У Юпитера и Сатурна есть десятки <w>лун</w>, которые вращаются вокруг них."
                            }
                        },
                        {
                            "text": "In sciencefictionfilms zie je vaak planeten met twee of meer <w>manen</w>.",
                            "target_lang_translations": {
                                "en": "In science fiction movies you often see planets with two or more <w>moons</w>.",
                                "ru": "В научно-фантастических фильмах часто можно увидеть планеты с двумя или более <w>лунами</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "halve manen",
                        "nieuwe manen"
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of <w>moon</w>.",
                        "ru": "Множественное число от слова <w>maan</w> (луна)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "moons",
                                "target_lang_sense_clarification": "celestial bodies"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "луны",
                                "target_lang_sense_clarification": "небесные тела"
                            },
                            {
                                "target_lang_word": "спутники",
                                "target_lang_sense_clarification": "астрономический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "551ebe79-6d6c-3fc2-8d9c-630cfba9e4ce",
                    "sense_definition": "De lange, ruige haren in de nek van bepaalde dieren, zoals paarden en leeuwen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het zwarte paard galoppeerde met wapperende <w>manen</w> door het open veld.",
                            "target_lang_translations": {
                                "en": "The black horse galloped through the open field with flowing <w>mane</w>.",
                                "ru": "Черная лошадь скакала галопом по открытому полю с развевающейся <w>гривой</w>."
                            }
                        },
                        {
                            "text": "Een mannetjesleeuw is makkelijk te herkennen aan zijn grote en indrukwekkende <w>manen</w>.",
                            "target_lang_translations": {
                                "en": "A male lion is easily recognizable by his large and impressive <w>mane</w>.",
                                "ru": "Самца льва легко узнать по его большой и впечатляющей <w>гриве</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wapperende manen",
                        "door de manen strijken"
                    ],
                    "target_lang_definitions": {
                        "en": "The long, rough hair on the neck of certain animals, such as horses and lions.",
                        "ru": "Длинные, густые волосы на шее определенных животных, таких как лошади и львы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mane",
                                "target_lang_sense_clarification": "hair on an animal's neck"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "грива",
                                "target_lang_sense_clarification": "шерсть на шее животного"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b588432a-c10e-36a7-9adc-9ffcdbae7c82",
                    "sense_definition": "Iemand dringend aansporen of bevelen iets te doen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "urge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De burgemeester <w>maande</w> de bevolking tot kalmte na de onrustige gebeurtenissen.",
                            "target_lang_translations": {
                                "en": "The mayor <w>urged</w> the population to remain calm after the turbulent events.",
                                "ru": "Мэр <w>призвал</w> население к спокойствию после беспорядков."
                            }
                        },
                        {
                            "text": "Zij <w>maande</w> haar kinderen om voorzichtig te zijn in het drukke verkeer.",
                            "target_lang_translations": {
                                "en": "She <w>admonished</w> her children to be careful in the busy traffic.",
                                "ru": "Она <w>призвала</w> своих детей быть осторожными в оживленном потоке машин."
                            }
                        }
                    ],
                    "synonyms": [
                        "aansporen",
                        "vermanen",
                        "opdragen"
                    ],
                    "common_phrases": [
                        "tot kalmte manen",
                        "tot spoed manen",
                        "tot voorzichtigheid manen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To urgently urge or order someone to do something.",
                        "ru": "Настойчиво побуждать или приказывать кому-либо сделать что-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to urge",
                                "target_lang_sense_clarification": "to encourage strongly"
                            },
                            {
                                "target_lang_word": "to admonish",
                                "target_lang_sense_clarification": "to warn or urge seriously"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "призывать",
                                "target_lang_sense_clarification": "настойчиво побуждать"
                            },
                            {
                                "target_lang_word": "увещевать",
                                "target_lang_sense_clarification": "предостерегать или наставлять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "923a7bf9-37f8-3700-b22d-4fa024925793",
                    "sense_definition": "Dringend vragen om betaling van een openstaande schuld.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "demand_payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het incassobureau <w>maande</w> de wanbetaler om het bedrag onmiddellijk over te maken.",
                            "target_lang_translations": {
                                "en": "The collection agency <w>pressed</w> the defaulter to transfer the amount immediately.",
                                "ru": "Коллекторское агентство <w>потребовало</w> от неплательщика немедленно перевести сумму."
                            }
                        },
                        {
                            "text": "Als je de rekening niet op tijd betaalt, zullen ze je blijven <w>manen</w>.",
                            "target_lang_translations": {
                                "en": "If you don't pay the bill on time, they will keep <w>sending reminders</w>.",
                                "ru": "Если ты не оплатишь счет вовремя, они продолжат <w>требовать уплаты</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanmanen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To urgently ask for payment of an outstanding debt.",
                        "ru": "Настойчиво требовать оплаты непогашенного долга."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to dun",
                                "target_lang_sense_clarification": "to persistently demand payment"
                            },
                            {
                                "target_lang_word": "to press for payment",
                                "target_lang_sense_clarification": "to demand settlement of a debt"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "требовать уплаты",
                                "target_lang_sense_clarification": "взыскивать долг"
                            }
                        ]
                    }
                }
            ]
        },
        {

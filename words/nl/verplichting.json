    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b78322fd-038f-3762-94ee-e1bb887b5738",
                    "sense_definition": "Iets wat je moet doen vanwege een regel, wet, afspraak of fatsoen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is een wettelijke <w>verplichting</w> om in de auto een gordel te dragen.",
                            "target_lang_translations": {
                                "en": "It is a legal <w>obligation</w> to wear a seatbelt in the car."
                            }
                        },
                        {
                            "text": "Hij kon helaas niet naar het feest komen vanwege andere <w>verplichtingen</w>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, he could not come to the party due to other <w>commitments</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "plicht",
                        "taak",
                        "vereiste"
                    ],
                    "antonyms": [
                        "recht",
                        "keuze",
                        "vrijblijvendheid"
                    ],
                    "common_phrases": [
                        "verplichtingen nakomen",
                        "zonder verplichting",
                        "sociale verplichtingen"
                    ],
                    "target_lang_definitions": {
                        "en": "Something you must do due to a rule, law, agreement, or courtesy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "obligation",
                                "target_lang_sense_clarification": "general duty or requirement"
                            },
                            {
                                "target_lang_word": "commitment",
                                "target_lang_sense_clarification": "social or scheduled task"
                            },
                            {
                                "target_lang_word": "requirement",
                                "target_lang_sense_clarification": "something mandated by rule or law"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e9d106a-0f8b-3371-a20f-87e20c9451b7",
                    "sense_definition": "Een bindende toezegging of contractuele schuld die men is aangegaan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf kan door de crisis niet meer aan zijn financiële <w>verplichtingen</w> voldoen.",
                            "target_lang_translations": {
                                "en": "Due to the crisis, the company can no longer meet its financial <w>obligations</w>."
                            }
                        },
                        {
                            "text": "Door dit contract te ondertekenen ga je een langdurige <w>verplichting</w> aan.",
                            "target_lang_translations": {
                                "en": "By signing this contract, you are entering into a long-term <w>commitment</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbintenis"
                    ],
                    "common_phrases": [
                        "verplichting aangaan",
                        "financiële verplichtingen"
                    ],
                    "target_lang_definitions": {
                        "en": "A binding pledge or contractual debt that one has entered into."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "obligation",

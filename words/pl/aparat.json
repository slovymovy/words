                {
                    "sense_id": "7fc2e5f5-b400-398b-8fa8-f403fa83227f",
                    "sense_definition": "Zespół narządów w organizmie, które współpracują ze sobą w celu pełnienia określonej funkcji życiowej.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ludzki <aparat> mowy pozwala na wydawanie bardzo skomplikowanych dźwięków.",
                            "target_lang_translations": {
                                "ru": "Человеческий речевой <аппарат> позволяет издавать очень сложные звуки."
                            }

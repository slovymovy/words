                },
                {
                    "sense_id": "4ad87061-12ee-37c4-ac80-80820914ec4e",
                    "sense_definition": "One of the two symbols used in Morse code, representing a short signal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The distress signal SOS consists of three dots, three dashes, and three <w>dots</w>.",
                            "target_lang_translations": {
                                "ru": "Сигнал бедствия SOS состоит из трех точек, трех тире и трех <w>точек</w>."
                            }
                        },
                        {
                            "text": "He tapped out a <w>dot</w> on the telegraph machine.",
                            "target_lang_translations": {
                                "ru": "Он отстучал <w>точку</w> на телеграфном аппарате."
                            }
                        }
                    ],
                    "synonyms": [
                        "dit"

                    "frequency": "High",
                    "semantic_group_id": "fruit_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She peeled a ripe <w>banana</w> for her breakfast.",
                            "target_lang_translations": {
                                "ru": "Она очистила спелый <w>банан</w> на завтрак."
                            }
                        },
                        {
                            "text": "I need to buy a bunch of <w>bananas</w> at the market.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить связку <w>бананов</w> на рынке."
                            }
                        }
                    ],
                    "common_phrases": [

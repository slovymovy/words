    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5f6638de-2fe4-3964-b4d2-91eb1a3bbd71",
                    "sense_definition": "Członek grupy etnicznej Romów; osoba narodowości romskiej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ethnicity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj spotkaliśmy starego <w>Cygana</w>, który pięknie grał na skrzypcach.",
                            "target_lang_translations": {
                                "ru": "Вчера мы встретили старого <w>цыгана</w>, который красиво играл на скрипке."
                            }
                        },
                        {
                            "text": "Przez naszą wieś przejeżdżał kiedyś kolorowy tabor <w>Cyganów</w>.",
                            "target_lang_translations": {
                                "ru": "Через нашу деревню когда-то проезжал пёстрый табор <w>цыган</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Rom"
                    ],
                    "common_phrases": [
                        "tabor Cyganów",
                        "król Cyganów"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "pisane wielką literą"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член этнической группы рома; лицо цыганской национальности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цыган",
                                "target_lang_sense_clarification": "национальность"
                            },
                            {
                                "target_lang_word": "рома",
                                "target_lang_sense_clarification": "самоназвание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4fd32f9f-7ed1-327f-95ca-bd6178b9f3f8",
                    "sense_definition": "Osoba, która kłamie, oszukuje lub wprowadza innych w błąd.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wierz mu, to straszny <w>cygan</w>, który nigdy nie mówi prawdy.",
                            "target_lang_translations": {
                                "ru": "Не верь ему, это страшный <w>обманщик</w>, который никогда не говорит правды."
                            }
                        },
                        {
                            "text": "Zachował się jak zwykły <w>cygan</w>, obiecując rzeczy niemożliwe do spełnienia.",
                            "target_lang_translations": {
                                "ru": "Он повёл себя как обычный <w>плут</w>, обещая невыполнимые вещи."
                            }
                        }
                    ],
                    "synonyms": [
                        "oszust",
                        "krętacz",
                        "matacz"
                    ],
                    "common_phrases": [
                        "nie bądź cygan"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который лжёт, обманывает или вводит других в заблуждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обманщик",
                                "target_lang_sense_clarification": "лгун"
                            },
                            {
                                "target_lang_word": "плут",
                                "target_lang_sense_clarification": "хитрец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06aa2460-1421-3491-bbce-d758de07424b",
                    "sense_definition": "Osoba prowadząca wędrowny, nieustabilizowany tryb życia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "lifestyle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego brat to wieczny <w>cygan</w>, który nie potrafi usiedzieć w jednym miejscu.",
                            "target_lang_translations": {
                                "ru": "Его брат — вечный <w>бродяга</w>, который не может усидеть на одном месте."
                            }
                        },
                        {
                            "text": "Żył jak <w>cygan</w>, podróżując z plecakiem od miasta do miasta.",
                            "target_lang_translations": {
                                "ru": "Он жил как <w>цыган</w>, путешествуя с рюкзаком из города в город."
                            }
                        }
                    ],
                    "synonyms": [
                        "włóczęga",
                        "wędrowiec",
                        "włóczykij",
                        "wagabunda"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, ведущий кочевой, неустроенный образ жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бродяга",
                                "target_lang_sense_clarification": "странник"
                            },
                            {
                                "target_lang_word": "кочевник",
                                "target_lang_sense_clarification": "перен. о стиле жизни"
                            },
                            {
                                "target_lang_word": "цыган",
                                "target_lang_sense_clarification": "перен. разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "424614af-d57b-3398-99af-95ed86ac8835",
                    "sense_definition": "Mężczyzna o bardzo ciemnej karnacji i ciemnych włosach.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po powrocie z wakacji wyglądał jak prawdziwy <w>cygan</w> z tą mocną opalenizną.",
                            "target_lang_translations": {
                                "ru": "После возвращения из отпуска он выглядел как настоящий <w>цыган</w> с этим сильным загаром."
                            }
                        },
                        {
                            "text": "Ten wysoki <w>cygan</w> o czarnych oczach przyciągał wzrok wszystkich kobiet.",
                            "target_lang_translations": {
                                "ru": "Этот высокий <w>смуглян</w> с черными глазами притягивал взгляды всех женщин."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина с очень смуглой кожей и тёмными волосами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цыган",
                                "target_lang_sense_clarification": "перен. о внешности"
                            },
                            {
                                "target_lang_word": "смуглян",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02db58c8-324f-3d71-8582-8989a5673b3a",
                    "sense_definition": "Rodzaj małego pieca kuchennego, często żeliwnego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starej chacie stał żeliwny <w>cygan</w>, w którym palono drewnem.",
                            "target_lang_translations": {
                                "ru": "В старой хате стояла чугунная <w>буржуйка</w>, которую топили дровами."
                            }
                        },
                        {
                            "text": "Zimą ogrzewali się przy małym <w>cyganie</w> stojącym w rogu izby.",
                            "target_lang_translations": {
                                "ru": "Зимой они грелись у маленькой <w>буржуйки</w>, стоящей в углу избы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "często jako zdrobnienie: cyganek"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность маленькой кухонной печи, часто чугунной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буржуйка",
                                "target_lang_sense_clarification": "печь"
                            },
                            {
                                "target_lang_word": "чугунка",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a287a327-923e-3826-9125-fa92168966fe",
                    "sense_definition": "Potoczna nazwa grzyba, znanego jako siniak.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas grzybobrania znalazłem dorodnego <w>cygana</w> pod dębem.",
                            "target_lang_translations": {
                                "ru": "Во время сбора грибов я нашел крупный <w>синяк</w> под дубом."
                            }
                        },
                        {
                            "text": "Nie każdy wie, że <w>cygan</w> to jadalny i smaczny grzyb.",
                            "target_lang_translations": {
                                "ru": "Не каждый знает, что <w>синяк</w> — это съедобный и вкусный гриб."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "nazwa ludowa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Народное название гриба, известного как синяк."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синяк",
                                "target_lang_sense_clarification": "гриб"
                            },
                            {
                                "target_lang_word": "гиропор синеющий",
                                "target_lang_sense_clarification": "научное название"
                            }
                        ]
                    }
                }
            ]
        },

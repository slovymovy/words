    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "697aaebb-54a9-39a0-b1b5-0d83d4867278",
                    "sense_definition": "An affectionate term of address for one's <w>mother</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "term_of_address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have brought you a warm shawl, my dearest <w>motherkin</w>, to keep out the winter chill.",
                            "target_lang_translations": {
                                "ru": "Я принес тебе теплую шаль, моя дорогая <w>матушка</w>, чтобы уберечь тебя от зимней стужи."
                            }
                        },
                        {
                            "text": "Please tell me a story from your childhood, <w>motherkin</w>, before we go to sleep.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, расскажи мне историю из своего детства, <w>маменька</w>, перед тем как мы ляжем спать."
                            }
                        }
                    ],
                    "synonyms": [
                        "mama",
                        "mommy"
                    ],
                    "common_phrases": [
                        "dearest motherkin",
                        "sweet motherkin"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "no longer in common use"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое обращение к <w>матери</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "матушка",
                                "target_lang_sense_clarification": "устаревшее, ласковое"
                            },
                            {
                                "target_lang_word": "маменька",
                                "target_lang_sense_clarification": "устаревшее, ласковое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11b880fe-1ea2-379a-9613-0ffac3b4c32b",
                    "sense_definition": "An affectionate term of address for a woman older than the speaker.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "term_of_address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The traveler bowed respectfully to the old woman and asked, \"Can you spare some water, <w>motherkin</w>?\"",
                            "target_lang_translations": {
                                "ru": "Путник почтительно поклонился старухе и спросил: «Не найдется ли у тебя немного воды, <w>матушка</w>?»"
                            }
                        },
                        {
                            "text": "Do not worry about the heavy basket, <w>motherkin</w>, for I will carry it to the market for you.",
                            "target_lang_translations": {
                                "ru": "Не беспокойся о тяжелой корзине, <w>матушка</w>, я отнесу ее на рынок для тебя."
                            }
                        }
                    ],
                    "synonyms": [
                        "granny"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "found in older literature"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое обращение к женщине старше говорящего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "матушка",
                                "target_lang_sense_clarification": "вежливое обращение к пожилой женщине"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
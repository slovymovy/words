    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cdd8d248-ec0f-3fe4-9a2f-bf106c3af4d2",
                    "sense_definition": "Twarda, mała płytka pokrywająca ciało ryb, gadów i niektórych innych zwierząt, pełniąca funkcję ochronną.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rybak musiał usunąć każdą <w>łuskę</w> z karpia przed smażeniem.",
                            "target_lang_translations": {
                                "ru": "Рыбак должен был удалить каждую <w>чешуйку</w> с карпа перед жаркой."
                            }
                        },
                        {
                            "text": "W słońcu każda <w>łuska</w> na ciele węża mieniła się innym kolorem.",
                            "target_lang_translations": {
                                "ru": "На солнце каждая <w>чешуйка</w> на теле змеи переливалась другим цветом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rybia łuska",
                        "srebrna łuska",
                        "pokryty łuską"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердая маленькая пластинка, покрывающая тело рыб, рептилий и некоторых других животных, выполняющая защитную функцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чешуйка",
                                "target_lang_sense_clarification": "отдельная пластинка"
                            },
                            {
                                "target_lang_word": "чешуя",
                                "target_lang_sense_clarification": "покров в целом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80c7d03e-4b5b-3e7c-b521-b7129055510b",
                    "sense_definition": "Sucha, zewnętrzna osłona niektórych nasion, owoców lub cebul, chroniąca ich wnętrze.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zewnętrzna <w>łuska</w> cebuli była brązowa i łatwo odchodziła od reszty warzywa.",
                            "target_lang_translations": {
                                "ru": "Внешняя <w>шелуха</w> лука была коричневой и легко отходила от остальной части овоща."
                            }
                        },
                        {
                            "text": "Dzieci zbierały kasztany, które wypadły ze swoich kolczastych <w>łusek</w>.",
                            "target_lang_translations": {
                                "ru": "Дети собирали каштаны, которые выпали из своих колючих <w>оболочек</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "łupina"
                    ],
                    "common_phrases": [
                        "łuska cebuli",
                        "twarda łuska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сухая внешняя оболочка некоторых семян, плодов или луковиц, защищающая их внутреннюю часть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шелуха",
                                "target_lang_sense_clarification": "лука, семян"
                            },
                            {
                                "target_lang_word": "оболочка",
                                "target_lang_sense_clarification": "плодов, каштанов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4029b700-b5a0-32d9-bffd-2f48c25ba70d",
                    "sense_definition": "Metalowa część naboju w kształcie cylindra, zawierająca ładunek miotający i spłonkę.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po oddaniu strzału pusta <w>łuska</w> upadła z brzękiem na betonową podłogę.",
                            "target_lang_translations": {
                                "ru": "После выстрела пустая <w>гильза</w> со звоном упала на бетонный пол."
                            }
                        },
                        {
                            "text": "Policja znalazła na miejscu zdarzenia <w>łuskę</w> kalibru dziewięć milimetrów.",
                            "target_lang_translations": {
                                "ru": "Полиция нашла на месте происшествия <w>гильзу</w> калибра девять миллиметров."
                            }
                        }
                    ],
                    "synonyms": [
                        "gilza"
                    ],
                    "common_phrases": [
                        "pusta łuska",
                        "łuska naboju",
                        "wyrzutnik łusek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлическая часть патрона цилиндрической формы, содержащая метательный заряд и капсюль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гильза",
                                "target_lang_sense_clarification": "патрона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08e67935-0792-3d1f-bcac-bd0ffab4ae7f",
                    "sense_definition": "Fragment martwego naskórka oddzielający się od skóry, często będący objawem choroby dermatologicznej.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zauważył na skórze pacjenta charakterystyczną srebrzystą <w>łuskę</w>.",
                            "target_lang_translations": {
                                "ru": "Врач заметил на коже пациента характерную серебристую <w>чешуйку</w>."
                            }
                        },
                        {
                            "text": "Stosowanie odpowiednich maści pomaga usunąć zrogowaciałą <w>łuskę</w> z powierzchni ciała.",
                            "target_lang_translations": {
                                "ru": "Использование соответствующих мазей помогает удалить ороговевшую <w>чешуйку</w> с поверхности тела."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фрагмент омертвевшего эпидермиса, отделяющийся от кожи, часто являющийся симптомом дерматологического заболевания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чешуйка",
                                "target_lang_sense_clarification": "кожи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21b9a151-10bf-32f9-aad0-f5d72d1a2aac",
                    "sense_definition": "Metalowa płytka będąca elementem dawnej zbroi, zachodząca dachówkowato na inne płytki.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Archeolodzy wykopali fragment pancerza, w którym zachowała się jedna żelazna <w>łuska</w>.",
                            "target_lang_translations": {
                                "ru": "Археологи откопали фрагмент доспеха, в котором сохранилась одна железная <w>чешуйка</w>."
                            }
                        },
                        {
                            "text": "Zbroja karacena składała się z setek metalowych <w>łusek</w> przymocowanych do skóry.",
                            "target_lang_translations": {
                                "ru": "Доспех из карацены состоял из сотен металлических <w>чешуек</w>, прикрепленных к коже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbroja z łusek"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "refers to historical armor types"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлическая пластинка, являющаяся элементом старинных доспехов, накладывающаяся на другие пластинки подобно черепице."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чешуйка",
                                "target_lang_sense_clarification": "элемент доспеха"
                            },
                            {
                                "target_lang_word": "чешуя",
                                "target_lang_sense_clarification": "тип доспеха"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1ece19fb-0839-3427-bb06-2963435e84bc",
                    "sense_definition": "Trzecia osoba liczby pojedynczej czasu teraźniejszego od <w>łuskać</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia siedzi na ganku i powoli <w>łuska</w> fasolę na obiad.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка сидит на крыльце и медленно <w>лущит</w> фасоль на обед."
                            }
                        },
                        {
                            "text": "On zawsze <w>łuska</w> orzechy włoskie, żeby dodać je do świątecznego ciasta.",
                            "target_lang_translations": {
                                "ru": "Он всегда <w>чистит</w> грецкие орехи, чтобы добавить их в праздничный пирог."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łuska orzechy",
                        "łuska fasolę",
                        "łuska groch"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb conjugation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Третье лицо единственного числа настоящего времени от глагола 'łuskać' (очищать от шелухи/скорлупы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лущит",
                                "target_lang_sense_clarification": "горох, фасоль"
                            },
                            {
                                "target_lang_word": "чистит",
                                "target_lang_sense_clarification": "орехи, семечки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
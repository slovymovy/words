            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5ca491dc-33c4-3eae-9f8e-030598746479",
                    "sense_definition": "Kobieta, która zajęła pierwsze miejsce w zawodach sportowych i zdobyła tytuł mistrzowski.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sport_winner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza <w>mistrzyni</w> olimpijska została powitana na lotnisku przez tłumy kibiców.",
                            "target_lang_translations": {
                                "ru": "Нашу олимпийскую <w>чемпионку</w> в аэропорту встречали толпы болельщиков."
                            }
                        },
                        {
                            "text": "Polska <w>mistrzyni</w> tenisa bez trudu pokonała rywalkę w finałowym meczu.",
                            "target_lang_translations": {
                                "ru": "Польская <w>чемпионка</w> по теннису без труда победила соперницу в финальном матче."
                            }
                        }
                    ],
                    "synonyms": [
                        "czempionka",
                        "zwyciężczyni",
                        "triumfatorka"
                    ],
                    "antonyms": [
                        "przegrana"
                    ],
                    "common_phrases": [
                        "mistrzyni świata",
                        "mistrzyni olimpijska",
                        "mistrzyni Polski",
                        "aktualna mistrzyni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, занявшая первое место в спортивных соревнованиях и завоевавшая чемпионский титул."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чемпионка",
                                "target_lang_sense_clarification": "победительница в спорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "900f5271-5d0c-3ea8-8b7b-3d474abea892",
                    "sense_definition": "Kobieta, która osiągnęła najwyższy poziom umiejętności i biegłości w jakiejś dziedzinie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "expert_skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia jest prawdziwą <w>mistrzynią</w> w pieczeniu tradycyjnych ciast drożdżowych.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка — настоящая <w>мастерица</w> печь традиционные дрожжевые пироги."
                            }
                        },
                        {
                            "text": "Ona jest absolutną <w>mistrzynią</w> słowa i potrafi pięknie opowiadać historie.",
                            "target_lang_translations": {
                                "ru": "Она абсолютный <w>мастер</w> слова и умеет красиво рассказывать истории."
                            }
                        }
                    ],
                    "synonyms": [
                        "ekspertka",
                        "wirtuozka",
                        "specjalistka"
                    ],
                    "antonyms": [
                        "amatorka",
                        "nowicjuszka",
                        "dyletantka"
                    ],
                    "common_phrases": [
                        "mistrzyni kuchni",
                        "mistrzyni ceremonii",
                        "mistrzyni słowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, достигшая высочайшего уровня мастерства и умений в какой-либо области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мастерица",
                                "target_lang_sense_clarification": "искусная в каком-либо деле женщина"
                            },
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "профессионал высокого уровня"
                            },
                            {
                                "target_lang_word": "виртуоз",
                                "target_lang_sense_clarification": "о человеке блестящего мастерства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51a03f88-bc37-3e6d-bf4e-a7562ed3034f",
                    "sense_definition": "Kobieta posiadająca najwyższy stopień kwalifikacji zawodowych w rzemiośle, żeńska forma słowa <w>mistrz</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vocational_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako dyplomowana <w>mistrzyni</w> krawiectwa prowadzi własny zakład i szkoli uczennice.",
                            "target_lang_translations": {
                                "ru": "Как дипломированный <w>мастер</w> швейного дела, она руководит собственным ателье и обучает учениц."
                            }
                        },
                        {
                            "text": "Aby uzyskać tytuł <w>mistrzyni</w> fryzjerstwa, musiała zdać bardzo trudny egzamin praktyczny.",
                            "target_lang_translations": {
                                "ru": "Чтобы получить звание <w>мастера</w> парикмахерского искусства, ей пришлось сдать очень сложный практический экзамен."
                            }
                        }
                    ],
                    "antonyms": [
                        "czeladniczka",
                        "uczennica"
                    ],
                    "common_phrases": [
                        "tytuł mistrzyni",
                        "dyplom mistrzyni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, обладающая высшей профессиональной квалификацией в ремесле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "квалификационное звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce8b0568-bb01-3d1f-9b5d-f9c87542e783",
                    "sense_definition": "Kobieta kierująca zespołem pracowników i nadzorująca ich pracę w zakładzie produkcyjnym.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "supervisor_job",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa <w>mistrzyni</w> zmiany wprowadziła surowe zasady bezpieczeństwa na hali produkcyjnej.",
                            "target_lang_translations": {
                                "ru": "Новый <w>мастер</w> смены ввела строгие правила безопасности в производственном цехе."
                            }
                        },
                        {
                            "text": "Pracownicy bardzo szanują swoją <w>mistrzynię</w> za jej sprawiedliwość i fachową wiedzę.",
                            "target_lang_translations": {
                                "ru": "Работники очень уважают своего <w>мастера</w> за справедливость и профессиональные знания."
                            }
                        }
                    ],
                    "synonyms": [
                        "brygadzistka",
                        "kierowniczka"
                    ],
                    "common_phrases": [
                        "mistrzyni zmiany",
                        "starsza mistrzyni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, руководящая группой рабочих и контролирующая их работу на производстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "руководитель производственного участка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fd2d3da-e2ae-3b01-9aba-173427360786",
                    "sense_definition": "Zakonnica odpowiedzialna za opiekę nad nowicjuszkami i ich formację duchową w klasztorze.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religious_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Siostra Krystyna przez wiele lat służyła wspólnocie jako <w>mistrzyni</w> nowicjatu.",
                            "target_lang_translations": {
                                "ru": "Сестра Кристина много лет служила общине как <w>наставница</w> новициата."
                            }
                        },
                        {
                            "text": "Młode zakonnice spędzają wiele godzin na rozmowach ze swoją duchową <w>mistrzynią</w>.",
                            "target_lang_translations": {
                                "ru": "Молодые монахини проводят много часов в беседах со своей духовной <w>наставницей</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mistrzyni nowicjatu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Монахиня, ответственная за воспитание послушниц и их духовное становление в монастыре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наставница",
                                "target_lang_sense_clarification": "духовный руководитель"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
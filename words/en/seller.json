    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4dab04ce-64e0-3060-bfac-ac922029e894",
                    "sense_definition": "A person or business that sells goods or services.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vendor_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>seller</w> agreed to lower the price of the car.",
                            "target_lang_translations": {
                                "ru": "<w>Продавец</w> согласился снизить цену на машину."
                            }
                        },
                        {
                            "text": "You should contact the <w>seller</w> if the item arrives damaged.",
                            "target_lang_translations": {
                                "ru": "Вам следует связаться с <w>продавцом</w>, если товар придет поврежденным."
                            }
                        }
                    ],
                    "synonyms": [
                        "vendor",
                        "merchant",
                        "dealer",
                        "retailer",
                        "trader"
                    ],
                    "antonyms": [
                        "buyer",
                        "purchaser",
                        "customer",
                        "consumer"
                    ],
                    "common_phrases": [
                        "private seller",
                        "street seller",
                        "online seller",
                        "market seller"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или компания, занимающиеся продажей товаров или услуг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продавец",
                                "target_lang_sense_clarification": "общее название для человека или организации, продающих что-либо"
                            },
                            {
                                "target_lang_word": "торговец",
                                "target_lang_sense_clarification": "человек, занимающийся торговлей, часто на рынке или как частный предприниматель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4f7d9bd-424b-3e43-bcfc-be1dd8350502",
                    "sense_definition": "A product considered in terms of how well or how much it is sold.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "product_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This new novel is expected to be a big <w>seller</w> this year.",
                            "target_lang_translations": {
                                "ru": "Ожидается, что этот новый роман станет <w>хитом продаж</w> в этом году."
                            }
                        },
                        {
                            "text": "The toy became a huge <w>seller</w> during the holiday season.",
                            "target_lang_translations": {
                                "ru": "Эта игрушка стала очень <w>ходовым товаром</w> в праздничный сезон."
                            }
                        }
                    ],
                    "synonyms": [
                        "product",
                        "item"
                    ],
                    "common_phrases": [
                        "best seller",
                        "big seller",
                        "poor seller"
                    ],
                    "target_lang_definitions": {
                        "ru": "Товар, оцениваемый с точки зрения успеха его продаж."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хит продаж",
                                "target_lang_sense_clarification": "о товаре, который очень хорошо продается"
                            },
                            {
                                "target_lang_word": "ходовой товар",
                                "target_lang_sense_clarification": "товар, пользующийся большим спросом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf31f2d3-9247-33ba-8300-f963f07975da",
                    "sense_definition": "Obsolete spelling of <w>cellar</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the old manuscript, the wine was stored in a dark <w>seller</w>.",
                            "target_lang_translations": {
                                "ru": "В старой рукописи вино хранилось в темном <w>подвале</w>."
                            }
                        },
                        {
                            "text": "He went down to the <w>seller</w> to fetch some supplies.",
                            "target_lang_translations": {
                                "ru": "Он спустился в <w>подвал</w>, чтобы принести припасы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": "spelling"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее написание слова cellar (подвал)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвал",
                                "target_lang_sense_clarification": "помещение ниже уровня земли"
                            }
                        ]
                    }
                }
            ]
        },

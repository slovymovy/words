{
    "word_family": [
        "stropowy",
        "stropodach",
        "stropnica"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "19e972e5-bd7d-324f-8448-6a99e31d6fd9",
                    "sense_definition": "Poziomy element konstrukcyjny budynku oddzielający poszczególne kondygnacje.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "construction_element",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy muszą wylać betonowy <w>strop</w> przed nadejściem zimy.",
                            "target_lang_translations": {
                                "ru": "Рабочие должны залить бетонное <w>перекрытие</w> до наступления зимы."
                            }
                        },
                        {
                            "text": "Stary, drewniany <w>strop</w> skrzypiał głośno pod każdym krokiem domowników.",
                            "target_lang_translations": {
                                "ru": "Старое деревянное <w>перекрытие</w> громко скрипело под каждым шагом жильцов."
                            }
                        }
                    ],
                    "antonyms": [
                        "podłoga"
                    ],
                    "common_phrases": [
                        "strop żelbetowy",
                        "strop drewniany",
                        "wylać strop",
                        "gęstożebrowy strop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горизонтальный конструктивный элемент здания, разделяющий этажи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перекрытие",
                                "target_lang_sense_clarification": "строительство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cda236c3-8c3f-38af-90a3-1fb7da209f97",
                    "sense_definition": "Górna powierzchnia ograniczająca wyrobisko górnicze, jaskinię lub warstwę skalną.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geology_mining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Górnicy dokładnie zabezpieczyli <w>strop</w> tunelu, aby uniknąć zawału.",
                            "target_lang_translations": {
                                "ru": "Шахтеры тщательно укрепили <w>кровлю</w> туннеля, чтобы избежать обвала."
                            }
                        },
                        {
                            "text": "Niski <w>strop</w> jaskini zmuszał turystów do ciągłego schylania głowy.",
                            "target_lang_translations": {
                                "ru": "Низкий <w>свод</w> пещеры заставлял туристов постоянно пригибать голову."
                            }
                        }
                    ],
                    "antonyms": [
                        "spąg"
                    ],
                    "common_phrases": [
                        "strop jaskini",
                        "strop wyrobiska",
                        "niebezpieczny strop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя поверхность горной выработки, пещеры или геологического слоя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кровля",
                                "target_lang_sense_clarification": "горное дело"
                            },
                            {
                                "target_lang_word": "свод",
                                "target_lang_sense_clarification": "пещеры"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "84823000-f734-30a9-a07d-7446a59c10dc",
                    "sense_definition": "Druga osoba liczby pojedynczej trybu rozkazującego od czasownika <w>stropić</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <w>strop</w> się tymi uwagami, one mają ci tylko pomóc w nauce.",
                            "target_lang_translations": {
                                "ru": "Не <w>смущайся</w> из-за этих замечаний, они должны лишь помочь тебе в учебе."
                            }
                        },
                        {
                            "text": "Proszę, nie <w>strop</w> go swoim surowym spojrzeniem na samym początku.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>смути</w> его своим суровым взглядом в самом начале."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма повелительного наклонения глагола «stropić» (смутить, озадачить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смути",
                                "target_lang_sense_clarification": "привести в замешательство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
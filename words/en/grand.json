                },
                {
                    "sense_id": "12c32b21-5b9d-3d65-b720-22a68391195c",
                    "sense_definition": "Standing in the second degree of parentage or descent.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "family_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is my <w>grand</w> uncle on my mother's side of the family.",
                            "target_lang_translations": {
                                "ru": "Он мой <w>двоюродный</w> дедушка по материнской линии."
                            }
                        },
                        {
                            "text": "The family gathered to celebrate the birth of the first <w>grand</w> nephew.",
                            "target_lang_translations": {
                                "ru": "Семья собралась, чтобы отпраздновать рождение первого <w>внучатого</w> племянника."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает родство через одно поколение (обычно используется как часть составных слов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внучатый",
                                "target_lang_sense_clarification": "о племянниках"
                            },
                            {
                                "target_lang_word": "двоюродный",
                                "target_lang_sense_clarification": "о дедушках/бабушках (в значении grand uncle)"
                            }
                        ]
                    }
                }
            ]
        },

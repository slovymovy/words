    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7ad55497-e1cc-3b94-9b30-9f717e34ebe4",
                    "sense_definition": "В настольных играх: фигурка или небольшой предмет, обозначающий положение игрока на игровом поле.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Игрок бросает кубик и передвигает свою <w>фишку</w> на три клетки вперёд."
                        },
                        {
                            "text": "В коробке с игрой не хватало одной красной <w>фишки</w>."
                        }
                    ],
                    "synonyms": [
                        "фигу́ра"
                    ],
                    "common_phrases": [
                        "двигать фишку",
                        "цветные фишки",
                        "расставить фишки"
                    ]
                },
                {
                    "sense_id": "86c42c02-2b7e-3324-912b-7970cb46764a",
                    "sense_definition": "В азартных играх: круглая пластинка или жетон, заменяющий деньги.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он поставил последнюю <w>фишку</w> на чёрное и выиграл."
                        },
                        {
                            "text": "В кассе казино можно обменять деньги на игровые <w>фишки</w>."
                        }
                    ],
                    "synonyms": [
                        "жетон"
                    ],
                    "common_phrases": [
                        "играть на фишки",
                        "гора фишек",
                        "покерные фишки"
                    ]
                },
                {
                    "sense_id": "a9cbedb1-1a02-3e5b-bb1b-aa24d12fd00b",
                    "sense_definition": "Характерная особенность, уникальная черта, «изюминка» или суть чего-либо.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "slang_feature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Главная <w>фишка</w> этого ресторана — открытая кухня, где видно работу поваров."
                        },
                        {
                            "text": "Я долго не мог понять, в чём <w>фишка</w> этой новой компьютерной программы."
                        }
                    ],
                    "synonyms": [
                        "осо́бенность",
                        "черта́",
                        "фи́ча",
                        "суть",
                        "достоинство"
                    ],
                    "common_phrases": [
                        "в чём фишка",
                        "главная фишка",
                        "фишка сезона"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "very common in modern speech"
                        }
                    ]
                },
                {
                    "sense_id": "eae33fd3-94ad-3a34-9ea3-aee5fdf68799",
                    "sense_definition": "Оригинальная идея, хитрая выдумка, неожиданный поступок или шутка.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "slang_feature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Они придумали новую рекламную <w>фишку</w> для привлечения молодых клиентов."
                        },
                        {
                            "text": "У него есть одна забавная <w>фишка</w>, которой он всегда смешит друзей."
                        }
                    ],
                    "synonyms": [
                        "вы́думка",
                        "прикол",
                        "затея",
                        "штука",
                        "выходка"
                    ],
                    "common_phrases": [
                        "прикольная фишка",
                        "новая фишка",
                        "выкинуть фишку"
                    ],
                    "traits": [

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поставленный в зависимость",
                                "target_lang_sense_clarification": "подчиненный чему-либо"
                            },
                            {
                                "target_lang_word": "обусловленный",

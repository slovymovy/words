            "senses": [
                {
                    "sense_id": "a883e43a-89d5-3e53-8b4b-612e695aa1b8",
                    "sense_definition": "Выражает сильное отвращение, презрение, раздражение или досаду.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бестолково, плохо, а вид-то, посмотри, какой, походка одна чего стоит: <w>тьфу</w>!"
                        },
                        {
                            "text": "Опять я забыл дома телефон, <w>тьфу</w> ты, какая незадача!"
                        }
                    ],
                    "synonyms": [
                        "фу",
                        "тьфу ты"
                    ],
                    "common_phrases": [
                        "тьфу ты",
                        "тьфу на вас",
                        "тьфу пропасть"
                    ]
                },
                {
                    "sense_id": "67bef29d-d13f-3b30-98a2-2739919d6276",
                    "sense_definition": "Обозначает звук, издаваемый при плевании.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он сдвинул брови, втянул носом воздух и, <w>тьфу</w>, смачно плюнул себе под ноги."
                        },
                        {
                            "text": "Чтобы не сглазить удачу, постучи по дереву и скажи «<w>тьфу</w>-<w>тьфу</w>-<w>тьфу</w>»."
                        }
                    ],
                    "common_phrases": [
                        "тьфу-тьфу-тьфу"
                    ]
                },
                {
                    "sense_id": "daf78b7c-e47b-3f50-b85a-a3c3584f0aca",
                    "sense_definition": "Употребляется для характеристики чего-либо незначительного, пустякового, не стоящего внимания.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Между седлом и твоими ногами расстояние чепуховое: <w>тьфу</w>, а не расстояние."
                        },
                        {
                            "text": "Для такого опытного мастера эта работа — <w>тьфу</w>, за час сделает."
                        }
                    ],
                    "common_phrases": [
                        "это мне тьфу"
                    ],

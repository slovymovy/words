            "senses": [
                {
                    "sense_id": "ef08a61b-0390-3014-94d2-23a49cdc7fce",
                    "sense_definition": "zonder enige waarde; niets waard",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "no_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze oude biljetten zijn nu <w>waardeloos</w> geworden door de inflatie.",
                            "target_lang_translations": {
                                "ru": "Эти старые банкноты теперь стали <w>ничего не стоящими</w> из-за инфляции.",
                                "en": "These old banknotes have now become <w>worthless</w> due to inflation."
                            }
                        },
                        {
                            "text": "Het kapotte horloge was helaas compleet <w>waardeloos</w> en niet meer te repareren.",
                            "target_lang_translations": {
                                "ru": "Сломанные часы, к сожалению, были совершенно <w>бесполезными</w> и не подлежали ремонту.",
                                "en": "The broken watch was unfortunately completely <w>worthless</w> and beyond repair."
                            }
                        }
                    ],
                    "synonyms": [
                        "nutteloos"
                    ],
                    "antonyms": [
                        "waardevol"
                    ],
                    "common_phrases": [
                        "financieel waardeloos",
                        "totaal waardeloos"
                    ],
                    "target_lang_definitions": {
                        "ru": "не имеющий никакой ценности; ничего не стоящий",
                        "en": "having no value; worth nothing"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничего не стоящий",
                                "target_lang_sense_clarification": "о материальной ценности"
                            },
                            {
                                "target_lang_word": "бесполезный",
                                "target_lang_sense_clarification": "непригодный к использованию"
                            },
                            {
                                "target_lang_word": "обесценившийся",
                                "target_lang_sense_clarification": "потерявший стоимость"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "worthless",
                                "target_lang_sense_clarification": "having no financial or material value"
                            },
                            {
                                "target_lang_word": "valueless",
                                "target_lang_sense_clarification": "lacking monetary worth"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fda3131-a562-3dce-92b2-6a371b4c6f38",
                    "sense_definition": "van zeer slechte kwaliteit; erg slecht of teleurstellend",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "bad_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat een <w>waardeloze</w> vertoning was dat, zeg!",
                            "target_lang_translations": {
                                "ru": "Ну и <w>никудышное</w> же это было представление!",
                                "en": "What a <w>lousy</w> performance that was!"
                            }
                        },
                        {
                            "text": "Hij voelde zich <w>waardeloos</w> omdat hij zijn belofte had gebroken.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя <w>никчемным</w>, так как нарушил свое обещание.",
                                "en": "He felt <w>worthless</w> because he had broken his promise."
                            }
                        }
                    ],
                    "synonyms": [
                        "slecht",
                        "belabberd"
                    ],
                    "common_phrases": [
                        "een waardeloos excuus",
                        "zich waardeloos voelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "очень плохого качества; ужасный или разочаровывающий",
                        "en": "of very poor quality; very bad or disappointing"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никудышный",
                                "target_lang_sense_clarification": "о качестве"

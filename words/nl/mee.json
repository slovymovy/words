                        "helemaal mee zijn"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "vooral in België"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Able to follow or understand; up to date.",
                        "ru": "Быть в состоянии следить за чем-то или понимать; быть в курсе дела."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "following",
                                "target_lang_sense_clarification": "understanding an explanation or situation"
                            },
                            {
                                "target_lang_word": "with it",
                                "target_lang_sense_clarification": "understanding what is happening"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "улавливать суть"
                            },
                            {
                                "target_lang_word": "быть в курсе",
                                "target_lang_sense_clarification": "быть осведомленным"
                            },
                            {
                                "target_lang_word": "успевать",
                                "target_lang_sense_clarification": "следить за мыслью"
                            }
                        ]
                    }
                }
            ]
        },

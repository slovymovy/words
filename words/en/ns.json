                {
                    "sense_id": "17007687-4495-33b6-a2dc-ed07e87d79ff",
                    "sense_definition": "Abbreviation of no show; a person who makes a reservation, buys a ticket, or schedules an appointment but does not arrive.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "no_show_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The airline did not refund the ticket because the passenger was a <ns>.",
                            "target_lang_translations": {
                                "ru": "Авиакомпания не вернула деньги за билет, так как пассажир <w>не явился на рейс</w>."
                            }
                        },
                        {
                            "text": "We had three <ns> guests at the dinner party last night.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером на званом ужине у нас было трое <w>не пришедших</w> гостей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "marked as ns",
                        "ns fee"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"no show\"; человек, который забронировал место, купил билет или назначил встречу, но не пришел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неявка",
                                "target_lang_sense_clarification": "факт отсутствия"
                            },
                            {
                                "target_lang_word": "неявившийся",
                                "target_lang_sense_clarification": "о человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "654cd18d-0a98-3799-ac8c-6e88fe716b68",
                    "sense_definition": "Abbreviation of namespace; a container that provides a context for identifiers (names, or technical terms) to avoid naming conflicts.",

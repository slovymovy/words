    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8063ad80-510f-3c85-ab70-a1412baf5bcc",
                    "sense_definition": "Мучительное сомнение в чьей-либо верности и любви, подозрение в измене или в большей привязанности к кому-то другому.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она устроила мужу громкую сцену <ревности> из-за позднего звонка.",
                            "target_lang_translations": {
                                "en": "She made a loud scene of <jealousy> to her husband because of a late phone call."
                            }
                        },
                        {
                            "text": "Его мучила <ревность>, когда он видел её в компании других мужчин.",
                            "target_lang_translations": {
                                "en": "He was tormented by <jealousy> when he saw her in the company of other men."
                            }
                        }
                    ],
                    "antonyms": [
                        "доверие"
                    ],
                    "common_phrases": [
                        "сцена ревности",
                        "чувство ревности",
                        "слепая ревность",
                        "сгорать от ревности",
                        "повод для ревности"
                    ],
                    "target_lang_definitions": {
                        "en": "Painful doubt regarding someone's fidelity or love; suspicion of cheating or fear of being replaced."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jealousy",
                                "target_lang_sense_clarification": "suspicion of unfaithfulness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21aeb4d4-2bb9-3000-aefe-24e78b652a00",
                    "sense_definition": "Зависть к успеху другого человека, нежелание делить что-либо или кого-либо с другими.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Между известными художниками возникла профессиональная <ревность>.",
                            "target_lang_translations": {
                                "en": "Professional <jealousy> arose between the famous artists."
                            }
                        },
                        {
                            "text": "Детская <ревность> часто проявляется при появлении в семье младшего брата или сестры.",
                            "target_lang_translations": {
                                "en": "Childhood <jealousy> often manifests when a younger brother or sister appears in the family."
                            }
                        }
                    ],
                    "synonyms": [
                        "зависть"
                    ],
                    "common_phrases": [
                        "профессиональная ревность",
                        "детская ревность"
                    ],
                    "target_lang_definitions": {
                        "en": "Envy of another's success; unwillingness to share someone or something with others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jealousy",
                                "target_lang_sense_clarification": "possessiveness or rivalry"
                            },
                            {
                                "target_lang_word": "envy",
                                "target_lang_sense_clarification": "resentment of success"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02c21832-744d-3c94-973d-c50771e68c1e",
                    "sense_definition": "Усердие, сильное рвение и старание при выполнении какого-либо дела.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "qualities",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодой офицер с особой <ревностью> исполнял все приказы генерала.",
                            "target_lang_translations": {
                                "en": "The young officer executed all the general's orders with particular <zeal>."
                            }
                        },
                        {
                            "text": "Он взялся за восстановление старого храма с великой <ревностью>.",
                            "target_lang_translations": {
                                "en": "He undertook the restoration of the old temple with great <zeal>."
                            }
                        }
                    ],
                    "synonyms": [
                        "усердие",
                        "рвение",
                        "старание"
                    ],
                    "common_phrases": [
                        "с ревностью",
                        "религиозная ревность"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'усердие' or 'рвение'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Diligence, strong eagerness, and effort in performing a task (archaic)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "zeal",
                                "target_lang_sense_clarification": "diligent enthusiasm"
                            },
                            {
                                "target_lang_word": "fervency",
                                "target_lang_sense_clarification": "intense diligence"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
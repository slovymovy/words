    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "366ce953-b2f5-31ac-9583-d24945757dae",
                    "sense_definition": "Połączyć końce sznurka, liny lub innej rzeczy, tworząc węzeł, albo przymocować coś za pomocą węzła.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_binding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <związać> włosy w kucyk, żeby nie wpadały mi do oczu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <завязать> волосы в хвост, чтобы они не лезли мне в глаза."
                            }
                        },
                        {
                            "text": "Czy możesz mi pomóc <związać> ten pęknięty sznurek?",
                            "target_lang_translations": {
                                "ru": "Ты можешь помочь мне <связать> эту порванную веревку?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zawiązać",
                        "spiąć"
                    ],
                    "antonyms": [
                        "rozwiązać"
                    ],
                    "common_phrases": [
                        "związać włosy",
                        "związać sznurowadła",
                        "związać koniec z końcem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединить концы веревки, шнура или чего-либо другого узлом, либо скрепить что-то с помощью узла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "соединить концы"
                            },
                            {
                                "target_lang_word": "завязать",
                                "target_lang_sense_clarification": "сделать узел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc1dd685-a405-3203-ab0d-bb9e08cd57c6",
                    "sense_definition": "Ograniczyć swobodę ruchów kogoś lub czegoś poprzez mocne owinięcie i zaciśnięcie liny lub sznura.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_binding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policjanci musieli <związać> ręce agresywnego napastnika.",
                            "target_lang_translations": {
                                "ru": "Полицейским пришлось <связать> руки агрессивному нападавшему."
                            }
                        },
                        {
                            "text": "Porywacze postanowili <związać> zakładnika i zostawić go w piwnicy.",
                            "target_lang_translations": {
                                "ru": "Похитители решили <связать> заложника и оставить его в подвале."
                            }
                        }
                    ],
                    "synonyms": [
                        "spętać",
                        "skrępować",
                        "unieruchomić"
                    ],
                    "antonyms": [
                        "uwolnić",
                        "oswobodzić"
                    ],
                    "common_phrases": [
                        "związać ręce",
                        "związać nogi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограничить свободу движений кого-либо или чего-либо, крепко обмотав и затянув веревкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "обездвижить веревкой"
                            },
                            {
                                "target_lang_word": "скрутить",
                                "target_lang_sense_clarification": "силой обездвижить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e64d5589-e907-31b0-8047-66f6bc0bddda",
                    "sense_definition": "Połączyć różne elementy, fakty lub sprawy w jedną całość lub dostrzec między nimi logiczną zależność.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trudno jednoznacznie <związać> te dwa odległe wydarzenia historyczne.",
                            "target_lang_translations": {
                                "ru": "Трудно однозначно <связать> эти два далеких исторических события."
                            }
                        },
                        {
                            "text": "Autor potrafi mistrzowsko <związać> wątki w swojej najnowszej powieści.",
                            "target_lang_translations": {
                                "ru": "Автор умеет мастерски <связать> сюжетные линии в своем новейшем романе."
                            }
                        }
                    ],
                    "synonyms": [
                        "połączyć",
                        "powiązać",
                        "skojarzyć"
                    ],
                    "antonyms": [
                        "rozdzielić"
                    ],
                    "common_phrases": [
                        "ściśle związać",
                        "nierozerwalnie związać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединить различные элементы, факты или дела в единое целое или найти между ними логическую связь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "соединить логически"
                            },
                            {
                                "target_lang_word": "увязать",
                                "target_lang_sense_clarification": "согласовать факты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b4fee5e-8b37-33ad-acec-90fb9da936b2",
                    "sense_definition": "Nawiązać z kimś trwałą relację osobistą, zawodową lub formalną; często używane w formie zwrotnej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ona planuje <związać> swoją przyszłość zawodową z tą firmą.",
                            "target_lang_translations": {
                                "ru": "Она планирует <связать> свое профессиональное будущее с этой компанией."
                            }
                        },
                        {
                            "text": "Młodzi ludzie postanowili <związać> się węzłem małżeńskim w przyszłym roku.",
                            "target_lang_translations": {
                                "ru": "Молодые люди решили <связать> себя узами брака в следующем году."
                            }
                        }
                    ],
                    "synonyms": [
                        "złączyć",
                        "zjednoczyć"
                    ],
                    "antonyms": [
                        "rozstać się",
                        "zerwać"
                    ],
                    "common_phrases": [
                        "związać się z kimś",
                        "związać swoje losy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установить с кем-либо прочные личные, профессиональные или официальные отношения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "соединить жизнь или судьбу"
                            }
                        ]
                    }
                }
            ]
        }
    ]

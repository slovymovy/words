                {
                    "sense_id": "278c7f5d-35f1-3b1e-bd41-9dcaa2ca260e",
                    "sense_definition": "Nuchter en feitelijk; niet geleid door emoties of persoonlijke voorkeuren.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij benaderde deze moeilijke kwestie op een uiterst <w>zakelijke</w> wijze.",

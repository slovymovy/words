    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b67fb261-5b4f-3a13-8785-ddc433346a61",
                    "sense_definition": "A small body; historically used in oaths referring to the body of Christ.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old playwright used the word <w>bodikins</w> to add historical flavor to the dialogue.",
                            "target_lang_translations": {
                                "ru": "Старый драматург использовал слово <w>тело Господне</w>, чтобы придать диалогу исторический колорит."
                            }
                        },
                        {
                            "text": "He exclaimed ods <w>bodikins</w> when he realized he had forgotten his lines.",
                            "target_lang_translations": {
                                "ru": "Он воскликнул «клянусь <w>телом Господним</w>», когда понял, что забыл слова."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ods bodikins"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькое тело; исторически использовалось в клятвах, относящихся к телу Христа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тельце",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное от «тело»"
                            },
                            {
                                "target_lang_word": "тело Господне",
                                "target_lang_sense_clarification": "в составе устаревших клятв"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21b85554-35ca-31b1-a386-3c6c8a55f5ad",
                    "sense_definition": "An alternative form of <w>bodkin</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She used a sharp <w>bodikin</w> to pierce the thick fabric while sewing.",
                            "target_lang_translations": {
                                "ru": "Она использовала острое <w>шило</w>, чтобы проткнуть толстую ткань во время шитья."
                            }
                        },
                        {
                            "text": "The antique text described a weapon similar to a <w>bodikin</w>.",
                            "target_lang_translations": {
                                "ru": "В старинном тексте описывалось оружие, похожее на <w>кинжал</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bodkin"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "archaic spelling"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Альтернативная форма слова «bodkin» (шило, кинжал или длинная игла)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шило",
                                "target_lang_sense_clarification": "инструмент"
                            },
                            {
                                "target_lang_word": "кинжал",
                                "target_lang_sense_clarification": "оружие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

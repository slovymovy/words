                },
                {
                    "sense_id": "ef060836-3115-3014-8170-23bf56e4cfe8",
                    "sense_definition": "Zachować dane cyfrowe w pamięci komputera lub na innym nośniku.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "preserving_info",
                    "name_type": "no",
                    "examples": [

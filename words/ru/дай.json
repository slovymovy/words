    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "228b3d1b-25aa-3cde-83b1-a68b5a04fa0f",
                    "sense_definition": "Форма повелительного наклонения глагола <w>дать</w> (единственное число); просьба передать, вручить или предоставить что-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "request_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Дай</w> мне, пожалуйста, твою ручку на минуту.",
                            "target_lang_translations": {
                                "en": "Please <w>give</w> me your pen for a minute."
                            }
                        },
                        {
                            "text": "Если увидишь новости, сразу <w>дай</w> знать.",
                            "target_lang_translations": {
                                "en": "If you see the news, <w>let</w> me know immediately."
                            }
                        }
                    ],
                    "synonyms": [
                        "подай",
                        "вручи",
                        "предоставь"
                    ],
                    "antonyms": [
                        "возьми",
                        "бери",
                        "забери"
                    ],
                    "common_phrases": [
                        "дай знать",
                        "дай бог",
                        "дай пять",
                        "дай сюда"
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative form of the verb <w>give</w> (singular); a request to transfer, hand over, or provide something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "transfer possession or provide"
                            },
                            {
                                "target_lang_word": "hand",
                                "target_lang_sense_clarification": "pass an object physically"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5309f14e-0a28-3ff2-bcaf-0ec3b4bfa111",
                    "sense_definition": "Употребляется в значении «позволь», «разреши» для выражения намерения совершить действие (часто с местоимением «я»).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "request_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Дай</w> я сам попробую решить эту задачу.",
                            "target_lang_translations": {
                                "en": "<w>Let</w> me try to solve this task myself."
                            }
                        },
                        {
                            "text": "Пожалуйста, отойди и <w>дай</w> пройти пассажирам.",
                            "target_lang_translations": {
                                "en": "Please step aside and <w>let</w> the passengers pass."
                            }
                        }
                    ],
                    "synonyms": [
                        "позволь",
                        "разреши",
                        "пусти"
                    ],
                    "common_phrases": [
                        "дай я",
                        "дай-ка",
                        "дай пройти",
                        "дай сказать"
                    ],
                    "target_lang_definitions": {
                        "en": "Used in the sense of \"allow\" or \"permit\" to express an intention to perform an action (often with the pronoun \"I\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let",
                                "target_lang_sense_clarification": "allow or permit an action"
                            }
                        ]
                    }
                }
            ]
        },
        {

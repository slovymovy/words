    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ed110aa2-c90d-33d6-8028-9623b33ca09c",
                    "sense_definition": "A specific variety of taste, especially of food, candy, or drink.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "taste_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chocolate is my favorite <w>flavor</w> of ice cream.",
                            "target_lang_translations": {
                                "ru": "Шоколад — мой любимый <w>вкус</w> мороженого."
                            }
                        },
                        {
                            "text": "The store sells gum in almost every fruit <w>flavor</w>.",
                            "target_lang_translations": {
                                "ru": "В магазине продается жвачка почти с любым фруктовым <w>вкусом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "variety",
                        "kind"
                    ],
                    "common_phrases": [
                        "favorite flavor",
                        "ice cream flavor",
                        "fruit flavor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность вкуса, особенно еды, сладостей или напитков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкус",
                                "target_lang_sense_clarification": "ощущение вкуса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8d8dd7b-97af-3b1f-a503-5534ebb54e55",
                    "sense_definition": "The particular quality of a food or drink that is perceived by the tongue and nose.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "taste_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fresh herbs can add a wonderful <w>flavor</w> to the soup.",
                            "target_lang_translations": {
                                "ru": "Свежая зелень может придать супу чудесный <w>вкус</w>."
                            }
                        },
                        {
                            "text": "I do not like the spicy <w>flavor</w> of this sauce.",
                            "target_lang_translations": {
                                "ru": "Мне не нравится острый <w>вкус</w> этого соуса."
                            }
                        }
                    ],
                    "synonyms": [
                        "taste",
                        "savour"
                    ],
                    "common_phrases": [
                        "strong flavor",
                        "rich flavor",
                        "mild flavor",
                        "distinct flavor"
                    ],
                    "target_lang_definitions": {

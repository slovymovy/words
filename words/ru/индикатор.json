    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "95b6dfe9-bae9-35ce-8476-a74f793c2be8",
                    "sense_definition": "Прибор, устройство или элемент (например, лампочка или дисплей), который отображает ход процесса или состояние наблюдаемого объекта.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На приборной панели автомобиля внезапно загорелся красный <w>индикатор</w>.",
                            "target_lang_translations": {
                                "en": "A red <w>indicator</w> suddenly lit up on the car's dashboard."
                            }
                        },
                        {
                            "text": "Зелёный <w>индикатор</w> на корпусе означает, что батарея полностью заряжена.",
                            "target_lang_translations": {
                                "en": "The green <w>indicator</w> on the case means that the battery is fully charged."
                            }
                        }
                    ],
                    "synonyms": [
                        "датчик",
                        "указатель",
                        "прибор"
                    ],
                    "common_phrases": [
                        "световой индикатор",
                        "индикатор питания",
                        "индикатор уровня",
                        "индикатор заряда"
                    ],
                    "target_lang_definitions": {
                        "en": "A device, light, or display that shows the status, progress, or condition of a system or object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "indicator",
                                "target_lang_sense_clarification": "general device or signal"
                            },
                            {
                                "target_lang_word": "light",
                                "target_lang_sense_clarification": "illuminated signal"
                            },
                            {
                                "target_lang_word": "gauge",
                                "target_lang_sense_clarification": "measuring instrument"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1adc3770-cdb2-37da-a7ef-af2d8476f75a",
                    "sense_definition": "Числовой или качественный показатель, позволяющий судить о состоянии, развитии или изменении ситуации в экономике, социологии и других сферах.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Уровень безработицы — это важнейший экономический <w>индикатор</w> для любой страны.",
                            "target_lang_translations": {
                                "en": "The unemployment rate is a crucial economic <w>indicator</w> for any country."
                            }
                        },
                        {
                            "text": "Мы используем этот <w>индикатор</w> эффективности, чтобы оценить результаты работы отдела.",
                            "target_lang_translations": {
                                "en": "We use this performance <w>indicator</w> to evaluate the department's results."
                            }
                        }
                    ],
                    "synonyms": [
                        "показатель",
                        "индекс",
                        "критерий"
                    ],
                    "common_phrases": [
                        "экономический индикатор",
                        "ключевой индикатор",
                        "индикатор успеха",
                        "социальный индикатор"
                    ],
                    "target_lang_definitions": {
                        "en": "A statistic, value, or metric used to evaluate the state or trends of a situation, especially in economics or sociology."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "indicator",
                                "target_lang_sense_clarification": "measure or metric"
                            },
                            {
                                "target_lang_word": "measure",
                                "target_lang_sense_clarification": "standard or metric"
                            },
                            {
                                "target_lang_word": "index",
                                "target_lang_sense_clarification": "statistical value"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a7f0cc6-271a-3c7c-a723-0c432b626387",
                    "sense_definition": "Химическое вещество, которое меняет цвет или другие свойства в зависимости от состава среды (например, при изменении кислотности).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На уроке химии мы добавили в раствор <w>индикатор</w>, и жидкость мгновенно посинела.",
                            "target_lang_translations": {
                                "en": "In chemistry class, we added an <w>indicator</w> to the solution, and the liquid instantly turned blue."
                            }
                        },
                        {
                            "text": "Лакмусовая бумага служит простым и надёжным <w>индикатором</w> для определения кислот.",
                            "target_lang_translations": {
                                "en": "Litmus paper serves as a simple and reliable <w>indicator</w> for detecting acids."
                            }
                        }
                    ],
                    "synonyms": [
                        "реактив"
                    ],
                    "common_phrases": [
                        "химический индикатор",
                        "кислотно-основной индикатор",
                        "природный индикатор"
                    ],
                    "target_lang_definitions": {
                        "en": "A chemical substance that changes color or properties to show the presence or concentration of a specific substance."
                    },
                    "translations": {

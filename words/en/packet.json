{
    "word_family": [
        "pack",
        "package",
        "packaging",
        "packer",
        "unpack"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e8bd2c2-b472-35f9-9c70-8a6cb6689a3f",
                    "sense_definition": "A small container or envelope, usually made of paper or cardboard, holding a specific quantity of something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a <w>packet</w> of biscuits for the guests.",
                            "target_lang_translations": {
                                "ru": "Она купила <w>пачку</w> печенья для гостей."
                            }
                        },
                        {
                            "text": "Don't throw the empty crisp <w>packet</w> on the ground.",
                            "target_lang_translations": {
                                "ru": "Не бросай пустой <w>пакет</w> из-под чипсов на землю."
                            }
                        }
                    ],
                    "synonyms": [
                        "package"
                    ],
                    "common_phrases": [
                        "packet of crisps",
                        "packet of cigarettes",
                        "packet of sugar",
                        "packet of seeds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая упаковка или конверт, обычно из бумаги или картона, содержащий определенное количество чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пачка",
                                "target_lang_sense_clarification": "коробка или плотная упаковка (сигарет, печенья)"
                            },
                            {
                                "target_lang_word": "пакет",
                                "target_lang_sense_clarification": "мягкая упаковка (чипсов, семян)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb5217b5-8d0e-3a41-aecb-8d178fb3217a",
                    "sense_definition": "A small unit of data transmitted over a computer network.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The file is split into small <w>packets</w> before being sent over the internet.",
                            "target_lang_translations": {
                                "ru": "Файл разбивается на небольшие <w>пакеты</w> перед отправкой через интернет."
                            }
                        },
                        {
                            "text": "We are experiencing high <w>packet</w> loss on the network.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдаем большую потерю <w>пакетов</w> в сети."
                            }
                        }
                    ],
                    "common_phrases": [
                        "data packet",
                        "packet loss",
                        "packet switching"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой блок данных, передаваемый по компьютерной сети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пакет",
                                "target_lang_sense_clarification": "блок данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89a337ec-8ede-37f3-b7f1-7869360ed0fe",
                    "sense_definition": "A large amount of money.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It will cost a <w>packet</w> to repair the damage to the car.",
                            "target_lang_translations": {
                                "ru": "Ремонт повреждений автомобиля будет стоить <w>кучу денег</w>."
                            }
                        },
                        {
                            "text": "He made a <w>packet</w> by selling his company.",
                            "target_lang_translations": {
                                "ru": "Он сколотил <w>состояние</w>, продав свою компанию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cost a packet",
                        "make a packet",
                        "spend a packet"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая сумма денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куча денег",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "состояние",
                                "target_lang_sense_clarification": "богатство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "482b64cd-2aef-3bc6-b05f-3e3d222f7976",
                    "sense_definition": "A boat that transports mail, cargo, and passengers on a fixed schedule.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The steam <w>packet</w> arrived at the harbour early in the morning.",
                            "target_lang_translations": {
                                "ru": "Паровой <w>пакетбот</w> прибыл в гавань рано утром."
                            }
                        },
                        {
                            "text": "Travel by <w>packet</w> was the fastest way to cross the channel in those days.",
                            "target_lang_translations": {
                                "ru": "Путешествие на <w>почтовом судне</w> было самым быстрым способом пересечь пролив в те дни."
                            }
                        }
                    ],
                    "common_phrases": [
                        "packet boat",
                        "steam packet"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Судно, перевозящее почту, грузы и пассажиров по фиксированному расписанию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пакетбот",
                                "target_lang_sense_clarification": "историческое"
                            },
                            {
                                "target_lang_word": "почтовое судно",
                                "target_lang_sense_clarification": "транспорт"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7355d617-1aeb-343f-a488-30e0d8aaadaf",
                    "sense_definition": "To wrap or put something into a packet or bundle.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The machine is designed to <w>packet</w> tea leaves into bags.",
                            "target_lang_translations": {
                                "ru": "Машина предназначена для того, чтобы <w>фасовать</w> чайные листья в пакетики."
                            }
                        },
                        {
                            "text": "Workers <w>packeted</w> the goods for shipment.",
                            "target_lang_translations": {
                                "ru": "Рабочие <w>упаковывали</w> товары для отправки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заворачивать или помещать что-либо в пакет или связку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фасовать",
                                "target_lang_sense_clarification": "распределять по упаковкам"
                            },
                            {
                                "target_lang_word": "упаковывать",
                                "target_lang_sense_clarification": "класть в упаковку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "857be5a3-5b72-3c93-83d2-c0c23808edc4",
                    "sense_definition": "To attack a computer or server by sending a large number of data packets.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computing_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The server crashed after being <w>packeted</w> by hackers.",
                            "target_lang_translations": {
                                "ru": "Сервер рухнул после того, как его <w>зафлудили</w> хакеры."
                            }
                        },
                        {
                            "text": "He claimed his internet connection was being <w>packeted</w>.",
                            "target_lang_translations": {
                                "ru": "Он утверждал, что его интернет-соединение <w>атаковали пакетами</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Атаковать компьютер или сервер путем отправки большого количества пакетов данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "флудить",
                                "target_lang_sense_clarification": "компьютерный сленг"
                            },
                            {
                                "target_lang_word": "атаковать пакетами",
                                "target_lang_sense_clarification": "кибератака"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
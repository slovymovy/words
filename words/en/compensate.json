{
    "word_family": [
        "compensation",
        "compensatory",
        "compensator",
        "compensable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "255cc2a5-9758-3424-81f1-43db71c02e6c",
                    "sense_definition": "To provide something good to balance or reduce the bad effect of damage, loss, or a mistake; to make up for something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "balance_effect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His enthusiasm helps <w>compensate</w> for his lack of experience.",
                            "target_lang_translations": {
                                "ru": "Его энтузиазм помогает <w>компенсировать</w> недостаток опыта."
                            }
                        },
                        {
                            "text": "Nothing can fully <w>compensate</w> for the loss of a loved one.",
                            "target_lang_translations": {
                                "ru": "Ничто не может полностью <w>возместить</w> потерю близкого человека."
                            }
                        }
                    ],
                    "synonyms": [
                        "make up for",
                        "offset",
                        "counterbalance",
                        "redress",
                        "make good"
                    ],
                    "common_phrases": [
                        "compensate for the loss",
                        "compensate for a lack",
                        "compensate for the mistake"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставить что-то хорошее, чтобы сбалансировать или уменьшить негативный эффект от ущерба, потери или ошибки; восполнить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компенсировать",
                                "target_lang_sense_clarification": "сглаживать негативный эффект"
                            },
                            {
                                "target_lang_word": "возмещать",
                                "target_lang_sense_clarification": "убытки или потери"
                            },
                            {
                                "target_lang_word": "уравновешивать",
                                "target_lang_sense_clarification": "балансировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81465564-aa4a-3642-81ca-5f0df7b8af39",
                    "sense_definition": "To pay someone money in exchange for work done or because they have suffered injury or loss.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company promised to <w>compensate</w> the workers for their overtime.",
                            "target_lang_translations": {
                                "ru": "Компания пообещала <w>компенсировать</w> работникам сверхурочные часы."
                            }
                        },
                        {
                            "text": "Victims of the accident were <w>compensated</w> for their injuries.",
                            "target_lang_translations": {
                                "ru": "Пострадавшим в аварии <w>возместили</w> ущерб за полученные травмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "reimburse",
                        "remunerate",
                        "pay",
                        "indemnify",
                        "recompense"
                    ],
                    "common_phrases": [
                        "compensate victims",
                        "compensate employees",
                        "compensate for damages"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выплатить кому-то деньги в обмен на выполненную работу или в качестве возмещения за понесенный ущерб или травму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компенсировать",
                                "target_lang_sense_clarification": "возмещать расходы или ущерб"
                            },
                            {
                                "target_lang_word": "возмещать",
                                "target_lang_sense_clarification": "финансово"
                            },
                            {
                                "target_lang_word": "заплатить",
                                "target_lang_sense_clarification": "оплатить труд или услуги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ff9c42b-d3b1-39ce-939a-bc1e429b8df4",
                    "sense_definition": "To make an adjustment or take action to correct a problem or adapt to a new situation.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "balance_effect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot had to <w>compensate</w> for the strong wind while landing.",
                            "target_lang_translations": {
                                "ru": "Пилоту пришлось <w>компенсировать</w> сильный ветер при посадке."
                            }
                        },
                        {
                            "text": "Our eyes naturally <w>compensate</w> for changes in light levels.",
                            "target_lang_translations": {
                                "ru": "Наши глаза естественным образом <w>компенсируют</w> изменения уровня освещенности."
                            }
                        }
                    ],
                    "synonyms": [
                        "adapt",
                        "adjust"
                    ],
                    "common_phrases": [
                        "compensate for the error",
                        "compensate for the drift"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать поправку или предпринять действия для исправления проблемы или адаптации к новой ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компенсировать",
                                "target_lang_sense_clarification": "корректировать воздействие"
                            },
                            {
                                "target_lang_word": "сбалансировать",
                                "target_lang_sense_clarification": "привести в равновесие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "пустой",
        "пустеть",
        "опустошать",
        "пустыня",
        "пустошь",
        "пустышка"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0ff1e63e-06e2-32a0-b85f-8155f6bcd8a6",
                    "sense_definition": "Ничем не занятое, свободное пространство.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Космонавт вышел из корабля в чёрную <пустоту>.",
                            "target_lang_translations": {
                                "en": "The cosmonaut stepped out of the ship into the black <void>."
                            }
                        },
                        {
                            "text": "Вокруг нас была только темнота и бесконечная <пустота>.",
                            "target_lang_translations": {
                                "en": "Around us was only darkness and infinite <emptiness>."
                            }
                        }
                    ],
                    "synonyms": [
                        "вакуум"
                    ],
                    "antonyms": [
                        "заполненность"
                    ],
                    "common_phrases": [
                        "смотреть в пустоту",
                        "шагнуть в пустоту",
                        "голос в пустоту"
                    ],
                    "target_lang_definitions": {
                        "en": "An unoccupied, free space; a vacuum."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "void",
                                "target_lang_sense_clarification": "vast empty space"
                            },
                            {
                                "target_lang_word": "emptiness",
                                "target_lang_sense_clarification": "state of containing nothing"
                            },
                            {
                                "target_lang_word": "vacuum",
                                "target_lang_sense_clarification": "space devoid of matter"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b84e1f2d-8ce2-3310-8326-400c50d68b2f",
                    "sense_definition": "Отсутствие содержания, людей или предметов где-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После отъезда гостей в доме воцарилась тишина и <пустота>.",
                            "target_lang_translations": {
                                "en": "After the guests left, silence and <emptiness> reigned in the house."
                            }
                        },
                        {
                            "text": "Меня пугала <пустота> этих заброшенных комнат.",
                            "target_lang_translations": {
                                "en": "The <emptiness> of these abandoned rooms frightened me."
                            }
                        }
                    ],
                    "antonyms": [
                        "наполненность"
                    ],
                    "common_phrases": [
                        "звенящая пустота",
                        "пустота в карманах"
                    ],
                    "target_lang_definitions": {
                        "en": "The absence of content, people, or objects in a place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "emptiness",
                                "target_lang_sense_clarification": "absence of people or things"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aafc55c0-cdc6-3a63-8877-13f68d15373f",
                    "sense_definition": "Душевная опустошённость, отсутствие мыслей, чувств, интересов или идеалов.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После тяжёлой потери он ощущал внутри лишь холодную <пустоту>.",
                            "target_lang_translations": {
                                "en": "After a heavy loss, he felt only a cold <emptiness> inside."
                            }
                        },
                        {
                            "text": "За внешним блеском и богатством скрывалась духовная <пустота>.",
                            "target_lang_translations": {
                                "en": "Behind the external gloss and wealth hid a spiritual <void>."
                            }
                        }
                    ],
                    "antonyms": [
                        "наполненность"
                    ],
                    "common_phrases": [
                        "душевная пустота",
                        "внутренняя пустота",
                        "чувствовать пустоту"
                    ],
                    "target_lang_definitions": {
                        "en": "Spiritual desolation; a lack of thoughts, feelings, or interests."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "emptiness",
                                "target_lang_sense_clarification": "internal feeling"
                            },
                            {
                                "target_lang_word": "void",
                                "target_lang_sense_clarification": "feeling of loss or lack"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
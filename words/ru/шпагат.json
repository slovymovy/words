            "senses": [
                {
                    "sense_id": "296b0cce-a443-3554-b8f4-11fa3710b634",
                    "sense_definition": "Тонкая прочная нить или верёвка, используемая для упаковки, перевязывания или сшивания чего-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужен <шпагат>, чтобы крепко перевязать эту картонную коробку.",
                            "target_lang_translations": {
                                "en": "I need <w>twine</w> to tie this cardboard box tightly."
                            }
                        },
                        {
                            "text": "В хозяйственном магазине он купил большой моток прочного <шпагата>.",
                            "target_lang_translations": {
                                "en": "He bought a large ball of strong <w>twine</w> at the hardware store."
                            }
                        }
                    ],
                    "synonyms": [
                        "бечёвка"
                    ],
                    "common_phrases": [
                        "завязать шпагатом",
                        "моток шпагата",
                        "прочный шпагат"
                    ],
                    "target_lang_definitions": {
                        "en": "A thin, strong thread or cord used for packing, tying, or sewing something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "twine",
                                "target_lang_sense_clarification": "cord for tying"
                            },
                            {
                                "target_lang_word": "string",
                                "target_lang_sense_clarification": "thin rope"
                            }
                        ]
                    }
                },
                {

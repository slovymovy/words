                    "antonyms": [
                        "full-fat"
                    ],
                    "common_phrases": [
                        "skinny latte",
                        "skinny cappuccino",
                        "skinny milk"
                    ],
                    "target_lang_definitions": {
                        "ru": "О еде или напитках: содержащий меньше калорий или жира, чем обычная версия."

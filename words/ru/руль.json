            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a5222e8a-b072-3087-a667-af804879aacf",
                    "sense_definition": "Устройство, с помощью которого водитель управляет направлением движения автомобиля, велосипеда или мотоцикла.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vehicle_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец разрешил мне немного покрутить <w>руль</w> в машине.",
                            "target_lang_translations": {
                                "en": "Father let me turn the <w>steering wheel</w> a bit in the car."
                            }
                        },
                        {
                            "text": "Держи <w>руль</w> прямо и смотри на дорогу.",
                            "target_lang_translations": {
                                "en": "Keep the <w>wheel</w> straight and look at the road."
                            }
                        }
                    ],
                    "synonyms": [
                        "баранка"
                    ],
                    "common_phrases": [
                        "сесть за руль",
                        "быть за рулём",
                        "крутить руль",
                        "правый руль"
                    ],
                    "target_lang_definitions": {
                        "en": "A device used by a driver to control the direction of a car, bicycle, or motorcycle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steering wheel",
                                "target_lang_sense_clarification": "for cars and trucks"
                            },
                            {
                                "target_lang_word": "handlebars",
                                "target_lang_sense_clarification": "for bicycles and motorcycles"
                            },
                            {
                                "target_lang_word": "wheel",
                                "target_lang_sense_clarification": "short for steering wheel"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1c90790-f9bf-3b01-9992-c70493634055",
                    "sense_definition": "Механизм или устройство на судах и летательных аппаратах для изменения направления движения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vehicle_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Капитан приказал повернуть <w>руль</w> лево на борт.",
                            "target_lang_translations": {
                                "en": "The captain ordered to turn the <w>rudder</w> hard to port."
                            }
                        },
                        {
                            "text": "Пилот проверил <w>руль</w> высоты перед взлётом.",
                            "target_lang_translations": {
                                "en": "The pilot checked the <w>elevator</w> before takeoff."
                            }
                        }
                    ],
                    "common_phrases": [
                        "руль высоты",
                        "руль направления",
                        "положить руль"
                    ],
                    "target_lang_definitions": {
                        "en": "A mechanism or device on ships and aircraft for changing the direction of movement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rudder",
                                "target_lang_sense_clarification": "nautical and aviation"
                            },
                            {
                                "target_lang_word": "helm",
                                "target_lang_sense_clarification": "steering gear of a ship"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2a0aece-66f1-3154-afc9-611b92e34f7c",
                    "sense_definition": "Руководящее положение, власть или контроль над ходом дел (в переносном смысле).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый директор встал у <w>руля</w> компании в сложный период.",
                            "target_lang_translations": {
                                "en": "The new director took the <w>helm</w> of the company during a difficult period."
                            }
                        },
                        {
                            "text": "Эта политическая партия находится у <w>руля</w> уже десять лет.",
                            "target_lang_translations": {
                                "en": "This political party has been at the <w>helm</w> for ten years."
                            }
                        }
                    ],
                    "common_phrases": [
                        "стоять у руля",
                        "быть у руля",
                        "прийти к рулю"

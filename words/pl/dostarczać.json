    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3893a6a6-1b00-355a-a3b9-95fc4e452465",
                    "sense_definition": "Przekazywać coś odbiorcy, transportować przedmiot do miejsca przeznaczenia lub zaopatrywać kogoś w potrzebne środki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "delivery_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kurier obiecał <dostarczyć> paczkę jeszcze dzisiaj po południu.",
                            "target_lang_translations": {
                                "ru": "Курьер обещал <w>доставить</w> посылку еще сегодня после обеда."
                            }
                        },
                        {
                            "text": "Firma cateringowa codziennie <dostarcza> świeże posiłki do biura.",
                            "target_lang_translations": {
                                "ru": "Кейтеринговая компания ежедневно <w>доставляет</w> свежие блюда в офис."
                            }
                        }
                    ],
                    "synonyms": [
                        "doręczać",
                        "dowozić",
                        "zaopatrywać",
                        "przywozić",
                        "przynosić",
                        "zasilać"
                    ],
                    "antonyms": [
                        "odbierać",
                        "zabierać"
                    ],
                    "common_phrases": [
                        "dostarczać towar",
                        "dostarczać przesyłkę",
                        "dostarczać żywność",
                        "dostarczać energię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать что-либо получателю, транспортировать предмет к месту назначения или снабжать кого-либо необходимыми средствами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доставлять",
                                "target_lang_sense_clarification": "привозить к месту назначения"
                            },
                            {
                                "target_lang_word": "поставлять",
                                "target_lang_sense_clarification": "снабжать товарами или ресурсами"
                            },
                            {
                                "target_lang_word": "снабжать",
                                "target_lang_sense_clarification": "обеспечивать чем-либо необходимым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b71a7cd-362c-300d-8635-1f7bf40593b6",
                    "sense_definition": "Być źródłem czegoś niematerialnego, sprawiać, że ktoś doświadcza określonych stanów, emocji lub otrzymuje informacje.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "delivery_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten emocjonujący mecz <dostarczył> kibicom wielu niezapomnianych wrażeń.",
                            "target_lang_translations": {
                                "ru": "Этот захватывающий матч <w>доставил</w> болельщикам массу незабываемых впечатлений."
                            }
                        },
                        {
                            "text": "Świadek <dostarczył> policji kluczowych dowodów w tej trudnej sprawie.",
                            "target_lang_translations": {
                                "ru": "Свидетель <w>предоставил</w> полиции ключевые доказательства по этому сложному делу."
                            }
                        }
                    ],
                    "synonyms": [
                        "dawać",
                        "udzielać",
                        "przekazywać",
                        "oferować",
                        "przynosić"
                    ],
                    "common_phrases": [
                        "dostarczać informacji",
                        "dostarczać rozrywki",
                        "dostarczać dowodów",
                        "dostarczać emocji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть источником чего-либо нематериального, вызывать у кого-либо определенные состояния, эмоции или передавать информацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доставлять",
                                "target_lang_sense_clarification": "вызывать чувства, эмоции или удовольствие"
                            },
                            {
                                "target_lang_word": "предоставлять",
                                "target_lang_sense_clarification": "давать информацию или доказательства"
                            },
                            {
                                "target_lang_word": "приносить",
                                "target_lang_sense_clarification": "давать результат или ощущения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

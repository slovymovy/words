                {
                    "sense_id": "f3a38cab-5f84-31f9-859c-d31c77cba83f",
                    "sense_definition": "De uiting van creativiteit en verbeelding in werken die gewaardeerd worden om hun schoonheid of betekenis.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "art_creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het museum kijken we graag naar moderne <w>kunst</w> uit de twintigste eeuw.",
                            "target_lang_translations": {
                                "ru": "В музее нам нравится смотреть на современное <w>искусство</w> двадцатого века."
                            }
                        },
                        {
                            "text": "Zij studeert geschiedenis van de <w>kunst</w> aan de universiteit in Amsterdam.",
                            "target_lang_translations": {
                                "ru": "Она изучает историю <w>искусств</w> в университете Амстердама."
                            }
                        }
                    ],
                    "common_phrases": [
                        "moderne kunst",
                        "kunst en cultuur",
                        "schone kunsten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение творчества и воображения в произведениях, которые ценятся за их красоту или смысл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искусство",
                                "target_lang_sense_clarification": "творчество, художественная деятельность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3ebd9ba-7b21-30aa-ad92-5b9754151142",
                    "sense_definition": "Een bijzondere vaardigheid of het vermogen om iets te doen wat niet iedereen kan.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "skill_ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is een hele <w>kunst</w> om iedereen in de groep tevreden te houden.",
                            "target_lang_translations": {
                                "ru": "Это целое <w>искусство</w> — сделать так, чтобы все в группе были довольны."
                            }
                        },
                        {
                            "text": "Hij verstaat de <w>kunst</w> van het luisteren als geen ander.",
                            "target_lang_translations": {
                                "ru": "Он владеет <w>искусством</w> слушать как никто другой."
                            }
                        }
                    ],
                    "synonyms": [
                        "vaardigheid",
                        "bekwaamheid"
                    ],
                    "common_phrases": [
                        "een hele kunst",
                        "de kunst verstaan",
                        "de kunst van het"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особый навык или способность делать то, что не каждому под силу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искусство",
                                "target_lang_sense_clarification": "мастерство, умение"
                            },
                            {
                                "target_lang_word": "мастерство",
                                "target_lang_sense_clarification": "высокий уровень навыка"
                            }
                        ]
                    }
                }
            ]
        }

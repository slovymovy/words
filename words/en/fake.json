{
    "word_family": [
        "faker",
        "fakery"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "043a2735-0904-3ff1-9393-57d510b87ee2",
                    "sense_definition": "Not real or genuine; made to look like something else, often to deceive.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "not_real",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was wearing a <w>fake</w> fur coat to the party.",
                            "target_lang_translations": {
                                "ru": "Она надела на вечеринку шубу из <w>искусственного</w> меха."
                            }
                        },
                        {
                            "text": "The police discovered that the suspect was using a <w>fake</w> passport.",
                            "target_lang_translations": {
                                "ru": "Полиция обнаружила, что подозреваемый использовал <w>поддельный</w> паспорт."
                            }
                        }
                    ],
                    "synonyms": [
                        "false",
                        "artificial",
                        "counterfeit",
                        "bogus",
                        "faux",
                        "forged",
                        "fraudulent",
                        "phoney",
                        "sham",
                        "synthetic"
                    ],
                    "antonyms": [
                        "authentic",
                        "genuine",
                        "real"
                    ],
                    "common_phrases": [
                        "fake news",
                        "fake ID",
                        "fake fur",
                        "fake tan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ненастоящий или поддельный; сделанный так, чтобы выглядеть как что-то другое, часто с целью обмана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддельный",
                                "target_lang_sense_clarification": "о документах, деньгах или товарах"
                            },
                            {
                                "target_lang_word": "ненастоящий",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "фальшивый",
                                "target_lang_sense_clarification": "ложный, обманный"
                            },
                            {
                                "target_lang_word": "искусственный",
                                "target_lang_sense_clarification": "о материалах (мех, кожа)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5725968e-a86d-3f8b-b3f1-05a33cb1cd77",
                    "sense_definition": "Not expressing genuine feelings or character; insincere.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "insincere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I could tell by his <w>fake</w> smile that he was not actually happy to see us.",
                            "target_lang_translations": {
                                "ru": "Я понял по его <w>фальшивой</w> улыбке, что он на самом деле не рад нас видеть."
                            }
                        },
                        {
                            "text": "She is tired of dealing with <w>fake</w> people who only pretend to be her friends.",
                            "target_lang_translations": {
                                "ru": "Она устала иметь дело с <w>фальшивыми</w> людьми, которые только притворяются её друзьями."
                            }
                        }
                    ],
                    "synonyms": [
                        "insincere",
                        "artificial",
                        "phoney"
                    ],
                    "antonyms": [
                        "genuine",
                        "real"
                    ],
                    "common_phrases": [
                        "fake smile",
                        "fake laugh"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не выражающий искренних чувств или характера; неискренний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фальшивый",
                                "target_lang_sense_clarification": "об улыбке, эмоциях"
                            },
                            {
                                "target_lang_word": "наигранный",
                                "target_lang_sense_clarification": "неестественный, притворный"
                            },
                            {
                                "target_lang_word": "ненастоящий",
                                "target_lang_sense_clarification": "о людях или чувствах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8c0df8a8-143b-3773-85ac-00874d8e9b1b",
                    "sense_definition": "To pretend to have a feeling, illness, or condition that one does not actually have.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pretend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to <w>fake</w> a stomach ache so he wouldn't have to go to school.",
                            "target_lang_translations": {
                                "ru": "Он попытался <w>симулировать</w> боль в животе, чтобы не ходить в школу."
                            }
                        },
                        {
                            "text": "You cannot just <w>fake</w> enthusiasm if you do not truly feel it.",
                            "target_lang_translations": {
                                "ru": "Нельзя просто <w>изображать</w> энтузиазм, если ты его на самом деле не чувствуешь."
                            }
                        }
                    ],
                    "synonyms": [
                        "feign",
                        "pretend",
                        "simulate",
                        "sham"
                    ],
                    "common_phrases": [
                        "fake it",
                        "fake a smile",
                        "fake illness",
                        "fake death"
                    ],
                    "target_lang_definitions": {
                        "ru": "Притворяться, что имеешь какое-то чувство, болезнь или состояние, которого на самом деле нет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "симулировать",
                                "target_lang_sense_clarification": "болезнь или состояние"
                            },
                            {
                                "target_lang_word": "притворяться",
                                "target_lang_sense_clarification": "делать вид"
                            },
                            {
                                "target_lang_word": "изображать",
                                "target_lang_sense_clarification": "разыгрывать чувство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11b5bdd3-8930-31a1-8a19-2656c1008447",
                    "sense_definition": "To make an illegal copy of something in order to deceive people.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "forge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The criminal tried to <w>fake</w> the artist's signature on the painting.",
                            "target_lang_translations": {
                                "ru": "Преступник попытался <w>подделать</w> подпись художника на картине."
                            }
                        },
                        {
                            "text": "It is very difficult to <w>fake</w> modern banknotes because of their security features.",
                            "target_lang_translations": {
                                "ru": "Очень сложно <w>подделывать</w> современные банкноты из-за их защитных признаков."
                            }
                        }
                    ],
                    "synonyms": [
                        "forge",
                        "counterfeit",
                        "falsify"
                    ],
                    "common_phrases": [
                        "fake a signature",
                        "fake documents"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изготавливать незаконную копию чего-либо с целью обмана людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подделывать",
                                "target_lang_sense_clarification": "документы, подписи, деньги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc7ca2d4-e9b4-3f45-981b-cfcdecf740cb",
                    "sense_definition": "To improvise a musical part, speech, or performance without preparation.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "improvise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I did not know the song well, so I had to <w>fake</w> the piano accompaniment.",
                            "target_lang_translations": {
                                "ru": "Я плохо знал песню, поэтому мне пришлось <w>импровизировать</w> фортепианный аккомпанемент."
                            }
                        },
                        {
                            "text": "He forgot his lines during the play and had to <w>fake</w> the rest of the scene.",
                            "target_lang_translations": {
                                "ru": "Он забыл свои слова во время пьесы, и ему пришлось <w>импровизировать</w> остаток сцены."
                            }
                        }
                    ],
                    "synonyms": [
                        "improvise",
                        "wing it"
                    ],
                    "common_phrases": [
                        "fake it"
                    ],
                    "target_lang_definitions": {
                        "ru": "Импровизировать музыкальную партию, речь или выступление без подготовки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "импровизировать",
                                "target_lang_sense_clarification": "в музыке или на сцене"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "858d4741-e95e-34ea-a25a-5913d0b0d4dc",
                    "sense_definition": "To coil a rope or line in layers to prevent it from twisting.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_coil",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailor began to <w>fake</w> the line on the deck so it would run out smoothly.",
                            "target_lang_translations": {
                                "ru": "Моряк начал <w>бухтовать</w> трос на палубе, чтобы он разматывался плавно."
                            }
                        },
                        {
                            "text": "You must <w>fake</w> the heavy cable in a figure-eight pattern.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>укладывать</w> тяжелый кабель восьмеркой."
                            }
                        }
                    ],
                    "synonyms": [
                        "coil"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "nautical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Укладывать веревку или трос слоями (бухтами), чтобы предотвратить скручивание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бухтовать",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "980d8bb3-6ea3-307f-9ac4-8f3b3f54b9c4",
                    "sense_definition": "An object or person that is not genuine or real.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "not_real_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The expert confirmed that the expensive painting was actually a <w>fake</w>.",
                            "target_lang_translations": {
                                "ru": "Эксперт подтвердил, что дорогая картина на самом деле была <w>подделкой</w>."
                            }
                        },
                        {
                            "text": "I suspect this designer handbag is a <w>fake</w> because the stitching is poor.",
                            "target_lang_translations": {
                                "ru": "Я подозреваю, что эта дизайнерская сумочка — <w>подделка</w>, потому что у неё плохая строчка."
                            }
                        }
                    ],
                    "synonyms": [
                        "forgery",
                        "counterfeit",
                        "imitation",
                        "sham",
                        "phoney"
                    ],
                    "antonyms": [
                        "authentic",
                        "genuine",
                        "real"
                    ],
                    "common_phrases": [
                        "spot a fake",
                        "cheap fake"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет или человек, который не является подлинным или настоящим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подделка",
                                "target_lang_sense_clarification": "о вещах"
                            },
                            {
                                "target_lang_word": "фальшивка",
                                "target_lang_sense_clarification": "о документах или предметах"
                            },
                            {
                                "target_lang_word": "фейк",
                                "target_lang_sense_clarification": "сленг: что-то ненастоящее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8c9371d-6b63-3a85-aa5b-da5327227939",
                    "sense_definition": "A body movement used in sports to deceive an opponent about one's intended direction or action.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports_move",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With a quick <w>fake</w> to the left, the player dribbled past the defender.",
                            "target_lang_translations": {
                                "ru": "Сделав быстрый <w>финт</w> влево, игрок обвел защитника."
                            }
                        },
                        {
                            "text": "The quarterback's <w>fake</w> completely fooled the opposing team.",
                            "target_lang_translations": {
                                "ru": "<w>Обманное движение</w> квотербека полностью одурачило команду противника."
                            }
                        }
                    ],
                    "synonyms": [
                        "feint",
                        "deke"
                    ],
                    "common_phrases": [
                        "pump fake",
                        "head fake"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Движение тела в спорте, используемое для обмана противника относительно намеренного направления или действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "финт",
                                "target_lang_sense_clarification": "обманный прием"
                            },
                            {
                                "target_lang_word": "обманное движение",
                                "target_lang_sense_clarification": "в спорте"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
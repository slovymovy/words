            "senses": [
                {
                    "sense_id": "1a77ddb6-7097-3249-8113-9501a36b09d3",
                    "sense_definition": "Een mondelinge presentatie, lezing of toespraak over een bepaald onderwerp voor een publiek.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "speech_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De professor hield een interessante <w>voordracht</w> over de geschiedenis van de stad.",
                            "target_lang_translations": {
                                "en": "The professor gave an interesting <w>lecture</w> on the history of the city.",
                                "ru": "Профессор сделал интересный <w>доклад</w> об истории города."
                            }
                        },
                        {
                            "text": "Na de <w>voordracht</w> mocht het publiek vragen stellen aan de spreker.",
                            "target_lang_translations": {
                                "en": "After the <w>presentation</w>, the audience was allowed to ask questions to the speaker.",
                                "ru": "После <w>выступления</w> публика могла задать вопросы оратору."
                            }
                        }
                    ],
                    "synonyms": [
                        "lezing",
                        "presentatie",
                        "toespraak",
                        "referaat"
                    ],
                    "common_phrases": [
                        "een voordracht houden"
                    ],
                    "target_lang_definitions": {
                        "en": "An oral presentation, lecture, or speech about a specific subject for an audience.",
                        "ru": "Устное выступление, лекция или доклад на определенную тему перед аудиторией."
                    },
                    "translations": {
                        "en": [
                            {

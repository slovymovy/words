            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9f4ce148-e245-36d7-8d69-e03c303277c2",
                    "sense_definition": "Een symbool of teken dat een richting aanwijst of ergens de aandacht op vestigt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volg de groene <w>pijl</w> om bij de nooduitgang te komen.",
                            "target_lang_translations": {
                                "en": "Follow the green <w>arrow</w> to get to the emergency exit.",
                                "ru": "Следуйте за зеленой <w>стрелкой</w>, чтобы добраться до аварийного выхода."
                            }
                        },
                        {
                            "text": "Op het verkeersbord wees de <w>pijl</w> naar rechts voor de omleiding.",
                            "target_lang_translations": {
                                "en": "On the traffic sign, the <w>arrow</w> pointed to the right for the diversion.",
                                "ru": "На дорожном знаке <w>стрелка</w> указывала направо для объезда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de pijl volgen"
                    ],
                    "target_lang_definitions": {
                        "en": "A symbol or sign that indicates a direction or draws attention to something.",
                        "ru": "Символ или знак, указывающий направление или привлекающий к чему-либо внимание."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arrow",
                                "target_lang_sense_clarification": "symbol/sign"
                            }
                        ],
                        "ru": [

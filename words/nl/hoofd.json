            "senses": [
                {
                    "sense_id": "72f1fa4d-1036-36be-9e1d-85b881f99069",
                    "sense_definition": "Het bovenste deel van het menselijk lichaam, waar de hersenen, ogen, oren, neus en mond zitten.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij stootte zijn <w>hoofd</w> hard tegen de lage deuropening.",
                            "target_lang_translations": {
                                "ru": "Он сильно ударился <w>головой</w> о низкий дверной проем."
                            }
                        },
                        {
                            "text": "Ik moet dit gedicht voor morgen helemaal uit mijn <w>hoofd</w> leren.",
                            "target_lang_translations": {
                                "ru": "Я должен выучить это стихотворение <w>наизусть</w> к завтрашнему дню."
                            }
                        }
                    ],
                    "synonyms": [
                        "harses",
                        "kanis",
                        "knar"
                    ],
                    "common_phrases": [
                        "uit het hoofd",
                        "hoofd en schouders",
                        "het hoofd bieden",
                        "het hoofd koel houden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя часть человеческого тела, где находятся мозг, глаза, уши, нос и рот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голова",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53083448-bf04-33ba-b98b-a1a5ed5e2e5a",
                    "sense_definition": "De persoon die de leiding heeft over een organisatie, afdeling of groep.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "leader_top",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als <w>hoofd</w> van de afdeling is zij verantwoordelijk voor het budget.",
                            "target_lang_translations": {
                                "ru": "Как <w>начальник</w> отдела, она отвечает за бюджет."
                            }
                        },
                        {
                            "text": "De koning is in naam het <w>hoofd</w> van de regering.",
                            "target_lang_translations": {
                                "ru": "Король номинально является <w>главой</w> правительства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan het hoofd staan",
                        "gekroonde hoofden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, возглавляющее организацию, отдел или группу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глава",
                                "target_lang_sense_clarification": "руководитель организации или группы"
                            },
                            {
                                "target_lang_word": "начальник",
                                "target_lang_sense_clarification": "руководитель отдела"
                            },
                            {
                                "target_lang_word": "руководитель",

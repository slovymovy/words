{
    "word_family": [
        "truchleć",
        "struchlały",
        "truchlenie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "26e75ef9-1721-3611-8488-77d7222f720c",
                    "sense_definition": "Martwe ciało, zazwyczaj zwierzęcia, rzadziej człowieka (często w kontekście rozkładu).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dead_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na leśnej ścieżce leżało rozkładające się <w>truchło</w> dzika.",
                            "target_lang_translations": {
                                "ru": "На лесной тропинке лежала разлагающаяся <w>туша</w> кабана."
                            }
                        },
                        {
                            "text": "Policjant zabezpieczył <w>truchło</w> kota, które znaleziono w worku.",
                            "target_lang_translations": {
                                "ru": "Полицейский изъял <w>труп</w> кота, который нашли в мешке."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwłoki",
                        "trup",
                        "ścierwo",
                        "zewłok"
                    ],
                    "common_phrases": [
                        "rozkładające się truchło",
                        "truchło zwierzęcia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мёртвое тело, обычно животного, реже человека (часто в контексте разложения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падаль",
                                "target_lang_sense_clarification": "разлагающийся труп животного"
                            },
                            {
                                "target_lang_word": "туша",
                                "target_lang_sense_clarification": "тело убитого или умершего крупного животного"
                            },
                            {
                                "target_lang_word": "труп",
                                "target_lang_sense_clarification": "мёртвое тело"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9969335c-1768-37d6-ad04-043a1f2ba490",
                    "sense_definition": "Człowiek bardzo stary, schorowany i niedołężny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "old_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z dawnej energicznej kobiety zostało jedynie bezsilne <w>truchło</w>.",
                            "target_lang_translations": {
                                "ru": "От некогда энергичной женщины осталась лишь бессильная <w>развалина</w>."
                            }
                        },
                        {
                            "text": "To biedne <w>truchło</w> nie ma już siły wstać z łóżka o własnych siłach.",
                            "target_lang_translations": {
                                "ru": "Эта бедная <w>развалина</w> уже не в силах встать с постели самостоятельно."
                            }
                        }
                    ],
                    "synonyms": [
                        "starzec"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często nacechowane negatywnie lub z politowaniem"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень старый, больной и немощный человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развалина",
                                "target_lang_sense_clarification": "разг. о дряхлом, больном человеке"
                            },
                            {
                                "target_lang_word": "доходяга",
                                "target_lang_sense_clarification": "разг. очень слабый, истощённый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d63e1293-e5e2-3fd1-8a1a-8820d621325f",
                    "sense_definition": "Dawne określenie na stan głębokiego smutku, przygnębienia lub trwogi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielkie <w>truchło</w> padło na mieszkańców oblężonego miasta.",
                            "target_lang_translations": {
                                "ru": "Великий <w>ужас</w> пал на жителей осаждённого города."
                            }
                        },
                        {
                            "text": "Jego serce ogarnęło <w>truchło</w> na widok nadchodzącej burzy.",
                            "target_lang_translations": {
                                "ru": "Его сердце сковал <w>трепет</w> при виде надвигающейся бури."
                            }
                        }
                    ],
                    "synonyms": [
                        "smutek",
                        "przygnębienie",
                        "strapienie"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее обозначение состояния глубокой печали, уныния или страха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ужас",
                                "target_lang_sense_clarification": "сильный страх"
                            },
                            {
                                "target_lang_word": "трепет",
                                "target_lang_sense_clarification": "дрожь, сильное волнение или страх"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
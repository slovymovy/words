                        {
                            "text": "Пожалуйста, <подай> мне соль и перец.",
                            "target_lang_translations": {
                                "en": "Please <w>pass</w> me the salt and pepper."
                            }
                        },
                        {
                            "text": "Мужчина вежливо <подал> даме руку, когда она выходила из автобуса.",
                            "target_lang_translations": {
                                "en": "The man politely <w>gave</w> the lady his hand as she got off the bus."
                            }
                        }
                    ],
                    "synonyms": [
                        "дать",
                        "преподнести",
                        "поднести"
                    ],
                    "antonyms": [
                        "отобрать"
                    ],
                    "common_phrases": [
                        "подать руку",
                        "подать пальто",
                        "подать книгу"
                    ],
                    "target_lang_definitions": {
                        "en": "To give or hand something to someone"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "general transfer"
                            },
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "at a table or hand over"
                            },
                            {
                                "target_lang_word": "hand",
                                "target_lang_sense_clarification": "physically give with hand"
                            }
                        ]
                    }
                },
                {

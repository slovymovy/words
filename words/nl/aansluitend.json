{
    "word_family": [
        "aansluiting"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b5806ab6-7869-3fe0-9519-0166dca668a3",
                    "sense_definition": "Direct volgend op iets anders in de tijd, zonder tussenruimte.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "temporal_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de vergadering is er <w>aansluitend</w> een gezamenlijke lunch in de kantine.",
                            "target_lang_translations": {
                                "ru": "После собрания <w>сразу же</w> состоится совместный обед в столовой.",
                                "en": "After the meeting, there is <w>immediately following</w> a joint lunch in the canteen."
                            }
                        },
                        {
                            "text": "Aansluitend zocht ze contact met haar ouders om het goede nieuws te vertellen.",
                            "target_lang_translations": {
                                "ru": "<w>Вслед за этим</w> она связалась с родителями, чтобы сообщить хорошую новость.",
                                "en": "<w>Subsequently</w>, she contacted her parents to tell them the good news."
                            }
                        }
                    ],
                    "synonyms": [
                        "opeenvolgend"
                    ],
                    "common_phrases": [
                        "aansluitend op",
                        "direct aansluitend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непосредственно следующий за чем-либо во времени, без перерыва.",
                        "en": "Immediately following something else in time, without a gap."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сразу после",
                                "target_lang_sense_clarification": "наречие: непосредственно вслед за чем-то"
                            },
                            {
                                "target_lang_word": "вслед за этим",
                                "target_lang_sense_clarification": "наречие: затем, после этого"
                            },
                            {
                                "target_lang_word": "последующий",
                                "target_lang_sense_clarification": "прилагательное: идущий следом"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "immediately following",
                                "target_lang_sense_clarification": "happening right after an event"
                            },
                            {
                                "target_lang_word": "subsequently",
                                "target_lang_sense_clarification": "adverb: afterwards, following that"
                            },
                            {
                                "target_lang_word": "consecutive",
                                "target_lang_sense_clarification": "following continuously"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b412b2f-3c33-3313-939f-016094e76e5f",
                    "sense_definition": "Fysiek verbonden met of grenzend aan iets anders.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben in het hotel een kamer geboekt met een <w>aansluitende</w> badkamer.",
                            "target_lang_translations": {
                                "ru": "Мы забронировали в отеле номер со <w>смежной</w> ванной комнатой.",
                                "en": "We booked a room in the hotel with an <w>adjoining</w> bathroom."
                            }
                        },
                        {
                            "text": "Het park en het <w>aansluitende</w> bos vormen samen een groot natuurgebied.",
                            "target_lang_translations": {
                                "ru": "Парк и <w>примыкающий</w> лес вместе образуют большую природную зону.",
                                "en": "The park and the <w>adjacent</w> forest together form a large nature reserve."
                            }
                        }
                    ],
                    "synonyms": [
                        "aangrenzend"
                    ],
                    "common_phrases": [
                        "aansluitend gebied",
                        "aansluitende kamer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически соединенный с чем-либо или граничащий с чем-либо.",
                        "en": "Physically connected to or bordering something else."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смежный",
                                "target_lang_sense_clarification": "имеющий общую границу (о комнатах, помещениях)"
                            },
                            {
                                "target_lang_word": "примыкающий",
                                "target_lang_sense_clarification": "расположенный вплотную, граничащий"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "adjoining",
                                "target_lang_sense_clarification": "next to or joined with (e.g., rooms)"
                            },
                            {
                                "target_lang_word": "adjacent",
                                "target_lang_sense_clarification": "lying near or close to"
                            },
                            {
                                "target_lang_word": "connecting",
                                "target_lang_sense_clarification": "linking two things together"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "620e4f38-216e-3dec-9949-647375dad4a4",
                    "sense_definition": "Precies passend of goed op elkaar afgestemd; strak zittend om het lichaam.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fitting_matching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze draagt graag een nauw <w>aansluitende</w> jurk naar het feest.",
                            "target_lang_translations": {
                                "ru": "Она любит надевать на вечеринку плотно <w>облегающее</w> платье.",
                                "en": "She likes wearing a <w>tight-fitting</w> dress to the party."
                            }
                        },
                        {
                            "text": "Het is belangrijk om een opleiding te kiezen met een <w>aansluitende</w> baan in het vooruitzicht.",
                            "target_lang_translations": {
                                "ru": "Важно выбрать образование с перспективой <w>соответствующей</w> работы.",
                                "en": "It is important to choose an education with a <w>corresponding</w> job in prospect."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nauw aansluitend",
                        "goed aansluitend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точно подходящий или плотно сидящий (об одежде); хорошо согласованный.",
                        "en": "Fitting exactly or sitting tightly around the body; well-coordinated."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облегающий",
                                "target_lang_sense_clarification": "плотно сидящий на теле (об одежде)"
                            },
                            {
                                "target_lang_word": "соответствующий",
                                "target_lang_sense_clarification": "подходящий, соотносящийся с чем-либо"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "tight-fitting",
                                "target_lang_sense_clarification": "fitting very closely (clothing)"
                            },
                            {
                                "target_lang_word": "form-fitting",
                                "target_lang_sense_clarification": "following the contours of the body"
                            },
                            {
                                "target_lang_word": "corresponding",
                                "target_lang_sense_clarification": "matching or suitable for"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bf977107-b241-3954-ae48-f08aefaad85e",
                    "sense_definition": "onvoltooid deelwoord van <w>aansluiten</w>",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zich bij de rij <w>aansluitende</w> mensen moesten lang wachten.",
                            "target_lang_translations": {
                                "ru": "Людям, <w>присоединяющимся</w> к очереди, приходилось долго ждать.",
                                "en": "The people <w>joining</w> the queue had to wait a long time."
                            }
                        },
                        {
                            "text": "Hij kwam de kamer binnen, <w>aansluitend</w> bij het gesprek dat al gaande was.",
                            "target_lang_translations": {
                                "ru": "Он вошел в комнату, <w>присоединяясь</w> к уже идущему разговору.",
                                "en": "He entered the room, <w>joining</w> the conversation that was already in progress."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола 'aansluiten' (присоединяться, подключаться).",
                        "en": "Present participle of the verb 'aansluiten' (to join, to connect)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присоединяющийся",
                                "target_lang_sense_clarification": "вступающий в группу или соединение"
                            },
                            {
                                "target_lang_word": "подключающийся",
                                "target_lang_sense_clarification": "устанавливающий соединение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "joining",
                                "target_lang_sense_clarification": "becoming part of a group or line"
                            },
                            {
                                "target_lang_word": "connecting",
                                "target_lang_sense_clarification": "establishing a link"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
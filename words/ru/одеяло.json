{
    "word_family": [
        "одеяльце",
        "одеялко",
        "одеяльный",
        "пододеяльник"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dd267b8f-3a6b-315c-ad12-1cde660bc442",
                    "sense_definition": "Постельная принадлежность из плотной ткани или с наполнителем, которой укрываются для тепла.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я люблю спать под тёплым пуховым <w>одеялом</w>.",
                            "target_lang_translations": {
                                "en": "I love sleeping under a warm down <w>blanket</w>."
                            }
                        },
                        {
                            "text": "Мама поправила <w>одеяло</w> спящему ребёнку и тихо вышла из комнаты.",
                            "target_lang_translations": {
                                "en": "Mom adjusted the sleeping child's <w>blanket</w> and quietly left the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "плед",
                        "покрывало"
                    ],
                    "common_phrases": [
                        "тёплое одеяло",
                        "укрыться одеялом",
                        "пуховое одеяло",
                        "шерстяное одеяло",
                        "залезть под одеяло"
                    ],
                    "target_lang_definitions": {
                        "en": "A piece of bedding made of thick fabric or filled with material, used to cover oneself for warmth."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blanket",
                                "target_lang_sense_clarification": "A large piece of fabric used on a bed for warmth"
                            },
                            {
                                "target_lang_word": "quilt",
                                "target_lang_sense_clarification": "A warm bed covering made of padding enclosed between layers of fabric"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e08d907d-8d7e-3c54-b4b5-64ada3c31cf6",
                    "sense_definition": "Сплошной слой чего-либо, полностью покрывающий поверхность (обычно о снеге, тумане или дыме).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Земля скрылась под белым снежным <w>одеялом</w>.",
                            "target_lang_translations": {
                                "en": "The earth disappeared under a white <w>blanket</w> of snow."
                            }
                        },
                        {
                            "text": "Город накрыло плотное <w>одеяло</w> смога, из-за чего стало трудно дышать.",
                            "target_lang_translations": {
                                "en": "A thick <w>blanket</w> of smog covered the city, making it difficult to breathe."
                            }
                        }
                    ],
                    "synonyms": [
                        "покров",
                        "пелена",
                        "слой"
                    ],
                    "common_phrases": [
                        "снежное одеяло",
                        "одеяло тумана"
                    ],
                    "target_lang_definitions": {
                        "en": "A continuous layer of something that completely covers a surface (usually referring to snow, fog, or smoke)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blanket",
                                "target_lang_sense_clarification": "A covering layer of something, like snow or fog"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "радеть",
        "нерадивый",
        "радение"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "695e6519-818f-352c-a22c-65bdcb5424b0",
                    "sense_definition": "В интересах или для пользы кого-либо, чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "benefit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители готовы на всё <ради> счастья своих детей.",
                            "target_lang_translations": {
                                "en": "Parents are ready for anything <for the sake of> their children's happiness."
                            }
                        },
                        {
                            "text": "Он отказался от выгодного предложения <ради> семьи.",
                            "target_lang_translations": {
                                "en": "He refused a profitable offer <for the sake of> his family."
                            }
                        }
                    ],
                    "antonyms": [
                        "вопреки"
                    ],
                    "common_phrases": [
                        "ради тебя",
                        "ради детей",
                        "ради семьи",
                        "ради мира"
                    ],
                    "target_lang_definitions": {
                        "en": "In the interests or for the benefit of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for the sake of",
                                "target_lang_sense_clarification": "indicating benefit or interest"
                            },
                            {
                                "target_lang_word": "for",
                                "target_lang_sense_clarification": "general benefit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e24f8c98-b232-3f98-a0bb-230ffbc8be3c",
                    "sense_definition": "С целью чего-либо, для достижения чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы поехали в этот город только <ради> экскурсии.",
                            "target_lang_translations": {
                                "en": "We went to this city only <for the sake of> the excursion."
                            }
                        },
                        {
                            "text": "Спортсмен тренируется каждый день <ради> победы.",
                            "target_lang_translations": {
                                "en": "The athlete trains every day <for the sake of> victory."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ради денег",
                        "ради славы",
                        "ради интереса",
                        "ради шутки"
                    ],
                    "target_lang_definitions": {
                        "en": "With the purpose of something, to achieve something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for the sake of",
                                "target_lang_sense_clarification": "indicating purpose or goal"
                            },
                            {
                                "target_lang_word": "for",
                                "target_lang_sense_clarification": "indicating the object of an aim"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4cc3cf7e-2c96-3829-80cb-fa84d376e5c3",
                    "sense_definition": "Употребляется при усиленной просьбе или мольбе (обычно в устойчивых сочетаниях).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Помогите мне, бога <ради>, я не знаю, что делать!",
                            "target_lang_translations": {
                                "en": "Help me, <for> God's <sake>, I don't know what to do!"
                            }
                        },
                        {
                            "text": "Прошу тебя <ради> всего святого, останься дома.",
                            "target_lang_translations": {
                                "en": "I beg you <for the sake of> all that is holy, stay home."
                            }
                        }
                    ],
                    "common_phrases": [
                        "бога ради",
                        "Христа ради",
                        "ради всего святого"
                    ],
                    "target_lang_definitions": {
                        "en": "Used in intensified requests or pleas (usually in fixed phrases)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for ... sake",
                                "target_lang_sense_clarification": "used in idiomatic pleas like 'for God's sake'"
                            },
                            {
                                "target_lang_word": "for the sake of",
                                "target_lang_sense_clarification": "formal plea"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20728de8-f245-3a6f-b89a-13fb26b56507",
                    "sense_definition": "По причине, из-за чего-либо (указывает на мотив действия).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она занимается рисованием <ради> самого процесса творчества.",
                            "target_lang_translations": {
                                "en": "She engages in painting <for the sake of> the creative process itself."
                            }
                        },
                        {
                            "text": "Мы встретились в кафе только <ради> приличия.",
                            "target_lang_translations": {
                                "en": "We met at the cafe only <for the sake of> propriety."
                            }
                        }
                    ],
                    "synonyms": [
                        "из-за",
                        "по причине"
                    ],
                    "common_phrases": [
                        "ради приличия",
                        "ради справедливости"
                    ],
                    "target_lang_definitions": {
                        "en": "For the reason of; due to (indicating the motive of an action)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for the sake of",
                                "target_lang_sense_clarification": "indicating motive"
                            },
                            {
                                "target_lang_word": "out of",
                                "target_lang_sense_clarification": "indicating underlying cause, e.g., out of propriety"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
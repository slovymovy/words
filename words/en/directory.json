    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "48ddd51b-ec26-3239-a450-6c66e2331ecb",
                    "sense_definition": "A list of names, addresses, or other contact information, usually arranged alphabetically.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I looked up the plumber's number in the local <w>directory</w>.",
                            "target_lang_translations": {
                                "ru": "Я нашел номер сантехника в местном <w>справочнике</w>."
                            }
                        },
                        {
                            "text": "The company <w>directory</w> helps employees find contact details for their colleagues.",
                            "target_lang_translations": {
                                "ru": "<w>Справочник</w> компании помогает сотрудникам найти контактные данные коллег."
                            }
                        }
                    ],
                    "common_phrases": [
                        "telephone directory",
                        "business directory",
                        "online directory",
                        "staff directory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Список имен, адресов или другой контактной информации, обычно расположенный в алфавитном порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спра́вочник",
                                "target_lang_sense_clarification": "книга или список с информацией"
                            },
                            {
                                "target_lang_word": "катало́г",
                                "target_lang_sense_clarification": "упорядоченный перечень"
                            },
                            {
                                "target_lang_word": "указа́тель",
                                "target_lang_sense_clarification": "алфавитный список"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "000f8b58-24ed-3cae-9242-469db02bd44d",
                    "sense_definition": "A virtual container in a computer's file system where files and other directories are stored.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should save the installation files in a separate <w>directory</w>.",
                            "target_lang_translations": {
                                "ru": "Вам следует сохранить установочные файлы в отдельный <w>каталог</w>."
                            }
                        },
                        {
                            "text": "The program could not find the specified <w>directory</w> on the hard drive.",
                            "target_lang_translations": {
                                "ru": "Программа не смогла найти указанную <w>директорию</w> на жестком диске."
                            }
                        }
                    ],
                    "synonyms": [
                        "folder"
                    ],
                    "common_phrases": [
                        "root directory",
                        "current directory",
                        "file directory",
                        "parent directory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Виртуальный контейнер в файловой системе компьютера, где хранятся файлы и другие каталоги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "катало́г",
                                "target_lang_sense_clarification": "структура данных"
                            },
                            {
                                "target_lang_word": "па́пка",
                                "target_lang_sense_clarification": "объект интерфейса"
                            },
                            {

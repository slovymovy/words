                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The explosion <w>wounded</w> several people near the market.",
                            "target_lang_translations": {
                                "ru": "Взрыв <w>ранил</w> нескольких человек возле рынка."
                            }
                        },
                        {
                            "text": "He was <w>wounded</w> in the leg during the accident.",
                            "target_lang_translations": {
                                "ru": "Он был <w>ранен</w> в ногу во время происшествия."
                            }
                        }
                    ],

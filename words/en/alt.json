    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d6df9b3c-9bd0-3c9a-87e7-17a1f520c88b",
                    "sense_definition": "A specific key on a computer keyboard used to change the function of other keys when pressed together.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computer_key",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hold down the <w>Alt</w> key and press F4 to close the window.",
                            "target_lang_translations": {
                                "ru": "Удерживайте клавишу <w>Alt</w> и нажмите F4, чтобы закрыть окно."
                            }
                        },
                        {
                            "text": "The <w>Alt</w> key is located to the left of the space bar.",
                            "target_lang_translations": {
                                "ru": "Клавиша <w>Alt</w> находится слева от пробела."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Alt key",
                        "press Alt",
                        "hold Alt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Клавиша на клавиатуре компьютера, используемая для изменения функции других клавиш при одновременном нажатии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Alt",
                                "target_lang_sense_clarification": "клавиша"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05424867-7e93-329c-8cd6-ec171a148ecc",
                    "sense_definition": "A secondary online account or character, distinct from a user's main one.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "internet_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He created an <w>alt</w> to play with his lower-level friends.",
                            "target_lang_translations": {
                                "ru": "Он создал <w>твинка</w>, чтобы играть со своими друзьями низкого уровня."
                            }
                        },
                        {
                            "text": "Using an <w>alt</w> to bypass a ban is against the rules.",
                            "target_lang_translations": {
                                "ru": "Использование <w>второго аккаунта</w> для обхода бана запрещено правилами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "create an alt",
                        "use an alt",
                        "main and alt"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet and gaming"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дополнительный онлайн-аккаунт или персонаж, отличный от основного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "твинк",
                                "target_lang_sense_clarification": "игровой сленг"
                            },
                            {
                                "target_lang_word": "альт",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "второй аккаунт",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "880a4c43-9ea0-3e10-8c52-a0da6389f6ad",
                    "sense_definition": "A type of dark, top-fermented beer traditionally from Düsseldorf; short for <w>altbier</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "beer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We ordered two <w>alts</w> at the German pub.",
                            "target_lang_translations": {
                                "ru": "Мы заказали два <w>альта</w> в немецком пабе."
                            }
                        },
                        {
                            "text": "The brewery specializes in traditional <w>alts</w> and lagers.",
                            "target_lang_translations": {
                                "ru": "Пивоварня специализируется на традиционных <w>альтах</w> и лагерах."
                            }
                        }
                    ],
                    "synonyms": [
                        "altbier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сорт темного пива верхового брожения, традиционно из Дюссельдорфа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "альт",
                                "target_lang_sense_clarification": "сорт пива"
                            },
                            {
                                "target_lang_word": "альтбир",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7aaaac98-f572-3812-8995-8d98f06704c9",
                    "sense_definition": "Height or elevation above a reference point; short for <w>altitude</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "abbreviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot checked the <w>alt</w> before starting the descent.",
                            "target_lang_translations": {
                                "ru": "Пилот проверил <w>высоту</w> перед началом снижения."
                            }
                        },
                        {
                            "text": "Our current <w>alt</w> is thirty thousand feet.",
                            "target_lang_translations": {
                                "ru": "Наша текущая <w>высота</w> — тридцать тысяч футов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "aviation or technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высота или возвышение над точкой отсчета (сокращение от altitude)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "авиация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed516d07-2f0d-340b-b0eb-98e8f06d5ec2",
                    "sense_definition": "A high pitch in voice or instrumentation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soprano's voice rose in <w>alt</w> during the climax of the opera.",
                            "target_lang_translations": {
                                "ru": "Голос сопрано поднялся <w>в верхний регистр</w> во время кульминации оперы."
                            }
                        },
                        {
                            "text": "He struggled to sing the notes in <w>alt</w>.",
                            "target_lang_translations": {
                                "ru": "Ему было трудно брать <w>высокие ноты</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in alt"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "or technical music context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокий регистр в голосе или инструментальной музыке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высокая нота",
                                "target_lang_sense_clarification": "музыка"
                            },
                            {
                                "target_lang_word": "верхний регистр",
                                "target_lang_sense_clarification": "музыка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7cec8da2-e87f-37a0-bf9a-07a7cf5c1c1f",
                    "sense_definition": "Relating to a style, genre, or culture that exists outside the mainstream; short for <w>alternative</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "alternative_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band plays a mix of punk and <w>alt</w> rock.",
                            "target_lang_translations": {
                                "ru": "Группа играет смесь панка и <w>альтернативного</w> рока."
                            }
                        },
                        {
                            "text": "She is interested in <w>alt</w> fashion and design.",
                            "target_lang_translations": {
                                "ru": "Она интересуется <w>альтернативной</w> модой и дизайном."
                            }
                        }
                    ],
                    "common_phrases": [
                        "alt rock",
                        "alt right",
                        "alt comedy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к стилю, жанру или культуре, существующим вне мейнстрима."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "альтернативный",
                                "target_lang_sense_clarification": "жанр или культура"
                            },
                            {
                                "target_lang_word": "альт-",
                                "target_lang_sense_clarification": "приставка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88876c26-1dd9-338f-8253-81c266e5bec4",
                    "sense_definition": "Serving as a substitute or option; short for <w>alternate</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "alternate_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I use an <w>alt</w> email address for signing up to newsletters.",
                            "target_lang_translations": {
                                "ru": "Я использую <w>дополнительный</w> адрес электронной почты для подписки на рассылки."
                            }
                        },
                        {
                            "text": "If the main road is closed, take the <w>alt</w> route.",
                            "target_lang_translations": {
                                "ru": "Если главная дорога закрыта, воспользуйтесь <w>альтернативным</w> маршрутом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Служащий заменой или вариантом; запасной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дополнительный",
                                "target_lang_sense_clarification": "email, аккаунт"
                            },
                            {
                                "target_lang_word": "альтернативный",
                                "target_lang_sense_clarification": "маршрут, вариант"
                            },
                            {
                                "target_lang_word": "запасной",
                                "target_lang_sense_clarification": "вариант"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "1eb97b44-99ea-36b8-a49c-56e2632bddcb",
                    "sense_definition": "A river in England that flows into the Irish Sea.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The River <w>Alt</w> runs through the county of Merseyside.",
                            "target_lang_translations": {
                                "ru": "Река <w>Альт</w> протекает через графство Мерсисайд."
                            }
                        },
                        {
                            "text": "Local authorities monitored the water levels of the <w>Alt</w> after the storm.",
                            "target_lang_translations": {
                                "ru": "Местные власти следили за уровнем воды в <w>Альте</w> после шторма."
                            }
                        }

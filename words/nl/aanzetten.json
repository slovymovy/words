                    ],
                    "synonyms": [
                        "vastmaken",
                        "bevestigen",
                        "aanhechten"
                    ],
                    "antonyms": [
                        "losmaken"
                    ],
                    "common_phrases": [
                        "een knoop aanzetten"
                    ],
                    "target_lang_definitions": {
                        "en": "To fasten something to something else, often by sewing or mounting.",
                        "ru": "Прикреплять что-либо к чему-либо другому, часто путем шитья или монтажа."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sew on",
                                "target_lang_sense_clarification": "specifically for buttons or fabric"
                            },
                            {
                                "target_lang_word": "attach",
                                "target_lang_sense_clarification": "general fastening"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пришивать",
                                "target_lang_sense_clarification": "о пуговицах, ткани"
                            },
                            {
                                "target_lang_word": "приделывать",
                                "target_lang_sense_clarification": "прикреплять, монтировать"
                            },
                            {
                                "target_lang_word": "пристраивать",
                                "target_lang_sense_clarification": "о частях здания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71344b82-c55d-326c-a74a-7f7f92b74e63",
                    "sense_definition": "Iemand dringend stimuleren of dwingen om iets te doen, vaak iets negatiefs.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "incite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De toespraak van de leider zou mensen kunnen <w>aanzetten</w> tot geweld.",
                            "target_lang_translations": {
                                "en": "The leader's speech could <w>incite</w> people to violence.",
                                "ru": "Речь лидера может <w>подстрекнуть</w> людей к насилию."
                            }
                        },
                        {
                            "text": "Hij werd door zijn vrienden <w>aangezet</w> om de regels te overtreden.",
                            "target_lang_translations": {
                                "en": "He was <w>urged</w> by his friends to break the rules.",
                                "ru": "Друзья <w>подбили</w> его нарушить правила."
                            }
                        }
                    ],
                    "synonyms": [
                        "aansporen",
                        "ophitsen",
                        "opjutten"
                    ],
                    "antonyms": [
                        "ontmoedigen",
                        "afremmen"
                    ],
                    "common_phrases": [
                        "aanzetten tot haat",
                        "aanzetten tot geweld",
                        "aanzetten tot denken"
                    ],
                    "target_lang_definitions": {
                        "en": "To urgently stimulate or force someone to do something, often something negative.",
                        "ru": "Настойчиво стимулировать или принуждать кого-то сделать что-то, часто что-то негативное."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "incite",
                                "target_lang_sense_clarification": "to provoke action, often negative"
                            },
                            {
                                "target_lang_word": "instigate",
                                "target_lang_sense_clarification": "to initiate bad behavior"
                            },
                            {
                                "target_lang_word": "urge",
                                "target_lang_sense_clarification": "to strongly encourage someone"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подстрекать",
                                "target_lang_sense_clarification": "склонять к чему-то плохому"
                            },
                            {
                                "target_lang_word": "побуждать",
                                "target_lang_sense_clarification": "стимулировать к действию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "348c00d8-da5c-34b0-96d2-ade2d7dd5538",
                    "sense_definition": "Aankomen of verschijnen, vaak onverwacht of met iets bij zich.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "arrive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kwam ineens met een heel vreemd verhaal <w>aanzetten</w>.",
                            "target_lang_translations": {
                                "en": "He suddenly <w>came up</w> with a very strange story.",
                                "ru": "Он вдруг <w>заявился</w> с очень странной историей."
                            }
                        },
                        {
                            "text": "Toen het feest al bijna voorbij was, kwam zij pas <w>aanzetten</w>.",
                            "target_lang_translations": {
                                "en": "When the party was almost over, she finally <w>showed up</w>.",
                                "ru": "Когда вечеринка уже почти закончилась, она только <w>явилась</w>."
                            }
                        }

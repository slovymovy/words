    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2e988ece-b4e0-3b2f-afbf-15cdab40a3cb",
                    "sense_definition": "Поставить отметку или оценку в официальные документы (журнал, ведомость, зачётку).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "grade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель <выставил> ученику отличную оценку за ответ у доски.",
                            "target_lang_translations": {
                                "en": "The teacher <gave> the student an excellent grade for the answer at the blackboard."
                            }
                        },
                        {
                            "text": "В конце семестра преподаватель <выставил> всем итоговые баллы.",
                            "target_lang_translations": {
                                "en": "At the end of the semester, the instructor <assigned> final scores to everyone."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выставить оценку",
                        "выставить балл",
                        "выставить зачёт",
                        "выставить в журнал"
                    ],
                    "target_lang_definitions": {
                        "en": "To record a mark or grade in official documents (register, record sheet, grade book)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "a grade or mark"
                            },
                            {
                                "target_lang_word": "assign",
                                "target_lang_sense_clarification": "a score or grade"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07ed0f2a-1fe5-384a-8f66-4dd991d69b28",
                    "sense_definition": "Переместить что-либо наружу или выдвинуть вперёд.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не нужно <выставлять> руку из окна движущегося автомобиля.",
                            "target_lang_translations": {
                                "en": "You shouldn't <stick> your arm <out> of the window of a moving car."
                            }
                        },
                        {
                            "text": "Летом мы решили <выставить> обеденный стол на террасу.",
                            "target_lang_translations": {
                                "en": "In the summer, we decided to <put> the dining table <out> on the terrace."
                            }
                        }
                    ],
                    "antonyms": [
                        "убрать",
                        "задвинуть"
                    ],
                    "common_phrases": [
                        "выставить руку",
                        "выставить ногу",
                        "выставить вперёд",
                        "выставить наружу"
                    ],
                    "target_lang_definitions": {
                        "en": "To move something outside or extend it forward."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stick out",
                                "target_lang_sense_clarification": "body parts"
                            },
                            {
                                "target_lang_word": "put out",
                                "target_lang_sense_clarification": "objects"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66f67cb0-6e6e-3b4c-bdb7-809543949cf6",
                    "sense_definition": "Поместить предметы так, чтобы их было видно, для всеобщего обозрения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "display",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Художник <выставил> свои лучшие картины в городской галерее.",
                            "target_lang_translations": {
                                "en": "The artist <exhibited> his best paintings in the city gallery."
                            }
                        },
                        {
                            "text": "Магазин <выставил> новые товары на витрину для привлечения покупателей.",
                            "target_lang_translations": {
                                "en": "The store <displayed> new goods in the window to attract buyers."
                            }
                        }
                    ],
                    "synonyms": [
                        "представить"
                    ],
                    "antonyms": [
                        "спрятать"
                    ],
                    "common_phrases": [
                        "выставить на продажу",
                        "выставить напоказ",
                        "выставить на обозрение",
                        "выставить экспонаты"
                    ],
                    "target_lang_definitions": {
                        "en": "To place items so they are visible for public viewing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exhibit",
                                "target_lang_sense_clarification": "art or museum pieces"
                            },
                            {
                                "target_lang_word": "display",
                                "target_lang_sense_clarification": "merchandise or goods"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac323bdc-c909-3377-8b7a-923ba21cddf6",
                    "sense_definition": "Грубо заставить кого-либо уйти, выгнать из помещения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "kick_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охрана вежливо попросила, а затем силой <выставила> хулигана из клуба.",
                            "target_lang_translations": {
                                "en": "Security asked politely, and then forcibly <kicked> the hooligan <out> of the club."
                            }
                        },
                        {
                            "text": "Если ты продолжишь шуметь, я буду вынужден <выставить> тебя за дверь.",
                            "target_lang_translations": {
                                "en": "If you keep making noise, I will be forced to <throw> you <out> the door."
                            }
                        }
                    ],
                    "antonyms": [
                        "впустить"
                    ],
                    "common_phrases": [
                        "выставить за дверь",
                        "выставить из дома",
                        "выставить на улицу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To rudely force someone to leave or kick them out of a place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kick out",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "throw out",
                                "target_lang_sense_clarification": "force to leave"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e7f6530-0e79-3cb0-ae0b-3fd6e4b60115",
                    "sense_definition": "Создать определённое впечатление о ком-либо, представить в каком-либо свете (часто негативном).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "portray",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он пытался <выставить> меня виноватым во всех наших проблемах.",
                            "target_lang_translations": {
                                "en": "He tried to <make> me <look> guilty for all our problems."
                            }
                        },
                        {
                            "text": "Пресса <выставила> этого политика настоящим героем нации.",
                            "target_lang_translations": {
                                "en": "The press <portrayed> this politician as a real hero of the nation."
                            }
                        }
                    ],
                    "synonyms": [
                        "представить"
                    ],
                    "common_phrases": [
                        "выставить дураком",
                        "выставить в дурном свете",
                        "выставить посмешищем",
                        "выставить виноватым"
                    ],
                    "target_lang_definitions": {
                        "en": "To create a certain impression of someone, to present them in a specific light (often negative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make ... look",
                                "target_lang_sense_clarification": "create an impression"
                            },
                            {
                                "target_lang_word": "portray",
                                "target_lang_sense_clarification": "describe or represent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7ee512b-b2f3-3364-951f-c05f751a1892",
                    "sense_definition": "Предложить кандидатуру или требование для официального рассмотрения.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "nominate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Партия решила <выставить> своего кандидата на предстоящих выборах мэра.",
                            "target_lang_translations": {
                                "en": "The party decided to <nominate> its candidate for the upcoming mayoral election."
                            }
                        },
                        {
                            "text": "Профсоюз <выставил> ряд требований к руководству завода.",
                            "target_lang_translations": {
                                "en": "The trade union <put forward> a series of demands to the factory management."
                            }
                        }
                    ],
                    "synonyms": [
                        "предложить"
                    ],
                    "antonyms": [
                        "снять"
                    ],
                    "common_phrases": [
                        "выставить кандидатуру",
                        "выставить требования",
                        "выставить претензии"
                    ],
                    "target_lang_definitions": {
                        "en": "To propose a candidate or demand for official consideration."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put forward",
                                "target_lang_sense_clarification": "a candidate or demands"
                            },
                            {
                                "target_lang_word": "nominate",
                                "target_lang_sense_clarification": "a candidate"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9daf5e7e-1eba-3331-8d02-afb759b49372",
                    "sense_definition": "Расположить людей (охрану, дозор) по местам для выполнения задачи.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "station",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир приказал <выставить> часовых вокруг военного лагеря.",
                            "target_lang_translations": {
                                "en": "The commander ordered to <post> sentries around the military camp."
                            }
                        },
                        {
                            "text": "Полиция <выставила> оцепление, чтобы никто не прошёл на место происшествия.",
                            "target_lang_translations": {
                                "en": "The police <set up> a cordon so that no one would pass to the scene of the incident."
                            }
                        }
                    ],
                    "antonyms": [
                        "снять"
                    ],
                    "common_phrases": [
                        "выставить охрану",
                        "выставить пост",
                        "выставить часовых",
                        "выставить дозор"
                    ],
                    "target_lang_definitions": {
                        "en": "To position people (guards, sentries) at specific posts to perform a task."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "post",
                                "target_lang_sense_clarification": "guards or sentries"
                            },
                            {
                                "target_lang_word": "station",
                                "target_lang_sense_clarification": "troops or police"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c640293-b18f-3ba2-8f59-b69669aa67e7",
                    "sense_definition": "Вынуть вставленный предмет (обычно раму или стекло) из проёма.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "remove_frame",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чтобы занести широкий диван, нам пришлось <выставить> оконную раму.",
                            "target_lang_translations": {
                                "en": "To bring in the wide sofa, we had to <take out> the window frame."
                            }
                        },
                        {
                            "text": "От сильного взрыва в соседнем доме <выставило> все стёкла.",
                            "target_lang_translations": {
                                "en": "The strong explosion in the neighboring house <blew out> all the windows."
                            }
                        }
                    ],
                    "antonyms": [
                        "вставить"
                    ],
                    "common_phrases": [
                        "выставить окно",
                        "выставить раму",
                        "выставить стекло"
                    ],
                    "target_lang_definitions": {
                        "en": "To take out an inserted object (usually a frame or glass) from an opening."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "remove physically"
                            },
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "general term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af165761-45b9-3f72-8fb5-2b1169ca380a",
                    "sense_definition": "Угостить кого-либо за свой счёт (обычно спиртными напитками).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "treat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В честь своего повышения он пообещал <выставить> коллегам угощение.",
                            "target_lang_translations": {
                                "en": "In honor of his promotion, he promised to <treat> his colleagues to a meal."
                            }
                        },
                        {
                            "text": "Ты проиграл спор, так что теперь должен <выставить> бутылку.",
                            "target_lang_translations": {
                                "en": "You lost the bet, so now you have to <buy> a bottle."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выставить магарыч",
                        "выставить бутылку",
                        "выставить ящик"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To treat someone at one's own expense (usually to alcoholic drinks)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "buy",
                                "target_lang_sense_clarification": "drinks or a bottle"
                            },
                            {
                                "target_lang_word": "treat",
                                "target_lang_sense_clarification": "pay for someone"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
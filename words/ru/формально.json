    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c58d7461-2aad-3bba-bef3-579afae1298f",
                    "sense_definition": "В соответствии с установленными правилами, нормами, этикетом или процедурами; официально.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "official_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы должны <w>формально</w> подтвердить своё участие в конференции письмом.",
                            "target_lang_translations": {
                                "en": "We must <w>formally</w> confirm our participation in the conference by letter."
                            }
                        },
                        {
                            "text": "Приветствие прозвучало слишком <w>формально</w> для старых друзей.",
                            "target_lang_translations": {
                                "en": "The greeting sounded too <w>formal</w> for old friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "официально"
                    ],
                    "antonyms": [
                        "неофициально",
                        "неформально"
                    ],
                    "common_phrases": [
                        "действовать формально",
                        "звучать формально",
                        "формально утвердить"
                    ],
                    "target_lang_definitions": {
                        "en": "In accordance with established rules, norms, etiquette, or procedures; officially."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "formally",
                                "target_lang_sense_clarification": "in an official or established manner"
                            },
                            {
                                "target_lang_word": "officially",
                                "target_lang_sense_clarification": "authorized or public"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1b2c147-a76e-35e1-984d-29e0be0df4f9",
                    "sense_definition": "Только внешне, по названию или на бумаге, но не соответствуя реальному положению дел.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "appearance_vs_reality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хотя <w>формально</w> они женаты, вместе они давно не живут.",
                            "target_lang_translations": {
                                "en": "Although <w>technically</w> they are married, they haven't lived together for a long time."
                            }
                        },
                        {
                            "text": "Этот закон существует лишь <w>формально</w> и никем не соблюдается.",
                            "target_lang_translations": {
                                "en": "This law exists only <w>on paper</w> and is observed by no one."
                            }
                        }
                    ],
                    "synonyms": [
                        "номинально",
                        "фиктивно"
                    ],
                    "antonyms": [
                        "неформально",
                        "неофициально"
                    ],
                    "common_phrases": [
                        "чисто формально",
                        "формально прав",
                        "формально независимый"
                    ],
                    "target_lang_definitions": {
                        "en": "Only outwardly, by name, or on paper, without corresponding to the real situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "technically",
                                "target_lang_sense_clarification": "strictly speaking, despite reality suggesting otherwise"
                            },
                            {
                                "target_lang_word": "nominally",
                                "target_lang_sense_clarification": "in name only"
                            },
                            {
                                "target_lang_word": "on paper",
                                "target_lang_sense_clarification": "in theory but not in practice"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d001ea1a-8db7-3e5a-845a-b2b95696f165",
                    "sense_definition": "Не вникая в суть дела, равнодушно, ограничиваясь лишь внешней стороной исполнения.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "superficial_attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чиновник отнёсся к моей просьбе <w>формально</w> и прислал стандартную отписку.",
                            "target_lang_translations": {
                                "en": "The official treated my request <w>perfunctorily</w> and sent a standard reply."
                            }
                        },
                        {
                            "text": "Ученик выполнил задание <w>формально</w>, не стараясь понять тему.",
                            "target_lang_translations": {
                                "en": "The student completed the assignment <w>mechanically</w>, without trying to understand the topic."
                            }
                        }
                    ],
                    "synonyms": [
                        "бюрократически"
                    ],
                    "common_phrases": [
                        "подойти формально",
                        "ответить формально",
                        "относиться формально"
                    ],
                    "target_lang_definitions": {
                        "en": "Without delving into the essence, indifferently, limiting oneself to the external aspect."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "perfunctorily",
                                "target_lang_sense_clarification": "done as a routine duty without interest"
                            },
                            {
                                "target_lang_word": "mechanically",
                                "target_lang_sense_clarification": "without thought or engagement"
                            }
                        ]
                    }
                }
            ]
        },
        {

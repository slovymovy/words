                    ],
                    "antonyms": [
                        "уронить",
                        "положить",
                        "опустить"
                    ],
                    "common_phrases": [
                        "поднять с пола",
                        "поднять трубку",
                        "поднять вещь"
                    ],
                    "target_lang_definitions": {
                        "en": "To take an object from the floor or another surface into one's hands."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pick up",
                                "target_lang_sense_clarification": "take from a surface"
                            },
                            {

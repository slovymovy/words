            "senses": [
                {
                    "sense_id": "08647f56-6499-374d-b226-cff6c7c1cbbc",
                    "sense_definition": "Een specifieke hoeveelheid of getal dat aangeeft hoeveel personen of dingen er in totaal zijn.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>aantal</w> bezoekers is dit jaar flink gestegen.",
                            "target_lang_translations": {
                                "en": "The <w>number</w> of visitors has risen sharply this year.",
                                "ru": "<w>Количество</w> посетителей в этом году значительно выросло."
                            }
                        },
                        {
                            "text": "We moeten het exacte <w>aantal</w> weten voor de reservering.",
                            "target_lang_translations": {
                                "en": "We need to know the exact <w>number</w> for the reservation.",
                                "ru": "Нам нужно знать точное <w>количество</w> для бронирования."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoeveelheid",
                        "getal"
                    ],
                    "common_phrases": [
                        "het aantal",
                        "groot aantal",
                        "klein aantal"
                    ],
                    "target_lang_definitions": {
                        "en": "A specific quantity or count indicating the total of people or things.",
                        "ru": "Конкретное количество или число, указывающее на общий итог людей или предметов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "number",
                                "target_lang_sense_clarification": "a specific count or total"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "количество",
                                "target_lang_sense_clarification": "определенное число людей или предметов"
                            },
                            {
                                "target_lang_word": "число",
                                "target_lang_sense_clarification": "численность, цифра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc168c69-a99d-34d6-9e38-2118679ef064",
                    "sense_definition": "Een onbepaalde hoeveelheid van iets; enkele of verscheidene.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb nog een <w>aantal</w> vragen over het huiswerk.",
                            "target_lang_translations": {
                                "en": "I still have a <w>number</w> of questions about the homework.",
                                "ru": "У меня есть еще <w>несколько</w> вопросов по домашнему заданию."
                            }
                        },
                        {
                            "text": "Er staan een <w>aantal</w> fietsen voor de deur geparkeerd.",
                            "target_lang_translations": {
                                "en": "There are a <w>number</w> of bicycles parked in front of the door.",
                                "ru": "Перед дверью припарковано <w>несколько</w> велосипедов."
                            }
                        },
                        {
                            "text": "Een <w>aantal</w> mensen was niet gekomen naar het feest.",
                            "target_lang_translations": {
                                "en": "A <w>number</w> of people did not come to the party.",
                                "ru": "<w>Несколько</w> человек не пришли на вечеринку."
                            }
                        }
                    ],
                    "synonyms": [
                        "enkele",
                        "verscheidene",
                        "wat",
                        "menige"
                    ],
                    "common_phrases": [
                        "een aantal",
                        "een flink aantal"
                    ],
                    "target_lang_definitions": {
                        "en": "An indefinite quantity of something; several or a few.",
                        "ru": "Неопределенное количество чего-либо; несколько предметов или людей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "number",
                                "target_lang_sense_clarification": "used in the phrase 'a number of' to mean several"
                            },
                            {
                                "target_lang_word": "several",
                                "target_lang_sense_clarification": "an indefinite number, more than two but not many"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "несколько",
                                "target_lang_sense_clarification": "неопределенное количество, больше двух, но не много"
                            },
                            {
                                "target_lang_word": "ряд",
                                "target_lang_sense_clarification": "группа или серия чего-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]

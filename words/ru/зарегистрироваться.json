    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4169c845-ba3d-3316-b83e-fd7c34f3db7f",
                    "sense_definition": "Внести свои личные данные в список, базу данных или систему учёта для получения доступа, прав или подтверждения присутствия.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пассажиры должны <зарегистрироваться> на рейс за два часа до вылета.",
                            "target_lang_translations": {
                                "en": "Passengers must <check in> for the flight two hours before departure."
                            }
                        },
                        {
                            "text": "Чтобы оставить комментарий, вам нужно <зарегистрироваться> на сайте.",
                            "target_lang_translations": {
                                "en": "To leave a comment, you need to <sign up> on the website."
                            }
                        }
                    ],
                    "synonyms": [
                        "вступить в брак"
                    ],
                    "antonyms": [
                        "выписаться"
                    ],
                    "common_phrases": [
                        "зарегистрироваться на сайте",
                        "зарегистрироваться в отеле",
                        "зарегистрироваться на рейс",
                        "зарегистрироваться по адресу"
                    ],
                    "target_lang_definitions": {
                        "en": "To enter one's personal details into a list, database, or record system to gain access, rights, or confirm presence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "general sense"
                            },
                            {
                                "target_lang_word": "sign up",
                                "target_lang_sense_clarification": "for a website, service, or account"
                            },
                            {
                                "target_lang_word": "check in",
                                "target_lang_sense_clarification": "for a flight or hotel"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd3945aa-fbbc-3e60-9ac0-c11eeec2c1d5",
                    "sense_definition": "Официально оформить брачный союз в государственных органах.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Они решили просто <зарегистрироваться> и не устраивать пышную свадьбу.",
                            "target_lang_translations": {
                                "en": "They decided to just <get married> (at the registry office) and not have a lavish wedding."
                            }
                        },
                        {
                            "text": "Мои друзья планируют <зарегистрироваться> в следующем месяце.",
                            "target_lang_translations": {
                                "en": "My friends plan to <get married> next month."
                            }
                        }
                    ],
                    "synonyms": [
                        "вступить в брак"
                    ],
                    "antonyms": [
                        "развестись"
                    ],
                    "common_phrases": [
                        "пойти зарегистрироваться",
                        "решили зарегистрироваться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To officially record a marriage union with government authorities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get married",
                                "target_lang_sense_clarification": "officially (at a registry office)"
                            },
                            {
                                "target_lang_word": "register one's marriage",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02adfa65-8fb9-3e49-9681-a580214a03d7",
                    "sense_definition": "Быть зафиксированным или отмеченным в документах или системе (обычно о событиях, фактах или явлениях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В системном журнале <зарегистрировалась> критическая ошибка.",
                            "target_lang_translations": {
                                "en": "A critical error <was logged> in the system journal."
                            }
                        },
                        {
                            "text": "Сигнал от датчика успешно <зарегистрировался> в базе данных.",
                            "target_lang_translations": {
                                "en": "The signal from the sensor was successfully <registered> in the database."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be recorded or noted in documents or a system (usually regarding events, facts, or phenomena)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be registered",
                                "target_lang_sense_clarification": "passive state of being recorded"
                            },
                            {
                                "target_lang_word": "be logged",
                                "target_lang_sense_clarification": "computing/technical context"
                            }
                        ]
                    }
                }
            ]
        }
    ]

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "38669769-fe06-3526-80db-a38c560d77e6",
                    "sense_definition": "Een kaartje met een chip dat in een mobiele telefoon wordt geplaatst om verbinding te maken met het netwerk.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet een nieuwe <w>sim</w> kopen omdat mijn oude beschadigd is.",
                            "target_lang_translations": {
                                "en": "I need to buy a new <w>SIM</w> because my old one is damaged.",
                                "ru": "Мне нужно купить новую <w>сим-карту</w>, потому что старая повреждена."
                            }
                        },
                        {
                            "text": "Zonder een geldige <w>sim</w> kun je met dit toestel niet bellen.",
                            "target_lang_translations": {
                                "en": "Without a valid <w>SIM</w>, you cannot make calls with this device.",
                                "ru": "Без действующей <w>сим-карты</w> с этого устройства нельзя звонить."
                            }
                        }
                    ],
                    "synonyms": [
                        "simkaart"
                    ],
                    "common_phrases": [
                        "nieuwe sim",
                        "sim plaatsen",
                        "geblokkeerde sim"
                    ],
                    "target_lang_definitions": {
                        "en": "A small card with a chip placed in a mobile phone to connect to the network.",
                        "ru": "Маленькая карта с чипом, вставляемая в мобильный телефон для подключения к сети."
                    },
                    "translations": {

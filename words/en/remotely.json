    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "64768825-72b0-3b5b-b86b-00fe39656d91",
                    "sense_definition": "At a distance; from a location far away, often used regarding work or technology.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many employees now prefer to work <w>remotely</w> rather than commute to the office.",
                            "target_lang_translations": {
                                "ru": "Многие сотрудники теперь предпочитают работать <w>удалённо</w>, а не ездить в офис."
                            }
                        },
                        {
                            "text": "You can access the security cameras <w>remotely</w> using an application on your phone.",
                            "target_lang_translations": {
                                "ru": "Вы можете получить доступ к камерам наблюдения <w>дистанционно</w> с помощью приложения на телефоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "distantly",
                        "afar"
                    ],
                    "antonyms": [
                        "locally",
                        "nearby",
                        "close"
                    ],
                    "common_phrases": [
                        "work remotely",
                        "access remotely",
                        "control remotely",
                        "operate remotely"
                    ],
                    "target_lang_definitions": {
                        "ru": "На расстоянии; из другого места (часто используется в контексте работы или технологий)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удалённо",
                                "target_lang_sense_clarification": "о режиме работы или доступе"
                            },
                            {
                                "target_lang_word": "дистанционно",
                                "target_lang_sense_clarification": "об управлении или обучении"
                            },
                            {
                                "target_lang_word": "на расстоянии",
                                "target_lang_sense_clarification": "физически далеко"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f56e899-83fc-36f6-b7ec-cb46f621ec8b",
                    "sense_definition": "To a very small degree; slightly or vaguely (often used in negative statements).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am not <w>remotely</w> interested in watching that boring movie again.",
                            "target_lang_translations": {
                                "ru": "Мне <w>ничуть</w> не интересно снова смотреть этот скучный фильм."
                            }
                        },
                        {
                            "text": "Is it even <w>remotely</w> possible that we made a mistake in the calculation?",
                            "target_lang_translations": {
                                "ru": "Возможно ли <w>хоть отдалённо</w>, что мы допустили ошибку в расчетах?"
                            }
                        }
                    ],
                    "synonyms": [
                        "slightly",
                        "faintly",
                        "vaguely",
                        "scarcely"
                    ],
                    "antonyms": [
                        "completely",
                        "totally",
                        "entirely"
                    ],
                    "common_phrases": [
                        "not remotely",
                        "remotely possible",
                        "remotely similar",
                        "remotely interested"
                    ],
                    "target_lang_definitions": {
                        "ru": "В очень малой степени; слегка или смутно (часто используется в отрицательных утверждениях)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничуть",
                                "target_lang_sense_clarification": "совсем не (в отрицаниях)"
                            },
                            {
                                "target_lang_word": "отдалённо",
                                "target_lang_sense_clarification": "смутно, приблизительно"
                            },
                            {
                                "target_lang_word": "хоть сколько-нибудь",
                                "target_lang_sense_clarification": "в малейшей степени"
                            }
                        ]
                    }
                }
            ]
        }
    ]

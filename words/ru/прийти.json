{
    "word_family": [
        "приход",
        "пришествие"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a9a45405-f60f-35f2-8e9b-b842cb1857b5",
                    "sense_definition": "Добраться до какого-либо места, появиться где-либо (пешком или на транспорте).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обещаю <прийти> домой сегодня пораньше."
                        },
                        {
                            "text": "Гости должны <прийти> к нам на ужин в семь часов."
                        }
                    ],
                    "synonyms": [
                        "прибыть"
                    ],
                    "antonyms": [
                        "уйти"
                    ],
                    "common_phrases": [
                        "прийти домой",
                        "прийти в школу",
                        "прийти на помощь",
                        "прийти вовремя",
                        "прийти в гости"
                    ]
                },
                {
                    "sense_id": "af86009a-7ca4-3e4c-a09f-6cc5d55e5837",
                    "sense_definition": "Наступить, начаться (о времени, событиях или природных явлениях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наконец <пришла> долгожданная весна, и снег растаял."
                        },
                        {
                            "text": "Я знал, что однажды <придёт> время для этого важного разговора."
                        }
                    ],
                    "synonyms": [
                        "наступить",
                        "настать"
                    ],
                    "antonyms": [
                        "закончиться"
                    ],
                    "common_phrases": [
                        "пришло время",
                        "пришла весна",
                        "пришла зима",
                        "пришёл конец"
                    ]
                },
                {
                    "sense_id": "f211532f-f4cf-3de6-aeb0-c98da52883b3",
                    "sense_definition": "Оказаться в каком-либо состоянии, настроении или достичь какого-либо результата (обычно с предлогами «в» или «к»).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгого обморока пациент наконец <пришёл> в себя."
                        },
                        {
                            "text": "Мы обсудили проблему и <пришли> к общему мнению."
                        }
                    ],
                    "common_phrases": [
                        "прийти в себя",
                        "прийти к выводу",
                        "прийти в восторг",
                        "прийти в негодность",
                        "прийти в ярость"
                    ]
                }
            ]
        }
    ]

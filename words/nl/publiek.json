            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8567dd6e-1e2f-36eb-927e-b18e1f406b71",
                    "sense_definition": "Een groep mensen die kijkt en luistert naar een voorstelling, wedstrijd of evenement.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>publiek</w> klapte enthousiast na het einde van het concert.",
                            "target_lang_translations": {
                                "ru": "<w>Публика</w> восторженно аплодировала после окончания концерта."
                            }
                        },
                        {
                            "text": "Tijdens de spannende wedstrijd moedigde het <w>publiek</w> de spelers luidkeels aan.",
                            "target_lang_translations": {
                                "ru": "Во время захватывающего матча <w>публика</w> громко подбадривала игроков."
                            }
                        }
                    ],
                    "synonyms": [
                        "openbaar",
                        "openbaar"
                    ],
                    "common_phrases": [
                        "groot publiek",
                        "enthousiast publiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, которые смотрят и слушают представление, матч или другое мероприятие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публика",
                                "target_lang_sense_clarification": "зрители, слушатели"
                            },
                            {
                                "target_lang_word": "зрители",
                                "target_lang_sense_clarification": "наблюдатели"
                            },
                            {
                                "target_lang_word": "аудитория",
                                "target_lang_sense_clarification": "слушатели, зал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60edae66-7236-3080-8665-2a01bad8902f",
                    "sense_definition": "Mensen in het algemeen; bezoekers die niet specifiek zijn uitgenodigd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het museum is vanaf volgende week weer geopend voor <w>publiek</w>.",
                            "target_lang_translations": {
                                "ru": "С следующей недели музей снова открыт для <w>публики</w>."
                            }
                        },
                        {
                            "text": "Dit ingewikkelde boek is niet echt geschikt voor het grote <w>publiek</w>.",
                            "target_lang_translations": {
                                "ru": "Эта сложная книга не очень подходит для широкой <w>публики</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "voor publiek geopend",
                        "het grote publiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди в целом; посетители, которые не были специально приглашены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публика",
                                "target_lang_sense_clarification": "люди, общество"
                            },
                            {
                                "target_lang_word": "посетители",
                                "target_lang_sense_clarification": "гости заведения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "70697e6b-1085-30ed-9438-7f07ede8a2b2",
                    "sense_definition": "Voor iedereen toegankelijk en niet privé; openbaar.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "public_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het park is een <w>publieke</w> ruimte waar iedereen vrij mag wandelen.",
                            "target_lang_translations": {
                                "ru": "Парк — это <w>общественное</w> пространство, где каждый может свободно гулять."
                            }
                        },
                        {
                            "text": "Hij durfde zijn mening niet te geven tijdens de <w>publieke</w> bijeenkomst.",
                            "target_lang_translations": {
                                "ru": "Он не осмелился высказать свое мнение во время <w>публичного</w> собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "openbaar"
                    ],
                    "antonyms": [
                        "privé"
                    ],
                    "common_phrases": [
                        "publieke ruimte",
                        "publiek domein"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доступный для всех, не частный; открытый для посещения."
                    },
                    "translations": {

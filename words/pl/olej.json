            "pos": "noun",
            "senses": [
                {
                    "sense_id": "89fe4324-7aa5-39c3-9e23-8509a4402ddd",
                    "sense_definition": "Płynny tłuszcz pochodzenia roślinnego, używany w kuchni do smażenia, pieczenia i jako dodatek do potraw.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_oil",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić <olej> rzepakowy, bo chcę usmażyć frytki na obiad.",
                            "target_lang_translations": {
                                "ru": "Я должен купить рапсовое <масло>, потому что хочу пожарить картошку фри на обед."
                            }
                        },
                        {
                            "text": "Do sałatki ze świeżych warzyw dodaj odrobinę octu i <oleju>.",
                            "target_lang_translations": {
                                "ru": "В салат из свежих овощей добавь немного уксуса и <масла>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "olej rzepakowy",
                        "olej słonecznikowy",
                        "smażyć na oleju",
                        "olej roślinny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкий жир растительного происхождения, используемый на кухне для жарки, запекания и как добавка к блюдам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растительное масло",
                                "target_lang_sense_clarification": "пищевой продукт"
                            },
                            {
                                "target_lang_word": "масло",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7927c6ce-c448-33f9-a441-398a633a774f",
                    "sense_definition": "Substancja smarująca lub paliwo uzyskiwane z ropy naftowej, używane w silnikach i maszynach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_oil",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mechanik powiedział, że trzeba wymienić <olej> silnikowy w moim samochodzie.",
                            "target_lang_translations": {
                                "ru": "Механик сказал, что нужно поменять моторное <масло> в моей машине."
                            }
                        },
                        {
                            "text": "Na podjeździe przed garażem zauważyłem dużą plamę <oleju>.",
                            "target_lang_translations": {
                                "ru": "На подъездной дорожке перед гаражом я заметил большое пятно <масла>."
                            }
                        }
                    ],
                    "synonyms": [
                        "olej mineralny"
                    ],
                    "common_phrases": [
                        "olej silnikowy",
                        "wymiana oleju",
                        "olej napędowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смазочное вещество или топливо, получаемое из нефти, используемое в двигателях и машинах."
                    },
                    "translations": {
                        "ru": [
                            {

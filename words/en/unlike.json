                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_media_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I decided to <w>unlike</w> the page after seeing the offensive content.",
                            "target_lang_translations": {
                                "ru": "Я решил <w>убрать лайк</w> со страницы после того, как увидел оскорбительный контент."
                            }
                        },
                        {
                            "text": "She accidentally <w>unliked</w> the photo while scrolling through her feed.",
                            "target_lang_translations": {
                                "ru": "Она случайно <w>убрала лайк</w> с фотографии, пролистывая ленту."
                            }
                        }
                    ],
                    "antonyms": [

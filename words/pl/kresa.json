            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aa8ec262-146a-3740-a35b-2856017ba663",
                    "sense_definition": "Długa, gruba linia lub pasmo, często występujące w kontekście anatomicznym lub biologicznym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "line_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To ten koń z wyraźną, czarną <w>kresą</w> biegnącą wzdłuż grzbietu.",
                            "target_lang_translations": {
                                "ru": "Это та лошадь с отчётливой чёрной <w>полосой</w>, бегущей вдоль спины."
                            }
                        },
                        {
                            "text": "Chirurg wykonał precyzyjne nacięcie wzdłuż <w>kresy</w> białej brzucha.",
                            "target_lang_translations": {
                                "ru": "Хирург сделал точный разрез вдоль белой <w>линии</w> живота."
                            }
                        }
                    ],
                    "synonyms": [
                        "krecha"
                    ],
                    "common_phrases": [
                        "kresa biała",
                        "kresa czarna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная, толстая линия или полоса, часто встречающаяся в анатомическом или биологическом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "линия",
                                "target_lang_sense_clarification": "анатомический термин (напр. белая линия живота)"
                            },
                            {
                                "target_lang_word": "полоса",
                                "target_lang_sense_clarification": "на шерсти животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4388205e-3439-3f07-aa8c-398432dfabea",
                    "sense_definition": "Ślad na skórze pozostały po zagojonej ranie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "scar_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na jego policzku widniała długa, czerwonawa <w>kresa</w> po dawnym pojedynku.",
                            "target_lang_translations": {
                                "ru": "На его щеке виднелся длинный красноватый <w>шрам</w> от давней дуэли."
                            }
                        },
                        {
                            "text": "Stara <w>kresa</w> na dłoni przypominała mu o wypadku z dzieciństwa.",
                            "target_lang_translations": {
                                "ru": "Старый <w>шрам</w> на ладони напоминал ему о происшествии из детства."
                            }
                        }
                    ],
                    "synonyms": [
                        "stójka",
                        "krecha"
                    ],
                    "target_lang_definitions": {
                        "ru": "След на коже, оставшийся после зажившей раны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шрам",
                                "target_lang_sense_clarification": "след от раны"
                            },
                            {
                                "target_lang_word": "рубец",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12c98a58-3c57-3186-9988-6ff93ffef4ce",
                    "sense_definition": "Jednodniowa służba lub kolej na posyłki w urzędzie wójta gminy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miejscowy chłop został wezwany, aby odbyć swoją <w>kresę</w> w gminie.",
                            "target_lang_translations": {
                                "ru": "Местного крестьянина вызвали отбыть своё <w>дежурство</w> в гмине."
                            }
                        },
                        {
                            "text": "Wójt wyznaczył go na <w>kresę</w>, więc nie mógł pracować tego dnia w polu.",
                            "target_lang_translations": {
                                "ru": "Войт назначил его на <w>дежурство</w>, поэтому он не мог работать в тот день в поле."
                            }
                        }
                    ],
                    "synonyms": [
                        "stójka"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Augustów"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Однодневная служба или очередь на посылки в канцелярии войта гмины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дежурство",
                                "target_lang_sense_clarification": "обязанность, служба"
                            },
                            {
                                "target_lang_word": "повинность",
                                "target_lang_sense_clarification": "обязательная работа"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "083e7061-06c5-3df2-947e-2b890adabbbb",
                    "sense_definition": "Zachtjes, soepel en regelmatig heen en weer bewegend.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De danseres bewoog met <w>wiegende</w> heupen op het ritme van de muziek.",
                            "target_lang_translations": {
                                "en": "The dancer moved with <w>swaying</w> hips to the rhythm of the music.",
                                "ru": "Танцовщица двигалась с <w>покачивающимися</w> бедрами в ритм музыки."
                            }
                        },
                        {
                            "text": "We genoten van het uitzicht op de zachtjes <w>wiegende</w> bomen in de wind.",
                            "target_lang_translations": {
                                "en": "We enjoyed the view of the gently <w>swaying</w> trees in the wind.",
                                "ru": "Мы наслаждались видом тихо <w>колышущихся</w> на ветру деревьев."
                            }
                        }
                    ],
                    "synonyms": [
                        "schommelend",
                        "deinend"
                    ],
                    "antonyms": [
                        "stilstaand"
                    ],
                    "common_phrases": [
                        "wiegende heupen",
                        "wiegende beweging",
                        "zachtjes wiegend"
                    ],
                    "target_lang_definitions": {
                        "en": "Moving back and forth gently, smoothly, and regularly.",
                        "ru": "Плавно, мягко и ритмично двигающийся из стороны в сторону."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swaying",
                                "target_lang_sense_clarification": "movement like hips or trees in wind"
                            },
                            {
                                "target_lang_word": "rocking",
                                "target_lang_sense_clarification": "rhythmic back and forth motion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "покачивающийся",
                                "target_lang_sense_clarification": "ритмично двигающийся из стороны в сторону"
                            },
                            {
                                "target_lang_word": "колышущийся",
                                "target_lang_sense_clarification": "плавно движущийся (о деревьях, траве)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ca3bd274-050b-38f2-9799-e845149a262a",
                    "sense_definition": "Het *onvoltooid deelwoord* van <w>wiegen</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De moeder zat <w>wiegend</w> in haar stoel om de baby te kalmeren.",
                            "target_lang_translations": {
                                "en": "The mother sat <w>rocking</w> in her chair to soothe the baby.",
                                "ru": "Мать сидела, <w>раскачиваясь</w> в кресле, чтобы успокоить ребенка."
                            }
                        },
                        {
                            "text": "De kleine boot lag <w>wiegend</w> op het water van het meer.",
                            "target_lang_translations": {
                                "en": "The small boat lay <w>rocking</w> on the water of the lake.",
                                "ru": "Маленькая лодка, <w>качаясь</w>, лежала на воде озера."
                            }
                        }
                    ],

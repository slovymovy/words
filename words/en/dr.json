        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "be3730de-2c7d-3c24-ac39-0f738be974bc",
                    "sense_definition": "Abbreviation of <w>doctor</w>; a title used before the name of a medical practitioner or a person holding a doctorate.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "title_abbrev",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "I have an appointment with <w>Dr</w> Smith at the clinic tomorrow.",
                            "target_lang_translations": {
                                "ru": "У меня завтра прием у <w>доктора</w> Смита в клинике."
                            }
                        },
                        {
                            "text": "Please welcome <w>Dr</w> Jones to the stage for her presentation.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поприветствуйте <w>доктора</w> Джонс на сцене для презентации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Dr Smith",
                        "dear Dr"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от слова \"доктор\"; титул, используемый перед именем врача или человека, имеющего докторскую степень."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "д-р",
                                "target_lang_sense_clarification": "письменное сокращение"
                            },
                            {
                                "target_lang_word": "доктор",
                                "target_lang_sense_clarification": "обращение или титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dffc8cca-f381-395c-897d-70e75be857a4",
                    "sense_definition": "Abbreviation of <w>drive</w>; commonly used in street names and postal addresses.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "street_abbrev",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The package was delivered to 123 Maple <w>Dr</w> yesterday.",
                            "target_lang_translations": {
                                "ru": "Посылка была доставлена вчера на Мейпл-<w>драйв</w>, 123."
                            }
                        },
                        {
                            "text": "Turn left onto Riverside <w>Dr</w> and continue for two miles.",
                            "target_lang_translations": {
                                "ru": "Поверните налево на Риверсайд-<w>драйв</w> и продолжайте движение две мили."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Ocean Dr",
                        "Sunset Dr"

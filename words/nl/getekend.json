                    ],
                    "common_phrases": [
                        "getekend gezicht",
                        "door het leven getekend"
                    ],
                    "target_lang_definitions": {
                        "en": "Visibly damaged or aged by worries, illness, or heavy experiences.",
                        "ru": "С видимыми следами старения или страданий, вызванными заботами, болезнью или тяжелыми переживаниями."
                    },
                    "translations": {
                        "en": [
                            {

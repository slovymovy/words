            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8ac3892c-fe24-31fa-935d-595b0691a9de",
                    "sense_definition": "zdrobnienie od słowa <w>żuraw</w>; mały lub młody ptak z rodziny żurawiowatych",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bird_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wysokiej trawie ukrył się mały <w>żurawik</w>, czekając na powrót rodziców.",
                            "target_lang_translations": {
                                "ru": "В высокой траве спрятался маленький <w>журавлик</w>, ожидая возвращения родителей."
                            }
                        },
                        {
                            "text": "Ten <w>żurawik</w> nie potrafi jeszcze latać na długie dystanse.",
                            "target_lang_translations": {
                                "ru": "Этот <w>журавлик</w> еще не умеет летать на дальние расстояния."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "уменьшительно-ласкательная форма от слова \"журавль\"; маленькая или молодая птица семейства журавлиных"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "журавлик",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71a2ed5b-f20f-35b1-98e8-a8b25afc2401",
                    "sense_definition": "zdrobnienie od słowa <w>żuraw</w>; niewielkie urządzenie dźwigowe używane do podnoszenia lżejszych ładunków",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "machine_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na placu budowy zamontowano mały <w>żurawik</w> do transportu cegieł na piętro.",
                            "target_lang_translations": {
                                "ru": "На строительной площадке установили <w>малый кран</w> для подъема кирпичей на этаж."
                            }
                        },
                        {
                            "text": "Operator obsługiwał <w>żurawik</w> warsztatowy, aby podnieść silnik samochodowy.",
                            "target_lang_translations": {
                                "ru": "Оператор управлял гаражным <w>краном</w>, чтобы поднять автомобильный двигатель."
                            }
                        }
                    ],
                    "common_phrases": [
                        "żurawik warsztatowy",
                        "żurawik budowlany"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "небольшое подъемное устройство, используемое для подъема легких грузов; уменьшительное от \"подъемный кран\""
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "малый кран",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "кран-укосина",
                                "target_lang_sense_clarification": "тип конструкции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d59fd118-5cba-37e6-96d2-7592549f0801",
                    "sense_definition": "urządzenie na statku służące do opuszczania i podnoszenia łodzi ratunkowych, kotwic lub ładunku",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Załoga sprawdziła <w>żurawik</w> szalupowy przed wypłynięciem w rejs.",
                            "target_lang_translations": {
                                "ru": "Экипаж проверил <w>шлюпбалку</w> перед выходом в рейс."
                            }
                        },
                        {
                            "text": "Ciężka kotwica została wciągnięta na pokład przez <w>żurawik</w> dziobowy.",
                            "target_lang_translations": {
                                "ru": "Тяжелый якорь был поднят на палубу через носовую <w>кран-балку</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "żurawik szalupowy",
                        "żurawik kotwiczny",
                        "żurawik burtowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "устройство на судне для спуска и подъема спасательных шлюпок, якорей или грузов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлюпбалка",
                                "target_lang_sense_clarification": "для шлюпок"
                            },
                            {
                                "target_lang_word": "кран-балка",
                                "target_lang_sense_clarification": "грузовое устройство"
                            }
                        ]
                    }
                }
            ]
        },

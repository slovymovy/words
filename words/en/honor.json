                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_award",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee <w>honored</w> him with a lifetime achievement award.",
                            "target_lang_translations": {
                                "ru": "Комитет <w>удостоил</w> его награды за жизненные достижения."
                            }

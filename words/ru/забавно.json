    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "41cbefc0-8716-3420-a24a-331e0f4a1bfc",
                    "sense_definition": "Оценка ситуации как смешной, интересной или способной развлечь.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "predicative_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне <w>забавно</w> видеть, как он пытается быть серьезным.",
                            "target_lang_translations": {
                                "en": "It is <w>amusing</w> for me to see how he tries to be serious."
                            }
                        },
                        {
                            "text": "Это <w>забавно</w>, что мы встретились именно здесь.",
                            "target_lang_translations": {
                                "en": "It is <w>funny</w> that we met right here."
                            }
                        }
                    ],
                    "synonyms": [
                        "смешно",
                        "потешно",
                        "увлекательно"
                    ],
                    "antonyms": [
                        "скучно",
                        "грустно",
                        "уныло"
                    ],
                    "common_phrases": [
                        "это забавно",
                        "довольно забавно",
                        "мне забавно"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to describe a situation as funny, interesting, or entertaining."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "funny",
                                "target_lang_sense_clarification": "causing laughter or amusement"
                            },
                            {
                                "target_lang_word": "amusing",
                                "target_lang_sense_clarification": "providing entertainment or fun"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14556437-cbf9-3a13-bf0b-9d3cbb3c378d",
                    "sense_definition": "Смешно, потешно; так, что это вызывает веселье или улыбку.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "manner_adverb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она умела очень <w>забавно</w> рассказывать истории о своих путешествиях.",
                            "target_lang_translations": {
                                "en": "She knew how to tell stories about her travels very <w>amusingly</w>."
                            }
                        },
                        {
                            "text": "Щенок <w>забавно</w> наклонил голову набок, глядя на хозяина.",
                            "target_lang_translations": {
                                "en": "The puppy <w>amusingly</w> tilted its head to the side, looking at the owner."
                            }
                        }
                    ],
                    "synonyms": [
                        "смешно",
                        "потешно"
                    ],
                    "antonyms": [
                        "скучно",
                        "уныло"
                    ],
                    "common_phrases": [
                        "выглядит забавно",
                        "звучит забавно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a funny or amusing manner; in a way that causes mirth or a smile."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "amusingly",
                                "target_lang_sense_clarification": "in an entertaining manner"
                            },
                            {
                                "target_lang_word": "funnily",
                                "target_lang_sense_clarification": "in a humorous way"
                            }
                        ]
                    }
                }
            ]
        },

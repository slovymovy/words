                        "portvrij"
                    ],
                    "common_phrases": [
                        "franco huis",
                        "franco levering"
                    ],
                    "target_lang_definitions": {
                        "ru": "С оплатой почтовых расходов отправителем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с оплаченной доставкой",
                                "target_lang_sense_clarification": "общая лексика"
                            },
                            {
                                "target_lang_word": "франко",
                                "target_lang_sense_clarification": "термин логистики"

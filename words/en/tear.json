                        {
                            "text": "They plan to <w>tear</w> down the old garage next week.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>снести</w> старый гараж на следующей неделе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tear down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушать или сносить здание или сооружение."

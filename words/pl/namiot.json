{
    "word_family": [
        "namiotowy",
        "namiocik"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c6cc99c-bc77-3b37-9fd7-f65100fa31a4",
                    "sense_definition": "Przenośne schronienie z tkaniny rozpinanej na stelażu, używane do spania na dworze, na przykład podczas wakacji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Postanowiliśmy spędzić urlop pod <w>namiotem</w> nad jeziorem.",
                            "target_lang_translations": {
                                "ru": "Мы решили провести отпуск в <w>палатке</w> у озера."
                            }
                        },
                        {
                            "text": "Musimy szybko rozbić <w>namiot</w>, zanim zacznie padać deszcz.",
                            "target_lang_translations": {
                                "ru": "Мы должны быстро разбить <w>палатку</w>, пока не начался дождь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rozbić namiot",
                        "spać pod namiotem",
                        "pole namiotowe",
                        "składać namiot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переносное укрытие из ткани, натянутой на каркас, используемое для сна на открытом воздухе, например, во время отдыха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "палатка",
                                "target_lang_sense_clarification": "туристическая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79b7050a-c20c-39a3-80cb-b8922e95efb6",
                    "sense_definition": "Konstrukcja pokryta folią, służąca do uprawy roślin wymagających ciepła.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek uprawia pomidory w dużym <w>namiocie</w> na działce.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка выращивает помидоры в большой <w>теплице</w> на даче."
                            }
                        },
                        {
                            "text": "Silny wiatr zniszczył foliowy <w>namiot</w> z warzywami.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер уничтожил плёночную <w>теплицу</w> с овощами."
                            }
                        }
                    ],
                    "synonyms": [
                        "tunel",
                        "foliak",
                        "foliówka"
                    ],
                    "common_phrases": [
                        "namiot foliowy",
                        "uprawa pod namiotem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конструкция, покрытая плёнкой, предназначенная для выращивания теплолюбивых растений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "теплица",
                                "target_lang_sense_clarification": "для выращивания растений"
                            },
                            {
                                "target_lang_word": "парник",
                                "target_lang_sense_clarification": "небольшое укрытие для растений"
                            }
                        ]
                    }
                }
            ]
        }
    ]

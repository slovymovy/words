                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baby cried loudly all <w>through</w> the night.",
                            "target_lang_translations": {
                                "ru": "Ребенок громко плакал <w>всю</w> ночь."
                            }

{
    "word_family": [
        "rush",
        "rusher",
        "rushy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d76baf0-4985-3599-99c9-9c3549c45660",
                    "sense_definition": "Simple past and past participle of <w>rush</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>rushed</w> out of the house to catch the bus on time.",
                            "target_lang_translations": {
                                "ru": "Он <w>выскочил</w> из дома, чтобы вовремя успеть на автобус."
                            }
                        },
                        {
                            "text": "The ambulance <w>rushed</w> the injured man to the nearest hospital.",
                            "target_lang_translations": {
                                "ru": "Скорая помощь <w>экстренно доставила</w> раненого в ближайшую больницу."
                            }
                        }
                    ],
                    "synonyms": [
                        "hurried",
                        "raced",
                        "dashed",
                        "scurried"
                    ],
                    "antonyms": [
                        "dawdled",
                        "crawled",
                        "delayed"
                    ],
                    "common_phrases": [
                        "rushed to",
                        "rushed into",
                        "rushed out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола rush (мчаться, торопиться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поспешил",
                                "target_lang_sense_clarification": "сделал что-то быстро из-за нехватки времени"
                            },
                            {
                                "target_lang_word": "помчался",
                                "target_lang_sense_clarification": "очень быстро побежал или поехал"
                            },
                            {
                                "target_lang_word": "бросился",
                                "target_lang_sense_clarification": "резко устремился куда-то"
                            },
                            {
                                "target_lang_word": "экстренно доставил",
                                "target_lang_sense_clarification": "быстро перевез кого-то (например, в больницу)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e076cb69-40b5-3f25-a2c6-1b05d0d4cade",
                    "sense_definition": "Done too quickly or hastily, often resulting in poor quality or mistakes.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a <w>rushed</w> decision that we later regretted deeply.",
                            "target_lang_translations": {
                                "ru": "Это было <w>поспешное</w> решение, о котором мы позже глубоко пожалели."
                            }
                        },
                        {
                            "text": "The report looked <w>rushed</w> and contained several spelling errors.",
                            "target_lang_translations": {
                                "ru": "Отчет выглядел <w>сделанным наспех</w> и содержал несколько орфографических ошибок."
                            }
                        }
                    ],
                    "synonyms": [
                        "hasty",
                        "hurried",
                        "precipitate"
                    ],
                    "antonyms": [
                        "deliberate",
                        "thorough",
                        "careful",
                        "unhurried"
                    ],
                    "common_phrases": [
                        "rushed decision",
                        "rushed job",
                        "rushed conclusion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный слишком быстро или поспешно, часто с потерей качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поспешный",
                                "target_lang_sense_clarification": "принятый или сделанный слишком быстро"
                            },
                            {
                                "target_lang_word": "сделанный наспех",
                                "target_lang_sense_clarification": "выполненный небрежно из-за спешки"
                            },
                            {
                                "target_lang_word": "торопливый",
                                "target_lang_sense_clarification": "свойственный спешке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ee4c03d-3e18-3c54-8b42-7ded1fad48ff",
                    "sense_definition": "Feeling that there is not enough time to do everything; very busy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I felt so <w>rushed</w> this morning that I forgot to eat breakfast.",
                            "target_lang_translations": {
                                "ru": "Я так <w>спешил</w> сегодня утром, что забыл позавтракать."
                            }
                        },
                        {
                            "text": "Don't feel <w>rushed</w>; take all the time you need to finish.",
                            "target_lang_translations": {
                                "ru": "Не <w>торопись</w>; потрать столько времени, сколько нужно, чтобы закончить."
                            }
                        }
                    ],
                    "synonyms": [
                        "busy",
                        "pressed",
                        "harried",
                        "hectic"
                    ],
                    "antonyms": [
                        "relaxed",
                        "calm",
                        "leisurely"
                    ],
                    "common_phrases": [
                        "feel rushed",
                        "rushed off one's feet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий нехватку времени; очень занятой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в спешке",
                                "target_lang_sense_clarification": "состояние нехватки времени"
                            },
                            {
                                "target_lang_word": "загнанный",
                                "target_lang_sense_clarification": "разг., очень занятой и уставший"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efc0bd4c-e80e-3bfb-8024-0a5df7003aad",
                    "sense_definition": "Abounding or covered with rushes (plants found in wet areas).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>rushed</w> banks of the river provided shelter for the waterfowl.",
                            "target_lang_translations": {
                                "ru": "<w>Поросшие камышом</w> берега реки служили укрытием для водоплавающих птиц."
                            }
                        },
                        {
                            "text": "We walked carefully through the <w>rushed</w> marshland.",
                            "target_lang_translations": {
                                "ru": "Мы осторожно шли через <w>заросшее тростником</w> болото."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поросший камышом или тростником (растениями, встречающимися во влажных местах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поросший камышом",
                                "target_lang_sense_clarification": "покрытый водными растениями"
                            },
                            {
                                "target_lang_word": "заросший тростником",
                                "target_lang_sense_clarification": "обильный тростником"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
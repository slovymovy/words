                    ],
                    "common_phrases": [
                        "kostka lodu",
                        "kostka mydła",
                        "kostka masła",
                        "kostka cukru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет в форме куба или параллелепипеда, часто являющийся порцией какого-либо вещества."

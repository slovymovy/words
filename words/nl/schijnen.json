    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0a2e5b02-734e-39f8-b5d9-539d3eeca408",
                    "sense_definition": "Licht geven of uitstralen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sense_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vandaag <w>schijnt</w> de zon de hele dag fel.",
                            "target_lang_translations": {
                                "en": "Today the sun is <w>shining</w> brightly all day.",
                                "ru": "Сегодня солнце ярко <w>светит</w> весь день."
                            }
                        },
                        {
                            "text": "De maan <w>scheen</w> door de bomen in het donkere bos.",
                            "target_lang_translations": {
                                "en": "The moon <w>shone</w> through the trees in the dark forest.",
                                "ru": "Луна <w>светила</w> сквозь деревья в темном лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "stralen"
                    ],
                    "common_phrases": [
                        "de zon schijnt"
                    ],
                    "target_lang_definitions": {
                        "en": "To give off or radiate light.",
                        "ru": "Излучать свет или сиять."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shine",
                                "target_lang_sense_clarification": "to emit light"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "светить",
                                "target_lang_sense_clarification": "излучать свет"
                            },
                            {
                                "target_lang_word": "сиять",
                                "target_lang_sense_clarification": "ярко светить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a156670-3847-3579-b8ab-95a903299bcd",
                    "sense_definition": "De indruk wekken of volgens informatie van anderen zo zijn.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sense_seem",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>schijnt</w> dat zij binnenkort gaan verhuizen naar een andere stad.",
                            "target_lang_translations": {
                                "en": "It <w>seems</w> that they are moving to another city soon.",
                                "ru": "<w>Кажется</w>, что они скоро переезжают в другой город."
                            }
                        },
                        {
                            "text": "Naar het <w>schijnt</w> is dit het beste restaurant van de omgeving.",
                            "target_lang_translations": {
                                "en": "It <w>seems</w> that this is the best restaurant in the area.",
                                "ru": "Как <w>говорят</w>, это лучший ресторан в округе."
                            }
                        }
                    ],
                    "synonyms": [
                        "lijken"
                    ],
                    "common_phrases": [
                        "naar het schijnt",
                        "het schijnt dat"
                    ],
                    "target_lang_definitions": {
                        "en": "To give an impression or to be the case according to information from others.",
                        "ru": "Производить впечатление или являться таковым согласно информации от других."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seem",
                                "target_lang_sense_clarification": "to give the impression"
                            },
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "to be apparent"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "казаться",
                                "target_lang_sense_clarification": "производить впечатление"
                            },
                            {
                                "target_lang_word": "производить впечатление",
                                "target_lang_sense_clarification": "выглядеть определенным образом"
                            }
                        ]
                    }
                }
            ]
        }
    ]

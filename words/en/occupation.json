        "occupational",
        "occupier"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6da8de3d-36d4-3a4f-b9a0-583359041663",
                    "sense_definition": "A job, profession, or activity that a person does, especially to earn a living.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "job_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please write your name and <w>occupation</w> on the application form.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, напишите ваше имя и <w>род занятий</w> в анкете."
                            }
                        },
                        {
                            "text": "She decided to change her <w>occupation</w> after ten years in banking.",
                            "target_lang_translations": {
                                "ru": "Она решила сменить <w>профессию</w> после десяти лет работы в банковской сфере."
                            }
                        }
                    ],
                    "synonyms": [
                        "job",
                        "profession",
                        "work",
                        "employment",
                        "vocation",
                        "line of work",
                        "calling"
                    ],
                    "common_phrases": [
                        "current occupation",
                        "main occupation",
                        "professional occupation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Работа, профессия или вид деятельности, которым занимается человек, особенно для заработка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "профессия",
                                "target_lang_sense_clarification": "основной род трудовой деятельности"
                            },
                            {
                                "target_lang_word": "род занятий",
                                "target_lang_sense_clarification": "вид деятельности или работы"
                            },
                            {
                                "target_lang_word": "занятие",
                                "target_lang_sense_clarification": "дело, которым кто-либо занят"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39f75f33-ce26-3519-a00c-bdca3e72cff0",
                    "sense_definition": "The control of a country or area by a foreign military force.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The citizens suffered greatly during the long military <w>occupation</w>.",
                            "target_lang_translations": {
                                "ru": "Граждане сильно пострадали во время длительной военной <w>оккупации</w>."
                            }
                        },
                        {
                            "text": "Many people fled the country to escape the foreign <w>occupation</w>.",
                            "target_lang_translations": {
                                "ru": "Многие люди бежали из страны, чтобы спастись от иностранной <w>оккупации</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "military occupation",
                        "under occupation",
                        "foreign occupation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Контроль над страной или территорией иностранными вооруженными силами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оккупация",
                                "target_lang_sense_clarification": "военный захват чужой территории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eef711e1-1aae-305a-bf7d-46f7ddca1fbd",
                    "sense_definition": "The act of moving into or using a building, room, or piece of land.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new apartments are now ready for immediate <w>occupation</w>.",
                            "target_lang_translations": {
                                "ru": "Новые квартиры теперь готовы к немедленному <w>заселению</w>."
                            }
                        },
                        {
                            "text": "The illegal <w>occupation</w> of the abandoned factory lasted for weeks.",
                            "target_lang_translations": {
                                "ru": "Незаконное <w>занятие</w> заброшенной фабрики длилось несколько недель."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ready for occupation",
                        "illegal occupation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт заселения или использования здания, помещения или земельного участка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заселение",
                                "target_lang_sense_clarification": "въезд в жилое помещение"
                            },
                            {
                                "target_lang_word": "занятие",
                                "target_lang_sense_clarification": "использование или захват места"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
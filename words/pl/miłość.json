                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja daleka krewna ma na imię <w>Miłość</w>.",
                            "target_lang_translations": {
                                "ru": "Мою дальнюю родственницу зовут <w>Любовь</w>."

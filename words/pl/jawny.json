                        "jawna konwersja",
                        "deklaracja jawna",
                        "typ jawny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямо определенный или записанный в коде программы, а не подразумеваемый по умолчанию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "явный",
                                "target_lang_sense_clarification": "спец. прямо заданный, эксплицитный"

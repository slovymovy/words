                },
                {
                    "sense_id": "ca9d7f0e-5359-3ebb-b987-056415744b85",
                    "sense_definition": "A young heifer.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer moved the <w>whies</w> to the upper field.",

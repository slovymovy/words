    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a98ea905-311c-376c-bc39-ea1db6a7a096",
                    "sense_definition": "Группа людей, стоящих друг за другом в ожидании чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы простояли в <w>очереди</w> за билетами больше часа.",
                            "target_lang_translations": {
                                "en": "We stood in <w>line</w> for tickets for over an hour."
                            }
                        },
                        {
                            "text": "Пожалуйста, не лезьте без <w>очереди</w>, здесь все ждут.",
                            "target_lang_translations": {
                                "en": "Please don't cut the <w>line</w>, everyone is waiting here."
                            }
                        }
                    ],
                    "synonyms": [
                        "хвост"
                    ],
                    "antonyms": [
                        "толпа"
                    ],
                    "common_phrases": [
                        "стоять в очереди",
                        "занять очередь",
                        "живая очередь",
                        "без очереди"
                    ],
                    "target_lang_definitions": {
                        "en": "A line of people or vehicles waiting their turn to be attended to or to proceed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "queue",
                                "target_lang_sense_clarification": "British English"
                            },
                            {
                                "target_lang_word": "line",
                                "target_lang_sense_clarification": "American English"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42396abc-700a-3b51-bb87-2b56c9fc85e1",
                    "sense_definition": "Установленный порядок следования или время для совершения какого-либо действия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Теперь твоя <w>очередь</w> мыть посуду после ужина.",
                            "target_lang_translations": {
                                "en": "Now it's your <w>turn</w> to wash the dishes after dinner."
                            }
                        },
                        {
                            "text": "Ученики отвечали на вопросы учителя по <w>очереди</w>.",
                            "target_lang_translations": {
                                "en": "The students answered the teacher's questions in <w>turn</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "очерёдность",
                        "порядок"
                    ],
                    "common_phrases": [
                        "в первую очередь",
                        "в свою очередь",
                        "по очереди",
                        "вне очереди"
                    ],
                    "target_lang_definitions": {
                        "en": "The time or opportunity when a person is allowed or required to do something in a particular order."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "opportunity to act"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3721f0a2-d7fc-31f7-ba22-667ce0eec38f",
                    "sense_definition": "Ряд выстрелов из автоматического оружия, следующих быстро и непрерывно друг за другом.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тишину леса внезапно нарушила громкая автоматная <w>очередь</w>.",
                            "target_lang_translations": {
                                "en": "The silence of the forest was suddenly broken by a loud machine-gun <w>burst</w>."
                            }
                        },
                        {
                            "text": "Солдат прицелился и дал короткую <w>очередь</w> по мишени.",
                            "target_lang_translations": {
                                "en": "The soldier took aim and fired a short <w>burst</w> at the target."
                            }
                        }
                    ],
                    "antonyms": [
                        "одиночный выстрел",
                        "залп"
                    ],
                    "common_phrases": [
                        "автоматная очередь",
                        "пулемётная очередь",
                        "дать очередь"
                    ],
                    "target_lang_definitions": {
                        "en": "A continuous series of shots fired rapidly from an automatic weapon."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "burst",
                                "target_lang_sense_clarification": "of gunfire"
                            },
                            {
                                "target_lang_word": "fusillade",
                                "target_lang_sense_clarification": "rapid continuous firing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "860d3337-a5fa-3957-8dfa-04c41a11d0ca",
                    "sense_definition": "Часть или этап осуществления крупного проекта, строительства.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Строители успешно сдали первую <w>очередь</w> нового жилого комплекса.",
                            "target_lang_translations": {
                                "en": "The builders successfully completed the first <w>phase</w> of the new residential complex."
                            }
                        },
                        {
                            "text": "Вторая <w>очередь</w> завода будет запущена в следующем году.",
                            "target_lang_translations": {
                                "en": "The second <w>stage</w> of the plant will be launched next year."
                            }
                        }
                    ],
                    "synonyms": [
                        "этап",
                        "фаза"
                    ],
                    "common_phrases": [
                        "первая очередь",
                        "сдача очереди"
                    ],
                    "target_lang_definitions": {
                        "en": "A distinct period or stage in the development of a large project or process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "phase",
                                "target_lang_sense_clarification": "stage of development"
                            },
                            {
                                "target_lang_word": "stage",
                                "target_lang_sense_clarification": "step in a process"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a96bbdf6-da98-3bda-876b-916b4f2fcd66",
                    "sense_definition": "Структура данных, в которой элементы добавляются в конец, а извлекаются из начала.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сервер обрабатывает <w>очередь</w> запросов в порядке их поступления.",
                            "target_lang_translations": {
                                "en": "The server processes the request <w>queue</w> in the order of their arrival."
                            }
                        },
                        {
                            "text": "При переполнении <w>очереди</w> новые данные могут быть потеряны.",
                            "target_lang_translations": {
                                "en": "If the <w>queue</w> overflows, new data may be lost."
                            }
                        }
                    ],
                    "antonyms": [
                        "стек"
                    ],
                    "common_phrases": [
                        "очередь печати",
                        "очередь событий",
                        "очередь сообщений"
                    ],
                    "target_lang_definitions": {
                        "en": "A data structure where elements are inserted at one end and removed from the other (FIFO)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "queue",
                                "target_lang_sense_clarification": "computing"
                            }
                        ]
                    }
                }
            ]
        }

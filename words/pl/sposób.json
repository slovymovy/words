                {
                    "sense_id": "871c39c7-74b3-3762-a606-9422f45b6126",
                    "sense_definition": "Charakterystyczny styl bycia, zachowania się lub cecha wyróżniająca daną osobę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "style_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego bezpośredni <w>sposób</w> bycia zjednuje mu wielu przyjaciół.",
                            "target_lang_translations": {
                                "ru": "Его непосредственная <w>манера</w> поведения привлекает к нему много друзей."
                            }
                        },
                        {
                            "text": "Nie podoba mi się twój <w>sposób</w> traktowania innych ludzi.",
                            "target_lang_translations": {
                                "ru": "Мне не нравится твоя <w>манера</w> обращения с другими людьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "styl",
                        "maniera"

                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "political_body",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "Wybory do Sejmu i <Senatu> odbywają się zazwyczaj tego samego dnia.",
                            "target_lang_translations": {
                                "ru": "Выборы в Сейм и <w>Сенат</w> обычно проходят в один и тот же день."
                            }
                        },
                        {
                            "text": "Obecny Marszałek Senatu poprowadził obrady w sposób wzorowy.",
                            "target_lang_translations": {
                                "ru": "Нынешний Маршал <w>Сената</w> провел заседание образцово."
                            }
                        }
                    ],
                    "antonyms": [
                        "Sejm"
                    ],
                    "common_phrases": [
                        "Marszałek Senatu",
                        "gmach Senatu",
                        "senatorowie Senatu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя палата парламента Республики Польша."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сенат",

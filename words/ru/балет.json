            "senses": [
                {
                    "sense_id": "98714219-f0ba-375e-ac0b-1f9f49cae6ac",
                    "sense_definition": "Искусство сценического танца, сочетающее хореографию, музыку и драматургию.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "art_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя сестра с детства занимается классическим <w>балетом</w>.",
                            "target_lang_translations": {
                                "en": "My sister has been doing classical <w>ballet</w> since childhood."
                            }
                        },
                        {
                            "text": "«Умирающий лебедь» стал символом расцвета русского <w>балета</w>.",
                            "target_lang_translations": {
                                "en": "The Dying Swan became a symbol of the flourishing of Russian <w>ballet</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "хореография"
                    ],
                    "common_phrases": [
                        "русский балет",
                        "классический балет",
                        "школа балета",
                        "заниматься балетом"
                    ],
                    "target_lang_definitions": {
                        "en": "The art of stage dance combining choreography, music, and drama."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ballet",
                                "target_lang_sense_clarification": "art form"
                            }

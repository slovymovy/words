                    }
                },
                {
                    "sense_id": "a7a87f74-06fe-32d8-b9e5-b371ae5176c1",
                    "sense_definition": "Een deel van een samenstelling dat een leidende of hogere functie aanduidt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "compound_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tambour-<w>majoor</w> liep voorop bij de optocht.",
                            "target_lang_translations": {

                        "impulsive",
                        "naive"
                    ],
                    "common_phrases": [
                        "cold and calculated"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действующий холодно, аналитически и без эмоций, часто в личных интересах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расчетливый",
                                "target_lang_sense_clarification": "преследующий личную выгоду"
                            },
                            {
                                "target_lang_word": "хладнокровный",
                                "target_lang_sense_clarification": "сдержанный, безэмоциональный"
                            }
                        ]
                    }
                }
            ]
        }

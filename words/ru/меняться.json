                        "меняться на глазах",
                        "времена меняются"
                    ],
                    "target_lang_definitions": {
                        "en": "To become different; to acquire new properties or qualities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "change",
                                "target_lang_sense_clarification": "to become different"
                            },
                            {
                                "target_lang_word": "alter",
                                "target_lang_sense_clarification": "to change in character or composition"
                            }
                        ]
                    }
                },
                {

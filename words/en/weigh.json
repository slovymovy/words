                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взвешивать",
                                "target_lang_sense_clarification": "to consider carefully (imperfective)"
                            },
                            {
                                "target_lang_word": "взвесить",
                                "target_lang_sense_clarification": "to consider carefully (perfective)"
                            }
                        ]
                    }
                },

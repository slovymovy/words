    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "29500d5a-86de-32dd-9f6c-407332256a85",
                    "sense_definition": "An enclosed shopping centre containing various stores, restaurants, and businesses.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "shopping_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went to the <w>mall</w> to buy a birthday present for my sister.",
                            "target_lang_translations": {
                                "ru": "Мы пошли в <w>торговый центр</w>, чтобы купить подарок на день рождения сестре."
                            }
                        },
                        {
                            "text": "The new <w>mall</w> has a large cinema and a food court on the top floor.",
                            "target_lang_translations": {
                                "ru": "В новом <w>молле</w> есть большой кинотеатр и фуд-корт на верхнем этаже."
                            }
                        }
                    ],
                    "synonyms": [
                        "shopping centre",
                        "shopping center",
                        "plaza"
                    ],
                    "common_phrases": [
                        "shopping mall",
                        "strip mall",
                        "outlet mall",
                        "at the mall",
                        "go to the mall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крытый торговый комплекс, в котором расположены различные магазины, рестораны и предприятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговый центр",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "молл",
                                "target_lang_sense_clarification": "крупный современный комплекс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06217966-9865-3ce1-919c-cec6c1e7721a",
                    "sense_definition": "A street or area closed to vehicles where people can walk and shop.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "shopping_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cars are not allowed on the pedestrian <w>mall</w> in the city center.",
                            "target_lang_translations": {
                                "ru": "Машинам запрещен въезд в <w>пешеходную зону</w> в центре города."
                            }
                        },
                        {
                            "text": "The city converted the busy street into a quiet <w>mall</w> lined with trees and benches.",
                            "target_lang_translations": {
                                "ru": "Город превратил оживленную улицу в тихую <w>аллею</w>, обсаженную деревьями и скамейками."
                            }
                        }
                    ],
                    "synonyms": [
                        "precinct",
                        "promenade",
                        "pedestrian zone"
                    ],
                    "common_phrases": [
                        "pedestrian mall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Улица или территория, закрытая для транспорта, где люди могут гулять и совершать покупки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пешеходная зона",
                                "target_lang_sense_clarification": "территория без машин"
                            },
                            {
                                "target_lang_word": "пешеходная аллея",
                                "target_lang_sense_clarification": "улица для прогулок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "726b492e-2f89-3707-8923-bab125867c9a",
                    "sense_definition": "A large public walkway or promenade, often shaded by trees and used for leisure or public events.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "public_walk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tourists gathered on the <w>mall</w> to watch the national parade.",
                            "target_lang_translations": {
                                "ru": "Туристы собрались на <w>аллее</w>, чтобы посмотреть национальный парад."
                            }
                        },
                        {
                            "text": "They took a long walk along the shaded <w>mall</w> near the palace.",
                            "target_lang_translations": {
                                "ru": "Они долго гуляли по тенистой <w>аллее</w> возле дворца."
                            }
                        }
                    ],
                    "synonyms": [
                        "promenade",
                        "esplanade",
                        "walkway"
                    ],
                    "target_lang_definitions": {
                        "ru": "Широкая общественная аллея или место для прогулок, часто обсаженное деревьями и используемое для отдыха или мероприятий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аллея",
                                "target_lang_sense_clarification": "широкая дорога с деревьями"
                            },
                            {
                                "target_lang_word": "променад",
                                "target_lang_sense_clarification": "место для прогулок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "94dd2a8e-945f-3da4-bf83-c22703b980e0",
                    "sense_definition": "To visit a shopping mall, usually for leisure or social activity.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shopping_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teenagers spent the whole Saturday <w>malling</w> with their friends.",
                            "target_lang_translations": {
                                "ru": "Подростки провели всю субботу, <w>гуляя по торговому центру</w> с друзьями."
                            }
                        },
                        {
                            "text": "We decided to go <w>malling</w> instead of staying home to watch TV.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>пойти по магазинам</w> вместо того, чтобы остаться дома и смотреть телевизор."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go malling"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in the form 'malling'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Посещать торговый центр, обычно ради развлечения или общения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гулять по торговому центру",
                                "target_lang_sense_clarification": "проводить время в ТЦ"
                            },
                            {
                                "target_lang_word": "ходить по магазинам",
                                "target_lang_sense_clarification": "шопинг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
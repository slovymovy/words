    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "84514922-8a5f-3e24-be42-a2f166acb9c5",
                    "sense_definition": "Wytrawna wódka gatunkowa produkowana na bazie owoców jałowca.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek pędził najlepszą <w>jałowcówkę</w> w całej okolicy.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка гнал лучшую <w>можжевеловку</w> во всей округе."
                            }
                        },
                        {
                            "text": "Do dziczyzny często podaje się kieliszek zimnej <w>jałowcówki</w>.",
                            "target_lang_translations": {
                                "ru": "К дичи часто подают рюмку холодной <w>можжевеловой водки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kieliszek jałowcówki",
                        "domowa jałowcówka",
                        "butelka jałowcówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сухая сортовая водка, производимая на основе ягод можжевельника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "можжевеловая водка",
                                "target_lang_sense_clarification": "алкогольный напиток"
                            },
                            {
                                "target_lang_word": "можжевеловка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd4873a9-8528-3730-8442-769a21fc4cec",
                    "sense_definition": "Odmiana gruszy rodząca niewielkie owoce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "pear_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sadzie u proboszcza rośnie stara <w>jałowcówka</w>.",
                            "target_lang_translations": {
                                "ru": "В саду у настоятеля растет старая <w>яловцовка</w>."
                            }
                        },
                        {
                            "text": "Ogrodnik zalecił nam, aby <w>jałowcówka</w> została przycięta na wiosnę.",
                            "target_lang_translations": {
                                "ru": "Садовник посоветовал нам, чтобы <w>яловцовка</w> была подрезана весной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stara jałowcówka",
                        "szczepić jałowcówkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сорт груши, дающий небольшие плоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яловцовка",
                                "target_lang_sense_clarification": "сорт груши"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa1160b8-5a6a-35f2-8a86-abe625563d8c",
                    "sense_definition": "Jadalny owoc gruszy odmiany jałowcówka.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "pear_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nazrywam ci <w>jałowcówek</w>, żebyś zawiózł je ciotce Honoracie.",
                            "target_lang_translations": {
                                "ru": "Я нарву тебе <w>яловцовок</w>, чтобы ты отвез их тете Гонорате."
                            }
                        },
                        {
                            "text": "Na targu kupiłem kilogram słodkich <w>jałowcówek</w> na kompot.",
                            "target_lang_translations": {
                                "ru": "На рынке я купил килограмм сладких <w>яловцовок</w> для компота."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kosz jałowcówek",
                        "smak jałowcówek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобный плод груши сорта яловцовка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яловцовка",
                                "target_lang_sense_clarification": "плод"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "7731df5e-91ab-38a6-96d0-29646c8e754a",
                    "sense_definition": "Met elkaar verbonden, verstrengeld of verenigd in een gezamenlijke actie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

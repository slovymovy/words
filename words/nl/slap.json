    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0445bc63-43b8-3912-bf1b-2b790bf0bb57",
                    "sense_definition": "Niet strak gespannen of niet stevig; buigzaam en zacht.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>slappe</w> koorden hingen losjes langs de muur.",
                            "target_lang_translations": {
                                "en": "The <w>slack</w> cords hung loosely along the wall."
                            }
                        },
                        {
                            "text": "Ik hou niet van <w>slappe</w> koffie in de ochtend.",
                            "target_lang_translations": {
                                "en": "I don't like <w>weak</w> coffee in the morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwak"
                    ],
                    "antonyms": [
                        "strak",
                        "stevig"
                    ],
                    "common_phrases": [
                        "slappe koffie",
                        "slappe was",
                        "slappe banden"
                    ],
                    "target_lang_definitions": {
                        "en": "Not tight or firm; flexible and soft."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slack",
                                "target_lang_sense_clarification": "not taut; loose (e.g., ropes, muscles)"
                            },
                            {
                                "target_lang_word": "weak",
                                "target_lang_sense_clarification": "lacking strength or concentration (e.g., coffee, tea)"
                            },
                            {
                                "target_lang_word": "limp",
                                "target_lang_sense_clarification": "lacking stiffness or firmness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3328ac8a-4bec-3795-86a0-263d87f5c32c",
                    "sense_definition": "Zonder geestelijke kracht of overtuiging; laf of flauw.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is echt een <w>slap</w> excuus voor je gedrag.",
                            "target_lang_translations": {
                                "en": "That is really a <w>lame</w> excuse for your behavior."
                            }
                        },
                        {
                            "text": "Door de zenuwen kregen ze de <w>slappe</w> lach.",
                            "target_lang_translations": {
                                "en": "Due to nerves, they got the <w>giggles</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwak"
                    ],
                    "antonyms": [
                        "krachtig",
                        "flink"
                    ],
                    "common_phrases": [
                        "een slap excuus",
                        "slappe lach",
                        "slappe praat"
                    ],
                    "target_lang_definitions": {
                        "en": "Lacking mental strength or conviction; cowardly or insipid."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weak",
                                "target_lang_sense_clarification": "lacking character or resolve"
                            },
                            {
                                "target_lang_word": "lame",
                                "target_lang_sense_clarification": "unconvincing (e.g., an excuse)"
                            },
                            {
                                "target_lang_word": "spineless",
                                "target_lang_sense_clarification": "lacking courage or determination"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07a4450b-8c36-32b3-9441-b3e4883ed0ea",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>slappen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>slap</w> de touwen zodat ze niet meer strak staan.",
                            "target_lang_translations": {
                                "en": "I <w>slacken</w> the ropes so they are no longer tight."
                            }
                        },
                        {
                            "text": "Als ik de teugels <w>slap</w>, gaat het paard langzamer lopen.",
                            "target_lang_translations": {
                                "en": "If I <w>loosen</w> the reins, the horse will walk slower."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "of slappen"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of <w>to slacken</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slacken",
                                "target_lang_sense_clarification": "to make or become less tight"
                            },
                            {
                                "target_lang_word": "loosen",
                                "target_lang_sense_clarification": "to make less tight"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "common_phrases": [
                        "wronged by someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться с кем-либо несправедливо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть несправедливым",
                                "target_lang_sense_clarification": "по отношению к кому-то"
                            },
                            {
                                "target_lang_word": "обижать",
                                "target_lang_sense_clarification": "причинять обиду"

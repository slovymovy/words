            "senses": [
                {
                    "sense_id": "b38bd81f-62b8-30d0-91ba-172bb66dd342",
                    "sense_definition": "Naar een bepaalde plaats voeren (gezegd van wegen, paden of deuren).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze lange weg <w>leidt</w> direct naar het centrum van de stad.",
                            "target_lang_translations": {
                                "en": "This long road <w>leads</w> directly to the city center.",
                                "ru": "Эта длинная дорога <w>ведет</w> прямо в центр города."
                            }
                        },
                        {
                            "text": "Alle wegen <w>leiden</w> naar Rome.",
                            "target_lang_translations": {
                                "en": "All roads <w>lead</w> to Rome.",
                                "ru": "Все дороги <w>ведут</w> в Рим."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leiden naar",
                        "de weg leidt"
                    ],
                    "target_lang_definitions": {
                        "en": "To go to a certain place (said of roads, paths, or doors).",
                        "ru": "Вести в определенное место (о дорогах, тропинках или дверях)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "go in a direction"
                            },
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "extend to a place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "иметь направление"
                            },
                            {
                                "target_lang_word": "пролегать",
                                "target_lang_sense_clarification": "о дороге или пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c4a9249-f93f-3057-9eaf-02a22edddfcc",
                    "sense_definition": "De leiding hebben over; de baas zijn van een groep, organisatie of activiteit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "management",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur <w>leidt</w> het bedrijf al meer dan twintig jaar.",
                            "target_lang_translations": {
                                "en": "The director has been <w>leading</w> the company for over twenty years.",
                                "ru": "Директор <w>руководит</w> компанией уже более двадцати лет."
                            }
                        },
                        {
                            "text": "Zij zal de vergadering vanmiddag <w>leiden</w> omdat de voorzitter ziek is.",
                            "target_lang_translations": {
                                "en": "She will <w>lead</w> the meeting this afternoon because the chairman is ill.",
                                "ru": "Она будет <w>вести</w> собрание сегодня днем, так как председатель болен."
                            }
                        }
                    ],
                    "synonyms": [
                        "besturen",
                        "aanvoeren",
                        "managen",
                        "regeren"
                    ],
                    "antonyms": [
                        "volgen"
                    ],
                    "common_phrases": [
                        "een vergadering leiden",
                        "een bedrijf leiden",
                        "een leven leiden"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in charge of; to be the boss of a group, organization, or activity.",
                        "ru": "Руководить, быть во главе группы, организации или деятельности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "be in charge of"
                            },
                            {
                                "target_lang_word": "manage",
                                "target_lang_sense_clarification": "control or organize"
                            },
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "direct the operations of"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "руководить",
                                "target_lang_sense_clarification": "управлять организацией"
                            },
                            {
                                "target_lang_word": "возглавлять",
                                "target_lang_sense_clarification": "быть во главе"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "управлять процессом или собранием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e872ad39-a19d-3028-82e1-2beac1fb83a9",
                    "sense_definition": "Iemand of iets de weg wijzen of meenemen naar een bestemming.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "guidance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gids <w>leidde</w> de toeristen door het oude museum.",
                            "target_lang_translations": {
                                "en": "The guide <w>led</w> the tourists through the old museum.",
                                "ru": "Гид <w>вел</w> туристов по старому музею."
                            }
                        },
                        {
                            "text": "Hij <w>leidde</w> zijn gasten naar de woonkamer voor een kopje koffie.",
                            "target_lang_translations": {
                                "en": "He <w>led</w> his guests to the living room for a cup of coffee.",
                                "ru": "Он <w>провел</w> своих гостей в гостиную на чашечку кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "gidsen",
                        "rondleiden",
                        "loodsen",
                        "dirigeren"
                    ],
                    "common_phrases": [
                        "iemand rondleiden",
                        "naar binnen leiden"
                    ],
                    "target_lang_definitions": {
                        "en": "To show someone or something the way or take them to a destination.",
                        "ru": "Указывать путь или сопровождать кого-либо к месту назначения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "guide or conduct"
                            },
                            {
                                "target_lang_word": "guide",
                                "target_lang_sense_clarification": "show the way"
                            },
                            {
                                "target_lang_word": "escort",
                                "target_lang_sense_clarification": "accompany"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "направлять движение"
                            },
                            {
                                "target_lang_word": "проводить",
                                "target_lang_sense_clarification": "сопровождать до места"
                            },
                            {
                                "target_lang_word": "сопровождать",
                                "target_lang_sense_clarification": "идти вместе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01f41688-5feb-32e6-be5f-c4970502e1c1",
                    "sense_definition": "Iets tot gevolg hebben of veroorzaken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Slecht onderhoud kan tot gevaarlijke situaties op de weg <w>leiden</w>.",
                            "target_lang_translations": {
                                "en": "Poor maintenance can <w>lead</w> to dangerous situations on the road.",
                                "ru": "Плохое обслуживание может <w>привести</w> к опасным ситуациям на дороге."
                            }
                        },
                        {
                            "text": "Zijn harde werk zal uiteindelijk tot succes <w>leiden</w>.",
                            "target_lang_translations": {
                                "en": "His hard work will eventually <w>lead</w> to success.",
                                "ru": "Его упорный труд в конечном итоге <w>приведет</w> к успеху."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanvoeren"
                    ],
                    "common_phrases": [
                        "leiden tot",
                        "tot gevolg hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "To have something as a result or to cause something.",
                        "ru": "Приводить к чему-либо, быть причиной чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "result in (used with 'to')"
                            },
                            {
                                "target_lang_word": "result",
                                "target_lang_sense_clarification": "cause an outcome (used with 'in')"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приводить",
                                "target_lang_sense_clarification": "иметь последствием"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "быть причиной"
                            }
                        ]
                    }
                }
            ]

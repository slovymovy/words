{
    "word_family": [
        "aanbieden",
        "aanbieder"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b9a04abb-853d-3892-9567-703ea53c60fa",
                    "sense_definition": "Een product dat tijdelijk voor een lagere prijs wordt verkocht.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "discount_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koffie is deze week in de <w>aanbieding</w> bij de supermarkt.",
                            "target_lang_translations": {
                                "ru": "На этой неделе в супермаркете кофе продается по <w>акции</w>.",
                                "en": "The coffee is on <w>sale</w> at the supermarket this week."
                            }
                        },
                        {
                            "text": "De winkel stond vol met verschillende <w>aanbiedingen</w> voor de feestdagen.",
                            "target_lang_translations": {
                                "ru": "В магазине было полно различных <w>спецпредложений</w> к праздникам.",
                                "en": "The shop was full of different <w>special offers</w> for the holidays."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de aanbieding",
                        "speciale aanbieding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Товар, который временно продается по сниженной цене.",
                        "en": "A product temporarily sold at a lower price."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акция",
                                "target_lang_sense_clarification": "временное снижение цен"
                            },
                            {
                                "target_lang_word": "спецпредложение",
                                "target_lang_sense_clarification": "товар по специальной цене"
                            },
                            {
                                "target_lang_word": "скидка",
                                "target_lang_sense_clarification": "снижение стоимости"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "special offer",
                                "target_lang_sense_clarification": "item sold at a reduced price"
                            },
                            {
                                "target_lang_word": "sale",
                                "target_lang_sense_clarification": "event with reduced prices"
                            },
                            {
                                "target_lang_word": "deal",
                                "target_lang_sense_clarification": "bargain or promotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a46588d7-65eb-3b09-9bcd-10d4d5496a62",
                    "sense_definition": "Een formeel voorstel om iets te doen, te geven of te verkopen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "proposal_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan u geen betere <w>aanbieding</w> doen dan ik al gedaan heb.",
                            "target_lang_translations": {
                                "ru": "Я не могу сделать вам <w>предложение</w> лучше того, что уже сделал.",
                                "en": "I cannot make you a better <w>offer</w> than I already have."
                            }
                        },
                        {
                            "text": "Hij accepteerde de <w>aanbieding</w> van de verkoper direct.",
                            "target_lang_translations": {
                                "ru": "Он сразу же принял <w>предложение</w> продавца.",
                                "en": "He accepted the seller's <w>offer</w> immediately."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanbod"
                    ],
                    "common_phrases": [
                        "een aanbieding doen",
                        "vrijblijvende aanbieding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное предложение сделать, дать или продать что-либо.",
                        "en": "A formal proposal to do, give, or sell something."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предложение",
                                "target_lang_sense_clarification": "инициатива или вариант"
                            },
                            {
                                "target_lang_word": "коммерческое предложение",
                                "target_lang_sense_clarification": "деловая оферта"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "offer",
                                "target_lang_sense_clarification": "proposal or bid"
                            },
                            {
                                "target_lang_word": "proposal",
                                "target_lang_sense_clarification": "formal suggestion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "725bff4f-ac82-3c7a-8af8-4d25a7eebeab",
                    "sense_definition": "Een uitnodiging om een bepaalde baan of functie te accepteren.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "proposal_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij kreeg een mooie <w>aanbieding</w> voor een nieuwe baan in het buitenland.",
                            "target_lang_translations": {
                                "ru": "Она получила отличное <w>предложение</w> новой работы за границей.",
                                "en": "She received a nice <w>offer</w> for a new job abroad."
                            }
                        },
                        {
                            "text": "Een <w>aanbieding</w> voor een hoofdrol in de film zou geweldig zijn.",
                            "target_lang_translations": {
                                "ru": "<w>Предложение</w> главной роли в фильме было бы чем-то потрясающим.",
                                "en": "An <w>offer</w> for a lead role in the movie would be amazing."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanbod"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приглашение принять определенную работу или должность.",
                        "en": "An invitation to accept a specific job or position."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предложение",
                                "target_lang_sense_clarification": "о работе или должности"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "offer",
                                "target_lang_sense_clarification": "job or role proposal"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
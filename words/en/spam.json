                        {
                            "text": "The player was disqualified for <w>spamming</w> the same move over and over.",
                            "target_lang_translations": {
                                "ru": "Игрока дисквалифицировали за то, что он <w>спамил</w> один и тот же прием снова и снова."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spam buttons",
                        "spam attacks"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often used in gaming contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то быстро и многократно (обычно в видеоиграх)."

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0e6e9e0c-cc02-3fd2-873d-2641d7946451",
                    "sense_definition": "Usuwać brud, kurz, plamy lub niepożądane substancje z powierzchni czegoś, czyniąc to czystym.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę dokładnie <czyścić> buty, zanim wejdę do środka.",
                            "target_lang_translations": {
                                "ru": "Мне нужно тщательно <почистить> ботинки, прежде чем войти внутрь."
                            }
                        },
                        {
                            "text": "Ona codziennie <czyści> lustro w łazience, żeby nie było na nim smug.",
                            "target_lang_translations": {
                                "ru": "Она каждый день <чистит> зеркало в ванной, чтобы на нём не было разводов."
                            }
                        }
                    ],
                    "synonyms": [
                        "myć",
                        "oczyszczać",
                        "pucować",
                        "uporządkowywać",
                        "odświeżać"
                    ],
                    "antonyms": [
                        "brudzić"
                    ],
                    "common_phrases": [
                        "czyścić buty",
                        "czyścić zęby",
                        "czyścić okna",
                        "czyścić ubranie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалять грязь, пыль, пятна или нежелательные вещества с поверхности чего-либо, делая это чистым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чистить",
                                "target_lang_sense_clarification": "удалять грязь трением или инструментом"
                            },
                            {
                                "target_lang_word": "мыть",
                                "target_lang_sense_clarification": "очищать с помощью воды"
                            },
                            {
                                "target_lang_word": "очищать",
                                "target_lang_sense_clarification": "делать чистым, освобождать от грязи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d0154af-8f1c-3b8f-aa52-6273fbef01d1",
                    "sense_definition": "Usuwać niejadalne lub zbędne części z produktów spożywczych lub surowców w trakcie ich obróbki.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cleaning_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kucharz zaczął <czyścić> rybę z łusek ostrym nożem.",
                            "target_lang_translations": {
                                "ru": "Повар начал <чистить> рыбу от чешуи острым ножом."
                            }
                        },
                        {
                            "text": "Najlepiej <czyścić> grzyby małym pędzelkiem zaraz po zebraniu.",
                            "target_lang_translations": {
                                "ru": "Лучше всего <чистить> грибы маленькой кисточкой сразу после сбора."
                            }
                        }
                    ],
                    "synonyms": [
                        "oczyszczać",
                        "wyczyszczać"
                    ],
                    "common_phrases": [

{
    "word_family": [
        "brukarz",
        "brukowiec",
        "brukować",
        "brukowy",
        "brukarski"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "230832ab-e82a-338d-86b4-c02c3bc11c6d",
                    "sense_definition": "Twarda nawierzchnia ulicy, drogi lub placu, wykonana z ułożonych ściśle obok siebie kamieni.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pavement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód podskakiwał na starym, nierównym <bruku> w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Машина подпрыгивала на старой, неровной <брусчатке> в центре города."
                            }
                        },
                        {
                            "text": "Latarnia przewróciła się podczas wichury i rozbiła o kamienny <bruk>.",
                            "target_lang_translations": {
                                "ru": "Фонарь упал во время бури и разбился о каменную <мостовую>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kocie łby"
                    ],
                    "common_phrases": [
                        "wyrzucić na bruk",
                        "kamienny bruk",
                        "układać bruk",
                        "nierówny bruk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердое покрытие улицы, дороги или площади, выполненное из плотно уложенных камней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мостовая",
                                "target_lang_sense_clarification": "дорожное покрытие"
                            },
                            {
                                "target_lang_word": "брусчатка",
                                "target_lang_sense_clarification": "покрытие из каменных брусков"
                            },
                            {
                                "target_lang_word": "булыжник",
                                "target_lang_sense_clarification": "камень для мощения или мостовая из него"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0d95e41-9e38-3cd3-9faf-212c1871a180",
                    "sense_definition": "Warstwa większych fragmentów skalnych pokrywająca powierzchnię ziemi, powstała w wyniku wywiania drobniejszego materiału (termin geologiczny).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na pustyni wiatr wywiał piasek, odsłaniając twardy <bruk>.",
                            "target_lang_translations": {
                                "ru": "В пустыне ветер выдул песок, обнажив твердую <пустынную мостовую>."
                            }
                        },
                        {
                            "text": "Geolodzy badali <bruk> deflacyjny pod kątem jego składu mineralnego.",
                            "target_lang_translations": {
                                "ru": "Геологи изучали дефляционную <мостовую> на предмет её минерального состава."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой крупных обломков горных пород, покрывающий поверхность земли, образовавшийся в результате выдувания мелкого материала (геологический термин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пустынная мостовая",
                                "target_lang_sense_clarification": "геологический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a16edac0-683a-3dba-a72c-d31c5716a28a",
                    "sense_definition": "Głośna zabawa, awantura lub bijatyka.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "disturbance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W karczmie rozpętał się straszny <bruk>, który obudził sąsiadów.",
                            "target_lang_translations": {
                                "ru": "В корчме поднялся страшный <шум>, который разбудил соседей."
                            }
                        },
                        {
                            "text": "Nie chcemy tu żadnych kłótni ani pijackiego <bruku>.",
                            "target_lang_translations": {
                                "ru": "Мы не хотим здесь никаких ссор или пьяной <драки>."
                            }
                        }
                    ],
                    "synonyms": [
                        "burda",
                        "hulanka"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "znaczenie rzadkie, dawne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Громкое веселье, скандал или драка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шум",
                                "target_lang_sense_clarification": "громкие звуки, гам"
                            },
                            {
                                "target_lang_word": "драка",
                                "target_lang_sense_clarification": "потасовка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72e83845-b69f-3df7-a818-120b6286d17d",
                    "sense_definition": "Ogrodzenie wykonane z ułożonych na sobie kamieni.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za starym domem ciągnął się omszały, kamienny <bruk>.",
                            "target_lang_translations": {
                                "ru": "За старым домом тянулась мшистая каменная <ограда>."
                            }
                        },
                        {
                            "text": "Owce pasły się na łące otoczonej przez niski <bruk>.",
                            "target_lang_translations": {
                                "ru": "Овцы паслись на лугу, окруженном низкой <оградой>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "rzadko spotykane"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограда, выполненная из сложенных друг на друга камней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каменная ограда",
                                "target_lang_sense_clarification": "забор из камней"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
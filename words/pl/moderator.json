                },
                {
                    "sense_id": "48374b58-c29e-36e8-82db-be7a00a7005b",
                    "sense_definition": "Osoba zarządzająca treścią oraz kontami użytkowników w serwisie internetowym lub na forum.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "34c6a4d5-e9b4-37e3-92c7-3946afcfcf6b",
                    "sense_definition": "To make an effort to do something; to attempt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attempt_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>try</w> to finish the homework before dinner.",
                            "target_lang_translations": {
                                "ru": "Я <w>постараюсь</w> закончить домашнюю работу до ужина."
                            }
                        },
                        {
                            "text": "She <w>tried</w> hard to open the stuck window.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>старалась</w> открыть застрявшее окно."
                            }

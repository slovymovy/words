                {
                    "sense_id": "63463cd7-9ae6-3726-ac88-8858a4236ea7",
                    "sense_definition": "Afkorting van Normaal Amsterdams Peil; het referentieniveau voor hoogtemetingen in Nederland (ongeveer gelijk aan het gemiddelde zeeniveau).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wonen op drie meter onder zeeniveau (<w>NAP</w>) is heel wat anders dan wonen in de heuvels.",
                            "target_lang_translations": {
                                "en": "Living three meters below sea level (<w>NAP</w>) is quite different from living in the hills."
                            }
                        },
                        {
                            "text": "Het laagste punt van Nederland ligt bijna zeven meter onder <w>NAP</w>.",
                            "target_lang_translations": {
                                "en": "The lowest point in the Netherlands lies almost seven meters below <w>NAP</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "boven NAP",
                        "onder NAP"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for Normaal Amsterdams Peil; the reference level for altitude measurements in the Netherlands (approximately equal to mean sea level)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "NAP",
                                "target_lang_sense_clarification": "Dutch reference for sea level"
                            },
                            {
                                "target_lang_word": "Amsterdam Ordnance Datum",
                                "target_lang_sense_clarification": "technical term"
                            },
                            {
                                "target_lang_word": "mean sea level",
                                "target_lang_sense_clarification": "approximate equivalent"
                            }
                        ]
                    }
                },
                {

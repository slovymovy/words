            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0b95faf1-84a6-3952-92eb-23b46cf4bd50",
                    "sense_definition": "Een netjes ingerichte kamer in een huis waar men gasten ontvangt (vaak chiquer dan een gewone woonkamer).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "room",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gasten werden ontvangen in de ruime <w>salon</w> met uitzicht op de tuin.",
                            "target_lang_translations": {
                                "ru": "Гостей принимали в просторной <w>гостиной</w> с видом на сад."
                            }
                        },
                        {
                            "text": "Na het diner dronken we koffie in de <w>salon</w>.",
                            "target_lang_translations": {
                                "ru": "После ужина мы пили кофе в <w>гостиной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "woonkamer",
                        "zitkamer",
                        "huiskamer",
                        "living",
                        "woonvertrek",
                        "leefkamer"
                    ],
                    "common_phrases": [
                        "in de salon",
                        "de chique salon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аккуратно обставленная комната в доме, где принимают гостей (часто более нарядная, чем обычная жилая комната)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гостиная",
                                "target_lang_sense_clarification": "комната для приема гостей"
                            },
                            {
                                "target_lang_word": "зал",
                                "target_lang_sense_clarification": "парадная комната в доме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87ec228a-ee50-3769-af2d-e439f1f88cd8",
                    "sense_definition": "Een set van bij elkaar horende zitmeubels (banken en zetels) voor in de woonkamer.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze hebben een nieuw leren <w>salon</w> gekocht voor hun nieuwe huis.",
                            "target_lang_translations": {
                                "ru": "Они купили новый кожаный <w>гарнитур</w> для своего нового дома."
                            }
                        },
                        {
                            "text": "Het hele gezin zat gezellig in het <w>salon</w> televisie te kijken.",
                            "target_lang_translations": {
                                "ru": "Вся семья уютно устроилась на <w>мягкой мебели</w> и смотрела телевизор."
                            }
                        }
                    ],
                    "synonyms": [
                        "bankstel",
                        "bank"
                    ],
                    "common_phrases": [
                        "een leren salon",
                        "een nieuw salon"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Belgisch-Nederlands"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплект мягкой мебели (диваны и кресла) для гостиной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гарнитур",
                                "target_lang_sense_clarification": "набор мебели"
                            },
                            {
                                "target_lang_word": "мягкая мебель",
                                "target_lang_sense_clarification": "диваны и кресла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c7fe6e4-45ac-3514-a020-f09bb615c859",
                    "sense_definition": "Een chique zaak waar men iets kan drinken of een behandeling kan ondergaan (zoals een theehuis of kapper).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "establishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het winkelen genoten de dames van een kopje thee in de <w>salon</w>.",
                            "target_lang_translations": {
                                "ru": "После шопинга дамы наслаждались чашечкой чая в <w>салоне</w>."
                            }
                        },
                        {
                            "text": "Zij heeft een afspraak bij de <w>salon</w> om haar haren te laten knippen.",
                            "target_lang_translations": {
                                "ru": "У нее запись в <w>салон</w> на стрижку."
                            }
                        }
                    ],
                    "synonyms": [
                        "lounge"
                    ],
                    "common_phrases": [
                        "naar de salon",
                        "in de salon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элегантное заведение, где можно выпить напитки или получить косметические услуги (например, чайная или парикмахерская)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "салон",
                                "target_lang_sense_clarification": "заведение (красоты, чайный)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a33ca29-61c8-38e0-9240-2836ff791934",
                    "sense_definition": "Een grote, georganiseerde tentoonstelling of beurs, vaak gericht op een specifieke industrie zoals auto's.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op het <w>salon</w> werden de nieuwste modellen van diverse automerken gepresenteerd.",
                            "target_lang_translations": {
                                "ru": "На <w>салоне</w> были представлены новейшие модели различных автомобильных марок."
                            }
                        },
                        {
                            "text": "Duizenden bezoekers kwamen dit weekend af op het internationale <w>salon</w>.",
                            "target_lang_translations": {
                                "ru": "Тысячи посетителей пришли в эти выходные на международный <w>салон</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het internationale salon",
                        "het autosalon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная выставка или ярмарка, часто посвященная определенной индустрии (например, автомобильной)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "салон",
                                "target_lang_sense_clarification": "выставка (напр. автосалон)"

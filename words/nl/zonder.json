            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "5513d9f0-712b-36c9-8943-09542e8abc99",
                    "sense_definition": "In afwezigheid van; niet in het bezit van of niet vergezeld door.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik drink mijn koffie graag <w>zonder</w> suiker en melk.",
                            "target_lang_translations": {
                                "en": "I like to drink my coffee <w>without</w> sugar and milk."
                            }
                        },
                        {
                            "text": "We gaan <w>zonder</w> hem naar het feest omdat hij ziek is.",
                            "target_lang_translations": {
                                "en": "We are going to the party <w>without</w> him because he is sick."
                            }
                        }
                    ],
                    "antonyms": [
                        "met"
                    ],
                    "common_phrases": [
                        "zonder twijfel",
                        "zonder meer",
                        "zonder dank",
                        "zonder zorgen"
                    ],
                    "target_lang_definitions": {
                        "en": "In the absence of; not in possession of or not accompanied by."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "without",
                                "target_lang_sense_clarification": "not having or accompanied by"
                            }
                        ]
                    }
                }
            ]
        },
        {

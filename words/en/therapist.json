    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c2854cde-edf3-3f56-b76d-60aeb15693b7",
                    "sense_definition": "A professional who helps people deal with mental or emotional problems by talking to them; a psychotherapist.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mental_health_professional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to see a <w>therapist</w> to help her cope with anxiety and stress.",
                            "target_lang_translations": {
                                "ru": "Она решила обратиться к <w>психотерапевту</w>, чтобы справиться с тревогой и стрессом."
                            }
                        },
                        {
                            "text": "A family <w>therapist</w> can guide parents and children through difficult conflicts.",
                            "target_lang_translations": {
                                "ru": "Семейный <w>психотерапевт</w> может помочь родителям и детям разрешить сложные конфликты."
                            }
                        }
                    ],
                    "synonyms": [
                        "psychotherapist",
                        "counselor"
                    ],
                    "antonyms": [
                        "patient",
                        "client"
                    ],
                    "common_phrases": [
                        "see a therapist",
                        "family therapist",
                        "licensed therapist",
                        "talk to a therapist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист, который помогает людям справляться с психическими или эмоциональными проблемами посредством беседы; психотерапевт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "психотерапевт",
                                "target_lang_sense_clarification": "специалист по лечению психических расстройств"
                            },
                            {
                                "target_lang_word": "психолог",
                                "target_lang_sense_clarification": "консультант по психологическим вопросам"
                            },
                            {
                                "target_lang_word": "терапевт",
                                "target_lang_sense_clarification": "разг. в значении психотерапевт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5371a70d-94b2-3567-a6f9-a52fca94fb6d",
                    "sense_definition": "A person trained to provide a specific type of treatment or rehabilitation for an illness or physical injury.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medical_professional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The physical <w>therapist</w> gave him exercises to strengthen his injured leg.",
                            "target_lang_translations": {
                                "ru": "<w>Физиотерапевт</w> дал ему упражнения для укрепления травмированной ноги."
                            }
                        },
                        {
                            "text": "You might need a speech <w>therapist</w> if you have difficulty pronouncing certain words.",
                            "target_lang_translations": {
                                "ru": "Вам может понадобиться <w>логопед</w>, если у вас есть трудности с произношением определенных слов."
                            }
                        }
                    ],
                    "antonyms": [
                        "patient",
                        "client"
                    ],
                    "common_phrases": [
                        "physical therapist",
                        "occupational therapist",
                        "massage therapist",
                        "speech therapist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обученный предоставлять определенный вид лечения или реабилитации при болезни или физической травме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "физиотерапевт",
                                "target_lang_sense_clarification": "специалист по физической реабилитации"
                            },
                            {
                                "target_lang_word": "логопед",
                                "target_lang_sense_clarification": "специалист по коррекции речи"
                            },
                            {
                                "target_lang_word": "специалист",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
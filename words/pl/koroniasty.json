    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "eb84421f-35b9-3bb2-a865-1950001604d1",
                    "sense_definition": "Przypominający kształtem koronę, często w odniesieniu do rozłożystych drzew lub poroża zwierząt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na środku polany rósł stary, <w>koroniasty</w> dąb, dający cień w upalne dni.",
                            "target_lang_translations": {
                                "ru": "Посреди поляны рос старый <w>раскидистый</w> дуб, дающий тень в жаркие дни."
                            }
                        },
                        {
                            "text": "Myśliwy z podziwem obserwował jelenia, który nosił na głowie ciężkie, <w>koroniaste</w> poroże.",
                            "target_lang_translations": {
                                "ru": "Охотник с восхищением наблюдал за оленем, который носил на голове тяжелые <w>ветвистые</w> рога."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozłożysty"
                    ],
                    "common_phrases": [
                        "koroniaste drzewo",
                        "koroniaste poroże"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий формой корону, часто говорится о раскидистых деревьях или рогах животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскидистый",
                                "target_lang_sense_clarification": "о деревьях с широкой кроной"
                            },
                            {
                                "target_lang_word": "ветвистый",
                                "target_lang_sense_clarification": "о рогах или деревьях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81c578f8-cddf-344f-bc06-3156c96352fc",
                    "sense_definition": "Mający na głowie naturalną ozdobę z piór, rogów lub sierści, która przypomina koronę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biological_feature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żuraw <w>koroniasty</w> jest łatwo rozpoznawalny dzięki charakterystycznemu pióropuszowi na głowie.",
                            "target_lang_translations": {
                                "ru": "<w>Венценосный</w> журавль легко узнаваем благодаря характерному хохолку на голове."
                            }
                        },
                        {
                            "text": "W ogrodzie zoologicznym widzieliśmy rzadki gatunek gołębia, który był <w>koroniasty</w>.",
                            "target_lang_translations": {
                                "ru": "В зоопарке мы видели редкий вид голубя, который был <w>венценосным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "czubaty"
                    ],
                    "common_phrases": [
                        "żuraw koroniasty",
                        "gołąb koroniasty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий на голове естественное украшение из перьев, рогов или шерсти, напоминающее корону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "венценосный",
                                "target_lang_sense_clarification": "в названиях видов животных"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
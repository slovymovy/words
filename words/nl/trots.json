{
    "word_family": [
        "trotseren",
        "trotsheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fbaa450f-143a-33e4-93eb-a03e06e55f43",
                    "sense_definition": "Erg blij en voldaan over iets dat men heeft bereikt, bezit, of over iemand anders.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotion_positive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij was erg <w>trots</w> op haar behaalde diploma.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>гордилась</w> полученным дипломом.",
                                "en": "She was very <w>proud</w> of the diploma she obtained."
                            }
                        },
                        {
                            "text": "Wij zijn <w>trots</w> op onze kinderen en hun prestaties.",
                            "target_lang_translations": {
                                "ru": "Мы <w>гордимся</w> нашими детьми и их достижениями.",
                                "en": "We are <w>proud</w> of our children and their achievements."
                            }
                        }
                    ],
                    "synonyms": [
                        "fier"
                    ],
                    "common_phrases": [
                        "trots zijn op",
                        "zo trots als een pauw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень рад и удовлетворен тем, чего достиг, чем владеет, или кем-то другим.",
                        "en": "Very happy and satisfied about something one has achieved, possesses, or about someone else."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гордый",
                                "target_lang_sense_clarification": "испытывающий чувство гордости"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "proud",
                                "target_lang_sense_clarification": "feeling deep pleasure or satisfaction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "132e564f-a0c8-33ea-a62b-0702ab750b58",
                    "sense_definition": "Met het gevoel beter, belangrijker of meer waard te zijn dan anderen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emotion_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is te <w>trots</w> om zijn fouten toe te geven.",
                            "target_lang_translations": {
                                "ru": "Он слишком <w>горд</w>, чтобы признать свои ошибки.",
                                "en": "He is too <w>proud</w> to admit his mistakes."
                            }
                        },
                        {
                            "text": "Met een <w>trotse</w> blik keek de koning op zijn volk neer.",
                            "target_lang_translations": {
                                "ru": "С <w>гордым</w> видом король смотрел на свой народ сверху вниз.",
                                "en": "With a <w>haughty</w> look, the king looked down on his people."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een trotse houding",
                        "te trots zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "С чувством превосходства, собственной важности или большей ценности по сравнению с другими.",
                        "en": "Having a feeling of superiority, self-importance, or being worth more than others."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гордый",
                                "target_lang_sense_clarification": "высокомерный, надменный"
                            },
                            {
                                "target_lang_word": "надменный",
                                "target_lang_sense_clarification": "презрительно-гордый"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "proud",
                                "target_lang_sense_clarification": "arrogant, having too high an opinion of oneself"
                            },
                            {
                                "target_lang_word": "haughty",
                                "target_lang_sense_clarification": "arrogantly superior and disdainful"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e2d48675-d9bb-3982-b4c9-18f0ebbf3f39",
                    "sense_definition": "Het gevoel van eigenwaarde, eer of grote tevredenheid over een prestatie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>trots</w> werd gekrenkt door de harde kritiek.",
                            "target_lang_translations": {
                                "ru": "Его <w>гордость</w> была уязвлена резкой критикой.",
                                "en": "His <w>pride</w> was hurt by the harsh criticism."
                            }
                        },
                        {
                            "text": "Zij vertelde met gepaste <w>trots</w> over haar nieuwe baan.",
                            "target_lang_translations": {
                                "ru": "Она с подобающей <w>гордостью</w> рассказала о своей новой работе.",
                                "en": "She spoke with appropriate <w>pride</w> about her new job."
                            }
                        }
                    ],
                    "synonyms": [
                        "fierheid",
                        "fierte"
                    ],
                    "common_phrases": [
                        "met gepaste trots",
                        "zijn trots inslikken",
                        "nationale trots"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство собственного достоинства, чести или большого удовлетворения от достижения.",
                        "en": "The feeling of self-worth, honor, or great satisfaction regarding an achievement."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гордость",
                                "target_lang_sense_clarification": "чувство достоинства или удовлетворения"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "pride",
                                "target_lang_sense_clarification": "feeling of deep pleasure or satisfaction"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "4dcc0492-bf36-3777-b575-c46611921f7b",
                    "sense_definition": "In weerwil van; ondanks.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "preposition_contrast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Trots</w> de regen gingen we toch wandelen in het bos.",
                            "target_lang_translations": {
                                "ru": "<w>Несмотря на</w> дождь, мы всё же пошли гулять в лес.",
                                "en": "<w>Despite</w> the rain, we still went for a walk in the forest."
                            }
                        },
                        {
                            "text": "Hij slaagde voor het examen <w>trots</w> zijn slechte voorbereiding.",
                            "target_lang_translations": {
                                "ru": "Он сдал экзамен <w>несмотря на</w> свою плохую подготовку.",
                                "en": "He passed the exam <w>despite</w> his poor preparation."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trots alles"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Voornamelijk in België"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопреки; несмотря на.",
                        "en": "In spite of; despite."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несмотря на",
                                "target_lang_sense_clarification": "предлог уступки"
                            },
                            {
                                "target_lang_word": "вопреки",
                                "target_lang_sense_clarification": "наперекор чему-либо"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "despite",
                                "target_lang_sense_clarification": "without being affected by"
                            },
                            {
                                "target_lang_word": "in spite of",
                                "target_lang_sense_clarification": "regardless of"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
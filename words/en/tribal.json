    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3defdfe9-8a32-3443-8c7e-f9dea6766046",
                    "sense_definition": "Of, relating to, or characteristic of a tribe or tribes.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>tribal</w> elders gathered to discuss the future of their community.",
                            "target_lang_translations": {
                                "ru": "<w>Племенные</w> старейшины собрались, чтобы обсудить будущее своей общины."
                            }
                        },
                        {
                            "text": "Many <w>tribal</w> traditions have been passed down through generations.",
                            "target_lang_translations": {
                                "ru": "Многие <w>племенные</w> традиции передавались из поколения в поколение."
                            }
                        }
                    ],
                    "synonyms": [
                        "clan-based",
                        "ancestral"
                    ],
                    "antonyms": [
                        "non-tribal"
                    ],
                    "common_phrases": [
                        "tribal leader",
                        "tribal warfare",
                        "tribal society",
                        "tribal dance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к племени или характерный для него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "племенной",
                                "target_lang_sense_clarification": "относящийся к племени"
                            },
                            {
                                "target_lang_word": "родовой",
                                "target_lang_sense_clarification": "связанный с родом/кланом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "246f056c-cf66-3d4c-9a6d-10b3a00d468c",
                    "sense_definition": "Incorporating designs or styles typical of an indigenous people.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a rug featuring a bold <w>tribal</w> pattern.",
                            "target_lang_translations": {
                                "ru": "Она купила ковер с выразительным <w>этническим</w> узором."
                            }
                        },
                        {
                            "text": "The fashion designer included <w>tribal</w> prints in his summer collection.",
                            "target_lang_translations": {
                                "ru": "Модельер включил <w>этнические</w> принты в свою летнюю коллекцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "ethnic"
                    ],
                    "common_phrases": [
                        "tribal tattoo",
                        "tribal art",
                        "tribal design",
                        "tribal print"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий узоры или стили, типичные для коренных народов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этнический",
                                "target_lang_sense_clarification": "о стиле или узоре"
                            },
                            {
                                "target_lang_word": "в стиле трайбл",
                                "target_lang_sense_clarification": "заимствование, о дизайне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f5f2687-1d92-37d0-a766-91f3cc1c8d29",
                    "sense_definition": "Relating to a taxonomic tribe in biology.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>tribal</w> classification of these plants is still a subject of debate.",
                            "target_lang_translations": {
                                "ru": "<w>Трибальная</w> классификация этих растений всё ещё является предметом споров."
                            }
                        },
                        {
                            "text": "Scientists analyzed the <w>tribal</w> history of the insect species.",
                            "target_lang_translations": {
                                "ru": "Ученые проанализировали <w>трибальную</w> историю этого вида насекомых."
                            }
                        }
                    ],
                    "synonyms": [
                        "phylogenic"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к таксономической категории \"триба\" в биологии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трибальный",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b23c051-f7e1-3ab5-b496-dc11e9220f72",
                    "sense_definition": "A member of a tribe.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government implemented new policies to protect the rights of local <w>tribals</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство внедрило новую политику для защиты прав местных <w>представителей племен</w>."
                            }
                        },
                        {
                            "text": "Many <w>tribals</w> in the region rely on the forest for their livelihood.",
                            "target_lang_translations": {
                                "ru": "Многие <w>члены племен</w> в этом регионе зависят от леса как источника средств к существованию."
                            }
                        }
                    ],
                    "synonyms": [
                        "tribesman"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in South Asia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член племени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "член племени",
                                "target_lang_sense_clarification": "человек"
                            },
                            {
                                "target_lang_word": "представитель племени",
                                "target_lang_sense_clarification": "официально/формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aff9c36e-3eff-3802-ab16-af0e10f6ce12",
                    "sense_definition": "A design or image, especially a tattoo, influenced by indigenous art styles.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to get a large <w>tribal</w> on his upper arm.",
                            "target_lang_translations": {
                                "ru": "Он решил сделать большое тату <w>трайбл</w> на плече."
                            }
                        },
                        {
                            "text": "The artist's portfolio is full of intricate <w>tribals</w>.",
                            "target_lang_translations": {
                                "ru": "Портфолио художника полно сложных узоров <w>трайбл</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get a tribal"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рисунок или изображение (особенно татуировка) в стиле искусства коренных народов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трайбл",
                                "target_lang_sense_clarification": "стиль татуировки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

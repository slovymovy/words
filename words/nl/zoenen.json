            "senses": [
                {
                    "sense_id": "13694a86-fa6c-391f-bff2-8a267449fb10",
                    "sense_definition": "Meervoud van <w>zoen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kreeg veel <w>zoenen</w> van mijn oma toen ik jarig was.",
                            "target_lang_translations": {
                                "en": "I got many <w>kisses</w> from my grandma when it was my birthday."

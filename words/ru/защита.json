    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5fb075d3-6a87-368f-917a-4512967aec5c",
                    "sense_definition": "Действие по ограждению кого-либо или чего-либо от опасности, нападения или враждебных действий; обеспечение безопасности.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Армия обеспечивает надёжную <защиту> государственных границ.",
                            "target_lang_translations": {
                                "en": "The army ensures reliable <protection> of the state borders."
                            }
                        },
                        {
                            "text": "Ему потребовалась <защита> от назойливого внимания журналистов.",
                            "target_lang_translations": {
                                "en": "He needed <protection> from the intrusive attention of journalists."
                            }
                        }
                    ],
                    "synonyms": [
                        "оборона"
                    ],
                    "antonyms": [
                        "нападение",
                        "атака",
                        "агрессия"
                    ],
                    "common_phrases": [
                        "защита прав",
                        "защита окружающей среды",
                        "встать на защиту",
                        "средства защиты"
                    ],
                    "target_lang_definitions": {
                        "en": "The action of shielding someone or something from danger, attack, or hostile actions; ensuring safety."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "protection",
                                "target_lang_sense_clarification": "general safety"
                            },
                            {
                                "target_lang_word": "defence",
                                "target_lang_sense_clarification": "resistance against attack"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "341670e9-5157-3768-aee1-eeb901b6245b",
                    "sense_definition": "Предмет, устройство или средство, которое предохраняет, укрывает или ограждает от чего-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тёплая куртка — это отличная <защита> от холодного ветра и дождя.",
                            "target_lang_translations": {
                                "en": "A warm jacket is excellent <protection> against cold wind and rain."
                            }
                        },
                        {
                            "text": "Эта программа служит <защитой> компьютера от вредоносных вирусов.",
                            "target_lang_translations": {
                                "en": "This program serves as <protection> for the computer against malicious viruses."
                            }
                        }
                    ],
                    "common_phrases": [
                        "биологическая защита",
                        "защита от солнца",
                        "защита информации"
                    ],
                    "target_lang_definitions": {
                        "en": "An object, device, or means that preserves, covers, or shields from something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "protection",
                                "target_lang_sense_clarification": "barrier or means of safety"
                            },
                            {
                                "target_lang_word": "shield",
                                "target_lang_sense_clarification": "protective barrier"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ffc4604-f828-312c-9cef-8fb14d96478e",
                    "sense_definition": "В командных видах спорта — игроки или тактика, основной задачей которых является оборона своих ворот и предотвращение гола.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_defense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша команда отлично сыграла в <защите> и не пропустила ни одного мяча.",
                            "target_lang_translations": {
                                "en": "Our team played excellently in <defence> and did not concede a single goal."
                            }
                        },
                        {
                            "text": "Тренер решил укрепить <защиту> новым опытным игроком.",
                            "target_lang_translations": {
                                "en": "The coach decided to strengthen the <defence> with a new experienced player."
                            }
                        }
                    ],
                    "synonyms": [
                        "оборона"
                    ],
                    "antonyms": [
                        "нападение",
                        "атака"
                    ],
                    "common_phrases": [
                        "игра в защите",
                        "провал в защите",
                        "глухая защита"
                    ],
                    "target_lang_definitions": {
                        "en": "In team sports, the players or tactics whose main task is to defend their goal and prevent scoring."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defence",
                                "target_lang_sense_clarification": "sports tactics or team members"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d50c5f9b-4b25-3efc-af3c-93a30a7fcaaf",
                    "sense_definition": "Сторона в судебном процессе, представляющая интересы обвиняемого или ответчика.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_defense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Защита> предоставила суду неопровержимые доказательства невиновности подсудимого.",
                            "target_lang_translations": {
                                "en": "The <defence> provided the court with irrefutable evidence of the defendant's innocence."
                            }
                        },
                        {
                            "text": "Свидетель <защиты> не смог явиться на сегодняшнее заседание.",
                            "target_lang_translations": {
                                "en": "The witness for the <defence> could not appear at today's hearing."
                            }
                        }
                    ],
                    "common_phrases": [
                        "свидетель защиты",
                        "линия защиты",
                        "адвокатская защита"
                    ],
                    "target_lang_definitions": {
                        "en": "The party in a legal process representing the interests of the accused or defendant."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defence",
                                "target_lang_sense_clarification": "legal party"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65ed4fa5-2003-35c5-8d55-2c9fced375fb",
                    "sense_definition": "Публичное представление научной работы (диссертации, диплома) для получения учёной степени или квалификации.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "academic_defense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Защита> докторской диссертации прошла успешно и вызвала большой интерес.",
                            "target_lang_translations": {
                                "en": "The <defence> of the doctoral thesis was successful and generated great interest."
                            }
                        },
                        {
                            "text": "Студенты очень волновались, так как завтра им предстояла <защита> дипломов.",
                            "target_lang_translations": {
                                "en": "The students were very worried, as their diploma <defence> was scheduled for tomorrow."
                            }
                        }
                    ],
                    "common_phrases": [
                        "защита диссертации",
                        "защита диплома",
                        "день защиты"
                    ],
                    "target_lang_definitions": {
                        "en": "Public presentation of a scientific work (dissertation, thesis) to obtain a degree or qualification."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defence",
                                "target_lang_sense_clarification": "academic presentation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfbffb12-c7a4-3700-aa6f-caf7fb51d247",
                    "sense_definition": "В шахматах — определённая система ходов чёрными фигурами в начале партии для отражения атаки белых.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chess_defense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гроссмейстер неожиданно для соперника разыграл староиндийскую <защиту>.",
                            "target_lang_translations": {
                                "en": "The grandmaster unexpectedly played the King's Indian <Defence> against his opponent."
                            }
                        },
                        {
                            "text": "Эта <защита> требует от игрока точного знания теории дебютов.",
                            "target_lang_translations": {
                                "en": "This <defence> requires the player to have precise knowledge of opening theory."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сицилианская защита",
                        "защита Каро-Канн",
                        "французская защита"
                    ],
                    "target_lang_definitions": {
                        "en": "In chess, a specific system of moves with black pieces at the beginning of a game to repel white's attack."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defence",
                                "target_lang_sense_clarification": "chess opening"
                            }
                        ]
                    }
                }
            ]
        }
    ]

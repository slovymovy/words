    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3372075c-e32f-3663-8919-bbe6ddc29374",
                    "sense_definition": "Вращающийся или снабжённый вращающимся устройством (обычно о механизмах, насосах).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для перекачки вязких жидкостей на производстве используют <w>коловратный</w> насос.",
                            "target_lang_translations": {
                                "en": "For pumping viscous liquids in production, a <w>rotary</w> pump is used."
                            }
                        },
                        {
                            "text": "Этот <w>коловратный</w> механизм обеспечивает равномерную подачу масла в систему.",
                            "target_lang_translations": {
                                "en": "This <w>rotary</w> mechanism ensures a uniform supply of oil to the system."
                            }
                        }
                    ],
                    "synonyms": [
                        "роторный"
                    ],
                    "common_phrases": [
                        "коловратный насос",
                        "коловратный компрессор",
                        "коловратный механизм"
                    ],
                    "target_lang_definitions": {
                        "en": "Rotating or equipped with a rotating device (usually referring to mechanisms or pumps)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rotary",
                                "target_lang_sense_clarification": "technical term for mechanisms and pumps"
                            },
                            {
                                "target_lang_word": "rotatory",
                                "target_lang_sense_clarification": "relating to or causing rotation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62fe91f3-e1c7-39bb-87a2-eb50b0f5fa68",
                    "sense_definition": "Изменчивый, непостоянный, подверженный частым переменам (обычно о судьбе или течении жизни).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "changeability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом <w>коловратном</w> мире трудно найти что-то действительно вечное и незыблемое.",
                            "target_lang_translations": {
                                "en": "In this <w>fickle</w> world, it is difficult to find something truly eternal and unshakable."
                            }
                        },
                        {
                            "text": "Судьба его была <w>коловратной</w>, полной неожиданных взлётов и падений.",
                            "target_lang_translations": {
                                "en": "His fate was <w>inconstant</w>, full of unexpected ups and downs."
                            }
                        }
                    ],
                    "synonyms": [
                        "изменчивый",
                        "непостоянный"
                    ],
                    "antonyms": [
                        "постоянный"
                    ],
                    "common_phrases": [
                        "коловратная судьба",
                        "коловратный мир",
                        "жизнь коловратная"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Changeable, inconstant, subject to frequent changes (usually referring to fate or the course of life)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fickle",
                                "target_lang_sense_clarification": "likely to change, especially regarding fate or fortune"
                            },
                            {
                                "target_lang_word": "inconstant",
                                "target_lang_sense_clarification": "frequently changing or variable"
                            },
                            {
                                "target_lang_word": "changeable",
                                "target_lang_sense_clarification": "general term for something not fixed"
                            }
                        ]
                    }
                }
            ]
        }

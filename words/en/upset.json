    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "65045610-4e53-36b0-9465-5b63fb9b7cb4",
                    "sense_definition": "Angry, distressed, or unhappy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was <w>upset</w> when she refused his friendship.",

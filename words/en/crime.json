    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "91c51093-d4f7-331b-91da-b5cc0a325423",
                    "sense_definition": "A specific act committed in violation of the law, especially criminal law.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "illegal_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police arrested the man shortly after he committed the <w>crime</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция арестовала мужчину вскоре после того, как он совершил <w>преступление</w>."
                            }
                        },
                        {
                            "text": "Stealing a car is a serious <w>crime</w> that can lead to years in prison.",
                            "target_lang_translations": {
                                "ru": "Угон автомобиля — это серьезное <w>преступление</w>, которое может привести к годам тюремного заключения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "commit a crime",
                        "violent crime",
                        "petty crime",
                        "solve a crime",
                        "scene of the crime"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное действие, совершенное в нарушение закона, особенно уголовного права."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преступление",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "злодеяние",
                                "target_lang_sense_clarification": "тяжкое преступление, книжное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c073f55-6c1f-35c4-8e16-e1010b3af3a4",
                    "sense_definition": "Illegal activities in general; the habit or practice of breaking the law.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "illegal_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mayor has promised to fight <w>crime</w> in the downtown area.",
                            "target_lang_translations": {
                                "ru": "Мэр пообещал бороться с <w>преступностью</w> в центре города."
                            }
                        },
                        {
                            "text": "High unemployment often leads to an increase in <w>crime</w>.",
                            "target_lang_translations": {
                                "ru": "Высокая безработица часто приводит к росту <w>преступности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "criminality",
                        "delinquency"
                    ],
                    "common_phrases": [
                        "organized crime",
                        "fight crime",
                        "crime rate",
                        "crime wave",
                        "crime prevention"
                    ],
                    "target_lang_definitions": {
                        "ru": "Незаконная деятельность в целом; привычка или практика нарушения закона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преступность",
                                "target_lang_sense_clarification": "социальное явление"
                            },
                            {
                                "target_lang_word": "криминал",
                                "target_lang_sense_clarification": "разговорное, собирательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69248d10-e52f-3cfa-be4e-beaacd9ddad1",
                    "sense_definition": "An action that is considered very wrong, shameful, or wasteful, though not necessarily illegal.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "shameful_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is a <w>crime</w> to waste so much good food when people are hungry.",
                            "target_lang_translations": {
                                "ru": "Это <w>преступление</w> — выбрасывать столько хорошей еды, когда люди голодают."
                            }
                        },
                        {
                            "text": "It would be a <w>crime</w> not to visit the museum while we are in Paris.",
                            "target_lang_translations": {
                                "ru": "Было бы <w>преступлением</w> не посетить музей, пока мы в Париже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "it is a crime",
                        "shame and a crime"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поступок, который считается очень неправильным, постыдным или расточительным, хотя и не обязательно незаконным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преступление",
                                "target_lang_sense_clarification": "в переносном смысле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0458fa4e-f200-3222-94e1-ed15375f362b",
                    "sense_definition": "That which occasions or causes a crime.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poet wrote of the ancient <w>crime</w> that led to the fall of man.",
                            "target_lang_translations": {
                                "ru": "Поэт писал о древнем <w>грехе</w>, который привел к грехопадению человека."
                            }
                        },
                        {
                            "text": "In the old text, the apple was the <w>crime</w> of their undoing.",
                            "target_lang_translations": {
                                "ru": "В старинном тексте яблоко стало тем <w>преступлением</w>, что привело к их гибели."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что служит поводом или причиной преступления."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "df355357-6dde-3188-b49d-b1d73e9762ec",
                    "sense_definition": "To commit a crime or engage in criminal activity.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "commit_crime",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent his teenage years <w>criming</w> around the neighborhood with his friends.",
                            "target_lang_translations": {
                                "ru": "Он провел свои подростковые годы, <w>хулиганя</w> с друзьями по всему району."
                            }
                        },
                        {
                            "text": "The gang was known for <w>criming</w> purely for the thrill of it.",
                            "target_lang_translations": {
                                "ru": "Эта банда была известна тем, что <w>совершала преступления</w> исключительно ради острых ощущений."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершать преступление или заниматься преступной деятельностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарушать закон",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "хулиганить",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "399e6a1f-9eff-30bb-8992-d6207e41eb6c",
                    "sense_definition": "A conclusive success or victory following an effort, conflict, or difficult struggle.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "success_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team celebrated their <w>triumph</w> in the championship final.",
                            "target_lang_translations": {
                                "ru": "Команда праздновала свой <w>триумф</w> в финале чемпионата."
                            }
                        },
                        {
                            "text": "Her graduation was a moment of personal <w>triumph</w> after years of hard work.",
                            "target_lang_translations": {
                                "ru": "Её выпускной стал моментом личного <w>триумфа</w> после долгих лет упорного труда."
                            }
                        }
                    ],
                    "synonyms": [
                        "victory",
                        "success",
                        "conquest",
                        "achievement",
                        "win"
                    ],
                    "antonyms": [
                        "defeat",
                        "failure",
                        "loss"
                    ],
                    "common_phrases": [
                        "triumph over adversity",
                        "great triumph",
                        "personal triumph",
                        "moment of triumph"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решающий успех или победа после усилий, конфликта или трудной борьбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "триумф",
                                "target_lang_sense_clarification": "блестящий успех"
                            },
                            {
                                "target_lang_word": "победа",
                                "target_lang_sense_clarification": "успех в борьбе"
                            },
                            {
                                "target_lang_word": "торжество",
                                "target_lang_sense_clarification": "великое достижение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b65c42d-6e20-3910-9986-42371507b879",
                    "sense_definition": "A state of great joy, pride, or exultation resulting from success or victory.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "success_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He raised his arms in <w>triumph</w> as he crossed the finish line first.",
                            "target_lang_translations": {
                                "ru": "Он вскинул руки в <w>ликовании</w>, первым пересекая финишную черту."
                            }
                        },
                        {
                            "text": "The crowd roared in <w>triumph</w> when their team scored the winning goal.",
                            "target_lang_translations": {
                                "ru": "Толпа взревела от <w>восторга</w>, когда их команда забила победный гол."
                            }
                        }
                    ],
                    "synonyms": [
                        "exultation",
                        "jubilation",
                        "elation",
                        "joy"
                    ],
                    "antonyms": [
                        "despair",
                        "sorrow"
                    ],
                    "common_phrases": [
                        "in triumph",
                        "shout in triumph",
                        "smile in triumph"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние огромной радости, гордости или ликования от успеха или победы."

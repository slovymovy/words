        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d9668418-5db3-3f4f-957e-a86979eda04e",
                    "sense_definition": "The activity or action of moving on foot at a regular pace for exercise or pleasure.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "activity_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Walking</w> is excellent exercise for people of all ages.",
                            "target_lang_translations": {
                                "ru": "<w>Ходьба</w> — отличное упражнение для людей всех возрастов."
                            }
                        },
                        {
                            "text": "The hotel is within <w>walking</w> distance of the city center.",
                            "target_lang_translations": {
                                "ru": "Отель находится в пределах <w>пешей</w> доступности от центра города."
                            }
                        }
                    ],
                    "synonyms": [
                        "strolling",
                        "hiking",
                        "ambulation"
                    ],
                    "antonyms": [
                        "running"
                    ],
                    "common_phrases": [
                        "go walking",
                        "walking distance",
                        "keep walking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс перемещения пешком в обычном темпе ради упражнения или удовольствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходьба",
                                "target_lang_sense_clarification": "процесс перемещения пешком"
                            },
                            {
                                "target_lang_word": "прогулка",
                                "target_lang_sense_clarification": "хождение ради удовольствия"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "632130b4-8bb3-3e34-8f36-ff77639b3366",
                    "sense_definition": "A container used for holding or heating glue.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter dipped his brush into the hot <w>gluepot</w>.",
                            "target_lang_translations": {
                                "ru": "Плотник окунул кисть в горячую <w>клееварку</w>."
                            }
                        },
                        {
                            "text": "Please clean the <w>gluepot</w> thoroughly before adding fresh adhesive.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, тщательно очистите <w>клееварку</w> перед добавлением свежего клея."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ёмкость, используемая для хранения или нагревания клея."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клееварка",
                                "target_lang_sense_clarification": "сосуд для варки клея"
                            },
                            {
                                "target_lang_word": "банка с клеем",
                                "target_lang_sense_clarification": "ёмкость для клея"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c78c3d6-cc61-33a3-8d57-7422d93819b6",
                    "sense_definition": "A sports playing field that has become very muddy and difficult to move on, usually due to rain.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sports_jargon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heavy rain turned the stadium pitch into a complete <w>gluepot</w>.",
                            "target_lang_translations": {
                                "ru": "Сильный дождь превратил поле стадиона в сплошное <w>месиво</w>."
                            }
                        },
                        {
                            "text": "The horses struggled to maintain speed while running through the <w>gluepot</w>.",
                            "target_lang_translations": {
                                "ru": "Лошади с трудом поддерживали скорость, бежа по <w>вязкой грязи</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивное поле, которое стало очень грязным и вязким, обычно из-за дождя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "месиво",
                                "target_lang_sense_clarification": "о грязном, размокшем грунте"
                            },
                            {
                                "target_lang_word": "грязь",
                                "target_lang_sense_clarification": "вязкое покрытие поля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "122f4c07-5ef1-341a-9d18-5c16fac180e6",
                    "sense_definition": "A difficult, awkward, or messy situation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "figurative_situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He found himself in a real <w>gluepot</w> after losing the important documents.",
                            "target_lang_translations": {
                                "ru": "Он попал в настоящий <w>переплёт</w> после потери важных документов."
                            }
                        },
                        {
                            "text": "The negotiation became a <w>gluepot</w> that took weeks to resolve.",
                            "target_lang_translations": {
                                "ru": "Переговоры превратились в <w>трясину</w>, на преодоление которой ушли недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "sticky wicket"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудная, неловкая или запутанная ситуация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переплёт",
                                "target_lang_sense_clarification": "неприятная ситуация"
                            },
                            {
                                "target_lang_word": "трясина",
                                "target_lang_sense_clarification": "затягивающая ситуация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "incredible",
        "credibility",
        "credible",
        "credibly",
        "incredibility"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d1fd7e3a-bccb-3368-bc44-4e3d155a2083",
                    "sense_definition": "To a great extent or degree; extremely.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The view from the top of the mountain was <w>incredibly</w> beautiful.",
                            "target_lang_translations": {
                                "ru": "Вид с вершины горы был <w>невероятно</w> красивым."
                            }
                        },
                        {
                            "text": "She felt <w>incredibly</w> lucky to have such supportive friends.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала, что ей <w>невероятно</w> повезло иметь таких отзывчивых друзей."
                            }
                        }
                    ],
                    "synonyms": [
                        "unbelievably",
                        "unbelievably"
                    ],
                    "common_phrases": [
                        "incredibly hard",
                        "incredibly lucky",
                        "incredibly fast",
                        "incredibly important"
                    ],
                    "target_lang_definitions": {
                        "ru": "В значительной степени или мере; чрезвычайно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невероятно",
                                "target_lang_sense_clarification": "в высшей степени"
                            },
                            {
                                "target_lang_word": "чрезвычайно",
                                "target_lang_sense_clarification": "очень"
                            },
                            {
                                "target_lang_word": "потрясающе",
                                "target_lang_sense_clarification": "удивительно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09f110fe-0639-34f6-8f64-228ddd96437f",
                    "sense_definition": "Used to introduce a statement that is surprising or difficult to believe, emphasizing its truth despite expectations.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sentence_adverb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Incredibly</w>, he survived the crash without a single scratch.",
                            "target_lang_translations": {
                                "ru": "<w>Невероятно</w>, но он выжил в аварии без единой царапины."
                            }
                        },
                        {
                            "text": "<w>Incredibly</w> enough, the lost dog found its way home after three weeks.",
                            "target_lang_translations": {
                                "ru": "<w>Невероятно</w>, но потерявшаяся собака нашла дорогу домой спустя три недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "unbelievably"
                    ],
                    "common_phrases": [
                        "incredibly enough"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения утверждения, которое вызывает удивление или в которое трудно поверить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невероятно",
                                "target_lang_sense_clarification": "удивительно"
                            },
                            {
                                "target_lang_word": "поразительно",
                                "target_lang_sense_clarification": "вызывает изумление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08384e6d-d973-365e-a83b-77c956cea3e9",
                    "sense_definition": "In a manner that is impossible or difficult to believe.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "literal_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The magician performed <w>incredibly</w>, leaving the audience in total shock.",
                            "target_lang_translations": {
                                "ru": "Фокусник выступил <w>невероятно</w>, повергнув публику в полный шок."
                            }
                        },
                        {
                            "text": "He stared <w>incredibly</w> at the strange lights in the sky, unable to trust his eyes.",
                            "target_lang_translations": {
                                "ru": "Он смотрел <w>невероятно</w> на странные огни в небе, не в силах поверить своим глазам."
                            }
                        }
                    ],
                    "synonyms": [
                        "unbelievably"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, в который невозможно или трудно поверить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невероятно",
                                "target_lang_sense_clarification": "немыслимо"
                            },
                            {
                                "target_lang_word": "неправдоподобно",
                                "target_lang_sense_clarification": "не похоже на правду"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комментировать",
                                "target_lang_sense_clarification": "давать пояснения"
                            },
                            {
                                "target_lang_word": "высказываться",
                                "target_lang_sense_clarification": "выражать мнение"
                            }

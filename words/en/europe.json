            "senses": [
                {
                    "sense_id": "0ad7c225-3613-316a-99b5-4829ca616675",
                    "sense_definition": "A continent located in the Northern Hemisphere, west of Asia and north of Africa.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "France and Germany are large countries in <w>Europe</w>.",
                            "target_lang_translations": {
                                "ru": "Франция и Германия — большие страны в <w>Европе</w>."
                            }
                        },
                        {
                            "text": "She plans to travel across <w>Europe</w> by train next summer.",
                            "target_lang_translations": {
                                "ru": "Она планирует путешествовать по <w>Европе</w> на поезде следующим летом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Western Europe",
                        "Eastern Europe",
                        "Central Europe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть света в Северном полушарии, к западу от Азии и к северу от Африки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Европа",
                                "target_lang_sense_clarification": "континент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0933246-86a4-3484-830e-f43c4980cc00",
                    "sense_definition": "The European Union considered as a political and economic entity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "politics",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "New trade laws in <w>Europe</w> will affect how we do business.",
                            "target_lang_translations": {
                                "ru": "Новые торговые законы в <w>Европе</w> повлияют на то, как мы ведем бизнес."
                            }
                        },
                        {
                            "text": "The leaders of <w>Europe</w> met to discuss the economic crisis.",
                            "target_lang_translations": {
                                "ru": "Лидеры <w>Европы</w> встретились, чтобы обсудить экономический кризис."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Европейский союз как политическое и экономическое образование."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Европа",
                                "target_lang_sense_clarification": "Евросоюз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "575fa51f-5269-3360-8290-0ad045c3cc1a",
                    "sense_definition": "The continental mainland of Europe, distinguishing it from offshore islands like Great Britain or Ireland.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d5354a6b-89ad-304f-b281-c257301ed114",
                    "sense_definition": "Tradycyjna polska potrawa z duszonej kapusty kiszonej i świeżej z dodatkiem różnych gatunków mięsa, wędlin i grzybów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na świąteczny obiad podano gorący <w>bigos</w> z ciemnym pieczywem.",
                            "target_lang_translations": {
                                "ru": "На праздничный обед подали горячий <w>бигос</w> с черным хлебом."
                            }
                        },
                        {
                            "text": "Mój tata gotuje najlepszy <w>bigos</w> myśliwski, jakiego kiedykolwiek jadłem.",
                            "target_lang_translations": {
                                "ru": "Мой папа готовит лучший охотничий <w>бигос</w>, который я когда-либо ел."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bigos myśliwski",
                        "garnek bigosu",
                        "tradycyjny bigos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционное польское блюдо из тушеной квашеной и свежей капусты с добавлением различных видов мяса, колбас и грибов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бигос",
                                "target_lang_sense_clarification": "блюдо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5fcc382-cc9a-3671-ac74-aef630a4f9d1",
                    "sense_definition": "Duże zamieszanie, nieporządek lub kłopotliwa sytuacja, z której trudno wyjść.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Swoim nieodpowiedzialnym zachowaniem narobiłeś nam niezłego <w>bigosu</w> w firmie.",
                            "target_lang_translations": {
                                "ru": "Своим безответственным поведением ты заварил нам в фирме порядочную <w>кашу</w>."
                            }
                        },
                        {
                            "text": "W niezły <w>bigos</w> się wpakowałeś, zapominając o ważnym spotkaniu z klientem.",
                            "target_lang_translations": {
                                "ru": "Ты попал в серьезный <w>переплёт</w>, забыв о важной встрече с клиентом."
                            }
                        }
                    ],
                    "synonyms": [
                        "bałagan",
                        "zamieszanie",
                        "kłopot"
                    ],
                    "antonyms": [
                        "porządek"
                    ],
                    "common_phrases": [
                        "narobić bigosu",
                        "niezły bigos"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая неразбериха, беспорядок или затруднительная ситуация, из которой трудно выбраться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каша",
                                "target_lang_sense_clarification": "перен. путаница, беспорядок"
                            },
                            {
                                "target_lang_word": "переплёт",
                                "target_lang_sense_clarification": "неприятная ситуация"
                            },
                            {
                                "target_lang_word": "неразбериха",
                                "target_lang_sense_clarification": "хаос"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "929c47dd-b614-3b18-bcca-9edad0228246",
                    "sense_definition": "Verdrietig of droevig gestemd; niet vrolijk.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vrouw keek erg <w>triest</w> toen ze het slechte nieuws hoorde.",
                            "target_lang_translations": {
                                "en": "The woman looked very <w>sad</w> when she heard the bad news."
                            }
                        },
                        {
                            "text": "Ik voel me vandaag een beetje <w>triest</w> zonder duidelijke reden.",
                            "target_lang_translations": {
                                "en": "I feel a bit <w>sad</w> today for no clear reason."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdrietig"
                    ],
                    "antonyms": [
                        "blij",
                        "vrolijk"
                    ],
                    "common_phrases": [
                        "zich triest voelen",
                        "een triest gezicht"
                    ],
                    "target_lang_definitions": {
                        "en": "Feeling sorrowful or unhappy; not cheerful."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sad",
                                "target_lang_sense_clarification": "feeling sorrow"
                            },
                            {
                                "target_lang_word": "unhappy",
                                "target_lang_sense_clarification": "not cheerful"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15ee5eac-9443-3989-9414-f23f21d74a37",
                    "sense_definition": "Somber stemmend; een gevoel van droefheid oproepend door de sfeer of uitstraling.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "atmosphere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was een gure, <w>trieste</w> dag met veel regen en windvlagen.",
                            "target_lang_translations": {
                                "en": "It was a raw, <w>dreary</w> day with lots of rain and gusts of wind."
                            }
                        },
                        {
                            "text": "Het verlaten huis zag er grijs en <w>triest</w> uit in de mist.",
                            "target_lang_translations": {
                                "en": "The abandoned house looked grey and <w>sad</w> in the fog."
                            }
                        }
                    ],
                    "antonyms": [
                        "opgewekt",
                        "zonnig"
                    ],
                    "common_phrases": [
                        "triest weer",
                        "een trieste dag",
                        "een trieste omgeving"
                    ],
                    "target_lang_definitions": {
                        "en": "Causing gloom; evoking a feeling of sadness through atmosphere or appearance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dreary",
                                "target_lang_sense_clarification": "dull and depressing"
                            },
                            {
                                "target_lang_word": "gloomy",
                                "target_lang_sense_clarification": "dark and depressing"
                            },
                            {
                                "target_lang_word": "sad",
                                "target_lang_sense_clarification": "evoking sadness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e2426d7-7f9b-3640-b17e-f2f5d4610e37",
                    "sense_definition": "Betreurenswaardig, armzalig of zielig; een situatie die medelijden of teleurstelling opwekt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <w>triest</w> dat er nog steeds zoveel armoede is in de wereld.",
                            "target_lang_translations": {
                                "en": "It is <w>sad</w> that there is still so much poverty in the world."
                            }
                        },
                        {
                            "text": "De ruzie om de erfenis was een <w>trieste</w> vertoning voor de hele familie.",
                            "target_lang_translations": {
                                "en": "The argument over the inheritance was a <w>sad</w> spectacle for the whole family."
                            }
                        }
                    ],
                    "synonyms": [
                        "tragisch"
                    ],
                    "common_phrases": [
                        "een trieste vertoning",
                        "een triest geval",
                        "diep triest"
                    ],
                    "target_lang_definitions": {
                        "en": "Regrettable, pitiful, or pathetic; a situation evoking pity or disappointment."

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8e5d87aa-2c18-39ef-a5d2-df0208382634",
                    "sense_definition": "Дать кому-либо часть того, что имеешь, или позволить пользоваться этим вместе с собой.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "share_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мальчик решил <поделиться> яблоком со своим лучшим другом.",
                            "target_lang_translations": {
                                "en": "The boy decided to <w>share</w> the apple with his best friend."
                            }
                        },
                        {
                            "text": "Ты не мог бы <поделиться> учебником, так как я забыл свой дома?",
                            "target_lang_translations": {
                                "en": "Could you <w>share</w> the textbook, since I forgot mine at home?"
                            }
                        }
                    ],
                    "synonyms": [
                        "удели́ть"
                    ],
                    "common_phrases": [
                        "поделиться едой",
                        "поделиться деньгами",
                        "поделиться игрушками"
                    ],
                    "target_lang_definitions": {
                        "en": "To give a portion of something to others or allow them to use it with you."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "share",
                                "target_lang_sense_clarification": "give a portion or use jointly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddae3cff-a261-3fa9-9127-04789027628d",
                    "sense_definition": "Рассказать кому-либо о своих мыслях, чувствах, планах или новостях.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "share_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <поделиться> с вами радостной новостью о моём повышении.",
                            "target_lang_translations": {
                                "en": "I want to <w>share</w> the happy news about my promotion with you."
                            }
                        },
                        {
                            "text": "Автор книги спешит <поделиться> опытом с молодыми специалистами.",
                            "target_lang_translations": {
                                "en": "The author of the book is eager to <w>share</w> his experience with young specialists."
                            }
                        }
                    ],
                    "synonyms": [
                        "рассказа́ть",
                        "сообщи́ть"
                    ],
                    "antonyms": [
                        "скры́ть",
                        "утаи́ть",
                        "промолча́ть"
                    ],
                    "common_phrases": [
                        "поделиться впечатлениями",
                        "поделиться опытом",
                        "поделиться секретом",
                        "поделиться мнением",
                        "поделиться новостями"
                    ],
                    "target_lang_definitions": {
                        "en": "To tell someone about one's thoughts, feelings, plans, or news."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "share",
                                "target_lang_sense_clarification": "communicate information or feelings"
                            },
                            {
                                "target_lang_word": "confide",
                                "target_lang_sense_clarification": "tell someone something personal or private"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1c9ec44-e741-364a-9c5b-48679a4c2157",
                    "sense_definition": "Произвести раздел чего-либо общего между собой и другими участниками.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "share_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наследники долго не могли <поделиться> имуществом, оставшимся от дедушки.",
                            "target_lang_translations": {
                                "en": "The heirs could not <w>divide</w> the property left by their grandfather for a long time."
                            }
                        },
                        {
                            "text": "Мы договорились честно <поделиться> прибылью от нашего совместного проекта.",
                            "target_lang_translations": {
                                "en": "We agreed to fairly <w>split</w> the profits from our joint project."
                            }
                        }
                    ],
                    "synonyms": [
                        "раздели́ть"
                    ],
                    "common_phrases": [
                        "поделиться поровну",
                        "поделиться по-братски"
                    ],
                    "target_lang_definitions": {
                        "en": "To divide something common among oneself and others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "split",
                                "target_lang_sense_clarification": "divide into parts or shares"
                            },
                            {
                                "target_lang_word": "divide",
                                "target_lang_sense_clarification": "distribute among a group"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
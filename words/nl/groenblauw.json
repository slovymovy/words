    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "18ce72ec-9977-35fd-a2e8-f2c3eafc3d73",
                    "sense_definition": "Met een kleur die het midden houdt tussen groen en blauw.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "color_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zijkant van het gebouw was een egaal <w>groenblauw</w> vlak.",
                            "target_lang_translations": {
                                "en": "The side of the building was a solid <w>blue-green</w> surface."
                            }
                        },
                        {
                            "text": "Hij rijdt in een opvallende <w>groenblauwe</w> auto door de stad.",
                            "target_lang_translations": {
                                "en": "He drives through the city in a striking <w>teal</w> car."
                            }
                        },
                        {
                            "text": "Ik keek naar het <w>groenblauwe</w> water dat rustig tegen de kade klotste.",
                            "target_lang_translations": {
                                "en": "I looked at the <w>blue-green</w> water quietly lapping against the quay."
                            }
                        }
                    ],
                    "synonyms": [
                        "blauwgroen",
                        "turkoois",
                        "cyaan"
                    ],
                    "common_phrases": [
                        "groenblauwe ogen",
                        "groenblauwe kleur",
                        "groenblauwe zee"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a colour that is halfway between green and blue."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "teal",
                                "target_lang_sense_clarification": "dark blue-green colour"
                            },
                            {
                                "target_lang_word": "blue-green",
                                "target_lang_sense_clarification": "colour mix"
                            },
                            {
                                "target_lang_word": "turquoise",
                                "target_lang_sense_clarification": "bright blue-green"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f4380ad-dade-3c82-a9b9-a4de44adf6a6",
                    "sense_definition": "Betrekking hebbend op de combinatie van natuur op het land (groen) en waterbeheer (blauw), vaak gebruikt in ruimtelijke ordening.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ecology_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De provincie wil nieuwe <w>groenblauwe</w> verbindingen tussen de natuurgebieden aanleggen.",
                            "target_lang_translations": {
                                "en": "The province wants to create new <w>green-blue</w> connections between the nature areas."
                            }
                        },
                        {
                            "text": "In de nieuwe woonwijk is veel aandacht voor <w>groenblauwe</w> structuren om wateroverlast te voorkomen.",
                            "target_lang_translations": {
                                "en": "In the new residential area, there is a lot of attention for <w>green-blue</w> structures to prevent flooding."
                            }
                        }
                    ],
                    "common_phrases": [
                        "groenblauwe diensten",
                        "groenblauwe dooradering",
                        "groenblauwe netwerken"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the combination of nature on land (green) and water management (blue), often used in spatial planning."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "green-blue",
                                "target_lang_sense_clarification": "combining vegetation and water management"
                            },
                            {
                                "target_lang_word": "blue-green",
                                "target_lang_sense_clarification": "infrastructure combining water and nature"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "df4ac7ea-43c1-3784-9a8f-b3a7f238ac48",
                    "sense_definition": "Een kleur die een mengeling is van groen en blauw.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "color_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kunt u die panelen voor de keuken ook leveren in <w>groenblauw</w>?",
                            "target_lang_translations": {
                                "en": "Can you also supply those panels for the kitchen in <w>teal</w>?"
                            }
                        },
                        {
                            "text": "Heeft u dit model trui ook in het <w>groenblauw</w>?",
                            "target_lang_translations": {
                                "en": "Do you also have this model of sweater in <w>teal</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "blauwgroen",
                        "turkoois"
                    ],
                    "common_phrases": [
                        "in het groenblauw",
                        "tint groenblauw"
                    ],
                    "target_lang_definitions": {
                        "en": "A colour that is a mixture of green and blue."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "teal",
                                "target_lang_sense_clarification": "dark blue-green colour"
                            },
                            {
                                "target_lang_word": "blue-green",
                                "target_lang_sense_clarification": "the colour"
                            }
                        ]
                    }
                }
            ]
        }
    ]

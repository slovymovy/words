{
    "word_family": [
        "tappen",
        "tapper",
        "tapkast",
        "tapkraan"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4cd02d0c-33b5-3146-b248-89811a465e1e",
                    "sense_definition": "Een kraan of afsluitmechanisme om vloeistof (zoals bier of water) uit een vat of leiding te laten stromen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik van jou een biertje van de <w>tap</w>?",
                            "target_lang_translations": {
                                "ru": "Можно мне бокал <w>разливного</w> пива?"
                            }
                        },
                        {
                            "text": "De barman stond de hele avond achter de <w>tap</w> glazen te vullen.",
                            "target_lang_translations": {
                                "ru": "Бармен весь вечер стоял у <w>крана</w>, наполняя бокалы."
                            }
                        }
                    ],
                    "synonyms": [
                        "kraan",
                        "aftapkraan",
                        "aftappunt"
                    ],
                    "common_phrases": [
                        "van de tap",
                        "achter de tap",
                        "aan de tap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кран или механизм для розлива жидкости (например, пива или воды) из бочки или трубы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кран",
                                "target_lang_sense_clarification": "устройство для подачи жидкости"
                            },
                            {
                                "target_lang_word": "разлив",
                                "target_lang_sense_clarification": "процесс наливания (пиво на разлив)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d510c272-b7f5-38b9-b4ea-337c4c890585",
                    "sense_definition": "Een aansluitpunt op een kabel of leiding, vaak specifiek bedoeld om communicatie af te luisteren.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie plaatste een <w>tap</w> op de telefoon van de verdachte.",
                            "target_lang_translations": {
                                "ru": "Полиция поставила <w>прослушку</w> на телефон подозреваемого."
                            }
                        },
                        {
                            "text": "Via een illegale <w>tap</w> werd er stroom afgetapt van het netwerk.",
                            "target_lang_translations": {
                                "ru": "Через незаконную <w>врезку</w> из сети воровали электричество."
                            }
                        }
                    ],
                    "synonyms": [
                        "afluisterpunt",
                        "aftakpunt"
                    ],
                    "common_phrases": [
                        "een tap plaatsen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точка подключения к кабелю или линии, часто используемая для прослушивания или нелегального отбора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прослушка",
                                "target_lang_sense_clarification": "скрытое наблюдение"
                            },
                            {
                                "target_lang_word": "отвод",
                                "target_lang_sense_clarification": "техническое ответвление"
                            },
                            {
                                "target_lang_word": "врезка",
                                "target_lang_sense_clarification": "нелегальное подключение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb96e6ed-d87f-3285-aa70-20f80ea183b3",
                    "sense_definition": "Een houten of metalen pen die dient om een verbinding tussen twee onderdelen vast te zetten.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De timmerman sloeg de houten <w>tap</w> stevig in de verbinding.",
                            "target_lang_translations": {
                                "ru": "Плотник плотно забил деревянный <w>шип</w> в соединение."
                            }
                        },
                        {
                            "text": "Zonder de borgende <w>tap</w> zou de constructie uit elkaar vallen.",
                            "target_lang_translations": {
                                "ru": "Без стопорного <w>штифта</w> конструкция развалилась бы."
                            }
                        }
                    ],
                    "synonyms": [
                        "borgpen",
                        "toognagel",
                        "wig"
                    ],
                    "antonyms": [
                        "gat",
                        "holte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревянный или металлический стержень, служащий для скрепления двух деталей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шип",
                                "target_lang_sense_clarification": "столярное соединение"
                            },
                            {
                                "target_lang_word": "штифт",
                                "target_lang_sense_clarification": "крепежный стержень"
                            },
                            {
                                "target_lang_word": "шкант",
                                "target_lang_sense_clarification": "деревянный крепеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42f544e7-2df6-3a5d-b998-04f9b6f388ab",
                    "sense_definition": "Een stuk gereedschap van hard staal met schroefdraad, gebruikt om inwendige schroefdraad in een gat te snijden.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gebruik de juiste maat <w>tap</w> om de schroefdraad te herstellen.",
                            "target_lang_translations": {
                                "ru": "Используй <w>метчик</w> правильного размера, чтобы восстановить резьбу."
                            }
                        },
                        {
                            "text": "Hij brak de <w>tap</w> per ongeluk af in het metaal.",
                            "target_lang_translations": {
                                "ru": "Он случайно сломал <w>метчик</w> в металле."
                            }
                        }
                    ],
                    "synonyms": [
                        "draadtap",
                        "schroefdraadtap"
                    ],
                    "antonyms": [
                        "snijijzer",
                        "snijmoer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструмент из твердой стали с резьбой, используемый для нарезания внутренней резьбы в отверстии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "метчик",
                                "target_lang_sense_clarification": "инструмент для резьбы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bae7f23-41d1-37cb-a09d-b3beb9af55eb",
                    "sense_definition": "Een uitstekend deel, stomp of uiteinde van een voorwerp.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de plek waar de tak was afgezaagd, is nog een kleine <w>tap</w> zichtbaar.",
                            "target_lang_translations": {
                                "ru": "На месте, где была спилена ветка, всё ещё виден маленький <w>пенёк</w>."
                            }
                        },
                        {
                            "text": "De <w>tap</w> van de as moet goed in het lager passen.",
                            "target_lang_translations": {
                                "ru": "<w>Цапфа</w> оси должна хорошо входить в подшипник."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступающая часть, обрубок или конец предмета (например, оси)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цапфа",
                                "target_lang_sense_clarification": "часть вала или оси"
                            },
                            {
                                "target_lang_word": "выступ",
                                "target_lang_sense_clarification": "выпирающая часть"
                            },
                            {
                                "target_lang_word": "пенёк",
                                "target_lang_sense_clarification": "остаток спиленного"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "338fb1bb-e184-326e-b604-5e3153ba73ae",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>tappen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>tap</w> elke vrijdagavond bier in de sportkantine.",
                            "target_lang_translations": {
                                "ru": "Я <w>наливаю</w> пиво в спортивной столовой каждую пятницу вечером."
                            }
                        },
                        {
                            "text": "Als ik een mop <w>tap</w>, moet iedereen altijd lachen.",
                            "target_lang_translations": {
                                "ru": "Когда я <w>отпускаю</w> шутку, все всегда смеются."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола tappen (наливать, отпускать шутки)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наливаю",
                                "target_lang_sense_clarification": "жидкость"
                            },
                            {
                                "target_lang_word": "отпускаю",
                                "target_lang_sense_clarification": "шутки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fea54732-6a22-3dc1-b595-274e3675510c",
                    "sense_definition": "Gebiedende wijs van <w>tappen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Tap</w> dat glas eens even helemaal vol.",
                            "target_lang_translations": {
                                "ru": "<w>Налей</w> этот стакан полным."
                            }
                        },
                        {
                            "text": "<w>Tap</w> het water uit de leidingen voordat het gaat vriezen.",
                            "target_lang_translations": {
                                "ru": "<w>Слей</w> воду из труб, пока не ударили морозы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола tappen."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "налей",
                                "target_lang_sense_clarification": "наполни"
                            },
                            {
                                "target_lang_word": "слей",
                                "target_lang_sense_clarification": "опустоши"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
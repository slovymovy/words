    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eb08071c-b815-3e42-9ecf-285dc2f43324",
                    "sense_definition": "A distinct part or branch of a nation's economy or society.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government has promised to increase wages for those working in the public <w>sector</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство пообещало повысить зарплаты тем, кто работает в государственном <w>секторе</w>."
                            }
                        },
                        {
                            "text": "Many jobs in the manufacturing <w>sector</w> have moved overseas in recent years.",
                            "target_lang_translations": {
                                "ru": "Многие рабочие места в производственном <w>секторе</w> за последние годы были перенесены за границу."
                            }
                        }
                    ],
                    "synonyms": [
                        "industry",
                        "field",
                        "branch",
                        "area"
                    ],
                    "common_phrases": [
                        "private sector",
                        "public sector",
                        "financial sector",
                        "service sector",
                        "agricultural sector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная часть или отрасль экономики страны или общества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сектор",
                                "target_lang_sense_clarification": "экономики или общества"
                            },
                            {
                                "target_lang_word": "отрасль",
                                "target_lang_sense_clarification": "сфера деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "badb52a0-8b44-3090-a751-d993255f179d",
                    "sense_definition": "A specific designated area or zone, often defined for military, administrative, or navigational purposes.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The commander ordered the troops to secure the northern <w>sector</w> of the city.",
                            "target_lang_translations": {
                                "ru": "Командир приказал войскам занять северный <w>сектор</w> города."
                            }
                        },
                        {
                            "text": "Patrols were increased in the border <w>sector</w> to prevent illegal crossings.",
                            "target_lang_translations": {
                                "ru": "Патрулирование в пограничном <w>секторе</w> было усилено для предотвращения незаконных пересечений."
                            }
                        }
                    ],
                    "synonyms": [
                        "zone",
                        "district",
                        "region",
                        "area"
                    ],
                    "common_phrases": [
                        "military sector",
                        "patrol sector",
                        "controlled sector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специально выделенная территория или зона, часто определяемая для военных, административных или навигационных целей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сектор",
                                "target_lang_sense_clarification": "территория, зона ответственности"
                            },
                            {
                                "target_lang_word": "зона",
                                "target_lang_sense_clarification": "область"
                            },
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "часть местности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db7756ea-a164-397e-a151-f21863a2d4a7",
                    "sense_definition": "A part of a circle enclosed by two radii and the arc between them.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The student was asked to calculate the area of the shaded <w>sector</w>.",
                            "target_lang_translations": {
                                "ru": "Студента попросили вычислить площадь заштрихованного <w>сектора</w>."
                            }
                        },
                        {
                            "text": "A slice of pizza is roughly the shape of a circular <w>sector</w>.",
                            "target_lang_translations": {
                                "ru": "Кусок пиццы имеет форму, примерно напоминающую круговой <w>сектор</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wedge"
                    ],
                    "common_phrases": [
                        "circular sector",
                        "area of a sector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть круга, ограниченная двумя радиусами и дугой между ними."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сектор",
                                "target_lang_sense_clarification": "геометрическая фигура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d75b85cc-d97a-35f4-9d86-f9a1ce6970e6",
                    "sense_definition": "A fixed-sized unit of data storage on a computer disk or track.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The technician discovered a bad <w>sector</w> on the hard drive that caused the crash.",
                            "target_lang_translations": {
                                "ru": "Техник обнаружил на жестком диске поврежденный <w>сектор</w>, который стал причиной сбоя."
                            }
                        },
                        {
                            "text": "Data is written to the disk <w>sector</w> by <w>sector</w> during the formatting process.",
                            "target_lang_translations": {
                                "ru": "В процессе форматирования данные записываются на диск <w>сектор</w> за <w>сектором</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "block"
                    ],
                    "common_phrases": [
                        "bad sector",
                        "boot sector",
                        "disk sector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица хранения данных фиксированного размера на компьютерном диске."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сектор",
                                "target_lang_sense_clarification": "информатика"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "41d3025d-f0fc-3f32-9d32-e528c10c7f91",
                    "sense_definition": "One of two things that are exactly the same or very similar to each other.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "matching_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This vase is the <w>twin</w> of the one standing in the hallway.",
                            "target_lang_translations": {
                                "ru": "Эта ваза — <w>близнец</w> той, что стоит в коридоре."
                            }
                        },
                        {
                            "text": "The city is often called the <w>twin</w> of its neighbor across the river.",
                            "target_lang_translations": {
                                "ru": "Этот город часто называют <w>близнецом</w> его соседа через реку."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из двух предметов, которые абсолютно одинаковы или очень похожи друг на друга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "близнец",
                                "target_lang_sense_clarification": "в переносном значении"
                            },
                            {
                                "target_lang_word": "двойник",
                                "target_lang_sense_clarification": "о предмете или человеке, очень похожем на другой"
                            }

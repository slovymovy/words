                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составление завещания",
                                "target_lang_sense_clarification": "юридический процесс"
                            },
                            {
                                "target_lang_word": "распоряжение имуществом",
                                "target_lang_sense_clarification": "распределение активов"
                            }
                        ]
                    }
                }
            ]
        },

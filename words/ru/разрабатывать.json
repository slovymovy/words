                            },
                            {
                                "target_lang_word": "work out",
                                "target_lang_sense_clarification": "strengthen a body part"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9d76d0e-1f16-33f1-be0b-afa638f9c2b2",

    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "870aa71d-39fc-3eb9-99b9-27c934d77203",
                    "sense_definition": "De manier waarop iets gebeurt of zich ontwikkelt in de tijd.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "process_development",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>verloop</w> van de vergadering was erg rommelig door de vele onderbrekingen.",
                            "target_lang_translations": {
                                "ru": "<w>Ход</w> собрания был очень сумбурным из-за множества перерывов."
                            }
                        },
                        {
                            "text": "Na <w>verloop</w> van tijd zul je merken dat het werk steeds makkelijker gaat.",
                            "target_lang_translations": {
                                "ru": "По <w>прошествии</w> времени ты заметишь, что работа идет все легче."
                            }
                        }
                    ],
                    "synonyms": [
                        "gang",
                        "ontwikkeling",
                        "proces",
                        "voortgang"
                    ],
                    "antonyms": [
                        "stilstand",
                        "stagnatie"
                    ],
                    "common_phrases": [
                        "na verloop van tijd",
                        "verloop van de dag",
                        "verloop van de ziekte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ, которым что-то происходит или развивается во времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ход",
                                "target_lang_sense_clarification": "событий, процесса"
                            },
                            {
                                "target_lang_word": "течение",
                                "target_lang_sense_clarification": "времени, болезни"
                            },
                            {
                                "target_lang_word": "прошествие",
                                "target_lang_sense_clarification": "в выражении 'na verloop van tijd'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fa15882-c1cf-3fcf-8e2c-e0708a6f453a",
                    "sense_definition": "Het vertrek van werknemers bij een bedrijf en de vervanging door nieuwe mensen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "turnover",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de hoge werkdruk is het <w>verloop</w> onder het personeel erg groot.",
                            "target_lang_translations": {
                                "ru": "Из-за высокого рабочего давления <w>текучесть</w> кадров очень высока."
                            }
                        },
                        {
                            "text": "Het bedrijf probeert het <w>verloop</w> te beperken door betere arbeidsvoorwaarden te bieden.",
                            "target_lang_translations": {
                                "ru": "Компания пытается ограничить <w>текучку</w>, предлагая лучшие условия труда."
                            }
                        }
                    ],
                    "synonyms": [
                        "personeelsverloop",
                        "uitstroom",
                        "personeelswisseling"
                    ],
                    "antonyms": [
                        "behoud"
                    ],
                    "common_phrases": [
                        "groot verloop",
                        "natuurlijk verloop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увольнение сотрудников из компании и их замена новыми людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "текучесть",
                                "target_lang_sense_clarification": "кадров, персонала"
                            },
                            {
                                "target_lang_word": "текучка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cff8037a-8cde-3fa5-9b0a-13d7b83503e5",
                    "sense_definition": "Een verbindingsstuk tussen twee onderdelen van verschillende afmetingen of vormen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adapter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je hebt een <w>verloop</w> nodig om de tuinslang op deze kraan aan te sluiten.",
                            "target_lang_translations": {
                                "ru": "Тебе нужен <w>переходник</w>, чтобы подсоединить садовый шланг к этому крану."
                            }
                        },
                        {
                            "text": "De loodgieter plaatste een <w>verloop</w> tussen de twee buizen omdat ze niet even dik waren.",
                            "target_lang_translations": {
                                "ru": "Сантехник установил <w>переходник</w> между двумя трубами, так как они были разной толщины."
                            }
                        }
                    ],
                    "synonyms": [
                        "koppelstuk",
                        "adapter",
                        "overgangsstuk"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединительная деталь между двумя частями разных размеров или форм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переходник",
                                "target_lang_sense_clarification": "техническое"
                            },
                            {
                                "target_lang_word": "адаптер",
                                "target_lang_sense_clarification": "универсальное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f6a63cc6-f644-3274-a597-32e9e9badb37",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>verlopen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>verloop</w> van de honger als we niet snel iets gaan eten.",
                            "target_lang_translations": {
                                "ru": "Я <w>умираю</w> с голоду, если мы скоро не поедим."
                            }
                        },
                        {
                            "text": "Als ik niet oppas, <w>verloop</w> ik weer in mijn oude slechte gewoontes.",
                            "target_lang_translations": {
                                "ru": "Если я не буду осторожен, я снова <w>скачусь</w> к своим старым вредным привычкам."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола <w>verlopen</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скатываюсь",
                                "target_lang_sense_clarification": "в негативное состояние"
                            },
                            {
                                "target_lang_word": "опускаюсь",
                                "target_lang_sense_clarification": "деградирую"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89484039-a1c6-309b-af8d-b81dee640b60",
                    "sense_definition": "Gebiedende wijs van <w>verlopen</w>.",
                    "learner_level": "B1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Verloop</w> niet in chaos, maar houd structuur in je werkzaamheden.",
                            "target_lang_translations": {
                                "ru": "Не <w>скатывайся</w> в хаос, а сохраняй структуру в своей работе."
                            }
                        },
                        {
                            "text": "<w>Verloop</w> niet tot een lager niveau door je emoties de overhand te laten nemen.",
                            "target_lang_translations": {
                                "ru": "Не <w>опускайся</w> на более низкий уровень, позволяя эмоциям взять верх."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола <w>verlopen</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скатывайся",
                                "target_lang_sense_clarification": "в хаос, негатив"
                            },
                            {
                                "target_lang_word": "опускайся",
                                "target_lang_sense_clarification": "на уровень ниже"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "pikowy",
        "pikować",
        "pika"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "74b4c019-217e-3309-86dd-4e4ddc3eb256",
                    "sense_definition": "Jeden z czterech kolorów w kartach do gry, oznaczony czarnym symbolem w kształcie liścia (♠); także karta należąca do tego koloru.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cards_suit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wistował w <w>pik</w>, licząc na to, że partner ma asa.",
                            "target_lang_translations": {
                                "ru": "Он зашел с <w>пик</w>, рассчитывая, что у партнера есть туз."
                            }
                        },
                        {
                            "text": "W moim rozdaniu miałem tylko jednego <w>pika</w>.",
                            "target_lang_translations": {
                                "ru": "В моей раздаче у меня была только одна <w>пика</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wino",
                        "reg. śl. grin"
                    ],
                    "common_phrases": [
                        "as pik",
                        "dama pik",
                        "król pik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из четырех карточных мастей, обозначаемая черным значком в форме листа (♠); также карта этой масти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пики",
                                "target_lang_sense_clarification": "масть"
                            },
                            {
                                "target_lang_word": "пика",
                                "target_lang_sense_clarification": "карта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cab5b462-24bb-374b-be99-5ee6791c3eaf",
                    "sense_definition": "Gwałtowny i krótkotrwały wzrost wartości sygnału, funkcji lub innej wielkości, widoczny na wykresie jako ostry wierzchołek.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "signal_peak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wykresie sprzedaży widać wyraźny <w>pik</w> w okresie przedświątecznym.",
                            "target_lang_translations": {
                                "ru": "На графике продаж виден отчетливый <w>пик</w> в предпраздничный период."
                            }
                        },
                        {
                            "text": "Aparatura pomiarowa zarejestrowała niepokojący <w>pik</w> promieniowania.",
                            "target_lang_translations": {
                                "ru": "Измерительная аппаратура зарегистрировала тревожный <w>пик</w> радиации."
                            }
                        }
                    ],
                    "synonyms": [
                        "wino",
                        "reg. śl. grin"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "w kontekście naukowym lub technicznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Резкое и кратковременное увеличение значения сигнала, функции или другой величины, видимое на графике как острый выступ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пик",
                                "target_lang_sense_clarification": "максимум, вершина графика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "852ab3d6-4a6e-3d58-a9fd-40423f04f54e",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>pika</w> (dawna broń drzewcowa).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "form_pika",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W zbrojowni zamkowej brakowało starych <w>pik</w> używanych przez piechotę.",
                            "target_lang_translations": {
                                "ru": "В замковом арсенале не хватало старых <w>пик</w>, использовавшихся пехотой."
                            }
                        },
                        {
                            "text": "Las długich <w>pik</w> uniemożliwił kawalerii szarżę na czworobok.",
                            "target_lang_translations": {
                                "ru": "Лес длинных <w>пик</w> сделал невозможной атаку кавалерии на каре."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного «пика» (старинное древковое оружие)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пик",
                                "target_lang_sense_clarification": "форма слова «пика»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78392337-b941-329d-8a1b-c95902acbc03",
                    "sense_definition": "Górny, tylny róg żagla czworokątnego (gaflowego).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_peak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fał <w>piku</w> zaciął się i nie mogliśmy poprawnie ustawić żagla.",
                            "target_lang_translations": {
                                "ru": "Фал <w>пика</w> заело, и мы не могли правильно поставить парус."
                            }
                        },
                        {
                            "text": "Żeglarz uważnie obserwował <w>pik</w>, sprawdzając napięcie płótna.",
                            "target_lang_translations": {
                                "ru": "Моряк внимательно наблюдал за <w>пиком</w>, проверяя натяжение полотна."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "żeglarstwo"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхний задний угол четырехугольного (гафельного) паруса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пик",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "238e1ffc-c81e-3ee7-9d2f-80461f78faa7",
                    "sense_definition": "Słowo oznaczające szczyt, występujące w nazwach własnych niektórych gór, głównie w Azji (z języka rosyjskiego).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mountain_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Szczytem marzeń każdego alpinisty w tamtych czasach było zdobycie <w>Piku</w> Komunizmu.",
                            "target_lang_translations": {
                                "ru": "Пределом мечтаний каждого альпиниста в те времена было покорение <w>Пика</w> Коммунизма."
                            }
                        },
                        {
                            "text": "Wyprawa ruszyła w stronę <w>Piku</w> Lenina wczesnym rankiem.",
                            "target_lang_translations": {
                                "ru": "Экспедиция выдвинулась в сторону <w>Пика</w> Ленина ранним утром."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "rosyjski"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, означающее вершину, встречающееся в собственных названиях некоторых гор, преимущественно в Азии (заимствовано из русского)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пик",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2b83d659-9ef2-3550-b417-9170ef1b1261",
                    "sense_definition": "Określający kolor pik w kartach; używany po rzeczowniku, nieodmienny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cards_suit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dołożyłem damę <w>pik</w> do stosu na stole.",
                            "target_lang_translations": {
                                "ru": "Я положил даму <w>пик</w> в стопку на столе."
                            }
                        },
                        {
                            "text": "To był walet <w>pik</w>, karta, która zmieniła losy rozgrywki.",
                            "target_lang_translations": {
                                "ru": "Это был валет <w>пик</w> — карта, которая изменила ход игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "pikowy",
                        "winny"
                    ],
                    "common_phrases": [
                        "dama pik",
                        "as pik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает масть пики в картах; используется после существительного, не изменяется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пик",
                                "target_lang_sense_clarification": "масть (несклоняемое)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "fc213681-2873-3ae8-83d3-6b7d0855f785",
                    "sense_definition": "Nazwisko męskie lub żeńskie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Pik</w> został wezwany do gabinetu dyrektora.",
                            "target_lang_translations": {
                                "ru": "Господина <w>Пика</w> вызвали в кабинет директора."
                            }
                        },
                        {
                            "text": "Nie znam nikogo o nazwisku <w>Pik</w> w tej okolicy.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого по фамилии <w>Пик</w> в этой округе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужская или женская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пик",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "8c25f6de-718a-36e5-8265-e4c40e66ae23",
                    "sense_definition": "Dźwięk naśladujący krótki, wysoki sygnał elektroniczny lub bicie serca.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Urządzenie robiło <w>pik</w>, <w>pik</w>, co bardzo mnie denerwowało.",
                            "target_lang_translations": {
                                "ru": "Устройство делало <w>пик</w>-<w>пик</w>, что меня очень раздражало."
                            }
                        },
                        {
                            "text": "Serce pacjenta na monitorze robiło miarowe <w>pik</w>.",
                            "target_lang_translations": {
                                "ru": "Сердце пациента на мониторе издавало мерное <w>пик</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, имитирующий короткий высокий электронный сигнал или биение сердца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пик",
                                "target_lang_sense_clarification": "звукоподражание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "3493a8c9-2083-33ba-9142-c97108e61594",
                    "sense_definition": "Niszczyć coś twardego lub sztywnego, dzieląc to na części pod wpływem siły",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_break",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny wiatr <łamał> stare gałęzie w parku.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер <ломал> старые ветки в парке."
                            }
                        },
                        {
                            "text": "Dziecko przez przypadek <łamało> wszystkie kredki.",
                            "target_lang_translations": {
                                "ru": "Ребенок случайно <ломал> все мелки."
                            }
                        }
                    ],
                    "synonyms": [
                        "niszczyć",
                        "rozłamywać"
                    ],
                    "antonyms": [
                        "sklejać",
                        "łączyć",
                        "naprawiać"
                    ],
                    "common_phrases": [
                        "łamać chleb",
                        "łamać gałęzie",
                        "łamać na kawałki"
                    ],
                    "target_lang_definitions": {

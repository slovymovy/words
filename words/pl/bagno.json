    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a8c9f9e6-ee29-3ecb-98d1-1c22b8893504",
                    "sense_definition": "Obszar trwale podmokły, grząski, nasycony wodą, często porośnięty specyficzną roślinnością.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "nature_terrain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musieliśmy obejść szerokim łukiem niebezpieczne <w>bagno</w>, aby nie ugrzęznąć.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось обойти опасное <w>болото</w> стороной, чтобы не увязнуть."
                            }
                        },
                        {
                            "text": "To rozległe <w>bagno</w> jest domem dla wielu gatunków ptaków wodnych.",
                            "target_lang_translations": {
                                "ru": "Это обширное <w>болото</w> служит домом для многих видов водоплавающих птиц."
                            }
                        }
                    ],
                    "synonyms": [
                        "mokradło",
                        "trzęsawisko",
                        "grzęzawisko",
                        "moczary",
                        "torfowisko",
                        "błota"
                    ],
                    "common_phrases": [
                        "wciągnąć w bagno",
                        "osuszać bagno",
                        "grząskie bagno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок местности с избыточным увлажнением, топкая, зыбкая земля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болото",
                                "target_lang_sense_clarification": "топкое место"
                            },
                            {
                                "target_lang_word": "трясина",
                                "target_lang_sense_clarification": "зыбкое, засасывающее место"
                            },
                            {
                                "target_lang_word": "топь",
                                "target_lang_sense_clarification": "труднопроходимое место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de98a8b3-864e-3230-9154-3e82c2fe4098",
                    "sense_definition": "Trudna, skomplikowana sytuacja życiowa bez wyjścia lub środowisko zdemoralizowane i nieetyczne.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek wpadł w finansowe <w>bagno</w>, z którego trudno mu się będzie wydostać.",
                            "target_lang_translations": {
                                "ru": "Марек попал в финансовое <w>болото</w>, из которого ему будет трудно выбраться."
                            }
                        },
                        {
                            "text": "Ta afera korupcyjna to prawdziwe moralne <w>bagno</w>.",
                            "target_lang_translations": {
                                "ru": "Эта коррупционная афера — настоящее моральное <w>болото</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "szambo",
                        "rynsztok",
                        "kloaka"
                    ],
                    "common_phrases": [
                        "moralne bagno",
                        "wpaść w bagno",
                        "wyciągnąć z bagna"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to describe a messy situation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудная, безвыходная ситуация или аморальная, застойная среда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болото",
                                "target_lang_sense_clarification": "перен. застой, рутина"
                            },
                            {
                                "target_lang_word": "трясина",
                                "target_lang_sense_clarification": "перен. безвыходное положение"
                            },
                            {
                                "target_lang_word": "грязь",
                                "target_lang_sense_clarification": "перен. аморальность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78474f69-b9aa-3f3d-b605-3383a36ed909",
                    "sense_definition": "Zimozielony krzew z rodziny wrzosowatych o silnym, charakterystycznym zapachu, rosnący na torfowiskach (bagno zwyczajne).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lesie unosił się intensywny zapach, który wydzielało kwitnące <w>bagno</w>.",
                            "target_lang_translations": {
                                "ru": "В лесу стоял сильный запах, который источал цветущий <w>багульник</w>."
                            }
                        },
                        {
                            "text": "<w>Bagno</w> zwyczajne było dawniej stosowane jako środek przeciwko molom.",
                            "target_lang_translations": {
                                "ru": "<w>Багульник</w> болотный раньше использовался как средство против моли."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziki rozmaryn",
                        "rozmaryn leśny"
                    ],
                    "common_phrases": [
                        "bagno zwyczajne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вечнозелёный кустарник семейства вересковых с сильным одурманивающим запахом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "багульник",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a3d86081-216f-355b-98f5-0f55cc59d98d",
                    "sense_definition": "Nazwa własna odnosząca się do wielu wsi, osad i części miejscowości w Polsce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "toponym",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mój dziadek pochodzi z małej miejscowości o nazwie <w>Bagno</w>.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка родом из маленькой местности под названием <w>Багно</w>."
                            }
                        },
                        {
                            "text": "Na mapie województwa dolnośląskiego znaleźliśmy wieś <w>Bagno</w>.",
                            "target_lang_translations": {
                                "ru": "На карте Нижнесилезского воеводства мы нашли деревню <w>Багно</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Собственное название многих деревень и поселений в Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Багно",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }

                    "sense_id": "1e38db8c-513d-3be7-87e5-1080a46010fb",
                    "sense_definition": "В этот день; то же, что сегодня.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_day",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Жаль, что <w>нынче</w> такая плохая погода.",
                            "target_lang_translations": {
                                "en": "It's a pity that the weather is so bad <w>today</w>."
                            }
                        },
                        {
                            "text": "Мы <w>нынче</w> собираемся пойти в гости к друзьям.",
                            "target_lang_translations": {
                                "en": "We are planning to visit friends <w>today</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "сегодня"
                    ],
                    "antonyms": [
                        "вчера",
                        "завтра"
                    ],
                    "common_phrases": [
                        "нынче утром",
                        "нынче вечером"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "On this day; same as today."
                    },
                    "translations": {
                        "en": [
                            {

            "senses": [
                {
                    "sense_id": "7e6ec043-52b9-3bc1-acbb-1f7d03b6f743",
                    "sense_definition": "Żeńskie imię pochodzenia germańskiego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja najlepsza przyjaciółka <w>Ida</w> mieszka w bloku obok mnie.",
                            "target_lang_translations": {
                                "ru": "Моя лучшая подруга <w>Ида</w> живет в многоквартирном доме рядом со мной."
                            }
                        },
                        {
                            "text": "Mam na imię <w>Ida</w> i pochodzę z małego miasteczka na południu.",
                            "target_lang_translations": {
                                "ru": "Меня зовут <w>Ида</w>, и я родом из маленького городка на юге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ciocia Ida",
                        "siostra Ida",
                        "święta Ida"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя германского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ида",
                                "target_lang_sense_clarification": "женское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "543bbf67-9d57-3883-a039-1aaba6e305bd",
                    "sense_definition": "Nimfa z mitologii greckiej, która opiekowała się małym Zeusem na Krecie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Według starożytnych mitów nimfa <w>Ida</w> ukrywała Zeusa w jaskini.",
                            "target_lang_translations": {
                                "ru": "Согласно древним мифам, нимфа <w>Ида</w> прятала Зевса в пещере."
                            }
                        },
                        {
                            "text": "Na cześć tej nimfy nazwano górę, na której <w>Ida</w> wychowywała boga.",
                            "target_lang_translations": {
                                "ru": "В честь этой нимфы была названа гора, на которой <w>Ида</w> воспитывала бога."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nimfa Ida",
                        "góra Ida"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нимфа из греческой мифологии, которая заботилась о маленьком Зевсе на Крите."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ида",
                                "target_lang_sense_clarification": "нимфа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "278a4fc2-dd45-3e7a-8d1a-569782fe85be",
                    "sense_definition": "Planetoida średniej wielkości z głównego pasa asteroid, oznaczona numerem 243.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Sonda kosmiczna Galileo wykonała szczegółowe zdjęcia, na których widoczna jest <w>Ida</w>.",
                            "target_lang_translations": {
                                "ru": "Космический зонд «Галилео» сделал детальные снимки, на которых видна <w>Ида</w>."
                            }
                        },
                        {
                            "text": "Astronomowie odkryli, że planetoida <w>Ida</w> posiada własny mały księżyc.",
                            "target_lang_translations": {
                                "ru": "Астрономы обнаружили, что астероид <w>Ида</w> имеет собственный маленький спутник."
                            }
                        }
                    ],
                    "common_phrases": [
                        "planetoida Ida",
                        "pas asteroid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Астероид среднего размера из главного пояса астероидов под номером 243."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ида",
                                "target_lang_sense_clarification": "астероид"
                            }
                        ]
                    }
                }
            ]
        }
    ]

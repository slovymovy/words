    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "449db7ba-9393-3191-80df-610541a28d76",
                    "sense_definition": "Dorosły mężczyzna, który nie ma żony i nigdy nie był żonaty.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "marital_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek to zatwardziały <w>kawaler</w>, który bardzo ceni sobie niezależność.",
                            "target_lang_translations": {
                                "ru": "Мой дядя — убежденный <w>холостяк</w>, который очень ценит свою независимость."
                            }
                        },
                        {
                            "text": "W małym miasteczku każdy stary <w>kawaler</w> wzbudzał zainteresowanie sąsiadek.",
                            "target_lang_translations": {
                                "ru": "В маленьком городке каждый старый <w>холостяк</w> вызывал интерес соседок."
                            }
                        }
                    ],
                    "synonyms": [
                        "singiel",
                        "wolny"
                    ],
                    "antonyms": [
                        "żonaty"
                    ],
                    "common_phrases": [
                        "stary kawaler",
                        "wieczny kawaler",
                        "zatwardziały kawaler",
                        "życie kawalera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрослый мужчина, который не имеет жены и никогда не был женат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "холостяк",
                                "target_lang_sense_clarification": "неженатый мужчина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4a96594-e227-3f7a-957c-99650dfd8ac0",
                    "sense_definition": "Młody, dorastający mężczyzna; określenie często używane z sympatią przez osoby starsze.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "youth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ależ z twojego syna wyrósł przystojny <w>kawaler</w>!",
                            "target_lang_translations": {
                                "ru": "Какой же из твоего сына вырос статный <w>молодой человек</w>!"
                            }
                        },
                        {
                            "text": "Ten młody <w>kawaler</w> pomógł mi wnieść ciężkie zakupy na trzecie piętro.",
                            "target_lang_translations": {
                                "ru": "Этот <w>молодой человек</w> помог мне занести тяжелые покупки на третий этаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "młodzieniec",
                        "młodzian"
                    ],
                    "common_phrases": [
                        "przystojny kawaler",
                        "duży kawaler"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used by older generation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодой, взрослеющий мужчина; определение, часто используемое с симпатией пожилыми людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодой человек",
                                "target_lang_sense_clarification": "юноша"
                            },
                            {
                                "target_lang_word": "парень",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37deeb7e-aa7a-3e79-8d25-2798ef2c5096",
                    "sense_definition": "Osoba, która została odznaczona wysokim orderem państwowym lub wojskowym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "honor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas uroczystości prezydent wręczył medale nowym <w>kawalerom</w> Orderu Odrodzenia Polski.",
                            "target_lang_translations": {
                                "ru": "Во время церемонии президент вручил медали новым <w>кавалерам</w> Ордена Возрождения Польши."
                            }
                        },
                        {
                            "text": "Mój dziadek był <w>kawalerem</w> Orderu Virtuti Militari za zasługi na polu walki.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был <w>кавалером</w> Ордена Virtuti Militari за заслуги на поле боя."
                            }
                        }
                    ],
                    "synonyms": [
                        "kawaler orderu"
                    ],
                    "common_phrases": [
                        "Kawaler Orderu Uśmiechu",
                        "Kawaler Orderu Orła Białego"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "official title"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, награждённое высоким государственным или военным орденом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кавалер",
                                "target_lang_sense_clarification": "награждённый орденом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d80e2af9-369f-38b7-a0e6-ae0bf762b265",
                    "sense_definition": "Członek zakonu rycerskiego lub szlachcic w dawnej Polsce; tytuł historyczny.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Średniowieczni <w>kawalerowie</w> maltańscy budowali szpitale i opiekowali się pielgrzymami.",
                            "target_lang_translations": {
                                "ru": "Средневековые мальтийские <w>кавалеры</w> строили больницы и заботились о паломниках."
                            }
                        },
                        {
                            "text": "Dzielny <w>kawaler</w> ruszył do boju z szablą w dłoni, by bronić honoru ojczyzny.",
                            "target_lang_translations": {
                                "ru": "Отважный <w>рыцарь</w> бросился в бой с саблей в руке, чтобы защитить честь родины."
                            }
                        }
                    ],
                    "synonyms": [
                        "rycerz",
                        "szlachcic"
                    ],
                    "common_phrases": [
                        "kawalerowie mieczowi",
                        "kawalerowie maltańscy"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член рыцарского ордена или дворянин в старой Польше; исторический титул."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыцарь",
                                "target_lang_sense_clarification": "воин, дворянин"
                            },
                            {
                                "target_lang_word": "кавалер",
                                "target_lang_sense_clarification": "член ордена"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5d06e786-f81c-3408-8e5f-6d2c82387126",
                    "sense_definition": "Lekker; met een goede en aangename smaak.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij genoten samen van een <w>smakelijke</w> maaltijd in het restaurant.",
                            "target_lang_translations": {
                                "ru": "Они вместе наслаждались <w>вкусной</w> едой в ресторане.",
                                "en": "They enjoyed a <w>tasty</w> meal together in the restaurant."
                            }
                        },
                        {
                            "text": "Mijn moeder maakt altijd zeer <w>smakelijke</w> soep in de winter.",
                            "target_lang_translations": {
                                "ru": "Моя мама всегда готовит очень <w>вкусный</w> суп зимой.",
                                "en": "My mother always makes very <w>tasty</w> soup in the winter."
                            }
                        }
                    ],
                    "antonyms": [
                        "vies",
                        "onsmakelijk"
                    ],
                    "common_phrases": [
                        "een smakelijke maaltijd",
                        "ziet er smakelijk uit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вкусный; имеющий хороший, приятный вкус.",
                        "en": "Tasty; having a good and pleasant flavor."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкусный",
                                "target_lang_sense_clarification": "о еде"
                            },
                            {
                                "target_lang_word": "аппетитный",
                                "target_lang_sense_clarification": "вызывающий аппетит"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "tasty",
                                "target_lang_sense_clarification": "having a pleasant flavor"
                            },
                            {
                                "target_lang_word": "delicious",
                                "target_lang_sense_clarification": "highly pleasant to the taste"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "8a92c60f-30dc-3bf8-a06f-b14053aaf6b5",
                    "sense_definition": "Een wens die men uitspreekt voordat men begint met eten; eet smakelijk.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen het eten op tafel stond, zei vader: \"<w>Smakelijk</w>!\"",
                            "target_lang_translations": {
                                "ru": "Когда еда была на столе, папа сказал: \"<w>Приятного аппетита</w>!\"",
                                "en": "When the food was on the table, father said: \"<w>Bon appétit</w>!\""
                            }
                        },
                        {
                            "text": "Ik wens jullie allemaal <w>smakelijk</w> eten en een fijne avond.",
                            "target_lang_translations": {
                                "ru": "Я желаю вам всем <w>приятного аппетита</w> и хорошего вечера.",
                                "en": "I wish you all a <w>tasty</w> meal and a nice evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "eet smakelijk",
                        "smakelijk eten"
                    ],
                    "common_phrases": [
                        "smakelijk eten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пожелание, которое произносят перед едой.",
                        "en": "A wish expressed before eating; enjoy your meal."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приятного аппетита",
                                "target_lang_sense_clarification": "пожелание перед едой"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "bon appétit",
                                "target_lang_sense_clarification": "common expression before eating"
                            },
                            {
                                "target_lang_word": "enjoy your meal",
                                "target_lang_sense_clarification": "standard phrase"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "404d8ef8-e776-341c-8ea2-43bf93ee9dc2",
                    "sense_definition": "Op een manier die laat zien dat men het lekker vindt of er plezier in heeft.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen zaten <w>smakelijk</w> te eten van de verse pannenkoeken.",
                            "target_lang_translations": {
                                "ru": "Дети <w>с аппетитом</w> ели свежие блины.",
                                "en": "The children were eating the fresh pancakes <w>with gusto</w>."
                            }
                        },
                        {
                            "text": "Het publiek moest <w>smakelijk</w> lachen om de grappen van de cabaretier.",
                            "target_lang_translations": {
                                "ru": "Публика <w>от души</w> смеялась над шутками комика.",
                                "en": "The audience laughed <w>heartily</w> at the comedian's jokes."
                            }
                        }
                    ],
                    "common_phrases": [
                        "smakelijk lachen",
                        "smakelijk vertellen"
                    ],
                    "target_lang_definitions": {
                        "ru": "С удовольствием, с аппетитом (о еде или смехе).",
                        "en": "In a way that shows one likes it or takes pleasure in it (often regarding eating or laughing)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с аппетитом",
                                "target_lang_sense_clarification": "о еде"
                            },
                            {
                                "target_lang_word": "от души",
                                "target_lang_sense_clarification": "о смехе"
                            },
                            {
                                "target_lang_word": "смачно",
                                "target_lang_sense_clarification": "разговорное; с большим удовольствием"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "heartily",
                                "target_lang_sense_clarification": "often used with laughing"
                            },
                            {
                                "target_lang_word": "with gusto",
                                "target_lang_sense_clarification": "with evident enjoyment"
                            }
                        ]
                    }
                }
            ]
        }

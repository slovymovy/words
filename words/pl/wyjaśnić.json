                            {
                                "target_lang_word": "проясниться",
                                "target_lang_sense_clarification": "стать ясным (о ситуации)"
                            },
                            {
                                "target_lang_word": "разрешиться",
                                "target_lang_sense_clarification": "о проблеме или недоразумении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cad9810-cab1-3b44-9c23-5bf258175820",
                    "sense_definition": "Podać powody lub motywy jakiegoś działania; usprawiedliwić swoje lub czyjeś zachowanie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tłumaczenie",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cf813ca0-535e-3e8e-b26b-9cdd3a76ec9e",
                    "sense_definition": "Класть что-либо в недоступное или незаметное для других место, чтобы это нельзя было найти или увидеть.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дети любят <прятать> свои игрушки под кровать.",
                            "target_lang_translations": {
                                "en": "Children love to <hide> their toys under the bed."
                            }
                        },
                        {
                            "text": "Не нужно <прятать> от меня важные документы.",
                            "target_lang_translations": {
                                "en": "You don't need to <hide> important documents from me."
                            }
                        }
                    ],
                    "synonyms": [
                        "скрывать"
                    ],
                    "antonyms": [
                        "находить",
                        "доставать",
                        "обнаруживать"
                    ],
                    "common_phrases": [
                        "прятать деньги",
                        "прятать ключи",
                        "прятать далеко"
                    ],
                    "target_lang_definitions": {
                        "en": "To put something in a place where it cannot be seen or found by others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hide",
                                "target_lang_sense_clarification": "put in a secret place"
                            },
                            {
                                "target_lang_word": "conceal",
                                "target_lang_sense_clarification": "prevent from being seen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19444ed4-0365-391e-b11b-860630a7628f",
                    "sense_definition": "Закрывать что-либо от взгляда, делать невидимым; утаивать чувства или мысли.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она пыталась <прятать> грустные глаза за тёмными очками.",
                            "target_lang_translations": {
                                "en": "She tried to <hide> her sad eyes behind dark glasses."
                            }
                        },
                        {
                            "text": "Он умеет мастерски <прятать> свои истинные намерения.",
                            "target_lang_translations": {
                                "en": "He knows how to masterfully <hide> his true intentions."
                            }
                        }
                    ],
                    "synonyms": [
                        "скрывать"
                    ],
                    "antonyms": [
                        "раскрывать",
                        "обнаруживать"
                    ],
                    "common_phrases": [
                        "прятать глаза",
                        "прятать улыбку",
                        "прятать слёзы",
                        "прятать руки"
                    ],
                    "target_lang_definitions": {
                        "en": "To keep something from being seen or known; to suppress feelings or thoughts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hide",
                                "target_lang_sense_clarification": "keep secret"
                            },
                            {
                                "target_lang_word": "conceal",
                                "target_lang_sense_clarification": "keep from being known"
                            },
                            {
                                "target_lang_word": "mask",
                                "target_lang_sense_clarification": "disguise an emotion"
                            }
                        ]
                    }
                }
            ]
        }
    ]

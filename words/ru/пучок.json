            "senses": [
                {
                    "sense_id": "d8e57e40-910b-3817-a659-a87d3f9b4f15",
                    "sense_definition": "Небольшая связка однородных предметов, собранных или скреплённых вместе",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "bundle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама попросила купить на рынке свежий <w>пучок</w> петрушки и укропа.",
                            "target_lang_translations": {
                                "en": "Mom asked to buy a fresh <w>bunch</w> of parsley and dill at the market."
                            }
                        },
                        {
                            "text": "Девушка собрала свои длинные волосы в тугой <w>пучок</w> на затылке.",
                            "target_lang_translations": {
                                "en": "The young woman gathered her long hair into a tight <w>bun</w> at the back of her head."
                            }
                        }
                    ],
                    "synonyms": [
                        "связка"
                    ],
                    "common_phrases": [
                        "пучок зелени",
                        "пучок волос",
                        "пучок редиски",
                        "пучок травы"
                    ],
                    "target_lang_definitions": {
                        "en": "A small collection of similar items gathered or fastened together"
                    },
                    "translations": {

                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat voorstel <w>lust</w> mij niet, dus ik ga er niet mee akkoord.",
                            "target_lang_translations": {
                                "en": "That proposal does not <w>appeal</w> to me, so I do not agree to it.",

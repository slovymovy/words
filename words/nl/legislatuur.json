{
    "word_family": [
        "legislatief",
        "legisleren"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f067f313-8270-31da-85a5-3654ab967d39",
                    "sense_definition": "De periode waarin een gekozen parlement of regering aan de macht is en zijn taken uitvoert.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als het plan nu niet slaagt, moet het zeker deze <w>legislatuur</w> nog lukken.",
                            "target_lang_translations": {
                                "ru": "Если план не сработает сейчас, это непременно должно получиться в этот <w>срок полномочий</w>."
                            }
                        },
                        {
                            "text": "De nieuwe wet verlengt de <w>legislatuur</w> van vier naar vijf jaar.",
                            "target_lang_translations": {
                                "ru": "Новый закон продлевает <w>срок полномочий</w> с четырех до пяти лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "zittingsperiode",
                        "regeerperiode",
                        "ambtstermijn",
                        "kabinetsperiode",
                        "ambtsperiode"
                    ],
                    "common_phrases": [
                        "deze legislatuur",
                        "lopende legislatuur",
                        "vorige legislatuur",
                        "einde van de legislatuur"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Период, в течение которого избранный парламент или правительство находятся у власти и выполняют свои обязанности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срок полномочий",
                                "target_lang_sense_clarification": "период действия мандата"
                            },
                            {
                                "target_lang_word": "легислатура",
                                "target_lang_sense_clarification": "срок деятельности парламента (формально)"
                            },
                            {
                                "target_lang_word": "созыв",
                                "target_lang_sense_clarification": "порядковый номер состава парламента"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e24ad01-92f5-39fd-be23-c9fdebecd6b7",
                    "sense_definition": "Het overheidsorgaan dat de bevoegdheid heeft om wetten te maken voor een land of staat.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>legislatuur</w> heeft gisteren ingestemd met het nieuwe wetsvoorstel.",
                            "target_lang_translations": {
                                "ru": "<w>Законодательный орган</w> вчера одобрил новый законопроект."
                            }
                        },
                        {
                            "text": "In een democratie wordt de <w>legislatuur</w> gekozen door het volk.",
                            "target_lang_translations": {
                                "ru": "В демократии <w>законодательный орган</w> избирается народом."
                            }
                        }
                    ],
                    "synonyms": [
                        "zittingsperiode"
                    ],
                    "common_phrases": [
                        "nationale legislatuur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государственный орган, обладающий полномочиями издавать законы для страны или штата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "законодательный орган",
                                "target_lang_sense_clarification": "орган власти"
                            },
                            {
                                "target_lang_word": "законодательное собрание",
                                "target_lang_sense_clarification": "собрание представителей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f045f554-9c68-3cfd-a02d-2d8f5033aa5d",
                    "sense_definition": "Het proces van het maken van wetten en het uitoefenen van wetgevende macht.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij bestudeert de geschiedenis van de <w>legislatuur</w> in de negentiende eeuw.",
                            "target_lang_translations": {
                                "ru": "Он изучает историю <w>законотворчества</w> в девятнадцатом веке."
                            }
                        },
                        {
                            "text": "De procedures voor de <w>legislatuur</w> zijn vastgelegd in de grondwet.",
                            "target_lang_translations": {
                                "ru": "Процедуры <w>законодательной деятельности</w> закреплены в конституции."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс создания законов и осуществления законодательной власти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "законотворчество",
                                "target_lang_sense_clarification": "процесс создания законов"
                            },
                            {
                                "target_lang_word": "законодательная деятельность",
                                "target_lang_sense_clarification": "деятельность по изданию законов"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
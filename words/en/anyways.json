            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "eaac663f-a0b3-3557-9a81-26b5390a0ddd",
                    "sense_definition": "Used to resume a subject after an interruption, to change the topic, or to indicate that the previous statement was not important; anyway.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I know it is raining heavily, but I plan to go for a walk <w>anyways</w>.",
                            "target_lang_translations": {
                                "ru": "Я знаю, что идет сильный дождь, но я <w>всё равно</w> планирую пойти на прогулку."
                            }
                        },
                        {
                            "text": "Well <w>anyways</w>, we should probably get back to work before the boss arrives.",
                            "target_lang_translations": {
                                "ru": "Ну, <w>в любом случае</w>, нам, наверное, стоит вернуться к работе до прихода начальника."
                            }
                        }
                    ],
                    "synonyms": [
                        "anyway",
                        "anyhow",
                        "regardless",
                        "in any case"
                    ],
                    "common_phrases": [
                        "but anyways",
                        "well anyways"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often considered non-standard or informal compared to 'anyway'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для возвращения к теме после отступления, смены темы или указания на то, что предыдущее утверждение неважно; в любом случае."
                    },
                    "translations": {

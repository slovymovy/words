            "senses": [
                {
                    "sense_id": "38a501ee-426f-3dbd-9b77-d188c90e181d",
                    "sense_definition": "Najniższa, wewnętrzna część naczynia, pojemnika lub innego przedmiotu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na <dnie> pudełka znalazłem stary list od przyjaciela.",
                            "target_lang_translations": {
                                "ru": "На <дне> коробки я нашел старое письмо от друга."
                            }
                        },
                        {
                            "text": "Wypiliśmy szampana do <dna> za zdrowie młodej pary.",
                            "target_lang_translations": {
                                "ru": "Мы выпили шампанское до <дна> за здоровье молодой пары."
                            }
                        }
                    ],
                    "synonyms": [
                        "spód"
                    ],
                    "antonyms": [
                        "wierzch",
                        "góra"
                    ],
                    "common_phrases": [
                        "do dna",
                        "dno butelki",
                        "podwójne dno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самая нижняя, внутренняя часть сосуда, контейнера или другого предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дно",
                                "target_lang_sense_clarification": "сосуда, коробки"

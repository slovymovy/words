    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "205634f2-f9f9-3648-994e-d59f8db4431b",
                    "sense_definition": "Przygotowany do działania, użytku lub drogi; będący w stanie, w którym można coś zacząć.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "readiness_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obiad jest już <w>gotowy</w>, więc możemy siadać do stołu.",
                            "target_lang_translations": {
                                "ru": "Обед уже <w>готов</w>, так что можем садиться за стол."
                            }
                        },
                        {
                            "text": "Czy jesteś już <w>gotowy</w> do wyjścia do szkoły?",
                            "target_lang_translations": {
                                "ru": "Ты уже <w>готов</w> идти в школу?"
                            }
                        }
                    ],
                    "synonyms": [
                        "przygotowany",
                        "naszykowany",
                        "przyszykowany"
                    ],
                    "antonyms": [
                        "niegotowy",
                        "nieprzygotowany"
                    ],
                    "common_phrases": [
                        "gotowy do drogi",
                        "gotowy do wyjścia",
                        "wszystko gotowe",
                        "bądź gotowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подготовленный к действию, использованию или в путь; находящийся в состоянии, когда можно что-то начать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовый",
                                "target_lang_sense_clarification": "подготовленный"
                            },
                            {
                                "target_lang_word": "подготовленный",
                                "target_lang_sense_clarification": "приведенный в готовность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc4064e9-908f-345c-94a1-f87fbc5376de",
                    "sense_definition": "Całkowicie wykonany, skończony, niewymagający dalszej pracy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "completion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój projekt jest <w>gotowy</w> i mogę go wysłać szefowi.",
                            "target_lang_translations": {
                                "ru": "Мой проект <w>готов</w>, и я могу отправить его начальнику."
                            }
                        },
                        {
                            "text": "Dom jest prawie <w>gotowy</w>, brakuje tylko mebli w salonie.",
                            "target_lang_translations": {
                                "ru": "Дом почти <w>готов</w>, не хватает только мебели в гостиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "skończony",
                        "wykonany",
                        "ukończony",
                        "zrobiony"
                    ],
                    "antonyms": [
                        "nieskończony",
                        "nieukończony"
                    ],
                    "common_phrases": [
                        "gotowy produkt",
                        "gotowy projekt",
                        "praca gotowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью сделанный, завершенный, не требующий дальнейшей работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовый",
                                "target_lang_sense_clarification": "сделанный"
                            },
                            {
                                "target_lang_word": "законченный",
                                "target_lang_sense_clarification": "завершенный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9a9bbc9-5a73-3243-a87d-84882a740455",
                    "sense_definition": "Wyrażający chęć i zgodę na zrobienie czegoś; skłonny do pomocy lub poświęceń.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "readiness_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "On jest zawsze <w>gotowy</w> pomóc swoim przyjaciołom w potrzebie.",
                            "target_lang_translations": {
                                "ru": "Он всегда <w>готов</w> помочь своим друзьям в беде."
                            }
                        },
                        {
                            "text": "Jestem <w>gotowy</w> zrobić wszystko, żeby naprawić ten błąd.",
                            "target_lang_translations": {
                                "ru": "Я <w>готов</w> сделать всё, чтобы исправить эту ошибку."
                            }
                        }
                    ],
                    "synonyms": [
                        "chętny",
                        "skłonny",
                        "zdecydowany"
                    ],
                    "antonyms": [
                        "niechętny",
                        "niezdecydowany"
                    ],
                    "common_phrases": [
                        "gotowy do pomocy",
                        "gotowy na wszystko",
                        "gotowy do poświęceń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражающий желание и согласие сделать что-то; склонный к помощи или жертвам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовый",
                                "target_lang_sense_clarification": "согласный на что-то"
                            },
                            {
                                "target_lang_word": "согласный",
                                "target_lang_sense_clarification": "выражающий согласие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9897157-5d46-394b-8ad3-22c5a311aeb6",
                    "sense_definition": "O produktach (zwłaszcza ubraniach lub jedzeniu): wyprodukowany seryjnie, dostępny w sprzedaży do natychmiastowego użycia, a nie robiony na zamówienie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "completion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym sklepie kupisz <w>gotowe</w> dania, które wystarczy tylko podgrzać.",
                            "target_lang_translations": {
                                "ru": "В этом магазине можно купить <w>готовые</w> блюда, которые нужно только разогреть."
                            }
                        },
                        {
                            "text": "Wolę kupić <w>gotowy</w> garnitur niż szyć go u krawca.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю купить <w>готовый</w> костюм, чем шить его у портного."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gotowe ubrania",
                        "gotowe dania",
                        "gotowa żywność"
                    ],
                    "target_lang_definitions": {
                        "ru": "О продуктах (одежде или еде): произведенный серийно, доступный для немедленного использования, а не сделанный на заказ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовый",
                                "target_lang_sense_clarification": "фабричный, не на заказ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d91cdf7-e9a9-3c87-91a4-e7b1530e7f7a",
                    "sense_definition": "Oznaczający sytuację, w której coś (zwykle złego) może się wydarzyć w każdej chwili; bliski zrobienia czegoś nieoczekiwanego.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "probability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary płot jest <w>gotowy</w> przewrócić się przy silniejszym wietrze.",
                            "target_lang_translations": {
                                "ru": "Этот старый забор <w>готов</w> рухнуть при сильном ветре."
                            }
                        },
                        {
                            "text": "Nie denerwuj go, bo on jest <w>gotowy</w> rzucić pracę z dnia na dzień.",
                            "target_lang_translations": {
                                "ru": "Не зли его, потому что он <w>готов</w> бросить работу в любой момент."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdolny"
                    ],
                    "common_phrases": [
                        "nieszczęście gotowe",
                        "gotowy spaść",
                        "awantura gotowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Означает ситуацию, когда что-то (обычно плохое) может произойти в любой момент; близкий к совершению чего-то неожиданного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовый",
                                "target_lang_sense_clarification": "способный (сделать что-то)"
                            },
                            {
                                "target_lang_word": "способный",
                                "target_lang_sense_clarification": "могущий совершить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b46128a-247c-383a-a3d7-fa95de1b1214",
                    "sense_definition": "Potocznie o kimś, kto jest bardzo pijany lub całkowicie wyczerpany.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wypił za dużo wódki na weselu i po godzinie był już <w>gotowy</w>.",
                            "target_lang_translations": {
                                "ru": "Он выпил слишком много водки на свадьбе и через час был уже <w>готов</w>."
                            }
                        },
                        {
                            "text": "Po przebiegnięciu maratonu byłem tak zmęczony, że czułem się kompletnie <w>gotowy</w>.",
                            "target_lang_translations": {
                                "ru": "После марафона я так устал, что чувствовал себя совершенно <w>готовым</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pijany",
                        "wstawiony",
                        "zalany",
                        "nawalony"
                    ],
                    "antonyms": [
                        "nieukończony"
                    ],
                    "common_phrases": [
                        "całkiem gotowy",
                        "już gotowy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное: о ком-то, кто очень пьян или полностью истощен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовый",
                                "target_lang_sense_clarification": "пьяный или без сил (разг.)"
                            },
                            {
                                "target_lang_word": "пьяный",
                                "target_lang_sense_clarification": "в состоянии опьянения"
                            },
                            {
                                "target_lang_word": "никакой",
                                "target_lang_sense_clarification": "сильно уставший или пьяный (разг.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

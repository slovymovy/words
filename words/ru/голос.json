    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fd1c8b9a-7144-31bb-b449-063e578bcc99",
                    "sense_definition": "Звуки, которые человек издаёт ртом при разговоре, крике, смехе или плаче.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sound_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я услышал знакомый <w>голос</w> в соседней комнате.",
                            "target_lang_translations": {
                                "en": "I heard a familiar <w>voice</w> in the next room."
                            }
                        },
                        {
                            "text": "Учитель попросил ученика не повышать <w>голос</w> во время урока.",
                            "target_lang_translations": {
                                "en": "The teacher asked the student not to raise his <w>voice</w> during the lesson."
                            }
                        },
                        {
                            "text": "Её тихий <w>голос</w> успокоил плачущего ребёнка.",
                            "target_lang_translations": {
                                "en": "Her quiet <w>voice</w> calmed the crying child."
                            }
                        }
                    ],
                    "synonyms": [
                        "глас"
                    ],
                    "common_phrases": [
                        "громкий голос",
                        "тихий голос",
                        "повысить голос",
                        "потерять голос",
                        "знакомый голос"
                    ],
                    "target_lang_definitions": {
                        "en": "Sounds produced by a person's mouth when speaking, shouting, laughing, or crying."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "voice",
                                "target_lang_sense_clarification": "sound produced by a person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b50efc6-7d8e-350e-9b03-832f0d1d1816",
                    "sense_definition": "Способность петь чисто, красиво и сильно; качество звучания при пении.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этой певицы удивительно сильный и красивый <w>голос</w>.",
                            "target_lang_translations": {
                                "en": "This singer has a surprisingly strong and beautiful <w>voice</w>."
                            }
                        },
                        {
                            "text": "Он очень любит петь в караоке, хотя у него совсем нет <w>голоса</w>.",
                            "target_lang_translations": {
                                "en": "He loves singing karaoke, even though he has no <w>voice</w> at all."
                            }
                        }
                    ],
                    "common_phrases": [
                        "красивый голос",
                        "нет голоса",
                        "поставить голос",
                        "сорвать голос"
                    ],
                    "target_lang_definitions": {
                        "en": "The ability to sing clearly, beautifully, and strongly; the quality of sound when singing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "voice",
                                "target_lang_sense_clarification": "singing ability"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bebd378c-9f36-3c42-a06d-ae824d27f841",
                    "sense_definition": "Мнение или решение участника выборов или собрания; право участвовать в таком решении.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "political",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На выборах новый президент получил большинство <w>голосов</w>.",
                            "target_lang_translations": {
                                "en": "In the election, the new president received the majority of <w>votes</w>."
                            }
                        },
                        {
                            "text": "Каждый гражданин страны старше восемнадцати лет имеет право <w>голоса</w>.",
                            "target_lang_translations": {
                                "en": "Every citizen of the country over eighteen has the right to <w>vote</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "право голоса",
                        "отдать свой голос",
                        "подсчёт голосов",
                        "большинство голосов"
                    ],
                    "target_lang_definitions": {
                        "en": "An opinion or decision of a participant in an election or meeting; the right to participate in such a decision."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vote",
                                "target_lang_sense_clarification": "in an election"
                            },
                            {
                                "target_lang_word": "voice",
                                "target_lang_sense_clarification": "right to express an opinion"
                            }
                        ]

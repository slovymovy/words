    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d3638041-5a91-3511-aaef-e396dffc665f",
                    "sense_definition": "Dotyczący sakramentu Eucharystii, liturgii lub hostii.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci w białych strojach podeszły powoli do stopnia <w>komunijnego</w>.",
                            "target_lang_translations": {
                                "ru": "Дети в белых нарядах медленно подошли к ступени <w>для причастия</w>."
                            }
                        },
                        {
                            "text": "Kapłan wyjaśnił wiernym symbolikę chleba <w>komunijnego</w> podczas kazania.",
                            "target_lang_translations": {
                                "ru": "Священник объяснил верующим символику хлеба <w>для причастия</w> во время проповеди."
                            }
                        }
                    ],
                    "synonyms": [
                        "eucharystyczny"
                    ],
                    "common_phrases": [
                        "chleb komunijny",
                        "stół komunijny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к таинству Евхаристии, литургии или гостии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для причастия",
                                "target_lang_sense_clarification": "предназначенный для таинства"
                            },
                            {
                                "target_lang_word": "евхаристический",
                                "target_lang_sense_clarification": "относящийся к евхаристии"
                            },
                            {
                                "target_lang_word": "причастный",
                                "target_lang_sense_clarification": "церковный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2b5f5a9-d376-3957-95dd-4ab0f436bbb6",
                    "sense_definition": "Związany z uroczystością Pierwszej Komunii Świętej, przyjęciem rodzinnym lub prezentami z tej okazji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W maju sklepy oferują szeroki wybór sukienek i garniturów <w>komunijnych</w>.",
                            "target_lang_translations": {
                                "ru": "В мае магазины предлагают широкий выбор платьев и костюмов <w>для Первого Причастия</w>."
                            }
                        },
                        {
                            "text": "Rodzice zorganizowali wystawne przyjęcie <w>komunijne</w> dla całej rodziny.",
                            "target_lang_translations": {
                                "ru": "Родители организовали пышный прием <w>по случаю Первого Причастия</w> для всей семьи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prezent komunijny",
                        "przyjęcie komunijne",
                        "sukienka komunijna",
                        "gość komunijny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с торжеством Первого Причастия, семейным праздником или подарками по этому случаю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для Первого Причастия",
                                "target_lang_sense_clarification": "об одежде или атрибутах"
                            },
                            {
                                "target_lang_word": "по случаю Первого Причастия",
                                "target_lang_sense_clarification": "о событии или празднике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bb26069-8ac5-3bc7-8ee9-6f6711ef0f2c",
                    "sense_definition": "Dotyczący głębokiej więzi, wspólnoty duchowej lub zjednoczenia między ludźmi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To spotkanie miało charakter <w>komunijny</w>, budujący poczucie jedności w grupie.",
                            "target_lang_translations": {
                                "ru": "Эта встреча носила <w>общинный</w> характер, укрепляющий чувство единства в группе."
                            }
                        },
                        {
                            "text": "Autor opisuje <w>komunijny</w> związek człowieka z otaczającą go przyrodą.",
                            "target_lang_translations": {
                                "ru": "Автор описывает <w>единящую</w> связь человека с окружающей природой."
                            }
                        }
                    ],
                    "synonyms": [
                        "zjednoczeniowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к глубокой связи, духовной общности или единению между людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общинный",
                                "target_lang_sense_clarification": "связанный с общностью"
                            },
                            {
                                "target_lang_word": "единящий",
                                "target_lang_sense_clarification": "создающий единство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

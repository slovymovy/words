    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "80531335-5baf-3621-bac9-91b546f25f94",
                    "sense_definition": "Смесь газов (преимущественно азота и кислорода), образующая земную атмосферу, которой дышат люди и животные",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы вышли на улицу, чтобы подышать свежим <w>воздухом</w>.",
                            "target_lang_translations": {
                                "en": "We went outside to breathe some fresh <w>air</w>."
                            }
                        },
                        {
                            "text": "В горах <w>воздух</w> очень чистый и прозрачный.",
                            "target_lang_translations": {
                                "en": "In the mountains, the <w>air</w> is very clean and clear."
                            }
                        }
                    ],
                    "antonyms": [
                        "вакуум"
                    ],
                    "common_phrases": [
                        "свежий воздух",
                        "дышать воздухом",
                        "на открытом воздухе",
                        "глоток воздуха"
                    ],
                    "target_lang_definitions": {
                        "en": "A mixture of gases (mainly nitrogen and oxygen) that forms the Earth's atmosphere, which humans and animals breathe"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "air",
                                "target_lang_sense_clarification": "mixture of gases"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3d1fc4c-6f3d-3dad-a2b4-2519df836b32",
                    "sense_definition": "Свободное пространство над землёй, простирающееся вверх",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Самолёт плавно поднялся в <w>воздух</w> и взял курс на юг.",
                            "target_lang_translations": {
                                "en": "The plane rose smoothly into the <w>air</w> and headed south."
                            }
                        },
                        {
                            "text": "Мяч подлетел высоко в <w>воздух</w> и упал на крышу.",
                            "target_lang_translations": {
                                "en": "The ball flew high into the <w>air</w> and landed on the roof."
                            }
                        }
                    ],
                    "common_phrases": [
                        "подняться в воздух",
                        "взлететь в воздух",
                        "висеть в воздухе"
                    ],
                    "target_lang_definitions": {
                        "en": "The open space above the ground extending upwards"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "air",
                                "target_lang_sense_clarification": "open space"
                            },
                            {
                                "target_lang_word": "sky",
                                "target_lang_sense_clarification": "space above earth"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1bdcdd7-73c3-3fff-a9dd-9950bb473779",
                    "sense_definition": "Нематериальная обстановка, настроение или дух, царящие в каком-либо месте или обществе",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_atmosphere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Воздух</w> в комнате был наполнен радостным ожиданием праздника.",
                            "target_lang_translations": {
                                "en": "The <w>air</w> in the room was filled with joyful anticipation of the holiday."
                            }
                        },
                        {
                            "text": "После революции в стране чувствовался <w>воздух</w> свободы и перемен.",
                            "target_lang_translations": {
                                "en": "After the revolution, the <w>air</w> of freedom and change was felt in the country."
                            }
                        }
                    ],
                    "synonyms": [
                        "атмосфера",
                        "атмосфера",
                        "атмосфера"
                    ],
                    "common_phrases": [
                        "воздух свободы",
                        "воздух перемен"
                    ],
                    "target_lang_definitions": {
                        "en": "The intangible atmosphere, mood, or spirit prevailing in a particular place or society"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "atmosphere",
                                "target_lang_sense_clarification": "mood or spirit"
                            },
                            {
                                "target_lang_word": "air",
                                "target_lang_sense_clarification": "distinctive quality"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d67a5719-66cb-37dd-8926-d4a364ae4545",
                    "sense_definition": "Военная команда, предупреждающая о появлении вражеской авиации",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир громко крикнул «<w>Воздух</w>!», и солдаты бросились в укрытие.",
                            "target_lang_translations": {
                                "en": "The commander shouted \"<w>Air</w>!\", and the soldiers rushed for cover."
                            }
                        },
                        {
                            "text": "При сигнале «<w>Воздух</w>» необходимо немедленно погасить свет.",
                            "target_lang_translations": {
                                "en": "At the signal \"<w>Air raid</w>\", lights must be extinguished immediately."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A military command warning of the appearance of enemy aircraft"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Air raid!",
                                "target_lang_sense_clarification": "warning signal"
                            },
                            {
                                "target_lang_word": "Air!",
                                "target_lang_sense_clarification": "command"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "929828dd-b325-3c28-9b9d-2f6cfd59f2c7",
                    "sense_definition": "Деньги (в жаргонном употреблении)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_money",
                    "name_type": "no",
                    "examples": [

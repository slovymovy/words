                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "товар",
                                "target_lang_sense_clarification": "предмет торговли"
                            },
                            {
                                "target_lang_word": "изделие",
                                "target_lang_sense_clarification": "продукт производства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd56d436-9a9e-35e1-997c-c4d1d7739dd6",
                    "sense_definition": "Een genummerd onderdeel of paragraaf in een wet, contract of reglement.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De advocaat verwees naar het eerste <w>artikel</w> van de wet.",
                            "target_lang_translations": {
                                "ru": "Адвокат сослался на первую <w>статью</w> закона."
                            }
                        },
                        {
                            "text": "In dit <w>artikel</w> staan de regels voor het contract beschreven.",
                            "target_lang_translations": {

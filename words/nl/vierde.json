                {
                    "sense_id": "a424ec4c-6897-33c5-98d8-9163196e7810",
                    "sense_definition": "Nummer vier in een reeks; volgend op de derde.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ordinal_numbers",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij woont op de <w>vierde</w> verdieping van dit gebouw.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "6a70d8d2-8cd9-3f91-baa9-2b01cfb78d18",
                    "sense_definition": "Vragend voornaamwoord dat gebruikt wordt om naar de identiteit van een of meerdere personen te vragen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "interrogative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wie</w> heeft mijn fiets geleend?",
                            "target_lang_translations": {
                                "en": "<w>Who</w> borrowed my bike?",
                                "ru": "<w>Кто</w> одолжил мой велосипед?"
                            }
                        },
                        {
                            "text": "Ik weet niet <w>Wie</w> er aan de deur staat.",
                            "target_lang_translations": {
                                "en": "I don't know <w>who</w> is at the door.",
                                "ru": "Я не знаю, <w>кто</w> стоит у двери."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wie dan ook",
                        "wie weet",
                        "wie is wie"
                    ],
                    "target_lang_definitions": {
                        "en": "Interrogative pronoun used to ask for the identity of one or more persons.",
                        "ru": "Вопросительное местоимение, которое используется, чтобы спросить о личности одного или нескольких человек."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "who",
                                "target_lang_sense_clarification": "interrogative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кто",
                                "target_lang_sense_clarification": "вопросительное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07c99661-8cb9-3a5c-a8e5-4c98e57f9a2f",
                    "sense_definition": "Betrekkelijk voornaamwoord met ingesloten antecedent dat verwijst naar een onbepaald persoon; degene die.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relative_indefinite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wie</w> niet waagt, die niet wint.",
                            "target_lang_translations": {
                                "en": "<w>He who</w> does not risk, does not win.",
                                "ru": "<w>Кто</w> не рискует, тот не выигрывает."
                            }
                        },
                        {
                            "text": "<w>Wie</w> het weet, mag het zeggen.",
                            "target_lang_translations": {
                                "en": "<w>Whoever</w> knows may say so.",
                                "ru": "<w>Кто</w> знает, может сказать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wie zoekt die vindt",
                        "wie zwijgt stemt toe"
                    ],
                    "target_lang_definitions": {
                        "en": "Relative pronoun with implied antecedent referring to an indefinite person; the one who.",
                        "ru": "Относительное местоимение с подразумеваемым антецедентом, указывающее на неопределенное лицо; тот, кто."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whoever",
                                "target_lang_sense_clarification": "indefinite relative"
                            },
                            {
                                "target_lang_word": "he who",
                                "target_lang_sense_clarification": "formal/literary"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кто",
                                "target_lang_sense_clarification": "относительное местоимение (тот, кто)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d4d9f95-c608-327e-a2f8-53d62c230871",
                    "sense_definition": "Betrekkelijk voornaamwoord dat terugverwijst naar een eerder genoemd persoon, voornamelijk gebruikt na een voorzetsel.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relative_prepositional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De collega met <w>wie</w> ik samenwerk is erg vriendelijk.",
                            "target_lang_translations": {
                                "en": "The colleague with <w>whom</w> I work is very friendly.",
                                "ru": "Коллега, с <w>которым</w> я работаю, очень дружелюбен."
                            }
                        },
                        {
                            "text": "Dit is de vriend aan <w>wie</w> ik het boek heb gegeven.",
                            "target_lang_translations": {
                                "en": "This is the friend to <w>whom</w> I gave the book.",
                                "ru": "Это друг, <w>которому</w> я дал книгу."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Relative pronoun referring back to a previously mentioned person, mainly used after a preposition.",
                        "ru": "Относительное местоимение, которое отсылает к ранее упомянутому лицу, в основном используется после предлога."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whom",
                                "target_lang_sense_clarification": "object of preposition"
                            },
                            {
                                "target_lang_word": "who",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "который",
                                "target_lang_sense_clarification": "союзное слово (с которым, которому)"
                            },
                            {
                                "target_lang_word": "кто",
                                "target_lang_sense_clarification": "в косвенных падежах (с кем, кому)"
                            }
                        ]
                    }
                }
            ]
        },

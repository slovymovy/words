    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e527c715-82fe-3190-8235-0ad13796cbea",
                    "sense_definition": "Trzymać kogoś lub coś, zapobiegając upadkowi, lub stanowić fizyczną podporę dla jakiejś konstrukcji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stare drewniane belki z trudem <w>podtrzymywały</w> ciężki dach stodoły.",
                            "target_lang_translations": {
                                "ru": "Старые деревянные балки с трудом <w>поддерживали</w> тяжелую крышу сарая."
                            }
                        },
                        {
                            "text": "Pielęgniarka delikatnie <w>podtrzymywała</w> osłabionego pacjenta, pomagając mu wstać.",
                            "target_lang_translations": {
                                "ru": "Медсестра осторожно <w>поддерживала</w> ослабленного пациента, помогая ему встать."
                            }
                        }
                    ],
                    "synonyms": [
                        "podpierać",
                        "trzymać"
                    ],
                    "common_phrases": [
                        "podtrzymywać ścianę",
                        "podtrzymywać głowę",
                        "podtrzymywać rannego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Держать кого-то или что-то, не давая упасть, или служить физической опорой для какой-либо конструкции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддерживать",
                                "target_lang_sense_clarification": "физически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d145a43-5ed1-33ed-9d0c-a74d1f7e559f",
                    "sense_definition": "Dbać o to, aby coś trwało nadal, nie przestało istnieć lub nie straciło na sile.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "maintenance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo zmęczenia gospodarz starał się <w>podtrzymywać</w> rozmowę z gośćmi.",
                            "target_lang_translations": {
                                "ru": "Несмотря на усталость, хозяин старался <w>поддерживать</w> разговор с гостями."
                            }
                        },
                        {
                            "text": "Harcerze przez całą noc <w>podtrzymywali</w> ogień w obozowym ognisku.",
                            "target_lang_translations": {
                                "ru": "Скауты всю ночь <w>поддерживали</w> огонь в лагерном костре."
                            }
                        }
                    ],
                    "synonyms": [
                        "utrzymywać"
                    ],
                    "common_phrases": [
                        "podtrzymywać rozmowę",
                        "podtrzymywać ogień",
                        "podtrzymywać kontakt",
                        "podtrzymywać tradycję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заботиться о том, чтобы что-то продолжалось, не прекращало существовать или не теряло силу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддерживать",
                                "target_lang_sense_clarification": "сохранять состояние"
                            },
                            {
                                "target_lang_word": "сохранять",
                                "target_lang_sense_clarification": "не давать исчезнуть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f42d94f-b30b-34e4-aee6-dd5aa46d9e77",
                    "sense_definition": "Dodawać komuś siły psychicznej, nadziei lub otuchy w trudnej sytuacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierni przyjaciele zawsze <w>podtrzymywali</w> go na duchu w chwilach zwątpienia.",
                            "target_lang_translations": {
                                "ru": "Верные друзья всегда <w>поддерживали</w> его дух в минуты сомнений."
                            }
                        },
                        {
                            "text": "Tylko nadzieja na szybki powrót do domu <w>podtrzymywała</w> ją przy życiu.",
                            "target_lang_translations": {
                                "ru": "Только надежда на скорое возвращение домой <w>поддерживала</w> в ней жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "wspierać",
                        "dodawać otuchy"
                    ],
                    "common_phrases": [
                        "podtrzymywać na duchu",
                        "podtrzymywać nadzieję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать кому-то душевные силы, надежду или ободрять в трудной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддерживать",
                                "target_lang_sense_clarification": "морально"
                            },
                            {
                                "target_lang_word": "подбадривать",
                                "target_lang_sense_clarification": "вселять бодрость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "009545c1-3de7-375a-8fd0-c5def16673ba",
                    "sense_definition": "Wyrażać zgodę na coś, uznawać coś za nadal obowiązujące lub nie zmieniać swojego zdania w jakiejś kwestii.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "formal_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd apelacyjny w całości <w>podtrzymał</w> wyrok wydany przez niższą instancję.",
                            "target_lang_translations": {
                                "ru": "Апелляционный суд полностью <w>оставил в силе</w> приговор, вынесенный нижестоящей инстанцией."
                            }
                        },
                        {
                            "text": "Prezes firmy <w>podtrzymuje</w> swoje stanowisko w sprawie planowanych zmian.",
                            "target_lang_translations": {
                                "ru": "Президент компании <w>подтверждает</w> свою позицию по поводу планируемых изменений."
                            }
                        }
                    ],
                    "synonyms": [
                        "popierać"
                    ],
                    "common_phrases": [
                        "podtrzymywać decyzję",
                        "podtrzymywać wyrok",
                        "podtrzymywać wniosek",
                        "podtrzymywać oskarżenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать согласие с чем-то, признавать что-то по-прежнему действующим или не менять своего мнения по какому-либо вопросу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставлять в силе",
                                "target_lang_sense_clarification": "решение или приговор"
                            },
                            {
                                "target_lang_word": "подтверждать",
                                "target_lang_sense_clarification": "мнение или позицию"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "synonyms": [
                        "motionless",
                        "stationary",
                        "unmoving"
                    ],
                    "antonyms": [
                        "moving",
                        "active"
                    ],
                    "common_phrases": [
                        "stay still",
                        "hold still",
                        "keep still",
                        "stock still"
                    ],
                    "target_lang_definitions": {

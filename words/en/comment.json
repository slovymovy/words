                    "target_lang_definitions": {
                        "ru": "Добавлять пояснительные заметки к исходному коду программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комментировать",
                                "target_lang_sense_clarification": "код программы"

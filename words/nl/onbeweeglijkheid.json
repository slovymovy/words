            "senses": [
                {
                    "sense_id": "ac0e33eb-f86e-3db3-bb21-32d5ff1e5137",
                    "sense_definition": "De toestand van volkomen stilstand of het niet kunnen bewegen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts maakte zich zorgen over de <w>onbeweeglijkheid</w> van het gewricht na de val.",
                            "target_lang_translations": {
                                "ru": "Врач беспокоился о <w>неподвижности</w> сустава после падения.",
                                "en": "The doctor was worried about the <w>immobility</w> of the joint after the fall."
                            }
                        },
                        {
                            "text": "Er heerste een totale <w>onbeweeglijkheid</w> in het bos, alsof de tijd stilstond.",
                            "target_lang_translations": {
                                "ru": "В лесу царила полная <w>неподвижность</w>, словно время остановилось.",
                                "en": "There was a total <w>motionlessness</w> in the forest, as if time stood still."
                            }
                        }
                    ],
                    "antonyms": [
                        "beweeglijkheid"
                    ],
                    "common_phrases": [
                        "totale onbeweeglijkheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние полной неподвижности или неспособности двигаться.",
                        "en": "The state of complete stillness or the inability to move."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неподвижность",
                                "target_lang_sense_clarification": "общее состояние покоя"
                            },
                            {
                                "target_lang_word": "обездвиженность",
                                "target_lang_sense_clarification": "потеря способности двигаться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "immobility",
                                "target_lang_sense_clarification": "inability to move, often medical"
                            },
                            {
                                "target_lang_word": "motionlessness",
                                "target_lang_sense_clarification": "state of being completely still"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9672ca66-4ba8-323a-a9af-60af66c9f6d2",
                    "sense_definition": "Het niet willen of kunnen veranderen van mening, houding of werkwijze.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mental_attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De onderhandelingen liepen vast door de <w>onbeweeglijkheid</w> van de directie.",
                            "target_lang_translations": {
                                "ru": "Переговоры зашли в тупик из-за <w>негибкости</w> руководства.",
                                "en": "The negotiations stalled due to the <w>inflexibility</w> of the management."
                            }
                        },
                        {
                            "text": "Zijn politieke <w>onbeweeglijkheid</w> zorgde voor veel frustratie bij de kiezers.",
                            "target_lang_translations": {
                                "ru": "Его политическая <w>инертность</w> вызывала сильное разочарование у избирателей.",
                                "en": "His political <w>inertia</w> caused a lot of frustration among the voters."
                            }
                        }
                    ],
                    "synonyms": [
                        "starheid",
                        "rigiditeit",
                        "onwrikbaarheid",
                        "inflexibiliteit",
                        "immobilisme"
                    ],
                    "common_phrases": [
                        "politieke onbeweeglijkheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нежелание или неспособность менять мнение, позицию или методы работы.",
                        "en": "The unwillingness or inability to change an opinion, attitude, or method."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "негибкость",

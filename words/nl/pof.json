    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "abcdf1eb-c2bd-35bb-b18a-f17f25f71c7c",
                    "sense_definition": "Krediet of uitstel van betaling, vrijwel uitsluitend gebruikt in een vaste uitdrukking.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "credit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omdat hij zijn portemonnee was vergeten, mocht hij de boodschappen <w>op de pof</w> meenemen.",
                            "target_lang_translations": {
                                "en": "Because he had forgotten his wallet, he was allowed to take the groceries <w>on credit</w>.",
                                "ru": "Так как он забыл кошелек, ему разрешили взять продукты <w>в долг</w>."
                            }
                        },
                        {
                            "text": "Vroeger was het in dorpswinkels heel normaal om <w>op de pof</w> te kopen.",
                            "target_lang_translations": {
                                "en": "In the past, it was quite normal in village shops to buy <w>on credit</w>.",
                                "ru": "Раньше в деревенских магазинах было совершенно нормально покупать <w>в долг</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op de pof"
                    ],
                    "target_lang_definitions": {
                        "en": "Credit or deferred payment, almost exclusively used in a fixed expression.",
                        "ru": "Кредит или отсрочка платежа, используется почти исключительно в устойчивом выражении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "credit",
                                "target_lang_sense_clarification": "deferred payment"
                            },
                            {
                                "target_lang_word": "tick",
                                "target_lang_sense_clarification": "informal British term for credit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "долг",
                                "target_lang_sense_clarification": "в выражении 'op de pof' (в долг)"
                            },
                            {
                                "target_lang_word": "кредит",
                                "target_lang_sense_clarification": "отсрочка оплаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10008e84-b7f6-3fd2-989e-d93c7db4d768",
                    "sense_definition": "Een dof, zwaar geluid of een stoot, vaak veroorzaakt door iets zachts dat valt of botst.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met een zachte <w>pof</w> landde de kat veilig op het kussen.",
                            "target_lang_translations": {
                                "en": "With a soft <w>thud</w>, the cat landed safely on the cushion.",
                                "ru": "С мягким <w>шлепком</w> кошка благополучно приземлилась на подушку."
                            }
                        },
                        {
                            "text": "We hoorden een doffe <w>pof</w> toen de zak aardappelen op de grond viel.",
                            "target_lang_translations": {
                                "en": "We heard a dull <w>thump</w> when the sack of potatoes fell on the floor.",
                                "ru": "Мы услышали глухой <w>стук</w>, когда мешок с картошкой упал на пол."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A dull, heavy sound or impact, often caused by something soft falling or colliding.",
                        "ru": "Глухой, тяжелый звук или удар, часто от падения чего-то мягкого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thud",
                                "target_lang_sense_clarification": "dull sound of impact"
                            },
                            {
                                "target_lang_word": "thump",
                                "target_lang_sense_clarification": "heavy dull sound"
                            },
                            {
                                "target_lang_word": "puff",
                                "target_lang_sense_clarification": "soft dull sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шлепок",
                                "target_lang_sense_clarification": "глухой звук падения"
                            },
                            {
                                "target_lang_word": "глухой стук",
                                "target_lang_sense_clarification": "звук удара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97e5776f-8636-3c91-b85b-74cd69351db1",
                    "sense_definition": "Poeder dat klimmers en turners gebruiken om hun handen stroever te maken voor meer grip.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De klimmer doopte zijn bezwete handen in de zak met <w>pof</w>.",
                            "target_lang_translations": {
                                "en": "The climber dipped his sweaty hands into the bag of <w>chalk</w>.",
                                "ru": "Скалолаз опустил потные руки в мешок с <w>магнезией</w>."
                            }
                        },
                        {
                            "text": "Zonder voldoende <w>pof</w> glijd je snel van de grepen af in de klimhal.",
                            "target_lang_translations": {
                                "en": "Without sufficient <w>chalk</w>, you quickly slip off the holds in the climbing gym.",
                                "ru": "Без достаточного количества <w>магнезии</w> ты быстро соскользнешь с зацепов на скалодроме."
                            }
                        }
                    ],
                    "synonyms": [
                        "magnesium",
                        "kalk"
                    ],
                    "target_lang_definitions": {
                        "en": "Powder used by climbers and gymnasts to make their hands rougher for better grip.",
                        "ru": "Порошок, который используют скалолазы и гимнасты для улучшения сцепления рук."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chalk",
                                "target_lang_sense_clarification": "magnesium carbonate for grip"
                            },
                            {
                                "target_lang_word": "magnesium powder",
                                "target_lang_sense_clarification": "specific material name"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "магнезия",
                                "target_lang_sense_clarification": "спортивный порошок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e40a3120-20bf-3af4-975d-51fa8f7e72b2",
                    "sense_definition": "Een bolstaande plooi of verwijding in een kledingstuk, bijvoorbeeld in een mouw of broek.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jurk van de prinses had een grote <w>pof</w> op de schouders.",
                            "target_lang_translations": {
                                "en": "The princess's dress had a large <w>puff</w> on the shoulders.",
                                "ru": "У платья принцессы был большой <w>буф</w> на плечах."
                            }
                        },
                        {
                            "text": "Door de elastische band valt de stof met een lichte <w>pof</w> over de pols.",
                            "target_lang_translations": {
                                "en": "Due to the elastic band, the fabric falls over the wrist with a slight <w>puff</w>.",
                                "ru": "Благодаря резинке ткань ложится легким <w>напуском</w> на запястье."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A bulging fold or widening in a garment, for example in a sleeve or trousers.",
                        "ru": "Выпуклая складка или расширение в одежде, например, на рукаве или брюках."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "puff",
                                "target_lang_sense_clarification": "swelling shape in fabric"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "буф",
                                "target_lang_sense_clarification": "пышная складка на рукаве"
                            },
                            {
                                "target_lang_word": "напуск",
                                "target_lang_sense_clarification": "свободное нависание ткани"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b80e50e-7131-3ff1-a9da-42afa485100a",
                    "sense_definition": "Een brok gedroogde veengrond of turf.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer gooide nog een <w>pof</w> in de kachel om het vuur op te stoken.",
                            "target_lang_translations": {
                                "en": "The farmer threw another <w>peat sod</w> into the stove to stoke the fire.",
                                "ru": "Фермер бросил еще один <w>кусок торфа</w> в печь, чтобы разжечь огонь."
                            }
                        },
                        {
                            "text": "Tijdens het graven in het veen stuitte hij op een harde <w>pof</w>.",
                            "target_lang_translations": {
                                "en": "While digging in the peat, he hit a hard <w>lump</w>.",
                                "ru": "Копая торф, он наткнулся на твердый <w>кусок</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A lump of dried peat soil or turf.",
                        "ru": "Кусок высушенного торфа."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "peat sod",
                                "target_lang_sense_clarification": "piece of dried peat"
                            },
                            {
                                "target_lang_word": "turf",
                                "target_lang_sense_clarification": "fuel material"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кусок торфа",
                                "target_lang_sense_clarification": "топливо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "77ead027-9167-3d62-b933-ecc6fb2f6282",
                    "sense_definition": "Vervoeging van <w>poffen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>pof</w> de kastanjes in de hete as van het haardvuur.",
                            "target_lang_translations": {
                                "en": "I <w>roast</w> the chestnuts in the hot ashes of the fireplace.",
                                "ru": "Я <w>пеку</w> каштаны в горячей золе камина."
                            }
                        },
                        {
                            "text": "<w>Pof</w> jij de aardappels even in de oven voor het avondeten?",
                            "target_lang_translations": {
                                "en": "Will you <w>roast</w> the potatoes in the oven for dinner?",
                                "ru": "Ты <w>запечешь</w> картошку в духовке к ужину?"
                            }
                        }
                    ],
                    "traits": [

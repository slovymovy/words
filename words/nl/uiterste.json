        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ed43eff5-7a8a-341a-b1a7-96ad30f9f93b",
                    "sense_definition": "Het verste punt of de hoogste graad die bereikt kan worden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "limit_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De atleet ging tijdens de wedstrijd tot het <w>uiterste</w>.",
                            "target_lang_translations": {
                                "ru": "Во время соревнований атлет выложился до <w>предела</w>."
                            }
                        },
                        {
                            "text": "In het <w>uiterste</w> geval zullen we de reis moeten annuleren.",
                            "target_lang_translations": {
                                "ru": "В <w>крайнем</w> случае нам придется отменить поездку."
                            }
                        }
                    ],
                    "synonyms": [
                        "maximale"
                    ],
                    "common_phrases": [
                        "tot het uiterste",
                        "in het uiterste geval"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самая дальняя точка или высшая степень, которой можно достичь; предел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предел",
                                "target_lang_sense_clarification": "возможностей, сил"
                            },
                            {
                                "target_lang_word": "крайний случай",
                                "target_lang_sense_clarification": "ситуация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "225caaa2-ba6a-39cd-9266-3fb010ce043f",
                    "sense_definition": "Iets dat lijnrecht tegenover iets anders staat; een uiterste pool of tegenstelling.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "extreme_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is een vrouw van <w>uitersten</w>: soms heel vrolijk, dan weer heel somber.",
                            "target_lang_translations": {
                                "ru": "Она женщина <w>крайностей</w>: то очень веселая, то снова очень мрачная."
                            }
                        },
                        {
                            "text": "In de politiek zoeken deze partijen vaak de <w>uitersten</w> op.",
                            "target_lang_translations": {
                                "ru": "В политике эти партии часто ищут <w>крайностей</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "van uitersten",
                        "twee uitersten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечто, прямо противоположное чему-то другому; крайность или противоположность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крайность",
                                "target_lang_sense_clarification": "поведение, характер"

                {
                    "sense_id": "645e2006-c747-36d5-9da7-eb0db26ac17d",
                    "sense_definition": "An item of clothing, usually worn by women or girls, that covers the body and hangs down over the legs.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a beautiful new <w>dress</w> for the party.",
                            "target_lang_translations": {
                                "ru": "Она купила красивое новое <w>платье</w> для вечеринки."
                            }
                        },
                        {
                            "text": "The bride wore a long white <w>dress</w> on her wedding day.",
                            "target_lang_translations": {
                                "ru": "Невеста была в длинном белом <w>платье</w> в день своей свадьбы."
                            }
                        }
                    ],
                    "synonyms": [
                        "robe"
                    ],
                    "common_phrases": [
                        "wedding dress",
                        "evening dress",
                        "summer dress",
                        "wear a dress"
                    ],
                    "target_lang_definitions": {

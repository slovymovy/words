                    "sense_id": "1c3742f8-0b79-3ce8-948d-be616a46642e",
                    "sense_definition": "Een getal dat wordt gebruikt om iets of iemand te identificeren of een rangorde aan te geven.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "identification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik woon in deze straat op <w>nummer</w> tien.",
                            "target_lang_translations": {
                                "en": "I live on this street at <w>number</w> ten."
                            }
                        },
                        {
                            "text": "De Nederlandse atleet staat op <w>nummer</w> één in de wereldranglijst.",
                            "target_lang_translations": {
                                "en": "The Dutch athlete is ranked <w>number</w> one in the world ranking."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op nummer één",
                        "een nummer trekken",
                        "het juiste nummer"
                    ],
                    "target_lang_definitions": {

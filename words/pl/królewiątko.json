                        "książątko"
                    ],
                    "common_phrases": [
                        "małe królewiątko",
                        "śpiące królewiątko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое название ребенка из королевской семьи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "королевич",
                                "target_lang_sense_clarification": "сын короля"
                            },
                            {
                                "target_lang_word": "королевское дитя",
                                "target_lang_sense_clarification": "ребенок королевской крови"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bceabee-7add-3fe3-8aa9-523baeb51ce7",
                    "sense_definition": "Władca małego państwa lub terytorium, często o niewielkim znaczeniu politycznym.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każde lokalne <w>królewiątko</w> chciało prowadzić w regionie własną politykę.",

            "pos": "name",
            "senses": [
                {
                    "sense_id": "e1c16d13-d14b-3d44-a842-0f86089ebd5b",
                    "sense_definition": "Sala na górze Syjon w Jerozolimie, tradycyjnie uznawana za miejsce Ostatniej Wieczerzy Jezusa z apostołami oraz Zesłania Ducha Świętego",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biblical_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Pielgrzymi udali się do Jerozolimy, aby odwiedzić <w>Wieczernik</w>.",
                            "target_lang_translations": {
                                "ru": "Паломники отправились в Иерусалим, чтобы посетить <w>Сионскую горницу</w>."
                            }
                        },
                        {
                            "text": "Apostołowie zgromadzili się w <w>Wieczerniku</w> w dniu Pięćdziesiątnicy.",
                            "target_lang_translations": {
                                "ru": "Апостолы собрались в <w>Сионской горнице</w> в день Пятидесятницы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drzwi Wieczernika",
                        "modlitwa w Wieczerniku",
                        "wyjście z Wieczernika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горница на горе Сион в Иерусалиме, традиционно считающаяся местом Тайной вечери Иисуса с апостолами и Сошествия Святого Духа"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сионская горница",
                                "target_lang_sense_clarification": "место Тайной вечери"
                            },
                            {
                                "target_lang_word": "Горница",
                                "target_lang_sense_clarification": "библейское место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0c323fe-cd06-3bf1-94d4-8797025de414",
                    "sense_definition": "Kaplica wchodząca w skład zespołu kalwaryjskiego, upamiętniająca ustanowienie Eucharystii",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biblical_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Architekt zaprojektował nowy <w>Wieczernik</w> na terenie sanktuarium.",
                            "target_lang_translations": {
                                "ru": "Архитектор спроектировал новую <w>Горницу</w> на территории святилища."
                            }
                        },
                        {
                            "text": "Wierni procesyjnie przeszli od stacji sądu do <w>Wieczernika</w>.",
                            "target_lang_translations": {
                                "ru": "Верующие прошли крестным ходом от стояния суда к <w>Горнице</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kalwaryjski Wieczernik",
                        "kaplica Wieczernika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часовня в составе комплекса Кальварии, посвященная установлению Евхаристии"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Горница",
                                "target_lang_sense_clarification": "часовня-стояние"
                            },
                            {
                                "target_lang_word": "Часовня Тайной вечери",
                                "target_lang_sense_clarification": "описательное название"
                            }
                        ]
                    }
                }
            ]
        },
        {

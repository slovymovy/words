    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0838c2f0-bb9c-393a-aef3-f275f6b9dabc",
                    "sense_definition": "Met veel variatie en zonder eentonigheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "varied",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zoek een baan met <w>afwisselend</w> werk zodat ik me niet ga vervelen.",
                            "target_lang_translations": {
                                "en": "I am looking for a job with <w>varied</w> work so I won't get bored.",
                                "ru": "Я ищу <w>разнообразную</w> работу, чтобы мне не стало скучно."
                            }
                        },
                        {
                            "text": "Tijdens de vakantie genoten we van een <w>afwisselend</w> landschap met bergen en meren.",
                            "target_lang_translations": {
                                "en": "During the vacation, we enjoyed a <w>varied</w> landscape with mountains and lakes.",
                                "ru": "Во время отпуска мы наслаждались <w>разнообразным</w> пейзажем с горами и озерами."
                            }
                        }
                    ],
                    "synonyms": [
                        "gevarieerd",
                        "veelzijdig",
                        "divers"
                    ],
                    "antonyms": [
                        "eentonig",
                        "saai",
                        "monotoon"
                    ],
                    "common_phrases": [
                        "afwisselend werk",
                        "een afwisselend programma",
                        "een afwisselend landschap"
                    ],
                    "target_lang_definitions": {
                        "en": "Characterized by variety and a lack of monotony.",
                        "ru": "С большим разнообразием и без однообразия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "varied",
                                "target_lang_sense_clarification": "containing different elements; not monotonous"
                            },
                            {
                                "target_lang_word": "diverse",
                                "target_lang_sense_clarification": "showing a great deal of variety"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "разнообразный",
                                "target_lang_sense_clarification": "содержащий различные элементы, не монотонный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea018d24-eb59-38ca-96a1-eefaf4324ed0",
                    "sense_definition": "Elkaar steeds opvolgend of vervangend.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "alternating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>afwisselende</w> temperaturen zorgden ervoor dat het ijs snel smolt.",
                            "target_lang_translations": {
                                "en": "The <w>fluctuating</w> temperatures caused the ice to melt quickly.",
                                "ru": "<w>Переменчивые</w> температуры привели к тому, что лед быстро растаял."
                            }
                        },
                        {
                            "text": "Door de <w>afwisselende</w> successen van beide teams bleef de wedstrijd spannend.",
                            "target_lang_translations": {
                                "en": "Due to the <w>alternating</w> successes of both teams, the match remained exciting.",
                                "ru": "Благодаря <w>переменным</w> успехам обеих команд матч оставался захватывающим."
                            }
                        }
                    ],
                    "synonyms": [
                        "beurtelings"
                    ],
                    "antonyms": [
                        "constant",
                        "onveranderlijk"
                    ],
                    "common_phrases": [
                        "afwisselende seizoenen"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c9d58fea-8177-35d2-bda0-8f2eac908e46",
                    "sense_definition": "mężczyzna żyjący z cudzej prostytucji, zabierający prostytutce część jej dochodu w zamian za ochronę",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "pimp_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jak tylko podszedł do dziewczyny, natychmiast pojawił się jej <w>alfons</w>.",
                            "target_lang_translations": {
                                "ru": "Как только он подошел к девушке, тут же появился её <w>сутенёр</w>."
                            }
                        },
                        {
                            "text": "Policja aresztowała <w>alfonsa</w>, który wymuszał haracze od pracujących kobiet.",
                            "target_lang_translations": {
                                "ru": "Полиция арестовала <w>сутенера</w>, который вымогал деньги у работающих женщин."
                            }
                        }
                    ],
                    "synonyms": [
                        "sutener",
                        "stręczyciel",
                        "kupler",
                        "rajfur",
                        "alfonsiak"
                    ],
                    "common_phrases": [
                        "brutalny alfons",
                        "znany alfons"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo potoczne, oficjalnie: sutener"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, живущий за счет проституции других, забирающий у проститутки часть дохода в обмен на защиту"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сутенёр",
                                "target_lang_sense_clarification": "организатор проституции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75e72b8f-b6b6-3179-951d-669cee663b71",
                    "sense_definition": "obraźliwe określenie mężczyzny, do którego czuje się niechęć lub pogardę",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę mieć z tym <w>alfonsem</w> nic wspólnego po tym, jak mnie oszukał.",
                            "target_lang_translations": {
                                "ru": "Я не хочу иметь ничего общего с этим <w>мерзавцем</w> после того, как он меня обманул."
                            }
                        },
                        {
                            "text": "Ten <w>alfons</w> znowu narobił kłopotów w naszej dzielnicy.",
                            "target_lang_translations": {
                                "ru": "Этот <w>подонок</w> снова натворил бед в нашем районе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "obraźliwe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оскорбительное обозначение мужчины, к которому испытывают неприязнь или презрение"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мерзавец",
                                "target_lang_sense_clarification": "ругательство"
                            },
                            {
                                "target_lang_word": "подонок",
                                "target_lang_sense_clarification": "ругательство"
                            }
                        ]
                    }
                }
            ]
        },
        {

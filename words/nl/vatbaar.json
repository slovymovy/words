                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постижимый",
                                "target_lang_sense_clarification": "доступный для понимания разумом"
                            },
                            {
                                "target_lang_word": "понятный",
                                "target_lang_sense_clarification": "ясный, доступный"

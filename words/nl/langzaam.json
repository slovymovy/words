{
    "word_family": [
        "langzaamheid",
        "verlangzamen"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7e302bd3-19b9-38ae-bd39-5376dcd0f136",
                    "sense_definition": "Met weinig snelheid; niet snel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die auto was de <w>langzaamste</w> auto die ik ooit gezien heb.",
                            "target_lang_translations": {
                                "en": "That car was the <w>slowest</w> car I have ever seen.",
                                "ru": "Эта машина была самой <w>медленной</w> машиной, которую я когда-либо видел."
                            }
                        },
                        {
                            "text": "Mijn oude computer is erg <w>langzaam</w> geworden.",
                            "target_lang_translations": {
                                "en": "My old computer has become very <w>slow</w>.",
                                "ru": "Мой старый компьютер стал очень <w>медленным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "traag"
                    ],
                    "antonyms": [
                        "vlug",
                        "vlug"
                    ],
                    "common_phrases": [
                        "langzaam maar zeker"
                    ],
                    "target_lang_definitions": {
                        "en": "With little speed; not fast.",
                        "ru": "С низкой скоростью; не быстрый."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slow",
                                "target_lang_sense_clarification": "moving, operating, or happening at a low speed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "медленный",
                                "target_lang_sense_clarification": "общая характеристика скорости"
                            },
                            {
                                "target_lang_word": "неспешный",
                                "target_lang_sense_clarification": "без суеты или спешки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62abd2f2-59c1-3b99-9209-fcb7c78e0082",
                    "sense_definition": "Geleidelijk; iets dat zich traag ontwikkelt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik voelde mijn slaapzak <w>langzaam</w> vochtig worden.",
                            "target_lang_translations": {
                                "en": "I felt my sleeping bag <w>slowly</w> becoming damp.",
                                "ru": "Я почувствовал, как мой спальный мешок <w>постепенно</w> становится влажным."
                            }
                        },
                        {
                            "text": "Het wordt <w>langzaam</w> donker buiten.",
                            "target_lang_translations": {
                                "en": "It is <w>slowly</w> getting dark outside.",
                                "ru": "На улице <w>постепенно</w> темнеет."
                            }
                        }
                    ],
                    "synonyms": [
                        "geleidelijk"
                    ],
                    "common_phrases": [
                        "langzaam aan"
                    ],
                    "target_lang_definitions": {
                        "en": "Gradual; something that develops slowly over time.",
                        "ru": "Постепенный; развивающийся медленно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slowly",
                                "target_lang_sense_clarification": "happening over a long period of time"
                            },
                            {
                                "target_lang_word": "gradually",
                                "target_lang_sense_clarification": "occurring in small stages or degrees"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "постепенный",
                                "target_lang_sense_clarification": "происходящий поэтапно"
                            },
                            {
                                "target_lang_word": "медленный",
                                "target_lang_sense_clarification": "затяжной процесс"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7c0920fb-1e2d-3f61-a477-243d77682fb4",
                    "sense_definition": "Op een trage manier; zonder haast.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Langzaam</w> werd hij wat beter.",
                            "target_lang_translations": {
                                "en": "<w>Slowly</w>, he got a bit better.",
                                "ru": "<w>Медленно</w> ему становилось лучше."
                            }
                        },
                        {
                            "text": "Ze liepen <w>langzaam</w> door het park.",
                            "target_lang_translations": {
                                "en": "They walked <w>slowly</w> through the park.",
                                "ru": "Они <w>медленно</w> шли по парку."
                            }
                        }
                    ],
                    "synonyms": [
                        "traag"
                    ],
                    "target_lang_definitions": {
                        "en": "In a slow manner; without haste.",
                        "ru": "Медленно; не торопясь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slowly",
                                "target_lang_sense_clarification": "at a slow speed; not quickly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "медленно",
                                "target_lang_sense_clarification": "наречие скорости"
                            },
                            {
                                "target_lang_word": "неспешно",
                                "target_lang_sense_clarification": "спокойно, без спешки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

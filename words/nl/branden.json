                {
                    "sense_id": "8c52b962-001a-3fcb-9fa8-17a6884007f7",
                    "sense_definition": "In brand staan; verteerd worden door vuur.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fire_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het droge hout in de open haard <w>brandt</w> erg goed.",
                            "target_lang_translations": {
                                "en": "The dry wood in the fireplace <w>burns</w> very well.",
                                "ru": "Сухие дрова в камине очень хорошо <w>горят</w>."
                            }
                        },
                        {
                            "text": "Kijk uit, het vuur <w>brandt</w> nog steeds fel.",
                            "target_lang_translations": {
                                "en": "Watch out, the fire is still <w>burning</w> fiercely.",
                                "ru": "Осторожно, огонь все еще сильно <w>горит</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "fikken"
                    ],
                    "antonyms": [
                        "doven"
                    ],
                    "common_phrases": [
                        "het vuur brandt",
                        "goed branden"
                    ],
                    "target_lang_definitions": {
                        "en": "To be on fire; to be consumed by flames.",
                        "ru": "Гореть, быть охваченным огнем."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "burn",
                                "target_lang_sense_clarification": "to be on fire"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гореть",
                                "target_lang_sense_clarification": "быть в огне"
                            },
                            {
                                "target_lang_word": "пылать",
                                "target_lang_sense_clarification": "гореть ярко или сильно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45344bae-3104-3be9-ae89-966c5b2bf873",
                    "sense_definition": "Licht geven of ingeschakeld zijn (van een lamp of lichtbron).",

                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>expecting</w> couple is busy preparing the nursery.",
                            "target_lang_translations": {
                                "ru": "<w>Ожидающая ребенка</w> пара занята подготовкой детской."
                            }
                        },
                        {
                            "text": "She is <w>expecting</w> and will go on maternity leave soon.",
                            "target_lang_translations": {
                                "ru": "Она <w>ждет ребенка</w> и скоро уйдет в декретный отпуск."
                            }
                        }
                    ],
                    "synonyms": [
                        "pregnant",
                        "in the family way"
                    ],
                    "common_phrases": [
                        "expecting a baby",
                        "expecting mother",
                        "expecting parents"
                    ],
                    "target_lang_definitions": {
                        "ru": "Беременная; ожидающая рождения ребенка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ждать ребенка",
                                "target_lang_sense_clarification": "готовиться к рождению ребенка"
                            },
                            {
                                "target_lang_word": "беременная",
                                "target_lang_sense_clarification": "находящаяся в состоянии беременности"
                            },

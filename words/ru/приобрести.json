                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "acquisition_social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В летнем лагере ребёнку будет легко <приобрести> новых друзей.",
                            "target_lang_translations": {

                {
                    "sense_id": "34c10294-3cc8-34bd-8981-138077eb59ea",
                    "sense_definition": "A piece of work or a project given to students to be done at home or in class.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "task_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher gave us a difficult <w>assignment</w> to finish over the weekend.",
                            "target_lang_translations": {
                                "ru": "Учитель дал нам сложное <w>задание</w>, которое нужно закончить за выходные."
                            }
                        },
                        {
                            "text": "She completed her history <w>assignment</w> just before the deadline.",
                            "target_lang_translations": {
                                "ru": "Она закончила своё <w>задание</w> по истории прямо перед сроком сдачи."
                            }
                        }
                    ],
                    "synonyms": [
                        "homework",
                        "project",
                        "coursework"
                    ],
                    "common_phrases": [
                        "homework assignment",
                        "written assignment",
                        "submit an assignment"
                    ],
                    "target_lang_definitions": {

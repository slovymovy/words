    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "29caee2b-caf1-38a6-a2a5-696fe8457b33",
                    "sense_definition": "Wynik jakiegoś działania, zdarzenia lub procesu; to, co nastąpiło jako następstwo czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "result_consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Najczęściej wymienianym <w>skutkiem</w> tej decyzji był gwałtowny wzrost przestępczości w całej okolicy.",
                            "target_lang_translations": {
                                "ru": "Чаще всего упоминаемым <w>последствием</w> этого решения был резкий рост преступности во всей округе."
                            }
                        },
                        {
                            "text": "Adam nie uważał na lekcjach, czego bezpośrednim <w>skutkiem</w> było niezaliczenie ważnego sprawdzianu.",
                            "target_lang_translations": {
                                "ru": "Адам не был внимателен на уроках, прямым <w>следствием</w> чего стала несдача важной контрольной."
                            }
                        }
                    ],
                    "synonyms": [
                        "wynik",
                        "rezultat",
                        "konsekwencja",
                        "następstwo"
                    ],
                    "antonyms": [
                        "przyczyna"
                    ],
                    "common_phrases": [
                        "odnieść skutek",
                        "przyczyna i skutek",
                        "mieć skutek",
                        "opłakane skutki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Результат какого-либо действия, события или процесса; то, что последовало за чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последствие",
                                "target_lang_sense_clarification": "часто о негативных или долгосрочных результатах"
                            },
                            {
                                "target_lang_word": "результат",
                                "target_lang_sense_clarification": "общий термин; итог действия"
                            },
                            {
                                "target_lang_word": "следствие",
                                "target_lang_sense_clarification": "то, что логически вытекает из причины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc642517-b431-356b-abb9-847343df3f9c",
                    "sense_definition": "Fakt, że planowane wydarzenie rzeczywiście miało miejsce lub zostało zrealizowane.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "result_consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo wielu trudności, planowana od dawna wyprawa w góry wreszcie doszła do <w>skutku</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на множество трудностей, давно запланированный поход в горы наконец <w>состоялся</w>."
                            }
                        },
                        {
                            "text": "Ważne spotkanie zarządu nie doszło do <w>skutku</w> z powodu nagłej choroby prezesa.",
                            "target_lang_translations": {
                                "ru": "Важное заседание правления не <w>состоялось</w> из-за внезапной болезни председателя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dojść do skutku",
                        "nie dojść do skutku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт того, что запланированное событие действительно произошло или было реализовано."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осуществление",
                                "target_lang_sense_clarification": "реализация намеченного плана"
                            },
                            {
                                "target_lang_word": "исполнение",
                                "target_lang_sense_clarification": "воплощение в жизнь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1191f059-190e-3dac-8968-952ee3f715ca",
                    "sense_definition": "Свойство того, что полностью соответствует действительности, истине или установленному порядку; отсутствие ошибок.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В его рассказе была удивительная <w>точность</w> деталей.",
                            "target_lang_translations": {
                                "en": "There was amazing <w>accuracy</w> of detail in his story."
                            }
                        },
                        {
                            "text": "От хирурга требуется ювелирная <w>точность</w> движений.",
                            "target_lang_translations": {
                                "en": "Jeweler-like <w>precision</w> of movement is required of a surgeon."
                            }
                        }
                    ],
                    "synonyms": [
                        "выверенность"
                    ],
                    "antonyms": [
                        "неточность"
                    ],
                    "common_phrases": [
                        "высокая точность",
                        "с абсолютной точностью",
                        "точность перевода",
                        "точность воспроизведения"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of being true, correct, or exact; freedom from mistake or error."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accuracy",
                                "target_lang_sense_clarification": "freedom from error; correctness"
                            },
                            {
                                "target_lang_word": "precision",
                                "target_lang_sense_clarification": "refinement and exactness in execution"
                            },
                            {
                                "target_lang_word": "exactness",
                                "target_lang_sense_clarification": "strict conformity to fact or reality"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f3ed1f8-64ee-3f7e-a2b5-497ec2b2d224",
                    "sense_definition": "Умение делать что-либо или приходить куда-либо строго в назначенное время; пунктуальность.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <w>точность</w> известна всем: он приходит ровно в девять.",
                            "target_lang_translations": {
                                "en": "His <w>punctuality</w> is known to everyone: he arrives exactly at nine."
                            }
                        },
                        {
                            "text": "Поезда в этой стране отличаются идеальной <w>точностью</w> расписания.",
                            "target_lang_translations": {
                                "en": "Trains in this country are distinguished by the ideal <w>punctuality</w> of the schedule."
                            }
                        }
                    ],
                    "synonyms": [
                        "аккуратность"
                    ],
                    "antonyms": [
                        "неаккуратность"
                    ],
                    "common_phrases": [
                        "любить точность",
                        "точность — вежливость королей"
                    ],
                    "target_lang_definitions": {
                        "en": "The characteristic of being able to complete a required task or fulfill an obligation before or at a previously designated time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "punctuality",
                                "target_lang_sense_clarification": "the habit of being on time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce9d4a46-8c01-3e60-8fad-0115dd242034",
                    "sense_definition": "Степень близости измеренного значения к истинному или эталонному; техническая характеристика прибора.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Точность</w> этих весов позволяет взвешивать золото.",
                            "target_lang_translations": {
                                "en": "The <w>precision</w> of these scales allows one to weigh gold."
                            }
                        },
                        {
                            "text": "Инженер проверил <w>точность</w> работы сложного механизма.",
                            "target_lang_translations": {
                                "en": "The engineer checked the <w>accuracy</w> of the complex mechanism's operation."
                            }
                        }
                    ],
                    "antonyms": [
                        "приблизительность",
                        "неточность"
                    ],
                    "common_phrases": [
                        "точность измерения",
                        "точность прибора",
                        "класс точности",
                        "точность вычислений"
                    ],
                    "target_lang_definitions": {
                        "en": "The degree to which the result of a measurement, calculation, or specification conforms to the correct value or a standard."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accuracy",
                                "target_lang_sense_clarification": "closeness to the true value"
                            },
                            {
                                "target_lang_word": "precision",
                                "target_lang_sense_clarification": "degree of refinement or resolution in measurement"
                            }
                        ]
                    }
                }
            ]
        }
    ]

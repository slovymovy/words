                        "examples": [
                            {
                                "example_id": "a1aa4850-f4d2-48c2-b593-0efa76c30c14",
                                "sense_id": "98f526f5-f5df-3166-87b8-04d2bde5ae32",
                                "text": "This is a very miserable conveyance, used by the middling rank of native men; but has been improved, by Europeans, into the Doli for conveying the sick, by lengthening it so as to admit the passenger to lie at length."
                            },
                            {
                                "example_id": "0ad8ecea-6208-40db-989d-75c25349cd41",
                                "sense_id": "98f526f5-f5df-3166-87b8-04d2bde5ae32",
                                "text": "...bahlis, chakras, bullock-carts, dolis, tattus, palkis, miyanas, kharkharinas, bajras and chaupalas, were the common vehicles."
                            },
                            {
                                "example_id": "e4c771a5-695d-4ca5-bbe3-a7fcb46bed8f",
                                "sense_id": "98f526f5-f5df-3166-87b8-04d2bde5ae32",
                                "text": "The Muslim ladies who earlier moved out in covered palanquins, dolis and muhafas or completely veiled coaches and victorias are now obliged to go about in tongas, rikshaws and buses leaving aside the earlier scruples."
                            }
                        ]
                    }

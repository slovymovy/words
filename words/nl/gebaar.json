                            },
                            {
                                "target_lang_word": "облик",
                                "target_lang_sense_clarification": "внешний вид"
                            },
                            {
                                "target_lang_word": "манера",
                                "target_lang_sense_clarification": "поведение"
                            }
                        ]
                    }
                }

{
    "word_family": [
        "ban"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e728c75b-ce5e-36cb-9257-e0bf9cf14e27",
                    "sense_definition": "A type of advertisement on a web page, usually in the form of a rectangular graphic at the top, bottom, or side of the screen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "digital_ad",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I clicked on the <w>banner</w> to learn more about the sale.",
                            "target_lang_translations": {
                                "ru": "Я нажал на <w>баннер</w>, чтобы узнать больше о распродаже."
                            }
                        },
                        {
                            "text": "The website makes money by displaying a <w>banner</w> on its homepage.",
                            "target_lang_translations": {
                                "ru": "Сайт зарабатывает, показывая <w>баннер</w> на главной странице."
                            }
                        }
                    ],
                    "synonyms": [
                        "advertisement",
                        "ad"
                    ],
                    "common_phrases": [
                        "banner ad",
                        "click on a banner",
                        "web banner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рекламное объявление на веб-странице, обычно в виде прямоугольного графического изображения вверху, внизу или сбоку экрана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баннер",
                                "target_lang_sense_clarification": "интернет-реклама"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41ef459a-300a-3fc0-8fb2-4e6eb77cf617",
                    "sense_definition": "A long piece of cloth or paper with words or a design on it, often hung in a public place or carried in a demonstration.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "signage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The students hung a large <w>banner</w> across the hallway to welcome the visitors.",
                            "target_lang_translations": {
                                "ru": "Студенты повесили большую <w>растяжку</w> поперек коридора, чтобы поприветствовать гостей."
                            }
                        },
                        {
                            "text": "Protesters marched through the city carrying a <w>banner</w> asking for peace.",
                            "target_lang_translations": {
                                "ru": "Протестующие шли по городу, неся <w>транспарант</w> с призывом к миру."
                            }
                        }
                    ],
                    "synonyms": [
                        "placard",
                        "sign",
                        "poster"
                    ],
                    "common_phrases": [
                        "hang a banner",
                        "protest banner",
                        "welcome banner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинное полотнище из ткани или бумаги с надписью или рисунком, вывешиваемое в общественном месте или используемое на демонстрациях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "транспарант",
                                "target_lang_sense_clarification": "на демонстрации"
                            },
                            {
                                "target_lang_word": "растяжка",
                                "target_lang_sense_clarification": "рекламная или приветственная над улицей"
                            },
                            {
                                "target_lang_word": "плакат",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e42007f4-4e25-3bfc-8e50-55f54eacf9d7",
                    "sense_definition": "A flag or standard used by a country, army, or leader, often symbolizing their authority.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "flag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knights rode into battle with their <w>banner</w> flying high.",
                            "target_lang_translations": {
                                "ru": "Рыцари ехали в бой с высоко поднятым <w>стягом</w>."
                            }
                        },
                        {
                            "text": "Soldiers rallied around the royal <w>banner</w> during the conflict.",
                            "target_lang_translations": {
                                "ru": "Солдаты сплотились вокруг королевского <w>знамени</w> во время конфликта."
                            }
                        }
                    ],
                    "synonyms": [
                        "flag",
                        "standard",
                        "ensign"
                    ],
                    "common_phrases": [
                        "star-spangled banner",
                        "royal banner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Флаг или штандарт, используемый страной, армией или лидером, часто символизирующий их власть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знамя",
                                "target_lang_sense_clarification": "символ воинской части или организации"
                            },
                            {
                                "target_lang_word": "стяг",
                                "target_lang_sense_clarification": "устаревшее, высокое"
                            },
                            {
                                "target_lang_word": "штандарт",
                                "target_lang_sense_clarification": "персональный флаг правителя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff5810ac-b711-36d4-8ff2-a9d16f6fd862",
                    "sense_definition": "A particular cause, principle, or belief that is supported by a group of people.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Different political groups united under the <w>banner</w> of freedom.",
                            "target_lang_translations": {
                                "ru": "Различные политические группы объединились под <w>знаменем</w> свободы."
                            }
                        },
                        {
                            "text": "They campaigned for change under the <w>banner</w> of environmental protection.",
                            "target_lang_translations": {
                                "ru": "Они проводили кампанию за перемены под <w>флагом</w> защиты окружающей среды."
                            }
                        }
                    ],
                    "synonyms": [
                        "cause",
                        "campaign",
                        "movement"
                    ],
                    "common_phrases": [
                        "under the banner of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенное дело, принцип или убеждение, поддерживаемое группой людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знамя",
                                "target_lang_sense_clarification": "символ идеи или движения"
                            },
                            {
                                "target_lang_word": "флаг",
                                "target_lang_sense_clarification": "в переносном смысле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dcf67488-0275-3c4f-b154-853ba640db4c",
                    "sense_definition": "The title of a newspaper printed in large letters on the front page, or a headline that runs across the entire width of the page.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "journalism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scandal was announced with a huge <w>banner</w> on the front page.",
                            "target_lang_translations": {
                                "ru": "О скандале было объявлено огромной <w>шапкой</w> на первой полосе."
                            }
                        },
                        {
                            "text": "The newspaper's <w>banner</w> has remained unchanged for fifty years.",
                            "target_lang_translations": {
                                "ru": "<w>Шапка</w> газеты оставалась неизменной в течение пятидесяти лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "headline",
                        "masthead",
                        "nameplate"
                    ],
                    "common_phrases": [
                        "banner headline"
                    ],
                    "target_lang_definitions": {
                        "ru": "Название газеты, напечатанное крупными буквами на первой полосе, или заголовок во всю ширину страницы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шапка",
                                "target_lang_sense_clarification": "газетный заголовок"
                            },
                            {
                                "target_lang_word": "заголовок",
                                "target_lang_sense_clarification": "крупный шрифт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f082e40a-cdbd-3480-a711-21065c6903b3",
                    "sense_definition": "Someone who bans or prohibits something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "agent_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>banner</w> of the book faced criticism from free speech advocates.",
                            "target_lang_translations": {
                                "ru": "<w>Запретитель</w> книги столкнулся с критикой со стороны защитников свободы слова."
                            }
                        },
                        {
                            "text": "As a strict <w>banner</w> of sugary drinks, the school removed all soda machines.",
                            "target_lang_translations": {
                                "ru": "Будучи строгим <w>противником</w> сладких напитков, школа убрала все автоматы с газировкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "prohibitor",
                        "censor"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "agent noun derived from ban"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто налагает запрет или запрещает что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запретитель",
                                "target_lang_sense_clarification": "инициатор запрета"
                            },
                            {
                                "target_lang_word": "цензор",
                                "target_lang_sense_clarification": "контролирующий орган"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81c3f35b-5d45-32a3-8f6e-5e42ed50c151",
                    "sense_definition": "An administrative division in Inner Mongolia, China.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He traveled to a remote <w>banner</w> in Inner Mongolia to study local customs.",
                            "target_lang_translations": {
                                "ru": "Он отправился в отдаленный <w>хошун</w> во Внутренней Монголии, чтобы изучить местные обычаи."
                            }
                        },
                        {
                            "text": "The administrative structure of the <w>banner</w> dates back to the Qing dynasty.",
                            "target_lang_translations": {
                                "ru": "Административная структура <w>хошуна</w> восходит к династии Цин."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Inner Mongolia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Административная единица во Внутренней Монголии, Китай."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хошун",
                                "target_lang_sense_clarification": "административная единица"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "711edb1e-d50a-36b6-918a-083403a010fd",
                    "sense_definition": "Extremely good, successful, or outstanding.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "exceptional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a <w>banner</w> year for the company, with record profits and growth.",
                            "target_lang_translations": {
                                "ru": "Это был <w>рекордный</w> год для компании, с рекордной прибылью и ростом."
                            }
                        },
                        {
                            "text": "After a <w>banner</w> season, the team finally won the championship.",
                            "target_lang_translations": {
                                "ru": "После <w>выдающегося</w> сезона команда наконец выиграла чемпионат."
                            }
                        }
                    ],
                    "synonyms": [
                        "exceptional",
                        "outstanding",
                        "excellent"
                    ],
                    "antonyms": [
                        "disastrous",
                        "poor"
                    ],
                    "common_phrases": [
                        "banner year",
                        "banner season"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно хороший, успешный или выдающийся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рекордный",
                                "target_lang_sense_clarification": "о показателях"
                            },
                            {
                                "target_lang_word": "выдающийся",
                                "target_lang_sense_clarification": "об успехе"
                            },
                            {
                                "target_lang_word": "успешный",
                                "target_lang_sense_clarification": "общий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "97ba6629-859a-36a4-ab8c-d75cfa03f103",
                    "sense_definition": "To display something prominently, especially as a large headline.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "display",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The newspapers <w>bannered</w> the victory across their front pages.",
                            "target_lang_translations": {
                                "ru": "Газеты <w>вынесли в заголовок</w> победу на своих первых полосах."
                            }
                        },
                        {
                            "text": "Websites are <w>bannering</w> the breaking news for everyone to see.",
                            "target_lang_translations": {
                                "ru": "Веб-сайты <w>крупно афишируют</w> срочные новости, чтобы все их видели."
                            }
                        }
                    ],
                    "synonyms": [
                        "headline",
                        "broadcast"
                    ],
                    "target_lang_definitions": {
                        "ru": "Демонстрировать что-либо на видном месте, особенно в виде крупного заголовка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выносить в заголовок",
                                "target_lang_sense_clarification": "в газете"
                            },
                            {
                                "target_lang_word": "крупно афишировать",
                                "target_lang_sense_clarification": "показывать всем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b8c3c9b-75bd-3813-b85f-87cfc1579e38",
                    "sense_definition": "To decorate or furnish with a banner.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "decorate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hall was <w>bannered</w> with the school colors for the graduation ceremony.",
                            "target_lang_translations": {
                                "ru": "Зал был <w>украшен знаменами</w> цветов школы для церемонии вручения дипломов."
                            }
                        },
                        {
                            "text": "They <w>bannered</w> the streets to celebrate the festival.",
                            "target_lang_translations": {
                                "ru": "Они <w>украсили</w> улицы <w>флагами</w>, чтобы отпраздновать фестиваль."
                            }
                        }
                    ],
                    "synonyms": [
                        "decorate",
                        "adorn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшать или снабжать знаменами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украшать знаменами",
                                "target_lang_sense_clarification": "декорировать"
                            },
                            {
                                "target_lang_word": "вывешивать флаги",
                                "target_lang_sense_clarification": "праздничное оформление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "13e80995-6d3d-38cf-869c-34c3103a4f75",
                    "sense_definition": "A surname originating as an occupation for a standard-bearer.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Banner</w> has been working at the firm for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Бэннер</w> работает в фирме уже двадцать лет."
                            }
                        },
                        {
                            "text": "We invited the <w>Banner</w> family to the neighborhood gathering.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили семью <w>Бэннеров</w> на соседскую вечеринку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, происходящая от занятия знаменосца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бэннер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "pozostawiać",
        "pozostawienie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9eb1c202-2ec7-373e-b351-655861e5f4fa",
                    "sense_definition": "Zostawić kogoś lub coś w określonym miejscu, odchodząc stamtąd; nie zabrać czegoś ze sobą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_leaving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem <pozostawić> samochód w warsztacie na kilka dni.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось <оставить> машину в мастерской на несколько дней."
                            }
                        },
                        {
                            "text": "Goście hotelowi powinni <pozostawić> klucze w recepcji przed wyjściem.",
                            "target_lang_translations": {
                                "ru": "Гости отеля должны <оставить> ключи на ресепшене перед выходом."
                            }
                        }
                    ],
                    "synonyms": [
                        "zostawić",
                        "porzucić"
                    ],
                    "antonyms": [
                        "zabrać",
                        "wziąć"
                    ],
                    "common_phrases": [
                        "pozostawić ślady",
                        "pozostawić na pastwę",
                        "pozostawić rodzinę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставить кого-то или что-то в определенном месте, уходя оттуда; не забрать с собой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "поместить в каком-то месте и уйти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32337439-7cc4-3749-8c44-c349647e907a",
                    "sense_definition": "Sprawić, że ktoś lub coś znajdzie się w określonym stanie lub sytuacji; nie zmieniać stanu rzeczy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To trudne pytanie na razie <pozostawię> bez odpowiedzi.",
                            "target_lang_translations": {
                                "ru": "Этот трудный вопрос я пока <оставлю> без ответа."
                            }
                        },
                        {
                            "text": "Proszę <pozostawić> mnie w spokoju i nie przeszkadzać w pracy.",
                            "target_lang_translations": {
                                "ru": "Прошу <оставить> меня в покое и не мешать работать."
                            }
                        }
                    ],
                    "synonyms": [
                        "zostawić"
                    ],
                    "antonyms": [
                        "zmienić"
                    ],
                    "common_phrases": [
                        "pozostawić w spokoju",
                        "pozostawić bez zmian",
                        "pozostawić do wyboru",
                        "pozostawić bez echa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести кого-то или что-то в определенное состояние или ситуацию; сохранить текущее положение вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "сохранить в определенном состоянии (напр. в покое, без изменений)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b84e859c-23be-3df6-be1a-04bb11472611",
                    "sense_definition": "Przekazać coś w spadku po swojej śmierci lub zostawić po sobie jako dorobek.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legacy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek <pozostawił> nam w spadku piękny dom na wsi.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка <оставил> нам в наследство красивый дом в деревне."
                            }
                        },
                        {
                            "text": "Artysta <pozostawił> po sobie wiele wybitnych dzieł, które podziwiamy do dziś.",
                            "target_lang_translations": {
                                "ru": "Художник <оставил> после себя много выдающихся произведений, которыми мы восхищаемся по сей день."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapisać",
                        "przekazać"
                    ],
                    "common_phrases": [
                        "pozostawić majątek",
                        "pozostawić spadek",
                        "pozostawić po sobie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать что-то в наследство после смерти или оставить после себя как достижение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "передать в наследство или оставить после себя"
                            },
                            {
                                "target_lang_word": "завещать",
                                "target_lang_sense_clarification": "официально передать имущество после смерти"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    ],
                    "common_phrases": [
                        "вот так печаль",
                        "одна сплошная печаль"
                    ],
                    "target_lang_definitions": {
                        "en": "An event, circumstance, or person that serves as a source of sadness or disappointment"
                    },
                    "translations": {

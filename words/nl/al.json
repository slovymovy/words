                            {
                                "target_lang_word": "весь",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "вся",
                                "target_lang_sense_clarification": "женский род"
                            },
                            {
                                "target_lang_word": "всё",
                                "target_lang_sense_clarification": "средний род"
                            },
                            {
                                "target_lang_word": "все",
                                "target_lang_sense_clarification": "множественное число"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "all",
                                "target_lang_sense_clarification": "determiner"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "36da099f-2eaa-3ca5-9da8-106fab43913a",
                    "sense_definition": "Drukt een toegeving uit; ook in het geval dat.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "concession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Al</w> regent het, ik ga toch naar buiten.",
                            "target_lang_translations": {

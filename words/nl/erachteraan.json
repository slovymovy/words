                    "sense_id": "7c1e1196-f550-35a3-b81d-b40b8fb82046",
                    "sense_definition": "Achter iets of iemand aan, verwijzend naar iets dat eerder is genoemd.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dief rende weg en de politie ging <w>erachteraan</w>.",
                            "target_lang_translations": {
                                "ru": "Вор убежал, и полиция бросилась <w>вдогонку</w>."
                            }
                        },
                        {
                            "text": "Ik ben de bijlage vergeten, dus ik stuur die <w>erachteraan</w>.",
                            "target_lang_translations": {
                                "ru": "Я забыл вложение, поэтому отправляю его <w>следом</w>."
                            }
                        }
                    ],
                    "common_phrases": [

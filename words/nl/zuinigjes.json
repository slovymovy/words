{
    "word_family": [
        "zuinig",
        "zuinigheid",
        "bezuinigen"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3b95f0a5-fa5a-349a-818f-b2e86ea07ad8",
                    "sense_definition": "Op een spaarzame manier; met weinig middelen of verbruik.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_thrifty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De Nijverheid doet het <w>zuinigjes</w> aan met het energieverbruik.",
                            "target_lang_translations": {
                                "ru": "Nijverheid <w>экономно</w> подходит к потреблению энергии.",
                                "en": "The Nijverheid is being <w>economical</w> with its energy consumption."
                            }
                        },
                        {
                            "text": "We moeten deze maand <w>zuinigjes</w> leven om geld te besparen.",
                            "target_lang_translations": {
                                "ru": "В этом месяце нам придется жить <w>экономно</w>, чтобы сберечь деньги.",
                                "en": "We have to live <w>frugally</w> this month to save money."
                            }
                        }
                    ],
                    "synonyms": [
                        "spaarzaam",
                        "economisch"
                    ],
                    "common_phrases": [
                        "zuinigjes aan doen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verkleinwoord"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Экономным образом; с использованием малого количества средств или ресурсов.",
                        "en": "In a thrifty manner; using few resources or means."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экономно",
                                "target_lang_sense_clarification": "об использовании ресурсов или денег"
                            },
                            {
                                "target_lang_word": "бережливо",
                                "target_lang_sense_clarification": "о расходовании средств"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "frugally",
                                "target_lang_sense_clarification": "regarding money or lifestyle"
                            },
                            {
                                "target_lang_word": "economically",
                                "target_lang_sense_clarification": "regarding use of resources"
                            },
                            {
                                "target_lang_word": "sparingly",
                                "target_lang_sense_clarification": "using small amounts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e05b3956-30a7-3f94-9553-bb1760df7145",
                    "sense_definition": "Op een voorzichtige manier, zonder veel enthousiasme of uitbundigheid te tonen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij reageerde <w>zuinigjes</w> op het voorstel van zijn collega.",
                            "target_lang_translations": {
                                "ru": "Он <w>сдержанно</w> отреагировал на предложение коллеги.",
                                "en": "He reacted <w>tepidly</w> to his colleague's proposal."
                            }
                        },
                        {
                            "text": "Ze lachte <w>zuinigjes</w> om de flauwe grap.",
                            "target_lang_translations": {
                                "ru": "Она <w>кисло</w> улыбнулась плохой шутке.",
                                "en": "She smiled <w>thinly</w> at the lame joke."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorzichtig"
                    ],
                    "common_phrases": [
                        "zuinigjes kijken",
                        "zuinigjes lachen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verkleinwoord"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Осторожно, не проявляя особого энтузиазма или бурной радости.",
                        "en": "In a cautious way, without showing much enthusiasm or exuberance."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдержанно",
                                "target_lang_sense_clarification": "без проявления сильных эмоций"
                            },
                            {
                                "target_lang_word": "кисло",
                                "target_lang_sense_clarification": "о выражении лица или улыбке"
                            },
                            {
                                "target_lang_word": "натянуто",
                                "target_lang_sense_clarification": "об улыбке или смехе"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "tepidly",
                                "target_lang_sense_clarification": "regarding a reaction or enthusiasm"
                            },
                            {
                                "target_lang_word": "thinly",
                                "target_lang_sense_clarification": "regarding a smile"
                            },
                            {
                                "target_lang_word": "coolly",
                                "target_lang_sense_clarification": "lacking warmth"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "349bed20-60bc-36d1-a2f1-a144b02ed69c",
                    "sense_definition": "Met ingehouden emotie; op een nuchtere of droge toon.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij merkte <w>zuinigjes</w> op dat de schade gelukkig meeviel.",
                            "target_lang_translations": {
                                "ru": "Он <w>сухо</w> заметил, что ущерб, к счастью, оказался невелик.",
                                "en": "He remarked <w>dryly</w> that the damage was fortunately not too bad."
                            }
                        },
                        {
                            "text": "De directeur keek <w>zuinigjes</w> toen hij de hoge rekening zag.",
                            "target_lang_translations": {
                                "ru": "Директор посмотрел <w>неодобрительно</w>, когда увидел высокий счет.",
                                "en": "The director looked <w>disapprovingly</w> when he saw the high bill."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zuinigjes opmerken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verkleinwoord"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "С сдерживаемыми эмоциями; трезвым или сухим тоном.",
                        "en": "With restrained emotion; in a sober or dry tone."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сухо",
                                "target_lang_sense_clarification": "о тоне речи"
                            },
                            {
                                "target_lang_word": "неодобрительно",
                                "target_lang_sense_clarification": "о взгляде или реакции"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "dryly",
                                "target_lang_sense_clarification": "regarding tone of voice"
                            },
                            {
                                "target_lang_word": "disapprovingly",
                                "target_lang_sense_clarification": "regarding a look or facial expression"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "49097508-6078-3ba0-899e-f99ec07ee1be",
                    "sense_definition": "Powietrze wciągane do płuc i z nich wydychane; proces oddychania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "air_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten niesamowity widok zapierał <w>dech</w> w piersiach wszystkim turystom.",
                            "target_lang_translations": {
                                "ru": "Этот невероятный вид захватывал <w>дух</w> у всех туристов."
                            }
                        },
                        {
                            "text": "Biegł tak szybko, że po chwili zabrakło mu <w>tchu</w>.",
                            "target_lang_translations": {
                                "ru": "Он бежал так быстро, что через мгновение ему не хватило <w>дыхания</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddech",
                        "tchnienie"
                    ],
                    "common_phrases": [
                        "zapierać dech",
                        "brak tchu",
                        "złapać dech",
                        "do utraty tchu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздух, вдыхаемый в легкие и выдыхаемый из них; процесс дыхания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дыхание",
                                "target_lang_sense_clarification": "физиологический процесс"
                            },
                            {
                                "target_lang_word": "дух",
                                "target_lang_sense_clarification": "в устойчивых выражениях, напр. захватывать дух"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b789a09a-bba0-3b39-9a6b-7a61173ab832",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>decha</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starym płocie brakowało kilku <w>dech</w>, przez co pies uciekł.",
                            "target_lang_translations": {
                                "ru": "В старом заборе не хватало нескольких <w>досок</w>, из-за чего собака убежала."
                            }
                        },
                        {
                            "text": "Użyliśmy grubych <w>dech</w> do wzmocnienia konstrukcji podłogi.",
                            "target_lang_translations": {
                                "ru": "Мы использовали толстые <w>доски</w> для укрепления конструкции пола."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural of decha"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного «decha» (доска)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доска",

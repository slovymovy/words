{
    "word_family": [
        "dragoński"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c0c58525-d8b5-3385-9583-39de9cb1f77c",
                    "sense_definition": "Żołnierz formacji lekkiej jazdy, walczący zarówno pieszo, jak i konno, uzbrojony w broń palną.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych czasach każdy <dragon> musiał być doskonałym strzelcem.",
                            "target_lang_translations": {
                                "ru": "В былые времена каждый <драгун> должен был быть отличным стрелком."
                            }
                        },
                        {
                            "text": "Podczas bitwy cały pułk <dragonów> ruszył do gwałtownego ataku.",
                            "target_lang_translations": {
                                "ru": "Во время битвы весь полк <драгун> ринулся в яростную атаку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pułk dragonów",
                        "szarża dragonów"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "termin historyczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Солдат легкой кавалерии, сражающийся как в пешем, так и в конном строю, вооруженный огнестрельным оружием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "драгун",
                                "target_lang_sense_clarification": "военный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2563b69-342d-308c-b21b-5db2d67f8372",
                    "sense_definition": "Określenie potężnej, energicznej kobiety o władczym usposobieniu, która lubi rozkazywać innym.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego teściowa to istny <dragon>, wszyscy w rodzinie się jej boją.",
                            "target_lang_translations": {
                                "ru": "Его теща — настоящая <гром-баба>, все в семье её боятся."
                            }
                        },
                        {
                            "text": "Nie bądź takim <dragonem> i pozwól innym dojść do słowa.",
                            "target_lang_translations": {
                                "ru": "Не будь такой <командиршей> и дай другим слово сказать."
                            }
                        }
                    ],
                    "synonyms": [
                        "herod-baba"
                    ],
                    "common_phrases": [
                        "istny dragon"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "przenośnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Властная, энергичная женщина, которая любит командовать другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гром-баба",
                                "target_lang_sense_clarification": "разг. о крупной, бойкой женщине"
                            },
                            {
                                "target_lang_word": "бой-баба",
                                "target_lang_sense_clarification": "разг. о боевой, энергичной женщине"
                            },
                            {
                                "target_lang_word": "мегера",
                                "target_lang_sense_clarification": "перен. о злой, сварливой женщине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "306df971-ea5b-3f8b-8cfa-204dc6865631",
                    "sense_definition": "Luźna, szeroka patka lub pasek materiału naszyty na tylnej części płaszcza, często w talii.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten zimowy płaszcz ma z tyłu ozdobny <dragon>.",
                            "target_lang_translations": {
                                "ru": "У этого зимнего пальто сзади есть декоративный <хлястик>."
                            }
                        },
                        {
                            "text": "Krawiec musiał przeszyć <dragon>, ponieważ był on zbyt luźny.",
                            "target_lang_translations": {
                                "ru": "Портной должен был перешить <хлястик>, так как он был слишком свободным."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полоска ткани или ремешок, пришитый сзади на пальто, плаще или куртке, обычно на линии талии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хлястик",
                                "target_lang_sense_clarification": "деталь одежды"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "dc119bd2-648f-3d96-8290-7e571ba294ce",
                    "sense_definition": "Nazwisko męskie lub żeńskie używane w Polsce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Dragon> jest znanym specjalistą w naszej firmie.",
                            "target_lang_translations": {
                                "ru": "Господин <Драгон> — известный специалист в нашей фирме."
                            }
                        },
                        {
                            "text": "Wczoraj spotkałem panią <Dragon> na spacerze w parku.",
                            "target_lang_translations": {
                                "ru": "Вчера я встретил госпожу <Драгон> на прогулке в парке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужская или женская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Драгон",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "expectation",
        "expectant",
        "expectancy",
        "unexpected"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "de23f440-6cf8-354c-bbf5-ca0cdef21ada",
                    "sense_definition": "To think or believe that something will happen or that someone will arrive.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "prediction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>expect</w> that it will rain later this afternoon.",
                            "target_lang_translations": {
                                "ru": "Я <w>ожидаю</w>, что сегодня днём пойдёт дождь."
                            }
                        },
                        {
                            "text": "We <w>expect</w> a reply from the company by tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Мы <w>ожидаем</w> ответа от компании к завтрашнему утру."
                            }
                        },
                        {
                            "text": "She did not <w>expect</w> to see her friend at the library.",
                            "target_lang_translations": {
                                "ru": "Она не <w>ожидала</w> увидеть свою подругу в библиотеке."
                            }
                        }
                    ],
                    "synonyms": [
                        "anticipate"
                    ],
                    "antonyms": [
                        "doubt"
                    ],
                    "common_phrases": [
                        "expect the worst",
                        "expect a miracle",
                        "expect to find"
                    ],
                    "target_lang_definitions": {
                        "ru": "Думать или верить, что что-то произойдет или кто-то придет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ожидать",
                                "target_lang_sense_clarification": "предвидеть событие"
                            },
                            {
                                "target_lang_word": "полагать",
                                "target_lang_sense_clarification": "считать вероятным"
                            },
                            {
                                "target_lang_word": "рассчитывать",
                                "target_lang_sense_clarification": "надеяться на что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4f0f6af-bc13-3532-9114-3497d64b95cb",
                    "sense_definition": "To require or demand something because it is a duty, obligation, or standard.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school <w>expects</w> all students to wear a uniform.",
                            "target_lang_translations": {
                                "ru": "Школа <w>ожидает</w>, что все ученики будут носить форму."
                            }
                        },
                        {
                            "text": "You are <w>expected</w> to arrive at the office on time every day.",
                            "target_lang_translations": {
                                "ru": "От вас <w>ожидается</w> прибытие в офис вовремя каждый день."
                            }
                        },
                        {
                            "text": "My parents <w>expect</w> too much of me when it comes to grades.",
                            "target_lang_translations": {
                                "ru": "Мои родители <w>ждут</w> от меня слишком многого, когда дело касается оценок."
                            }
                        }
                    ],
                    "synonyms": [
                        "demand",
                        "call for"
                    ],
                    "common_phrases": [
                        "expect too much",
                        "be expected to",
                        "what to expect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требовать или рассчитывать на что-то в силу долга или обязательств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ожидать",
                                "target_lang_sense_clarification": "требовать выполнения"
                            },
                            {
                                "target_lang_word": "ждать",
                                "target_lang_sense_clarification": "рассчитывать на кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "588ca3e2-d436-32c3-bdf1-110ca5d48a77",
                    "sense_definition": "To be pregnant; to wait for the birth of a child.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pregnancy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My sister is <w>expecting</w> her first child in June.",
                            "target_lang_translations": {
                                "ru": "Моя сестра <w>ждет</w> своего первого ребенка в июне."
                            }
                        },
                        {
                            "text": "They are <w>expecting</w> twins and need to buy a larger car.",
                            "target_lang_translations": {
                                "ru": "Они <w>ждут</w> двойню, и им нужно купить машину побольше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "expecting a baby",
                        "she is expecting",
                        "expecting a child"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть беременной; ждать рождения ребёнка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ждать ребёнка",
                                "target_lang_sense_clarification": "быть беременной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14d447e1-fdfa-3ba1-b4fe-0a9d7bff268d",
                    "sense_definition": "To wait for or await someone or something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "waiting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We shall remain here and <w>expect</w> his coming.",
                            "target_lang_translations": {
                                "ru": "Мы останемся здесь и будем <w>ожидать</w> его прихода."
                            }
                        },
                        {
                            "text": "The knight knelt and <w>expected</w> the king's command.",
                            "target_lang_translations": {
                                "ru": "Рыцарь преклонил колени и <w>ожидал</w> приказа короля."
                            }
                        }
                    ],
                    "synonyms": [
                        "await",
                        "wait for"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This sense is largely obsolete in modern English, found mostly in older literature."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ждать или поджидать кого-либо или что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ожидать",
                                "target_lang_sense_clarification": "ждать прибытия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
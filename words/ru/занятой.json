    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8769ec37-afbc-3a78-9f52-ddd998712b95",
                    "sense_definition": "Не имеющий свободного времени, обременённый делами или работой (о человеке); наполненный делами (о времени).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой отец очень <w>занятой</w> человек, он работает даже в выходные.",
                            "target_lang_translations": {
                                "en": "My father is a very <w>busy</w> man, he works even on weekends."
                            }
                        },
                        {
                            "text": "У нас был очень <w>занятой</w> день, мы многое успели сделать.",
                            "target_lang_translations": {
                                "en": "We had a very <w>busy</w> day, we managed to get a lot done."
                            }
                        }
                    ],
                    "synonyms": [
                        "загруженный"
                    ],
                    "antonyms": [
                        "свободный"
                    ],
                    "common_phrases": [
                        "занятой человек",
                        "занятой день",
                        "занятой вид",
                        "занятой график"
                    ],
                    "target_lang_definitions": {
                        "en": "Having no free time, burdened with tasks or work (referring to a person); filled with activities (referring to time)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "busy",
                                "target_lang_sense_clarification": "having a lot to do; full of activity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aeaabb48-8f76-3f90-af21-8740288f9495",
                    "sense_definition": "Форма родительного, дательного, творительного и предложного падежа женского рода единственного числа прилагательного <w>занятый</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы говорили о <w>занятой</w> им должности.",
                            "target_lang_translations": {
                                "en": "We spoke about the position <w>held</w> by him."
                            }
                        },
                        {
                            "text": "Он прошёл мимо <w>занятой</w> скамейки.",
                            "target_lang_translations": {
                                "en": "He walked past the <w>occupied</w> bench."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Inflected form (feminine singular oblique cases) of the adjective/participle 'занятый' (occupied)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "occupied",
                                "target_lang_sense_clarification": "taken up or filled by someone or something"
                            },
                            {
                                "target_lang_word": "taken",
                                "target_lang_sense_clarification": "not available; in use"
                            },
                            {
                                "target_lang_word": "held",
                                "target_lang_sense_clarification": "possessed or occupied (e.g., a position)"
                            }
                        ]
                    }
                }
            ]
        }

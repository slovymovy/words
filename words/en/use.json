{
    "word_family": [
        "user",
        "useful",
        "useless",
        "usage",
        "usable",
        "misuse",
        "abuse",
        "reuse",
        "overuse",
        "unused",
        "usability"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "84dc7c36-79e0-3b47-8740-e8aa86ba6ebf",
                    "sense_definition": "To employ or apply something for a specific purpose or function.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "utilize_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can I <w>use</w> your pen for a moment?",
                            "target_lang_translations": {
                                "ru": "Могу я <w>воспо́льзоваться</w> твоей ручкой на минутку?"
                            }
                        },
                        {
                            "text": "We <w>use</w> this knife to slice the bread.",
                            "target_lang_translations": {
                                "ru": "Мы <w>испо́льзуем</w> этот нож, чтобы резать хлеб."
                            }
                        }
                    ],
                    "synonyms": [
                        "employ",
                        "utilize",
                        "apply"
                    ],
                    "common_phrases": [
                        "use a tool",
                        "use a method",
                        "easy to use"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать или применять что-либо для определенной цели или функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испо́льзовать",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "по́льзоваться",
                                "target_lang_sense_clarification": "использовать инструмент или вещь"
                            },
                            {
                                "target_lang_word": "применя́ть",
                                "target_lang_sense_clarification": "использовать метод или знание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af6fa34c-ef06-3f44-ac27-34430fe54f87",
                    "sense_definition": "To have done something regularly in the past but not anymore (found in the form 'used to').",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "habitual_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>used</w> to play football every weekend when I was younger.",
                            "target_lang_translations": {
                                "ru": "Я <w>ра́ньше</w> играл в футбол каждые выходные, когда был моложе."
                            }
                        },
                        {
                            "text": "She didn't <w>use</w> to like spicy food, but now she loves it.",
                            "target_lang_translations": {
                                "ru": "Она <w>ра́ньше</w> не любила острую еду, но теперь обожает её."
                            }
                        }
                    ],
                    "common_phrases": [
                        "used to be",
                        "used to do",
                        "didn't use to"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "followed by infinitive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо регулярно в прошлом, но не сейчас (встречается в форме 'used to')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ра́ньше",
                                "target_lang_sense_clarification": "указание на действие в прошлом"
                            },
                            {
                                "target_lang_word": "име́ть обыкнове́ние",
                                "target_lang_sense_clarification": "формальное выражение привычки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b4920d3-5c72-3f02-af7a-592b36f4563f",
                    "sense_definition": "To be or become familiar with something through experience (found in 'be used to' or 'get used to').",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "accustom_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>used</w> to waking up early for work.",
                            "target_lang_translations": {
                                "ru": "Я <w>привы́к</w> просыпаться рано на работу."
                            }
                        },
                        {
                            "text": "It took him a while to get <w>used</w> to the cold weather.",
                            "target_lang_translations": {
                                "ru": "Ему потребовалось время, чтобы <w>привы́кнуть</w> к холодной погоде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get used to",
                        "be used to"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "followed by noun or gerund"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть знакомым с чем-либо благодаря опыту или привыкнуть к чему-либо (встречается в 'be used to' или 'get used to')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привыка́ть",
                                "target_lang_sense_clarification": "процесс адаптации"
                            },
                            {
                                "target_lang_word": "быть привы́чным",
                                "target_lang_sense_clarification": "состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf4c3f96-d4a5-3c2f-9863-8cd769160ea1",
                    "sense_definition": "To consume or expend a resource until it is finished.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "utilize_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We have <w>used</w> up all the milk, so we need to buy more.",
                            "target_lang_translations": {
                                "ru": "Мы <w>израсхо́довали</w> всё молоко, поэтому нам нужно купить ещё."
                            }
                        },
                        {
                            "text": "She <w>used</w> all her savings to pay for the car.",
                            "target_lang_translations": {
                                "ru": "Она <w>потра́тила</w> все свои сбережения, чтобы заплатить за машину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "use up",
                        "use energy",
                        "use resources"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потреблять или расходовать ресурс до его окончания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "израсхо́довать",
                                "target_lang_sense_clarification": "потратить полностью"
                            },
                            {
                                "target_lang_word": "потра́тить",
                                "target_lang_sense_clarification": "расходовать деньги или время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acba47cc-cb23-3b4b-85b2-40f57deb681a",
                    "sense_definition": "To treat someone unfairly for one's own advantage.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "exploit_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt that his boss just <w>used</w> him to get the project done.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, что начальник просто <w>испо́льзовал</w> его, чтобы закончить проект."
                            }
                        },
                        {
                            "text": "Don't let people <w>use</w> you for their own gain.",
                            "target_lang_translations": {
                                "ru": "Не позволяй людям <w>испо́льзовать</w> тебя ради собственной выгоды."
                            }
                        }
                    ],
                    "synonyms": [
                        "exploit",
                        "take advantage of"
                    ],
                    "common_phrases": [
                        "feel used",
                        "use people"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относиться к кому-либо несправедливо ради собственной выгоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испо́льзовать",
                                "target_lang_sense_clarification": "эксплуатировать человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0f5efda-e6c5-31cc-9e58-8bbde1f1b066",
                    "sense_definition": "To need or want something that would be beneficial (usually in the phrase 'could use').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "need_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I could really <w>use</w> a cup of coffee right now.",
                            "target_lang_translations": {
                                "ru": "Мне бы сейчас очень <w>не помеша́ла</w> чашка кофе."
                            }
                        },
                        {
                            "text": "This room could <w>use</w> a fresh coat of paint.",
                            "target_lang_translations": {
                                "ru": "Этой комнате <w>не помеша́л бы</w> свежий слой краски."
                            }
                        }
                    ],
                    "common_phrases": [
                        "could use help",
                        "could use a break"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "idiomatic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нуждаться или хотеть чего-либо полезного (обычно во фразе 'could use')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не помеша́ло бы",
                                "target_lang_sense_clarification": "идиоматическое выражение нужды"
                            },
                            {
                                "target_lang_word": "пригоди́ться",
                                "target_lang_sense_clarification": "быть полезным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8703db52-c0af-36d8-9647-c2312b01fc1b",
                    "sense_definition": "To consume alcohol or drugs, especially regularly.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consume_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He admitted that he <w>uses</w> cocaine occasionally.",
                            "target_lang_translations": {
                                "ru": "Он признался, что иногда <w>употребля́ет</w> кокаин."
                            }
                        },
                        {
                            "text": "The doctor asked if she <w>used</w> any illegal substances.",
                            "target_lang_translations": {
                                "ru": "Врач спросил, <w>употребля́ла</w> ли она какие-либо запрещённые вещества."
                            }
                        }
                    ],
                    "common_phrases": [
                        "use drugs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Употреблять алкоголь или наркотики, особенно регулярно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "употребля́ть",
                                "target_lang_sense_clarification": "принимать вещества"
                            },
                            {
                                "target_lang_word": "принима́ть",
                                "target_lang_sense_clarification": "использовать лекарства или наркотики"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da8aa449-d56a-3fe0-85bd-95aa6a88c6ec",
                    "sense_definition": "To suggest or request that others employ specific gender pronouns when referring to oneself.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "identity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My colleague <w>uses</w> they/them pronouns.",
                            "target_lang_translations": {
                                "ru": "Мой коллега <w>испо́льзует</w> местоимения они/их."
                            }
                        },
                        {
                            "text": "Please ask people which pronouns they <w>use</w> before assuming.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, спросите людей, какие местоимения они <w>испо́льзуют</w>, прежде чем делать предположения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "use pronouns"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлагать или просить использовать определенные местоимения при обращении к себе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испо́льзовать",
                                "target_lang_sense_clarification": "применять слова"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e573f8e2-1dac-3e69-b6bd-f37b993417aa",
                    "sense_definition": "The act of employing or applying something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "act_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>use</w> of mobile phones is not allowed in the library.",
                            "target_lang_translations": {
                                "ru": "<w>Испо́льзование</w> мобильных телефонов в библиотеке запрещено."
                            }
                        },
                        {
                            "text": "Excessive <w>use</w> of water can lead to shortages.",
                            "target_lang_translations": {
                                "ru": "Чрезмерное <w>употребле́ние</w> воды может привести к дефициту."
                            }
                        }
                    ],
                    "synonyms": [
                        "usage",
                        "employment"
                    ],
                    "common_phrases": [
                        "make use of",
                        "in use",
                        "put to use"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по использованию или применению чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испо́льзование",
                                "target_lang_sense_clarification": "процесс применения"
                            },
                            {
                                "target_lang_word": "употребле́ние",
                                "target_lang_sense_clarification": "использование в речи или потреблении"
                            },
                            {
                                "target_lang_word": "примене́ние",
                                "target_lang_sense_clarification": "практическое использование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ed637b7-bf70-3323-8d79-c1b4eea26ab4",
                    "sense_definition": "A function or purpose for which something is employed.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "purpose_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This kitchen tool has many different <w>uses</w>.",
                            "target_lang_translations": {
                                "ru": "У этого кухонного инструмента много различных <w>примене́ний</w>."
                            }
                        },
                        {
                            "text": "I cannot find a <w>use</w> for this old cardboard box.",
                            "target_lang_translations": {
                                "ru": "Я не могу найти <w>примене́ние</w> этой старой картонной коробке."
                            }
                        }
                    ],
                    "synonyms": [
                        "usage",
                        "utility"
                    ],
                    "common_phrases": [
                        "have many uses",
                        "find a use"
                    ],
                    "target_lang_definitions": {
                        "ru": "Функция или цель, для которой что-либо используется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "примене́ние",
                                "target_lang_sense_clarification": "сфера использования"
                            },
                            {
                                "target_lang_word": "назначе́ние",
                                "target_lang_sense_clarification": "цель предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8322a83-a651-3ee4-8393-4dad5625b240",
                    "sense_definition": "The quality of being helpful or beneficial; the point or value of doing something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "value_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is no <w>use</w> arguing with him because he won't listen.",
                            "target_lang_translations": {
                                "ru": "Нет <w>смы́сла</w> спорить с ним, потому что он не будет слушать."
                            }
                        },
                        {
                            "text": "What is the <w>use</w> of buying a car if you can't drive?",
                            "target_lang_translations": {
                                "ru": "Какой <w>толк</w> покупать машину, если ты не умеешь водить?"
                            }
                        }
                    ],
                    "synonyms": [
                        "benefit",
                        "good",
                        "point",
                        "utility",
                        "usefulness"
                    ],
                    "common_phrases": [
                        "no use",
                        "what's the use",
                        "of no use"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полезность или выгода; смысл или ценность действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по́льза",
                                "target_lang_sense_clarification": "благо, выгода"
                            },
                            {
                                "target_lang_word": "смысл",
                                "target_lang_sense_clarification": "целесообразность"
                            },
                            {
                                "target_lang_word": "толк",
                                "target_lang_sense_clarification": "разг. польза, результат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "496a789a-c2fe-38e5-9aa3-7c39d95804b2",
                    "sense_definition": "Occasion, need, or permission to employ something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "need_noun_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have no further <w>use</w> for these old textbooks.",
                            "target_lang_translations": {
                                "ru": "У меня больше нет <w>на́добности</w> в этих старых учебниках."
                            }
                        },
                        {
                            "text": "Do you have any <w>use</w> for a spare ticket to the concert?",
                            "target_lang_translations": {
                                "ru": "Найдется ли у тебя <w>примене́ние</w> лишнему билету на концерт?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "have use for",
                        "no use for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случай, необходимость или разрешение использовать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на́добность",
                                "target_lang_sense_clarification": "потребность"
                            },
                            {
                                "target_lang_word": "нужда́",
                                "target_lang_sense_clarification": "необходимость"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
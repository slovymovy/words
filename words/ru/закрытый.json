    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2a2c64b6-415a-31bb-8920-109549a8f0c0",
                    "sense_definition": "Не открытый; такой, у которого створки, крышка или другие части сомкнуты, преграждая доступ внутрь или наружу.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, не стучите в <w>закрытую</w> дверь.",
                            "target_lang_translations": {
                                "en": "Please do not knock on a <w>closed</w> door."
                            }
                        },
                        {
                            "text": "Он лежал на диване с <w>закрытыми</w> глазами и отдыхал.",
                            "target_lang_translations": {
                                "en": "He lay on the sofa with <w>closed</w> eyes and rested."
                            }
                        }
                    ],
                    "synonyms": [
                        "запертый",
                        "сомкнутый",
                        "сложенный"
                    ],
                    "antonyms": [
                        "открытый",
                        "раскрытый"
                    ],
                    "common_phrases": [
                        "закрытая дверь",
                        "закрытое окно",
                        "закрытые глаза",
                        "закрытый магазин"
                    ],
                    "target_lang_definitions": {
                        "en": "Not open; having doors, lids, or other parts joined, obstructing access in or out."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "closed",
                                "target_lang_sense_clarification": "physically shut or obstructed"
                            },
                            {
                                "target_lang_word": "shut",
                                "target_lang_sense_clarification": "fastened or closed tightly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ff998ae-21a8-3b1e-b51a-62305f3106fb",
                    "sense_definition": "Недоступный для свободного посещения; предназначенный только для узкого круга лиц.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это <w>закрытая</w> территория, и посторонним вход сюда воспрещён.",
                            "target_lang_translations": {
                                "en": "This is a <w>restricted</w> area, and entry is forbidden to unauthorized personnel."
                            }
                        },
                        {
                            "text": "Совет директоров провёл <w>закрытое</w> заседание по вопросам бюджета.",
                            "target_lang_translations": {
                                "en": "The board of directors held a <w>closed</w> meeting regarding budget issues."
                            }
                        }
                    ],
                    "synonyms": [
                        "недоступный"
                    ],
                    "antonyms": [
                        "публичный",
                        "открытый"
                    ],
                    "common_phrases": [
                        "закрытое заседание",
                        "закрытая вечеринка",
                        "закрытый клуб",
                        "закрытое учреждение"
                    ],
                    "target_lang_definitions": {
                        "en": "Not accessible to the general public; intended only for a select group of people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "closed",
                                "target_lang_sense_clarification": "restricted to a specific group"
                            },
                            {
                                "target_lang_word": "private",
                                "target_lang_sense_clarification": "not public; restricted access"
                            },
                            {
                                "target_lang_word": "restricted",
                                "target_lang_sense_clarification": "limited access"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d782337-fa86-3b65-a193-be6fe7deb0d7",
                    "sense_definition": "Секретный, не подлежащий разглашению или широкому распространению.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы используем <w>закрытый</w> канал связи для передачи важных данных.",
                            "target_lang_translations": {
                                "en": "We use a <w>secure</w> communication channel to transmit important data."
                            }
                        },
                        {
                            "text": "Эта папка содержит <w>закрытую</w> информацию о клиентах банка.",
                            "target_lang_translations": {
                                "en": "This folder contains <w>confidential</w> information about the bank's clients."
                            }
                        }
                    ],
                    "synonyms": [
                        "загрифованный"
                    ],
                    "antonyms": [
                        "открытый",
                        "публичный"
                    ],
                    "common_phrases": [
                        "закрытая информация",
                        "закрытая тема",
                        "закрытый процесс"
                    ],
                    "target_lang_definitions": {
                        "en": "Secret; not subject to disclosure or wide distribution."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "classified",
                                "target_lang_sense_clarification": "official secrecy"
                            },
                            {
                                "target_lang_word": "confidential",
                                "target_lang_sense_clarification": "private or sensitive information"
                            },
                            {
                                "target_lang_word": "secure",
                                "target_lang_sense_clarification": "protected from interception"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4e78da2-74ea-3ca5-996e-df4b82884981",
                    "sense_definition": "О платье или кофте: с высоким воротом, полностью закрывающий шею и грудь.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для посещения старинного храма туристы должны выбрать скромную <w>закрытую</w> одежду.",
                            "target_lang_translations": {
                                "en": "To visit the ancient temple, tourists must choose modest, <w>conservative</w> clothing."
                            }
                        },
                        {
                            "text": "Её строгое <w>закрытое</w> платье выглядело очень элегантно на приёме.",
                            "target_lang_translations": {
                                "en": "Her strict <w>high-necked</w> dress looked very elegant at the reception."
                            }
                        }
                    ],
                    "antonyms": [
                        "открытый"
                    ],
                    "common_phrases": [
                        "закрытое платье",
                        "закрытый купальник",
                        "закрытый ворот"
                    ],
                    "target_lang_definitions": {
                        "en": "Of a dress or blouse: having a high collar, fully covering the neck and chest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "high-necked",
                                "target_lang_sense_clarification": "covering the neck"
                            },
                            {
                                "target_lang_word": "modest",
                                "target_lang_sense_clarification": "covering the body conservatively"
                            },
                            {
                                "target_lang_word": "conservative",
                                "target_lang_sense_clarification": "not revealing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acba3f04-4b56-3e6f-988f-99f97861ff3a",
                    "sense_definition": "Непостижимый, недоступный для понимания или внешнего восприятия (обычно о человеке или внутреннем мире).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой новый сосед — очень <w>закрытый</w> человек, он почти ни с кем не общается.",
                            "target_lang_translations": {
                                "en": "My new neighbor is a very <w>private</w> person; he hardly communicates with anyone."
                            }
                        },
                        {
                            "text": "Для меня мир высшей математики так и остался абсолютно <w>закрытой</w> книгой.",
                            "target_lang_translations": {
                                "en": "For me, the world of higher mathematics remained an absolutely <w>closed</w> book."
                            }
                        }
                    ],
                    "synonyms": [
                        "непостижимый"
                    ],
                    "antonyms": [
                        "открытый"
                    ],
                    "common_phrases": [
                        "закрытый человек",
                        "закрытый характер",
                        "закрытый мир"
                    ],
                    "target_lang_definitions": {
                        "en": "Inscrutable; not open to understanding or external perception (usually regarding a person or inner world)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reserved",
                                "target_lang_sense_clarification": "slow to reveal emotion"
                            },
                            {
                                "target_lang_word": "private",
                                "target_lang_sense_clarification": "keeping personal matters secret"
                            },
                            {
                                "target_lang_word": "withdrawn",
                                "target_lang_sense_clarification": "socially detached"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7afe38be-50f4-31d5-aae8-c4da0234798b",
                    "sense_definition": "Страдательное причастие прошедшего времени от глагола <w>закрыть</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Магазин, <w>закрытый</w> на ремонт, откроется только через неделю.",
                            "target_lang_translations": {
                                "en": "The shop, <w>closed</w> for repairs, will open only in a week."
                            }
                        },
                        {
                            "text": "Все окна в доме были плотно <w>закрыты</w> деревянными ставнями.",
                            "target_lang_translations": {
                                "en": "All the windows in the house were tightly <w>closed</w> with wooden shutters."
                            }
                        }
                    ],
                    "antonyms": [
                        "открытый"
                    ],
                    "common_phrases": [

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4d24408e-e20e-3122-b10c-7f81e31daa58",
                    "sense_definition": "Lekki przedmiot w kształcie stożka, wykonany z piór lub plastiku, używany do gry w badmintona.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sport_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiatr zniósł <w>lotkę</w> na pole przeciwnika.",
                            "target_lang_translations": {
                                "ru": "Ветер снес <w>волан</w> на поле противника."
                            }
                        },
                        {
                            "text": "Musimy kupić nowe <w>lotki</w>, bo stare się zniszczyły.",
                            "target_lang_translations": {
                                "ru": "Нам нужно купить новые <w>воланы</w>, потому что старые испортились."
                            }
                        }
                    ],
                    "synonyms": [
                        "kometka"
                    ],
                    "common_phrases": [
                        "grać w lotkę",
                        "odbijać lotkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкий конусообразный предмет из перьев или пластика, используемый для игры в бадминтон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волан",
                                "target_lang_sense_clarification": "для бадминтона"
                            },
                            {
                                "target_lang_word": "воланчик",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f07f8f0-675d-3fb2-a44b-5b91586e54e2",
                    "sense_definition": "Gra towarzyska polegająca na rzucaniu małymi strzałkami do tarczy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sport_game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W piątkowe wieczory często gramy w <w>lotki</w> w pubie.",
                            "target_lang_translations": {
                                "ru": "По пятничным вечерам мы часто играем в <w>дартс</w> в пабе."
                            }
                        },
                        {
                            "text": "Mój brat jest mistrzem w <w>lotki</w> i zawsze wygrywa.",
                            "target_lang_translations": {
                                "ru": "Мой брат — мастер игры в <w>дартс</w> и всегда выигрывает."
                            }
                        }
                    ],
                    "synonyms": [
                        "dart"
                    ],
                    "common_phrases": [
                        "grać w lotki",
                        "turniej w lotki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игра, в которой участники метают небольшие дротики в круглую мишень."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дартс",
                                "target_lang_sense_clarification": "игра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14fb6edc-6c15-34ba-a15c-ec48848c45b1",
                    "sense_definition": "Mała strzałka z ostrym grotem, służąca do rzucania do tarczy w grze w rzutki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sport_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzucił <w>lotką</w> i trafił w sam środek tarczy.",
                            "target_lang_translations": {
                                "ru": "Он бросил <w>дротик</w> и попал в самый центр мишени."
                            }
                        },
                        {
                            "text": "Zgubiłem jedną <w>lotkę</w> z mojego ulubionego zestawu.",
                            "target_lang_translations": {
                                "ru": "Я потерял один <w>дротик</w> из моего любимого набора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzucać lotką",
                        "celna lotka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая стрела с острым наконечником для метания в мишень."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дротик",
                                "target_lang_sense_clarification": "для дартса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b400b87-72fe-3303-8522-1e935e058519",
                    "sense_definition": "Ruchoma część skrzydła samolotu, służąca do sterowania jego przechyleniem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "flight_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pilot użył <w>lotek</w>, aby wykonać gwałtowny skręt.",
                            "target_lang_translations": {
                                "ru": "Пилот использовал <w>элероны</w>, чтобы выполнить резкий поворот."
                            }
                        },
                        {
                            "text": "Awaria prawej <w>lotki</w> utrudniła bezpieczne lądowanie.",
                            "target_lang_translations": {
                                "ru": "Поломка правого <w>элерона</w> затруднила безопасную посадку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wychylenie lotki",
                        "sterowanie lotkami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижная часть крыла самолета, предназначенная для управления креном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "элерон",
                                "target_lang_sense_clarification": "часть крыла самолета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffb45c13-dba3-31dc-b0e5-be7a9d620963",
                    "sense_definition": "Duże, sztywne pióro w skrzydle ptaka, niezbędne do latania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "flight_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znaleźliśmy w lesie piękną, czarną <w>lotkę</w> kruka.",
                            "target_lang_translations": {
                                "ru": "Мы нашли в лесу красивое черное <w>маховое перо</w> ворона."
                            }
                        },
                        {
                            "text": "Ptak czyścił swoje <w>lotki</w> siedząc na gałęzi.",
                            "target_lang_translations": {
                                "ru": "Птица чистила свои <w>маховые перья</w>, сидя на ветке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lotki pierwszego rzędu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное, жесткое перо в крыле птицы, необходимое для полета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маховое перо",
                                "target_lang_sense_clarification": "птицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d21a5779-249a-37a0-ada9-dece2676bb2c",
                    "sense_definition": "Pióra lub elementy z tworzywa na końcu strzały, stabilizujące jej lot.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "flight_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Strzała miała jaskrawe <w>lotki</w>, co ułatwiało jej odnalezienie.",
                            "target_lang_translations": {
                                "ru": "У стрелы было яркое <w>оперение</w>, что облегчало ее поиск."
                            }
                        },
                        {
                            "text": "Łucznik samodzielnie przyklejał <w>lotki</w> do promieni strzał.",
                            "target_lang_translations": {
                                "ru": "Лучник самостоятельно приклеивал <w>оперение</w> к древкам стрел."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перья или пластиковые элементы на конце стрелы, стабилизирующие её полет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оперение",
                                "target_lang_sense_clarification": "стрелы"
                            },
                            {
                                "target_lang_word": "стабилизатор",
                                "target_lang_sense_clarification": "стрелы"
                            }

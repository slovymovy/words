    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e74a3fc1-4a81-3a9a-811d-a02e2311814f",
                    "sense_definition": "Погружаться в размышления, начинать думать о ком-либо или чём-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы часто <задумываемся> о смысле жизни, когда смотрим на звёзды.",
                            "target_lang_translations": {
                                "en": "We often <think> about the meaning of life when we look at the stars."
                            }
                        },
                        {
                            "text": "Тебе стоит серьёзно <задуматься> над своим поведением в школе.",
                            "target_lang_translations": {
                                "en": "You should seriously <reflect on> your behavior at school."
                            }
                        }
                    ],
                    "synonyms": [
                        "размышлять",
                        "помышлять"
                    ],
                    "common_phrases": [
                        "задумываться о будущем",
                        "задумываться над вопросом",
                        "всерьёз задумываться"
                    ],
                    "target_lang_definitions": {
                        "en": "To start thinking deeply about something or someone; to become lost in thought."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "think about",
                                "target_lang_sense_clarification": "general reflection"
                            },
                            {
                                "target_lang_word": "ponder",
                                "target_lang_sense_clarification": "deep thought"
                            },
                            {
                                "target_lang_word": "reflect on",
                                "target_lang_sense_clarification": "careful consideration"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ead07ad-9661-3cf8-a8b7-842f643cdd79",
                    "sense_definition": "Проявлять нерешительность, колебаться перед совершением действия.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он бросился на помощь другу, не <задумываясь> ни на секунду.",
                            "target_lang_translations": {
                                "en": "He rushed to help his friend without <hesitating> for a second."
                            }
                        },
                        {
                            "text": "Я ответил на сложный вопрос сразу, даже не <задумываясь>.",
                            "target_lang_translations": {
                                "en": "I answered the difficult question immediately, without even <thinking twice>."
                            }
                        }
                    ],
                    "synonyms": [
                        "колебаться",
                        "сомневаться",
                        "медлить"
                    ],
                    "antonyms": [
                        "решаться"
                    ],
                    "common_phrases": [
                        "не задумываясь",
                        "долго не задумываться"
                    ],
                    "target_lang_definitions": {
                        "en": "To hesitate or pause to consider before taking action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hesitate",
                                "target_lang_sense_clarification": "pause before acting"
                            },
                            {
                                "target_lang_word": "think twice",
                                "target_lang_sense_clarification": "reconsider"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27342a07-defa-3a91-b6e9-292dcb06a657",
                    "sense_definition": "Страдательный залог к глаголу <w>задумывать</w>; быть планируемым или замышляемым.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это грандиозное здание изначально <задумывалось> как музей науки.",
                            "target_lang_translations": {
                                "en": "This grand building was originally <conceived> as a science museum."
                            }
                        },
                        {
                            "text": "Всё пошло совсем не так, как <задумывалось> архитекторами проекта.",
                            "target_lang_translations": {
                                "en": "Everything went completely differently than was <intended> by the project architects."
                            }
                        }
                    ],
                    "synonyms": [
                        "планироваться"
                    ],
                    "common_phrases": [
                        "как задумывалось"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be planned, designed, or intended (passive form)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be intended",
                                "target_lang_sense_clarification": "purpose"
                            },
                            {
                                "target_lang_word": "be conceived",
                                "target_lang_sense_clarification": "creation/idea"
                            },
                            {
                                "target_lang_word": "be planned",
                                "target_lang_sense_clarification": "arrangement"
                            }
                        ]
                    }
                }
            ]
        }
    ]

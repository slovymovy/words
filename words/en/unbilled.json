        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6d3381c0-435e-3fd4-bad7-a9d90500635e",
                    "sense_definition": "Not yet charged to a customer or client; not yet included in a bill.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "financial_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accountant discovered a significant amount of <w>unbilled</w> revenue at the end of the quarter.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер обнаружил значительную сумму <w>невыставленной</w> выручки в конце квартала."
                            }
                        },
                        {
                            "text": "Please review the project logs to ensure there are no <w>unbilled</w> hours remaining.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте журналы проекта, чтобы убедиться, что не осталось <w>невыставленных</w> часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "uninvoiced",
                        "uncharged"
                    ],
                    "antonyms": [
                        "billed",
                        "invoiced",
                        "charged"
                    ],
                    "common_phrases": [
                        "unbilled revenue",
                        "unbilled hours",
                        "unbilled expenses",
                        "unbilled receivables"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ещё не выставленный к оплате клиенту или не включённый в счёт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невыставленный",
                                "target_lang_sense_clarification": "о счёте или сумме"
                            },
                            {
                                "target_lang_word": "не включённый в счёт",
                                "target_lang_sense_clarification": "об услугах или товарах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03a5f5d3-7714-3ebf-8d3c-c8afb7e7107b",
                    "sense_definition": "Not mentioned or credited in an official list of performers, cast members, or event participants.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "entertainment_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The famous actor made a brief, <w>unbilled</w> appearance in the comedy film.",
                            "target_lang_translations": {
                                "ru": "Известный актёр совершил короткое появление в комедии, <w>не указанное в титрах</w>."
                            }
                        },
                        {
                            "text": "Fans were delighted when an <w>unbilled</w> band played a surprise set at the festival.",
                            "target_lang_translations": {
                                "ru": "Фанаты были в восторге, когда <w>не заявленная</w> группа сыграла неожиданный сет на фестивале."
                            }
                        }
                    ],
                    "synonyms": [
                        "uncredited",
                        "unlisted",
                        "unscheduled"
                    ],
                    "antonyms": [
                        "billed",
                        "credited",
                        "listed"
                    ],
                    "common_phrases": [
                        "unbilled appearance",
                        "unbilled cameo",
                        "unbilled role"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не упомянутый в официальном списке исполнителей, в титрах или афише."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не указанный в титрах",

            "senses": [
                {
                    "sense_id": "faf0bda7-62a4-3ebd-b123-d3ad504e7608",
                    "sense_definition": "zdrobnienie od: <w>ptak</w>; małe zwierzę latające pokryte piórami lub pisklę",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "bird_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wróbel to pospolity <w>ptaszek</w> o szaro-brązowym upierzeniu.",
                            "target_lang_translations": {
                                "ru": "Воробей — это обычная <w>птичка</w> с серо-коричневым оперением."
                            }
                        },
                        {
                            "text": "Na gałęzi siedział mały <w>ptaszek</w> i pięknie śpiewał.",
                            "target_lang_translations": {
                                "ru": "На ветке сидела маленькая <w>птичка</w> и красиво пела."
                            }
                        }
                    ],
                    "synonyms": [
                        "ptaszyna"
                    ],
                    "common_phrases": [
                        "śpiew ptaszka",
                        "mały ptaszek",
                        "ćwierkający ptaszek"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "уменьшительная форма от: <w>птица</w>; маленькое летающее животное, покрытое перьями, или птенец"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "птичка",

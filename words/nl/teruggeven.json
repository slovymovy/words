{
    "word_family": [
        "teruggave"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c5666cd8-27e1-3726-be8e-1cbee168026c",
                    "sense_definition": "Iets weer aan de eigenaar of gever overhandigen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "return_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal je het geleende boek morgen <w>teruggeven</w>.",
                            "target_lang_translations": {
                                "en": "I will <w>give</w> the borrowed book <w>back</w> to you tomorrow.",
                                "ru": "Я завтра <w>верну</w> тебе одолженную книгу."
                            }
                        },
                        {
                            "text": "Hij <w>gaf</w> de gevonden portemonnee eerlijk <w>terug</w> aan de eigenaar.",
                            "target_lang_translations": {
                                "en": "He honestly <w>returned</w> the found wallet to the owner.",
                                "ru": "Он честно <w>вернул</w> найденный кошелек владельцу."
                            }
                        }
                    ],
                    "synonyms": [
                        "retourneren",
                        "terugbezorgen"
                    ],
                    "antonyms": [
                        "houden",
                        "behouden"
                    ],
                    "common_phrases": [
                        "geld teruggeven",
                        "iets teruggeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To hand something back to the owner or giver.",
                        "ru": "Вернуть что-либо владельцу или дарителю."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give back",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "возвращать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "отдавать",
                                "target_lang_sense_clarification": "передавать обратно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73626f9c-61ca-38a9-a11e-720df003fbd5",
                    "sense_definition": "Het verschil uitbetalen tussen het betaalde bedrag en de prijs.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "return_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De caissière <w>gaf</w> mij twee euro te weinig <w>terug</w>.",
                            "target_lang_translations": {
                                "en": "The cashier <w>gave</w> me two euros too little <w>back</w>.",
                                "ru": "Кассир <w>дала</w> мне на два евро меньше <w>сдачи</w>."
                            }
                        },
                        {
                            "text": "Kunt u van vijftig euro <w>teruggeven</w>?",
                            "target_lang_translations": {
                                "en": "Can you <w>give change</w> for fifty euros?",
                                "ru": "Вы можете <w>дать сдачи</w> с пятидесяти евро?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "wisselgeld teruggeven",
                        "van een tientje teruggeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To pay the difference between the amount paid and the price.",
                        "ru": "Выплатить разницу между уплаченной суммой и ценой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give change",
                                "target_lang_sense_clarification": "money"
                            },
                            {
                                "target_lang_word": "give back",
                                "target_lang_sense_clarification": "change"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "давать сдачу",
                                "target_lang_sense_clarification": "о деньгах"
                            },
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57709945-0673-37b0-991b-0ed94acceb45",
                    "sense_definition": "Reageren op een handeling of uiting met een gelijke handeling of uiting.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "retaliate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen hij werd geduwd, <w>gaf</w> hij direct <w>terug</w>.",
                            "target_lang_translations": {
                                "en": "When he was pushed, he immediately <w>pushed back</w>.",
                                "ru": "Когда его толкнули, он сразу <w>дал сдачи</w>."
                            }
                        },
                        {
                            "text": "Zij wist niet wat ze moest <w>teruggeven</w> op die scherpe opmerking.",
                            "target_lang_translations": {
                                "en": "She didn't know what to <w>retort</w> to that sharp remark.",
                                "ru": "Она не знала, что <w>ответить</w> на это резкое замечание."
                            }
                        }
                    ],
                    "synonyms": [
                        "vergelden",
                        "pareren"
                    ],
                    "antonyms": [
                        "incasseren"
                    ],
                    "common_phrases": [
                        "een klap teruggeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To react to an action or utterance with a similar action or utterance.",
                        "ru": "Реагировать на действие или высказывание аналогичным действием или высказыванием."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hit back",
                                "target_lang_sense_clarification": "physical retaliation"
                            },
                            {
                                "target_lang_word": "retort",
                                "target_lang_sense_clarification": "verbal response"
                            },
                            {
                                "target_lang_word": "retaliate",
                                "target_lang_sense_clarification": "general"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "давать сдачи",
                                "target_lang_sense_clarification": "физический отпор"
                            },
                            {
                                "target_lang_word": "отвечать",
                                "target_lang_sense_clarification": "словесная реакция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5b0f531-ae1b-37e4-9411-462b6aacaa14",
                    "sense_definition": "Voedsel of drank uit de maag weer naar buiten brengen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "vomit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De baby heeft na de voeding een beetje melk <w>teruggegeven</w>.",
                            "target_lang_translations": {
                                "en": "The baby <w>spat up</w> a little milk after the feeding.",
                                "ru": "После кормления ребенок <w>срыгнул</w> немного молока."
                            }
                        },
                        {
                            "text": "Hij voelde zich zo ziek dat hij zijn lunch moest <w>teruggeven</w>.",
                            "target_lang_translations": {
                                "en": "He felt so sick that he had to <w>bring up</w> his lunch.",
                                "ru": "Ему было так плохо, что его <w>вырвало</w> обедом."
                            }
                        }
                    ],
                    "synonyms": [
                        "overgeven",
                        "braken"
                    ],
                    "antonyms": [
                        "binnenhouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring food or drink from the stomach back out.",
                        "ru": "Извергать пищу или питье из желудка наружу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spit up",
                                "target_lang_sense_clarification": "babies"
                            },
                            {
                                "target_lang_word": "bring up",
                                "target_lang_sense_clarification": "euphemism"
                            },
                            {
                                "target_lang_word": "vomit",
                                "target_lang_sense_clarification": "medical"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "срыгивать",
                                "target_lang_sense_clarification": "о младенцах"
                            },
                            {
                                "target_lang_word": "рвать",
                                "target_lang_sense_clarification": "тошнота"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
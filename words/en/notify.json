{
    "word_family": [
        "notification",
        "notifiable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e5fe6106-9be9-3a30-9ef7-79b7e2386bae",
                    "sense_definition": "To officially tell someone about an event or situation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>notify</w> the bank immediately if you lose your credit card.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, немедленно <w>уведомите</w> банк, если вы потеряете кредитную карту."
                            }
                        },
                        {
                            "text": "The school will <w>notify</w> parents by email if the trip is cancelled.",
                            "target_lang_translations": {
                                "ru": "Школа <w>уведомит</w> родителей по электронной почте, если поездка будет отменена."
                            }
                        }
                    ],
                    "synonyms": [
                        "inform",
                        "apprise"
                    ],
                    "common_phrases": [
                        "notify the police",
                        "notify in writing",
                        "notify immediately",
                        "notify the authorities"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально сообщить кому-либо о событии или ситуации"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уведомить",
                                "target_lang_sense_clarification": "официально известить"
                            },
                            {
                                "target_lang_word": "оповестить",
                                "target_lang_sense_clarification": "поставить в известность"
                            },
                            {
                                "target_lang_word": "сообщить",
                                "target_lang_sense_clarification": "передать информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "831f312f-f6d8-3547-909c-d700e3b714b5",
                    "sense_definition": "To formally report or make something known.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doctors are required to <w>notify</w> certain infectious diseases to the health department.",
                            "target_lang_translations": {
                                "ru": "Врачи обязаны <w>сообщать</w> об определенных инфекционных заболеваниях в департамент здравоохранения."
                            }
                        },
                        {
                            "text": "The final decision was <w>notified</w> to all parties involved in the dispute.",
                            "target_lang_translations": {
                                "ru": "Окончательное решение было <w>сообщено</w> всем сторонам, участвующим в споре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "notify a change",
                        "notify a death",
                        "notify a claim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формально доложить о чём-либо или довести до сведения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообщить",
                                "target_lang_sense_clarification": "довести до сведения"
                            },
                            {
                                "target_lang_word": "уведомить",
                                "target_lang_sense_clarification": "известить о факте"

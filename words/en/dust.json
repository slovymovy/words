{
    "word_family": [
        "dusty",
        "duster",
        "dustbin",
        "dustpan",
        "sawdust",
        "stardust",
        "dustman"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f6b21fc6-6498-3f64-a089-8e581e168b08",
                    "sense_definition": "Fine, dry particles of matter found in the air and covering the surface of objects, typically consisting of soil, pollen, or dirt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old books were covered in a thick layer of <w>dust</w>.",
                            "target_lang_translations": {
                                "ru": "Старые книги были покрыты толстым слоем <w>пыли</w>."
                            }
                        },
                        {
                            "text": "She wiped the <w>dust</w> off the table with a cloth.",
                            "target_lang_translations": {
                                "ru": "Она вытерла <w>пыль</w> со стола тряпкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "powder",
                        "dirt",
                        "grit",
                        "grime"
                    ],
                    "common_phrases": [
                        "gather dust",
                        "collect dust",
                        "dust mite",
                        "layer of dust",
                        "gold dust"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкие сухие частицы вещества в воздухе или на поверхности предметов, обычно состоящие из земли, пыльцы или грязи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пыль",
                                "target_lang_sense_clarification": "мелкие частицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb8ce096-2e37-3759-b46c-2538f491526e",
                    "sense_definition": "The physical remains of a human body after death, or the earth associated with the grave.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient bones had long since crumbled to <w>dust</w>.",
                            "target_lang_translations": {
                                "ru": "Древние кости давно рассыпались в <w>прах</w>."
                            }
                        },
                        {
                            "text": "We are born from <w>dust</w> and will return to it in the end.",
                            "target_lang_translations": {
                                "ru": "Мы рождены из <w>праха</w> и в конце концов обратимся в него же."
                            }
                        }
                    ],
                    "synonyms": [
                        "remains",
                        "ashes",
                        "earth"
                    ],
                    "common_phrases": [
                        "turn to dust",
                        "bite the dust",
                        "ashes to ashes dust to dust"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary or religious context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Останки человеческого тела после смерти или земля, ассоциируемая с могилой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прах",
                                "target_lang_sense_clarification": "останки, тлен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cfaaf61-0bad-3387-affc-19d7789a91c3",
                    "sense_definition": "The act of cleaning by removing dust from surfaces.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shelves just need a quick <w>dust</w> before the guests arrive.",
                            "target_lang_translations": {
                                "ru": "Полкам нужна лишь быстрая <w>протирка пыли</w> перед приходом гостей."
                            }
                        },
                        {
                            "text": "I gave the living room a <w>dust</w> this morning.",
                            "target_lang_translations": {
                                "ru": "Я <w>протер пыль</w> в гостиной сегодня утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "cleaning",
                        "dusting"
                    ],
                    "common_phrases": [
                        "give it a dust"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used with 'give'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс уборки путем удаления пыли с поверхностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протирка пыли",
                                "target_lang_sense_clarification": "процесс уборки"
                            },
                            {
                                "target_lang_word": "уборка",
                                "target_lang_sense_clarification": "чистка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c0f4f057-d10f-313d-ad54-6550510c6e55",
                    "sense_definition": "To clean dirt or fine particles from the surface of something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>dust</w> the furniture in the living room.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>вытереть пыль</w> с мебели в гостиной."
                            }
                        },
                        {
                            "text": "She <w>dusted</w> the old photographs carefully before putting them in the album.",
                            "target_lang_translations": {
                                "ru": "Она осторожно <w>протерла</w> старые фотографии, прежде чем поместить их в альбом."
                            }
                        }
                    ],
                    "synonyms": [
                        "clean",
                        "wipe"
                    ],
                    "antonyms": [
                        "soil",
                        "dirty"
                    ],
                    "common_phrases": [
                        "dust the furniture",
                        "dust off"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очищать поверхность чего-либо от грязи или мелких частиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытирать пыль",
                                "target_lang_sense_clarification": "удалять пыль"
                            },
                            {
                                "target_lang_word": "протирать",
                                "target_lang_sense_clarification": "чистить поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acfac45d-80f8-3502-9694-1219e75a2afc",
                    "sense_definition": "To cover or sprinkle a surface lightly with a fine powder.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "application",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baker <w>dusted</w> the chocolate cake with powdered sugar.",
                            "target_lang_translations": {
                                "ru": "Пекарь <w>посыпал</w> шоколадный торт сахарной пудрой."
                            }
                        },
                        {
                            "text": "Police officers <w>dusted</w> the door handle for fingerprints.",
                            "target_lang_translations": {
                                "ru": "Полицейские <w>обработали</w> дверную ручку порошком для снятия отпечатков пальцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprinkle",
                        "powder",
                        "dredge"
                    ],
                    "common_phrases": [
                        "dust with sugar",
                        "dust for fingerprints"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать или слегка посыпать поверхность мелким порошком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посыпать",
                                "target_lang_sense_clarification": "покрывать порошком"
                            },
                            {
                                "target_lang_word": "припудривать",
                                "target_lang_sense_clarification": "легко посыпать"
                            }
                        ]
                    }
                }
            ]
        },
        {

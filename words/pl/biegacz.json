    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bcf0dc7b-92fa-32f1-95df-f3cfa8b28ca4",
                    "sense_definition": "Osoba uprawiająca bieganie jako dyscyplinę sportową lub formę rekreacji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "athlete",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polski <w>biegacz</w> zdobył złoty medal na ostatnich mistrzostwach.",
                            "target_lang_translations": {
                                "ru": "Польский <w>бегун</w> завоевал золотую медаль на последнем чемпионате."
                            }
                        },
                        {
                            "text": "Każdy początkujący <w>biegacz</w> powinien zadbać o odpowiednie buty.",
                            "target_lang_translations": {
                                "ru": "Каждый начинающий <w>бегун</w> должен позаботиться о подходящей обуви."
                            }
                        }
                    ],
                    "common_phrases": [
                        "biegacz długodystansowy",
                        "biegacz narciarski",
                        "biegacz przełajowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, занимающийся бегом как видом спорта или формой отдыха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегун",
                                "target_lang_sense_clarification": "спортсмен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb28035e-b61b-3a83-be2a-bff48d812645",
                    "sense_definition": "Drapieżny chrząszcz z rodziny biegaczowatych, szybko poruszający się po ziemi.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lesie pod kamieniem schował się duży <w>biegacz</w>.",
                            "target_lang_translations": {
                                "ru": "В лесу под камнем спряталась большая <w>жужелица</w>."
                            }
                        },
                        {
                            "text": "Większość gatunków <w>biegaczy</w> poluje na inne owady nocą.",
                            "target_lang_translations": {
                                "ru": "Большинство видов <w>жужелиц</w> охотится на других насекомых по ночам."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хищный жук из семейства жужелиц, быстро передвигающийся по земле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жужелица",
                                "target_lang_sense_clarification": "насекомое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f7169c3-69f9-3d81-a804-a8210832272c",
                    "sense_definition": "Osoba zajmująca się przenoszeniem wiadomości lub przesyłek.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "messenger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Królewski <w>biegacz</w> dostarczył ważną wiadomość na zamek.",
                            "target_lang_translations": {
                                "ru": "Королевский <w>гонец</w> доставил важное сообщение в замок."
                            }
                        },
                        {
                            "text": "Zmęczony <w>biegacz</w> przekazał list dowódcy wojsk.",
                            "target_lang_translations": {
                                "ru": "Уставший <w>гонец</w> передал письмо командующему войсками."
                            }
                        }
                    ],
                    "synonyms": [
                        "goniec"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie rzadko używane w tym znaczeniu"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, доставляющий сообщения или посылки (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гонец",
                                "target_lang_sense_clarification": "посыльный"
                            },
                            {
                                "target_lang_word": "скороход",
                                "target_lang_sense_clarification": "быстрый гонец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78b15cd7-9ba0-3b32-8513-e0c33f9df531",
                    "sense_definition": "Roślina, która po wyschnięciu odrywa się od korzenia i jest toczona przez wiatr.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiatr toczył suchy <w>biegacz</w> przez piaszczystą pustynię.",
                            "target_lang_translations": {
                                "ru": "Ветер катил сухое <w>перекати-поле</w> по песчаной пустыне."
                            }
                        },
                        {
                            "text": "Na stepach można spotkać toczące się <w>biegacze</w>.",
                            "target_lang_translations": {
                                "ru": "В степях можно встретить катящееся <w>перекати-поле</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "chamaechor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение, которое после высыхания отрывается от корня и переносится ветром."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перекати-поле",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

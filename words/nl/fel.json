                },
                {
                    "sense_id": "c4324268-37aa-3acd-b965-3a9f3d054e71",
                    "sense_definition": "Hevig, scherp of gepassioneerd van aard (bijvoorbeeld bij pijn, kritiek of emoties).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er was <w>felle</w> kritiek op de nieuwe plannen van de regering.",
                            "target_lang_translations": {
                                "en": "There was <w>fierce</w> criticism of the government's new plans.",
                                "ru": "В адрес новых планов правительства звучала <w>резкая</w> критика."
                            }
                        },
                        {
                            "text": "Hij voelde plotseling een <w>felle</w> pijn in zijn linkerzij.",
                            "target_lang_translations": {
                                "en": "He suddenly felt a <w>sharp</w> pain in his left side.",
                                "ru": "Он внезапно почувствовал <w>острую</w> боль в левом боку."
                            }
                        }
                    ],
                    "synonyms": [
                        "rabiaat"
                    ],
                    "antonyms": [

                        "kres anarchii",
                        "pogrążyć się w anarchii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние общества или государства, в котором отсутствует правящая власть, административные органы или действующие законы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анархия",
                                "target_lang_sense_clarification": "отсутствие государственной власти"
                            },
                            {
                                "target_lang_word": "безвластие",
                                "target_lang_sense_clarification": "отсутствие управления"

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ориндж",
                                "target_lang_sense_clarification": "город в США/Австралии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14bf97cc-420c-3e30-96e6-f1032f42e373",
                    "sense_definition": "The longest river in South Africa.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "geo_group",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "The <w>Orange</w> flows westwards into the Atlantic Ocean.",
                            "target_lang_translations": {
                                "ru": "<w>Оранжевая</w> течет на запад в Атлантический океан."
                            }
                        },
                        {
                            "text": "Farmers rely on water from the <w>Orange</w> for irrigation.",
                            "target_lang_translations": {
                                "ru": "Фермеры зависят от воды из <w>Оранжевой</w> для орошения."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Самая длинная река в Южной Африке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ора́нжевая",
                                "target_lang_sense_clarification": "река"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7639b1e7-8a0d-3f29-994e-51594f691060",
                    "sense_definition": "A royal title used by the House of Orange-Nassau, historically associated with the Netherlands.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "title_group",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "William of <w>Orange</w> played a major role in European history.",
                            "target_lang_translations": {
                                "ru": "Вильгельм <w>Оранский</w> сыграл важную роль в европейской истории."
                            }
                        },
                        {
                            "text": "The Prince of <w>Orange</w> is the heir apparent to the Dutch throne.",
                            "target_lang_translations": {
                                "ru": "Принц <w>Оранский</w> является наследником голландского престола."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Королевский титул, используемый династией Оранских-Нассау, исторически связанной с Нидерландами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ора́нский",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ef0907f5-51d4-3594-bad3-e47c7d72c4d4",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cb4a92d9-7104-3f5b-978f-0ffebfb4473f",
                    "sense_definition": "Een loofboom of struik uit de berkenfamilie die vaak op vochtige grond langs het water groeit.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Langs de oever van de rivier staat een grote <w>els</w>.",
                            "target_lang_translations": {
                                "en": "Along the river bank stands a large <w>alder</w>."
                            }
                        },
                        {
                            "text": "In de herfst verliest de <w>els</w> zijn bladeren, maar de elzenproppen blijven hangen.",
                            "target_lang_translations": {
                                "en": "In autumn the <w>alder</w> loses its leaves, but the alder cones remain hanging."
                            }
                        }
                    ],
                    "synonyms": [
                        "elzeboom",
                        "elzenstruik"
                    ],
                    "common_phrases": [
                        "zwarte els",
                        "witte els"
                    ],
                    "target_lang_definitions": {
                        "en": "A deciduous tree or shrub of the birch family that often grows on moist soil along water."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "alder",
                                "target_lang_sense_clarification": "tree"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06206d4c-665f-3fff-80ec-068488466b42",
                    "sense_definition": "Een scherp, puntig stuk gereedschap dat gebruikt wordt om gaatjes te prikken in materialen zoals leer of hout.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schoenmaker gebruikte een <w>els</w> om door het stugge leer te komen.",
                            "target_lang_translations": {
                                "en": "The shoemaker used an <w>awl</w> to get through the stiff leather."
                            }
                        },
                        {
                            "text": "Leg de <w>els</w> voorzichtig weg, want de punt is erg scherp.",
                            "target_lang_translations": {
                                "en": "Put the <w>awl</w> away carefully, because the point is very sharp."
                            }
                        }
                    ],
                    "synonyms": [
                        "priem",
                        "aal"
                    ],
                    "target_lang_definitions": {
                        "en": "A sharp, pointed tool used to punch holes in materials like leather or wood."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "awl",
                                "target_lang_sense_clarification": "tool"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d56f3775-1fa1-3424-bdee-3c8da204e2a3",
                    "sense_definition": "Een meisjesnaam, vaak gebruikt als verkorte vorm van Elisabeth.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn buurvrouw heet <w>Els</w> en ze is erg aardig.",
                            "target_lang_translations": {
                                "en": "My neighbor is called <w>Els</w> and she is very nice."
                            }
                        },
                        {
                            "text": "We gaan dit weekend op bezoek bij tante <w>Els</w>.",
                            "target_lang_translations": {
                                "en": "We are going to visit aunt <w>Els</w> this weekend."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A girl's name, often used as a shortened form of Elisabeth."
                    },
                    "translations": {
                        "en": [

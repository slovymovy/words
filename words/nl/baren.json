            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d033ceef-407d-37b1-82ce-8b0b711c12fa",
                    "sense_definition": "Een kind of jong ter wereld brengen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "birth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij heeft in het ziekenhuis een gezonde zoon <w>gebaard</w>.",
                            "target_lang_translations": {
                                "ru": "Она <w>родила</w> в больнице здорового сына."
                            }
                        },
                        {
                            "text": "De koe heeft vannacht zonder problemen een kalf <w>gebaard</w>.",
                            "target_lang_translations": {
                                "ru": "Этой ночью корова без проблем <w>родила</w> теленка."
                            }
                        }
                    ],
                    "synonyms": [
                        "het leven schenken",
                        "voortbrengen"
                    ],
                    "common_phrases": [
                        "kinderen baren",
                        "in pijn baren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рожать ребенка или производить на свет детеныша."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рожать",
                                "target_lang_sense_clarification": "давать жизнь ребенку или детенышу"
                            },
                            {
                                "target_lang_word": "производить на свет",
                                "target_lang_sense_clarification": "формальное выражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b23d6d29-05fb-38bf-9209-701df6dd33d6",
                    "sense_definition": "Veroorzaken of teweegbrengen, vaak gebruikt in combinatie met abstracte begrippen zoals zorgen of opzien.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De slechte economische situatie <w>baart</w> de regering grote zorgen.",
                            "target_lang_translations": {
                                "ru": "Плохая экономическая ситуация <w>вызывает</w> у правительства большую тревогу."
                            }
                        },
                        {
                            "text": "Zijn plotselinge vertrek zal ongetwijfeld veel opzien <w>baren</w>.",
                            "target_lang_translations": {
                                "ru": "Его внезапный отъезд, несомненно, <w>вызовет</w> большую сенсацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "teweegbrengen"
                    ],
                    "common_phrases": [
                        "zorgen baren",
                        "opzien baren",
                        "vreugde baren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать или порождать (обычно абстрактные понятия, такие как беспокойство или сенсация)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "быть причиной чувств или реакции"
                            },
                            {
                                "target_lang_word": "порождать",
                                "target_lang_sense_clarification": "создавать или влечь за собой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7378f4ca-a45e-3f71-8741-96de5b9ebc30",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>baar</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De woeste <w>baren</w> sloegen tijdens de storm hard tegen de kust.",
                            "target_lang_translations": {
                                "ru": "Свирепые <w>волны</w> во время шторма с силой бились о берег."
                            }
                        },
                        {
                            "text": "In de kluis lagen tientallen gouden <w>baren</w> opgestapeld.",
                            "target_lang_translations": {
                                "ru": "В сейфе лежали сложенные десятками золотые <w>слитки</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число существительного \"baar\" (волна или слиток)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слитки",
                                "target_lang_sense_clarification": "бруски металла"
                            },
                            {
                                "target_lang_word": "волны",
                                "target_lang_sense_clarification": "поэтическое или устаревшее значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

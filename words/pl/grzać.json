            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ca129183-4a35-34dd-84f4-79b151a8c434",
                    "sense_definition": "Podnosić temperaturę czegoś lub emitować ciepło.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "temperature_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem słońce bardzo mocno <w>grzeje</w> już od wczesnego ranka.",
                            "target_lang_translations": {
                                "ru": "Летом солнце очень сильно <w>греет</w> уже с раннего утра."
                            }
                        },
                        {
                            "text": "Muszę <w>grzać</w> wodę w dużym garnku, ponieważ zepsuł nam się bojler.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>греть</w> воду в большой кастрюле, так как у нас сломался бойлер."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogrzewać",
                        "podgrzewać",
                        "ocieplać",
                        "nagrzewać"
                    ],
                    "antonyms": [
                        "chłodzić",
                        "studzić",
                        "ziębić"
                    ],
                    "common_phrases": [
                        "słońce grzeje",
                        "grzać wodę",
                        "kaloryfer grzeje",
                        "grzać mieszkanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повышать температуру чего-либо или излучать тепло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "греть",
                                "target_lang_sense_clarification": "излучать тепло или делать теплым"
                            },
                            {
                                "target_lang_word": "нагревать",
                                "target_lang_sense_clarification": "повышать температуру жидкости или предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2731f50-cc02-36b2-9f24-936e8d24712a",
                    "sense_definition": "Stawać się ciepłym lub korzystać ze źródła ciepła, aby podnieść temperaturę swojego ciała (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "temperature_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmarznięci turyści siedzieli przy ognisku i <w>grzali</w> się w jego cieple.",
                            "target_lang_translations": {
                                "ru": "Замерзшие туристы сидели у костра и <w>грелись</w> в его тепле."
                            }
                        },
                        {
                            "text": "Silnik w starym samochodzie szybko <w>grzeje</w> się podczas jazdy pod górę.",
                            "target_lang_translations": {
                                "ru": "Двигатель в старой машине быстро <w>греется</w> во время езды в гору."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogrzewać się",
                        "nagrzewać się"
                    ],
                    "antonyms": [
                        "marznąć",
                        "stygnąć"
                    ],
                    "common_phrases": [
                        "grzać się przy kominku",
                        "silnik się grzeje",
                        "grzać się w słońcu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться теплым или пользоваться источником тепла, чтобы согреть свое тело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "греться",
                                "target_lang_sense_clarification": "о людях или животных"
                            },
                            {
                                "target_lang_word": "нагреваться",
                                "target_lang_sense_clarification": "о механизмах или предметах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41fc4493-69b2-3fed-803e-d3a0a1f087a0",
                    "sense_definition": "Przemieszczać się bardzo szybko, zwykle jadąc samochodem lub biegnąc.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca zaczął niebezpiecznie <w>grzać</w> autostradą, przekraczając dozwoloną prędkość.",
                            "target_lang_translations": {
                                "ru": "Водитель начал опасно <w>гнать</w> по автостраде, превышая допустимую скорость."
                            }
                        },
                        {
                            "text": "Musimy <w>grzać</w> na przystanek, bo za chwilę odjedzie nasz autobus.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>гнать</w> на остановку, потому что через минуту отойдет наш автобус."
                            }
                        }
                    ],
                    "synonyms": [
                        "pędzić",
                        "gnać",
                        "zasuwać"
                    ],
                    "antonyms": [
                        "wlec się"
                    ],
                    "common_phrases": [
                        "grzać ile fabryka dała",
                        "grzać do przodu",
                        "grzać na złamanie karku"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться очень быстро, обычно на машине или бегом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнать",
                                "target_lang_sense_clarification": "ехать или бежать очень быстро (разг.)"
                            },
                            {
                                "target_lang_word": "мчаться",
                                "target_lang_sense_clarification": "быстро двигаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec5a12eb-3435-3d06-88e5-26d576dae0cd",
                    "sense_definition": "Uderzać kogoś mocno lub bić, często wielokrotnie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W filmie akcji bohater zaczął <w>grzać</w> przeciwników pięściami bez litości.",
                            "target_lang_translations": {
                                "ru": "В боевике герой начал безжалостно <w>лупить</w> противников кулаками."
                            }
                        },
                        {
                            "text": "Nie wolno nikogo <w>grzać</w> tylko dlatego, że ma inne zdanie niż ty.",
                            "target_lang_translations": {
                                "ru": "Нельзя никого <w>бить</w> только за то, что у него другое мнение, чем у тебя."
                            }
                        }
                    ],
                    "synonyms": [
                        "bić",
                        "lać",
                        "tłuc",
                        "walić"
                    ],
                    "common_phrases": [
                        "grzać z liścia",
                        "grzać pięściami"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "aggressive context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно ударять или бить кого-то, часто многократно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лупить",
                                "target_lang_sense_clarification": "сильно бить (разг.)"
                            },
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "наносить удары"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2a10f32-ed4b-3f9c-843c-cb4cfe299ac8",
                    "sense_definition": "Pić alkohol, zazwyczaj w dużych ilościach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "alcohol_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiedzi znowu będą <w>grzać</w> wódkę przez całą noc i hałasować.",
                            "target_lang_translations": {
                                "ru": "Соседи снова будут <w>глушить</w> водку всю ночь и шуметь."
                            }
                        },
                        {
                            "text": "Jeśli będziesz tyle <w>grzać</w>, jutro nie wstaniesz do pracy.",
                            "target_lang_translations": {
                                "ru": "Если будешь столько <w>пить</w>, завтра не встанешь на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "pić",
                        "chlać",
                        "tankować",
                        "łoić"
                    ],
                    "antonyms": [
                        "trzeźwieć"
                    ],
                    "common_phrases": [
                        "grzać wódę",
                        "grzać do upadłego"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar or very informal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пить алкоголь, обычно в больших количествах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глушить",
                                "target_lang_sense_clarification": "пить много алкоголя (разг.)"
                            },
                            {
                                "target_lang_word": "бухать",
                                "target_lang_sense_clarification": "пить спиртное (сленг)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "47da3a16-0456-3e38-ade7-0dd8a72adfbd",
                    "sense_definition": "De gezagvoerder die het bevel voert over een schip.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <kapitein> besloot om vanwege de storm niet uit te varen.",
                            "target_lang_translations": {
                                "ru": "<Капитан> решил не выходить в море из-за шторма."
                            }
                        },
                        {
                            "text": "Als <kapitein> van het schip ben je verantwoordelijk voor de veiligheid van de bemanning.",
                            "target_lang_translations": {
                                "ru": "Как <капитан> корабля, вы несете ответственность за безопасность экипажа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kapitein van het schip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Командир, управляющий судном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капитан",
                                "target_lang_sense_clarification": "судна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a41abfd-6d3c-3d24-a3eb-53449ae5d3f4",
                    "sense_definition": "Een militaire rang bij de landmacht of luchtmacht, hoger dan luitenant en lager dan majoor.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na jaren van trouwe dienst werd hij bevorderd tot <kapitein>.",
                            "target_lang_translations": {
                                "ru": "После долгих лет верной службы он был повышен до <капитана>."
                            }
                        },
                        {
                            "text": "De <kapitein> gaf zijn eenheid de opdracht om zich klaar te maken voor vertrek.",
                            "target_lang_translations": {
                                "ru": "<Капитан> приказал своему подразделению готовиться к отправлению."
                            }
                        }
                    ],
                    "synonyms": [
                        "ritmeester"
                    ],
                    "common_phrases": [
                        "bevorderd tot kapitein"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воинское звание в сухопутных войсках или ВВС, выше лейтенанта и ниже майора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капитан",
                                "target_lang_sense_clarification": "звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f629648a-8a92-392a-a803-34e1788c2351",
                    "sense_definition": "Het dorpshoofd van een inheemse of marrongemeenschap.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bewoners van het dorp kozen een nieuwe <kapitein> om hun belangen te behartigen.",
                            "target_lang_translations": {
                                "ru": "Жители деревни выбрали нового <капитана>, чтобы он представлял их интересы."
                            }
                        },
                        {
                            "text": "De <kapitein> riep alle dorpsoudsten bijeen voor een belangrijke vergadering.",
                            "target_lang_translations": {
                                "ru": "<Капитан> созвал всех старейшин деревни на важное собрание."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Suriname"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Глава деревни в общинах коренных народов или маронов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капитан",
                                "target_lang_sense_clarification": "глава общины (Суринам)"
                            },
                            {
                                "target_lang_word": "вождь",
                                "target_lang_sense_clarification": "племени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfcebd7b-a330-3a88-bb31-a284d1255f90",
                    "sense_definition": "Iemand die de leiding neemt of de baas speelt zonder daartoe bevoegd te zijn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het project mislukte omdat er twee <kapiteins> op één schip waren.",
                            "target_lang_translations": {
                                "ru": "Проект провалился, потому что на одном корабле было два <капитана>."
                            }
                        },
                        {
                            "text": "Hij probeert altijd <kapitein> te spelen, maar niemand luistert echt naar hem.",
                            "target_lang_translations": {
                                "ru": "Он всегда пытается строить из себя <командира>, но никто его на самом деле не слушает."
                            }
                        }
                    ],
                    "common_phrases": [
                        "twee kapiteins op één schip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто берет на себя руководство или командует, не имея на то полномочий."
                    },
                    "translations": {
                        "ru": [

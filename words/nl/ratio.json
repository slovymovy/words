            "pos": "noun",
            "senses": [
                {
                    "sense_id": "24fab6f2-39d0-3e21-a04c-b2f4f3f05fa9",
                    "sense_definition": "Een getalsmatige verhouding tussen twee grootheden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "proportion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>ratio</w> tussen de kosten en de baten moet positief blijven.",
                            "target_lang_translations": {
                                "ru": "<w>Соотношение</w> затрат и выгод должно оставаться положительным.",
                                "en": "The <w>ratio</w> between costs and benefits must remain positive."
                            }
                        },
                        {
                            "text": "In deze klas is de <w>ratio</w> jongens tegen meisjes precies gelijk.",
                            "target_lang_translations": {
                                "ru": "В этом классе <w>соотношение</w> мальчиков и девочек совершенно одинаковое.",
                                "en": "In this class, the <w>ratio</w> of boys to girls is exactly equal."
                            }
                        }
                    ],
                    "synonyms": [
                        "verhouding"
                    ],
                    "common_phrases": [
                        "financiële ratio",
                        "de gouden ratio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Числовое соотношение между двумя величинами.",
                        "en": "A numerical relationship between two quantities."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соотношение",
                                "target_lang_sense_clarification": "взаимная связь между величинами"
                            },
                            {
                                "target_lang_word": "пропорция",
                                "target_lang_sense_clarification": "соразмерность частей"
                            },
                            {
                                "target_lang_word": "коэффициент",
                                "target_lang_sense_clarification": "числовой показатель"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "ratio",
                                "target_lang_sense_clarification": "numerical comparison"
                            },
                            {
                                "target_lang_word": "proportion",
                                "target_lang_sense_clarification": "comparative relation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23acdfe5-e2e0-3c68-a351-1e87c4288f11",
                    "sense_definition": "De achterliggende gedachte, reden of logica achter een besluit of plan.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rationale",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik begrijp de <w>ratio</w> achter deze plotselinge beslissing niet helemaal.",
                            "target_lang_translations": {
                                "ru": "Я не совсем понимаю <w>логику</w> этого внезапного решения.",
                                "en": "I don't quite understand the <w>rationale</w> behind this sudden decision."
                            }
                        },
                        {
                            "text": "De <w>ratio</w> van het nieuwe beleid werd duidelijk uitgelegd tijdens de vergadering.",
                            "target_lang_translations": {
                                "ru": "<w>Обоснование</w> новой политики было четко разъяснено на собрании.",
                                "en": "The <w>rationale</w> for the new policy was clearly explained during the meeting."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de ratio achter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Логическое обоснование, причина или мысль, лежащая в основе решения.",
                        "en": "The underlying thought, reason, or logic behind a decision or plan."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "логика",
                                "target_lang_sense_clarification": "внутренняя закономерность"
                            },
                            {
                                "target_lang_word": "обоснование",
                                "target_lang_sense_clarification": "довод или причина"
                            },
                            {
                                "target_lang_word": "смысл",
                                "target_lang_sense_clarification": "разумное основание"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "rationale",
                                "target_lang_sense_clarification": "underlying reason"
                            },
                            {
                                "target_lang_word": "logic",
                                "target_lang_sense_clarification": "reasoning behind something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77217dde-7d63-3015-a4bd-2195b732fd9b",
                    "sense_definition": "Het menselijk vermogen om logisch te denken en te begrijpen, in tegenstelling tot het gevoel.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "faculty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In dit debat moeten we ons laten leiden door de <w>ratio</w> en niet door emotie.",
                            "target_lang_translations": {
                                "ru": "В этих дебатах мы должны руководствоваться <w>разумом</w>, а не эмоциями.",
                                "en": "In this debate, we must be guided by <w>reason</w> and not by emotion."
                            }
                        },
                        {
                            "text": "Zijn handelen werd volledig gestuurd door pure <w>ratio</w>.",
                            "target_lang_translations": {
                                "ru": "Его действия были продиктованы чистым <w>разумом</w>.",
                                "en": "His actions were driven entirely by pure <w>reason</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rede",
                        "verstand"
                    ],
                    "common_phrases": [
                        "emotie en ratio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человеческая способность логически мыслить и понимать; разум.",
                        "en": "The human capacity to think logically and understand, as opposed to emotion."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разум",
                                "target_lang_sense_clarification": "способность мыслить"
                            },
                            {
                                "target_lang_word": "рассудок",
                                "target_lang_sense_clarification": "здравый смысл"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "reason",
                                "target_lang_sense_clarification": "mental faculty"
                            },
                            {
                                "target_lang_word": "rationality",
                                "target_lang_sense_clarification": "quality of being rational"
                            }
                        ]
                    }
                }

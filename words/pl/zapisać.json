                    "sense_id": "8846871b-7b3e-3a24-9e4c-72e39f6924b6",
                    "sense_definition": "Utrwalić informację za pomocą pisma, aby jej nie zapomnieć.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "preserving_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <w>zapisać</w> ten temat w zeszycie.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>запишите</w> эту тему в тетрадь."
                            }
                        },
                        {
                            "text": "Nie zdążyłem <w>zapisać</w> twojego numeru telefonu.",
                            "target_lang_translations": {
                                "ru": "Я не успел <w>записать</w> твой номер телефона."
                            }
                        }
                    ],
                    "synonyms": [
                        "zanotować",
                        "spisać"
                    ],
                    "common_phrases": [

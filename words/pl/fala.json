    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4e0490f0-6bbd-3165-b44c-addfa895f5e9",
                    "sense_definition": "Ruchome wzniesienie na powierzchni wody, powstające zwykle pod wpływem wiatru.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "water_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wysoka <fala> zalała plażę i zniszczyła zamki z piasku.",
                            "target_lang_translations": {
                                "ru": "Высокая <волна> залила пляж и разрушила замки из песка."
                            }
                        },
                        {
                            "text": "Statek kołysał się na <falach> podczas sztormu.",
                            "target_lang_translations": {
                                "ru": "Корабль качался на <волнах> во время шторма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fala morska",
                        "szum fal",
                        "na fali",
                        "wzburzone fale"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движущееся возвышение на поверхности воды, возникающее обычно под воздействием ветра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волна",
                                "target_lang_sense_clarification": "на воде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "877842d6-500f-3681-b5d5-d2b62bb22ee9",
                    "sense_definition": "Nagłe pojawienie się dużej ilości czegoś lub kogoś; gwałtowne nasilenie się jakiegoś zjawiska.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_influx",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez kraj przetoczyła się <fala> upałów, bijąc rekordy temperatur.",
                            "target_lang_translations": {
                                "ru": "По стране прокатилась <волна> жары, побив температурные рекорды."
                            }
                        },
                        {
                            "text": "Nowa <fala> turystów przyjechała do miasta w weekend majowy.",
                            "target_lang_translations": {
                                "ru": "Новая <волна> туристов приехала в город на майские праздники."
                            }
                        }
                    ],
                    "synonyms": [
                        "nasilenie",
                        "przypływ",
                        "nawałnica"
                    ],
                    "common_phrases": [
                        "fala upałów",
                        "fala protestów",
                        "fala uchodźców",
                        "fala krytyki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное появление большого количества чего-либо или кого-либо; резкое усиление какого-либо явления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волна",
                                "target_lang_sense_clarification": "массовое явление"
                            },
                            {
                                "target_lang_word": "наплыв",
                                "target_lang_sense_clarification": "большое количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e569f7af-cacd-37b4-9a1a-da56b75bb4ba",
                    "sense_definition": "Zaburzenie rozprzestrzeniające się w jakimś ośrodku lub przestrzeni (np. dźwięk, światło).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Radio odbiera <fale> długie i krótkie.",
                            "target_lang_translations": {
                                "ru": "Радио принимает длинные и короткие <волны>."
                            }
                        },
                        {
                            "text": "Kuchenka mikrofalowa wykorzystuje <fale> elektromagnetyczne do podgrzewania jedzenia.",
                            "target_lang_translations": {
                                "ru": "Микроволновая печь использует электромагнитные <волны> для разогрева еды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fale radiowe",
                        "fale dźwiękowe",
                        "długość fali",
                        "fala uderzeniowa"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возмущение, распространяющееся в какой-либо среде или пространстве (например, звук, свет)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волна",
                                "target_lang_sense_clarification": "физика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c4db3c2-186a-31c8-93a7-449b99d62efa",
                    "sense_definition": "Nagły i silny napływ uczuć lub doznań fizycznych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_influx",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ogarnęła go nagła <fala> radości na wieść o wygranej.",
                            "target_lang_translations": {
                                "ru": "Его охватила внезапная <волна> радости при известии о победе."
                            }
                        },
                        {
                            "text": "Poczuła <falę> gorąca na twarzy ze wstydu.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала <волну> жара на лице от стыда."
                            }
                        }
                    ],
                    "synonyms": [
                        "przypływ"
                    ],
                    "common_phrases": [
                        "fala gorąca",
                        "fala nienawiści",
                        "fala entuzjazmu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапный и сильный прилив чувств или физических ощущений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волна",
                                "target_lang_sense_clarification": "чувств"
                            },
                            {
                                "target_lang_word": "прилив",
                                "target_lang_sense_clarification": "ощущений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f6743c6-ce1c-3de5-b87f-5e6bd7ac19f5",
                    "sense_definition": "Sposób ułożenia włosów przypominający kształt fal wodnych.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fryzjer ułożył jej włosy w piękne, miękkie <fale>.",
                            "target_lang_translations": {
                                "ru": "Парикмахер уложил её волосы в красивые, мягкие <волны>."
                            }
                        },
                        {
                            "text": "Na starych zdjęciach kobiety często mają <fale> zamiast prostych włosów.",
                            "target_lang_translations": {
                                "ru": "На старых фотографиях у женщин часто <волны> вместо прямых волос."
                            }
                        }
                    ],
                    "common_phrases": [
                        "robić fale",
                        "włosy w fale"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ укладки волос, напоминающий форму водных волн."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волны",
                                "target_lang_sense_clarification": "прическа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e630073-415f-394d-a627-54acc5bccdfd",
                    "sense_definition": "Nieformalna hierarchia i przemoc starszych stażem nad młodszymi, głównie w wojsku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hazing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wojsku od lat walczy się ze zjawiskiem <fali>.",
                            "target_lang_translations": {
                                "ru": "В армии годами борются с <дедовщиной>."
                            }
                        },
                        {
                            "text": "Młodzi rekruci bali się <fali> ze strony starszych żołnierzy.",
                            "target_lang_translations": {
                                "ru": "Молодые новобранцы боялись <дедовщины> со стороны старослужащих."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ofiara fali",
                        "zjawisko fali"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "military/sociological"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальная иерархия и насилие старших по сроку службы над младшими, в основном в армии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дедовщина",
                                "target_lang_sense_clarification": "в армии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
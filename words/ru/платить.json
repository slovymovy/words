    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "72bb29af-e1b3-306d-97c6-f5e25eb752c2",
                    "sense_definition": "Отдавать деньги или другие ценности в обмен на товары, услуги или в качестве исполнения обязательств.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "payment_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом магазине можно <w>платить</w> картой или наличными.",
                            "target_lang_translations": {
                                "en": "In this store you can <w>pay</w> by card or in cash."
                            }
                        },
                        {
                            "text": "Мы должны <w>платить</w> за аренду квартиры каждый месяц.",
                            "target_lang_translations": {
                                "en": "We have to <w>pay</w> rent for the apartment every month."
                            }
                        }
                    ],
                    "synonyms": [
                        "уплачивать",
                        "расплачиваться"
                    ],
                    "common_phrases": [
                        "платить наличными",
                        "платить картой",
                        "платить налоги",
                        "платить по счетам",
                        "платить за проезд"
                    ],
                    "target_lang_definitions": {
                        "en": "To give money or other valuables in exchange for goods, services, or to fulfill obligations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay",
                                "target_lang_sense_clarification": "give money"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02341df2-0eed-35ff-85e2-1397d80835fd",
                    "sense_definition": "Нести наказание, лишаться чего-либо или жертвовать чем-то как следствие своих действий или ошибок.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "payment_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За свои ошибки часто приходится дорого <w>платить</w>.",
                            "target_lang_translations": {
                                "en": "One often has to <w>pay</w> dearly for one's mistakes."
                            }
                        },
                        {
                            "text": "Спортсмены иногда <w>платят</w> здоровьем за олимпийские медали.",
                            "target_lang_translations": {
                                "en": "Athletes sometimes <w>pay</w> with their health for Olympic medals."
                            }
                        }
                    ],
                    "synonyms": [
                        "расплачиваться"
                    ],
                    "common_phrases": [
                        "дорого платить",
                        "платить здоровьем",
                        "платить жизнью"
                    ],
                    "target_lang_definitions": {
                        "en": "To suffer punishment, loss, or sacrifice as a consequence of one's actions or mistakes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay",
                                "target_lang_sense_clarification": "suffer consequences"
                            },
                            {
                                "target_lang_word": "pay for",
                                "target_lang_sense_clarification": "atone for"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99a13464-d43d-3a2d-8202-bfde529bfb8c",
                    "sense_definition": "Совершать ответное действие, отвечать добром или злом на чьи-либо поступки.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "payment_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он никогда не забывал помощи и всегда <w>платил</w> добром за добро.",
                            "target_lang_translations": {
                                "en": "He never forgot help and always <w>repaid</w> kindness with kindness."
                            }
                        },
                        {
                            "text": "Не стоит <w>платить</w> той же монетой грубому человеку.",
                            "target_lang_translations": {
                                "en": "It is not worth <w>paying</w> a rude person back in their own coin."
                            }
                        }
                    ],
                    "synonyms": [
                        "отплачивать"
                    ],
                    "common_phrases": [
                        "платить той же монетой",
                        "платить добром за добро"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in phrases about retribution or gratitude"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To reciprocate; to respond to someone's actions with a similar action, whether good or bad."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "repay",
                                "target_lang_sense_clarification": "give in return"
                            },
                            {
                                "target_lang_word": "pay back",
                                "target_lang_sense_clarification": "retaliate or reward"
                            },
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "reciprocate"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f07967e2-b42a-39d5-94e6-37078b93d2df",
                    "sense_definition": "Чинить одежду или ткань, накладывая заплаты.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mending",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка села у окна <w>платить</w> старую рубаху.",
                            "target_lang_translations": {
                                "en": "Grandma sat by the window to <w>patch</w> the old shirt."
                            }
                        },
                        {
                            "text": "В старину умели искусно <w>платить</w> прохудившуюся одежду.",
                            "target_lang_translations": {
                                "en": "In the old days, they knew how to skillfully <w>mend</w> worn-out clothes."
                            }
                        }
                    ],
                    "synonyms": [
                        "латать",
                        "починять"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "regional",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To mend clothes or fabric by applying patches."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "patch",
                                "target_lang_sense_clarification": "mend with fabric"
                            },
                            {
                                "target_lang_word": "mend",
                                "target_lang_sense_clarification": "repair"
                            }
                        ]
                    }
                }
            ]
        }

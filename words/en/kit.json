                    "target_lang_definitions": {
                        "ru": "Снабжать или экипировать кого-либо набором одежды, инструментов или оборудования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экипировать",
                                "target_lang_sense_clarification": "снаряжать"
                            },
                            {
                                "target_lang_word": "снаряжать",
                                "target_lang_sense_clarification": "обеспечивать снаряжением"
                            }
                        ]
                    }
                }

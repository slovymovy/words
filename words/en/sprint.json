            "senses": [
                {
                    "sense_id": "337f1f98-6946-3976-81fd-df7763aad172",
                    "sense_definition": "A short race run at top speed.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rapid_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The athlete won the gold medal in the 100-meter <w>sprint</w>.",

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "54c258e9-e65d-301b-bf49-da7cecb58ed4",
                    "sense_definition": "A large number or amount; a great deal.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_large_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She made <w>lots</w> of new friends during her trip to Spain.",
                            "target_lang_translations": {
                                "ru": "Она завела <w>много</w> новых друзей во время поездки в Испанию."
                            }
                        },
                        {
                            "text": "Don't worry about the bill because my family has <w>lots</w> of money.",
                            "target_lang_translations": {
                                "ru": "Не беспокойся о счете, потому что у моей семьи <w>куча</w> денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "plenty",
                        "loads",
                        "heaps",
                        "tons"
                    ],
                    "antonyms": [
                        "little",
                        "few"
                    ],
                    "common_phrases": [
                        "lots of",
                        "lots to do",
                        "lots and lots"
                    ],
                    "traits": [
                        {

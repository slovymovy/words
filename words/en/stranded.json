    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b4054ed9-b83f-39e3-974a-0b56bc527527",
                    "sense_definition": "Left in a place or situation without a way to leave, move, or escape.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "stuck_situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We were left <w>stranded</w> at the airport after our flight was cancelled.",
                            "target_lang_translations": {
                                "ru": "Мы <w>застряли</w> в аэропорту после того, как наш рейс был отменен."
                            }
                        },
                        {
                            "text": "The hikers found themselves <w>stranded</w> on the mountain when the heavy snow began to fall.",
                            "target_lang_translations": {
                                "ru": "Туристы оказались <w>заблокированными</w> на горе, когда пошел сильный снег."
                            }
                        }
                    ],
                    "synonyms": [
                        "marooned",
                        "stuck",
                        "abandoned",
                        "helpless"
                    ],
                    "antonyms": [
                        "rescued",
                        "saved"
                    ],
                    "common_phrases": [
                        "left stranded",
                        "stranded passengers",
                        "stranded on an island"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставленный в месте или ситуации без возможности уйти, уехать или спастись."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застрявший",
                                "target_lang_sense_clarification": "не имеющий возможности выбраться"
                            },
                            {
                                "target_lang_word": "покинутый",
                                "target_lang_sense_clarification": "оставленный на произвол судьбы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4d83e05-4a3d-3a9d-b395-78e29c6740ab",
                    "sense_definition": "Driven or run onto a shore, reef, or shallow water and unable to return to the water.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "stuck_situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volunteers worked for hours to keep the <w>stranded</w> whale wet until the tide came in.",
                            "target_lang_translations": {
                                "ru": "Волонтеры часами работали, чтобы поддерживать влажность <w>выброшенного на берег</w> кита, пока не начался прилив."
                            }
                        },
                        {
                            "text": "The captain called for help after his vessel became <w>stranded</w> on a sandbank.",
                            "target_lang_translations": {
                                "ru": "Капитан позвал на помощь после того, как его судно <w>село на мель</w> на песчаной отмели."
                            }
                        }
                    ],
                    "synonyms": [
                        "beached",
                        "grounded",
                        "aground"
                    ],
                    "antonyms": [
                        "afloat"
                    ],
                    "common_phrases": [
                        "stranded whale",
                        "stranded ship",
                        "stranded vessel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выброшенный на берег или севший на мель (о судне или морском животном)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выброшенный на берег",
                                "target_lang_sense_clarification": "о морских животных или обломках"
                            },
                            {
                                "target_lang_word": "севший на мель",
                                "target_lang_sense_clarification": "о судах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cadc32a-bf8b-38c4-8fa0-a7236685b494",
                    "sense_definition": "Referring to investments, assets, or costs that have become unrecoverable, obsolete, or redundant.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new environmental regulations turned the old coal plant into a <w>stranded</w> asset.",
                            "target_lang_translations": {
                                "ru": "Новые экологические нормы превратили старую угольную электростанцию в <w>обесценившийся</w> актив."
                            }
                        },
                        {
                            "text": "Investors are concerned about the potential for <w>stranded</w> costs in the changing market.",
                            "target_lang_translations": {

                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "agent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The intelligence <w>operative</w> gathered classified information from the embassy.",
                            "target_lang_translations": {
                                "ru": "<w>Оперативник</w> разведки собрал секретную информацию из посольства."
                            }

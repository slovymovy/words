    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d1f78843-6974-3c66-9aa2-1964b087c10c",
                    "sense_definition": "Женский эквивалент слова <w>принц</w>; титул нецарствующего члена королевского дома, дочь короля или жена принца.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В высокой башне жила прекрасная <w>принцесса</w>.",
                            "target_lang_translations": {
                                "en": "A beautiful <w>princess</w> lived in a high tower."
                            }
                        },
                        {
                            "text": "Молодая <w>принцесса</w> вышла замуж за принца из соседнего королевства.",
                            "target_lang_translations": {
                                "en": "The young <w>princess</w> married a prince from a neighboring kingdom."
                            }
                        }
                    ],
                    "synonyms": [
                        "королевна"
                    ],
                    "antonyms": [
                        "принц"
                    ],
                    "common_phrases": [
                        "прекрасная принцесса",
                        "наследная принцесса",
                        "сказка о принцессе"
                    ],
                    "target_lang_definitions": {
                        "en": "A female member of a royal family, such as the daughter of a monarch or the wife of a prince."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "princess",
                                "target_lang_sense_clarification": "royal title"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a85c89d-edf6-337d-9e70-8c00fe78f44d",
                    "sense_definition": "Эпитет по отношению к девушке или женщине, часто капризной, изнеженной или требующей особого внимания.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она ведёт себя как <w>принцесса</w> и отказывается мыть посуду.",
                            "target_lang_translations": {
                                "en": "She acts like a <w>princess</w> and refuses to wash the dishes."
                            }
                        },
                        {
                            "text": "Хватит строить из себя <w>принцессу</w>, здесь все работают на равных.",
                            "target_lang_translations": {
                                "en": "Stop acting like a <w>princess</w>; everyone here works on equal terms."
                            }
                        }
                    ],
                    "common_phrases": [
                        "строить из себя принцессу",
                        "как принцесса",
                        "настоящая принцесса"
                    ],
                    "target_lang_definitions": {
                        "en": "A term used to describe a girl or woman who is vain, spoiled, or demands special treatment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "princess",
                                "target_lang_sense_clarification": "spoiled person"
                            }
                        ]
                    }
                }
            ]
        }
    ]

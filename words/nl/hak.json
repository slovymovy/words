                    "sense_id": "a33c2c1d-2f6b-3662-b5ae-711701a17f87",
                    "sense_definition": "Een verhoging onder de achterkant van een schoen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "heel_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij draagt graag laarzen met een hoge <w>hak</w>.",
                            "target_lang_translations": {
                                "en": "She likes wearing boots with a high <w>heel</w>.",
                                "ru": "Она любит носить сапоги на высоком <w>каблуке</w>."
                            }
                        },
                        {
                            "text": "De <w>hak</w> van mijn linkerschoen is afgebroken.",
                            "target_lang_translations": {
                                "en": "The <w>heel</w> of my left shoe has broken off.",
                                "ru": "<w>Каблук</w> моей левой туфли сломался."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hoge hakken",
                        "op hakken lopen",
                        "naaldhakken"
                    ],
                    "target_lang_definitions": {
                        "en": "A raised part under the back of a shoe.",
                        "ru": "Возвышение под задней частью обуви."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heel",

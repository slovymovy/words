        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "924e483f-e099-3bd6-93d9-dd692ff0b8db",
                    "sense_definition": "związany z religią, Bogiem lub kultem; otoczony czcią religijną",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "religious_cult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W kościele wiszą <święte> obrazy przedstawiające sceny biblijne.",
                            "target_lang_translations": {
                                "ru": "В церкви висят <святые> иконы, изображающие библейские сцены."
                            }
                        },
                        {
                            "text": "Jerozolima jest uważana za <święte> miasto przez wyznawców trzech religii.",
                            "target_lang_translations": {
                                "ru": "Иерусалим считается <святым> городом последователями трех религий."
                            }
                        }
                    ],
                    "synonyms": [
                        "nienaruszalny"
                    ],
                    "antonyms": [
                        "świecki"
                    ],
                    "common_phrases": [
                        "Pismo Święte",
                        "Duch Święty",
                        "msza święta",
                        "Ziemia Święta"
                    ],
                    "target_lang_definitions": {
                        "ru": "связанный с религией, Богом или культом; окруженный религиозным почитанием"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "святой",
                                "target_lang_sense_clarification": "религиозный"
                            },
                            {
                                "target_lang_word": "священный",
                                "target_lang_sense_clarification": "относящийся к культу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e315346a-d856-3504-b4a6-48aa906c9a26",
                    "sense_definition": "o osobie: uznany przez Kościół za godnego kultu, kanonizowany; żyjący w sposób doskonały religijnie",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "religious_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Święta> Cecylia jest znaną patronką muzyki kościelnej.",
                            "target_lang_translations": {
                                "ru": "<Святая> Цецилия — известная покровительница церковной музыки."
                            }
                        },
                        {
                            "text": "Papież ogłosił go <świętym> człowiekiem za jego zasługi dla ubogich.",
                            "target_lang_translations": {
                                "ru": "Папа провозгласил его <святым> человеком за его заслуги перед бедными."
                            }
                        }
                    ],
                    "antonyms": [
                        "grzeszny"
                    ],
                    "common_phrases": [
                        "święty Mikołaj",
                        "święty patron",
                        "rodzina święta"
                    ],
                    "target_lang_definitions": {
                        "ru": "о человеке: канонизированный Церковью, признанный достойным поклонения; живущий праведной жизнью"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "святой",
                                "target_lang_sense_clarification": "канонизированный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71eb4183-a147-37e3-a53e-94152ff52ccd",
                    "sense_definition": "całkowity, zupełny; używane dla podkreślenia wagi spokoju lub cierpliwości",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emphatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Daj mi wreszcie <święty> spokój i przestań zadawać tyle pytań.",
                            "target_lang_translations": {
                                "ru": "Оставь меня наконец в <полном> покое и перестань задавать столько вопросов."
                            }
                        },
                        {
                            "text": "Podziwiam go, bo trzeba mieć <świętą> cierpliwość do takich klientów.",
                            "target_lang_translations": {
                                "ru": "Я восхищаюсь им, потому что нужно иметь <ангельское> терпение к таким клиентам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "święty spokój",
                        "święta cierpliwość",
                        "święte oburzenie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane głównie w wyrażeniach idiomatycznych"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "полный, абсолютный; используется для усиления значения (обычно покоя или терпения)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ангельский",
                                "target_lang_sense_clarification": "о терпении"
                            },
                            {
                                "target_lang_word": "святой",
                                "target_lang_sense_clarification": "в идиомах"
                            },
                            {
                                "target_lang_word": "полный",
                                "target_lang_sense_clarification": "абсолютный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffd2cf57-f745-386b-b6b6-0f3a021ca473",
                    "sense_definition": "taki, którego nie wolno naruszyć ani podważyć; niezwykle ważny i szanowany",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "inviolable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pamiętaj, że dla nich nawet ustna umowa jest <święta> i wiążąca.",
                            "target_lang_translations": {
                                "ru": "Помни, что для них даже устный договор <свят> и обязателен."
                            }
                        },
                        {
                            "text": "Obrona ojczyzny to dla każdego obywatela <święty> obowiązek.",
                            "target_lang_translations": {
                                "ru": "Защита отечества — это <святая> обязанность каждого гражданина."
                            }
                        }
                    ],
                    "synonyms": [
                        "nienaruszalny",
                        "nietykalny",
                        "niepodważalny"
                    ],
                    "common_phrases": [
                        "święte prawo",
                        "rzecz święta",
                        "święty obowiązek"
                    ],
                    "target_lang_definitions": {
                        "ru": "такой, который нельзя нарушать или оспаривать; чрезвычайно важный и уважаемый"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "святой",
                                "target_lang_sense_clarification": "нерушимый"
                            },
                            {
                                "target_lang_word": "священный",
                                "target_lang_sense_clarification": "высокий, важный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2904ea70-dabf-3dea-81d4-be09d45b8097",
                    "sense_definition": "niewinny, bez grzechu; często używane ironicznie o kimś, kto udaje niewinnego",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "moral_innocence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ty wcale nie jesteś taki <święty>, też masz swoje wady.",
                            "target_lang_translations": {
                                "ru": "Ты вовсе не такой <святой>, у тебя тоже есть свои недостатки."
                            }
                        },
                        {
                            "text": "Nie udawaj <świętego>, wszyscy wiemy, że to ty zjadłeś ciastka.",
                            "target_lang_translations": {
                                "ru": "Не прикидывайся <святым>, мы все знаем, что это ты съел печенье."
                            }
                        }
                    ],
                    "antonyms": [
                        "grzeszny",
                        "winny"
                    ],
                    "common_phrases": [
                        "udawać świętego"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często w kontekście ironicznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "невиновный, безгрешный; часто используется иронично о ком-то, кто притворяется невинным"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "святой",
                                "target_lang_sense_clarification": "безгрешный"
                            },
                            {
                                "target_lang_word": "безгрешный",
                                "target_lang_sense_clarification": "невинный"
                            }
                        ]
                    }
                }
            ]
        },

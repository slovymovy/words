    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5b8124f1-9c0d-3bd9-8322-884b86d5086d",
                    "sense_definition": "A thin white layer of ice crystals that forms on surfaces when the temperature is below freezing.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ice_weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The grass was covered in white <w>frost</w> early this morning.",
                            "target_lang_translations": {
                                "ru": "Сегодня рано утром трава была покрыта белым <w>инеем</w>."
                            }
                        },
                        {
                            "text": "You will need to scrape the <w>frost</w> off the car windshield before driving.",
                            "target_lang_translations": {
                                "ru": "Тебе придется соскрести <w>иней</w> с лобового стекла машины перед поездкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoarfrost",
                        "rime"
                    ],
                    "common_phrases": [
                        "morning frost",
                        "ground frost",
                        "heavy frost",
                        "white frost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий слой ледяных кристаллов, образующийся на поверхностях при температуре ниже нуля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и́ней",
                                "target_lang_sense_clarification": "кристаллы льда на поверхностях"
                            },
                            {
                                "target_lang_word": "и́зморозь",
                                "target_lang_sense_clarification": "снежный налет в туманную погоду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd772162-2fd6-37c9-8017-99964e5e1902",
                    "sense_definition": "A period of cold weather when the temperature falls below freezing.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ice_weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmers are worried that a late <w>frost</w> might damage the crops.",
                            "target_lang_translations": {
                                "ru": "Фермеры беспокоятся, что поздние <w>заморозки</w> могут повредить посевы."
                            }
                        },
                        {
                            "text": "We are expecting a sharp <w>frost</w> tonight, so bring the plants inside.",
                            "target_lang_translations": {
                                "ru": "Мы ожидаем сильный <w>мороз</w> сегодня ночью, так что занеси растения внутрь."
                            }
                        }
                    ],
                    "synonyms": [
                        "freeze"
                    ],
                    "antonyms": [
                        "thaw",
                        "heatwave"
                    ],
                    "common_phrases": [
                        "hard frost",
                        "sharp frost",
                        "severe frost",
                        "first frost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период холодной погоды, когда температура опускается ниже нуля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моро́з",
                                "target_lang_sense_clarification": "сильный холод, низкая температура"
                            },
                            {
                                "target_lang_word": "за́морозки",
                                "target_lang_sense_clarification": "легкий мороз (обычно весной или осенью)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e4b810d-39d7-3b41-a1a5-c10d5acb2e5d",
                    "sense_definition": "A lack of warmth, friendliness, or emotion in someone's manner.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "emotion_coldness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was a distinct <w>frost</w> in her tone when she spoke to him.",
                            "target_lang_translations": {
                                "ru": "В её тоне чувствовался явный <w>холод</w>, когда она говорила с ним."
                            }
                        },
                        {
                            "text": "He tried to smile, but the <w>frost</w> in his eyes remained.",
                            "target_lang_translations": {
                                "ru": "Он попытался улыбнуться, но <w>холод</w> в его глазах остался."
                            }
                        }
                    ],
                    "synonyms": [
                        "coldness",
                        "iciness",
                        "frigidity"
                    ],
                    "antonyms": [
                        "warmth",
                        "friendliness"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "metaphorical usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствие теплоты, дружелюбия или эмоций в чьем-либо поведении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хо́лод",
                                "target_lang_sense_clarification": "эмоциональная отстраненность"
                            },
                            {
                                "target_lang_word": "холо́дность",
                                "target_lang_sense_clarification": "свойство характера или манеры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "383619ce-0ee0-3bc9-8366-baa1180cd63d",
                    "sense_definition": "A translucent material or filter used to diffuse light, often in photography or theater.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lighting technician placed a <w>frost</w> over the spotlight to soften the beam.",
                            "target_lang_translations": {
                                "ru": "Осветитель поместил <w>матовый фильтр</w> на прожектор, чтобы смягчить луч."
                            }
                        },
                        {
                            "text": "Using a <w>frost</w> helps to reduce harsh shadows on the subject's face.",
                            "target_lang_translations": {
                                "ru": "Использование <w>фрост-фильтра</w> помогает уменьшить резкие тени на лице объекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "diffuser"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical jargon in photography/lighting"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полупрозрачный материал или фильтр, используемый для рассеивания света, часто в фотографии или театре."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "47bae874-9947-38ac-acce-1eae00d8a7ec",
                    "sense_definition": "A man who tends free-range cattle, especially in the American West.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cattle_herder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>cowboy</w> rode his horse across the dusty plains to check on the herd.",
                            "target_lang_translations": {
                                "ru": "<w>Ковбой</w> ехал на лошади по пыльным равнинам, чтобы проверить стадо."
                            }
                        },
                        {
                            "text": "Every <w>cowboy</w> needs a sturdy pair of boots for working on the ranch.",
                            "target_lang_translations": {
                                "ru": "Каждому <w>ковбою</w> нужна пара прочных сапог для работы на ранчо."
                            }
                        }
                    ],
                    "synonyms": [
                        "cowhand",
                        "cattleman",
                        "cowpoke",
                        "buckaroo"
                    ],
                    "common_phrases": [
                        "cowboy hat",
                        "cowboy boots",
                        "cowboy movie"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "American West"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пастух, пасущий стада, особенно на западе США"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ковбой",
                                "target_lang_sense_clarification": "американский пастух"
                            },
                            {
                                "target_lang_word": "пастух",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68333827-f143-3f78-9071-7b6ef89a800b",
                    "sense_definition": "A man who identifies with cowboy culture, including wearing a cowboy hat and being a fan of country and western music.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cattle_herder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is not a rancher, just a <w>cowboy</w> at heart who loves country music.",
                            "target_lang_translations": {
                                "ru": "Он не владелец ранчо, просто <w>ковбой</w> в душе, который любит кантри."
                            }
                        },
                        {
                            "text": "The dance hall was filled with urban <w>cowboys</w> enjoying their Friday night.",
                            "target_lang_translations": {
                                "ru": "Танцевальный зал был полон городских <w>ковбоев</w>, наслаждающихся вечером пятницы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "urban cowboy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, отождествляющий себя с ковбойской культурой (носящий шляпу, любящий кантри)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ковбой",
                                "target_lang_sense_clarification": "стиль жизни/имидж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c1300ff-ce16-37fc-b64d-df5444aaf30d",
                    "sense_definition": "A dishonest or incompetent independent tradesman.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hired a <w>cowboy</w> who left the roof leaking worse than before he started.",
                            "target_lang_translations": {
                                "ru": "Мы наняли <w>халтурщика</w>, который оставил крышу протекающей еще сильнее, чем до начала работ."
                            }
                        },
                        {
                            "text": "Don't trust that <w>cowboy</w> builder with your expensive home renovations.",
                            "target_lang_translations": {
                                "ru": "Не доверяйте этому <w>горе-строителю</w> дорогой ремонт дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "rogue trader",
                        "charlatan"
                    ],
                    "antonyms": [
                        "professional"
                    ],
                    "common_phrases": [
                        "cowboy builder",
                        "cowboy plumber"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Недобросовестный или некомпетентный наемный работник (обычно в строительстве)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "халтурщик",
                                "target_lang_sense_clarification": "недобросовестный работник"
                            },
                            {
                                "target_lang_word": "бракодел",
                                "target_lang_sense_clarification": "плохой мастер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d629a124-6816-3ef7-b17e-4b87a4934c4b",
                    "sense_definition": "A person who engages in reckless behavior, especially for the purpose of showing off.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He drives like a <w>cowboy</w>, weaving in and out of traffic at high speeds.",
                            "target_lang_translations": {
                                "ru": "Он водит как <w>лихач</w>, петляя в потоке машин на высокой скорости."
                            }
                        },
                        {
                            "text": "The manager acted like a <w>cowboy</w> and ignored all the safety regulations.",
                            "target_lang_translations": {
                                "ru": "Менеджер повел себя как <w>сорвиголова</w> и проигнорировал все правила безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "daredevil",
                        "hothead"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, ведущий себя безрассудно, особенно напоказ"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лихач",
                                "target_lang_sense_clarification": "о вождении"
                            },
                            {
                                "target_lang_word": "сорвиголова",
                                "target_lang_sense_clarification": "безрассудный человек"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6cab84ea-215a-39a0-9510-b77e8ed3e57b",
                    "sense_definition": "To work as a cowboy, herding cattle.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cattle_herder_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>cowboyed</w> for ten years before finally settling down in the city.",
                            "target_lang_translations": {
                                "ru": "Он <w>работал ковбоем</w> десять лет, прежде чем окончательно осесть в городе."
                            }
                        },
                        {
                            "text": "They spent the summer <w>cowboying</w> on a large ranch in Montana.",
                            "target_lang_translations": {
                                "ru": "Они провели лето, <w>работая ковбоями</w> на большом ранчо в Монтане."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Работать ковбоем, пасти скот"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работать ковбоем",
                                "target_lang_sense_clarification": "фраза"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "aa60671e-818a-37de-8253-5a24ad819adb",
                    "sense_definition": "Impossible to defeat, overcome, or control.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team played with an <w>unconquerable</w> spirit and won the championship.",
                            "target_lang_translations": {
                                "ru": "Команда играла с <w>несокрушимым</w> духом и выиграла чемпионат."
                            }
                        },
                        {
                            "text": "Despite the difficulties, she maintained an <w>unconquerable</w> belief in her success.",
                            "target_lang_translations": {
                                "ru": "Несмотря на трудности, она сохраняла <w>несокрушимую</w> веру в свой успех."
                            }
                        }
                    ],
                    "synonyms": [
                        "inconquerable"
                    ],
                    "antonyms": [
                        "vulnerable",
                        "beatable"
                    ],
                    "common_phrases": [
                        "unconquerable spirit",
                        "unconquerable will"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кого невозможно победить, преодолеть или подчинить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непобедимый",
                                "target_lang_sense_clarification": "о человеке, армии или сопернике"
                            },
                            {
                                "target_lang_word": "несокрушимый",
                                "target_lang_sense_clarification": "о духе, воле или вере"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e75f26ff-ca9d-3801-b3cf-c91bac786078",
                    "sense_definition": "Someone or something that cannot be conquered.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In ancient myths, the heroes were often portrayed as <w>unconquerables</w>.",
                            "target_lang_translations": {
                                "ru": "В древних мифах герои часто изображались как <w>непобедимые</w>."
                            }
                        },
                        {
                            "text": "They thought they were <w>unconquerables</w>, but every empire eventually falls.",
                            "target_lang_translations": {
                                "ru": "Они считали себя <w>непобедимыми</w>, но каждая империя рано или поздно падает."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или объект, который невозможно завоевать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непобедимый",
                                "target_lang_sense_clarification": "субстантивированное прилагательное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
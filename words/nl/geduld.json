            "senses": [
                {
                    "sense_id": "40d11b84-0015-3390-9d38-dd847a94f1b0",
                    "sense_definition": "Het vermogen om rustig te wachten, vol te houden of iets te verdragen zonder geïrriteerd te raken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het belangrijkste is dat je veel <w>geduld</w> hebt.",
                            "target_lang_translations": {
                                "en": "The most important thing is that you have a lot of <w>patience</w>.",
                                "ru": "Самое главное, чтобы у тебя было много <w>терпения</w>."
                            }
                        },
                        {
                            "text": "Je moet wat <w>geduld</w> hebben voordat het resultaat zichtbaar is.",
                            "target_lang_translations": {
                                "en": "You need to have some <w>patience</w> before the result is visible.",
                                "ru": "Нужно иметь немного <w>терпения</w>, прежде чем результат станет виден."
                            }
                        }
                    ],
                    "synonyms": [
                        "geduldigheid"
                    ],
                    "antonyms": [
                        "ongeduld",
                        "haast",
                        "ongeduldigheid"
                    ],
                    "common_phrases": [
                        "geduld hebben",
                        "geduld verliezen",
                        "geduld bewaren",
                        "engelengeduld"
                    ],
                    "target_lang_definitions": {
                        "en": "The ability to wait calmly, persevere, or endure something without becoming irritated.",
                        "ru": "Способность спокойно ждать, проявлять упорство или выносить что-либо, не раздражаясь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "patience",
                                "target_lang_sense_clarification": "ability to wait or endure"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "терпение",
                                "target_lang_sense_clarification": "способность ждать или переносить трудности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {

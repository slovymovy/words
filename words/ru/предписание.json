            "senses": [
                {
                    "sense_id": "456c438f-f294-35ce-a598-6050a09a1001",
                    "sense_definition": "Официальное распоряжение или документ, содержащий приказ, обязательный для исполнения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "official_order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир вручил офицеру срочное <предписание> немедленно явиться в штаб.",
                            "target_lang_translations": {
                                "en": "The commander handed the officer an urgent <order> to report to headquarters immediately."
                            }
                        },
                        {
                            "text": "Завод получил официальное <предписание> устранить все нарушения пожарной безопасности.",
                            "target_lang_translations": {
                                "en": "The factory received an official <directive> to eliminate all fire safety violations."
                            }
                        }
                    ],
                    "synonyms": [
                        "приказ"
                    ],
                    "common_phrases": [
                        "получить предписание",
                        "выполнить предписание",
                        "нарушить предписание",
                        "служебное предписание"
                    ],
                    "target_lang_definitions": {
                        "en": "An official order or document containing a command that is mandatory for execution."
                    },
                    "translations": {

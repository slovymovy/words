    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "431a0bfc-db9a-30dd-9efa-843181a3980a",
                    "sense_definition": "Исторический двухколёсный экипаж, запряжённый лошадьми, который использовался в древности для войн, торжественных процессий или спортивных состязаний.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Римский полководец въехал в город на золотой <w>колеснице</w>.",
                            "target_lang_translations": {
                                "en": "The Roman commander rode into the city on a golden <w>chariot</w>."
                            }
                        },
                        {
                            "text": "Боевая <w>колесница</w> была грозным оружием древних армий.",
                            "target_lang_translations": {
                                "en": "The war <w>chariot</w> was a formidable weapon of ancient armies."
                            }
                        }
                    ],
                    "common_phrases": [
                        "боевая колесница",
                        "триумфальная колесница",
                        "гонки на колесницах"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A historical two-wheeled horse-drawn carriage used in antiquity for warfare, ceremonial processions, or sporting competitions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chariot",
                                "target_lang_sense_clarification": "historical vehicle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6408cfa5-3a0d-31bc-9556-833b2ed32a0d",
                    "sense_definition": "Транспортное средство или движущая сила (употребляется в переносном смысле, часто в поэтической или возвышенной речи).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "vehicle_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Неумолимая <w>колесница</w> времени быстро уносит нас в будущее.",
                            "target_lang_translations": {
                                "en": "The inexorable <w>chariot</w> of time quickly carries us into the future."
                            }
                        },
                        {
                            "text": "Поэт описал солнце как огненную <w>колесницу</w>, летящую по небу.",
                            "target_lang_translations": {
                                "en": "The poet described the sun as a fiery <w>chariot</w> flying across the sky."
                            }
                        }
                    ],
                    "common_phrases": [
                        "колесница истории",
                        "колесница времени"
                    ],
                    "target_lang_definitions": {
                        "en": "A vehicle or driving force (used figuratively, often in poetic or elevated speech)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chariot",
                                "target_lang_sense_clarification": "figurative"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96268e96-74a1-32f7-ac82-5fe8ab22cd7c",
                    "sense_definition": "Народное название ядовитого кустарника волчеягодник обыкновенный.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В тени деревьев цвела ядовитая <w>колесница</w>.",
                            "target_lang_translations": {
                                "en": "In the shade of the trees, a poisonous <w>mezereon</w> was blooming."
                            }
                        },
                        {
                            "text": "Местные жители называют это растение <w>колесницей</w> и обходят его стороной.",
                            "target_lang_translations": {
                                "en": "Locals call this plant <w>mezereon</w> and avoid it."
                            }
                        }
                    ],
                    "synonyms": [
                        "волчеягодник обыкновенный"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A folk name for the poisonous shrub Daphne mezereum (mezereon)."
                    },

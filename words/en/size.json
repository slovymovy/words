    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6595205a-2ef5-39b7-9eaf-0479253f50e8",
                    "sense_definition": "The physical dimensions, magnitude, or extent of something; how big or small something is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "dimensions_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>size</w> of the building was much larger than I expected.",
                            "target_lang_translations": {
                                "ru": "<w>Размер</w> здания был намного больше, чем я ожидал."
                            }
                        },
                        {
                            "text": "Please arrange these boxes according to their <w>size</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, расставьте эти коробки по их <w>размеру</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "magnitude",
                        "scale",
                        "bigness",
                        "largeness"
                    ],
                    "common_phrases": [
                        "in size",
                        "good size",
                        "size matters",
                        "full size",
                        "average size"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физические параметры, величина или протяженность чего-либо; насколько большим или маленьким является объект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размер",
                                "target_lang_sense_clarification": "параметры объекта"
                            },
                            {
                                "target_lang_word": "величина",
                                "target_lang_sense_clarification": "объем или масштаб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88a0f482-f54a-3085-bffd-1548849e15cc",
                    "sense_definition": "A standard measure used to classify clothing, shoes, or other manufactured items to ensure they fit.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "dimensions_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not think they have this shirt in your <w>size</w>.",
                            "target_lang_translations": {
                                "ru": "Я не думаю, что у них есть эта рубашка твоего <w>размера</w>."
                            }
                        },
                        {
                            "text": "You should check the <w>size</w> chart before buying shoes online.",
                            "target_lang_translations": {
                                "ru": "Тебе следует проверить таблицу <w>размеров</w> перед покупкой обуви онлайн."
                            }
                        }
                    ],
                    "common_phrases": [
                        "what size",
                        "wrong size",
                        "my size",
                        "plus size",
                        "one size"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стандартная мера, используемая для классификации одежды, обуви или других изделий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размер",
                                "target_lang_sense_clarification": "одежды или обуви"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f5df5e3-c02a-3ce8-8b47-cebf2ad947ed",
                    "sense_definition": "A gelatinous or glutinous substance, such as weak glue or starch, used as a glaze or filler for porous materials like paper, cloth, or wall surfaces.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "material_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist applied a coat of <w>size</w> to the canvas to prepare it for painting.",
                            "target_lang_translations": {
                                "ru": "Художник нанес слой <w>грунтовки</w> на холст, чтобы подготовить его к живописи."
                            }
                        },
                        {
                            "text": "You need to mix the <w>size</w> correctly before applying the wallpaper.",
                            "target_lang_translations": {
                                "ru": "Нужно правильно смешать <w>клей</w> перед поклейкой обоев."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical or art context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Клейкое вещество, такое как слабый клей или крахмал, используемое для грунтовки пористых материалов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грунтовка",
                                "target_lang_sense_clarification": "состав для обработки поверхности"
                            },
                            {
                                "target_lang_word": "клей",
                                "target_lang_sense_clarification": "клейкое вещество"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d07a2da3-04ac-317e-afc4-99e05b08f4b3",
                    "sense_definition": "To estimate or measure the dimensions or extent of something; often used in the phrase 'size up' to mean assessing a situation or person.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dimensions_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boxer <w>sized</w> up his opponent before the match began.",
                            "target_lang_translations": {
                                "ru": "Боксер <w>оценил</w> своего противника перед началом боя."
                            }
                        },
                        {
                            "text": "She quickly <w>sized</w> up the situation and decided to leave.",
                            "target_lang_translations": {
                                "ru": "Она быстро <w>оценила</w> ситуацию и решила уйти."
                            }
                        }
                    ],
                    "common_phrases": [
                        "size up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценивать или измерять параметры чего-либо; часто используется в значении 'оценивать ситуацию или человека'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "составлять мнение"
                            },
                            {
                                "target_lang_word": "смерить взглядом",
                                "target_lang_sense_clarification": "осматривать кого-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7df5ccc-718b-3ae6-9b7a-90b5fa75ee0b",
                    "sense_definition": "To alter or adjust something to a specific dimension or magnitude.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dimensions_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The jeweler <w>sized</w> the ring to fit her finger perfectly.",
                            "target_lang_translations": {
                                "ru": "Ювелир <w>подогнал по размеру</w> кольцо, чтобы оно идеально сидело на её пальце."
                            }
                        },
                        {
                            "text": "We need to <w>size</w> the image so it fits on the webpage.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>изменить размер</w> изображения, чтобы оно поместилось на веб-странице."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменять или подгонять что-либо под определенный размер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подгонять по размеру",
                                "target_lang_sense_clarification": "изменять параметры вещи"
                            },
                            {
                                "target_lang_word": "изменять размер",
                                "target_lang_sense_clarification": "масштабировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7bf52c2-0fa1-35cd-821d-07d0f643f457",
                    "sense_definition": "To classify, arrange, or sort items according to their physical dimensions.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dimensions_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The machine automatically <w>sizes</w> the fruit before packaging.",
                            "target_lang_translations": {
                                "ru": "Машина автоматически <w>сортирует</w> фрукты по размеру перед упаковкой."
                            }
                        },
                        {
                            "text": "Workers were asked to <w>size</w> the timber by length.",
                            "target_lang_translations": {
                                "ru": "Рабочих попросили <w>рассортировать</w> древесину по длине."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Классифицировать, распределять или сортировать предметы по их физическим параметрам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сортировать",
                                "target_lang_sense_clarification": "по размеру"
                            },
                            {
                                "target_lang_word": "калибровать",
                                "target_lang_sense_clarification": "техническая сортировка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b39e515-452f-375d-8280-7fdd49acdc85",
                    "sense_definition": "To treat a surface with a glaze, filler, or primer (size) to seal pores or stiffen it.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "material_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>sized</w> the paper to prevent the ink from bleeding.",
                            "target_lang_translations": {
                                "ru": "Он <w>проклеил</w> бумагу, чтобы чернила не расплывались."
                            }
                        },
                        {
                            "text": "The wall must be <w>sized</w> before you hang the heavy wallpaper.",
                            "target_lang_translations": {
                                "ru": "Стену необходимо <w>прогрунтовать</w> перед тем, как клеить тяжелые обои."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical or art context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обрабатывать поверхность грунтовкой или клеем для закупоривания пор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грунтовать",
                                "target_lang_sense_clarification": "обрабатывать поверхность"
                            },
                            {
                                "target_lang_word": "проклеивать",
                                "target_lang_sense_clarification": "наносить клей"
                            }
                        ]
                    }
                }
            ]
        }
    ]

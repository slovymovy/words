    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b750001a-d281-3441-85bd-5db12352ebb8",
                    "sense_definition": "Твёрдая горная порода зернистого строения, используемая в строительстве и архитектуре.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Набережная реки была облицована серым <w>гранитом</w>.",
                            "target_lang_translations": {
                                "en": "The river embankment was faced with gray <w>granite</w>."
                            }
                        },
                        {
                            "text": "Памятник Петру Первому сделан из бронзы и <w>гранита</w>.",
                            "target_lang_translations": {
                                "en": "The monument to Peter the Great is made of bronze and <w>granite</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "грызть гранит науки",
                        "серый гранит",
                        "холодный гранит"
                    ],
                    "target_lang_definitions": {
                        "en": "A hard igneous rock of granular structure used in construction and architecture."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "granite",
                                "target_lang_sense_clarification": "rock type"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dad28816-1198-3331-be6c-c60904f08dec",
                    "sense_definition": "Символ твёрдости, прочности и несокрушимости (переносно).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его характер — настоящий <w>гранит</w>, его невозможно сломить.",
                            "target_lang_translations": {
                                "en": "His character is true <w>granite</w>; he cannot be broken."
                            }
                        },
                        {
                            "text": "В своих убеждениях он был твёрд как <w>гранит</w>.",
                            "target_lang_translations": {
                                "en": "In his convictions, he was as hard as <w>granite</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "скала"
                    ],
                    "antonyms": [
                        "размазня"
                    ],
                    "common_phrases": [
                        "твёрд как гранит"
                    ],
                    "target_lang_definitions": {
                        "en": "A symbol of hardness, durability, and invincibility (figurative)."
                    },
                    "translations": {
                        "en": [
                            {

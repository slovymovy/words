    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "53d67783-306b-3884-856a-5c99a563334e",
                    "sense_definition": "W miejscu znajdującym się w dużej odległości od ziemi lub poziomu odniesienia; w górę.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samolot leci bardzo <w>wysoko</w> nad chmurami.",
                            "target_lang_translations": {
                                "ru": "Самолёт летит очень <w>высоко</w> над облаками."
                            }
                        },
                        {
                            "text": "Mieszkamy <w>wysoko</w>, bo aż na dziesiątym piętrze.",
                            "target_lang_translations": {
                                "ru": "Мы живём <w>высоко</w>, аж на десятом этаже."
                            }
                        }
                    ],
                    "antonyms": [
                        "nisko"
                    ],
                    "common_phrases": [
                        "wysoko w górach",
                        "latać wysoko",
                        "skakać wysoko",
                        "głowa wysoko"
                    ],
                    "target_lang_definitions": {
                        "ru": "На большом расстоянии от земли или уровня отсчета; вверху."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высоко",
                                "target_lang_sense_clarification": "о физическом расположении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10bed6ea-c1db-39ec-b50f-17a56fc2d672",
                    "sense_definition": "W dużym stopniu, w dużej ilości lub o dużej wartości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "degree_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma ubezpieczyła ten dom bardzo <w>wysoko</w>.",
                            "target_lang_translations": {
                                "ru": "Фирма застраховала этот дом на очень <w>крупную</w> сумму."
                            }
                        },
                        {
                            "text": "Nasza drużyna wygrała <w>wysoko</w> ostatni mecz.",
                            "target_lang_translations": {
                                "ru": "Наша команда <w>крупно</w> выиграла последний матч."
                            }
                        }
                    ],
                    "antonyms": [
                        "nisko"
                    ],
                    "common_phrases": [
                        "wysoko płatny",
                        "wysoko rozwinięty",
                        "wysoko wyceniony"
                    ],
                    "target_lang_definitions": {
                        "ru": "В значительной степени, в большом количестве или на большую сумму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крупно",
                                "target_lang_sense_clarification": "о счете в игре или сумме"
                            },
                            {
                                "target_lang_word": "значительно",
                                "target_lang_sense_clarification": "в большой степени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30368fe8-0804-3182-af54-0eca1b3f70cb",
                    "sense_definition": "W sposób świadczący o szacunku, uznaniu lub zajmowaniu ważnej pozycji w hierarchii.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "rank_esteem",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice bardzo <w>wysoko</w> cenią uczciwość.",
                            "target_lang_translations": {
                                "ru": "Мои родители очень <w>высоко</w> ценят честность."
                            }
                        },
                        {
                            "text": "On zaszedł <w>wysoko</w> w strukturach tej korporacji.",
                            "target_lang_translations": {
                                "ru": "Он <w>высоко</w> поднялся в структуре этой корпорации."
                            }
                        }
                    ],
                    "antonyms": [
                        "nisko"
                    ],
                    "common_phrases": [
                        "wysoko cenić",
                        "wysoko mierzyć",
                        "wysoko postawiony"
                    ],
                    "target_lang_definitions": {
                        "ru": "С уважением, признанием или занимая важное положение в иерархии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высоко",
                                "target_lang_sense_clarification": "о статусе или оценке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1582de17-1f33-3a37-b366-76ceaf5a8dec",
                    "sense_definition": "O dźwięku: tonem o dużej częstotliwości drgań.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie potrafię zaśpiewać tak <w>wysoko</w> jak ty.",
                            "target_lang_translations": {
                                "ru": "Я не могу спеть так <w>высоко</w>, как ты."
                            }
                        },
                        {
                            "text": "Skrzypce brzmiały czysto i <w>wysoko</w>.",
                            "target_lang_translations": {
                                "ru": "Скрипка звучала чисто и <w>высоко</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "nisko"
                    ],
                    "common_phrases": [
                        "śpiewać wysoko",
                        "brzmieć wysoko"
                    ],
                    "target_lang_definitions": {
                        "ru": "О звуке: на высокой частоте, тонким тоном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высоко",
                                "target_lang_sense_clarification": "о звуке или голосе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "85467537-4dae-33eb-bcda-978602007e2c",
                    "sense_definition": "Mający gęstą, zwartą strukturę; mocno ściśnięty lub ugnieciony.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "density",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Śnieg na stoku był twardy i <w>zbity</w>, co ułatwiało szybką jazdę.",
                            "target_lang_translations": {
                                "ru": "Снег на склоне был твердым и <w>утрамбованным</w>, что облегчало быструю езду."
                            }
                        },
                        {
                            "text": "Ziemia w doniczce jest zbyt <w>zbita</w>, więc woda nie wsiąka prawidłowo.",
                            "target_lang_translations": {
                                "ru": "Земля в горшке слишком <w>плотная</w>, поэтому вода не впитывается должным образом."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwarty",
                        "ścisły",
                        "gęsty",
                        "utwardzony"
                    ],
                    "antonyms": [
                        "luźny",
                        "rzadki"
                    ],
                    "common_phrases": [
                        "zbity śnieg",
                        "zbita masa",
                        "zbity tłum",
                        "zbita ziemia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий густую, плотную структуру; сильно сжатый или утрамбованный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плотный",
                                "target_lang_sense_clarification": "о структуре, массе"
                            },
                            {
                                "target_lang_word": "сбитый",
                                "target_lang_sense_clarification": "плотно соединенный, свалявшийся"
                            },
                            {
                                "target_lang_word": "утрамбованный",
                                "target_lang_sense_clarification": "о земле, снеге"

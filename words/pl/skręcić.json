    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b9785e52-db8f-3ecf-9dcb-0ef43f36cf9b",
                    "sense_definition": "Zmienić kierunek poruszania się; pójść lub pojechać w inną stronę.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na najbliższym skrzyżowaniu musisz <skręcić> w lewo, aby dojechać do centrum.",
                            "target_lang_translations": {
                                "ru": "На ближайшем перекрестке тебе нужно <повернуть> налево, чтобы доехать до центра."
                            }
                        },
                        {
                            "text": "Kierowca nagle <skręcił>, próbując ominąć przeszkodę na drodze.",
                            "target_lang_translations": {
                                "ru": "Водитель внезапно <свернул>, пытаясь объехать препятствие на дороге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skręcić w lewo",
                        "skręcić w prawo",
                        "skręcić na skrzyżowaniu",
                        "gwałtownie skręcić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить направление движения; пойти или поехать в другую сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повернуть",
                                "target_lang_sense_clarification": "сменить направление (налево, направо)"
                            },
                            {
                                "target_lang_word": "свернуть",
                                "target_lang_sense_clarification": "съехать с дороги или резко изменить курс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09c9d993-0e4d-39c6-b406-cf7327977d2d",
                    "sense_definition": "Uszkodzić staw, zwykle kostkę lub kolano, poprzez jego gwałtowne i nienaturalne wygięcie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas biegania w lesie niefortunnie upadł i <skręcił> prawą kostkę.",
                            "target_lang_translations": {
                                "ru": "Во время пробежки в лесу он неудачно упал и <подвернул> правую лодыжку."
                            }
                        },
                        {
                            "text": "To prawdziwy cud, że nikt nie <skręcił> karku w tym groźnym wypadku.",
                            "target_lang_translations": {
                                "ru": "Это настоящее чудо, что никто не <свернул> шею в этой страшной аварии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skręcić kostkę",
                        "skręcić nogę",
                        "skręcić kark",
                        "skręcić staw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повредить сустав, обычно лодыжку или колено, в результате резкого и неестественного изгиба."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвернуть",
                                "target_lang_sense_clarification": "легкая травма сустава (ноги, лодыжки)"
                            },
                            {
                                "target_lang_word": "свернуть",
                                "target_lang_sense_clarification": "серьезная травма или смещение (шею)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e72fb848-a19e-39e6-94f2-5514ebc35a28",
                    "sense_definition": "Połączyć elementy czegoś w całość za pomocą śrub lub wkrętów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "assembly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <skręcić> tę szafę dzisiaj wieczorem, zanim wniesiemy ją do sypialni.",
                            "target_lang_translations": {
                                "ru": "Мы должны <собрать> этот шкаф сегодня вечером, прежде чем внесем его в спальню."
                            }
                        },
                        {
                            "text": "Mój tata pomógł mi <skręcić> nowe biurko, które kupiłem do pracy.",
                            "target_lang_translations": {
                                "ru": "Мой папа помог мне <собрать> новый письменный стол, который я купил для работы."
                            }
                        }
                    ],
                    "antonyms": [
                        "rozkręcić"
                    ],
                    "common_phrases": [
                        "skręcić meble",
                        "skręcić szafę",
                        "skręcić regał",
                        "skręcić długopis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединить элементы чего-либо в единое целое с помощью винтов или шурупов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собрать",
                                "target_lang_sense_clarification": "мебель или конструкцию"
                            },
                            {
                                "target_lang_word": "скрутить",
                                "target_lang_sense_clarification": "соединить винтами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57d57e1d-29e2-35ab-b633-398d553cb4f4",
                    "sense_definition": "Zmniejszyć natężenie działania urządzenia, na przykład ogrzewania lub dźwięku, używając pokrętła.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "regulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W pokoju zrobiło się bardzo gorąco, więc <skręciłem> kaloryfer na minimum.",
                            "target_lang_translations": {
                                "ru": "В комнате стало очень жарко, поэтому я <убавил> батарею на минимум."
                            }
                        },
                        {
                            "text": "Proszę, <skręć> trochę radio, bo nie słyszę własnych myśli.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <прикрути> немного радио, а то я не слышу собственных мыслей."
                            }
                        }
                    ],
                    "synonyms": [
                        "przykręcić"
                    ],
                    "antonyms": [
                        "podkręcić"
                    ],
                    "common_phrases": [
                        "skręcić kaloryfer",
                        "skręcić gaz",
                        "skręcić głośność",
                        "skręcić ogrzewanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшить интенсивность работы устройства, например отопления или звука, используя регулятор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убавить",
                                "target_lang_sense_clarification": "уменьшить (звук, газ, огонь)"
                            },
                            {
                                "target_lang_word": "прикрутить",
                                "target_lang_sense_clarification": "разговорное; уменьшить поворотом вентиля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "289c1e9e-578d-3f39-8e4a-03b68d60bac7",
                    "sense_definition": "Zwinąć coś ciasno lub spleść ze sobą, nadając temu spiralny kształt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starszy mężczyzna usiadł na ławce i powoli <skręcił> sobie papierosa.",
                            "target_lang_translations": {
                                "ru": "Пожилой мужчина сел на скамейку и медленно <свернул> себе сигарету."
                            }
                        },
                        {
                            "text": "Musisz mocno <skręcić> te dwa druty, żeby połączenie było trwałe.",
                            "target_lang_translations": {
                                "ru": "Ты должен крепко <скрутить> эти два провода, чтобы соединение было прочным."
                            }
                        }
                    ],
                    "synonyms": [
                        "spleść"
                    ],
                    "antonyms": [
                        "rozprostować"
                    ],
                    "common_phrases": [
                        "skręcić papierosa",
                        "skręcić linę",
                        "skręcić włosy",
                        "skręcić bicz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плотно свернуть что-либо или сплести вместе, придавая спиральную форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрутить",
                                "target_lang_sense_clarification": "сплести (провода, веревку) или свернуть (сигарету)"
                            },
                            {
                                "target_lang_word": "свернуть",
                                "target_lang_sense_clarification": "скатать в трубку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cc0c865-a624-348d-b62d-476fc1d036d0",
                    "sense_definition": "Wygiąć ciało w nienaturalny sposób, często pod wpływem silnych emocji lub bólu (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała widownia <skręciła> się ze śmiechu po jego niezwykle zabawnym występie.",
                            "target_lang_translations": {
                                "ru": "Вся аудитория <корчилась> от смеха после его невероятно забавного выступления."
                            }
                        },
                        {
                            "text": "Chłopiec <skręcił> się z bólu, gdy piłka uderzyła go prosto w brzuch.",
                            "target_lang_translations": {
                                "ru": "Мальчик <скорчился> от боли, когда мяч ударил его прямо в живот."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skręcić się z bólu",
                        "skręcić się ze śmiechu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изогнуть тело неестественным образом, часто под влиянием сильных эмоций или боли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скорчиться",
                                "target_lang_sense_clarification": "согнуться от боли или смеха"
                            },
                            {
                                "target_lang_word": "скрутиться",
                                "target_lang_sense_clarification": "свернуться калачиком или от спазма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17ee3012-c213-32f2-b4e2-2d400643a789",
                    "sense_definition": "Zorganizować lub urządzić coś, zazwyczaj imprezę lub nieoficjalne spotkanie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopaki chcą <skręcić> jakąś szybką domówkę w ten weekend u Marka.",
                            "target_lang_translations": {
                                "ru": "Парни хотят <замутить> какую-нибудь быструю вечеринку в эти выходные у Марка."
                            }
                        },
                        {
                            "text": "Udało im się <skręcić> niezły interes w bardzo krótkim czasie.",
                            "target_lang_translations": {
                                "ru": "Им удалось <провернуть> неплохое дельце за очень короткое время."
                            }
                        }
                    ],
                    "synonyms": [
                        "zorganizować",
                        "urządzić"
                    ],
                    "common_phrases": [
                        "skręcić imprezę",
                        "skręcić melanż",
                        "skręcić interes"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Организовать или устроить что-либо, обычно вечеринку или неофициальную встречу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замутить",
                                "target_lang_sense_clarification": "сленг; организовать мероприятие"
                            },
                            {
                                "target_lang_word": "провернуть",
                                "target_lang_sense_clarification": "успешно осуществить (дело, аферу)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
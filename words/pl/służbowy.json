                        {
                            "text": "Pracownik otrzymał nowy telefon <w>służbowy</w> do kontaktu z klientami.",
                            "target_lang_translations": {
                                "ru": "Сотрудник получил новый <w>служебный</w> телефон для связи с клиентами."
                            }
                        }
                    ],
                    "synonyms": [
                        "firmowy",
                        "zawodowy"
                    ],
                    "antonyms": [
                        "prywatny",
                        "osobisty"
                    ],
                    "common_phrases": [

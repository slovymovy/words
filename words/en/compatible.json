{
    "word_family": [
        "compatibility",
        "compatibly"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d66ab58a-1bff-37cc-bb19-764c00e47b2f",
                    "sense_definition": "Capable of operating or working together with another device, system, or software without problems.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This printer isn't <w>compatible</w> with my computer."
                        },
                        {
                            "text": "Please check if the software is fully <w>compatible</w> with your operating system."
                        }
                    ],
                    "antonyms": [
                        "incompatible"
                    ],
                    "common_phrases": [
                        "compatible with",
                        "backward compatible",
                        "fully compatible"
                    ]
                },
                {
                    "sense_id": "12fc3ed3-c195-317d-aa6c-b10e92c7a558",
                    "sense_definition": "Able to have a good relationship or exist together without arguing or conflict.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "interpersonal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My neighbours and I are not very <w>compatible</w> because they are loud and I prefer quiet."
                        },
                        {
                            "text": "They are a very <w>compatible</w> couple who share many of the same interests."
                        }
                    ],
                    "antonyms": [
                        "incompatible"
                    ],
                    "common_phrases": [
                        "compatible couple",
                        "socially compatible"
                    ]
                },
                {
                    "sense_id": "2fb16ec4-d024-3e14-ae52-8679d43a7389",
                    "sense_definition": "Consistent or in agreement with another statement, fact, or principle.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "consistency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His actions were not <w>compatible</w> with his sermons about honesty."
                        },
                        {
                            "text": "We need to find a solution that is <w>compatible</w> with the company's budget."
                        }
                    ],
                    "antonyms": [
                        "incompatible"
                    ],
                    "common_phrases": [
                        "compatible with"
                    ]
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e92509a4-b194-35ed-8fb3-4fef96dab6fb",
                    "sense_definition": "A device, especially a computer, that can function with the software or hardware of another system.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The store sells various <w>compatibles</w> that cost less than the brand-name models."
                        },
                        {
                            "text": "In the early days of computing, many companies produced IBM <w>compatibles</w>."
                        }
                    ],
                    "common_phrases": [
                        "IBM compatible"
                    ]
                }
            ]
        }
    ]

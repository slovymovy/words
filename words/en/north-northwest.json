    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a4cd1b71-fd0f-3fe9-8b34-eb65d8805af9",
                    "sense_definition": "The compass point halfway between northwest and north.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "compass_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The needle on the compass settled pointing to the <w>north-northwest</w>.",
                            "target_lang_translations": {
                                "ru": "Стрелка компаса замерла, указывая на <w>северо-северо-запад</w>."
                            }
                        },
                        {
                            "text": "The storm is currently located to the <w>north-northwest</w> of the island.",
                            "target_lang_translations": {
                                "ru": "Шторм в настоящее время находится к <w>северо-северо-западу</w> от острова."
                            }
                        }
                    ],
                    "antonyms": [
                        "south-southeast"
                    ],
                    "common_phrases": [
                        "to the north-northwest",
                        "from the north-northwest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точка компаса, находящаяся посередине между северо-западом и севером"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "северо-северо-запад",
                                "target_lang_sense_clarification": "сторона света"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "258a7068-329d-3338-9827-b6c88566f62c",
                    "sense_definition": "Situated toward or in the direction of the point between north and northwest.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "compass_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain plotted a <w>north-northwest</w> course to avoid the rough seas.",
                            "target_lang_translations": {
                                "ru": "Капитан проложил <w>северо-северо-западный</w> курс, чтобы избежать сильной качки."
                            }
                        },
                        {
                            "text": "A cold <w>north-northwest</w> wind blew across the open fields all day.",
                            "target_lang_translations": {
                                "ru": "Холодный <w>северо-северо-западный</w> ветер дул над открытыми полями весь день."
                            }
                        }
                    ],
                    "antonyms": [
                        "south-southeast"
                    ],
                    "common_phrases": [
                        "north-northwest wind",
                        "north-northwest course"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный по направлению к точке между севером и северо-западом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "северо-северо-западный",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "dcb82565-9470-3495-88ed-84da2407412b",
                    "sense_definition": "Towards or in the direction of the point between north and northwest.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "compass_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are travelling <w>north-northwest</w> at the moment to reach the harbor.",
                            "target_lang_translations": {
                                "ru": "В данный момент мы движемся <w>на северо-северо-запад</w>, чтобы добраться до гавани."
                            }
                        },
                        {
                            "text": "The flock of migrating birds flew <w>north-northwest</w> across the sky.",
                            "target_lang_translations": {
                                "ru": "Стая перелетных птиц летела <w>на северо-северо-запад</w> по небу."
                            }
                        }
                    ],
                    "antonyms": [
                        "south-southeast"
                    ],
                    "common_phrases": [
                        "travel north-northwest",
                        "bearing north-northwest"
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении точки между севером и северо-западом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на северо-северо-запад",
                                "target_lang_sense_clarification": "направление движения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

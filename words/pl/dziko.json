            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "67154efa-add5-3abe-ae0b-1ba6c4709451",
                    "sense_definition": "W warunkach naturalnych, bez opieki lub ingerencji człowieka.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele rzadkich gatunków roślin rośnie <w>dziko</w> w tej dolinie.",
                            "target_lang_translations": {
                                "ru": "Многие редкие виды растений растут <w>в диком виде</w> в этой долине."
                            }
                        },
                        {
                            "text": "Koty te żyją <w>dziko</w> i rzadko zbliżają się do ludzkich osiedli.",
                            "target_lang_translations": {
                                "ru": "Эти коты живут <w>на воле</w> и редко приближаются к человеческим поселениям."
                            }
                        }
                    ],
                    "synonyms": [
                        "naturalnie"
                    ],
                    "antonyms": [
                        "hodowlanie",
                        "domowo"
                    ],
                    "common_phrases": [
                        "rosnąć dziko",
                        "żyć dziko"
                    ],
                    "target_lang_definitions": {
                        "ru": "В естественных условиях, без ухода или вмешательства человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дико",
                                "target_lang_sense_clarification": "в естественной среде"
                            },
                            {
                                "target_lang_word": "в диком виде",
                                "target_lang_sense_clarification": "о растениях"
                            },
                            {
                                "target_lang_word": "на воле",
                                "target_lang_sense_clarification": "о животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87e7d4e8-0874-3dd1-bcd5-a1283561e707",
                    "sense_definition": "W sposób gwałtowny, nieopanowany, pełen silnych emocji lub chaosu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej serce zaczęło bić <w>dziko</w> ze strachu przed nieznanym.",
                            "target_lang_translations": {
                                "ru": "Ее сердце начало <w>дико</w> биться от страха перед неизвестным."
                            }
                        },
                        {
                            "text": "Tłum bawił się <w>dziko</w> przy głośnej muzyce do białego rana.",
                            "target_lang_translations": {
                                "ru": "Толпа <w>дико</w> веселилась под громкую музыку до самого утра."
                            }
                        }
                    ],
                    "synonyms": [
                        "gwałtownie",
                        "szaleńczo",
                        "żywiołowo"
                    ],
                    "antonyms": [
                        "spokojnie",
                        "łagodnie",
                        "opanowanie"
                    ],
                    "common_phrases": [
                        "bić dziko",
                        "krzyczeć dziko",
                        "bawić się dziko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бурно, неудержимо, с сильными эмоциями или хаотично."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дико",
                                "target_lang_sense_clarification": "неистово"
                            },
                            {
                                "target_lang_word": "бешено",
                                "target_lang_sense_clarification": "яростно, интенсивно"
                            },
                            {
                                "target_lang_word": "неистово",
                                "target_lang_sense_clarification": "без ограничений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1242a11d-7e2d-3fcb-8a42-c136a0214c53",
                    "sense_definition": "Bardzo, niezwykle, w ogromnym stopniu (używane dla podkreślenia intensywności cechy lub uczucia).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poczuła się nagle <w>dziko</w> szczęśliwa, że on jest obok niej.",
                            "target_lang_translations": {
                                "ru": "Она вдруг почувствовала себя <w>дико</w> счастливой от того, что он рядом."
                            }
                        },
                        {
                            "text": "Jestem <w>dziko</w> głodny po całym dniu ciężkiej pracy w polu.",
                            "target_lang_translations": {
                                "ru": "Я <w>дико</w> голоден после целого дня тяжелой работы в поле."
                            }
                        }
                    ],
                    "synonyms": [
                        "bardzo",
                        "niesamowicie",
                        "szalenie",
                        "okropnie"
                    ],
                    "common_phrases": [
                        "dziko szczęśliwy",
                        "dziko przystojny"
                    ],
                    "traits": [

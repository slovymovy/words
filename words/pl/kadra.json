                    }
                },
                {
                    "sense_id": "e804cd6c-1794-330a-9766-5ccd484e8a75",
                    "sense_definition": "Zespół wykwalifikowanych pracowników danej instytucji lub przedsiębiorstwa",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "personnel_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szkoła szczyci się doskonale wykształconą <w>kadrą</w> pedagogiczną.",

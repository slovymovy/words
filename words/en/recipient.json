                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>recipient</w> country benefited greatly from the foreign aid.",
                            "target_lang_translations": {
                                "ru": "Страна-<w>получатель</w> получила значительную выгоду от иностранной помощи."
                            }
                        },
                        {
                            "text": "We need to identify the <w>recipient</w> bank account for the transfer.",
                            "target_lang_translations": {
                                "ru": "Нам нужно определить банковский счет <w>получателя</w> для перевода."
                            }
                        }
                    ],
                    "common_phrases": [
                        "recipient country",
                        "recipient account"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполняющий функцию получения чего-либо; принимающий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получатель",
                                "target_lang_sense_clarification": "в составе устойчивых сочетаний (напр. страна-получатель)"
                            },
                            {
                                "target_lang_word": "принимающий",
                                "target_lang_sense_clarification": "прилагательное/причастие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

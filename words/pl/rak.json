            "senses": [
                {
                    "sense_id": "43d8b843-44a9-3efa-9149-a8f1f6b4c441",
                    "sense_definition": "Słodkowodny skorupiak o twardym pancerzu i dużych szczypcach, żyjący w czystych wodach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W czystym strumieniu można czasem spotkać <w>raka</w>.",
                            "target_lang_translations": {
                                "ru": "В чистом ручье иногда можно встретить <w>рака</w>."
                            }
                        },
                        {
                            "text": "Na obiad podano gotowane <w>raki</w> z koperkiem.",
                            "target_lang_translations": {
                                "ru": "На обед подали вареных <w>раков</w> с укропом."
                            }
                        },
                        {
                            "text": "Kiedy go o to zapytała, zrobił się czerwony jak <w>rak</w>.",
                            "target_lang_translations": {
                                "ru": "Когда она спросила его об этом, он покраснел как <w>рак</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czerwony jak rak",
                        "wycofać się rakiem",
                        "pokazać gdzie raki zimują",
                        "spiec się na raka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пресноводное ракообразное с твердым панцирем и большими клешнями, обитающее в чистой воде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рак",
                                "target_lang_sense_clarification": "животное"

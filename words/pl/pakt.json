                            },
                            {
                                "target_lang_word": "сделка",
                                "target_lang_sense_clarification": "договор, часто с оттенком компромисса"
                            },
                            {
                                "target_lang_word": "пакт",
                                "target_lang_sense_clarification": "торжественный договор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8196573c-031e-37ff-9c04-d1fc8ba61a3c",
                    "sense_definition": "Grupa państw, które zawarły ze sobą umowę o charakterze politycznym lub wojskowym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "political_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały <w>pakt</w> zdecydował o nałożeniu sankcji gospodarczych na agresora.",
                            "target_lang_translations": {
                                "ru": "Весь <w>блок</w> решил наложить экономические санкции на агрессора."
                            }
                        },
                        {
                            "text": "Do <w>paktu</w> dołączyły w tym roku dwa nowe państwa członkowskie.",
                            "target_lang_translations": {
                                "ru": "К <w>пакту</w> в этом году присоединились два новых государства-члена."
                            }
                        }
                    ],
                    "synonyms": [
                        "układ",
                        "traktat"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8064d1c8-8994-3598-aff8-282b4033e2e3",
                    "sense_definition": "Całokształt działań rodziców i opiekunów mających na celu ukształtowanie osobowości, moralności i zachowania dziecka; przygotowanie do życia w społeczeństwie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "upbringing_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dialog między rodzicami a dziećmi jest podstawą <w>wychowania</w>.",
                            "target_lang_translations": {
                                "ru": "Диалог между родителями и детьми — это основа <w>воспитания</w>."
                            }
                        },
                        {
                            "text": "Rodzice poświęcają wiele czasu na dobre <w>wychowanie</w> swoich dzieci.",
                            "target_lang_translations": {
                                "ru": "Родители посвящают много времени хорошему <w>воспитанию</w> своих детей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dobre wychowanie",
                        "trudne wychowanie",
                        "błędy w wychowaniu",
                        "wychowanie dzieci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность действий родителей и опекунов, направленных на формирование личности, нравственности и поведения ребенка; подготовка к жизни в обществе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспитание",
                                "target_lang_sense_clarification": "процесс формирования личности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc535025-7b23-3147-90e2-091b00d0813e",
                    "sense_definition": "Proces kształcenia i rozwoju w określonej dziedzinie, najczęściej używane w nazwach przedmiotów szkolnych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "education_subject",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdy wtorek mamy w szkole <w>wychowanie</w> fizyczne.",
                            "target_lang_translations": {
                                "ru": "Каждый вторник у нас в школе физическое <w>воспитание</w>."
                            }
                        },
                        {
                            "text": "Nauczycielka prowadzi <w>wychowanie</w> plastyczne dla najmłodszych uczniów.",
                            "target_lang_translations": {
                                "ru": "Учительница ведет художественное <w>воспитание</w> для младших школьников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wychowanie fizyczne",
                        "wychowanie obywatelskie",
                        "wychowanie do życia w rodzinie",
                        "wychowanie muzyczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс обучения и развития в определенной области, чаще всего используется в названиях школьных предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспитание",
                                "target_lang_sense_clarification": "в названиях дисциплин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "315ed845-11eb-31ca-a989-51a292d504eb",
                    "sense_definition": "Umiejętność właściwego zachowania się w towarzystwie, dobre maniery i ogłada towarzyska.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "manners",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego aroganckie zachowanie przy stole świadczy o braku <w>wychowania</w>.",
                            "target_lang_translations": {
                                "ru": "Его высокомерное поведение за столом свидетельствует об отсутствии <w>воспитания</w>."
                            }
                        },
                        {
                            "text": "To jest bardzo uprzejma kobieta o nienagannym <w>wychowaniu</w>.",
                            "target_lang_translations": {
                                "ru": "Это очень вежливая женщина с безупречным <w>воспитанием</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykształcenie"
                    ],
                    "common_phrases": [
                        "brak wychowania",
                        "człowiek o wysokim wychowaniu",
                        "nienaganne wychowanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умение правильно вести себя в обществе, хорошие манеры и светский этикет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспитание",
                                "target_lang_sense_clarification": "хорошие манеры"
                            },
                            {
                                "target_lang_word": "воспитанность",
                                "target_lang_sense_clarification": "умение вести себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72dc494a-9084-30bb-b3da-db93f2b3b1ec",
                    "sense_definition": "Zapewnienie komuś środków do życia i utrzymania.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "maintenance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawniej dwór królewski łożył na <w>wychowanie</w> licznej służby i wojska.",
                            "target_lang_translations": {
                                "ru": "Раньше королевский двор тратился на <w>содержание</w> многочисленной прислуги и войска."
                            }
                        },
                        {
                            "text": "Bogaty stryj zapewnił sierocie <w>wychowanie</w> i dach nad głową.",
                            "target_lang_translations": {
                                "ru": "Богатый дядя обеспечил сироте <w>содержание</w> и крышу над головой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечение кого-либо средствами к существованию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "содержание",
                                "target_lang_sense_clarification": "обеспечение средствами"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "17f84d5e-eec0-3667-be1e-e955b1e0b15e",
                    "sense_definition": "Избавиться от чего-либо, выбросить как ненужное.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_discard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я решил <w>выкинуть</w> старые газеты и журналы.",
                            "target_lang_translations": {
                                "en": "I decided to <w>throw away</w> the old newspapers and magazines."
                            }
                        },
                        {
                            "text": "Не забудь <w>выкинуть</w> мусор перед уходом.",
                            "target_lang_translations": {
                                "en": "Don't forget to <w>throw out</w> the trash before leaving."
                            }
                        }
                    ],
                    "synonyms": [
                        "выбросить"
                    ],
                    "common_phrases": [
                        "выкинуть мусор",
                        "выкинуть хлам",
                        "выкинуть на помойку",
                        "выкинуть старые вещи"
                    ],
                    "target_lang_definitions": {
                        "en": "To get rid of something; to discard as useless."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throw away",
                                "target_lang_sense_clarification": "general term for discarding something"
                            },
                            {
                                "target_lang_word": "throw out",
                                "target_lang_sense_clarification": "synonym for throw away, often used for trash"
                            },
                            {
                                "target_lang_word": "discard",
                                "target_lang_sense_clarification": "more formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6697bb94-0e49-36ac-9048-b7c905541b82",

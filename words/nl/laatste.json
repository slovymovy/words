    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "740fd841-f450-39bf-b3d7-4a9f1c09b408",
                    "sense_definition": "In een reeks door niets meer gevolgd worden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "last_final",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>laatste</w> deelnemer had veel meer tijd nodig dan de eerste.",
                            "target_lang_translations": {
                                "ru": "<w>Последнему</w> участнику потребовалось гораздо больше времени, чем первому."
                            }
                        },
                        {
                            "text": "Dit zou toch niet mijn <w>laatste</w> nacht op aarde worden?",
                            "target_lang_translations": {
                                "ru": "Неужели это будет моя <w>последняя</w> ночь на земле?"
                            }
                        }
                    ],
                    "synonyms": [
                        "achterste"
                    ],
                    "antonyms": [
                        "eerste"
                    ],
                    "common_phrases": [
                        "de laatste keer",
                        "de laatste dag",
                        "de laatste loodjes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в самом конце ряда; такой, за которым больше ничего не следует."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последний",
                                "target_lang_sense_clarification": "конечный в ряду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1c45ce0-3d33-35fc-9e55-d1a354584ba1",
                    "sense_definition": "Verbogen vorm van de overtreffende trap van <w>laat</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "latest_recent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze wilden graag het <w>laatste</w> nieuws horen.",
                            "target_lang_translations": {
                                "ru": "Они очень хотели услышать <w>последние</w> новости."
                            }
                        },
                        {
                            "text": "Heb je de <w>laatste</w> film van die regisseur al gezien?",
                            "target_lang_translations": {
                                "ru": "Ты уже видел <w>последний</w> фильм этого режиссёра?"
                            }
                        }
                    ],
                    "synonyms": [
                        "nieuwste",
                        "recentste"
                    ],
                    "antonyms": [
                        "oudste",
                        "eerste"
                    ],
                    "common_phrases": [
                        "het laatste nieuws",
                        "de laatste mode",
                        "de laatste tijd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый недавний; появившийся или произошедший совсем недавно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последний",
                                "target_lang_sense_clarification": "самый недавний"
                            },
                            {
                                "target_lang_word": "новейший",
                                "target_lang_sense_clarification": "самый новый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "27498318-55ee-3ecf-8851-f6dbc487a4a4",
                    "sense_definition": "Wie of wat niet meer door anderen wordt gevolgd.",

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "10c8bf42-1637-37a2-a215-0c7c0a3ff980",
                    "sense_definition": "Occurring or happening regularly or frequently; usual.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "frequency_abundance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>common</w> to see snow in this region during winter.",
                            "target_lang_translations": {
                                "ru": "В этом регионе зимой <w>обычно</w> выпадает снег."
                            }
                        },
                        {
                            "text": "Back pain is a <w>common</w> problem for people who sit all day.",
                            "target_lang_translations": {
                                "ru": "Боль в спине — <w>частая</w> проблема для людей, которые сидят весь день."
                            }
                        }
                    ],
                    "synonyms": [
                        "usual",
                        "frequent",
                        "regular",
                        "everyday",
                        "normal"
                    ],
                    "antonyms": [
                        "rare",
                        "unusual",
                        "uncommon",
                        "extraordinary"
                    ],
                    "common_phrases": [
                        "common practice",
                        "common mistake",
                        "common occurrence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий регулярно или часто; обычный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обычный",
                                "target_lang_sense_clarification": "usual, normal"
                            },
                            {
                                "target_lang_word": "частый",
                                "target_lang_sense_clarification": "frequent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82c5f9a4-e592-3229-a69f-afe9a1afb7a5",
                    "sense_definition": "Found in large numbers or in a large quantity.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "frequency_abundance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pigeons are very <w>common</w> in the city square.",
                            "target_lang_translations": {
                                "ru": "Голуби очень <w>распространены</w> на городской площади."
                            }
                        },
                        {
                            "text": "This type of tree is <w>common</w> throughout the forest.",
                            "target_lang_translations": {
                                "ru": "Этот вид деревьев <w>распространён</w> по всему лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "widespread",
                        "abundant",
                        "prevalent",
                        "numerous"
                    ],
                    "antonyms": [
                        "rare",
                        "endangered",
                        "exotic",
                        "unique"
                    ],
                    "common_phrases": [
                        "common sight",
                        "common species"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встречающийся в больших количествах; широко представленный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распространённый",
                                "target_lang_sense_clarification": "widespread"
                            },
                            {
                                "target_lang_word": "повсеместный",
                                "target_lang_sense_clarification": "ubiquitous"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cdefff1-bced-3ffb-81c4-c064acb11180",
                    "sense_definition": "Shared by, or belonging to, two or more people or groups.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shared_mutual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two countries share a <w>common</w> border.",
                            "target_lang_translations": {
                                "ru": "У двух стран есть <w>общая</w> граница."
                            }
                        },
                        {
                            "text": "We found that we have a <w>common</w> interest in classical music.",
                            "target_lang_translations": {
                                "ru": "Мы обнаружили, что у нас есть <w>общий</w> интерес к классической музыке."
                            }
                        }
                    ],
                    "synonyms": [
                        "shared",
                        "mutual",
                        "joint"
                    ],
                    "antonyms": [
                        "individual",
                        "personal",
                        "sole",
                        "special"
                    ],
                    "common_phrases": [
                        "common goal",
                        "common ground",
                        "have in common",
                        "common interest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий двум или более людям или группам; совместный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общий",
                                "target_lang_sense_clarification": "shared, mutual"
                            },
                            {
                                "target_lang_word": "совместный",
                                "target_lang_sense_clarification": "joint"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5ae4c4b-cfa1-32aa-bc9d-708cc6b1451c",
                    "sense_definition": "Existing among virtually all people; universal.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "shared_mutual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>common</w> knowledge that the earth orbits the sun.",
                            "target_lang_translations": {
                                "ru": "То, что Земля вращается вокруг Солнца, — <w>общеизвестный</w> факт."
                            }
                        },
                        {
                            "text": "Using <w>common</w> sense is often better than following complex rules.",
                            "target_lang_translations": {
                                "ru": "Использование <w>здравого</w> смысла часто лучше, чем следование сложным правилам."
                            }
                        }
                    ],
                    "synonyms": [
                        "universal",
                        "general"
                    ],
                    "common_phrases": [
                        "common knowledge",
                        "common sense",
                        "common decency"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий практически среди всех людей; всеобщий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общеизвестный",
                                "target_lang_sense_clarification": "in 'common knowledge'"
                            },
                            {
                                "target_lang_word": "здравый",
                                "target_lang_sense_clarification": "in 'common sense'"
                            },
                            {
                                "target_lang_word": "всеобщий",
                                "target_lang_sense_clarification": "universal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c599937e-1886-38e2-b1e7-e929304b2d22",
                    "sense_definition": "Ordinary and not special; sometimes implying low social status or lack of refinement.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ordinary_vulgar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was treated like a <w>common</w> criminal despite his high status.",
                            "target_lang_translations": {
                                "ru": "Несмотря на его высокий статус, с ним обращались как с <w>обычным</w> преступником."
                            }
                        },
                        {
                            "text": "The politician claimed to speak for the <w>common</w> man.",
                            "target_lang_translations": {
                                "ru": "Политик утверждал, что говорит от имени <w>простого</w> человека."
                            }
                        }
                    ],
                    "synonyms": [
                        "ordinary",
                        "plain",
                        "average",
                        "vulgar"
                    ],
                    "antonyms": [
                        "exceptional",
                        "extraordinary",
                        "noteworthy",
                        "unconventional"
                    ],
                    "common_phrases": [
                        "common man",
                        "common people"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обыкновенный, ничем не примечательный; иногда подразумевает низкий социальный статус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простой",
                                "target_lang_sense_clarification": "ordinary, plain"
                            },
                            {
                                "target_lang_word": "обыкновенный",
                                "target_lang_sense_clarification": "average, not special"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fefbc597-7361-3ff0-bb79-cafa3d32b84e",
                    "sense_definition": "Of a noun: referring to a class of entities rather than a specific individual.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In English, a <w>common</w> noun is usually not capitalized unless it starts a sentence.",
                            "target_lang_translations": {
                                "ru": "В английском языке <w>нарицательное</w> существительное обычно пишется со строчной буквы, если оно не стоит в начале предложения."
                            }
                        },
                        {
                            "text": "The teacher asked the students to identify every <w>common</w> noun in the paragraph.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников найти каждое <w>нарицательное</w> существительное в абзаце."
                            }
                        }
                    ],
                    "synonyms": [
                        "appellative"
                    ],
                    "common_phrases": [
                        "common noun"
                    ],
                    "target_lang_definitions": {
                        "ru": "О существительном: обозначающее класс объектов, а не конкретный индивидуум."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарицательный",
                                "target_lang_sense_clarification": "grammar term"
                            }
                        ]
                    }
                }
            ]
        },
        {

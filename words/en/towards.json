    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "18f3fa77-e157-31ed-b462-7113de0e816a",
                    "sense_definition": "In the direction of someone or something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To the left towers the Jungfrau, with the train heading directly <w>towards</w> it.",
                            "target_lang_translations": {
                                "ru": "Слева возвышается Юнгфрау, и поезд направляется прямо <w>к</w> ней."
                            }
                        },
                        {
                            "text": "She started walking <w>towards</w> the city center.",
                            "target_lang_translations": {
                                "ru": "Она пошла <w>в сторону</w> центра города."
                            }
                        }
                    ],
                    "synonyms": [
                        "toward"
                    ],
                    "antonyms": [
                        "fromwards"
                    ],
                    "common_phrases": [
                        "walk towards",
                        "move towards",
                        "look towards",
                        "turn towards"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "alternative form of toward"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "предлог направления (указывает на приближение)"
                            },
                            {
                                "target_lang_word": "в сторону",
                                "target_lang_sense_clarification": "указывает на общее направление движения"
                            },
                            {
                                "target_lang_word": "по направлению к",
                                "target_lang_sense_clarification": "формальное указание направления"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "aed4a7a9-0810-3f34-a755-7d776f16a235",
                    "sense_definition": "In the direction of something indicated by context.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thus as he spake, lo far away they spyde / A varlet running <w>towards</w> hastily.",
                            "target_lang_translations": {
                                "ru": "Пока он говорил, они заметили вдалеке слугу, поспешно бегущего <w>навстречу</w>."
                            }
                        },
                        {
                            "text": "He saw the light in the distance and moved <w>towards</w>.",
                            "target_lang_translations": {
                                "ru": "Он увидел свет вдалеке и двинулся <w>в ту сторону</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "toward"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "usage as an adverb without an object is largely archaic or dialectal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении чего-либо, понятного из контекста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навстречу",
                                "target_lang_sense_clarification": "движение по направлению к кому-либо/чему-либо"
                            },
                            {
                                "target_lang_word": "в ту сторону",
                                "target_lang_sense_clarification": "указание направления"
                            }
                        ]
                    }
                }
            ]
        },
        {

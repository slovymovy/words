{
    "word_family": [
        "remain",
        "remains",
        "remnant"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "841c1a26-614d-3a1c-8166-243dfdc0cde8",
                    "sense_definition": "The amount left over after subtracting the divisor as many times as possible from the dividend without producing a negative result.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "math_calc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Seven divided by two gives a quotient of three and a <w>remainder</w> of one.",
                            "target_lang_translations": {
                                "ru": "Семь, деленное на два, дает частное три и <w>остаток</w> один."
                            }
                        },
                        {
                            "text": "If you divide ten by five, the <w>remainder</w> is zero.",
                            "target_lang_translations": {
                                "ru": "Если разделить десять на пять, <w>остаток</w> будет равен нулю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "remainder of",
                        "quotient and remainder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Величина, остающаяся после деления одного числа на другое, если они не делятся нацело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "математический термин (при делении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e06d764a-1750-36c6-b8a7-f62e498cc75e",
                    "sense_definition": "A part or parts remaining after some has been removed or has already occurred.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We spent the <w>remainder</w> of the day walking around the city.",
                            "target_lang_translations": {
                                "ru": "Мы провели <w>остаток</w> дня, гуляя по городу."
                            }
                        },
                        {
                            "text": "She ate half of the sandwich and saved the <w>remainder</w> for later.",
                            "target_lang_translations": {
                                "ru": "Она съела половину сэндвича и оставила <w>остальное</w> на потом."
                            }
                        }
                    ],
                    "synonyms": [
                        "rest",
                        "remains",
                        "remnant",
                        "leftover"
                    ],
                    "common_phrases": [
                        "for the remainder",
                        "the remainder of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть или части, оставшиеся после того, как что-то было убрано, использовано или уже произошло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "оставшаяся часть"
                            },
                            {
                                "target_lang_word": "остальное",
                                "target_lang_sense_clarification": "то, что осталось"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ea38db8-f7dc-3484-b4f6-2f55f2d3af91",
                    "sense_definition": "The number left over after a simple subtraction.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "math_calc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When you subtract five from twelve, the <w>remainder</w> is seven.",
                            "target_lang_translations": {
                                "ru": "Если вычесть пять из двенадцати, <w>разность</w> будет равна семи."
                            }
                        },
                        {
                            "text": "Calculate the <w>remainder</w> after deducting the expenses from the total.",
                            "target_lang_translations": {
                                "ru": "Рассчитайте <w>остаток</w> после вычета расходов из общей суммы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Число, получаемое в результате вычитания; разница между уменьшаемым и вычитаемым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разность",
                                "target_lang_sense_clarification": "результат вычитания"
                            },
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "баланс, оставшаяся сумма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc633d66-029f-34de-93e1-68a4bd09edc0",
                    "sense_definition": "Excess stock items left unsold and subject to reduction in price.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "commerce_stock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I bought this hardcover book cheaply because it was a <w>remainder</w>.",
                            "target_lang_translations": {
                                "ru": "Я дешево купил эту книгу в твердом переплете, потому что это был <w>уцененный экземпляр</w>."
                            }
                        },
                        {
                            "text": "The shop placed every <w>remainder</w> in a discount bin near the entrance.",
                            "target_lang_translations": {
                                "ru": "Магазин поместил все <w>остатки</w> в корзину для уцененных товаров у входа."
                            }
                        }
                    ],
                    "synonyms": [
                        "surplus",
                        "leftover"
                    ],
                    "antonyms": [
                        "shortage",
                        "deficit"
                    ],
                    "common_phrases": [
                        "remainder bin",
                        "remainder book"
                    ],
                    "target_lang_definitions": {
                        "ru": "Излишки товаров, оставшиеся нераспроданными и подлежащие уценке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остатки",
                                "target_lang_sense_clarification": "нераспроданный товар"
                            },
                            {
                                "target_lang_word": "неликвид",
                                "target_lang_sense_clarification": "залежалый товар"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9e2b58b-f23b-3c82-9eb5-9710f4780e25",
                    "sense_definition": "An estate in expectancy which only comes in its heir's possession after a prior estate has ended.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "law_estate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>remainder</w> of the property will pass to his children upon his death.",
                            "target_lang_translations": {
                                "ru": "<w>Оставшаяся часть</w> имущества перейдет к его детям после его смерти."
                            }
                        },
                        {
                            "text": "She holds a vested <w>remainder</w> in the family estate.",
                            "target_lang_translations": {
                                "ru": "Она владеет закрепленным <w>правом на остаточную собственность</w> в семейном поместье."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имущественное право, которое переходит во владение наследника только после прекращения действия предыдущего имущественного права."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "71aa65e9-20b3-3d91-88c8-ca9013a5c687",
                    "sense_definition": "To mark or declare items left unsold as subject to reduction in price.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "commerce_stock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The publisher decided to <w>remainder</w> the unsold copies of the biography.",
                            "target_lang_translations": {
                                "ru": "Издатель решил <w>уценить</w> непроданные экземпляры биографии."
                            }
                        },
                        {
                            "text": "After the holiday season, the store <w>remaindered</w> all the themed calendars.",
                            "target_lang_translations": {
                                "ru": "После праздничного сезона магазин <w>распродал</w> все тематические календари <w>со скидкой</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помечать или объявлять нераспроданные товары подлежащими уценке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уценять",
                                "target_lang_sense_clarification": "снижать цену на остатки"
                            },
                            {
                                "target_lang_word": "распродавать остатки",
                                "target_lang_sense_clarification": "ликвидировать товар"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4648ce5c-d3e9-34e1-b07a-32623088eb38",
                    "sense_definition": "Remaining; being left over.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "general_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>remainder</w> ingredients should be mixed into the bowl slowly.",
                            "target_lang_translations": {
                                "ru": "<w>Оставшиеся</w> ингредиенты следует медленно смешать в миске."
                            }
                        },
                        {
                            "text": "Please distribute the <w>remainder</w> funds to the charity.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, распределите <w>оставшиеся</w> средства на благотворительность."
                            }
                        }
                    ],
                    "synonyms": [
                        "leftover"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставшийся; являющийся остатком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставшийся",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eb58ded9-dfb0-3037-837b-f009a5154262",
                    "sense_definition": "Broń miotająca składająca się ze sprężystego pręta i cięciwy, służąca do wystrzeliwania strzał",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Średniowieczny rycerz potrafił celnie strzelać, używając prostego drewnianego <w>łuku</w>.",
                            "target_lang_translations": {
                                "ru": "Средневековый рыцарь умел метко стрелять, используя простой деревянный <w>лук</w>."
                            }
                        },
                        {
                            "text": "Zawodnik napiął <w>łuk</w> i skupił się na tarczy przed oddaniem strzału.",
                            "target_lang_translations": {
                                "ru": "Спортсмен натянул <w>лук</w> и сосредоточился на мишени перед выстрелом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strzelać z łuku",
                        "napiąć łuk",
                        "łuk i strzały"
                    ],
                    "target_lang_definitions": {
                        "ru": "Метательное оружие, состоящее из гибкого стержня и тетивы, служащее для стрельбы стрелами"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лук",
                                "target_lang_sense_clarification": "оружие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00dc83ea-2232-3ae0-9994-31c7ed9ba858",
                    "sense_definition": "Linia krzywa lub przedmiot wygięty w kształt półkola",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shape_curve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód pokonał zakręt szerokim <w>łukiem</w>, nie zwalniając tempa.",
                            "target_lang_translations": {
                                "ru": "Автомобиль преодолел поворот по широкой <w>дуге</w>, не сбавляя скорости."
                            }
                        },
                        {
                            "text": "Rzeka płynie tutaj łagodnym <w>łukiem</w> przez malowniczą dolinę.",
                            "target_lang_translations": {
                                "ru": "Река течет здесь плавной <w>дугой</w> через живописную долину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "omijać szerokim łukiem",
                        "szerokim łukiem",
                        "w kształcie łuku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кривая линия или предмет, изогнутый в форме полукруга"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуга",
                                "target_lang_sense_clarification": "кривая линия"
                            },
                            {
                                "target_lang_word": "изгиб",
                                "target_lang_sense_clarification": "форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93efe09e-b250-35de-b639-c06e312cd910",
                    "sense_definition": "Element architektoniczny o kształcie łukowatym, zamykający otwór w murze lub stanowiący element konstrukcyjny",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Paryżu turyści bardzo chętnie odwiedzają słynny <w>Łuk</w> Triumfalny.",
                            "target_lang_translations": {
                                "ru": "В Париже туристы очень охотно посещают знаменитую Триумфальную <w>арку</w>."
                            }
                        },
                        {
                            "text": "Stare gotyckie katedry charakteryzują się ostro zakończonymi <w>łukami</w>.",
                            "target_lang_translations": {
                                "ru": "Старые готические соборы характеризуются остроконечными <w>арками</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łuk triumfalny",
                        "łuk gotycki",
                        "sklepienie łukowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Архитектурный элемент дугообразной формы, перекрывающий проем в стене или являющийся элементом конструкции"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арка",
                                "target_lang_sense_clarification": "архитектура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "371621d2-c4c2-3090-a755-d3e645deef6f",
                    "sense_definition": "Fragment okręgu lub innej krzywej ograniczony dwoma punktami",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "shape_curve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lekcji geometrii obliczaliśmy długość <w>łuku</w> wycinka koła.",
                            "target_lang_translations": {
                                "ru": "На уроке геометрии мы вычисляли длину <w>дуги</w> сектора круга."
                            }
                        },
                        {
                            "text": "Kąt wpisany oparty na tym samym <w>łuku</w> co kąt środkowy jest od niego dwa razy mniejszy.",
                            "target_lang_translations": {
                                "ru": "Вписанный угол, опирающийся на ту же <w>дугу</w>, что и центральный угол, в два раза меньше него."
                            }
                        }
                    ],
                    "common_phrases": [
                        "długość łuku",
                        "łuk okręgu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть окружности или другой кривой, ограниченная двумя точками"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуга",
                                "target_lang_sense_clarification": "геометрия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bd66e42-b74c-30be-90c4-09d716450aab",
                    "sense_definition": "Ciągłe wyładowanie elektryczne w gazie między dwiema elektrodami, dające silne światło",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas spawania powstaje jasny <w>łuk</w>, który może uszkodzić wzrok bez odpowiedniej ochrony.",
                            "target_lang_translations": {
                                "ru": "Во время сварки возникает яркая <w>дуга</w>, которая может повредить зрение без соответствующей защиты."
                            }
                        },
                        {
                            "text": "Awaria linii wysokiego napięcia spowodowała powstanie niebezpiecznego <w>łuku</w> elektrycznego.",
                            "target_lang_translations": {
                                "ru": "Авария на линии высокого напряжения привела к возникновению опасной электрической <w>дуги</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyładowanie"
                    ],
                    "common_phrases": [
                        "łuk elektryczny"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Длительный электрический разряд в газе между двумя электродами, дающий сильный свет"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуга",
                                "target_lang_sense_clarification": "электричество"
                            },
                            {
                                "target_lang_word": "электрическая дуга",
                                "target_lang_sense_clarification": "термин"
                            }
                        ]
                    }
                }
            ]

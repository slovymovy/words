                {
                    "sense_id": "6dd3dda4-5eff-3ab1-a5ed-67533070477a",
                    "sense_definition": "Excessively enthusiastic about or infatuated with someone or something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teenagers went absolutely <w>gaga</w> over the new pop star.",
                            "target_lang_translations": {
                                "ru": "Подростки были просто <w>без ума</w> от новой поп-звезды."
                            }
                        },
                        {
                            "text": "She is completely <w>gaga</w> about her new puppy.",
                            "target_lang_translations": {
                                "ru": "Она совершенно <w>помешана</w> на своем новом щенке."
                            }
                        }
                    ],
                    "synonyms": [
                        "infatuated",
                        "enamored",
                        "crazy",
                        "wild"
                    ],
                    "antonyms": [
                        "indifferent",
                        "unimpressed"
                    ],
                    "common_phrases": [
                        "go gaga",
                        "gaga over"
                    ],
                    "target_lang_definitions": {

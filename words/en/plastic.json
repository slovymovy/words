                            {
                                "target_lang_word": "гибкий",
                                "target_lang_sense_clarification": "легко гнущийся, приспосабливаемый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "561acdce-2641-3fab-8c33-c72d7ba03f82",
                    "sense_definition": "Fake, insincere, or artificial; pretending to be perfect or happy.",

                },
                {
                    "sense_id": "b130a501-6770-3e4a-a5da-475fe1d400e8",
                    "sense_definition": "Silne, gwałtowne drgnięcie ziemi, budynku lub innego obiektu, często o podłożu sejsmicznym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkańcy regionu odczuli w nocy silny <w>wstrząs</w> sejsmiczny.",
                            "target_lang_translations": {

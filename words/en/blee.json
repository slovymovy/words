            "senses": [
                {
                    "sense_id": "eab114bf-d7d7-317e-bb40-97cee98b551d",
                    "sense_definition": "Color or hue.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sem_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient poem described a mysterious flower of golden <w>blee</w>.",
                            "target_lang_translations": {
                                "ru": "В древней поэме описывался таинственный цветок золотистого <w>цвета</w>."
                            }
                        },
                        {
                            "text": "Time had faded the tapestry, causing its vibrant <w>blee</w> to disappear.",
                            "target_lang_translations": {
                                "ru": "Время заставило гобелен выцвести, и его яркий <w>цвет</w> исчез."
                            }
                        }
                    ],
                    "synonyms": [
                        "color",
                        "colour",
                        "hue"
                    ],
                    "common_phrases": [
                        "golden blee"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "found in older literature and poetry"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Цвет или оттенок (устаревшее, поэтическое)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цвет",
                                "target_lang_sense_clarification": "окраска, колер"
                            },
                            {
                                "target_lang_word": "оттенок",
                                "target_lang_sense_clarification": "тон, нюанс цвета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c22dd5c2-c13b-32ec-a0f3-c608f17b8e81",
                    "sense_definition": "The color of the face or complexion.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sem_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knight fell in love with a lady who was bright of <w>blee</w>.",
                            "target_lang_translations": {
                                "ru": "Рыцарь влюбился в даму, светлую <w>ликом</w>."
                            }
                        },
                        {
                            "text": "Her rosy <w>blee</w> turned pale when she heard the frightening news.",
                            "target_lang_translations": {
                                "ru": "Её румяный <w>цвет лица</w> побледнел, когда она услышала пугающие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "complexion"
                    ],
                    "common_phrases": [
                        "bright of blee"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "common in ballads and old texts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Цвет лица (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цвет лица",
                                "target_lang_sense_clarification": "внешний вид кожи лица"
                            },
                            {
                                "target_lang_word": "лик",
                                "target_lang_sense_clarification": "лицо (поэтическое, высокое)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9d133f3-ff27-3c6f-833e-91d88282485f",
                    "sense_definition": "General resemblance or likeness, often regarding family appearance.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Everyone remarks that the boy has the <w>blee</w> of his father.",
                            "target_lang_translations": {
                                "ru": "Все замечают, что у мальчика есть <w>сходство</w> с отцом."
                            }
                        },
                        {
                            "text": "There is a strong <w>blee</w> between the two brothers in that photograph.",
                            "target_lang_translations": {
                                "ru": "На этой фотографии между двумя братьями заметно сильное <w>сходство</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "blee of"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Northern English usage"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "rare outside specific dialects"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Общее сходство или подобие (диалектное)."
                    },
                    "translations": {

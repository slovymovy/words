                    ],
                    "synonyms": [
                        "opportunity",
                        "path",
                        "opening",
                        "way"
                    ],
                    "common_phrases": [
                        "open the door to",
                        "close the door on",
                        "foot in the door"
                    ],
                    "target_lang_definitions": {
                        "ru": "Средство доступа, возможность или вероятность достижения чего-либо."

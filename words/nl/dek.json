                        {
                            "text": "Je moet het <w>dek</w> goed vastmaken zodat het niet afglijdt in de stal.",
                            "target_lang_translations": {
                                "en": "You must fasten the <w>rug</w> securely so it doesn't slide off in the stable.",
                                "ru": "Ты должен хорошо закрепить <w>попону</w>, чтобы она не сползла в конюшне."
                            }
                        }
                    ],
                    "synonyms": [
                        "paardendeken"
                    ],
                    "common_phrases": [
                        "een dek opleggen"
                    ],
                    "target_lang_definitions": {

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвалить",
                                "target_lang_sense_clarification": "выражать одобрение"
                            },
                            {
                                "target_lang_word": "делать комплименты",
                                "target_lang_sense_clarification": "говорить любезности"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "564f615e-a2c1-3a52-b0b8-d14d55907690",
                    "sense_definition": "Женщина, которая подверглась мучениям или приняла смерть за свою веру, убеждения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Святая <w>мученица</w> не отреклась от своих убеждений даже перед лицом смерти.",
                            "target_lang_translations": {
                                "en": "The holy <w>martyr</w> did not renounce her beliefs even in the face of death."
                            }
                        },
                        {
                            "text": "На иконе была изображена великая <w>мученица</w> в красном одеянии.",
                            "target_lang_translations": {
                                "en": "The icon depicted a great <w>martyr</w> in red robes."
                            }
                        }
                    ],
                    "common_phrases": [
                        "святая мученица",
                        "великая мученица"
                    ],
                    "target_lang_definitions": {
                        "en": "A woman who was subjected to torture or accepted death for her faith or beliefs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "martyr",
                                "target_lang_sense_clarification": "religious context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a816485b-edce-3393-9d47-bd881ac9397e",
                    "sense_definition": "Женщина, чья жизнь полна тяжких страданий, лишений и горя (часто в переносном смысле).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она вела себя как <w>мученица</w>, хотя никто не заставлял её так много работать.",
                            "target_lang_translations": {
                                "en": "She acted like a <w>martyr</w>, although no one forced her to work so much."
                            }
                        },
                        {
                            "text": "Эта бедная <w>мученица</w> всю жизнь терпела сложный характер своего мужа.",
                            "target_lang_translations": {
                                "en": "This poor <w>martyr</w> endured her husband's difficult character all her life."
                            }
                        }
                    ],
                    "synonyms": [
                        "страдалица"
                    ],
                    "common_phrases": [
                        "смотреть как мученица",
                        "вид мученицы"
                    ],
                    "target_lang_definitions": {
                        "en": "A woman whose life is full of severe suffering, hardship, and grief (often used figuratively)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "martyr",
                                "target_lang_sense_clarification": "figurative"
                            },
                            {
                                "target_lang_word": "sufferer",
                                "target_lang_sense_clarification": "one who endures hardship"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
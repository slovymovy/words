                    ],
                    "synonyms": [
                        "nijaki"
                    ],
                    "common_phrases": [
                        "żaden problem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который не имеет характерных свойств, является бесполезным или незначительным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никакой",
                                "target_lang_sense_clarification": "плохой, не имеющий значения"

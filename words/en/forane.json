            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6e5bd149-85c0-3dab-8bcf-7fd783efcb56",
                    "sense_definition": "Designating a priest who has been appointed to exercise limited jurisdiction over a specific district or deanery within a diocese.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ecclesiastical_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bishop appointed a new vicar <w>forane</w> to oversee the rural deanery.",
                            "target_lang_translations": {
                                "ru": "Епископ назначил нового викария-<w>форана</w> для надзора за сельским благочинием."
                            }
                        },
                        {
                            "text": "As a vicar <w>forane</w>, he was responsible for coordinating the clergy in the outlying district.",
                            "target_lang_translations": {
                                "ru": "Как викарий-<w>форан</w>, он отвечал за координацию духовенства в отдаленном округе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vicar forane",
                        "vicariate forane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает священника, назначенного осуществлять ограниченную юрисдикцию над определенным округом или благочинием в епархии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "форан",
                                "target_lang_sense_clarification": "часть титула 'викарий-форан'"
                            },
                            {
                                "target_lang_word": "окружной",
                                "target_lang_sense_clarification": "в контексте церковного управления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7f2a681-0bdd-3a9f-92e8-c4effe152ae5",
                    "sense_definition": "An obsolete spelling of <w>foreign</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "foreign_variant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient manuscript described <w>forane</w> lands located far beyond the sea.",
                            "target_lang_translations": {
                                "ru": "Древняя рукопись описывала <w>чужеземные</w> земли, расположенные далеко за морем."
                            }

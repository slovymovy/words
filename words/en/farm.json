    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1e31b173-8f18-36de-ae9f-81e7bf94d0a5",
                    "sense_definition": "A place where agricultural and similar activities take place, especially the growing of crops or the raising of livestock.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "agriculture_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandparents live on a small <w>farm</w> in the countryside.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка живут на маленькой <w>ферме</w> в сельской местности."
                            }
                        },
                        {
                            "text": "They bought fresh eggs and milk directly from the dairy <w>farm</w>.",
                            "target_lang_translations": {
                                "ru": "Они покупали свежие яйца и молоко прямо с молочной <w>фермы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ranch",
                        "plantation",
                        "holding",
                        "croft"
                    ],
                    "antonyms": [
                        "city",
                        "wilderness"
                    ],
                    "common_phrases": [
                        "on the farm",
                        "dairy farm",
                        "family farm",
                        "farm animal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где ведется сельскохозяйственная деятельность, особенно выращивание культур или разведение скота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фе́рма",
                                "target_lang_sense_clarification": "сельскохозяйственное предприятие"
                            },
                            {
                                "target_lang_word": "хозя́йство",
                                "target_lang_sense_clarification": "земельный участок с домом и постройками"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edef6c17-bfe2-3208-a114-818ff26ee687",
                    "sense_definition": "A large area or collection of similar structures or machines used for a specific industrial purpose.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "industrial_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new wind <w>farm</w> generates enough electricity for the whole town.",
                            "target_lang_translations": {
                                "ru": "Новая ветряная <w>ферма</w> вырабатывает достаточно электричества для всего города."
                            }
                        },
                        {
                            "text": "All the data is stored in a massive server <w>farm</w> located in the desert.",
                            "target_lang_translations": {
                                "ru": "Все данные хранятся на огромной серверной <w>ферме</w>, расположенной в пустыне."
                            }
                        }
                    ],
                    "synonyms": [
                        "park",
                        "array"
                    ],
                    "common_phrases": [
                        "wind farm",
                        "solar farm",
                        "server farm",
                        "tank farm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая территория или совокупность однотипных сооружений или машин, используемых для конкретной промышленной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фе́рма",
                                "target_lang_sense_clarification": "группа серверов или оборудования"
                            },
                            {
                                "target_lang_word": "парк",
                                "target_lang_sense_clarification": "ветропарк или массив установок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "69986383-06ed-3206-92c1-95e6a25a76d4",
                    "sense_definition": "To work on a farm, especially in the growing and harvesting of crops.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "agriculture_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His family has <w>farmed</w> this land for over a hundred years.",
                            "target_lang_translations": {
                                "ru": "Его семья <w>возделывала</w> эту землю более ста лет."
                            }
                        },
                        {
                            "text": "It is difficult to <w>farm</w> in this region because of the dry climate.",
                            "target_lang_translations": {
                                "ru": "В этом регионе трудно <w>заниматься сельским хозяйством</w> из-за сухого климата."
                            }
                        }
                    ],
                    "synonyms": [
                        "cultivate",
                        "till",
                        "work"
                    ],
                    "common_phrases": [
                        "farm the land"
                    ],
                    "target_lang_definitions": {
                        "ru": "Работать на ферме, особенно занимаясь выращиванием и сбором урожая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "занима́ться се́льским хозя́йством",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "фе́рмерствовать",
                                "target_lang_sense_clarification": "быть фермером"
                            },
                            {
                                "target_lang_word": "возде́лывать",
                                "target_lang_sense_clarification": "обрабатывать землю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17f9a9ad-4dfc-3354-8cfe-e12637178e4b",
                    "sense_definition": "To grow a particular crop or raise animals commercially.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "agriculture_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>farm</w> salmon in the cold waters of the loch.",
                            "target_lang_translations": {
                                "ru": "Они <w>разводят</w> лосося в холодных водах озера."
                            }
                        },
                        {
                            "text": "Many people in this area <w>farm</w> corn and soybeans for a living.",
                            "target_lang_translations": {
                                "ru": "Многие люди в этом районе зарабатывают на жизнь тем, что <w>выращивают</w> кукурузу и сою."
                            }
                        }
                    ],
                    "synonyms": [
                        "grow",
                        "raise",
                        "breed",
                        "rear"
                    ],
                    "common_phrases": [
                        "farm fish",
                        "farm cattle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выращивать определенную культуру или разводить животных в коммерческих целях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разводи́ть",
                                "target_lang_sense_clarification": "животных или рыбу"
                            },
                            {
                                "target_lang_word": "выра́щивать",
                                "target_lang_sense_clarification": "растения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09261fec-102e-3ce5-bb29-8d2c8a004467",
                    "sense_definition": "To assign or contract out work to others, usually for a fee.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "outsourcing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company decided to <w>farm</w> out the maintenance work to a smaller firm.",
                            "target_lang_translations": {
                                "ru": "Компания решила <w>передать</w> работы по техническому обслуживанию более мелкой фирме."
                            }
                        },
                        {
                            "text": "She often <w>farms</w> out the tedious administrative tasks to freelancers.",
                            "target_lang_translations": {
                                "ru": "Она часто <w>перепоручает</w> утомительные административные задачи фрилансерам."
                            }
                        }
                    ],
                    "synonyms": [
                        "outsource",
                        "subcontract",
                        "delegate"
                    ],
                    "common_phrases": [
                        "farm out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать или перепоручать работу другим, обычно за плату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перепоруча́ть",
                                "target_lang_sense_clarification": "делегировать задачи"
                            },
                            {
                                "target_lang_word": "передава́ть",
                                "target_lang_sense_clarification": "отдавать на исполнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa9a39f2-d54c-3311-b818-101670a37843",
                    "sense_definition": "To perform repetitive actions in a video game to collect items, currency, or experience points.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gaming_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent the whole weekend <w>farming</w> gold to buy the best armor.",
                            "target_lang_translations": {
                                "ru": "Он провел все выходные, <w>фармя</w> золото, чтобы купить лучшую броню."
                            }
                        },
                        {
                            "text": "Players often <w>farm</w> this level because it drops rare items.",
                            "target_lang_translations": {
                                "ru": "Игроки часто <w>фармят</w> этот уровень, потому что там выпадают редкие предметы."
                            }
                        }
                    ],
                    "synonyms": [
                        "grind"
                    ],
                    "common_phrases": [
                        "farm gold",
                        "farm XP",
                        "farm likes"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gaming context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять повторяющиеся действия в видеоигре для сбора предметов, валюты или очков опыта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фа́рмить",
                                "target_lang_sense_clarification": "игровой сленг"
                            }
                        ]
                    }
                }
            ]
        },

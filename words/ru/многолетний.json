{
    "word_family": [
        "многолетник",
        "многолетие"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e43db1b7-4cb4-38ff-bc0e-0bcad04901c4",
                    "sense_definition": "Продолжающийся, существующий в течение многих лет.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У него есть <многолетний> опыт работы в этой сфере.",
                            "target_lang_translations": {
                                "en": "He has <many years of> work experience in this field."
                            }
                        },
                        {
                            "text": "Нас связывает крепкая и <многолетняя> дружба.",
                            "target_lang_translations": {
                                "en": "We are bound by a strong and <long-standing> friendship."
                            }
                        }
                    ],
                    "synonyms": [
                        "долгий"
                    ],
                    "antonyms": [
                        "краткосрочный",
                        "недолгий"
                    ],
                    "common_phrases": [
                        "многолетний опыт",
                        "многолетний труд",
                        "многолетняя дружба"
                    ],
                    "target_lang_definitions": {
                        "en": "Continuing or existing for many years."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "long-standing",
                                "target_lang_sense_clarification": "existing for a long time (e.g., friendship, tradition)"
                            },
                            {
                                "target_lang_word": "of many years",
                                "target_lang_sense_clarification": "indicating a long duration"
                            },
                            {
                                "target_lang_word": "long-term",
                                "target_lang_sense_clarification": "extending over a relatively long period"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "675d00fc-3c1b-3bf4-bbd9-fb7234ae37b7",
                    "sense_definition": "Живущий и развивающийся более двух лет (о растениях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом саду растут красивые <многолетние> цветы.",
                            "target_lang_translations": {
                                "en": "Beautiful <perennial> flowers grow in this garden."
                            }
                        },
                        {
                            "text": "Это <многолетнее> растение не требует особого ухода зимой.",
                            "target_lang_translations": {
                                "en": "This <perennial> plant does not require special care in winter."
                            }
                        }
                    ],
                    "antonyms": [
                        "однолетний"
                    ],
                    "common_phrases": [
                        "многолетнее растение",
                        "многолетние травы",
                        "многолетние цветы"
                    ],
                    "target_lang_definitions": {
                        "en": "Living and developing for more than two years (regarding plants)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "perennial",
                                "target_lang_sense_clarification": "botanical term for plants living more than two years"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "874107a0-7add-3858-b45e-881b829fdc97",
                    "sense_definition": "Проживший много лет (о человеке).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В лесу стоял огромный <многолетний> дуб.",
                            "target_lang_translations": {
                                "en": "A huge <ancient> oak stood in the forest."
                            }
                        },
                        {
                            "text": "<Многолетний> старец рассказал нам древнюю легенду.",
                            "target_lang_translations": {
                                "en": "The <ancient> elder told us an ancient legend."
                            }
                        }
                    ],
                    "antonyms": [
                        "молодой"
                    ],
                    "common_phrases": [
                        "многолетний старец"
                    ],
                    "target_lang_definitions": {
                        "en": "Having lived for many years (regarding a person or living thing)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ancient",
                                "target_lang_sense_clarification": "very old, often used for trees or elders in a literary context"
                            },
                            {
                                "target_lang_word": "old",
                                "target_lang_sense_clarification": "general term for advanced age"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d8e837b6-2f03-33f7-beaf-7c66c885992c",
                    "sense_definition": "Taki, który odbywa się bez udziału osób trzecich, bez etapów przejściowych lub prowadzi wprost do celu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relation_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mamy <w>bezpośredni</w> wpływ na jakość naszych produktów.",
                            "target_lang_translations": {
                                "ru": "Мы имеем <w>непосредственное</w> влияние на качество наших продуктов."
                            }
                        },
                        {
                            "text": "To była <w>bezpośrednia</w> przyczyna wypadku na drodze.",
                            "target_lang_translations": {
                                "ru": "Это была <w>непосредственная</w> причина аварии на дороге."
                            }
                        }
                    ],
                    "antonyms": [
                        "pośredni"
                    ],
                    "common_phrases": [
                        "bezpośredni kontakt",
                        "bezpośredni wpływ",
                        "bezpośrednia przyczyna",
                        "bezpośrednie zagrożenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осуществляемый без участия третьих лиц, без промежуточных этапов или ведущий прямо к цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непосредственный",
                                "target_lang_sense_clarification": "без посредников"
                            },
                            {
                                "target_lang_word": "прямой",
                                "target_lang_sense_clarification": "без промежуточных этапов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "976a0ef1-adbb-3133-9a18-56e355fa6d79",
                    "sense_definition": "Taki, który zachowuje się szczerze, otwarcie i naturalnie, nie trzymając się sztywnych konwenansów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>bezpośredni</w> sposób bycia zjednuje mu wielu przyjaciół.",
                            "target_lang_translations": {
                                "ru": "Его <w>непосредственная</w> манера поведения привлекает к нему много друзей."
                            }
                        },
                        {
                            "text": "Marek jest bardzo <w>bezpośredni</w> i zawsze mówi to, co myśli.",
                            "target_lang_translations": {
                                "ru": "Марек очень <w>прямой</w> и всегда говорит то, что думает."
                            }
                        }
                    ],
                    "synonyms": [
                        "szczery",
                        "otwarty",
                        "naturalny"
                    ],
                    "common_phrases": [
                        "bezpośredni człowiek",
                        "być bezpośrednim",
                        "zbyt bezpośredni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ведущий себя искренне, открыто и естественно, не придерживаясь строгих условностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непосредственный",
                                "target_lang_sense_clarification": "естественный, непринуждённый"
                            },
                            {
                                "target_lang_word": "прямой",
                                "target_lang_sense_clarification": "откровенный, правдивый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7b6a612-c49a-3065-93d0-b930670a4880",
                    "sense_definition": "Znajdujący się tuż obok, nieoddzielony niczym od innego obiektu lub miejsca.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dom znajduje się w <w>bezpośrednim</w> sąsiedztwie lasu.",
                            "target_lang_translations": {
                                "ru": "Дом находится в <w>непосредственном</w> соседстве с лесом."
                            }
                        },
                        {
                            "text": "Mieszkamy w <w>bezpośredniej</w> bliskości centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Мы живём в <w>непосредственной</w> близости от центра города."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyległy"
                    ],
                    "common_phrases": [
                        "bezpośrednie sąsiedztwo",
                        "bezpośrednia bliskość",
                        "w bezpośrednim otoczeniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся совсем рядом, ничем не отделённый от другого объекта или места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непосредственный",
                                "target_lang_sense_clarification": "очень близкий, смежный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

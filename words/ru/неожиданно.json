    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c14ee1c2-645b-310d-a39d-7939eede18ee",
                    "sense_definition": "Быстро и внезапно, без всякого предупреждения",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sudden_unexpected",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за угла <неожиданно> выехала большая машина.",
                            "target_lang_translations": {
                                "en": "A big car <suddenly> drove out from around the corner."
                            }
                        },
                        {
                            "text": "Дождь пошёл так <неожиданно>, что мы сразу промокли.",
                            "target_lang_translations": {
                                "en": "The rain started so <suddenly> that we got wet immediately."
                            }
                        }
                    ],
                    "synonyms": [
                        "вдруг",
                        "внезапно"
                    ],
                    "common_phrases": [
                        "неожиданно появиться",
                        "неожиданно остановиться",
                        "неожиданно исчезнуть"
                    ],
                    "target_lang_definitions": {
                        "en": "Quickly and suddenly, without any warning"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suddenly",
                                "target_lang_sense_clarification": "happening quickly and abruptly"
                            },
                            {
                                "target_lang_word": "unexpectedly",
                                "target_lang_sense_clarification": "without warning"
                            },
                            {
                                "target_lang_word": "out of the blue",
                                "target_lang_sense_clarification": "idiomatic, completely unexpected"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bbd2f09-c369-3c11-80b3-b8ec6b01a258",
                    "sense_definition": "Вопреки чьим-либо ожиданиям или представлениям",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sudden_unexpected",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Конец этого фильма оказался <неожиданно> грустным.",
                            "target_lang_translations": {
                                "en": "The end of this movie turned out to be <unexpectedly> sad."
                            }
                        },
                        {
                            "text": "Он <неожиданно> получил очень высокую оценку на экзамене.",
                            "target_lang_translations": {
                                "en": "He <unexpectedly> received a very high grade on the exam."
                            }
                        }
                    ],
                    "synonyms": [
                        "непредвиденно",
                        "нежданно"
                    ],
                    "antonyms": [
                        "ожидаемо",
                        "предсказуемо"
                    ],
                    "common_phrases": [
                        "совершенно неожиданно",
                        "неожиданно для всех",
                        "неожиданно приятно"
                    ],
                    "target_lang_definitions": {
                        "en": "Contrary to someone's expectations or ideas"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unexpectedly",
                                "target_lang_sense_clarification": "against expectations"
                            },
                            {
                                "target_lang_word": "surprisingly",
                                "target_lang_sense_clarification": "causing surprise"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4a0f56d9-4931-3fa7-b210-fd449498679b",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>неожиданный</w>",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ваше решение уехать сегодня было для нас <неожиданно>.",
                            "target_lang_translations": {
                                "en": "Your decision to leave today was <unexpected> for us."
                            }

                    ],
                    "target_lang_definitions": {
                        "ru": "распространяться в пространстве, быть слышимым или ощущаемым на большом расстоянии (обычно о звуке или запахе)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разноситься",
                                "target_lang_sense_clarification": "распространяться (о звуке, запахе)"
                            },
                            {
                                "target_lang_word": "доноситься",
                                "target_lang_sense_clarification": "быть слышным издалека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc0cdda6-fc8f-37da-8f3e-8d1766b15a4b",
                    "sense_definition": "o ptakach, zwłaszcza kurach: składać jaja",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eb5163f1-2847-3c18-b831-f0f4185826f0",
                    "sense_definition": "An instrument of attack or defense in combat or hunting, such as a gun, missile, or sword.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer drew his <w>weapon</w> when the suspect tried to run away.",
                            "target_lang_translations": {
                                "ru": "Полицейский достал своё <w>оружие</w>, когда подозреваемый попытался убежать."
                            }
                        },
                        {
                            "text": "He was arrested for carrying a concealed <w>weapon</w> without a permit.",
                            "target_lang_translations": {
                                "ru": "Его арестовали за ношение скрытого <w>оружия</w> без разрешения."
                            }
                        }
                    ],
                    "synonyms": [
                        "arm",
                        "armament"
                    ],
                    "common_phrases": [
                        "nuclear weapon",
                        "murder weapon",
                        "deadly weapon",
                        "concealed weapon",
                        "biological weapon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Орудие нападения или защиты в бою или на охоте, например, пистолет, ракета или меч."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оружие",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0686e1c-b7b7-3d9a-9bfb-6df0053da0fa",
                    "sense_definition": "An instrument or means used to harm, convince, or exert control over others.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Education is the most powerful <w>weapon</w> which you can use to change the world.",
                            "target_lang_translations": {
                                "ru": "Образование — это самое мощное <w>оружие</w>, которое можно использовать, чтобы изменить мир."
                            }
                        },
                        {
                            "text": "Humor was his secret <w>weapon</w> during the tense negotiations.",
                            "target_lang_translations": {
                                "ru": "Юмор был его секретным <w>оружием</w> во время напряженных переговоров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "secret weapon",
                        "political weapon",
                        "main weapon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Средство или способ, используемые для нанесения вреда, убеждения или контроля над другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оружие",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07ea3fbd-2f20-32f6-9239-6244e4268cf4",
                    "sense_definition": "A tool or object used for a specific task, often referred to humorously.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "For this difficult repair job, my <w>weapon</w> of choice is a heavy hammer.",
                            "target_lang_translations": {
                                "ru": "Для этой сложной ремонтной работы моё <w>оружие</w> выбора — тяжелый молоток."
                            }
                        },
                        {
                            "text": "He picked up the guitar, ready to play his <w>weapon</w> on stage.",
                            "target_lang_translations": {
                                "ru": "Он взял гитару, готовый сыграть на своём <w>оружии</w> на сцене."
                            }
                        }
                    ],
                    "common_phrases": [
                        "weapon of choice",
                        "choose your weapon"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often humorous"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструмент или предмет, используемый для выполнения конкретной задачи (часто употребляется в шутку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оружие",
                                "target_lang_sense_clarification": "шутливо о предмете"
                            },
                            {
                                "target_lang_word": "орудие",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91b87ff6-fe17-327b-8851-70ab2451f53b",
                    "sense_definition": "A foolish, incompetent, or annoying person.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't be such a <w>weapon</w> and listen to the instructions properly.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>тупицей</w> и слушай инструкции внимательно."
                            }
                        },
                        {
                            "text": "He acted like a total <w>weapon</w> when he forgot the tickets at home.",
                            "target_lang_translations": {
                                "ru": "Он вел себя как полный <w>идиот</w>, когда забыл билеты дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "moron",
                        "arse"
                    ],
                    "common_phrases": [
                        "absolute weapon",
                        "total weapon"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Глупый, некомпетентный или раздражающий человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тупица",
                                "target_lang_sense_clarification": "разг., оскорбительное"
                            },
                            {
                                "target_lang_word": "идиот",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "119f6072-de9e-345a-8c05-38f948dc3cf2",
                    "sense_definition": "A very skilled, capable, or impressive person.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is an absolute <w>weapon</w> on the tennis court and rarely loses a match.",
                            "target_lang_translations": {
                                "ru": "Она настоящая <w>машина</w> на теннисном корте и редко проигрывает матчи."
                            }

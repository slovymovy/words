    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c9774d31-65e2-3830-bd4c-6fbc7f47f5cc",
                    "sense_definition": "Een kap of scherm, vaak bevestigd aan een kinderwagen of voertuig, dat bescherming biedt tegen fel zonlicht.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De baby slaapt rustig onder de <w>zonnekap</w> van de wandelwagen.",
                            "target_lang_translations": {
                                "en": "The baby is sleeping peacefully under the <w>sunshade</w> of the stroller."
                            }
                        },
                        {
                            "text": "Zet de <w>zonnekap</w> omhoog zodat de zon niet in je ogen schijnt.",
                            "target_lang_translations": {
                                "en": "Put the <w>sunshade</w> up so the sun doesn't shine in your eyes."
                            }
                        }
                    ],
                    "synonyms": [
                        "zonnescherm"
                    ],
                    "common_phrases": [
                        "zonnekap van de kinderwagen",
                        "zonnekap omhoog doen",
                        "zonnekap naar beneden"
                    ],
                    "target_lang_definitions": {
                        "en": "A hood or screen, often attached to a stroller or vehicle, that provides protection against bright sunlight."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sunshade",
                                "target_lang_sense_clarification": "general protection"
                            },
                            {
                                "target_lang_word": "canopy",
                                "target_lang_sense_clarification": "for strollers"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "246b8607-0cb6-386a-bd78-0c93d76df3a8",
                    "sense_definition": "Een opzetstuk voor een cameralens dat voorkomt dat direct zonlicht in de lens valt en schittering veroorzaakt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gebruik een <w>zonnekap</w> om ongewenste lichtvlekken op je foto's te vermijden.",
                            "target_lang_translations": {
                                "en": "Use a <w>lens hood</w> to avoid unwanted light spots on your photos."
                            }
                        },
                        {
                            "text": "De fotograaf bevestigde de <w>zonnekap</w> op zijn lens voor de buitenopnames.",
                            "target_lang_translations": {
                                "en": "The photographer attached the <w>lens hood</w> to his lens for the outdoor shots."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zonnekap op de lens",
                        "zonnekap gebruiken"
                    ],
                    "target_lang_definitions": {
                        "en": "An attachment for a camera lens that prevents direct sunlight from entering the lens and causing glare."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lens hood",
                                "target_lang_sense_clarification": "photography"
                            }
                        ]
                    }
                }
            ]
        }
    ]

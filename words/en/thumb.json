    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9e032d9c-3615-397d-8040-a18628f81197",
                    "sense_definition": "The short, thick first digit of the human hand that is opposable to the other four fingers.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She accidentally hit her <w>thumb</w> with the hammer while hanging a picture.",
                            "target_lang_translations": {
                                "ru": "Она случайно ударила молотком по <w>большому пальцу</w>, когда вешала картину."
                            }
                        },
                        {
                            "text": "He gave a <w>thumb</w> up to show that he was ready to leave.",
                            "target_lang_translations": {
                                "ru": "Он поднял <w>большой палец</w> вверх, чтобы показать, что готов уходить."
                            }
                        }
                    ],
                    "synonyms": [
                        "pollex"
                    ],
                    "common_phrases": [
                        "thumbs up",
                        "thumbs down",
                        "rule of thumb",
                        "under someone's thumb",
                        "green thumb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий толстый первый палец кисти руки человека, противопоставленный остальным четырем пальцам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "большой палец",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f2b86c1-0ae0-3cbc-acf8-2801d091e138",
                    "sense_definition": "A small version of a digital picture or image, used for quick browsing or previewing.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Click on the <w>thumb</w> to view the full-size image in a new window.",
                            "target_lang_translations": {
                                "ru": "Нажмите на <w>миниатюру</w>, чтобы просмотреть полноразмерное изображение в новом окне."
                            }
                        },
                        {
                            "text": "The gallery displays a <w>thumb</w> for each photo in the album.",
                            "target_lang_translations": {
                                "ru": "В галерее отображается <w>миниатюра</w> для каждой фотографии в альбоме."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for thumbnail"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшенная версия цифрового изображения, используемая для быстрого просмотра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миниатюра",
                                "target_lang_sense_clarification": "изображение"
                            },
                            {
                                "target_lang_word": "эскиз",
                                "target_lang_sense_clarification": "предварительный просмотр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ac46875-d7ca-3cd5-a6db-d86a66d951bb",
                    "sense_definition": "The movable part of a scroll bar or slider in a graphical user interface that indicates position.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can drag the <w>thumb</w> to move quickly through the document.",
                            "target_lang_translations": {
                                "ru": "Вы можете перетащить <w>бегунок</w>, чтобы быстро перемещаться по документу."
                            }
                        },
                        {
                            "text": "The size of the scroll bar <w>thumb</w> indicates how much of the page is currently visible.",
                            "target_lang_translations": {
                                "ru": "Размер <w>бегунка</w> полосы прокрутки показывает, какая часть страницы видна в данный момент."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical computing context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижная часть полосы прокрутки или ползунка в графическом интерфейсе, указывающая текущую позицию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегунок",
                                "target_lang_sense_clarification": "элемент интерфейса"
                            },
                            {
                                "target_lang_word": "ползунок",
                                "target_lang_sense_clarification": "элемент управления"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "821fabd4-1057-3a89-a3b7-b8d2616ee238",
                    "sense_definition": "To turn the pages of a book or magazine quickly, often to glance at the contents without reading carefully.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I like to <w>thumb</w> through a magazine while waiting for my appointment.",
                            "target_lang_translations": {
                                "ru": "Мне нравится <w>листать</w> журнал в ожидании приема."
                            }
                        },
                        {
                            "text": "She <w>thumbed</w> through the report to find the summary at the end.",
                            "target_lang_translations": {
                                "ru": "Она <w>пролистала</w> отчет, чтобы найти резюме в конце."
                            }
                        }
                    ],
                    "synonyms": [
                        "flip through",
                        "browse",
                        "leaf"
                    ],
                    "common_phrases": [
                        "thumb through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро переворачивать страницы книги или журнала, часто чтобы бегло ознакомиться с содержанием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "листать",
                                "target_lang_sense_clarification": "переворачивать страницы"
                            },
                            {
                                "target_lang_word": "пролистывать",
                                "target_lang_sense_clarification": "бегло просматривать"
                            },
                            {
                                "target_lang_word": "перелистывать",
                                "target_lang_sense_clarification": "переворачивать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72e7bf64-20ae-3452-aeae-03d81b39411c",
                    "sense_definition": "To solicit a free ride from a passing vehicle by pointing one's thumb in the desired direction of travel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We missed the last bus and had to <w>thumb</w> a ride home.",
                            "target_lang_translations": {
                                "ru": "Мы опоздали на последний автобус, и нам пришлось <w>голосовать</w> на дороге, чтобы добраться домой."
                            }
                        },
                        {
                            "text": "He stood by the highway and <w>thumbed</w> a lift from a truck driver.",
                            "target_lang_translations": {
                                "ru": "Он стоял у шоссе и <w>поймал</w> попутку, которую вел водитель грузовика."
                            }
                        }
                    ],
                    "common_phrases": [
                        "thumb a ride",
                        "thumb a lift"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Просить подвезти на попутной машине, указывая большим пальцем в нужном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голосовать",
                                "target_lang_sense_clarification": "на дороге"
                            },
                            {
                                "target_lang_word": "ловить машину",
                                "target_lang_sense_clarification": "останавливать попутку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cfb1e3e-ad62-3a0e-a131-a21478bebf1d",
                    "sense_definition": "To touch, handle, or manipulate something using the thumb.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nervous speaker began to <w>thumb</w> his notes while waiting to start.",
                            "target_lang_translations": {
                                "ru": "Нервный оратор начал <w>теребить</w> свои заметки в ожидании начала."
                            }
                        },
                        {
                            "text": "He <w>thumbed</w> the control stick gently to steer the drone.",
                            "target_lang_translations": {
                                "ru": "Он осторожно <w>сдвинул</w> джойстик большим пальцем, чтобы направить дрон."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Трогать, держать или манипулировать чем-либо с помощью большого пальца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "теребить",
                                "target_lang_sense_clarification": "нервно перебирать пальцами"
                            },
                            {
                                "target_lang_word": "мусолить",
                                "target_lang_sense_clarification": "разг. трогать грязными пальцами"
                            },
                            {
                                "target_lang_word": "сдвигать",
                                "target_lang_sense_clarification": "управлять пальцем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8989f6b5-653b-37d9-b345-42ffc72ce7ba",
                    "sense_definition": "To pull back the hammer of a firearm using the thumb.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cowboy <w>thumbed</w> back the hammer of his revolver before aiming.",
                            "target_lang_translations": {
                                "ru": "Ковбой <w>взвел</w> курок своего револьвера большим пальцем перед тем, как прицелиться."
                            }
                        },
                        {
                            "text": "He quickly <w>thumbed</w> the mechanism to prepare for the next shot.",
                            "target_lang_translations": {
                                "ru": "Он быстро <w>взвел</w> механизм большим пальцем, чтобы подготовиться к следующему выстрелу."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Взводить курок огнестрельного оружия большим пальцем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взводить",
                                "target_lang_sense_clarification": "курок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {

                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please give the toy back to Mummy, <w>ta</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отдай игрушку маме, <w>спасибо</w>."
                            }
                        },
                        {
                            "text": "Hand it over, <w>ta</w>.",
                            "target_lang_translations": {
                                "ru": "Давай сюда, <w>спасибо</w>."
                            }

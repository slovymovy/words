    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e2fb982-b111-3f44-9527-d70e11bc49a7",
                    "sense_definition": "An organization founded to promote a specific cause, such as education, science, or research.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "organization_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She works as a scientist at a prestigious research <w>institute</w>.",
                            "target_lang_translations": {
                                "ru": "Она работает научным сотрудником в престижном научно-исследовательском <w>институте</w>."
                            }
                        },
                        {
                            "text": "The <w>institute</w> was founded to preserve local history and culture.",
                            "target_lang_translations": {
                                "ru": "Этот <w>институт</w> был основан для сохранения местной истории и культуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "organization",
                        "establishment",
                        "center",
                        "association"
                    ],
                    "common_phrases": [
                        "research institute",
                        "medical institute",
                        "national institute"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организация, созданная для поддержки определенного дела, например, образования, науки или исследований"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "институт",
                                "target_lang_sense_clarification": "научно-исследовательская организация"
                            },
                            {
                                "target_lang_word": "учреждение",
                                "target_lang_sense_clarification": "организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d676310e-688e-3a8d-9b9b-4a69b8018d04",
                    "sense_definition": "An institution of learning, especially one focusing on technical or scientific subjects.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "organization_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He applied to a technical <w>institute</w> to study engineering.",
                            "target_lang_translations": {
                                "ru": "Он подал документы в технический <w>институт</w>, чтобы изучать инженерное дело."
                            }
                        },
                        {
                            "text": "The <w>institute</w> offers evening classes for adults in various trades.",
                            "target_lang_translations": {
                                "ru": "<w>Институт</w> предлагает вечерние занятия для взрослых по различным профессиям."
                            }
                        }
                    ],
                    "synonyms": [
                        "college",
                        "academy",
                        "school"
                    ],
                    "common_phrases": [
                        "technical institute",
                        "institute of technology"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учебное заведение, особенно специализирующееся на технических или научных дисциплинах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "институт",
                                "target_lang_sense_clarification": "высшее учебное заведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2a9bed1-d10f-3498-b6cb-3275b9b0e745",
                    "sense_definition": "The building where an institute is located.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "organization_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old <w>institute</w> is being renovated this summer.",
                            "target_lang_translations": {
                                "ru": "Старый <w>институт</w> ремонтируют этим летом."
                            }
                        },
                        {
                            "text": "We met in front of the <w>institute</w> before the lecture started.",
                            "target_lang_translations": {
                                "ru": "Мы встретились перед <w>институтом</w> до начала лекции."
                            }
                        }
                    ],
                    "synonyms": [
                        "building"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание, в котором располагается институт"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "институт",
                                "target_lang_sense_clarification": "здание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "041c0a83-5475-34d5-90e3-89b7a0c92262",
                    "sense_definition": "To officially begin, introduce, or establish a system, rule, or legal process.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government plans to <w>institute</w> new reforms next year.",
                            "target_lang_translations": {
                                "ru": "Правительство планирует <w>ввести</w> новые реформы в следующем году."
                            }
                        },
                        {
                            "text": "They decided to <w>institute</w> a policy of zero tolerance for bullying.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>ввести</w> политику нулевой терпимости к травле."
                            }
                        }
                    ],
                    "synonyms": [
                        "establish",
                        "initiate",
                        "introduce",
                        "implement",
                        "start"
                    ],
                    "antonyms": [
                        "abolish",
                        "repeal",
                        "cancel",
                        "end"
                    ],
                    "common_phrases": [
                        "institute a policy",
                        "institute measures",
                        "institute proceedings",
                        "institute a rule"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально начинать, вводить или устанавливать систему, правило или юридический процесс"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вводить",
                                "target_lang_sense_clarification": "начинать применять правило или систему"
                            },
                            {
                                "target_lang_word": "учреждать",
                                "target_lang_sense_clarification": "основывать"
                            },
                            {
                                "target_lang_word": "инициировать",
                                "target_lang_sense_clarification": "давать начало"
                            }
                        ]
                    }
                }
            ]
        }

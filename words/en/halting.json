    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bc9bfab4-158b-30db-883d-18e0ccad8e81",
                    "sense_definition": "Prone to pauses or breaks; hesitant; broken.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The student read the difficult text in a <w>halting</w> voice.",
                            "target_lang_translations": {
                                "ru": "Студент читал сложный текст <w>запинающимся</w> голосом."
                            }
                        },
                        {
                            "text": "She made <w>halting</w> progress on the project due to a lack of resources.",
                            "target_lang_translations": {
                                "ru": "Её прогресс в проекте был <w>прерывистым</w> из-за нехватки ресурсов."
                            }
                        }
                    ],
                    "synonyms": [
                        "pause",
                        "recess",
                        "hiatus",
                        "moratorium"
                    ],
                    "antonyms": [
                        "fluent",
                        "steady",
                        "continuous"
                    ],
                    "common_phrases": [
                        "halting speech",
                        "halting steps",
                        "halting English"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный к паузам, нерешительный или прерывистый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запинающийся",
                                "target_lang_sense_clarification": "о речи или голосе"
                            },
                            {
                                "target_lang_word": "прерывистый",
                                "target_lang_sense_clarification": "о движении или прогрессе"
                            },
                            {
                                "target_lang_word": "нерешительный",
                                "target_lang_sense_clarification": "о шагах или действиях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bc702e51-13cb-3172-8b6d-6ae702b79bb6",
                    "sense_definition": "Present participle and gerund of <w>halt</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train is <w>halting</w> at the next station for inspection.",
                            "target_lang_translations": {
                                "ru": "Поезд <w>останавливается</w> на следующей станции для осмотра."
                            }
                        },
                        {
                            "text": "We are <w>halting</w> all operations until the safety check is complete.",
                            "target_lang_translations": {
                                "ru": "Мы <w>приостанавливаем</w> все операции до завершения проверки безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "pause",
                        "recess",
                        "hiatus",
                        "moratorium"
                    ],
                    "antonyms": [
                        "proceeding",
                        "continuing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола halt (останавливать, останавливаться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "останавливающийся",
                                "target_lang_sense_clarification": "прекращающий движение"
                            },
                            {
                                "target_lang_word": "приостанавливающий",
                                "target_lang_sense_clarification": "временно прекращающий деятельность"
                            }
                        ]
                    }
                }
            ]
        },
        {

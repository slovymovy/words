                {
                    "sense_id": "83eddcb5-9236-36c3-ba37-bccdd910055e",
                    "sense_definition": "Verlegen, schuchter en terughoudend in het contact met anderen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "timid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jongen was te <blood> om het meisje aan te spreken.",
                            "target_lang_translations": {
                                "en": "The boy was too <timid> to speak to the girl.",
                                "ru": "Мальчик был слишком <w>робким</w>, чтобы заговорить с девушкой."
                            }
                        },
                        {
                            "text": "Met een <blood> gezicht nam ze het compliment in ontvangst.",
                            "target_lang_translations": {
                                "en": "She accepted the compliment with a <bashful> face.",
                                "ru": "Она приняла комплимент с <w>застенчивым</w> лицом."
                            }
                        }
                    ],
                    "synonyms": [
                        "verlegen",
                        "beschroomd",
                        "bleu"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "wordt in modern Nederlands vaak als formeel of ouderwets ervaren"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Shy, timid, and reserved in interactions with others.",
                        "ru": "Застенчивый, робкий и сдержанный в общении с другими."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "timid",
                                "target_lang_sense_clarification": "showing a lack of courage or confidence; easily frightened"

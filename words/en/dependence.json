    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d4df6f8c-6e56-3543-b747-e5efb7930876",
                    "sense_definition": "The state of being dependent, of relying upon another.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "reliance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had a deep <w>dependence</w> on her for guidance.",
                            "target_lang_translations": {
                                "ru": "Он находился в сильной <w>зависимости</w> от её руководства."
                            }
                        },
                        {
                            "text": "The country must reduce its heavy <w>dependence</w> on imported oil.",
                            "target_lang_translations": {
                                "ru": "Страна должна снизить свою сильную <w>зависимость</w> от импортной нефти."
                            }
                        }
                    ],
                    "synonyms": [
                        "reliance"
                    ],
                    "antonyms": [
                        "independence"
                    ],
                    "common_phrases": [
                        "dependence on",
                        "financial dependence",
                        "mutual dependence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, когда кто-то нуждается в ком-то или чём-то для поддержки или существования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зави́симость",
                                "target_lang_sense_clarification": "состояние, подчиненность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a451f42f-04cf-32c1-9547-d228c24e62a4",
                    "sense_definition": "An irresistible physical or psychological need, especially for a chemical substance.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "addiction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <w>dependence</w> on illegal drugs led to her financial ruin.",
                            "target_lang_translations": {
                                "ru": "Её <w>зависимость</w> от нелегальных наркотиков привела её к финансовому краху."
                            }
                        },
                        {
                            "text": "The doctor treated the patient for alcohol <w>dependence</w> at the clinic.",
                            "target_lang_translations": {
                                "ru": "Врач лечил пациента от алкогольной <w>зависимости</w> в клинике."
                            }
                        }
                    ],
                    "synonyms": [
                        "addiction"
                    ],
                    "common_phrases": [
                        "drug dependence",
                        "chemical dependence",
                        "physical dependence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Навязчивая физическая или психологическая потребность, особенно в наркотиках или алкоголе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зави́симость",
                                "target_lang_sense_clarification": "болезненное пристрастие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
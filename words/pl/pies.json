                    "sense_id": "977ac9bd-0be6-31ab-8397-e726da5c5081",
                    "sense_definition": "Pospolite zwierzę domowe, ssak z rodziny psowatych, często trzymany do towarzystwa lub pilnowania domu",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <pies> uwielbia długie spacery po lesie i zabawę piłką.",
                            "target_lang_translations": {
                                "ru": "Мой <пёс> обожает долгие прогулки по лесу и игру с мячом."
                            }
                        },
                        {
                            "text": "Wierny <pies> czekał na swojego pana przed wejściem do sklepu.",
                            "target_lang_translations": {
                                "ru": "Верный <пёс> ждал своего хозяина перед входом в магазин."
                            }
                        },
                        {
                            "text": "Pies jest najlepszym przyjacielem człowieka.",
                            "target_lang_translations": {
                                "ru": "<Собака> — лучший друг человека."
                            }
                        }
                    ],
                    "synonyms": [
                        "czworonożny przyjaciel",
                        "stróż domu",
                        "domowego ogniska",
                        "reg. pozn. kejter",
                        "reg. białost. sobaka"
                    ],
                    "common_phrases": [
                        "wierny jak pies",
                        "pogoda pod psem",
                        "zejść na psy",
                        "żyć jak pies z kotem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обычное домашнее животное, млекопитающее семейства псовых, которое часто держат для компании или охраны дома"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собака",
                                "target_lang_sense_clarification": "общее название животного"

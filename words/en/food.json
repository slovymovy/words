    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc75b8af-bf66-3dd4-8b4b-8dd15d4a113a",
                    "sense_definition": "Any solid substance that people or animals eat to stay alive and grow.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sustenance_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to buy some <w>food</w> for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Нам нужно купить <w>еды</w> на ужин сегодня вечером."
                            }
                        },
                        {
                            "text": "Eating healthy <w>food</w> helps you feel strong and energetic.",
                            "target_lang_translations": {
                                "ru": "Употребление здоровой <w>пищи</w> помогает чувствовать себя сильным и энергичным."
                            }
                        }
                    ],
                    "synonyms": [
                        "nourishment",
                        "sustenance",
                        "grub",
                        "chow",
                        "fare",
                        "meals"
                    ],
                    "common_phrases": [
                        "fast food",
                        "junk food",
                        "healthy food",
                        "food and drink",
                        "cat food"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любое вещество, которое люди или животные едят для поддержания жизни и роста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "еда",
                                "target_lang_sense_clarification": "самое общее слово, обозначающее то, что едят"
                            },
                            {
                                "target_lang_word": "пища",
                                "target_lang_sense_clarification": "более формальный термин; то, что служит питанием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b9bc5f8-04ab-3aa4-b8b4-e05f1167ce8a",
                    "sense_definition": "A specific type or category of edible product, often used in the plural.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sustenance_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The supermarket has a large section for frozen <w>foods</w>.",
                            "target_lang_translations": {
                                "ru": "В супермаркете есть большой отдел замороженных <w>продуктов</w>."
                            }
                        },
                        {
                            "text": "Doctors recommend avoiding processed <w>foods</w> with high sugar content.",
                            "target_lang_translations": {
                                "ru": "Врачи рекомендуют избегать обработанных <w>продуктов</w> с высоким содержанием сахара."
                            }
                        }
                    ],
                    "synonyms": [
                        "foodstuff",
                        "comestible",
                        "product"
                    ],
                    "common_phrases": [
                        "frozen foods",
                        "processed foods",
                        "organic foods",
                        "canned foods"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный вид или категория съедобных продуктов, часто используется во множественном числе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продукты",
                                "target_lang_sense_clarification": "используется для перевода 'foods' во множественном числе (напр. замороженные продукты)"
                            },
                            {
                                "target_lang_word": "питание",
                                "target_lang_sense_clarification": "в контексте 'продукты питания' или детское питание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dcd181ed-d32f-3f43-8556-35e10bdbea09",
                    "sense_definition": "Something that provides mental stimulation, inspiration, or spiritual nourishment.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_sustenance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor's lecture gave us plenty of <w>food</w> for thought.",
                            "target_lang_translations": {
                                "ru": "Лекция профессора дала нам обильную <w>пищу</w> для размышлений."
                            }
                        },
                        {
                            "text": "Listening to classical music is <w>food</w> for the soul.",
                            "target_lang_translations": {
                                "ru": "Прослушивание классической музыки — это <w>пища</w> для души."
                            }
                        }
                    ],
                    "synonyms": [
                        "stimulation",
                        "fuel",
                        "inspiration"
                    ],
                    "common_phrases": [
                        "food for thought",
                        "food for the soul",
                        "food for the mind"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что дает умственную стимуляцию, вдохновение или духовную подпитку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пища",
                                "target_lang_sense_clarification": "в переносном смысле (пища для ума, пища для размышлений)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83921e52-1c60-3d73-9fe3-7528ffb0671d",
                    "sense_definition": "Illegal drugs.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_drugs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police arrested the gang for selling <w>food</w> on the corner.",
                            "target_lang_translations": {
                                "ru": "Полиция арестовала банду за продажу <w>наркотиков</w> на углу."
                            }
                        },
                        {
                            "text": "He got into trouble for moving <w>food</w> across the city.",
                            "target_lang_translations": {
                                "ru": "У него были неприятности из-за перевозки <w>наркотиков</w> через город."
                            }
                        }
                    ],
                    "synonyms": [
                        "drugs"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often used in MLE or urban contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нелегальные наркотики (слэнг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наркотики",
                                "target_lang_sense_clarification": "значение сленгового слова"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
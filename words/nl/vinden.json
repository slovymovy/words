            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3ed7318d-067b-386f-8aea-5663be51a2e5",
                    "sense_definition": "Iets of iemand aantreffen of ontdekken na ernaar gezocht te hebben.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lang zoeken heb ik eindelijk mijn verloren sleutels <w>gevonden</w>.",
                            "target_lang_translations": {
                                "en": "After searching for a long time, I finally <w>found</w> my lost keys."
                            }
                        },
                        {
                            "text": "Kun jij de weg naar het dichtstbijzijnde station <w>vinden</w>?",
                            "target_lang_translations": {
                                "en": "Can you <w>find</w> the way to the nearest station?"
                            }
                        }
                    ],
                    "synonyms": [
                        "aantreffen",
                        "ontdekken",
                        "opsporen"
                    ],
                    "antonyms": [
                        "verliezen",
                        "kwijtraken"
                    ],
                    "common_phrases": [
                        "de weg vinden",
                        "niet te vinden",
                        "terug vinden",
                        "iets mooi vinden"
                    ],
                    "target_lang_definitions": {
                        "en": "To find or discover something or someone after looking for them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find",
                                "target_lang_sense_clarification": "to locate or discover"
                            },
                            {
                                "target_lang_word": "locate",
                                "target_lang_sense_clarification": "to determine the place of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48f6b06e-09da-3e61-8ed1-0f3e299b4e87",
                    "sense_definition": "Een bepaalde mening over iets hebben of iets op een bepaalde manier beschouwen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>vind</w> het belangrijk dat we allemaal op tijd komen.",
                            "target_lang_translations": {
                                "en": "I <w>think</w> it is important that we all arrive on time."
                            }
                        },
                        {
                            "text": "De meeste kinderen <w>vinden</w> het leuk om buiten te spelen.",
                            "target_lang_translations": {
                                "en": "Most children <w>find</w> it fun to play outside."
                            }
                        }
                    ],
                    "synonyms": [
                        "achten",
                        "menen",
                        "oordelen"
                    ],
                    "common_phrases": [
                        "leuk vinden",
                        "belangrijk vinden",
                        "lekker vinden",
                        "aardig vinden",
                        "wat vind jij"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a specific opinion about something or to consider something in a certain way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "think",
                                "target_lang_sense_clarification": "to have an opinion"
                            },
                            {
                                "target_lang_word": "find",
                                "target_lang_sense_clarification": "to consider or regard"
                            },
                            {
                                "target_lang_word": "consider",
                                "target_lang_sense_clarification": "to view in a certain way"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "730b0676-9369-312a-b955-59edcc743f74",
                    "sense_definition": "Iets ervaren, ondervinden of toevallig tegenkomen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "experiencing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij heeft na die moeilijke periode veel steun <w>gevonden</w> bij haar familie.",
                            "target_lang_translations": {
                                "en": "She <w>found</w> a lot of support from her family after that difficult period."
                            }
                        },
                        {
                            "text": "De soldaat heeft helaas de dood <w>gevonden</w> tijdens de zware gevechten.",
                            "target_lang_translations": {
                                "en": "The soldier unfortunately <w>met</w> his death during the heavy fighting."
                            }
                        }
                    ],
                    "synonyms": [
                        "ervaren",
                        "ondervinden",
                        "tegenkomen"
                    ],
                    "common_phrases": [
                        "steun vinden",
                        "de dood vinden",
                        "rust vinden",
                        "troost vinden"
                    ],
                    "target_lang_definitions": {
                        "en": "To experience, undergo, or encounter something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find",
                                "target_lang_sense_clarification": "to meet with or experience"
                            },
                            {
                                "target_lang_word": "meet",
                                "target_lang_sense_clarification": "to encounter (e.g. death)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
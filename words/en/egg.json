        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f5683d2-e1ca-3a76-b309-f7c174b75386",
                    "sense_definition": "To throw eggs at a person or object, usually as a prank or insult.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_egg",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The angry crowd threatened to <w>egg</w> the politician's car.",
                            "target_lang_translations": {
                                "ru": "Разъярённая толпа угрожала <w>закидать яйцами</w> машину политика."
                            }
                        },
                        {
                            "text": "Teenagers were caught trying to <w>egg</w> the house on Halloween night.",
                            "target_lang_translations": {
                                "ru": "Подростков поймали при попытке <w>забросать яйцами</w> дом в ночь на Хэллоуин."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get egged",
                        "egg a house"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бросать яйца в человека или предмет, обычно в качестве хулиганства или оскорбления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закидать яйцами",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "забросать яйцами",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "280f9270-8d76-31ce-9aae-ebaf2b593ae1",
                    "sense_definition": "To cover food with beaten egg before cooking.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_egg",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>egg</w> the fish fillets before dipping them in breadcrumbs.",
                            "target_lang_translations": {
                                "ru": "Рыбное филе нужно <w>обмакнуть в яйцо</w> перед тем, как обвалять в сухарях."
                            }
                        },
                        {
                            "text": "The chef <w>egged</w> the pastry to give it a golden shine when baked.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар <w>смазал яйцом</w> выпечку, чтобы придать ей золотистый блеск при выпекании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "egg and breadcrumb"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "cooking"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать еду взбитым яйцом перед приготовлением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смазать яйцом",
                                "target_lang_sense_clarification": "кулинария"
                            },
                            {
                                "target_lang_word": "обмакнуть в яйцо",
                                "target_lang_sense_clarification": "кулинария"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eef09276-69da-3bc2-98d6-913ee535ef8f",
                    "sense_definition": "To encourage or incite someone to do something, often something foolish or risky.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "incite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His friends <w>egged</w> him on to jump into the cold lake.",
                            "target_lang_translations": {
                                "ru": "Друзья <w>подстрекали</w> его прыгнуть в холодное озеро."
                            }
                        },
                        {
                            "text": "Do not <w>egg</w> the dog on, or it will start barking loudly.",
                            "target_lang_translations": {
                                "ru": "Не <w>дразни</w> собаку, иначе она начнёт громко лаять."
                            }
                        }
                    ],
                    "synonyms": [
                        "provoke",
                        "tempt"
                    ],
                    "common_phrases": [
                        "egg on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поощрять или подстрекать кого-то сделать что-то, часто глупое или рискованное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подстрекать",
                                "target_lang_sense_clarification": "побуждение"
                            },
                            {
                                "target_lang_word": "науськивать",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [
        "chalcedonowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "51afe21f-a126-3a14-8040-1879957e487e",
                    "sense_definition": "Skrytokrystaliczna odmiana kwarcu o włóknistej budowie, występująca w różnych barwach, będąca minerałem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Barwne odmiany <w>chalcedonu</w> mają szerokie zastosowanie w jubilerstwie.",
                            "target_lang_translations": {
                                "ru": "Цветные разновидности <w>халцедона</w> находят широкое применение в ювелирном деле."
                            }
                        },
                        {
                            "text": "Geolog znalazł w skale żyłę mlecznego <w>chalcedonu</w>.",
                            "target_lang_translations": {
                                "ru": "Геолог нашел в скале жилу молочного <w>халцедона</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odmiana chalcedonu",
                        "błękitny chalcedon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скрытокристаллическая разновидность кварца волокнистого строения, встречающаяся в различных цветах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "халцедон",
                                "target_lang_sense_clarification": "минерал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36d0711d-07cb-3149-be8c-20290953d027",
                    "sense_definition": "Oszlifowany kamień ozdobny wykonany z tego minerału, używany do wyrobu biżuterii.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej nowy pierścionek zdobi duży, błękitny <w>chalcedon</w>.",
                            "target_lang_translations": {
                                "ru": "Ее новое кольцо украшает крупный голубой <w>халцедон</w>."
                            }
                        },
                        {
                            "text": "Jubiler zaprezentował nam pięknie oszlifowany <w>chalcedon</w>.",
                            "target_lang_translations": {
                                "ru": "Ювелир показал нам прекрасно ограненный <w>халцедон</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naszyjnik z chalcedonem",
                        "wisiorek z chalcedonem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обработанный поделочный камень из этого минерала, используемый для изготовления ювелирных изделий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "халцедон",
                                "target_lang_sense_clarification": "камень"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "b3a396bc-eea1-32ab-a7ff-2f06902e374d",
                    "sense_definition": "Starożytne miasto w Bitynii, położone nad Bosforem, na terenie dzisiejszego Stambułu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Dzięki recepcji soboru w <w>Chalcedonie</w> osiągnięto nowy stopień rozwoju idei soborowej.",
                            "target_lang_translations": {
                                "ru": "Благодаря рецепции собора в <w>Халкидоне</w> была достигнута новая ступень развития соборной идеи."
                            }
                        },
                        {
                            "text": "Starożytny <w>Chalcedon</w> był ważnym ośrodkiem handlowym i religijnym.",
                            "target_lang_translations": {
                                "ru": "Древний <w>Халкидон</w> был важным торговым и религиозным центром."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sobór w Chalcedonie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древний город в Вифинии, расположенный на берегу Босфора, на территории современного Стамбула."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Халкидон",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
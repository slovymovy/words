{
    "word_family": [
        "negatie",
        "negatief"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cdfc1e52-4831-3edc-a2fe-fd2fdaa75264",
                    "sense_definition": "Geen aandacht schenken aan iemand of iets; doen alsof iets niet bestaat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ignore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>negeert</w> haar al een aantal dagen.",
                            "target_lang_translations": {
                                "ru": "Он <w>игнорирует</w> её уже несколько дней."
                            }
                        },
                        {
                            "text": "Je moet de waarschuwingsborden niet zomaar <w>negeren</w>.",
                            "target_lang_translations": {
                                "ru": "Нельзя просто так <w>игнорировать</w> предупреждающие знаки."
                            }
                        }
                    ],
                    "antonyms": [
                        "reageren"
                    ],
                    "common_phrases": [
                        "een bevel negeren",
                        "iemand volkomen negeren",
                        "de regels negeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не обращать внимания на кого-либо или что-либо; делать вид, что чего-то не существует."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игнорировать",
                                "target_lang_sense_clarification": "намеренно не замечать"
                            },
                            {
                                "target_lang_word": "пренебрегать",
                                "target_lang_sense_clarification": "относиться без должного внимания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1033b278-9525-32d1-81ca-abcad2209491",
                    "sense_definition": "Ontkennen; beweren dat iets niet waar is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "deny",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte <w>negeerde</w> zijn betrokkenheid bij de misdaad.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый <w>отрицал</w> свою причастность к преступлению."
                            }
                        },
                        {
                            "text": "Hij <w>negeert</w> het feit dat hij fout zat.",
                            "target_lang_translations": {
                                "ru": "Он <w>отрицает</w> тот факт, что был неправ."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontkennen",
                        "loochenen"
                    ],
                    "antonyms": [
                        "bekennen",
                        "toegeven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрицать; утверждать, что что-то не является правдой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрицать",
                                "target_lang_sense_clarification": "не признавать истинность"
                            },
                            {
                                "target_lang_word": "отвергать",
                                "target_lang_sense_clarification": "не принимать, отказываться признавать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf46956c-6e02-35c5-97e5-fef300cd03bf",
                    "sense_definition": "Opzettelijk slecht behandelen, pesten of treiteren.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "bully",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten <w>negerden</w> de gevangenen zonder genade.",
                            "target_lang_translations": {
                                "ru": "Солдаты безжалостно <w>издевались</w> над заключенными."
                            }
                        },
                        {
                            "text": "Hij werd op school vaak <w>genegerd</w> door de oudere jongens.",
                            "target_lang_translations": {
                                "ru": "В школе его часто <w>травили</w> старшие мальчики."
                            }
                        }
                    ],
                    "synonyms": [
                        "pesten",
                        "treiteren"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Dit gebruik is verouderd."
                        },
                        {
                            "trait_type": "form",
                            "comment": "Klemtoon op de eerste lettergreep (négeren); vervoeging: negerde, genegerd."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренно плохо обращаться, издеваться или дразнить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "издеваться",
                                "target_lang_sense_clarification": "подвергать унижению или мучениям"
                            },
                            {
                                "target_lang_word": "третировать",
                                "target_lang_sense_clarification": "обращаться грубо или свысока"
                            },
                            {
                                "target_lang_word": "травить",
                                "target_lang_sense_clarification": "преследовать, не давать прохода"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c9cdfe59-ba28-3c60-aeba-2f95c8fee15f",
                    "sense_definition": "Журналист или автор, который пишет аналитические статьи, ведет рубрику обзоров в СМИ (газетах, журналах, на телевидении).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Политический <обозреватель> подробно рассказал о предстоящих выборах в вечернем эфире.",
                            "target_lang_translations": {
                                "en": "The political <commentator> spoke in detail about the upcoming elections during the evening broadcast."
                            }
                        },
                        {
                            "text": "Наш спортивный <обозреватель> подготовил статью о результатах чемпионата.",
                            "target_lang_translations": {
                                "en": "Our sports <columnist> prepared an article about the championship results."
                            }
                        }
                    ],
                    "synonyms": [
                        "обзорщик"
                    ],
                    "common_phrases": [
                        "политический обозреватель",
                        "спортивный обозреватель",
                        "военный обозреватель"
                    ],
                    "target_lang_definitions": {
                        "en": "A journalist or author who writes analytical articles or leads review columns in media (newspapers, magazines, TV)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "columnist",
                                "target_lang_sense_clarification": "journalist writing a regular column"
                            },
                            {
                                "target_lang_word": "commentator",
                                "target_lang_sense_clarification": "person who discusses news or events"
                            },
                            {
                                "target_lang_word": "analyst",
                                "target_lang_sense_clarification": "expert who analyzes a specific field"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6acfb290-4843-3e29-b231-bf3d3d888fc5",
                    "sense_definition": "Компьютерная программа для поиска и просмотра информации в интернете.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот <обозреватель> работает быстрее и потребляет меньше памяти.",
                            "target_lang_translations": {
                                "en": "This <browser> works faster and consumes less memory."
                            }
                        },
                        {
                            "text": "Чтобы открыть ссылку, запустите ваш интернет-<обозреватель>.",
                            "target_lang_translations": {
                                "en": "To open the link, launch your web <browser>."
                            }
                        }
                    ],
                    "synonyms": [
                        "браузер"
                    ],
                    "common_phrases": [
                        "интернет-обозреватель",
                        "обозреватель решений"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "в разговорной речи обычно используется слово «браузер»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A computer program for searching and viewing information on the internet."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "browser",
                                "target_lang_sense_clarification": "software for accessing the web"
                            },
                            {
                                "target_lang_word": "web browser",
                                "target_lang_sense_clarification": "full term for internet navigation software"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89457f69-3401-3e27-889e-354f8cd1f729",
                    "sense_definition": "Тот, кто осматривает местность или наблюдает за ходом каких-либо событий со стороны.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Как сторонний <обозреватель>, он не вмешивался в спор, а лишь слушал.",
                            "target_lang_translations": {
                                "en": "As an outside <observer>, he did not interfere in the dispute, but only listened."
                            }
                        },
                        {
                            "text": "С вершины башни <обозреватель> мог видеть все окрестности города.",
                            "target_lang_translations": {
                                "en": "From the top of the tower, the <observer> could see the entire surroundings of the city."
                            }
                        }
                    ],
                    "synonyms": [
                        "наблюдатель"
                    ],
                    "common_phrases": [
                        "сторонний обозреватель"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who surveys an area or watches events from the outside."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "observer",
                                "target_lang_sense_clarification": "person who watches or notices something"
                            },
                            {
                                "target_lang_word": "onlooker",
                                "target_lang_sense_clarification": "spectator who does not participate"
                            }
                        ]
                    }
                }
            ]
        }
    ]

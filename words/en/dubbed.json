    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "714e23bb-c13d-3330-bb7d-02eff624182a",
                    "sense_definition": "Simple past and past participle of <w>dub</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The newspapers <w>dubbed</w> him the \"King of Pop\" after his record-breaking album.",
                            "target_lang_translations": {
                                "ru": "Газеты <w>окрестили</w> его «королём поп-музыки» после его рекордного альбома."
                            }
                        },
                        {
                            "text": "They <w>dubbed</w> the movie into three different languages to reach a wider audience.",
                            "target_lang_translations": {
                                "ru": "Они <w>дублировали</w> фильм на три разных языка, чтобы охватить более широкую аудиторию."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола <w>dub</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дублировать",
                                "target_lang_sense_clarification": "заменять звуковую дорожку в фильме"
                            },
                            {
                                "target_lang_word": "прозвать",
                                "target_lang_sense_clarification": "дать прозвище"
                            },
                            {
                                "target_lang_word": "окрестить",
                                "target_lang_sense_clarification": "дать имя или название (перен.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0fa1e68a-1a52-3ddf-ae67-2bdf9f1b036b",
                    "sense_definition": "Having the original soundtrack replaced with a synchronized translation in another language.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "media_translation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I usually prefer watching the <w>dubbed</w> version of an animation so I can focus on the visuals.",
                            "target_lang_translations": {
                                "ru": "Я обычно предпочитаю смотреть <w>дублированную</w> версию мультфильма, чтобы сосредоточиться на визуальном ряде."
                            }
                        },
                        {
                            "text": "The <w>dubbed</w> dialogue did not match the actors' lip movements perfectly.",
                            "target_lang_translations": {
                                "ru": "<w>Дублированный</w> диалог не идеально совпадал с движениями губ актёров."
                            }
                        }
                    ],
                    "antonyms": [
                        "subtitled"
                    ],
                    "common_phrases": [
                        "dubbed movie",
                        "dubbed version",
                        "dubbed anime",
                        "badly dubbed"
                    ],
                    "target_lang_definitions": {
                        "ru": "О фильме или видео: с заменённой оригинальной звуковой дорожкой на синхронизированный перевод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дублированный",
                                "target_lang_sense_clarification": "озвученный на другом языке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45959d55-1a61-389e-a5f2-0dba6908e5a2",
                    "sense_definition": "Having been ceremonially invested with a knighthood or title.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "titles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The newly <w>dubbed</w> knight rose from his knees and pledged loyalty to the king.",
                            "target_lang_translations": {
                                "ru": "Недавно <w>посвящённый</w> рыцарь поднялся с колен и поклялся в верности королю."
                            }
                        },
                        {
                            "text": "In the story, the <w>dubbed</w> hero sets out on a dangerous quest to prove his worth.",
                            "target_lang_translations": {
                                "ru": "В этой истории <w>посвящённый в рыцари</w> герой отправляется в опасное странствие, чтобы доказать свою доблесть."
                            }
                        }
                    ],
                    "synonyms": [
                        "knighted"
                    ],
                    "common_phrases": [
                        "dubbed knight"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "often used in historical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Церемониально посвящённый в рыцари или удостоенный титула."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посвящённый в рыцари",
                                "target_lang_sense_clarification": "получивший рыцарский титул"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
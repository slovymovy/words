{
    "word_family": [
        "getter",
        "beget",
        "forget"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "94a1798d-30a6-3186-96a2-9720233f254d",
                    "sense_definition": "To obtain or acquire something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>get</w> some food from the supermarket.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>купить</w> немного еды в супермаркете."
                            }
                        },
                        {
                            "text": "Did you <w>get</w> tickets for the concert tonight?",
                            "target_lang_translations": {
                                "ru": "Ты <w>достал</w> билеты на сегодняшний концерт?"
                            }
                        }
                    ],
                    "synonyms": [
                        "obtain",
                        "acquire"
                    ],
                    "antonyms": [
                        "lose"
                    ],
                    "common_phrases": [
                        "get a job",
                        "get a life",
                        "get permission"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать, приобретать или добывать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достать",
                                "target_lang_sense_clarification": "раздобыть, приобрести (часто с усилием)"
                            },
                            {
                                "target_lang_word": "купить",
                                "target_lang_sense_clarification": "приобрести за деньги"
                            },
                            {
                                "target_lang_word": "взять",
                                "target_lang_sense_clarification": "общее значение получения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08568f92-36d7-32e5-812c-f87990fef2ea",
                    "sense_definition": "To receive something from someone.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>got</w> a lovely letter from my grandmother.",
                            "target_lang_translations": {
                                "ru": "Я <w>получил</w> чудесное письмо от бабушки."
                            }
                        },
                        {
                            "text": "What did you <w>get</w> for your birthday this year?",
                            "target_lang_translations": {
                                "ru": "Что ты <w>получил</w> на день рождения в этом году?"
                            }
                        }
                    ],
                    "synonyms": [
                        "receive"
                    ],
                    "common_phrases": [
                        "get a message",
                        "get a call",
                        "get a present"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать что-либо от кого-либо (письмо, подарок и т.д.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получить",
                                "target_lang_sense_clarification": "принять что-то вручаемое или присылаемое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30a0d4c8-f871-3cb8-9115-d3d878df73b5",
                    "sense_definition": "To go to a place, take something, and bring it back.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>get</w> me a glass of water, please?",
                            "target_lang_translations": {
                                "ru": "Можешь <w>принести</w> мне стакан воды, пожалуйста?"
                            }
                        },
                        {
                            "text": "I will go and <w>get</w> the car from the parking lot.",
                            "target_lang_translations": {
                                "ru": "Я пойду и <w>заберу</w> машину с парковки."
                            }
                        }
                    ],
                    "synonyms": [
                        "fetch",
                        "bring",
                        "retrieve"
                    ],
                    "common_phrases": [
                        "go and get",
                        "come and get"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пойти куда-либо, взять что-то и принести обратно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принести",
                                "target_lang_sense_clarification": "доставить что-то кому-то"
                            },
                            {
                                "target_lang_word": "взять",
                                "target_lang_sense_clarification": "сходить и взять"
                            },
                            {
                                "target_lang_word": "забрать",
                                "target_lang_sense_clarification": "взять что-то, находящееся в другом месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df81c9ff-3378-3791-a1bf-4944ea85963b",
                    "sense_definition": "To start to be in a new state or condition; to become.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>getting</w> dark outside, so we should go home.",
                            "target_lang_translations": {
                                "ru": "На улице <w>темнеет</w>, так что нам пора домой."
                            }
                        },
                        {
                            "text": "He <w>got</w> very angry when he heard the bad news.",
                            "target_lang_translations": {
                                "ru": "Он очень <w>рассердился</w>, когда услышал плохие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "become",
                        "grow"
                    ],
                    "common_phrases": [
                        "get ready",
                        "get better",
                        "get married",
                        "get lost",
                        "get old"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переходить в новое состояние или условие; становиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "становиться",
                                "target_lang_sense_clarification": "приобретать какое-либо свойство"
                            },
                            {
                                "target_lang_word": "стать",
                                "target_lang_sense_clarification": "совершенный вид от 'становиться'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba02ee79-05ae-3648-bd26-169244c5f10b",
                    "sense_definition": "To reach a destination.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "What time do you usually <w>get</w> home from work?",
                            "target_lang_translations": {
                                "ru": "Во сколько ты обычно <w>приходишь</w> домой с работы?"
                            }
                        },
                        {
                            "text": "We <w>got</w> to the train station just in time.",
                            "target_lang_translations": {
                                "ru": "Мы <w>добрались</w> до вокзала как раз вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "arrive",
                        "reach"
                    ],
                    "common_phrases": [
                        "get home",
                        "get there",
                        "get to work"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добраться до места назначения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добраться",
                                "target_lang_sense_clarification": "достичь места (часто с усилием или затратой времени)"
                            },
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "оказаться в каком-либо месте"
                            },
                            {
                                "target_lang_word": "прийти",
                                "target_lang_sense_clarification": "прибыть (обычно пешком или домой)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cd92d8a-90ce-376d-991e-c12a7245eff8",
                    "sense_definition": "To understand or grasp the meaning of something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I don't <w>get</w> the joke, can you explain it?",
                            "target_lang_translations": {
                                "ru": "Я не <w>понимаю</w> шутку, можешь объяснить?"
                            }
                        },
                        {
                            "text": "Do you <w>get</w> what I mean by that statement?",
                            "target_lang_translations": {
                                "ru": "Ты <w>понимаешь</w>, что я имею в виду под этим заявлением?"
                            }
                        }
                    ],
                    "synonyms": [
                        "understand",
                        "grasp",
                        "follow"
                    ],
                    "common_phrases": [
                        "get it",
                        "get the picture",
                        "get the idea"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать или улавливать смысл чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "постигать смысл"
                            },
                            {
                                "target_lang_word": "понять",
                                "target_lang_sense_clarification": "уяснить, догадаться"
                            },
                            {
                                "target_lang_word": "врубаться",
                                "target_lang_sense_clarification": "разг. сленг: понимать, догадываться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cd1015b-b674-351b-af84-e56bb4d43620",
                    "sense_definition": "To catch or take a form of public transport.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We usually <w>get</w> the bus to school every morning.",
                            "target_lang_translations": {
                                "ru": "Мы обычно <w>ездим</w> в школу на автобусе каждое утро."
                            }
                        },
                        {
                            "text": "I need to <w>get</w> a taxi to the airport.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>поймать</w> такси до аэропорта."
                            }
                        }
                    ],
                    "synonyms": [
                        "catch",
                        "take"
                    ],
                    "common_phrases": [
                        "get the bus",
                        "get a train",
                        "get a cab"
                    ],
                    "target_lang_definitions": {
                        "ru": "Садиться на общественный транспорт или брать такси."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сесть",
                                "target_lang_sense_clarification": "на автобус, поезд и т.д."
                            },
                            {
                                "target_lang_word": "поймать",
                                "target_lang_sense_clarification": "о такси"
                            },
                            {
                                "target_lang_word": "взять",
                                "target_lang_sense_clarification": "о такси или билете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9410fb6a-4ede-3201-bf84-803f47592390",
                    "sense_definition": "To answer a phone call or a knock at the door.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>get</w> the phone, please? I am busy cooking.",
                            "target_lang_translations": {
                                "ru": "Можешь <w>ответить</w> на звонок, пожалуйста? Я занят готовкой."
                            }
                        },
                        {
                            "text": "I will <w>get</w> the door if someone knocks.",
                            "target_lang_translations": {
                                "ru": "Я <w>открою</w> дверь, если кто-то постучит."
                            }
                        }
                    ],
                    "synonyms": [
                        "answer"
                    ],
                    "common_phrases": [
                        "get the door",
                        "get the phone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ответить на телефонный звонок или открыть дверь на стук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответить",
                                "target_lang_sense_clarification": "на звонок"
                            },
                            {
                                "target_lang_word": "открыть",
                                "target_lang_sense_clarification": "дверь"
                            },
                            {
                                "target_lang_word": "подойти",
                                "target_lang_sense_clarification": "к телефону или двери"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "811f0cea-250d-3cdb-b0db-53249b29a61f",
                    "sense_definition": "To become sick with a disease or virus.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I think I am <w>getting</w> a cold because my throat hurts.",
                            "target_lang_translations": {
                                "ru": "Кажется, я <w>заболеваю</w> простудой, потому что у меня болит горло."
                            }
                        },
                        {
                            "text": "She <w>got</w> the flu last winter and stayed in bed for a week.",
                            "target_lang_translations": {
                                "ru": "Она <w>переболела</w> гриппом прошлой зимой и пролежала в постели неделю."
                            }
                        }
                    ],
                    "synonyms": [
                        "catch",
                        "contract"
                    ],
                    "common_phrases": [
                        "get sick",
                        "get ill",
                        "get a cold"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заболеть болезнью или вирусом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заболеть",
                                "target_lang_sense_clarification": "стать больным"
                            },
                            {
                                "target_lang_word": "подхватить",
                                "target_lang_sense_clarification": "разг. заразиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecb1d4b9-c938-33b7-b087-6734fb242562",
                    "sense_definition": "Used with a past participle to form the passive voice, often emphasizing the action or event itself.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>got</w> caught by the police for speeding.",
                            "target_lang_translations": {
                                "ru": "Он <w>попался</w> полиции за превышение скорости."
                            }
                        },
                        {
                            "text": "My car <w>got</w> stolen from the street yesterday.",
                            "target_lang_translations": {
                                "ru": "Мою машину вчера <w>угнали</w> с улицы."
                            }
                        }
                    ],
                    "synonyms": [
                        "be"
                    ],
                    "common_phrases": [
                        "get hurt",
                        "get fired",
                        "get paid",
                        "get dressed"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used instead of 'be' in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для образования пассивного залога, подчеркивая само действие или событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "вспомогательный глагол (часто опускается или заменяется возвратным глаголом)"
                            },
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "в контексте неприятных ситуаций (попасться)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef1e76ca-dbab-3584-b3f0-7444d102fe7a",
                    "sense_definition": "To cause something to happen or to be done.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>get</w> my car fixed before the trip.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>починить</w> машину перед поездкой."
                            }
                        },
                        {
                            "text": "She <w>got</w> the work done very quickly.",
                            "target_lang_translations": {
                                "ru": "Она <w>сделала</w> работу очень быстро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get it done",
                        "get something ready"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организовать выполнение чего-либо; сделать так, чтобы что-то произошло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сделать",
                                "target_lang_sense_clarification": "выполнить или организовать выполнение"
                            },
                            {
                                "target_lang_word": "починить",
                                "target_lang_sense_clarification": "в контексте ремонта (get fixed)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a023688c-8b0c-37f6-8df7-833a70ba0f93",
                    "sense_definition": "To persuade or force someone to do something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I couldn't <w>get</w> him to leave the party.",
                            "target_lang_translations": {
                                "ru": "Я не мог <w>заставить</w> его уйти с вечеринки."
                            }
                        },
                        {
                            "text": "How did you <w>get</w> her to agree to the plan?",
                            "target_lang_translations": {
                                "ru": "Как ты <w>уговорил</w> её согласиться на этот план?"
                            }
                        }
                    ],
                    "synonyms": [
                        "persuade",
                        "convince"
                    ],
                    "common_phrases": [
                        "get someone to help"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убедить или заставить кого-то сделать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уговорить",
                                "target_lang_sense_clarification": "убедить согласиться"
                            },
                            {
                                "target_lang_word": "заставить",
                                "target_lang_sense_clarification": "принудить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e18c085-802d-3de6-ac8b-3d6a47577a90",
                    "sense_definition": "To have the opportunity or permission to do something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>got</w> to meet the actors after the show.",
                            "target_lang_translations": {
                                "ru": "Нам <w>удалось</w> встретиться с актерами после шоу."
                            }
                        },
                        {
                            "text": "I never <w>get</w> to drive the new car.",
                            "target_lang_translations": {
                                "ru": "Мне никогда не <w>удаётся</w> поводить новую машину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get to know",
                        "get to see",
                        "get to go"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь возможность или разрешение сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удаваться",
                                "target_lang_sense_clarification": "получаться, иметь возможность"
                            },
                            {
                                "target_lang_word": "доводиться",
                                "target_lang_sense_clarification": "случаться, выпадать на долю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fba2b96-e4e6-3178-ad2b-162c60705ed7",
                    "sense_definition": "To begin doing something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>get</w> moving if we want to arrive on time.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>выдвигаться</w>, если мы хотим приехать вовремя."
                            }
                        },
                        {
                            "text": "After lunch, we <w>got</w> chatting about our holidays.",
                            "target_lang_translations": {
                                "ru": "После обеда мы <w>разговорились</w> о наших отпусках."
                            }
                        }
                    ],
                    "synonyms": [
                        "start",
                        "begin",
                        "commence"
                    ],
                    "common_phrases": [
                        "get going",
                        "get moving",
                        "get started"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать делать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начать",
                                "target_lang_sense_clarification": "приступить к действию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c3b8907-536a-3647-a5c0-5124603ea200",
                    "sense_definition": "To leave immediately.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boss is angry, so we should probably <w>get</w>.",
                            "target_lang_translations": {
                                "ru": "Босс злится, так что нам, наверное, пора <w>сваливать</w>."
                            }
                        },

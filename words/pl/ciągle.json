                    "sense_id": "c17570f1-4383-32e7-a25a-a4af21eab04b",
                    "sense_definition": "w dalszym ciągu; oznacza, że stan rzeczy lub czynność trwa i nie uległy zmianie",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "persistence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy ty <w>ciągle</w> jesteś w domu?",
                            "target_lang_translations": {
                                "ru": "Ты <w>всё ещё</w> дома?"
                            }
                        },
                        {
                            "text": "Mimo późnej godziny on <w>ciągle</w> pracuje w biurze.",
                            "target_lang_translations": {
                                "ru": "Несмотря на поздний час, он <w>всё ещё</w> работает в офисе."
                            }
                        }
                    ],
                    "synonyms": [
                        "wciąż",
                        "nadal",
                        "jeszcze",
                        "dalej"
                    ],
                    "common_phrases": [
                        "ciągle jeszcze",
                        "ciągle to samo"
                    ],
                    "target_lang_definitions": {
                        "ru": "по-прежнему; означает, что состояние вещей или действие длится и не изменилось"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всё ещё",
                                "target_lang_sense_clarification": "указание на продолжение действия"
                            },
                            {
                                "target_lang_word": "по-прежнему",
                                "target_lang_sense_clarification": "как и раньше"
                            }
                        ]
                    }
                },
                {

                {
                    "sense_id": "0d92423f-030e-3e7b-b889-503efa7f2e62",
                    "sense_definition": "Powodować, że coś się dzieje, powstaje lub ktoś odczuwa określone emocje; być przyczyną czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nieodpowiedzialne zachowanie <w>sprawia</w>, że wszyscy czują się nieswojo.",
                            "target_lang_translations": {
                                "ru": "Его безответственное поведение <w>заставляет</w> всех чувствовать себя неловко."
                            }
                        },
                        {
                            "text": "To proste zadanie nie powinno <w>sprawiać</w> ci żadnego kłopotu.",
                            "target_lang_translations": {
                                "ru": "Эта простая задача не должна <w>доставлять</w> тебе никаких хлопот."
                            }
                        },
                        {
                            "text": "Obfity deszcz <w>sprawiał</w>, że widoczność na drodze była bardzo niska.",
                            "target_lang_translations": {
                                "ru": "Сильный дождь <w>приводил к тому</w>, что видимость на дороге была очень низкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "powodować",
                        "robić"
                    ],
                    "common_phrases": [
                        "sprawiać wrażenie",
                        "sprawiać przyjemność",
                        "sprawiać kłopot",
                        "sprawiać ból",
                        "sprawiać radość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть причиной чего-либо, вызывать определенные события, эмоции или состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "причинять",
                                "target_lang_sense_clarification": "боль, беспокойство, неприятности"
                            },
                            {
                                "target_lang_word": "доставлять",
                                "target_lang_sense_clarification": "радость, удовольствие, хлопоты"
                            },
                            {
                                "target_lang_word": "производить",

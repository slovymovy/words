            "senses": [
                {
                    "sense_id": "390eeecd-d2f2-3cd8-8e53-0c9b3e108a7a",
                    "sense_definition": "Of common or inferior quality, often referring to standard-issue military supplies.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He refused to wear the <w>provant</w> uniform, preferring to purchase his own clothing of better quality.",
                            "target_lang_translations": {
                                "ru": "Он отказался носить <w>казённую</w> форму, предпочитая покупать собственную одежду лучшего качества."
                            }
                        },
                        {
                            "text": "The <w>provant</w> weapons supplied to the militia broke easily in the heat of battle.",
                            "target_lang_translations": {
                                "ru": "<w>Низкосортное</w> оружие, выданное ополчению, легко ломалось в разгар битвы."
                            }
                        }
                    ],
                    "synonyms": [
                        "inferior",
                        "common",
                        "shoddy",
                        "standard-issue"
                    ],
                    "antonyms": [
                        "superior",
                        "premium",
                        "high-quality"
                    ],
                    "common_phrases": [

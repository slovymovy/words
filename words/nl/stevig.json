    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1faf5d9e-63d9-3dc5-aaf0-e7bbef6f3ca8",
                    "sense_definition": "Sterk en solide; bestand tegen druk, stoten of zwaar gebruik.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_strength",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft een <stevig> huis gebouwd dat elke storm kan doorstaan.",
                            "target_lang_translations": {
                                "ru": "Он построил <прочный> дом, который может выдержать любой шторм.",
                                "en": "He built a <sturdy> house that can withstand any storm."
                            }
                        },
                        {
                            "text": "Zorg ervoor dat de knoop <stevig> vastzit voordat je vertrekt.",
                            "target_lang_translations": {
                                "ru": "Убедись, что узел <крепко> завязан, прежде чем уходить.",
                                "en": "Make sure the knot is <firmly> attached before you leave."
                            }
                        }

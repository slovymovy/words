                {
                    "sense_id": "21fe6101-b81d-368e-bb00-d5b6694f3cbc",
                    "sense_definition": "Relating to members of the military who do not hold a commission as an officer.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>enlisted</w> personnel live in barracks separate from the officers' quarters.",
                            "target_lang_translations": {
                                "ru": "<w>Рядовой</w> состав живет в казармах отдельно от офицерских помещений."
                            }
                        },
                        {
                            "text": "He rose through the <w>enlisted</w> ranks to become a sergeant major.",
                            "target_lang_translations": {
                                "ru": "Он выслужился из <w>рядовых</w> и стал старшим сержантом."
                            }
                        }
                    ],
                    "synonyms": [
                        "non-commissioned"
                    ],
                    "antonyms": [

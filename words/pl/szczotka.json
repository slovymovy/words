                    ],
                    "common_phrases": [
                        "szczotki węglowe",
                        "wymiana szczotek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент электродвигателя или генератора, обеспечивающий электрический контакт с вращающейся частью машины"
                    },
                    "translations": {

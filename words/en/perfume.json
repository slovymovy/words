{
    "word_family": [
        "perfumer",
        "perfumery",
        "perfumed"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "04d14be6-a602-37bb-a83a-3cff549590c1",
                    "sense_definition": "A liquid substance created to provide a pleasant smell, usually worn on the body.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a bottle of expensive <w>perfume</w> at the airport.",
                            "target_lang_translations": {
                                "ru": "Она купила флакон дорогих <w>духов</w> в аэропорту."
                            }
                        },
                        {
                            "text": "He likes to wear <w>perfume</w> when he goes out for dinner.",
                            "target_lang_translations": {
                                "ru": "Ему нравится пользоваться <w>парфюмом</w>, когда он идет ужинать."
                            }
                        }
                    ],
                    "synonyms": [
                        "fragrance",
                        "scent",
                        "parfum"
                    ],
                    "common_phrases": [
                        "wear perfume",
                        "bottle of perfume",
                        "expensive perfume"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкое вещество, созданное для придания приятного запаха, обычно наносимое на тело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "духи́",
                                "target_lang_sense_clarification": "парфюмерное средство"
                            },
                            {
                                "target_lang_word": "парфю́м",
                                "target_lang_sense_clarification": "разговорное, общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72416554-1a1a-3095-a402-39291d3f9d6d",
                    "sense_definition": "A pleasant smell, especially one coming from flowers or a sweet-smelling substance.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The garden was filled with the sweet <w>perfume</w> of roses.",
                            "target_lang_translations": {
                                "ru": "Сад был наполнен сладким <w>ароматом</w> роз."
                            }
                        },
                        {
                            "text": "A soft breeze brought the <w>perfume</w> of lilacs into the room.",
                            "target_lang_translations": {
                                "ru": "Легкий ветерок принес в комнату <w>благоухание</w> сирени."
                            }
                        }
                    ],
                    "synonyms": [
                        "aroma",
                        "fragrance",
                        "scent"
                    ],
                    "common_phrases": [
                        "sweet perfume",
                        "perfume of flowers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приятный запах, особенно исходящий от цветов или душистого вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арома́т",
                                "target_lang_sense_clarification": "приятный запах"
                            },
                            {
                                "target_lang_word": "благоуха́ние",
                                "target_lang_sense_clarification": "сильный, приятный запах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "eab109dd-7f4d-3f51-9cb0-153e7c7c8c23",
                    "sense_definition": "To fill a space or object with a pleasant smell; to apply scent to something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The blooming flowers <w>perfumed</w> the entire garden in the spring.",
                            "target_lang_translations": {
                                "ru": "Весной цветущие цветы <w>наполнили ароматом</w> весь сад."
                            }
                        },
                        {
                            "text": "She <w>perfumed</w> her handkerchief before putting it in her purse.",
                            "target_lang_translations": {
                                "ru": "Она <w>надушила</w> свой носовой платок, перед тем как положить его в сумочку."
                            }
                        }
                    ],
                    "synonyms": [
                        "scent"
                    ],
                    "common_phrases": [
                        "perfume the air",
                        "perfume the room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наполнять пространство или предмет приятным запахом; наносить духи на что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надуши́ть",
                                "target_lang_sense_clarification": "нанести духи на предмет или человека"
                            },
                            {
                                "target_lang_word": "наполня́ть арома́том",
                                "target_lang_sense_clarification": "насыщать запахом пространство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
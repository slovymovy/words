    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9654f278-a986-3d38-93e1-1fe8f12b76e1",
                    "sense_definition": "Zich intensief met een onderwerp of situatie bezighouden om er meer kennis over te krijgen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_study",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil me graag <w>verdiepen</w> in de geschiedenis van deze oude stad.",
                            "target_lang_translations": {
                                "en": "I would like to <w>delve into</w> the history of this old city.",
                                "ru": "Я бы хотел <w>углубиться</w> в историю этого старого города."
                            }
                        },
                        {
                            "text": "Hij heeft zich de afgelopen weken volledig in het dossier <w>verdiept</w>.",
                            "target_lang_translations": {
                                "en": "He has fully <w>immersed himself</w> in the file over the past few weeks.",
                                "ru": "В последние недели он полностью <w>погрузился</w> в изучение этого досье."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestuderen",
                        "onderzoeken",
                        "uitpluizen"
                    ],
                    "common_phrases": [
                        "zich verdiepen in",
                        "zich verder verdiepen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wederkerend (zich verdiepen)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To engage intensively with a subject or situation to gain more knowledge about it.",
                        "ru": "Интенсивно заниматься предметом или ситуацией, чтобы получить о них больше знаний."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "delve into",
                                "target_lang_sense_clarification": "to research thoroughly"
                            },
                            {
                                "target_lang_word": "immerse oneself in",
                                "target_lang_sense_clarification": "to involve oneself deeply"
                            },
                            {
                                "target_lang_word": "go into",
                                "target_lang_sense_clarification": "to examine details"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "углубляться",
                                "target_lang_sense_clarification": "в изучение, в детали"
                            },
                            {
                                "target_lang_word": "вникать",
                                "target_lang_sense_clarification": "разбираться в сути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92dcf50a-dc45-393b-8078-ce11ca24e072",
                    "sense_definition": "De afstand tussen het oppervlak en de bodem van iets vergroten; iets dieper maken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_deepening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente gaat de vaargeul <w>verdiepen</w> voor grotere schepen.",
                            "target_lang_translations": {
                                "en": "The municipality is going to <w>deepen</w> the fairway for larger ships.",
                                "ru": "Муниципалитет собирается <w>углубить</w> фарватер для более крупных судов."
                            }
                        },
                        {
                            "text": "We moeten de sloot <w>verdiepen</w> om wateroverlast te voorkomen.",
                            "target_lang_translations": {
                                "en": "We need to <w>deepen</w> the ditch to prevent flooding.",
                                "ru": "Нам нужно <w>углубить</w> канаву, чтобы предотвратить подтопление."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitdiepen"
                    ],
                    "antonyms": [
                        "ophogen"
                    ],
                    "common_phrases": [
                        "een kanaal verdiepen",
                        "de bodem verdiepen"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase the distance between the surface and the bottom of something; to make something deeper.",
                        "ru": "Делать что-либо более глубоким; увеличивать глубину."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deepen",
                                "target_lang_sense_clarification": "physical distance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "углублять",
                                "target_lang_sense_clarification": "делать глубже физически"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "fc660d2e-5e19-3f69-b05c-7ae8d3db37e5",
                    "sense_definition": "Forma dopełniacza liczby pojedynczej rzeczownika <w>lotek</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cena jednego zakładu <w>lotka</w> znowu wzrosła.",
                            "target_lang_translations": {
                                "ru": "Цена одной ставки в <w>лото</w> снова выросла."

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ee683c02-293f-34b6-bbc3-47dbcf41a525",
                    "sense_definition": "Słodkie ciasto, którego głównym składnikiem jest masa z białego sera (twarogu), jajek i cukru.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na deser zamówiłem kawę i kawałek pysznego <w>sernika</w>.",
                            "target_lang_translations": {
                                "ru": "На десерт я заказал кофе и кусочек вкусного <w>чизкейка</w>."
                            }
                        },
                        {
                            "text": "Moja babcia zawsze piecze <w>sernik</w> z rodzynkami na święta.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка всегда печет на праздники <w>творожник</w> с изюмом."
                            }
                        }
                    ],
                    "synonyms": [
                        "serowiec",
                        "serownik",
                        "gw. (Śląsk Cieszyński) syrzok"
                    ],
                    "common_phrases": [
                        "sernik na zimno",
                        "sernik wiedeński",
                        "sernik krakowski",
                        "kawałek sernika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкий пирог, основным ингредиентом которого является масса из белого сыра (творога), яиц и сахара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чизкейк",
                                "target_lang_sense_clarification": "популярный десерт"
                            },
                            {
                                "target_lang_word": "творожный пирог",
                                "target_lang_sense_clarification": "выпечка"
                            },
                            {
                                "target_lang_word": "творожник",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "942fc480-132c-3e2d-871c-0bb7344907d4",
                    "sense_definition": "Pomieszczenie przeznaczone do suszenia serów w procesie ich tradycyjnego wyrobu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starym <w>serniku</w> panował specyficzny zapach dojrzewającego nabiału.",
                            "target_lang_translations": {
                                "ru": "В старой <w>сушильне</w> стоял специфический запах созревающих молочных продуктов."
                            }
                        },
                        {
                            "text": "Gospodarz zaprowadził nas do drewnianego <w>sernika</w>, gdzie schły oscypki.",
                            "target_lang_translations": {
                                "ru": "Хозяин отвел нас в деревянную <w>сушильню</w>, где сохли осцыпки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "określenie rzadkie, dotyczące tradycyjnego przetwórstwa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещение, предназначенное для сушки сыров в процессе их традиционного изготовления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сушильня для сыра",
                                "target_lang_sense_clarification": "помещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e499033d-ea29-3ed2-ba90-798b6230daea",
                    "sense_definition": "Dawna nazwa kazeiny, białka występującego w mleku.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych podręcznikach chemii kazeinę nazywano czasem <w>sernikiem</w>.",
                            "target_lang_translations": {
                                "ru": "В старых учебниках химии казеин иногда называли <w>сырником</w>."
                            }
                        },
                        {
                            "text": "Substancja ta, znana niegdyś jako <w>sernik</w>, jest podstawowym białkiem mleka.",
                            "target_lang_translations": {
                                "ru": "Это вещество, известное когда-то как <w>сырник</w>, является основным белком молока."
                            }
                        }
                    ],
                    "synonyms": [
                        "kazeina"
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": "współcześnie używa się terminu kazeina"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название казеина, белка, содержащегося в молоке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "казеин",
                                "target_lang_sense_clarification": "химическое вещество"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e921101-0751-35df-9be2-88a44dcb1c2c",
                    "sense_definition": "The sweet and fleshy product of a tree or other plant that contains seed and can be eaten as food.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I try to eat at least one piece of fresh <w>fruit</w> every day for a snack.",
                            "target_lang_translations": {
                                "ru": "Я стараюсь съедать хотя бы один свежий <w>фрукт</w> каждый день в качестве перекуса."
                            }
                        },
                        {
                            "text": "The market stall was full of colourful <w>fruit</w> like apples, bananas, and oranges.",
                            "target_lang_translations": {
                                "ru": "Рыночный прилавок был полон разноцветных <w>фруктов</w>, таких как яблоки, бананы и апельсины."
                            }
                        }
                    ],
                    "synonyms": [
                        "produce"
                    ],
                    "antonyms": [
                        "vegetable"
                    ],
                    "common_phrases": [
                        "fresh fruit",
                        "fruit salad",
                        "fruit juice",
                        "tropical fruit",
                        "ripe fruit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкий и мясистый продукт дерева или другого растения, содержащий семена и употребляемый в пищу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фрукт",
                                "target_lang_sense_clarification": "съедобный плод"
                            },
                            {
                                "target_lang_word": "плод",
                                "target_lang_sense_clarification": "ботанический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09ba7ef7-ab0e-3152-988f-e0005fd15fd5",
                    "sense_definition": "The good result or reward that comes from work or an activity.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After years of saving money, they finally enjoyed the <w>fruits</w> of their labour.",
                            "target_lang_translations": {
                                "ru": "После долгих лет экономии они наконец насладились <w>плодами</w> своего труда."
                            }
                        },
                        {
                            "text": "The success of the new product was the <w>fruit</w> of the team's hard work.",
                            "target_lang_translations": {
                                "ru": "Успех нового продукта стал <w>плодом</w> упорной работы команды."
                            }
                        }
                    ],
                    "synonyms": [
                        "result",
                        "outcome",
                        "reward",
                        "consequence",
                        "benefit"
                    ],
                    "common_phrases": [
                        "fruits of one's labor",
                        "bear fruit",
                        "first fruits"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хороший результат или награда, полученные в результате работы или деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плод",
                                "target_lang_sense_clarification": "результат усилий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1d55f3a-2910-30cb-823a-a11049a5d68e",
                    "sense_definition": "A child or offspring from a parent.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the old text, the king looked upon the prince as the <w>fruit</w> of his royal line.",
                            "target_lang_translations": {
                                "ru": "В старинном тексте король взирал на принца как на <w>плод</w> своего королевского рода."
                            }
                        },
                        {
                            "text": "She blessed the <w>fruit</w> of her womb with a prayer.",
                            "target_lang_translations": {
                                "ru": "Она благословила <w>плод</w> своего чрева молитвой."
                            }
                        }
                    ],
                    "synonyms": [
                        "offspring",
                        "progeny",
                        "issue"
                    ],
                    "common_phrases": [
                        "fruit of one's loins",
                        "fruit of the womb"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary or biblical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ребёнок или потомство родителя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плод",
                                "target_lang_sense_clarification": "устаревшее или библейское"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "568cc8e2-8e20-397d-91fc-09777bb8899b",
                    "sense_definition": "An offensive term for a homosexual man.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was deeply offended when the bully called him a <w>fruit</w> in front of everyone.",
                            "target_lang_translations": {
                                "ru": "Он был глубоко оскорблен, когда хулиган назвал его <w>гомиком</w> при всех."
                            }
                        },
                        {
                            "text": "Using the word <w>fruit</w> to describe a person is considered derogatory and rude.",
                            "target_lang_translations": {
                                "ru": "Использование слова «<w>гомик</w>» для описания человека считается унизительным и грубым."
                            }
                        }
                    ],
                    "synonyms": [
                        "fairy",
                        "pansy",
                        "poof",
                        "queer",
                        "homosexual"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оскорбительный термин для гомосексуального мужчины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гомик",
                                "target_lang_sense_clarification": "сленг, оскорбительное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "18ef0ea0-8445-37d6-b016-36631e48d1f2",
                    "sense_definition": "To produce fruit, seeds, or spores.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The apple trees in the orchard usually <w>fruit</w> late in the summer.",
                            "target_lang_translations": {
                                "ru": "Яблони в саду обычно <w>плодоносят</w> в конце лета."
                            }
                        },
                        {
                            "text": "Some types of fungi will <w>fruit</w> only when the weather is very damp.",
                            "target_lang_translations": {
                                "ru": "Некоторые виды грибов <w>плодоносят</w> только в очень сырую погоду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "start fruiting",
                        "fruit early"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приносить плоды, семена или споры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плодоносить",
                                "target_lang_sense_clarification": "давать плоды"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a8c508eb-251a-355e-93ac-90ada34bdae0",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Fruit</w> signed the guestbook at the hotel reception.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Фрут</w> расписался в гостевой книге на стойке регистрации отеля."
                            }
                        },
                        {
                            "text": "We looked up the family history of the <w>Fruits</w> in the local archives.",
                            "target_lang_translations": {
                                "ru": "Мы искали историю семьи <w>Фрутов</w> в местных архивах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "przetrwać",
        "wytrwać",
        "dotrwać",
        "trwały",
        "trwałość",
        "wytrwałość"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c70dc1e6-2d03-3095-8131-25024db0651b",
                    "sense_definition": "Istnieć, dziać się lub odbywać się przez określony czas",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten ciekawy film <trwał> ponad dwie godziny.",
                            "target_lang_translations": {
                                "ru": "Этот интересный фильм <длился> более двух часов."
                            }
                        },
                        {
                            "text": "Zima w tym roku <trwa> wyjątkowo długo i jest mroźna.",
                            "target_lang_translations": {
                                "ru": "Зима в этом году <длится> исключительно долго и она морозная."
                            }
                        }
                    ],
                    "antonyms": [
                        "kończyć się",
                        "ustawać"
                    ],
                    "common_phrases": [
                        "trwać wiecznie",
                        "trwać chwilę",
                        "trwać godzinami",
                        "trwać w nieskończoność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить, существовать или занимать определенный промежуток времени"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "длиться",
                                "target_lang_sense_clarification": "о времени или событиях"
                            },
                            {
                                "target_lang_word": "продолжаться",
                                "target_lang_sense_clarification": "не прекращаться, идти дальше"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57428a3a-0037-355e-9587-fd927a7d50b1",
                    "sense_definition": "Pozostawać w niezmienionym stanie, sytuacji lub miejscu",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała sala <trwała> w milczeniu, czekając na ogłoszenie wyników.",
                            "target_lang_translations": {
                                "ru": "Весь зал <пребывал> в молчании, ожидая объявления результатов."
                            }
                        },
                        {
                            "text": "Mimo zmęczenia żołnierze musieli <trwać> na posterunku do świtu.",
                            "target_lang_translations": {
                                "ru": "Несмотря на усталость, солдаты должны были <оставаться> на посту до рассвета."
                            }
                        }
                    ],
                    "synonyms": [
                        "pozostawać",
                        "utrzymywać się"
                    ],
                    "antonyms": [
                        "zmieniać się"
                    ],
                    "common_phrases": [
                        "trwać w milczeniu",
                        "trwać w bezruchu",
                        "trwać w niepewności",
                        "trwać w zachwycie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставаться в неизменном состоянии, ситуации или месте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пребывать",
                                "target_lang_sense_clarification": "находиться в каком-либо состоянии (книжн.)"
                            },
                            {
                                "target_lang_word": "оставаться",
                                "target_lang_sense_clarification": "сохранять положение или состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "554808fa-63bd-37d6-9a94-e92ea26c1860",
                    "sense_definition": "Uporczywie obstawać przy czymś, nie zmieniać zdania ani postawy",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo przedstawionych dowodów, on nadal <trwa> przy swoim zdaniu.",
                            "target_lang_translations": {
                                "ru": "Несмотря на представленные доказательства, он продолжает <стоять> на своём."
                            }
                        },
                        {
                            "text": "Nie warto <trwać> w błędzie, gdy fakty mówią co innego.",
                            "target_lang_translations": {
                                "ru": "Не стоит <упорствовать> в заблуждении, когда факты говорят об обратном."
                            }
                        }
                    ],
                    "synonyms": [
                        "obstawać"
                    ],
                    "antonyms": [
                        "ustępować",
                        "rezygnować"
                    ],
                    "common_phrases": [
                        "trwać przy swoim",
                        "trwać w błędzie",
                        "trwać w przekonaniu",
                        "trwać w postanowieniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво придерживаться чего-либо, не меняя мнения или позиции"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоять на своём",
                                "target_lang_sense_clarification": "не уступать в споре или мнении"
                            },
                            {
                                "target_lang_word": "упорствовать",
                                "target_lang_sense_clarification": "проявлять упрямство или настойчивость"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
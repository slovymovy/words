            "pos": "verb",
            "senses": [
                {
                    "sense_id": "45078df3-0265-3595-998f-4deca74f9356",
                    "sense_definition": "Oddzielić siłą jedną część od drugiej lub coś od powierzchni, do której było przytwierdzone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz mocno pociągnąć, żeby <oderwać> ten stary plaster od skóry.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно сильно потянуть, чтобы <оторвать> этот старый пластырь от кожи."
                            }
                        },
                        {
                            "text": "Udało mi się <oderwać> kawałek świeżego, chrupiącego chleba.",
                            "target_lang_translations": {
                                "ru": "Мне удалось <оторвать> кусок свежего, хрустящего хлеба."
                            }
                        }
                    ],
                    "synonyms": [
                        "odłączyć",
                        "urwać",
                        "odczepić"
                    ],
                    "antonyms": [
                        "przyczepić",
                        "przymocować",
                        "przykleić"
                    ],
                    "common_phrases": [
                        "oderwać kawałek",
                        "oderwać metkę",
                        "oderwać naklejkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отделить силой одну часть от другой или что-то от поверхности, к которой оно было прикреплено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оторвать",
                                "target_lang_sense_clarification": "физическое действие"
                            },
                            {
                                "target_lang_word": "отделить",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "754c4c55-cfd1-3aa5-a994-4cadc1fc1aef",
                    "sense_definition": "Odłączyć się samoistnie od całości lub odpaść od powierzchni (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zauważyłem, że od mojego płaszcza <oderwał> się guzik.",
                            "target_lang_translations": {
                                "ru": "Я заметил, что от моего пальто <оторвалась> пуговица."
                            }
                        },
                        {
                            "text": "Stara farba zaczęła płatami <oderwać> się od sufitu w łazience.",
                            "target_lang_translations": {
                                "ru": "Старая краска начала кусками <отрываться> от потолка в ванной."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpaść",
                        "odczepić się"
                    ],
                    "antonyms": [
                        "przyczepić się"
                    ],
                    "common_phrases": [
                        "guzik się oderwał",
                        "ucho się oderwało"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самопроизвольно отделиться от целого или отпасть от поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оторваться",
                                "target_lang_sense_clarification": "возвратный глагол"
                            },
                            {
                                "target_lang_word": "отвалиться",
                                "target_lang_sense_clarification": "разговорное, об отпадании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8aa1874-ed71-3dbc-9dcf-032881728a9e",
                    "sense_definition": "Sprawić, że ktoś przestanie wykonywać jakąś czynność lub przestanie o czymś myśleć; często w kontekście odwracania wzroku lub uwagi.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attention_interruption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Widok był tak piękny, że nie mogłem od niego <oderwać> wzroku.",
                            "target_lang_translations": {
                                "ru": "Вид был так красив, что я не мог <оторвать> от него глаз."
                            }
                        },
                        {
                            "text": "Nagły hałas na ulicy <oderwał> mnie od czytania książki.",
                            "target_lang_translations": {
                                "ru": "Внезапный шум на улице <оторвал> меня от чтения книги."
                            }
                        }
                    ],
                    "synonyms": [
                        "odciągnąć",
                        "rozproszyć"
                    ],
                    "antonyms": [
                        "skupić"
                    ],
                    "common_phrases": [
                        "oderwać wzrok",
                        "oderwać myśli",
                        "oderwać od pracy",
                        "oderwać od rzeczywistości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то прекратить действие или перестать думать о чем-то; часто в контексте отвода взгляда или внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оторвать",
                                "target_lang_sense_clarification": "взгляд или от дела"
                            },
                            {
                                "target_lang_word": "отвлечь",
                                "target_lang_sense_clarification": "внимание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "daa1742d-d04f-3878-be9c-1ebf3837d3d7",
                    "sense_definition": "Odłączyć się od jakiejś grupy, organizacji lub terytorium, aby stać się niezależnym (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "political_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten bogaty region próbuje <oderwać> się od reszty kraju i ogłosić niepodległość.",
                            "target_lang_translations": {
                                "ru": "Этот богатый регион пытается <отделиться> от остальной части страны и провозгласить независимость."
                            }
                        },
                        {
                            "text": "Grupa niezadowolonych posłów postanowiła <oderwać> się od partii rządzącej.",
                            "target_lang_translations": {
                                "ru": "Группа недовольных депутатов решила <отколоться> от правящей партии."
                            }
                        }
                    ],
                    "synonyms": [
                        "odłączyć się",
                        "uniezależnić się"
                    ],
                    "antonyms": [
                        "przyłączyć się",
                        "zjednoczyć się"
                    ],
                    "common_phrases": [
                        "oderwać się od grupy",
                        "oderwać się od stada"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отделиться от группы, организации или территории, чтобы стать независимым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отделиться",
                                "target_lang_sense_clarification": "стать независимым"
                            },
                            {
                                "target_lang_word": "отколоться",
                                "target_lang_sense_clarification": "о части группы"
                            }
                        ]
                    }
                }
            ]
        }

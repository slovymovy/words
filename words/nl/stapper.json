{
    "word_family": [
        "stap",
        "stappen",
        "staptocht"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c16064ab-f669-3c37-b4d5-e0db559f2590",
                    "sense_definition": "Iemand die wandelt of een tocht te voet maakt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_walking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>stapper</w> genoot van de mooie natuur tijdens zijn lange tocht.",
                            "target_lang_translations": {
                                "ru": "<w>Пеший турист</w> наслаждался красивой природой во время своего долгого похода."
                            }
                        },
                        {
                            "text": "Als snelle <w>stapper</w> was hij veel eerder op de bestemming dan de rest.",
                            "target_lang_translations": {
                                "ru": "Будучи быстрым <w>ходоком</w>, он добрался до места назначения намного раньше остальных."
                            }
                        }
                    ],
                    "synonyms": [
                        "wandelaar",
                        "voetganger"
                    ],
                    "common_phrases": [
                        "geoefende stapper",
                        "stevige stapper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который ходит пешком или совершает пешую прогулку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходок",
                                "target_lang_sense_clarification": "человек, который ходит быстро или много"
                            },
                            {
                                "target_lang_word": "пеший турист",
                                "target_lang_sense_clarification": "человек, совершающий поход"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6a369cc-5505-3756-b109-4b8b89d26056",
                    "sense_definition": "Een stevige en comfortabele schoen, geschikt om ver mee te wandelen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "shoe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trek je <w>stappers</w> aan, want we gaan een heel eind lopen in het bos.",
                            "target_lang_translations": {
                                "ru": "Надевай свои <w>ботинки</w>, потому что нам предстоит долгая прогулка по лесу."
                            }
                        },
                        {
                            "text": "Ik heb nieuwe <w>stappers</w> gekocht omdat mijn oude schoenen versleten waren.",
                            "target_lang_translations": {
                                "ru": "Я купил новые <w>ботинки для ходьбы</w>, потому что мои старые туфли износились."
                            }
                        }
                    ],
                    "synonyms": [
                        "wandelschoen",
                        "loopschoen"
                    ],
                    "common_phrases": [
                        "stevige stappers",
                        "nieuwe stappers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочная и удобная обувь, подходящая для длительных прогулок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ботинки для ходьбы",
                                "target_lang_sense_clarification": "специализированная обувь"
                            },
                            {
                                "target_lang_word": "удобная обувь",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe4b7281-230e-386f-98d4-5af9b095a78d",
                    "sense_definition": "Iemand die vaak uitgaat voor zijn plezier, bijvoorbeeld naar een café of club.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_going_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een echte <w>stapper</w> en ligt in het weekend nooit vroeg in bed.",
                            "target_lang_translations": {
                                "ru": "Он настоящий <w>тусовщик</w> и в выходные никогда не ложится спать рано."
                            }
                        },
                        {
                            "text": "De stad was op zaterdagavond vol met vrolijke <w>stappers</w>.",
                            "target_lang_translations": {
                                "ru": "В субботу вечером город был полон веселых <w>гуляк</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitgaanstype"
                    ],
                    "common_phrases": [
                        "een echte stapper",
                        "verwoed stapper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который часто ходит развлекаться, например, в кафе или клубы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тусовщик",
                                "target_lang_sense_clarification": "любитель вечеринок"
                            },
                            {
                                "target_lang_word": "гуляка",
                                "target_lang_sense_clarification": "любитель разгульной жизни"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d5031669-2b28-3d49-ac27-57014addf742",
                    "sense_definition": "Начать делать что-либо снова после перерыва.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "resume_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <возобновить> наши еженедельные встречи после отпуска.",
                            "target_lang_translations": {
                                "en": "We decided to <resume> our weekly meetings after the vacation."
                            }
                        },
                        {
                            "text": "Команда планирует <возобновить> работу над проектом в понедельник.",
                            "target_lang_translations": {
                                "en": "The team plans to <resume> work on the project on Monday."
                            }
                        }
                    ],
                    "synonyms": [
                        "продлить"
                    ],
                    "antonyms": [
                        "прекратить",
                        "прервать"
                    ],
                    "common_phrases": [
                        "возобновить переговоры",
                        "возобновить работу",
                        "возобновить занятия"
                    ],
                    "target_lang_definitions": {
                        "en": "To start doing something again after a break."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resume",
                                "target_lang_sense_clarification": "to start again after an interruption or pause"
                            },
                            {
                                "target_lang_word": "restart",
                                "target_lang_sense_clarification": "to start again"
                            },
                            {
                                "target_lang_word": "renew",
                                "target_lang_sense_clarification": "to begin again, often with fresh vigor"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9fb17f0-6d29-3da3-8be3-073cecd3ef82",
                    "sense_definition": "Продлить срок действия документа, соглашения или подписки.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "renew_document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно срочно <возобновить> читательский билет в библиотеке.",
                            "target_lang_translations": {
                                "en": "I urgently need to <renew> my library card."
                            }

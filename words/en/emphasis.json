{
    "word_family": [
        "emphasize",
        "emphatic",
        "emphatically"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eb9b01f4-15f9-37c5-bfc4-cdb34af586f3",
                    "sense_definition": "Special attention or prominence given to something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our school puts a strong <w>emphasis</w> on academic achievement.",
                            "target_lang_translations": {
                                "ru": "Наша школа делает большой <w>упор</w> на академическую успеваемость."
                            }
                        },
                        {
                            "text": "The <w>emphasis</w> is on quality rather than quantity.",
                            "target_lang_translations": {
                                "ru": "<w>Акцент</w> делается на качество, а не на количество."
                            }
                        }
                    ],
                    "synonyms": [
                        "priority",
                        "stress",
                        "prominence",
                        "weight",
                        "significance"
                    ],
                    "antonyms": [
                        "insignificance"
                    ],
                    "common_phrases": [
                        "put emphasis on",
                        "lay emphasis on",
                        "place emphasis on",
                        "shift of emphasis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особое внимание или значимость, придаваемые чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акцент",
                                "target_lang_sense_clarification": "выделение важного"
                            },
                            {
                                "target_lang_word": "упор",
                                "target_lang_sense_clarification": "сосредоточение усилий"
                            },
                            {
                                "target_lang_word": "особое внимание",
                                "target_lang_sense_clarification": "пристальное внимание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af4992c9-809d-33fa-82bb-4ce9f7889f68",
                    "sense_definition": "Prominence given to a syllable or words, by raising the voice or printing in italic or underlined type.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "linguistic_stress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the word 'development', the <w>emphasis</w> is on the second syllable.",
                            "target_lang_translations": {
                                "ru": "В слове «development» <w>ударение</w> падает на второй слог."
                            }
                        },
                        {
                            "text": "He used a yellow highlighter to indicate where to give <w>emphasis</w> in his speech.",
                            "target_lang_translations": {
                                "ru": "Он использовал жёлтый маркер, чтобы отметить, где нужно сделать <w>акцент</w> в речи."
                            }
                        }
                    ],
                    "synonyms": [
                        "stress",
                        "accent"
                    ],
                    "common_phrases": [
                        "syllable emphasis",
                        "give emphasis to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделение слога или слова повышением голоса или шрифтом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ударение",
                                "target_lang_sense_clarification": "фонетическое выделение"
                            },
                            {
                                "target_lang_word": "акцент",
                                "target_lang_sense_clarification": "выделение в речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e9d4631-c42b-3e28-8875-042f29412bbe",
                    "sense_definition": "Special weight or forcefulness given to something considered important.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "forcefulness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He paused for <w>emphasis</w> before saying who had won.",
                            "target_lang_translations": {
                                "ru": "Он сделал паузу для <w>выразительности</w>, прежде чем сказать, кто победил."
                            }
                        },
                        {
                            "text": "She shook her head with great <w>emphasis</w> to show her disagreement.",
                            "target_lang_translations": {
                                "ru": "Она покачала головой с особой <w>выразительностью</w>, чтобы показать своё несогласие."
                            }
                        }
                    ],
                    "synonyms": [
                        "force",
                        "vigor",
                        "vehemence"
                    ],
                    "common_phrases": [
                        "pause for emphasis",
                        "add emphasis",
                        "with emphasis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особая сила или выразительность, придаваемая словам или действиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выразительность",
                                "target_lang_sense_clarification": "экспрессивность"
                            },
                            {
                                "target_lang_word": "значительность",
                                "target_lang_sense_clarification": "важность вида или тона"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "df38c9fc-8845-3e2c-9669-eb13b69c2e84",
                    "sense_definition": "Сестра отца или матери, а также жена дяди.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kinship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя <w>тётя</w> живёт в соседнем городе и часто приезжает к нам.",
                            "target_lang_translations": {
                                "en": "My <w>aunt</w> lives in a neighboring city and often comes to visit us."
                            }
                        },
                        {
                            "text": "На летние каникулы родители отправили меня в деревню к <w>тёте</w>.",
                            "target_lang_translations": {
                                "en": "For the summer holidays, my parents sent me to the village to stay with my <w>aunt</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "тётка",
                        "тётушка",
                        "тётенька"
                    ],
                    "antonyms": [
                        "дядя",
                        "племянник",
                        "племянница"
                    ],
                    "common_phrases": [
                        "родная тётя",
                        "двоюродная тётя",
                        "ехать к тёте",
                        "любимая тётя"
                    ],
                    "target_lang_definitions": {
                        "en": "The sister of one's father or mother, or the wife of one's uncle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "aunt",
                                "target_lang_sense_clarification": "family member"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd0d06fc-3a1b-35b2-b298-9c104e199b9f",
                    "sense_definition": "Взрослая женщина (обычно в речи детей или в разговорном стиле).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "generic_woman",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький мальчик подошёл к незнакомой <w>тёте</w> и спросил время.",
                            "target_lang_translations": {
                                "en": "A little boy approached an unfamiliar <w>lady</w> and asked for the time."
                            }
                        },
                        {
                            "text": "Какая-то добрая <w>тётя</w> помогла мне найти дорогу домой.",
                            "target_lang_translations": {
                                "en": "Some kind <w>lady</w> helped me find my way home."
                            }
                        }
                    ],
                    "synonyms": [
                        "женщина",
                        "тётенька"
                    ],
                    "antonyms": [
                        "дядя"
                    ],
                    "common_phrases": [
                        "чужая тётя",
                        "незнакомая тётя",
                        "добрая тётя"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное, часто детское"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An adult woman (often used in children's speech or colloquially)."
                    },
                    "translations": {

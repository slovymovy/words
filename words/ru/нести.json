                    "target_lang_definitions": {
                        "en": "To move or transport something along by the force of a current, wind, or other movement"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "to transport by force of motion"
                            },
                            {
                                "target_lang_word": "sweep",
                                "target_lang_sense_clarification": "to move something along forcefully"
                            },
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "to propel along (by wind/water)"
                            }

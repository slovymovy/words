                    "synonyms": [
                        "rheme"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть предложения, которая сообщает новую информацию относительно текущей темы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рема",
                                "target_lang_sense_clarification": "лингвистический термин"
                            },
                            {
                                "target_lang_word": "комментарий",
                                "target_lang_sense_clarification": "в актуальном членении предложения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "435e466e-5aad-3502-a763-76bec96591d6",

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "aba7ad07-5899-3a80-bbfe-666c417e4fc6",
                    "sense_definition": "Niet altijd, maar op bepaalde momenten.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga <w>soms</w> met de fiets naar mijn werk.",
                            "target_lang_translations": {
                                "en": "I <w>sometimes</w> go to work by bike.",
                                "ru": "Я <w>иногда</w> езжу на работу на велосипеде."
                            }
                        },
                        {
                            "text": "In Nederland regent het <w>soms</w> de hele dag.",
                            "target_lang_translations": {
                                "en": "In the Netherlands it <w>sometimes</w> rains all day.",
                                "ru": "В Нидерландах <w>иногда</w> весь день идет дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "af en toe",
                        "weleens"
                    ],
                    "antonyms": [
                        "altijd",
                        "nooit"
                    ],
                    "common_phrases": [
                        "soms wel",
                        "soms niet"
                    ],
                    "target_lang_definitions": {
                        "en": "Not always, but at certain times.",
                        "ru": "Не всегда, время от времени; указывает на периодичность действия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sometimes",
                                "target_lang_sense_clarification": "at times"
                            },
                            {
                                "target_lang_word": "occasionally",
                                "target_lang_sense_clarification": "now and then"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "иногда",
                                "target_lang_sense_clarification": "время от времени"
                            },
                            {
                                "target_lang_word": "порой",
                                "target_lang_sense_clarification": "изредка, временами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5c76180-f94a-3206-befb-2f0f144e0b19",
                    "sense_definition": "Misschien; drukt een vermoeden of een voorzichtige vraag uit.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ben je <w>soms</w> boos op mij?",
                            "target_lang_translations": {
                                "en": "Are you angry with me <w>by any chance</w>?",
                                "ru": "Ты <w>случайно</w> не сердишься на меня?"
                            }
                        },
                        {
                            "text": "Heb je <w>soms</w> een pen voor mij?",
                            "target_lang_translations": {
                                "en": "Do you <w>happen to</w> have a pen for me?",
                                "ru": "У тебя <w>случайно</w> нет для меня ручки?"
                            }
                        }
                    ],
                    "synonyms": [
                        "af en toe",
                        "weleens"
                    ],
                    "target_lang_definitions": {
                        "en": "Maybe; expresses a suspicion or a tentative question.",
                        "ru": "Случайно, может быть; используется в вопросительных предложениях для выражения вежливого предположения или догадки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "by any chance",
                                "target_lang_sense_clarification": "used in questions"
                            },
                            {
                                "target_lang_word": "happen to",
                                "target_lang_sense_clarification": "used to ask politely"
                            },
                            {
                                "target_lang_word": "perhaps",
                                "target_lang_sense_clarification": "maybe"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "случайно",
                                "target_lang_sense_clarification": "в вопросах (не... ли случайно)"
                            },
                            {
                                "target_lang_word": "часом",
                                "target_lang_sense_clarification": "разговорное, в значении «случайно»"
                            },
                            {
                                "target_lang_word": "может быть",
                                "target_lang_sense_clarification": "возможно"
                            }
                        ]
                    }
                }
            ]
        },

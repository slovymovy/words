    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0bd0271e-ab97-3cfd-8dfd-88bbf3868b30",
                    "sense_definition": "Een plaats verlaten door te lopen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verlaten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zag hem gisteren snel <w>weglopen</w>.",
                            "target_lang_translations": {
                                "ru": "Я видел, как он вчера быстро <w>уходил</w>."
                            }
                        },
                        {
                            "text": "Waarom <w>loop</w> je boos <w>weg</w> als ik praat?",
                            "target_lang_translations": {
                                "ru": "Почему ты сердито <w>уходишь</w>, когда я говорю?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wegvloeien",
                        "wegvloeien"
                    ],
                    "antonyms": [
                        "blijven"
                    ],
                    "common_phrases": [
                        "boos weglopen",
                        "snel weglopen",
                        "zomaar weglopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покинуть место пешком"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "покидать место"
                            },
                            {
                                "target_lang_word": "уйти",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0506f89c-d7e8-309c-aa72-cf3eb698d97e",
                    "sense_definition": "Vluchten uit een situatie of van een persoon, vaak zonder toestemming",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verlaten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tiener dreigde van huis <w>weg</w> te <w>lopen</w>.",
                            "target_lang_translations": {
                                "ru": "Подросток грозился <w>сбежать</w> из дома."
                            }
                        },
                        {
                            "text": "Je kunt niet altijd voor je problemen <w>weglopen</w>.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь вечно <w>убегать</w> от своих проблем."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegvloeien"
                    ],
                    "common_phrases": [
                        "van huis weglopen",
                        "voor problemen weglopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбежать из ситуации или от человека, часто без разрешения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбегать",
                                "target_lang_sense_clarification": "тайно покидать (напр. из дома)"
                            },
                            {
                                "target_lang_word": "убегать",
                                "target_lang_sense_clarification": "избегать (напр. проблем)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4ef2a9a-955e-3cdb-a907-bd23b0f52bab",
                    "sense_definition": "Wegstromen of verdwijnen van vloeistof",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "stromen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het regenwater kan niet goed <w>weglopen</w> door de verstopte put.",
                            "target_lang_translations": {
                                "ru": "Дождевая вода не может нормально <w>уходить</w> из-за засоренного стока."
                            }
                        },
                        {
                            "text": "Zorg dat het water in de douche makkelijk kan <w>weglopen</w>.",
                            "target_lang_translations": {
                                "ru": "Убедись, что вода в душе может легко <w>стекать</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegvloeien"
                    ],
                    "common_phrases": [
                        "het water loopt weg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вытекать или исчезать (о жидкости)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стекать",
                                "target_lang_sense_clarification": "течь вниз"
                            },
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "исчезать в сток"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fdda984d-be2e-3c0b-9121-de7d980ae19f",
                    "sense_definition": "Suffering from diarrhea, particularly used in reference to livestock such as calves.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer separated the <w>scoury</w> calves from the healthy ones to prevent the spread of disease.",
                            "target_lang_translations": {
                                "ru": "Фермер отделил <w>страдающих поносом</w> телят от здоровых, чтобы предотвратить распространение болезни."
                            }
                        },
                        {
                            "text": "Proper nutrition is essential to ensure that young animals do not become weak and <w>scoury</w>.",
                            "target_lang_translations": {
                                "ru": "Правильное питание необходимо для того, чтобы молодые животные не ослабли и не стали <w>страдать поносом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "diarrheic",
                        "sick"
                    ],
                    "antonyms": [
                        "healthy"
                    ],
                    "common_phrases": [
                        "scoury calf",
                        "scoury livestock"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страдающий диареей (обычно используется по отношению к домашнему скоту, например, телятам)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страдающий поносом",
                                "target_lang_sense_clarification": "о животных"
                            },
                            {
                                "target_lang_word": "поносящий",
                                "target_lang_sense_clarification": "ветеринарный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33ea7f30-c7ec-3dde-8cc2-c0bc79254e79",
                    "sense_definition": "Marked by intermittent rain showers and wind; showery and blustery.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to postpone our sailing trip because of the <w>scoury</w> weather forecast.",
                            "target_lang_translations": {
                                "ru": "Мы решили отложить наше морское путешествие из-за прогноза о <w>ненастной</w> погоде."
                            }
                        },
                        {
                            "text": "The captain looked up at the <w>scoury</w> sky and predicted a rough night ahead.",
                            "target_lang_translations": {
                                "ru": "Капитан посмотрел на <w>хмурое, дождливое</w> небо и предсказал тяжелую ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "showery",
                        "blustery",
                        "gusty"
                    ],
                    "antonyms": [
                        "calm",
                        "clear",
                        "sunny"
                    ],
                    "common_phrases": [
                        "scoury sky",
                        "scoury night",
                        "scoury weather"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "often dialectal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся кратковременными дождями и порывистым ветром; ненастный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ненастный",
                                "target_lang_sense_clarification": "о погоде"
                            },
                            {
                                "target_lang_word": "дождливый и ветреный",
                                "target_lang_sense_clarification": "о погоде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "693089c5-d17d-3b78-8a98-ae2e39e9a7ce",
                    "sense_definition": "Having a shabby, worn, or unkempt appearance.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old man wore a <w>scoury</w> coat that looked like it had not been cleaned in years.",
                            "target_lang_translations": {
                                "ru": "Старик был одет в <w>потрепанное</w> пальто, которое выглядело так, будто его не чистили годами."
                            }
                        },
                        {
                            "text": "He was a <w>scoury</w> fellow who seemed to have lived a hard life on the streets.",
                            "target_lang_translations": {
                                "ru": "Это был <w>неопрятный</w> тип, который, казалось, прожил тяжелую жизнь на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "shabby",
                        "ragged",
                        "scruffy",
                        "worn"
                    ],
                    "antonyms": [
                        "neat",
                        "smart",
                        "tidy"
                    ],
                    "common_phrases": [
                        "scoury coat",
                        "scoury look"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        },
                        {
                            "trait_type": "regional",
                            "comment": "dialectal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий потрепанный, поношенный или неопрятный вид."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потрепанный",

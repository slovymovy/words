                {
                    "sense_id": "96220e8f-66e6-34b2-af93-72496c55f318",
                    "sense_definition": "Предмет, предназначенный для детских игр.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький ребёнок радостно тянул руки к новой яркой <w>игрушке</w>.",
                            "target_lang_translations": {
                                "en": "The small child joyfully reached out for the new bright <w>toy</w>."
                            }
                        },
                        {
                            "text": "В углу комнаты стояла большая корзина, полная детских <w>игрушек</w>.",
                            "target_lang_translations": {
                                "en": "In the corner of the room stood a large basket full of children's <w>toys</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мягкая игрушка",
                        "магазин игрушек",
                        "детские игрушки",
                        "ёлочная игрушка",
                        "любимая игрушка"
                    ],
                    "target_lang_definitions": {
                        "en": "An object designed for children to play with."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "toy",
                                "target_lang_sense_clarification": "primary meaning"
                            },
                            {
                                "target_lang_word": "plaything",
                                "target_lang_sense_clarification": "formal or literary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2475db8-ad89-3cf7-aeb5-8728bffa2e34",
                    "sense_definition": "Компьютерная игра или видеоигра.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "software",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат вчера скачал на телефон новую интересную <w>игрушку</w>.",
                            "target_lang_translations": {
                                "en": "My brother downloaded a new interesting <w>game</w> on his phone yesterday."
                            }
                        },
                        {
                            "text": "Разработчики выпустили обновление для этой популярной <w>игрушки</w>.",
                            "target_lang_translations": {
                                "en": "The developers released an update for this popular <w>game</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "игра",
                        "видеоигра"
                    ],
                    "antonyms": [
